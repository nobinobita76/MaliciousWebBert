<script language="javascript">
	function trim(str) 
	{
		if (!str || str=="") 
			return "";
		while ((str.charAt(0)==' ') || (str.charAt(0)=='\n') || (str.charAt(0,1)=='\r')) 
			str=str.substring(1,str.length);
		while ((str.charAt(str.length-1)==' ') || (str.charAt(str.length-1)=='\n') || (str.charAt(str.length-1)=='\r')) 
			str=str.substring(0,str.length-1);
		return str;
	}

 function check()
 {
	var curForm = document.form1;
	var key = curForm.keyword.value;
	if(key == "" || trim(key) == "")
	{
		alert("请输入关键词然后进行检索");
		return false;
	}
	curForm.searchword.value=key;
	return true;
 }
</script> 		     

  <form name="form1" method="post" action="/search/site_search.jsp" target="_blank" language="javascript" onsubmit="return check()"></form> 
