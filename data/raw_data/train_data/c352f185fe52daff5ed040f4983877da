<script type="text/javascript">
//输入框获取焦点和失去焦点效果
function eventFocusAndBlur(id,text){
	$(id).focus(function(){
		if($(this).val()=="" || $(this).val()==text){
			$(this).val("");
			$(this).removeClass("color999");
		}
		else{
			$(this).removeClass("color999");
		}
	});
	$(id).blur(function(){
		if($(this).val()=="" || $(this).val()==text){
			$(this).val(text);
			$(this).addClass("color999");
		}
	});
}
</script>

