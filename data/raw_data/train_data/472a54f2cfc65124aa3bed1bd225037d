<script src="/cust/js/??esf/jquery-1.7.2.min.js,esf/jquery.json-2.2.min.js,esf/core.js,esf/jq-overlay.js,custweb/resetpwd/resetpwd.js,common/progress.js,common/jquery.mouseDelay.min.js,common/jquery.cookie.min.js,component/ValidateUtils.js,component/aircity.js,component/cityAutoComplete.js,component/suggest.js,component/mails.js,component/loginmailsuggest.js,component/mailsuggest.js" language="javascript" type="text/javascript"></script>



<script language="javascript" type="text/javascript" src="/cust/js/component/verifycode.js"></script>
<script language="javascript" type="text/javascript">
jQuery(function ($) {
    $('#verifyCodeImg').attr("src", Esf.contextPath + "/verifyCode/verifyCode.action");
    
   var processSteps=new Array("输入账号","身份验证","重置密码","成功");
  $(".fs-step").process({
    source:processSteps,
    currentStep:1,
    divwidth:985
  });
  
     var name = GetRequest("resetpassBean.loginName");
     
   if(name == "" || name == null){
        $("#emailName").val("邮箱/手机/上网卡/固网（无区号）");
    }else{
        $("#emailName").val(name);
        $("#emailName").css("color","#333");
    }
    
     $("#arrcity").focus(function(){
      $("#hotCity").show();
      if($("#arrcity").val() == '地市（中文/拼音）' || ''== $.trim($("#arrcity").val())){
      $("#arrcity").val('');
      }
    }).blur(function(){
    if($("#arrcity").val() == '地市（中文/拼音）' || ''== $.trim($("#arrcity").val())){
        $("#arrcity").css({"color":"rgb(153, 153, 153)","font-weight":""});
        $("#arrcity").val('地市（中文/拼音）');
        }
   }).keydown(function(e){
  var currKey=0,e=e||event; currKey=e.keyCode||e.which||e.charCode;
  if(currKey==32){
   return false;
     }
     var _this=$(this),datav=_this.attr("data-value"),val=_this.val();
     if(val==datav)_this.val("").css({"color":"#333","font-weight":"bold"});
   });
 $("#arrcity").suggest(citys,{
     hot_list:commoncitys,
     dataContainer:'#arrcityWord',
     attachObject:'#suggestCity',
     hotObject:'#hotCity',
     hintText:$("#arrcity").attr("data-value"),
     maxItems:10,
     hideCode:'#cityCode',
     change:function(){
        $("#arrcity").css("color",'#333');
     }
   });
    $(".js-hotcitylist").click(function(){
     var selectedCity = $(this).html().trim();
     $("#arrcity").css("color",'#333');
     $("#arrcity").val(selectedCity);
     for(var i=0;i<citys.length;i++){
        if (selectedCity ==citys[i][1]){
         $("#cityCode").val(citys[i][4]);
         break;
        }
     }
     $("#arrcity").focus();
     $("#hotCity").hide();
  });
 
 $(".close-ico").click(function(){
     $("#hotCity").hide();
   });
$("#verifyCode").live({
    blur:function(){
        var _this=$(this),datav=_this.attr("data-value"),val=_this.val(),len=_this.attr("maxlength");
        if(val=="" || val==datav){
            $(".errortips").html("请输入验证码");
            $("#error-span").show();
            $("#verifyCode").css("border", "1px solid #FF0000");
            return ;
            
        }else if(val.length!=len){
            $(".errortips").html("验证码是4位字符");
            $("#error-span").show();
            $("#sl-verifycode").addClass("sl-error-verifycode");
            $("#sl-verifycode").show();
            $("#verifyCode").css("border", "1px solid #FF0000");
            return ;
        } 
        $("#verifyCode").css("border", "1px solid #C0C0C0");
        return;
    },keyup:function(){
        var _this=$(this),val=_this.val(),len=_this.attr("maxlength");
        if (val.length==len) {
            var verifyId = _this.attr("id");
            if (verifyId == "verifyCode" || verifyId == "verifyCodeUnicom") {
                checkVerifyCode();
            } 
        } else {
             $("#sl-verifycode").removeClass("sl-error-verifycode");
             $("#sl-verifycode").removeClass("sl-correct-verifycode");
            
        }
    },keydown:function(e) {
        var currKey=0,e=e||event; currKey=e.keyCode||e.which||e.charCode;
        if(currKey==32) {
          return false;
        }
        if (currKey == 13) {
            var _this=$(this),verifyId = _this.attr("id");
            if (verifyId == "verifyCode") {
                $("#submit").focus();
                $("#submit").trigger("click");
            } 
        }
    }
}); 
 
 
 
 $("#emailName").live({
    keyup:function(){
        var _this=$(this),val=_this.val();
        var defVal = $("#emailName").attr("data-value");
        if (val.trim().length==0) {
            $("#sl-delect").hide();
        } else {
            $("#sl-delect").show();
            $("#emailName").val(val.replace(defVal,''));
        }
    },keydown:function(e) {
        var currKey=0,e=e||event; currKey=e.keyCode||e.which||e.charCode;
        if(currKey==32) {
          return false;
        }
    }
}); 
 
$("#sl-delect").live({
    click:function(){
        var inputObj=$(this).parents("dd").find("input"),datav=$(inputObj).attr("data-value"),objId=$(inputObj).attr("id");
        $(inputObj).val(datav).css({"color":"rgb(153, 153, 153)","font-weight":""});;
        $(this).hide();
        $(".errortips").html("");
        $("#emailName").css("border", "1px solid #C0C0C0");
        $("#error-span").hide();
        $("#cardtypedl").hide();
        $("#usertypedl").hide();
    }
}); 
 
 
 
 
    
});

</script>
