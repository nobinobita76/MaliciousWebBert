<script>
function mo(links) {
    var clientType = (function () {
        var ua = navigator.userAgent,
            result = '';

        if (!!ua.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/)) {
            result = 'ios';
        } else {
            result = 'android';
        }

        if (!!/\bMicroMessenger\b/i.test(ua) || !!/\bQQ\b/i.test(ua)) {
            result =  'qq-' + result;
        }
        return result;
    }());

    function optimizeDownloadLink(anchor) {
        if (!anchor) {
            return;
        }

        var href = links[clientType];

        if (!href || (href && href.indexOf('.babytree.com') >= 0)) {
            return;
        }

        anchor.href = href;
    }
    var as = document.querySelectorAll('a');
    var a = as[as.length - 1];
    optimizeDownloadLink(a);
    a.addEventListener('click', function (e) {
        try{
            one.stat(links['refcode'] + '-' + clientType);
        }catch(e){};
    }, false);
}
</script>


