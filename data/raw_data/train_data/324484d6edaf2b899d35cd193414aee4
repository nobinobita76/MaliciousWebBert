	function dr_top_search() {
		var keyword = $('#keyword').val();
		if (keyword == '请输入关键字') keyword = '';
		if (!keyword) {
			alert("请输入关键字");
			return;
		}
		// 按模块组合搜索地址
		var module = "gongfu";
		if (module) {
			window.location.href='http://www.wfeng.net//index.php?c=so&module=gongfu&keyword='+keyword;
		} else {
			alert("你没有选择模块呢");
		}
	}
