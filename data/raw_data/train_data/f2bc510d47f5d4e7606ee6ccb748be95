<script type="text/javascript">
$(document).ready(function(){
	var $divLi=$(".big div");
	var $IcoLi=$(".small li");
	var $IcoSpan=$(".small li span");
	var i=0;
	var iNow=0;
	for(i=0; i<$IcoSpan.length; i++){
		$IcoLi[i].index=i;
		$IcoLi[i].onmouseover=function(){
			clearInterval(timer);
			if(iNow==this.index){
				return;	
			}
			iNow=this.index;
			tab();
		}
		$IcoLi[i].onmouseout=function(){
			timer=setInterval(autoPlay,5000);
		}
	}
	function tab(){
		for(i=0; i<$IcoSpan.length; i++){
				$IcoSpan[i].style.background='black';
				$divLi[i].style.display='none';
			}
			$IcoSpan[iNow].style.background='none';
			$divLi[iNow].style.display='block';
	}
	function autoPlay(){
		iNow++;
		if(iNow>=$IcoSpan.length){
			iNow=0;	
		}
		tab();
	}	
	var timer=setInterval(autoPlay,5000);
})
</script>

