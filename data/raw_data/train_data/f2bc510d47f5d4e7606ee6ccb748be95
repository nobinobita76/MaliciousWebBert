<script type="text/javascript">
$(function(){
    flush_login();
})

function flush_login(){
	var user = get_user();
	var curDate = new Date();
	var curHour = curDate.getHours();
	var hello = '';
	if(curHour>=18){
		hello = '晚上好';
	}else if(curHour>=13){
		hello = '下午好';
	}else if(curHour>=12){
		hello = '中午好';
	}else if(curHour>=9){
		hello = '上午好';
	}else if(curHour>=6){
		hello = '早上好';
	}else if(curHour>=0){
		hello = '凌晨好';
	}
	
	
	if(user){
		/* 判断是否为疑似手机注册的用户 */
		if(parseInt(user.username)==user.username && (user.username.length)==11 && user.username.substring(0,1)==1 && user.reg_time<=1338969600)
		{
			window.location.href=_cfg_host_passport+'\/member\/reset_username.php';
		}
			/*var acur_username = user.username;
			var convert_username = "ac**********e";*/
			var html='<span class="fl">'+hello+'，</span><a href="'+_cfg_host_user+'" class="username">'+user.nickname+'</a><span class="fl"> </span><a href="javascript:logout_ajax();" class="quit">[退出]</a>';
			
			$('#userbar').html(html);
			if(user.group_author==1){
				var center_html = '';
				center_html+='<a href="'+_cfg_host_user_comic_new+'" target="_blank">漫画管理</a>';
				center_html+='<a href="'+_cfg_host_user_comic_new+'/comic/comic_add.php" target="_blank">发表漫画</a>';
				center_html+='<a href="'+_cfg_host_user_comic_new+'/author/cash.php" target="_blank">我的钱包</a>';
				center_html+='<a href="'+_cfg_host_user_comic_new+'/message/message_list.php" target="_blank">作者短信</a>';
				center_html+='<p class="spline"></p>';
				$('#topcenter dd').prepend(center_html);
			}
			get_notify_count();
			MessageTimeId=setInterval(get_notify_count,5*30000);
	}else{
		var html='<span class="fl"></span><a href="javascript:login_ajax();" class="username">[登录]</a><a href="javascript:reg_ajax();" class="quit">[注册]</a>';
		$('#userbar').html(html);
		if(MessageTimeId!=0)
			clearInterval(MessageTimeId);
	}

}

</script>
