    function AddInputUB() { 
        try {
            if(document.getElementById("hid_inputUBb") && document.getElementById("hid_inputUBp")) {
                var inputUBb = document.getElementById("hid_inputUBb").value;
                var inputUBp = document.getElementById("hid_inputUBp").value;
                if(inputUBp != "{}") {
                    _ub.collect(parseInt(inputUBb),JSON.parse(inputUBp));
                    setTimeout(function(){},5000); //延迟500毫秒
                }
            }
        }
        catch(err) { 
        }
    }
    
    //添加"地图交通 搜周边"ub
    function AddMapTrafficUB(ubb,name) {
        try {
            if(jQuery("#MapTrafficUBp")) {
                var mapTrafficUBp = jQuery("#MapTrafficUBp").val();
                if(mapTrafficUBp != "{}") {
                    var model = JSON.parse(mapTrafficUBp);
                    model["vwe.aroundfacility"] = EncodeUtf8(name);
                    _ub.collect(parseInt(ubb),model);
                }
            }
        }
        catch(err) { 
        }
    }
    //添加"地图交通 查路线"ub
    function AddMapRouteUB(ubb,trafficType,startName,endName) {
        try {
            if(jQuery("#MapTrafficUBp")) {
                var mapTrafficUBp = jQuery("#MapTrafficUBp").val();
                if(mapTrafficUBp != "{}") {
                    var model = JSON.parse(mapTrafficUBp);
                    model["vwe.arriveway"] = EncodeUtf8(trafficType);
                    model["vwe.startaddress"] = EncodeUtf8(startName);
                    model["vwe.endaddress"] = EncodeUtf8(endName);
                    _ub.collect(parseInt(ubb),model);
                }
            }
        }
        catch(err) { 
            
        }
    }
    //议价"ub
    function AddbargainingUB(name, mobile, price, email) {
        try {
            if(jQuery("#bargainingUBb")&&jQuery("#bargainingUBp")) {
                var bargainingUBb = jQuery("#bargainingUBb").val();
                var bargainingUBp = jQuery("#bargainingUBp").val();
                if(bargainingUBp != "{}") {
                    var model = JSON.parse(bargainingUBp);
                  
                    model["vwe.name"] = EncodeUtf8(name);
                    model["vwe.totalprice"] = EncodeUtf8(price);
                    model["vwe.phone"] = mobile;
                    model["vwe.mail"] = email;
                    _ub.collect(parseInt(bargainingUBb),model);
                }
            }
        }
        catch(err) { 
            
        }
    }

    //备注"ub
    function AddRemarkUB() {
        try {
            if( document.getElementById("RemarkUBB")&&document.getElementById("RemarkUBP")) {
                var remarkUBB = document.getElementById("RemarkUBB").value;
                var remarkUBP = document.getElementById("RemarkUBP").value;
                if(remarkUBP != "{}") {
                    var model = JSON.parse(remarkUBP);
                    _ub.collect(parseInt(remarkUBB),model);
                }
            }
        }
        catch(err) { 
            
        }
    }

    // 直约业主"ub
