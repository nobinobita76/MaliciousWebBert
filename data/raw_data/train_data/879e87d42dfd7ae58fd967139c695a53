    //异步加载收藏房源按钮
    function initShouCangHouse() {
        jQuery.ajax({
            url: "/newsecond/mysoufun.aspx?pagetype=initpage&houseid=" + houseID + "&linkurl=" + window.location.href,
            type: "GET",
            success: function (data) {
                var jsons = eval("(" + data + ")");
                if (jsons.result != "" && jsons.result != "-1")//已经收藏过
                {
                    $(".btnbg .star").addClass("star1");
                    $("#sc").html("已收藏");
                    $("#myselectedid").val(jsons.result);
                }
            }
        });
    }

    function shoucang() {
        var sc = document.getElementById("sc");
        var val = sc.innerHTML;
        if (val == "收藏") {
            jQuery.ajax({
                type: "GET",
                cache: false,
                dataType: "text",
                url: "/newsecond/include/DefaultUserLogin.aspx?method=init",
                success: function (msg) {
                    var Json = eval("(" + msg + ")");
                    if (Json.result == "100") {
                        //ajax保存
                        jQuery.get('/newsecond/mysoufun.aspx?returnid=1&userid=' + Json.userid + '&face=http://cdnsfb.soufunimg.com/1/2017_2/14/M16/4/ecbd56e2412f47ca9827d474e3eb88ea.jpg&name=%d3%ad%d4%f3%ce%f7%b4%f3%bd%d6+%b0%c2%c1%d6%c6%a5%bf%cb%bb%a8%d4%b0%c8%ab%d1%f4%c1%bd%be%d3+%be%ab%d7%b0%d0%de+%c1%e0%b0%fc%c8%eb%d7%a1+%cd%f8%c7%a9%b3%ac%b5%cd%bc%db&address=%d3%ad%d4%f3%ce%f7%b4%f3%bd%d6%2c%ce%f7%bb%aa%d4%b7%b6%d4%c3%e6&price=47&city=%cc%ab%d4%ad&linkurl=http://esf.taiyuan.fang.com/chushou/3_184377561.htm&type=esf&pricetype=%cd%f2%d4%aa%2f%cc%d7&compareid=184377561&projurl=http%3a%2f%2faolinpikehuayuanty.fang.com%2f&fangdai=http%3a%2f%2f8.fang.com%2fjsq%2f%3funit_price%3d6026%26buildingarea%3d78%26cshort%3dty&room=2&hall=2&floor=3&totalfloor=26&forward=%c4%cf&createtime=2012&area=78&inserttime=2017/3/1 19:00:37&unitprice=6026&propertype=0&comarea=%d3%ad%d4%f3%ce%f7%b4%f3%bd%d6&district=%cd%f2%b0%d8%c1%d6', function (data) {
                            //弹出到我的选房单提示框
                            var jsons = eval("(" + data + ")");
                            if (jsons.result != "房源成功添加到选房单啦" && jsons.result != "你已经添加过该房源了" && isNaN(jsons.result)) {
                                jErpPopup(jsons.result, '/NewSecond/sale_info/boxes/collection.aspx', 240, 120);
                            }
                            else {
                                $(".btnbg .star").addClass("star1");
                                $("#sc").html("已收藏");
                                if (!isNaN(jsons.result)) {
                                    $("#myselectedid").val(jsons.result);
                                }
                            }
                        });
                        //ub 收藏 
                        AddShouCangUB();
                    }
                    else {
                        //弹出登录
                        jErpPopup("您还没有登录，请先登录", '/NewSecond/sale_info/boxes/dboxLogin.aspx?fun=shoucang', 410, 180);
                    }
                }
            });
        }
        else {
            jQuery.ajax({
                url: "/newsecond/mysoufun.aspx?pagetype=cancelshoucang&Myselectid=" + jQuery("#myselectedid").val(),
                type: "GET",
                success: function (data) {
                    var jsons = eval("(" + data + ")");
                    if (jsons.result != "" && jsons.result == "100")//已经收藏过
                    {
                        $(".btnbg .star").removeClass("star1");
                        $("#sc").html("收藏");
                        $("#myselectedid").val("");
                    }
                }
            });
        }
    }
</script>
