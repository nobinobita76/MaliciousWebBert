                <script type="text/javascript" src="http://esf.js.soufunimg.com/esf/esf/online/secondhouse/JsFiles/Common/Behavioural_app_new2016.js?v=3.201703302"></script>

<script type="text/javascript">
    _ub.city = '太原';
    var strdistrict = '', strprice = '', stroperastion = '', num = "4";
    var IsOpenNewHouseDingTou = true;
    _ub.request("vwg.usertype,vwe.source,vwe.housetab,vwz.housetype,vwz.housetype,vwz.housetab", function () {
        //用户类型
        if (document.getElementById("UserType")) {
            if (_ub.getValue('vwg.usertype', 1) != '') {
                document.getElementById("UserType").value = _ub.getValue('vwg.usertype', 1);
            }
        }
        /*二手房住宅*/
        //列表页访次数
        if (document.getElementById("vwe.source")) {
            document.getElementById("ee").value = _ub["vwe.source"];
        }
        //查看的房源量
        if (document.getElementById("ef")) {
            document.getElementById("ef").value = _ub["vwe.housetab"];
        }
        /*租房住宅*/
        //列表页访次
        if (document.getElementById("zc")) {
            document.getElementById("zc").value = _ub["vwz.housetype"];
        }
        //查看的房源量
        if (document.getElementById("zd")) {
            document.getElementById("zd").value = _ub["vwz.housetype"];
        }

        /*租房别墅*/
        //列表页访次
        if (document.getElementById("zr")) {
            document.getElementById("zr").value = _ub["vwz.housetab"];
        }
    
    });
    
    var isload = true;
    _ub.request("v",function(){
        jQuery(window).scroll(function () {
            var fold = jQuery(window).height() + jQuery(window).scrollTop();         
            if (fold > jQuery("#InterestedHouse").offset().top && isload) {
                var condition1;
                var condition2;
                var condition3;
                var condition4;
                var condition5;

                condition1 = behavioural.getCondition(_ub, "house", "esf", 1);
                getDingTouCondtion(_ub, dingtoupurpose, 1);
                var ubi = 2;
                while (ubi <= 5) {
                    _ub.load(0);
                    if (ubi == 2) {
                        condition2 = behavioural.getCondition(_ub, "house", "esf", ubi);
                        getDingTouCondtion(_ub, dingtoupurpose, 2);
                    } else if (ubi == 3) {
                        condition3 = behavioural.getCondition(_ub, "house", "esf", ubi);
                        getDingTouCondtion(_ub, dingtoupurpose, 3);
                    } else if (ubi == 4) {
                        condition4 = behavioural.getCondition(_ub, "house", "esf", ubi);
                        getDingTouCondtion(_ub, dingtoupurpose, 4);
                    } else if (ubi == 5) {
                        condition5 = behavioural.getCondition(_ub, "house", "esf", ubi);
                        getDingTouCondtion(_ub, dingtoupurpose, 5);
                    }
                    ubi = ubi + 1;
                }
                var clickidhouse =""; 
                var clickidnew =""; 
                try {
                    var url = window.location.href;
                    //默认chushou/3 为 agantesfxq_B04_01 
                    if (url.indexOf("chushou/14")>-1) {
                    }
                     else if (url.indexOf("chushou/3")>1) {
                         clickidhouse="agantesfxq_B04_01"; 
                         clickidnew="agantesfxq_B04_02"; 
                     }
                    
                } catch (e) {

                } 
                jQuery.ajax({
                    url: "/NewSecond/Include/InterestedHouseDetail2014.aspx?condition1=" + condition1 + "&condition2=" + condition2 + "&condition3=" + condition3 + "&condition4=" + condition4 + "&condition5=" + condition5 + "&recenthouse=0&clickid="+clickidhouse+"&r=" + Math.random(),
                    type: "GET",
                    beforeSend: function (XMLHttpRequest) {
                        isload = false;
                    },
                    success: function(data) {
                        SetInterestHouse(data);
                    },
                    error: function () {
                        isload = false;
                    }
                });

                
                jQuery.ajax({
                    url: "/EsfHouse/Ajax/UBNewHouseDingTou.aspx?strdistrict=" + strdistrict + "&strprice=" + strprice + "&stroperastion=" + stroperastion + "&num=" + num + "&page=esfxq",
                    type: "GET",
                    beforeSend: function (XMLHttpRequest) {
                        isload = false;
                    },
                    success: function (data) {
                        jQuery("#dingtou_newhouse").html("<div id='"+clickidnew+"'>"+data+"</div>");
                    },
                    error: function () {
                        isload = false;
                    }
                });
                    
                    }
        });
    });
    //_ub用户行为对象
    //当前页物业类型
    //次数
    function getDingTouCondtion(_ub, purpose, num) {
        if(IsOpenNewHouseDingTou) {
            switch (purpose) {
                case "住宅":
                    if (typeof (_ub["vwe.position"]) != "undefined" && _ub["vwe.position"] != "") {
                        strdistrict += "|" + _ub["vwe.position"];
                    }
                    if (typeof (_ub["vwe.totalprice"]) != "undefined" && _ub["vwe.totalprice"] != "" && num == 1) {
                        strprice = _ub["vwe.totalprice"];
                    }
                    if (num == 1) {
                        stroperastion = "r";
                    }
                    break;
                case "别墅":
                    if (typeof (_ub["vwe.position"]) != "undefined" && _ub["vwe.position"] != "") {
                        strdistrict += "|" + _ub["vwe.position"];
                    }
                    if (typeof (_ub["vwe.totalprice"]) != "undefined" && _ub["vwe.totalprice"] != "" && num == 1) {
                        strprice = _ub["vwe.totalprice"];
                    }
                    if (num == 1) {
                        stroperastion = "v";
                    }
                    break;
            }
        }
        
    }

  
</script>
