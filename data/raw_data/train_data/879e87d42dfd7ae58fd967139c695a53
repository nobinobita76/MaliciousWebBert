<!--在线客服开始-->
<script src="http://js.soufunimg.com/upload/webim/newim/scripts/im1.0.1.js" charset="utf-8"></script>
<script type="text/javascript">

    //在线咨询经纪人列表，json格式
    var IMAgentList = [{"AgentID":"164184689","AgentName":"侯占燕","ManagerName":"shenhou89","ShopUrl":"http://esf.taiyuan.fang.com/a/shenhou89","Avatar":"http://img1n.soufun.com/usercenter/2015_08/05/14/avatar/120_164184689_0.jpg","Tel":"18636680903"}];
    //在线咨询经纪人用户名，多个以","隔开
    var IMManagerName = "shenhou89";
    //在线图片
    var onlineImg = "http://img.soufun.com/rent/image/rent/FangJsCss/images/talkAgentIcon.gif";
    //不在线图片
    var offlineImg = "http://img.soufun.com/rent/image/rent/FangJsCss/images/btnLixian.gif";
    function getCookie(name) {
        var arr = document.cookie.match(new RegExp('(^| )' + name + '=([^;]*)(;|$)'));
        return arr && arr[2] || '';
    }
    //初始化IM
    jQuery(document).ready(function () {
        var sfutcookievalue = '';
        try
        {
            sfutcookievalue = getCookie('sfut');
        }
        catch(e){
            sfutcookievalue = '';
        }
        if (sfutcookievalue != '') {
            jQuery.ajax({
                type: "get",
                url: "/newsecond/include/DefaultUserLoginNew.aspx?method=init",
                dataType: "json",
                success: function (userData) {
                    var data = eval(userData);
                    window.fangWebIM.init({
                        type: "webesf",
                        userid: userData.userid,//登录用户请传通行证用户ID，非登录不传
                        username: userData.realusername,//登录用户请传通行证用户名，非登录不传
                        showname: userData.realusername,//显示用
                        city: "太原"//城市必填
                    });
                },
                error: function () {
                    window.fangWebIM.init({
                        type: "webesf",
                        userid: '',//登录用户请传通行证用户ID，非登录不传
                        username: '',//登录用户请传通行证用户名，非登录不传
                        showname: '',//显示用
                        city: "太原"//城市必填
                    });
                }
            });
        }
        else
        {
            window.fangWebIM.init({
                type: "webesf",
                userid: '',//登录用户请传通行证用户ID，非登录不传
                username: '',//登录用户请传通行证用户名，非登录不传
                showname: '',//显示用
                city: "太原"//城市必填
            });
        }

        //遍历经纪人列表，绑定事件
        jQuery.each(IMAgentList, function (i, agent) {
            jQuery("[name='" + agent.AgentID + "online']").click(function () {
                window.fangWebIM.openChat({
                    "type": "webesf",//新房为xf，二手房为agent，家居为home
                    "contactId": agent.ManagerName,
                    "showname": agent.AgentName,
                    "city": "太原",
                    "pname": '0',
                    "tip": "我正在浏览您的房源",
                    "shop": agent.ShopUrl,
                    "avatar": agent.Avatar,
                    "tel": agent.Tel
                });
            });
        });

        jQuery.ajax({
            type: "get",
            url: "/EsfHouse/Ajax/IMOnlieStatus.aspx",
            data: "username=" + IMManagerName,
            dataType: "json",
            success: function (onlineData) {
                var data = eval(onlineData);
                jQuery.each(data, function (n, statusValue) {
                    //查找agentid
                    jQuery.each(IMAgentList, function (m, agentValue) {
                        if (agentValue.ManagerName.replace("ae:","") == statusValue.UserName.replace("ae:","")) {
                            //在线/留言图片
                            var talkImg = jQuery("[name='" + agentValue.AgentID + "online']");

                            if (statusValue.Status == 1) {//在线
                                talkImg.attr("src", onlineImg);
                            } else {//不在线
                                talkImg.attr("src", offlineImg);
                            }

                            talkImg.show();
                        }
                    });

                });
            }
        });
    });
</script>
<!--在线客服结束-->
<!--网店房源点击量-->
<div id="mallHouseDiv" style="display:none;"></div>
<script language="JavaScript" type="text/javascript">
    var mallInfo = imageHits.queryString("mall");
    var mallType = '';
    var mallNum = '';
    var mallSource = '';            //来源:1表示来自列表页楼盘名片处网店房源
    var url = '';
    var purpose = "住宅";
    function mallHouse() {
        try {
            if (mallInfo != null) {
                mallType = mallInfo.split(',')[0];
                mallNum = mallInfo.split(',')[1];
                if (mallInfo.split(',')[2])
                {
                    mallSource = mallInfo.split(',')[2];
                }
                if (mallNum != null && mallType != null) {
                    if (isNorth == "Y") {
                        url = "http://clickstoren.esf.soufun.com/agent_";
                    }
                    else {
                        url = "http://clickstores.esf.soufun.com/agent_";
                    }
                    if (purpose == '住宅' || purpose == '别墅') {
                        url += agentId + "_" + houseID + "_" + shortCity + "_cs_" + mallType + "_" + mallNum + (mallSource == "" ? "" : "_" + mallSource) + ".gif";
                    }
                    else {
                        url += agentId + "_" + houseID + "_" + shortCity + "_bus_" + mallType + "_" + mallNum + ".gif";
                    }
                    var img = document.createElement("img");
                    img.src = url;
                    document.getElementById("mallHouseDiv").appendChild(img);
                }
            }
        }
        catch (err) { }
    }
    mallHouse();
</script>
<!--网店房源点击量end-->
