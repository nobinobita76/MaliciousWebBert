        }
    }
    //登录ub
    function AddLoginUB(un) {
        try {
            var vb=document.getElementById("__vb");
            if (vb&&un != "" && un != undefined) {
                var model = {};
                 model["vwe.username"] = un;
                 model["vwg.page"] = vb.src.substr(vb.src.indexOf("?")+1).split('=')[1];
                _ub.collect(9, model);

            }   
        } catch (e) {

        }  
    }
