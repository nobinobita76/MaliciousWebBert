        } 
        jErpPopup('议价', ' /newsecond/sale_info/boxes/yijia.aspx?houseid=184377561&agentID=164184689&agentName=侯占燕&projCode=1410105317&projName=太原奥林匹克花园&room=2&hall=2&buildarea=78&type='+type, 420, 350);
