(function(){
var appid = 'cyrqcP276';
var conf = 'prod_a12dadb8024fd2ddc5a86120ebf50c27';
var topicid=$('#ucomments_id').html();
var categoryid;

if(topicid==null){
    if($("meta[name='contentid']").attr("content")!=null){
        topicid= 't'+$("meta[name='contentid']").attr("content");
    }else{
        topicid= 'tl'+$("meta[name='catalogs']").attr("content");
    }
}
if($("meta[name='categoryid']").attr("content")!=null){
       categoryid = $("meta[name='categoryid']").attr("content");
}

