					<script src="//i.alicdn.com/ae-channel-ui/1.1.0/material/presell/index.js"></script>
					<script>
     $(window).load(function () {
        if(!window.flag){
            window.flag = true;
            var presells = $('.presell-container');
                $(window).on('resize', function () {
                    presells.each(function () {
                        var _self = $(this);
                        var $li = _self.find('li');
                        clearTimeout(_self.timer);
                        _self.timer = setTimeout(function () {
                            $li.height($li.width() * 1.34 + 15);
                        }, 500)
                    })
                })
            }
    })</script>
