    (function(seajs){
        function hasAttribute(dom, attr) {
            return dom.hasAttribute ? dom.hasAttribute(attr) : dom.getAttribute(attr) !== '';
        }
        function setLocaleFromHtmlLang() {
            var htmlTag = document.documentElement;
    
            if(hasAttribute(htmlTag, 'lang')){
                var locale = htmlTag.getAttribute('lang');
                if(locale){
                    seajs.config({
                        vars: {
                            'locale': locale.toLowerCase().replace(/\_/g, '-')
                        }
                    });
                }
            }
        }
        setLocaleFromHtmlLang();
    })(seajs);</script>
