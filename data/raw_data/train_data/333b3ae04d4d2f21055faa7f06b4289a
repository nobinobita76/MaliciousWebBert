        function getCookie(name) {
            var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");

            if (arr = document.cookie.match(reg))
                //alert(unescape(arr[2]));
                return unescape(arr[2]);
            else
                //alert("null");
                return "null";
        }
        function getRef() {
            var ref = document.referrer;
            if (ref != undefined && ref != "") {
                return ref;
            }
            else {
                return "null";
            }
