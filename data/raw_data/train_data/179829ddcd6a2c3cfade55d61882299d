		         if(param["paramMap.cellPhone"] ==null||param["paramMap.cellPhone"]==''){
		           alerthtml("邮箱验证码不正确");
		         }else{
		           alerthtml("手机验证码不正确");
		         }
	         	 falg = true;
	         }
	         else if(data.mailAddress=='123'){//邮箱发送失败
	             alerthtml("邮件发送失败");
	             $('#btn_register').attr('value',"免费注册");
	             falg = true;
	         }
	         else if(data.mailAddress=='99'){
	         	var userPhone=$("#userName").val();
	         	var userID=data.id;
				var temp="userid="+userID+"&phone="+userPhone;
