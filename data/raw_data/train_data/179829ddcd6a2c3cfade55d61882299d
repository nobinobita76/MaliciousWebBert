  <script>
	function switchCode(){
	   var timenow = new Date();
	   $("#codeNum").attr("src","admin/imageCode.do?pageId=userregister&d="+timenow);
    }

	function alerthtml(info){
		$.tips({
		    content: info,
		    type: 'info', //  info, success, warn
		})
	}
	function Numbercount(dom, callback){
		var dom = $(dom);
		var endNumber = parseFloat(dom.html().replace(/[^\d\.-]/g, ""));
		if( endNumber == 0 ){
			callback && callback();
		return;
		}
		var i = 0;
		var x = parseInt(endNumber/200);
		var per = 4;
		var j;
		var timer = setInterval(function(){
			if( i >= endNumber ){
				clearInterval(timer);
				j = fmoney(endNumber,2);
				dom.html(j);
				callback && callback();
				return;
			}
			j = fmoney(i,2);
			dom.html(j);
			i = i + x;
		}, per);
	}	
	function fmoney(s, n){  
	   n = n > 0 && n <= 20 ? n : 2;  
	   s = parseFloat((s + "").replace(/[^\d\.-]/g, "")).toFixed(n) + "";  
	   var l = s.split(".")[0].split("").reverse(),  
	   r = s.split(".")[1];  
	   t = "";  
	   for(i = 0; i < l.length; i ++ )  
	   {  
	      t += l[i] + ((i + 1) % 3 == 0 && (i + 1) != l.length ? "," : "");  
	   }  
	   return t.split("").reverse().join("") + "." + r;  
	}
    $(function () {
    	$('.number-count').each(function(index, item){
    		Numbercount(item);
    	})    
    	$('#passwordShow').on('click',function(){
    		if($(this).hasClass('password-off')){
    			$(this).removeClass('password-off');
    			$('input#password').attr('type','text');
    		}else{
    			$(this).addClass('password-off');
    			$('input#password').attr('type','password');
    		}
    	})
    	$('#btn_register_xy').on('tap', function () {
			$('#btn_register_xy-template').dialog('show')
		})
		
		if($("#refferee").val()!= ''){
			$("#refferdiv").hide();
		}
		
		$('#userName').on('blur', function () {
		var phone=$("#userName").val();
		
		var param={};
	
		if($("#userName").val()==""){
               alerthtml("请输入手机号码！");
                $('#userName').focus();
            }else if((!/^1[3,4,5,8,7]\d{9}$/.test($("#userName").val()))){ 
               alerthtml("手机号码错误！");
               $('#userName').focus();
            }else{
            	param["paramMap.phone"] = phone;
			 $.post("ajaxCheckRegister.do",param,function(data){
			 	if(data == -1){
			 	   alerthtml("请输入手机号码！");
               		$('#userName').focus();
			 	}else if(data==-2){
			 	  alerthtml("手机号码已注册！");
			 		$('#userName').focus();
			 	}
			 });
			}
		})
	})
	var timers=180;
    var tipId;
    var flags = false;

	$(function(){
		$("#btn_register").click(function(){
	      regg();
	    });
	    $("#clickCode").click(function(){
           var phone=$("#userName").val();
           var code = $("#code").val();
           //验证手机号码是
           if($("#userName").val()==""){
               alerthtml("请填写手机号码！");
            }else if((!/^1[3,4,5,8,7]\d{9}$/.test($("#userName").val()))){ 
               alerthtml("请正确填写手机号码！");
            }else if(code == null || code=="" ){
   				 alerthtml("请输入验证码！");
   				  switchCode();
   				 $("#code").focus();
       			return null;
            }else{
	            if($("#clickCode").html()=="获取验证码"||$("#clickCode").html()=="重新获取手机验证码"||$("#clickCode").html()=="点击获取验证码"||$("#clickCode").html()=="重新发送验证码") {   
	            if(flags) return;
	              $.post("sendSMS.do","phone="+phone+"&code="+code+"&pageId=userregister"+"&ipStr=/HYiXDxgElimWsp00JT/LA==",function(data){
	                 if(data==1){
	                    timers=180;
	                    flags = true;
	                    tipId=window.setInterval("timer()",1000);
	                 }else if(data==-1){
	                 	alerthtml("验证码错误！");
       				  	switchCode();
       				 	$("#code").focus();
	                 }else{
	                    alerthtml("手机验证码发送失败");
	                 }
	              });
	            }
            }
       });
	});
	
	//定时
   function timer(){
       if(timers>=0){
         $("#clickCode").html("获取中："+timers+"秒");
         timers--;
         
       }else{
          window.clearInterval(tipId);
           $("#clickCode").html("获取验证码");
           flags = false;
           $.post("removeCode.do","",function(data){});
           
       }
   }
   
    var falg = true;
    function regg(){
       if(falg){
	       falg = false;
	       var errornum=$("form .onError").length;
	       if($("#userName").val()==""){
	          alerthtml("请输入手机号");
	          falg = true;
	          return false;
	       }else if($("#password").val()==""){
	          alerthtml("请设置您的密码"); 
	          falg = true;
	          return false;
	       }else if($("#code").val()==""){
	        alerthtml("请输入验证码"); 
	        falg = true;
	        return false;
	      }else if(errornum > 0){   
	      	alerthtml("请正确填写注册信息");
	      	falg = true;
	          return false;
	      }else{
	          if(!$("#checkbox").attr("checked")){
	          	alerthtml("请勾选我已阅读并同意《新联在线在线条款》");
	          	falg = true;
	          return false;
	         }
	      }
	      $('#btn_register').html($('#btn_register').html()+"...");
	      var param = {};
          param["paramMap.email"] = "";
          param["paramMap.userName"] = "";
  		  param["paramMap.cellPhone"] = $("#userName").val();    		     	  
	      param["paramMap.pageId"] = "userregister";
	     // param["paramMap.email"] = $("#email").val();
	      param["paramMap.password"] = $("#password").val();
	      param["paramMap.confirmPassword"] = $("#password").val();//$("#confirmPassword").val();
	      param["paramMap.radomCode"] = $("#radomCode").val();
	      param["paramMap.refferee"] = $("#refferee").val();
	      param["paramMap.code"] = $("#code").val();
	      param["paramMap.param"] = $("#param").val();
	      
	    
	      
	      $.post("register.do",param,function(data){
	      	if(data.mailAddress == undefined){
	      		data = eval("(" + data + ")"); 
	      	}
	         if(data.mailAddress=='0'){
	            alerthtml("验证码输入有误，请重新输入");
	            $('#btn_register').attr('value',"免费注册");
	            falg = true;
	         }else if(data.mailAddress=='1'){
	          	$('#btn_register').attr('value',"免费注册");
	          	alerthtml("两次输入的密码不一致"); 
	          	falg = true;
	         }else if(data.mailAddress=='2'){
		         $('#btn_register').attr('value',"免费注册");
		         alerthtml("该用户已存在");
		         falg = true;
	         }else if(data.mailAddress=='3'){
		          $('#btn_register').attr('value',"免费注册");
		          alerthtml("该邮箱已注册");
		          falg = true;
	         }else if(data.mailAddress=='4'){
		        alerthtml("注册失败！");
		         $('#btn_register').attr('value',"免费注册");
		         falg = true;
	         }else if(data.mailAddress=='5'){
	          	$('#btn_register').attr('value',"免费注册");
	          	falg = true;
	         	alerthtml("推荐人填写错误！");
	         }else if(data.mailAddress=='6'){
	            
	          	$('#btn_register').attr('value',"免费注册");
	          	falg = true;
	         	alerthtml("手机号已经被注册！");
	         }else if(data.mailAddress=='12'){
	          	$('#btn_register').attr('value',"免费注册");
	          	falg = true;
	            alerthtml("请输入您的邮箱地址");
	         }
	         else if(data.mailAddress=='13'){
	          	$('#btn_register').attr('value',"免费注册");
	          	falg = true;
	            alerthtml("请输入登录用户名");
	         }
	         else if(data.mailAddress=='14'){
	          	$('#btn_register').attr('value',"免费注册");
	            alerthtml("请设置您的密码"); 
	            falg = true;
	         }
	           else if(data.mailAddress=='15'){
	            $('#btn_register').attr('value',"免费注册");
	           	alerthtml("请再次输入密码确认"); 
	           	falg = true;
	         }else if(data.mailAddress=='16'){
		          $('#btn_register').attr('value',"免费注册");
		          falg = true;
		          alerthtml("注册失败！");
	         }
