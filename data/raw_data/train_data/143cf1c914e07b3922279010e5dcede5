var huanNum=0;
$("#newsid_D02_30").on("click",function(){
	var ranSize = $("#juheyuedudetail>div").size();
	huanNum++;
	if(huanNum*5 >= ranSize){
		huanNum=0;
	}
	for(var i = 0 ;i<ranSize;i++ ){
		$(".tj-news")[i].style.display='none';
	}
	var arr =	haun(5,huanNum,ranSize);
	for(var x = 0 ;x<arr.length; x++){
		$(".tj-news")[arr[x]].style.display='table';
	}

});
function haun(n,huanNum,ranSize){
	 var arr=[];
		if(huanNum*5<=ranSize){
			for(var i = 0 ;i<5;i++){
				arr[i] = huanNum*5+i
			}
		}
	 return arr;
	}
</script>
                
