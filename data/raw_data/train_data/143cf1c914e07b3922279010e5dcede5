<script type="text/javascript" charset="gbk">
	var url = window.location.href;
	var title;
	var fld;
	var dir;
	var action;
	var titleName;
	var titleNames = new Array(1);
	titleNames[0] ="rdzt";
	titleName = getTitleName(titleNames,url);
	setTitleValue(titleName);
	
	function getTitleName(names,url){
	   for(var i =0;i<names.length;i++){
		if(url.indexOf(names)!=-1)
			return names[i];
		}
		return "defaults";
	} 
	
	function setTitleValue(titleName){
		if(titleName == "defaults"){
			title = "资讯搜索";
			fld = "";
			dir = "news";
			action = "http://search.soufun.com/search";
		}else if(titleName == "rdzt"){
			title = "专题搜索";
			fld = "subject";
			dir = "";
			action = "http://search.soufun.com/news/search0.jsp";
		}else{
			title = "资讯搜索";
			fld = "";
			dir = "news";
			action = "http://search.soufun.com/search";
		}
	}
</script>
<form action="http://search.soufun.com/search" id="searchForm" method="post" target="_self" name="searchForm" class="ifhd_search mt20" onsubmit="return searchSubmit();" accept-charset="GBK">
    	<input class="ifhd_search_text" id="q" name="q" type="text" onblur="if(!this.value){this.value=this.defaultValue;this.style.color='#999'}" value="请输入关键字" onfocus="if(this.value==this.defaultValue){this.value='';this.select();this.style.color='#000'}">
        <input class="ifhd_search_btn" type="submit" onmouseover="style.cursor='hand'" value="搜索" id="newxq_C01_03">
        <input type="hidden" name="dir" value="news">
		<input type="hidden" name="fld" value="">
		<input type="hidden" name="sort" value="date">
</form>

<script type="text/javascript">
searchForm.fld.value = fld;
searchForm.dir.value = dir;
searchForm.action = action;
</script>
<script type="text/javascript" charset="gbk">
function searchSubmit(){
	var so_city = '\u5317\u4eac';
	var q = jQuery('#q').val();
	var keyword = encodeURI(encodeURI(q));
	var cityScope = encodeURI(encodeURI(so_city));
	checkKeyWord(keyword,cityScope);
}
function checkKeyWord(keyword,cityScope){

	
	jQuery.ajax({
		type:'get',
        dataType:'jsonp',
        url:"http://act.news.fang.com/news/checkKeyWord",
        data:"keyWord="+keyword+"&city="+cityScope,
        complete:function(){
        },
        error:function(){
        },
        success:function(msgRes){
        	var res = msgRes.feed.result;
        	if(res!=null && res=='no'){
        		window.open('http://www.fang.com','_self')
        	}else{
        		jQuery('#searchForm').submit();
        	}
        }
	});
	return false;
}
</script>
  	<!-- 搜索 end-->
</div>
<!--头部 end-->
