    $(function(){
        $(".uesrname input").focus(function(){
            $(".pop-main .uesrname ").css("border","1px solid #ff7e00");
        });
        $(".password  input").focus(function(){
            $(".pop-main .password ").css("border","1px solid #ff7e00");
        })
        $(".uesrname input").blur(function(){
            $(".pop-main .uesrname ").css("border","1px solid #e0e0e0");
        });
        $(".password  input").blur(function(){
            $(".pop-main .password ").css("border","1px solid #e0e0e0");
        })
    })

    function openurl(url){
        var url = 'index.php?app=Home&mod=News&act=newsList&category_name='+encodeURI(url);
        window.open(url);
    }

    /*********************最新、最热资源************************************/
    function ajaxResourceList(o){

        var num = 3;
        jQuery.ajax({
//            url: U('Search/Resource/indexResource'),
            url: 'http://www.shmjy.changyan.cn/App.Search/index.php?m=Search&c=Resource&a=indexResource',
            data: {order:o,num:num},
            type: 'POST',
            success: function (res) {
//	    	jQuery("#res_list").empty();

                var result = eval("(" + res + ")");
                list = result.data;
                var Op_title = '';
                if(o=='vcount'){
                    for ( var i = 0; i < list.length; i++) {
                        Op_title = list[i].nametitle;
                        if(Op_title.length > 12){
                            title = Op_title.substr(0,12) +'...';
                        }else{
                            title = Op_title;
                        }
                        var extensionurl = list[i].extensionurl;
//                        var imgurl = extensionurl.substr(0,extensionurl.lastIndexOf('/'))   + '/ico_'
                        var imgurl = extensionurl.substr(0,extensionurl.lastIndexOf('/'))   + '/'
                                + extensionurl.substr(extensionurl.lastIndexOf('/')+1);



                        var html = '<dl class="clearfix">'
                                +'<dt class="fl"><img src="'  + imgurl +  '" height="70px" width="70px;"></dt>'
                                +'<dd class="fl">'
                                +'<div class="clearfix">'
                                +'<a class="fl title " target="_blank" title ="'+Op_title+'" href='+getResourcePreviewUrl(list[i])+ '>'+title+'</a>'
                                +'<span class="fr lineht44"><span class="ff7e00">'+list[i].vcount+'</span>人阅览</span>'
                                +'</div>'
                                +'<div>'
                                +'<em class="school">神木县第五小学</em>'
                                +'<em class="name">'+list[i].author+'</em>'
                                +'<em class="date">'+list[i].friendlytime+'</em>'
                                +'<em>发布</em>'
                                +'</div>'
                                +'</dd>'
                                +'</dl>';
//                        console.log(getResourcePreviewUrl(list[i]));
                        jQuery("#zrzylist").append(html);;
                    }
                }else{
                    for ( var j = 0; j < list.length; j++) {
                        Op_title = list[j].nametitle;
                        if(Op_title.length > 14){
                            title = Op_title.substr(0,14) +'...';
                        }else{
                            title = Op_title;
                        }
                        var extensionurl = list[j].extensionurl;
                        var imgurl = extensionurl.substr(0,extensionurl.lastIndexOf('/'))   + '/'
                                + extensionurl.substr(extensionurl.lastIndexOf('/')+1);

                        var html = '<dl class="clearfix">'
                                +'<dt class="fl"><img src="'  + imgurl +  '" height="70px" width="70px;"></dt>'
                                +'<dd class="fl">'
                                +'<div class="clearfix">'
                                +'<a class="fl title " target="_blank" title ="'+Op_title+'" href='+getResourcePreviewUrl(list[j])+ '>'+title+'</a>'
                                +'<span class="fr lineht44"><span class="ff7e00">'+list[j].vcount+'</span>人阅览</span>'
                                +'</div>'
                                +'<div>'
                                +'<em class="school">神木县第五小学</em>'
                                +'<em class="name">'+list[j].author+'</em>'
                                +'<em class="date">'+list[j].friendlytime+'</em>'
                                +'<em>发布</em>'
                                +'</div>'
                                +'</dd>'
                                +'</dl>';
                        jQuery("#zxzylist").append(html);;
                    }
                }
            }
        });
    }

    /*
     *
     * @returns {undefined}获取资源预览url
     */
    function getResourcePreviewUrl(res){

        var url ='';
        if(res.rrtlevel1 == '13'){
            url='http://www.shmjy.changyan.cn/App.Search/index.php?m=Search&c=Resource&a=preview&resId='+res.vendorResId;
        }else if( res.goodId == null || res.goodId == undefined || res.goodId == '' ){
            url='http://www.shmjy.changyan.cn/App.Search/index.php?m=Search&c=Resource&a=preview&resId='+res.vendorResId;
        }else {
//            url=Market_url+"goods.php?id="+res.vendorResId;
        }
        return url;
    }
</script>


</body></html>