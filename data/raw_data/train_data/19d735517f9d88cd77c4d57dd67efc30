<script type="text/javascript">
    var edusso = new EduSSO("http://www.shmjy.changyan.cn/web/Application/Common/sso/ssoservice.php", "http://desktop.shmjy.changyan.cn/sso/");
	
	var url = window.location.href;
	
	var userId = "";

    if(userId <=0 && url.indexOf('web') <= 0  && url.indexOf('ResourceCloud') <= 0) {
        window.location.href = "http://www.shmjy.changyan.cn/web" + "/index.php?";
    }

    
    if(url.indexOf('web') > 0){
        $(".menu_home").parent('li').addClass("selected");
    }
    if(url.indexOf('ResourceCloud') > 0){
        $(".menu_home").parent('li').addClass("selected");
    }
    if(url.indexOf('Search') > 0){
        $(".menu_zy").parent('li').addClass("selected");
    }
    if(url.indexOf('EduSNS') >0 ){
        $(".menu_space").parent('li').addClass("selected");
    }
    if(url.indexOf('ESchool')>0){
        $(".menu_zx").parent('li').addClass("selected");
    }
    if(url.indexOf('AppCenter')>0){
        $(".menu_app").parent('li').addClass("selected");
    }

