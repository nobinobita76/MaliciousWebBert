<script type="text/javascript">

    $(function() {
        $("#homeDoLogin").click(function() {
            $("#login_box").show();
        });
        $("#close_login_box").click(function() {
            $("#login_box").hide();
        });

        // 登录操作
        $("#btnLogin").click(function() {
            var inputName = $.trim($('#username').val());
            var inputPass = $.trim($('#password').val());
            if (inputName == "" || inputName == $('#username').attr("dafaltVal")) {
                layer.alert("请输入用户名！", {
                    icon : 0
                });
                return;
            }
            if (inputPass == "") {
                layer.alert("请输入密码！", {
                    icon : 0
                });
                return;
            }

            $.ajax({
                url: "index.php?app=Home&mod=Ajax&act=getLoginName",
                type: "POST",
                dataType: "json",
                data: {"inputName": inputName},
                success:function(data) {
                    if (data == null || data == ""){
                        $("#err_msg").html("用户名或密码错误");
                        $("#err_msg_area").show();
                        return;
                    }
                    checkLogin(data, inputPass);
                },
                error:function(e) {
                    $("#err_msg").html("用户名或密码错误");
                    $("#err_msg_area").show();
                }
            })
        });


        // 账号输入框点击样式改变
        $("#username").focus(function() {
            if ($(this).val() == $(this).attr("dafaltVal")) {
                $(this).val("");
                $(this).css("color", "black");
            }
        });

        // 账号输入框失去焦点改变样式
        $("#username").blur(function() {
            if ($(this).val() == "") {
                $(this).val($(this).attr("dafaltVal"));
                $(this).css("color", "#bdc6ce");
            }
        });

        // 回车登录
        $("body").keydown(function() {
            if (event.keyCode == "13") {
                $("#btnLogin").click();
            }
        });

        // 用户名输入事件
        $("#username").change(function() {
            $("#err_msg_area").hide();
        });

    });

    function checkLogin(loginName, inputPass) {
        $.post("index.php?app=Home&mod=Ajax&act=checkLoginIn", {
            inputName: loginName,
            password: inputPass
        }, function (data) {
//        	if (data == "" || data == null) {
            if (data == "" || data == null || data.status == 0) {
                $("#err_msg").html("用户名或密码错误");
                $("#err_msg_area").show();
                return;
            }else if(data.status == 1){
                $("#err_msg").html(data.info);
                $("#err_msg_area").show();
                return;
            }

            //var reviewStatus = data.userReviewInfo[0].reviewStatus;
            //用户身份类型
            var userType = data.userReviewInfo[0].userType;
            //根据登录名获取用户信息

            $.post("index.php?app=Home&mod=Register&act=getUserBaseInfoByLoginName",{
                loginName : loginName
            },function(result){
                var result = eval("("+result+")");
                var obj = result.Data
                //用户Id
                var userId = obj.id;
                //用户身份证号码（用户判断是否为后台用户）
                var idCardNo = obj.idCardNo;
                var phone = obj.phone;
                //根据userId判断用户是否认证
                $.post("index.php?app=Home&mod=Ajax&act=isAuthentication",{
                    userId : userId
                },function(auResult){
                    var auResult = eval("("+auResult+")");
                    //用户认证状态
                    var userAutStatus = auResult.Data;
                    // if(userType == "002" && phone == "" && userAutStatus == "0" && idCardNo != "")
                    if (userAutStatus == "0" && idCardNo != "" && userType != "004") {
                        var href = "";
                        if (userType == "002" && phone == "") {
                            window.location.href = "index.php?app=Home&mod=Register&act=binding_phone&inputLg=" + Base64.encode(loginName) + "&inputPw=" + Base64.encode(inputPass);//教师绑定手机页面
                        } else if(userType == "002" && phone != ""){
                            var redirectUrl ='';
                            var currentUrl = window.location.href;
                            var nextpage_index = currentUrl.indexOf("nextpage=");
                            var app_key = "KtSNKxk3";
                            var nextpage_url="http://www.shmjy.changyan.cn/App.EduSNS/index.php?app=public&mod=Index&act=index";
                            nextpage_url = nextpage_url + ((nextpage_url.indexOf("?")>=0)?"&":"?") + "from=ew";
                            var original_nextpage_url = nextpage_url;
                            if(nextpage_index >= 0) {
                                nextpage_url = currentUrl.substr(nextpage_index+9);
                                nextpage_url = Base64.decode(nextpage_url);
                                nextpage_url = nextpage_url.replace(new RegExp(/(&amp;)/g),'&');
                                if (nextpage_url.indexOf("http://www.shmjy.changyan.cn/web/index.php?app=Home&mod=Index&act=login") < 0) {
                                    var start_app_key_index = nextpage_url.indexOf("appkey=");
                                    var end_app_key_index = nextpage_url.indexOf("&", start_app_key_index);
                                    if (end_app_key_index < 0) {
                                        end_app_key_index = nextpage_url.length;
                                    }
                                    if(start_app_key_index>=0){
                                        app_key = nextpage_url.substr(start_app_key_index+7, end_app_key_index-start_app_key_index-7);
                                    }
                                }
                                else {
                                    nextpage_url=original_nextpage_url;
                                }
                            }
                            var edusso = new EduSSO(nextpage_url, "http://desktop.shmjy.changyan.cn/sso/");
                            edusso.login(app_key,"", "", 'login_name', loginName, inputPass, function(result) {
                                if (result.result != "success") {
                                    if (result.code == "1002") {
                                        $("#err_msg").html("用户名或密码错误");
                                        $("#err_msg_area").show();
                                    } else {
                                        layer.alert(result.data);
                                        return;
                                    }
                                    loginOut();
                                } else {
                                    $.post("index.php?app=Home&mod=Homepage&act=recordLogin",{
                                        login_name : loginName,
                                        userId : userId
                                    },function(data){

                                    });
                                    nextpage_url = nextpage_url + ((nextpage_url.indexOf("?")>=0)?"&":"?") + "ticket=" + result.data.st;
                                    window.location.href = decodeURIComponent(nextpage_url);
                                }
                            });
                        }else if (userType == "003" || userType == "001") {
                            //学生登录,修改状态为已认证
                            if (userType == "003") {
                                //修改用户认证状态
                                $.post("index.php?app=Home&mod=Ajax&act=updateStatus",{
                                    userId : userId,
                                    login_name : loginName
                                },function(statusResult) {
                                    var statusResult = eval("("+statusResult+")");
                                    if (statusResult.Code == 0) {
                                        //修改成功后登录个人桌面
                                        var redirectUrl ='';
                                        var currentUrl = window.location.href;
                                        var nextpage_index = currentUrl.indexOf("nextpage=");
                                        var app_key = "KtSNKxk3";
                                        var nextpage_url="http://www.shmjy.changyan.cn/App.EduSNS/index.php?app=public&mod=Index&act=index";
                                        nextpage_url = nextpage_url + ((nextpage_url.indexOf("?")>=0)?"&":"?") + "from=ew";
                                        var original_nextpage_url = nextpage_url;
                                        if(nextpage_index >= 0) {
                                            nextpage_url = currentUrl.substr(nextpage_index+9);
                                            nextpage_url = Base64.decode(nextpage_url);
                                            nextpage_url = nextpage_url.replace(new RegExp(/(&amp;)/g),'&');
                                            if (nextpage_url.indexOf("http://www.shmjy.changyan.cn/web/index.php?app=Home&mod=Index&act=login") < 0) {
                                                var start_app_key_index = nextpage_url.indexOf("appkey=");
                                                var end_app_key_index = nextpage_url.indexOf("&", start_app_key_index);
                                                if (end_app_key_index < 0) {
                                                    end_app_key_index = nextpage_url.length;
                                                }
                                                if(start_app_key_index>=0){
                                                    app_key = nextpage_url.substr(start_app_key_index+7, end_app_key_index-start_app_key_index-7);
                                                }
                                            }
                                            else {
                                                nextpage_url=original_nextpage_url;
                                            }
                                        }

                                        var edusso = new EduSSO(nextpage_url, "http://desktop.shmjy.changyan.cn/sso/");
                                        edusso.login(app_key,"", "", 'login_name', loginName, inputPass, function(result) {
                                            if (result.result != "success") {
                                                if (result.code == "1002") {
                                                    $("#err_msg").html("用户名或密码错误");
                                                    $("#err_msg_area").show();
                                                } else {
                                                    layer.alert(result.data);
                                                    return;
                                                }
                                                loginOut();
                                            } else {
                                                $.post("index.php?app=Home&mod=Homepage&act=recordLogin",{
                                                    login_name : loginName,
                                                    userId : userId
                                                },function(data){
                                                });
                                                nextpage_url = nextpage_url + ((nextpage_url.indexOf("?")>=0)?"&":"?") + "ticket=" + result.data.st;
                                                window.location.href = decodeURIComponent(nextpage_url);
                                            }
                                        });
                                    }
                                });
                            }  else {
                                var redirectUrl ='';
                                var currentUrl = window.location.href;
                                var nextpage_index = currentUrl.indexOf("nextpage=");
                                var app_key = "KtSNKxk3";
                                var nextpage_url="http://www.shmjy.changyan.cn/App.EduSNS/index.php?app=public&mod=Index&act=index";
                                nextpage_url = nextpage_url + ((nextpage_url.indexOf("?")>=0)?"&":"?") + "from=ew";
                                var original_nextpage_url = nextpage_url;
                                if(nextpage_index >= 0) {
                                    nextpage_url = currentUrl.substr(nextpage_index+9);
                                    nextpage_url = Base64.decode(nextpage_url);
                                    nextpage_url = nextpage_url.replace(new RegExp(/(&amp;)/g),'&');
                                    if (nextpage_url.indexOf("http://www.shmjy.changyan.cn/web/index.php?app=Home&mod=Index&act=login") < 0) {
                                        var start_app_key_index = nextpage_url.indexOf("appkey=");
                                        var end_app_key_index = nextpage_url.indexOf("&", start_app_key_index);
                                        if (end_app_key_index < 0) {
                                            end_app_key_index = nextpage_url.length;
                                        }
                                        if(start_app_key_index>=0){
                                            app_key = nextpage_url.substr(start_app_key_index+7, end_app_key_index-start_app_key_index-7);
                                        }
                                    }
                                    else {
                                        nextpage_url=original_nextpage_url;
                                    }
                                }
                                var edusso = new EduSSO(nextpage_url, "http://desktop.shmjy.changyan.cn/sso/");
                                edusso.login(app_key,"", "", 'login_name', loginName, inputPass, function(result) {
                                    if (result.result != "success") {
                                        if (result.code == "1002") {
                                            $("#err_msg").html("用户名或密码错误");
                                            $("#err_msg_area").show();
                                        } else {
                                            layer.alert(result.data);
                                            return;
                                        }
                                        loginOut();
                                    } else {
                                        $.post("index.php?app=Home&mod=Homepage&act=recordLogin",{
                                            login_name : loginName,
                                            userId : userId
                                        },function(data){
                                        });
                                        nextpage_url = nextpage_url + ((nextpage_url.indexOf("?")>=0)?"&":"?") + "ticket=" + result.data.st;
                                        window.location.href = decodeURIComponent(nextpage_url);
                                    }
                                });
                            }
                        }
                        if(!!href && href != ""){
                            window.location.href = href;
                        }
                    }else {
                        //前台注册的账号（认证或不认证都可以登录）
                        var redirectUrl ='';
                        var currentUrl = window.location.href;
                        var nextpage_index = currentUrl.indexOf("nextpage=");
                        var app_key = "KtSNKxk3";
                        var nextpage_url="http://www.shmjy.changyan.cn/App.EduSNS/index.php?app=public&mod=Index&act=index";
                        nextpage_url = nextpage_url + ((nextpage_url.indexOf("?")>=0)?"&":"?") + "from=ew";
                        var original_nextpage_url = nextpage_url;
                        if(nextpage_index >= 0) {
                            nextpage_url = currentUrl.substr(nextpage_index+9);
                            nextpage_url = Base64.decode(nextpage_url);
                            nextpage_url = nextpage_url.replace(new RegExp(/(&amp;)/g),'&');
                            if (nextpage_url.indexOf("http://www.shmjy.changyan.cn/web/index.php?app=Home&mod=Index&act=login") < 0) {
                                var start_app_key_index = nextpage_url.indexOf("appkey=");
                                var end_app_key_index = nextpage_url.indexOf("&", start_app_key_index);
                                if (end_app_key_index < 0) {
                                    end_app_key_index = nextpage_url.length;
                                }
                                if(start_app_key_index>=0){
                                    app_key = nextpage_url.substr(start_app_key_index+7, end_app_key_index-start_app_key_index-7);
                                }
                            }
                            else {
                                nextpage_url=original_nextpage_url;
                            }
                        }
                        var edusso = new EduSSO(nextpage_url, "http://desktop.shmjy.changyan.cn/sso/");
                        edusso.login(app_key,"", "", 'login_name', loginName, inputPass, function(result) {
                            if (result.result != "success") {
                                if (result.code == "1002") {
                                    $("#err_msg").html("用户名或密码错误");
                                    $("#err_msg_area").show();
                                } else {
                                    layer.alert(result.data);
                                    return;
                                }
                                loginOut();
                            } else {
                                $.post("index.php?app=Home&mod=Homepage&act=recordLogin",{
                                    login_name : loginName,
                                    userId : userId
                                },function(data){
                                });
                                nextpage_url = nextpage_url + ((nextpage_url.indexOf("?")>=0)?"&":"?") + "ticket=" + result.data.st;
                                window.location.href = decodeURIComponent(nextpage_url);
                            }
                        });
                    }
                });
            });

        }, "json");
    }


    var Base64={_keyStr:"ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=",encode:function(e){var t="";var n,r,i,s,o,u,a;var f=0;e=Base64._utf8_encode(e);while(f<e.length){n=e.charCodeAt(f++);r=e.charCodeAt(f++);i=e.charCodeAt(f++);s=n>>2;o=(n&3)<<4|r>>4;u=(r&15)<<2|i>>6;a=i&63;if(isNaN(r)){u=a=64}else if(isNaN(i)){a=64}t=t+this._keyStr.charAt(s)+this._keyStr.charAt(o)+this._keyStr.charAt(u)+this._keyStr.charAt(a)}return t},decode:function(e){var t="";var n,r,i;var s,o,u,a;var f=0;e=e.replace(/[^A-Za-z0-9+/=]/g,"");while(f<e.length){s=this._keyStr.indexOf(e.charAt(f++));o=this._keyStr.indexOf(e.charAt(f++));u=this._keyStr.indexOf(e.charAt(f++));a=this._keyStr.indexOf(e.charAt(f++));n=s<<2|o>>4;r=(o&15)<<4|u>>2;i=(u&3)<<6|a;t=t+String.fromCharCode(n);if(u!=64){t=t+String.fromCharCode(r)}if(a!=64){t=t+String.fromCharCode(i)}}t=Base64._utf8_decode(t);return t},_utf8_encode:function(e){e=e.replace(/rn/g,"n");var t="";for(var n=0;n<e.length;n++){var r=e.charCodeAt(n);if(r<128){t+=String.fromCharCode(r)}else if(r>127&&r<2048){t+=String.fromCharCode(r>>6|192);t+=String.fromCharCode(r&63|128)}else{t+=String.fromCharCode(r>>12|224);t+=String.fromCharCode(r>>6&63|128);t+=String.fromCharCode(r&63|128)}}return t},_utf8_decode:function(e){var t="";var n=0;var r=c1=c2=0;while(n<e.length){r=e.charCodeAt(n);if(r<128){t+=String.fromCharCode(r);n++}else if(r>191&&r<224){c2=e.charCodeAt(n+1);t+=String.fromCharCode((r&31)<<6|c2&63);n+=2}else{c2=e.charCodeAt(n+1);c3=e.charCodeAt(n+2);t+=String.fromCharCode((r&15)<<12|(c2&63)<<6|c3&63);n+=3}}return t}}

</script>
