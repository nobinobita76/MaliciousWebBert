<script>
    $(function () {
        $('.banner').unslider({
            dots: true,
            fluid:true

        });
        $('.banner2').unslider({
            dots: true
        });
        $('.slider-box').unslider({
            dots: true,
            delay: false
        });
        if (cookie("pin") == null || cookie("pin") == "" || cookie("pin") == "undefined") {
           
        } else {
            $("#userPin").html(decodeURIComponent(getCookie("pin")));
        }

        // 公告滚动
        function initscroll(){
            var scrollPic_01 = new ScrollPic();
            scrollPic_01.scrollContId = "notice";//内容容器ID
            scrollPic_01.frameWidth   = 1020;//显示框宽度
            scrollPic_01.pageWidth    = 1020;//翻页宽度
            scrollPic_01.speed        = 15;//移动速度(单位毫秒，越小越快)
            scrollPic_01.space        = 30;//每次移动像素(单位px，越大越快)
            scrollPic_01.autoPlay     = true;//自动播放
            scrollPic_01.autoPlayTime = 2;//自动播放间隔时间(秒)
            scrollPic_01.initialize();//初始化
        }

    });
    

    //获得coolie的值
    function cookie(name) {
        var cookieArray = document.cookie.split("; "); //得到分割的cookie名值对
        var cookie = new Object();
        for (var i = 0; i < cookieArray.length; i++) {
            var arr = cookieArray[i].split("=");       //将名和值分开
            if (arr[0] == name)return unescape(arr[1]); //如果是指定的cookie，则返回它的值
        }
        return "";
    }
    //删除cookie
    function delCookie(name) {
        var date = new Date();
        date.setTime(date.getTime() - 10000);
        document.cookie = name + "=v;expire=" + date.toGMTString() + ";path=/;domain=.media.jd.com;";
    }
    //删除cookie
    function deleteCookie(name) {
        var date = new Date();
        date.setTime(date.getTime() - 10000);
        document.cookie = name + "=;expire=" + date.toGMTString() + ";path=/;domain=.jd.com";
    }
    
    function toEnter() {
        deleteCookie("pin");
        window.location.href = "/logout";
    }
    function getCookie(name) {
        var arr = document.cookie.match(new RegExp("(^| )" + name + "=([^;]*)(;|$)"));
        if (arr != null) return arr[2];
        return null;
    }


    $("#toLogin").bind("click", function () {
        window.location.href = "https://passport.jd.com/new/login.aspx?ReturnUrl=" + encodeURIComponent("https://" + window.location.host + "/index/overview");
    });
    $("#toRegister").bind("click", function () {
        window.location.href = "https://reg.jd.com/reg/person?ReturnUrl=" + encodeURIComponent("https://" + window.location.host);
    });
</script>

