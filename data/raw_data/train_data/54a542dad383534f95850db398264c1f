<script>
    var u = 'http://carprice.58.com';
    var p = $('#market_price_span').attr('param');
    $.ajax({
        url    : u + p,
        dataType: "jsonp",
        async  : false,
        jsonp  : "callback",
        success: function(data){
            if(data.code == '0'){
                $('.market_price_area').css('display', 'inline');
                $('#market_price_span').html(data.data.marketPrice[0] + '万 ~ ' + data.data.marketPrice[1] + '万');
            }
            else{
                $('.market_price_area').css('display', 'none');
            }
        }
    });

    $(".recom-veh-ul .pub-picture").hover(function(){
        var brand = $(this).find(".car-brand");
        $(this).find(".recom-car-info").stop().animate({"height":"80px"},"slow");
        brand.addClass("brand-no-wrap");
        setEllipsis(brand);
    },function(){
        $(this).find(".recom-car-info").stop().animate({"height":"15px"},"slow");
        $(this).find(".car-brand").removeClass("brand-no-wrap");
    });

    setEllipsis($(".pub-font a"));
    /*两行省略*/
    function setEllipsis(target){
        var val = target.attr("title");
        var textNum = 25;
        if(!$("body").hasClass("lg-screen")){
            textNum = 15;
        }
        if(val.length > textNum){
            val = val.substr(0,textNum)+"...";
        }
        target.html(val);
    }
</script>
</body></html>