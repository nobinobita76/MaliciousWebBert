            (function () {
                $("#menu-box").hover(function () {
                    $(".shop-menu").show();
                }, function () {
                    $(".shop-menu").hide();
                });

                var li = $(".shop-menu .firstLi");
                li.hover(function () {
                    li.find('.sec-menu').hide();
                    $(this).find('.sec-menu').show();
                }, function () {
                    $(this).find('.sec-menu').hide();
                });
            })();
