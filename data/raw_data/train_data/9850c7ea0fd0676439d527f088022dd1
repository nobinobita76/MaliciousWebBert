            $(function () {
                var _viewicons = $(".viewicons");
                var icons = _viewicons.find("a.ico");
                if (icons.length > 8) {
                    //icons.slice(8).hide();
                    _viewicons.append("<a class='expandicon' href='javascript:;'></a><div class='more-icons'></div>");
                    var _more = _viewicons.find(".more-icons");
                    icons.slice(8).appendTo(_more).show().attr({ "jstyle": "left", "offsetx": 10, "offsety": 14 });
                    var l = icons.slice(8).length;
                    var h = (l - 1) * 24 + 8;
                    _more.height(h < 20 ? 20 : h);
                    _viewicons.find("a.expandicon").click(function (event) {
                        if ($(this).hasClass("expandicon-selected")) {
                            $(document).click();
                            return;
                        }
                        $(this).addClass("expandicon-selected");
                        var _this = $(this);
                        _more.slideDown(100);
                        addEventsQueue("click", "expdocclick", function () {
                            _more.slideUp(100);
                            _this.removeClass("expandicon-selected");
                        });
                        event.stopPropagation();
                    });
                }
                //绑定事件
                _viewicons.find("a.ico").jtip({ width: 300, autohide: true, useanimate: false });


