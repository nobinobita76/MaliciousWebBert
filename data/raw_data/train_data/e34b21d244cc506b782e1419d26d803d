  <script>
      $(function(){
          $('.theme-poptit .close').click(function(){
            $('.theme-popover-mask').fadeOut(100);
            $('.theme-popover').slideUp(200);
          });
        $('#sidebarad').portamento({
        wrapper: $('#container'),
        yuanpx:1095
        }); 
         loginshow();
      });      
      function theme_click(){
         $('.theme-popover-mask').fadeIn(100);
         $('.theme-popover').slideDown(200);         
      }  

      function loginCheck(form)
      {
              var username = form.username;
              var password = form.password;
              if(username.value == ''){alert("请输入用户名");username.focus();return false;}
              if(password.value == ''){alert("请输入密码");password.focus();return false;}
              return true;
      }

      function loginSubmit(login)
      {        
              var username = login.elements['username'];
              var password = login.elements['password'];
              var cookietime = login.elements['cookietime'];
              var dosubmit = login.elements['dosubmit'];
          if(!loginCheck(login)) return false;
              $.post(login.action, 'username='+username.value+'&password='+password.value+'&cookietime='+cookietime.value+'&dosubmit='+dosubmit.value, function(data){
                      if(data.success == 1){
                              $('#mytest').html(data.msg);
                              loginshow();
                              $('.theme-popover-mask').fadeOut(100);
                              $('.theme-popover').slideUp(200);  
                              if(window.document.getElementById('comment_iframe')){window.document.getElementById('comment_iframe').contentWindow.location.reload();}                                                       
                              if (login_last==1) {aa();}
                              if (login_last==2) {window.location.href="/index.php?m=member&c=content&a=publish";}
                                           
  
                      }
                      else if(data==-1){ alert('用户不存在');username.value = '';}
                      else if(data==-2){ alert('密码错误');}
                      else if(data==-3){ alert('登录失败');}
                      else if(data==-4){ alert('错误次数过多，请稍后再试');}
                      else if(data==2){ alert('帐号已锁定');}
                      else if(data==-5){ alert('验证码错误');}//验证码
                      password.value = '';

              }, "json");
              return false;
      }

      function logout(url)
      {
              $.get(url, function(data){
                      if(data.success = !1){ alert('注销失败');
                      }else{
                              $('#mytest').html(data.msg);
                              loginshow();
                      }
              },"json");
              
      }

function loginshow() {
    var baba;
    $.get('/index.php?m=member&c=ajax&a=login', function(msg) {
        if (msg.success == 1) {
            $('#div_login').hide();
            $('#div_logined').show();
            $('#logined_username').html(msg.username);
            if (msg.nickname) {
                $('#logined_nickname').html('(' + msg.nickname + ')');
            }

            baba = true;
            daka();
        } else {
            $('#div_logined').hide();
            $('#div_login').show();

            baba = false;
        }
    }, "json");

    return baba;
}
function HidDetail(logid)
{
    //console.log("1111111");
    if($("#aa"+logid).html()=="展开全文")
    { 
     
      
      $("#pp"+logid).slideDown();
      $("#aa"+logid).removeClass("aaShow");
      $("#aa"+logid).addClass("aaHid");
      $("#aa"+logid).html("向上收起");

    }else{
     
      $("#aa"+logid).removeClass("aaHid");
      $("#aa"+logid).addClass("aaShow");
      $("#pp"+logid).slideUp();
      //$("#ppt"+logid).show();
      $("#aa"+logid).html("展开全文");
  }
}
      function aa(ys)
      {

      if (ys == "ok") {
        var ccurl='/index.php?m=member&c=index&a=public_sign&cc=1';
      }else{
        var ccurl='/index.php?m=member&c=index&a=public_sign';       
      };
      $.get(ccurl, function(msg){
                     if (msg.message == "-1") {theme_click();login_last=1;}
                     else if(msg.message == "-2"){ 
                        $(".h-sign-in-top1").text("今日已签到");
                        $(".h-sign-in-top1").css("background-color","#E26C09")
                        $(".h-sign-in-top1").next().text("今日签到获得"+msg.jifen+"个积分，连续签到"+msg.stime+"天！");

                     }
                     else if(msg.message == "1"){ 
                        alert(" 签到成功 \n 今日签到获得"+msg.jifen+"个积分\n 连续签到"+msg.stime+"天！");
                        $(".h-sign-in-top1").text("今日已签到");
                        $(".h-sign-in-top1").css("background-color","#E26C09")
                        $(".h-sign-in-top1").next().text("今日签到获得"+msg.jifen+"个积分，连续签到"+msg.stime+"天！");
                     }
              },"json");
      }
function tougao() {
  var isdisplay=$("#div_logined").css("display");
    console.log(isdisplay);
    if (isdisplay == 'block') {

        window.location.href = "/index.php?m=member&c=content&a=publish";
    } else {
        theme_click();
        login_last = 2;
    };
}
