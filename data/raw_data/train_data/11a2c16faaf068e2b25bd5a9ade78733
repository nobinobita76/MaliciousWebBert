<script src="js/common.js?20170206"></script>
<script src="js/bootstrap.min.js"></script>
<script type="text/javascript">
$(function() {
    FastClick.attach(document.body);
});
var btc_arr = [],
    cny_arr = [];


for(coinName_en in glb_cny_mk_arr){cny_arr.push(coinName_en);};
for(coinName_en in glb_btc_mk_arr){btc_arr.push(coinName_en);};

var trade = function(){
    var self = this;
    this.httpAPI();
    setInterval(function(){
        self.httpAPI();
    },60000)
    $('.sort').click(function(){
        var showCur = $(this).attr('data-show');
        if (showCur == "0") {
            $(this).attr('data-show','1');
            $(this).find('ul').show();
        }else{
            $(this).attr('data-show','0');
            $(this).find('ul').hide();
        }
    })
    $('.sort ul li').click(function(){
        $(this).addClass('active').siblings('li').removeClass('active');
        $('.sort h2').attr('data-sort',$(this).index());
        self.httpAPI();
    })
}
trade.prototype = {
    httpAPI:function(){
        var sortType = $('.sort h2').attr('data-sort');
        var _this = this;
        $.get("./httpAPI.php?n="+Math.random(),function(res){
            res = eval("("+res+")");
            var cnyPriceTotalArr = [];
            var cnyPriceGrow = [];
            var cnyPriceDefault = [];

            var btcPriceTotalArr = [];
            var btcPriceGrow = [];
            var btcPriceDefault = [];

            for(coinnname_en in glb_cny_mk_arr){
                var cnyPriceArr = [];
                cnyPriceArr.push(coinnname_en);
                cnyPriceArr.push(res[coinnname_en+'2cny_vol']);
                cnyPriceTotalArr.push(cnyPriceArr);//总交易量;

                var buyorsell = [];
                buyorsell.push(coinnname_en);
                buyorsell.push((res[coinnname_en+'2cny']-res[coinnname_en+'2cny_24h'])/res[coinnname_en+'2cny']);
                cnyPriceGrow.push(buyorsell);

                var pretermission = [];
                pretermission.push(coinnname_en);
                pretermission.push(0);
                cnyPriceDefault.push(pretermission);
            }

            for(coinnname_en in glb_btc_mk_arr){
                var btcPriceArr = [];
                btcPriceArr.push(coinnname_en);
                btcPriceArr.push(res[coinnname_en+'2btc_vol']);
                btcPriceTotalArr.push(btcPriceArr);//总交易量;

                var buyorsell = [];
                buyorsell.push(coinnname_en);
                buyorsell.push((res[coinnname_en+'2btc']-res[coinnname_en+'2btc_24h'])/res[coinnname_en+'2btc']);
                btcPriceGrow.push(buyorsell);

                var pretermission = [];
                pretermission.push(coinnname_en);
                pretermission.push(0);
                btcPriceDefault.push(pretermission);
            }

            var cnyPriceTotalArrSort = cnyPriceTotalArr.sort(function(x,y){return y[1]-x[1];});  //成交量排序 1  默认 0
            var cnyPriceGrowup = cnyPriceGrow.sort(function(x,y){return y[1]-x[1];});            //涨跌幅排序 2

            var btcPriceTotalArrSort = btcPriceTotalArr.sort(function(x,y){return y[1]-x[1];});  //成交量排序 1  默认 0
            var btcPriceGrowup = btcPriceGrow.sort(function(x,y){return y[1]-x[1];});            //涨跌幅排序 2
            if (sortType == 1) {
                btc_arr.length = 0;
                cny_arr.length = 0;
                for (var i = 0; i < cnyPriceTotalArrSort.length; i++) {
                    cny_arr.push(cnyPriceTotalArrSort[i][0])
                };
                for (var i = 0; i < btcPriceTotalArrSort.length; i++) {
                    btc_arr.push(btcPriceTotalArrSort[i][0])
                };
            }else if (sortType == 2) {
                btc_arr.length = 0;
                cny_arr.length = 0;
                for (var i = 0; i < cnyPriceGrowup.length; i++) {
                    cny_arr.push(cnyPriceGrowup[i][0])
                };
                for (var i = 0; i < btcPriceGrowup.length; i++) {
                    btc_arr.push(btcPriceGrowup[i][0])
                };
            }else if (sortType == 0) {
                btc_arr.length = 0;
                cny_arr.length = 0;
                for(coinName_en in glb_cny_mk_arr){cny_arr.push(coinName_en);};
                for(coinName_en in glb_btc_mk_arr){btc_arr.push(coinName_en);};
            };
            _this.createTradeList(res);
        })
    },
    createTradeList:function(data){
        var $cny_container = $('#cny-trade-list tbody'),
            $btc_container = $('#btc-trade-list tbody');

        $cny_container.find('tr').remove();
        $btc_container.find('tr').remove();

        var myTmpl_cny = Handlebars.compile($("#cny_trade_list_tmpl").html()),
            myTmpl_btc = Handlebars.compile($("#btc_trade_list_tmpl").html());

        Handlebars.registerHelper("logoCny",function(_index){
            return cny_arr[_index].toUpperCase();
        })
        Handlebars.registerHelper("star",function(_index){
            var starName = cny_arr[_index];
            //console.log(starName);
            //if (starName == "anc" || starName == "bec" || starName == "dgc") return "* ";
        })
        Handlebars.registerHelper("logoBtc",function(_index){
            return btc_arr[_index].toUpperCase();
        })

        Handlebars.registerHelper("cnyNameZh",function(_index){
            return glb_cny_mk_arr[cny_arr[_index]];
        })
        Handlebars.registerHelper("btcNameZh",function(_index){
            return glb_btc_mk_arr[btc_arr[_index]];
        })

        Handlebars.registerHelper("cnyPrice",function(_index){
            var coinname_en = cny_arr[_index];
            var price = data[coinname_en+"2cny"];
            return price = parseFloat(price).toFixed(getFixedNum('cny', coinname_en))
        })
        Handlebars.registerHelper("btcPrice",function(_index){
            var coinname_en = btc_arr[_index];
            var price = data[coinname_en+"2btc"];
            return price = parseFloat(price).toFixed(getFixedNum('btc', coinname_en));
        })

        Handlebars.registerHelper("btcVolume",function(_index){
            var coinname_en = btc_arr[_index];
            var price = data[coinname_en+"2btc_vol"];
            return price;
        })
        Handlebars.registerHelper("cnyVolume",function(_index){
            var coinname_en = cny_arr[_index];
            var price = data[coinname_en+"2cny_vol"];
            return price;
        })

        Handlebars.registerHelper("cnyRange",function(_index,color){
            var coinname_en = cny_arr[_index],
                price = data[coinname_en+"2cny"],
                price_24h = data[coinname_en+"2cny_24h"],
                range;

            if (parseFloat(price_24h) == 0)
                range = 0;
            else
                range = (parseFloat(price) / parseFloat(price_24h)) - 1.0;

            if (color == 0) {
                if (range >= 0)
                    return "green";
                else
                    return "red";
            }else{
                range = range >= 0 ? "+" + (range * 100).toFixed(1).toString() + "%" : (range * 100).toFixed(1).toString() + "%";
                return range;
            }

        })

        Handlebars.registerHelper("btcRange",function(_index,color){
            var coinname_en = btc_arr[_index],
                price = data[coinname_en+"2btc"],
                price_24h = data[coinname_en+"2btc_24h"],
                range;

            if (parseFloat(price_24h) == 0)
                range = 0;
            else
                range = (parseFloat(price) / parseFloat(price_24h)) - 1.0;

            if (color == 0) {
                if (range >= 0)
                    return "green";
                else
                    return "red";
            }else{
                range = range >= 0 ? "+" + (range * 100).toFixed(1).toString() + "%" : (range * 100).toFixed(1).toString() + "%";
                return range;
            }
        })

        $cny_container.append(myTmpl_cny(cny_arr));
        $btc_container.append(myTmpl_btc(btc_arr));

        $cny_container.find('tr').click(function(){
            var coin = $(this).attr('data-coinname').toLowerCase();
            window.location.href = "./trade_detailed.html?mk_type=cny&btc38_trade_coin_name="+coin;
        })

        $btc_container.find('tr').click(function(){
            var coin = $(this).attr('data-coinname').toLowerCase();
            window.location.href = "./trade_detailed.html?mk_type=btc&btc38_trade_coin_name="+coin;
        })
    }
}
new trade;
</script>




</body></html>