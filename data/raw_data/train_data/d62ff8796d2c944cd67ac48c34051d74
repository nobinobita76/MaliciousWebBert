<SCRIPT>
function CheckForm(){
  var Msg = "";
  if (document.weblogin.LoginName.value == ""){
	alert("请输入用户名！");
	document.weblogin.LoginName.focus();
	return false;
    Msg = "Err";}
  if (document.weblogin.LoginPasswd.value == ""){
	alert("请输入密码！");
	if (weblogin.LoginSafe.value == "1"){
	   document.weblogin.EKEDIT.focus();
	}else{
	   document.weblogin.LoginPasswd.focus();
	}
	return false;
    Msg = "Err";}  
  if (document.weblogin.RndCode.value == ""){
	alert("请输入验证码！");
	document.weblogin.RndCode.focus();
	return false;
    Msg = "Err";}
  if (Msg != "Err"){
    weblogin.act.value = "Login";
    if (weblogin.LoginSafe.value == "1"){
       weblogin.action="WebLogin.asp";
    }else{
       weblogin.action="WapLogin.asp";
    }
    weblogin.submit();}
}
function keySubmit()
{
if (event.keyCode == 13) CheckForm();
}
function ChangeSafe(IsSafe)
{
if (IsSafe=="0"){
   weblogin.LoginSafe.value = "0";
   document.getElementById("button1").className = "qiea";
   document.getElementById("button2").className = "";
   document.getElementById("button0").className = "qie";
   }
if (IsSafe=="1"){
   weblogin.LoginSafe.value = "0";
   document.getElementById("button1").className = "";
   document.getElementById("button2").className = "qiea";
   document.getElementById("button0").className = "qie next";   
   }
}
</SCRIPT>

