    <script>
        $(function(){
            countDown('2017/04/04 23:59:59','#colockbox1');
        })
        function countDown(time,id){
            var day=$(id).find('.day');
            var hour=$(id).find('.hour');
            var minute=$(id).find('.minute');
            var second=$(id).find('.second');
            var end_time = new Date(time);
            tick();
            setInterval(function(){
                tick()
            },1000)

            function tick(){
                var nowTime=new Date();
                var s=parseInt((end_time.getTime()-nowTime.getTime())/1000);
              
                var date=parseInt(s/86400);
                if(date<10){
                    date='0'+date
                }
                 s%=86400;
                 var h=parseInt(s/3600);
                 if(h<10){
                    h='0'+h
                }
                 s%=3600;
                 var min=parseInt(s/60);
                 if(min<10){
                    min='0'+min
                }
                 // var sec=s%60;
                 day.text(date); 
                 hour.text(h); 
                 minute.text(min); 
                 // second.text(sec); 
             }
        }
    </script>
    <script>
      $(function () {
            showScroll();
            function showScroll() {
                $(window).scroll(function () {
                  var scrollValue = $(window).scrollTop();
                  scrollValue > 80 ? $('.toTop').fadeIn() : $('.toTop').fadeOut();
                });
                $('.toTop').click(function () {
                  $("html,body").animate({ scrollTop: 0 }, 200);
                });
            }
      });
      $(window).ready(function () {
            $(window).scroll(function () {
                var this_scrollTop = $(this).scrollTop();
                var _top=$('#leftTabBox').offset().top-40;
                if (this_scrollTop >= _top) {
                    $('#leftTabBox .hd_parent').addClass('fixed');
                }
                else {
                    $('#leftTabBox .hd_parent').removeClass('fixed');
                }
            })

        });
      $(function(){
         var _width=parseInt($(window).width());
         var _toTop=-parseInt(($(window).width()*1.5-_width)/2);
         $('.exchange_detail_product').css({height:_width});
         $('.exchange_detail_product .top_bottom').css({'margin-top':_toTop});
      })
      $(function(){
          var _width=$(window).width();
          var _widthAfter=(_width-115)/2;
          $('.line_through2').css({ "width":_widthAfter + "px" });
        })
      $(function(){
            var tab_box_content=$('.tab-box-content');
            var ul_width=$(window).width()
            var lis=tab_box_content.find('.n');
            var li_width_2=(ul_width-14)/2;
            var li_width_3=(ul_width-18)/3;
            if($(window).width()<350){
                lis.width(li_width_2);
            }else{
                lis.width(li_width_3);
            }
        })

       $(function(){
          var invit_friends=$('.invit_friends');
          invit_friends.on('click',function(){
             $('.invit_friends_alert').removeClass('hide');
          })
          $('.invit_friends_alert .close').on('click',function(){
             $('.invit_friends_alert').addClass('hide');
          })

      })
    
    </script>
    <script>
      $(function(){
          var bd_child=$('.bd_child');
          var hd_parent=$('.hd_parent');
          var top=hd_parent.offset().top
          var lis=hd_parent.find('li');
          lis.click(function(){
              $('body').animate({ scrollTop: top }, 200);
                 
          })
      })

      $(function (){
        var _width = $(window).width();

        var _w;

        if($(window).width()<350){
            _w = (_width-14)/2;
        }else{
            _w = (_width-18)/3;
        }

        $.ajax({
            type: 'POST',
            url: '/detail/get_shop_others',
            data: {task_sn:"f331316184549526",width:_w},
            dataType: 'json',
            success: function(data){
                $('#others').html(data['d']);
            }
        });
      });
    </script>
