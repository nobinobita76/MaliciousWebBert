<script type="text/javascript">
    ;$(function(){
        var $pvid_link = $("a[data-pvid]");
        if($pvid_link.length > 0){
            $pvid_link.on('mousedown', function(event){
                var link = $(this).attr('href');
                var pvid = $(this).data('pvid');
                if(link.indexOf('pvid=') > 0){
                    return true;
                }
                if(!pvid){
                    return true;
                }
                if(link.indexOf('?') > 0){
                    link += '&pvid=' + pvid;
                }else{
                    link += '?pvid=' + pvid;
                }
                $(this).attr('href', link);
            });
        }
    });
</script>



