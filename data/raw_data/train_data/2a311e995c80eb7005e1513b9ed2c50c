<script>
    //选择分类
    function choseArtClass(){
        if(!isUserlogin()){ //判断是否登陆
            return ;
        }
    window.location.href='http://blog.cnfol.com/index.php/concern?type=1'; return;
        var choseDia=$("#choseArtClass");
        var addDia=$("#addArtClass");
        var close=choseDia.find(".BtnCos").eq(0);
        if(addDia.css("display")=="block"){
            addDia.css("display","none");
        }
        choseDia.css("display","block");
        $("#WBg").css("display","block");
        close.click(function(){
            $("#WBg").css("display","none");
            choseDia.css("display","none");
        });
    }
    //创建分类
    function newArtClass(){
        if(!isUserlogin()){ //判断是否登陆
            return ;
        }
    window.location.href='http://blog.cnfol.com/index.php/concern?type=1'; return;
        var choseDia=$("#choseArtClass");
        var addDia=$("#addArtClass");
        var close=addDia.find(".BtnCos").eq(0);
        if(choseDia.css("display")=="block"){
            choseDia.css("display","none");
        }
        addDia.css("display","block");
        $("#WBg").css("display","block");
        close.click(function(){
            $("#WBg").css("display","none");
            addDia.css("display","none");
        });
    }
    //保存转载信息
    function SaveSort(type){

        if(type == 1){ //已选文章分类转载
            sortid = $('#nowSortid').val();
            $("#choseArtClass").css("display","none");
            transshipmentsort(2,'1319597979','70093531','788173','788173','transshipmentnum','0',sortid);
            return true;
        }
        if(type == 2){ //新增文章分类转载
            //新增分类
            $("#addArtClass").css("display","none");
            addSortNameSort('addSortName','addName',"",'70093531','1319597979','788173','788173','0');
            return true;
        }

    }
</script>

<script type="text/javascript">
    function TDCode(){
        var T = 0,btn = $('#TDCode'),
                win = $('#ImgWin'),
                cbtn = $(win).find('.ImgClose');
        $(btn).hover(
                function(){
                    clearTimeout(T);
                    var l = $(this).offset().left - 20,
                            t = $(this).offset().top + $(this).outerHeight() +5;
                    $(win).css({"left":l+"px","top":t+"px"});
                },
                function(){
                    T = setTimeout(function(){$(win).css({"left":"-1000px","top":"-1000px"})},1000);
                }
        )
        $(win).hover(
                function(){
                    clearTimeout(T);
                },
                function(){
                    clearTimeout(T);
                    $(win).css({"left":"-1000px","top":"-1000px"});
                    //T = setTimeout(function(){$(win).css({"left":"-1000px","top":"-1000px"})},1000);
                }
        );
        $(cbtn).click(function(){
            clearTimeout(T);
            $(win).css({"left":"-1000px","top":"-1000px"})
        });
    }
    TDCode();
</script>

<script language="javascript">
    var memberid = 788173;
    var flashCode = "d5b36f685a29a8d7";
    var baseuri = "http://blog.cnfol.com/qvrcetis";
    var action = baseuri+'/article/Action';
    var editurl = baseuri+'/article/Add';
	    $(function(){
        $('#ajaxdiv').load("http://blog.cnfol.com/articleclick/art/70093531?"+new Date().getTime(),function(){
        });
    });
	    $.fn.initRwin = function(){
        $(this).each(function(){
            var w = $(this),
                    c = $(w).find('.Close'),
                    udo = $(w).find('.udo');
            $(c).click(function(){
                $(this).parent().parent().css({"display":"none"});
            });
            $(udo).click(function(){
                $(this).parent().parent().parent().css({"display":"none"});
            });
            where(w);
            if($.browser.msie){
                if($.browser.version == '6.0'){
                    var t = parseInt($(w).css('top')),
                            it = t + $(document).scrollTop();
                    $(w).css({'top':it+'px'});
                    $(window).scroll(function(){
                        var _t = $(document).scrollTop(),
                                top = t + _t;
                        $(w).css({'top':top+'px'});
                    });
                }
            }
            $(window).resize(function(){
                where(w);
            });
            function where(o){
                var w = document.documentElement.clientWidth - $(o).outerWidth(true),
                        h = document.documentElement.clientHeight - $(o).outerHeight(true),
                        l = w/2,
                        t = h/2;
                $(o).css({"left":l+"px","top":t+"px"});
            }
        });
    }



    function newReport(o)
    {

        $('#NRwin').css({"display":"block"});//登录后举报弹窗显示

    }

    function reportSubmit(ArticleID,MemberID,loginuserid)
    {

        var reasonText=$('#reasonText').val();
        var nameText=$('#nameText').val();
        var telText=$('#telText').val();


        reasonText=reasonText.replace(/^\s+|\s+$/g,'');
        nameText=nameText.replace(/^\s+|\s+$/g,'');
        telText=telText.replace(/^\s+|\s+$/g,'');
        if(reasonText==''||nameText==''||telText=='')
        {
            alert('* 为必填项');
            return;
        }
        var mat=telText.match(/^(\d+(\-)?)+$/);
        if(mat==null)
        {
            alert('联系电话有误');
            return;
        }

        if(reasonText.length>20)
        {
            alert('举报理由不能大于20个字节');
            return;
        }

        articlereport('2',ArticleID,MemberID,loginuserid,reasonText,nameText,telText,'1');
    }

    $(function(){
        $('#NRwin').initRwin();
    })

</script>
