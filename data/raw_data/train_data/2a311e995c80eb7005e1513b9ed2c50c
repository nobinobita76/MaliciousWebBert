   <script type="text/javascript">
    var InpTextArea=document.getElementById("InpWord"),
	    InpNum=document.getElementById("textnum"),
		len=100-InpTextArea.value.length;
		InpNum.innerHTML=len;
    InpTextArea.onkeyup=function()
	{
        len =100-InpTextArea.value.length;
		if(len<0)
		{
			InpTextArea.value=InpTextArea.value.substring(0,100);
			len=0;
		}
		InpNum.innerHTML=len;
	};

	var sendFlowerNum	= $('#flowerNum2');	// 用户想要赠送的鲜花数
	var gold 			= $('#goldNum');	// 用户需要支付的金币数
	var money 			= $('#payMoney');	// 用户账户内的人民币余额
	var needMoney 		= $("#how_money2");	// 用户还需要支付的人民币金额
	var baseUrl="http://blog.cnfol.com";
	// 赠送鲜花
	function sendFlower2()
	{
		var fromuserid="0";
		var touserid=$('#ToUserID').val();
		var flowerNum=sendFlowerNum.val();

		var goldNum = parseInt(gold.html()); // 用户金币余额
		var rmbNum = 0;
		
		var sourceUrl=$('#SourceUrl').val();
		var sourcetabid=$('#SourceTabID').val();
		var note=$('#InpWord').val();
		if(fromuserid==false)
		{
			MyDialog.Close('FlowerDialog');
			alert('请先登录');
			return;
		}
		
		if(flowerNum==false)
		{
			MyDialog.Close('FlowerDialog');
			alert('鲜花数不能为空');
			return;
		}
		var regnum = /[0-9]+/;
		if(!regnum.test(flowerNum))
		{
			MyDialog.Close('FlowerDialog');
			alert('鲜花数请输入数字!');
			return;
		}
		//2017-03-06 同意协议
		if($('#isAgreeSend2').is(':checked') ==false){
			alert('未勾选已阅读《增值服务协议》');
			return ;
		}
		// 用户账户余额不足够
		if(needMoney.parent().css('display') == 'block') {

			var buytype = ($('#payMoneyChk').is(':checked')? '1': '0');

			var str = 'fromuserid='+fromuserid+'&touserid='+touserid+'&flowerNum='+flowerNum+'&buytype='+buytype+'&note='+note+'&sourceUrl='+sourceUrl+'&sourcetabid='+sourcetabid+'&callback='+"http://blog.cnfol.com/qvrcetis/article/70093531.html";
			location.href =  "http://blog.cnfol.com/index.php/myfocus/sendFlowerMoney?"+str;
		}
		// 用户账户余额足够
		else {

			// 用户金币余额小于赠送的鲜花数，用人民币支付差额
			if(flowerNum > goldNum) rmbNum = (flowerNum - goldNum) / 10;

			$.ajax({
				url:baseUrl + "/index.php/myfocus/sendflower",
				type:"post",
				dataType:"json",
				data:{fromuserid:fromuserid,touserid:touserid,flowerNum:flowerNum,rmbNum:rmbNum,note:note,sourceUrl:sourceUrl,sourcetabid:sourcetabid},
				success:function(result)
				{
					MyDialog.Close('FlowerDialog');
					alert(result.info);
					location.reload();

				}
			});
		}


		// 旧版支付
		/*if(flowerNum >0){ //2016-01-06 金币不够直接跳转充值送鲜花
			var str = 'fromuserid='+fromuserid+'&touserid='+touserid+'&flowerNum='+flowerNum+'&sourceUrl='+sourceUrl+'&sourcetabid='+sourcetabid+'&callback='+"http://blog.cnfol.com/qvrcetis/article/70093531.html";
			window.open( "http://blog.cnfol.com/index.php/myfocus/sendFlowerMoney?"+str);
		}else{
			$.ajax({
				url:baseUrl + "/index.php/myfocus/sendflower",
				type:"post",
				dataType:"json",
				data:{fromuserid:fromuserid,touserid:touserid,flowerNum:flowerNum,note:note,sourceUrl:sourceUrl,sourcetabid:sourcetabid},
				success:function(result)
				{
					MyDialog.Close('FlowerDialog');
					alert(result.info);
					location.reload();
					
				}
			});
		}*/

	}

	// 计算金额
	function FlowerKeyUp(obj){//2016-05-24 鲜花数 金币 余额 还需支付 计算

		var sendNum = sendFlowerNum.val(),
				prg = /^[0-9]*$/,
				goldNum = parseInt(gold.html()),
				payMoney = parseFloat(money.html());

		payMoney = parseInt(payMoney * 100); // 人民币余额放大100倍
		var Rmb = (sendNum - goldNum) * 10; // 金币差额放大10倍，相当于人民币差额放大100倍

		if(prg.test(sendNum)){
			if(sendNum > goldNum){//金币不足
				var chk = money.parent().find('input:checked');
				if($(chk).length > 0){//勾选余额
					if(Rmb > payMoney){//余额不足
						Rmb = (Rmb - payMoney) / 100; // 解决浮点数相减尾数不精确的问题
						needMoney.html(Rmb).parent().css('display','block');
					}else{//余额足够
						needMoney.parent().css('display','none');
					}
				}else{//未勾选余额
					needMoney.html((sendNum - goldNum) / 10).parent().css('display','block');
				}
				money.parent().removeClass('BeHide');
			}else{//金币足够
				needMoney.parent().css('display','none');
				money.parent().addClass('BeHide');
			}
		}else{
			alert('请输入数字');
			sendFlowerNum.val(0);
			money.parent().css('display','none');
			needMoney.parent().css('display','none');
		}
	}

	// 选择使用人民币支付
	function payMoneyChk(obj){
		var sendNum = sendFlowerNum.val(),
				goldNum = parseInt(gold.html()),
				payMoney = parseFloat(money.html());

		payMoney = parseInt(payMoney * 100); // 人民币余额放大100倍
		var Rmb = (sendNum - goldNum) * 10; // 金币差额放大10倍，相当于人民币差额放大100倍

		if(Rmb > 0){
			if($(obj).is(':checked')){
				if(Rmb > payMoney){ // 还需要的人民币大于人民币余额
					Rmb = (Rmb - payMoney) / 100; // 解决浮点数相减尾数不精确的问题
					needMoney.html(Rmb).parent().css('display','block');
				}else{
					needMoney.parent().css('display','none');
				}
			}else{
				needMoney.html((sendNum - goldNum) / 10).parent().css('display','block');
			}
		}else{
			needMoney.parent().css('display','none');
		}
	}

	// 是否成功加载余额
	var hasLoaded = false;

	// 获取用户账户余额
	function getUserMoney() {

		// 如果成功加载，拒绝请求
		if(hasLoaded) {

			// 触发计算金额事件
			FlowerKeyUp(sendFlowerNum);
			return;
		}

		// 请求余额接口
		var url = baseUrl+'/index.php/blogapi/getUserMoney?' + new Date().getTime();

		$.getJSON(url, function(data) {

			if(data.errCode == '00') {

				gold.html(data.golden);
				money.html(data.money);
				hasLoaded = true;
				// 触发选择使用人民币支付事件
				payMoneyChk($('#payMoneyChk'));
				FlowerKeyUp(null);
			}
			else {

				alert('查询用户账户失败');
			}
		});
	}
	
</script>
    </div>
</div>
<script type="text/javascript">
    function MyDialog(id){
        var dialog=$("#"+id);
        $("#WBg").css("display","block");
        $("#WBg").css("height",document.body.offsetHeight);
        $(dialog).css("display","block");
        MyDialog.Close=function(){
            $(dialog).css("display","none");
            $("#WBg").css("display","none");
        }
    }
    //送鲜花2(文章鲜鲜花)
    function SendFlower2(ToUserID,GiftID,SourceUrl,SourceTabID,FlowerNum,FlowerStatus){
        var UserID="0";
        if(UserID==false)
        {
            alert('请先登录,谢谢');
            return false;
        }
        $('#flowerNum2').attr('value',FlowerNum);
        $('#flowerNum2').attr('text',FlowerNum);
        $('#flowerNum2').attr('disabled',FlowerStatus);
        $('#SourceUrl').attr('value',SourceUrl);
        $('#SourceTabID').attr('value',SourceTabID);
        $('#ToUserID').attr('value',ToUserID);

        MyDialog("FlowerDialog2");
        getUserMoney();

		// get_rmb();

    }

    //兼容旧版直播文章 2015-08-27
    if($('div').hasClass('BlogLive_op')){
        $('.BlogLive_op').css('display','none');
    }

</script>
