<script>
    $(".BloggerFaceBox").load("http://blog.cnfol.com/index.php/myfocus/domainInfo/qvrcetis");
    function delAttention(userid,act)
    {
        var url = baseurl+"/index.php/myfocus/action?act="+act;
        var params = {
            "friendIDs":userid,
            "myID" : $.cookie('cookie\\[passport\\]\\[userId\\]') // 2015-11-26 防止请求伪造，get请求参数添加
        };
	
        $.getJSON(url+"&r="+new Date().getTime(), params, function(json){
	    
            if(json.erron=='01')
            {
                if(act == "del")
                {
                    var focusNum = parseInt($(".fansNum").first().text())-1;
                    $(".fansNum").each(function(){
                        $(this).text(focusNum);		
                    })			
                    $(".focus").html("<a class=\"Plus\"  href=\"javascript:delAttention('6012441','add');\">加关注</a>");
		
                }
                else
                {
                    var focusNum = parseInt($(".fansNum").first().text())+1;
                    $(".fansNum").each(function(){
                        $(this).text(focusNum);		
                    })						
                    $(".focus").html("<a class=\"HasPlus\"  href=\"javascript:delAttention('6012441','del');\">取消关注</a>");
				
                }
            }
            else
            {
                alert(json.error);
            }
		
        });
    }
</script>        <!--end 单独的博客信息-->
