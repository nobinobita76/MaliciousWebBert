  <script type="text/javascript">
$(function(){
	$('.more_game').click(function(){
		if($(this).hasClass('wating'))
			return false;
		$(this).addClass('wating');
		var page = $(this).attr('page'),
			_this = this;
		if(page <= 0){
			return false;
		}
		$.post('/m/singlemobile/ajaxgetartlist',{'cat_id':'','game_id':50844,'page':page,'pageSize':5,'ajax_view':'zq_ajax_detail'},function(data){
			$(_this).removeClass('wating');
			if(data.html){
				$('#ajax_list').append(data.html);
			}
			if(data.page!="end"){
				$(_this).attr('page',Number(page) + 1);
			}else{
				$(_this).attr('page',-1);
				$(_this).html("暂无更多信息");
			}
		},"json");
	});
});
</script> 
