function ad_count(id,obj){
	var url=$(obj).attr('data_href');
	$.ajax({
		type:"POST",
		async:false,
		url:'/admin/adcount/create',
		data:'id='+id,
		success:function(msg){			
			window.location.href=url; 
		}
	});
}
function pos_count(posid,obj){
	var url=$(obj).attr('data_href');
	$.ajax({
		type:"POST",
		async:false,
		url:'/api/positioncount',
		data:'posid='+posid,
		success:function(msg){
			window.location.href=url; 
		}
	
	});
}
</script> 
