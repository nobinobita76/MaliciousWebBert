            $(function(){
                var isIndex = true,
                    isMiniClose = false,
                    t = 0;

                $('#J_guide_mini .btn_close').on('click',function(e){
                    e.preventDefault();
                    $('#J_guide_mini').remove();
                });

                $(window).on('scroll',function(){
                    var scrollTop = $(document).scrollTop(),
                    
                    H1 = $('header').height(),
                    H2 = $('nav').height(),
                    H3 = H1 * 1 + H2 * 1;

                    if(scrollTop > H3 && !isMiniClose && !isIndex){
                        $('#J_guide_mini').addClass('actived');
                    }else{
                        isMiniClose = false
                        $('#J_guide_mini').removeClass('actived');
                    }

                    p = $(this).scrollTop();  
                    if(t <= p){
                        //下滚  
                        $('#J_guide_mini').addClass('actived');
                        //$('#J_guide').removeClass('actived');
                    }else{
                        //上滚  
                        $('#J_guide_mini').removeClass('actived');
                    }  
                setTimeout(function(){
t = p;
},0);     
                });
            })
        </script>
