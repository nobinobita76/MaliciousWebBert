$(function(){
	$(".notice_news").find('li').eq(0).click(function(){
		$(".notice_news").find('li').each(function(){
			$(this).removeClass("on");
		});
		$(this).addClass("on");
		$("#activityList").hide();
		$("#noticeList").show();
	});
	$(".notice_news").find('li').eq(1).click(function(){
		$(".notice_news").find('li').each(function(){
			$(this).removeClass("on");
		});
		$(this).addClass("on");
		$("#noticeList").hide();
		$("#activityList").show();
	});
