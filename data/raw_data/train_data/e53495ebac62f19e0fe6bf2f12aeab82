		$("#hotSearchBox_product").empty();
		var product = '';
		// 搜索框--商品
	   	$.each(hotsearch.mall,function(i,value){
   		if (value != null && value.def == "1") { // 搜索框默认
   			$("#searchInputBox").val(value.word);
   		}   		
	   	// 底线条	
	   	if (value != null && value.url != null && value.url != 'null') {
   			product += '<a href="'+value.url+'" class="school-item ">'+value.word+'</a>';	   		
	   		$("#hotSearchBox_product").html(product);   		
	   	}
	   	});
	   	
		$("#hotSearchBox_service").empty();
		var service = '';
		// 搜索框--商品
	   	$.each(hotsearch.service,function(i,value){
   		if (value != null && value.def == 1) { // 搜索框默认
   			$("#searchInputBox").attr("value",value.word);
   		}   		
	   	// 底线条	
	   	if (value != null && value.url != null && value.url != 'null') {
	   		service += '<a href=//iservice.10010.com/e4/search/searchGoods.html?keyword='+value.word+' class="school-item ">'+value.word+'</a>';	   		
	   		$("#hotSearchBox_service").html(service);   		
	   	}
	   	});
		//搜索商品
		$("#searchBtnBox").click(function(){
			var val=$("#searchInputBox").val();
			if (val == "") {
			 val = 	$("#searchInputBox").attr("placeholder");
			}
			if($("#searchType .li-one").text()=="商品"){
				window.location.href = encodeURI("//s.10010.com/SearchApp/chseSearchList/init?keyword="+val+ "&wd=" +val);
			}else if($("#searchType .li-one").text()=="服务"){
				window.location.href = encodeURI("//iservice.10010.com/e4/search/searchGoods.html?keyword="+ val);
			}
			return false;
		});
		$("#searchInputBox").keypress(function(event){			
			 if(event.keyCode==13){
				 event.preventDefault();
				 $("#searchBtnBox").click();
			}
			
		});
		var userLoginCookie = {};
	    var userLoginInfo = $.evalJSON($.cookie("piw"))||{};
	    if ($.cookie('JUT') != null && userLoginInfo != null && !isEmpty(userLoginInfo.nickName)) { // 已登录用户
		    userLoginCookie.NICK_NAME = (isEmpty(userLoginInfo.nickName) ? "" : userLoginInfo.nickName);
		    userLoginCookie.VERIFY_STATE = (isEmpty(userLoginInfo.verifyState) ? "" : userLoginInfo.verifyState);
		    var urlhtml = currentTime() +','+userLoginCookie.NICK_NAME+'<a onclick="logout()" style="cursor:pointer">[退出]</a>&nbsp;&nbsp;<a target="_blank"'
                +'href="https://uac.10010.com/cust/order/myorderinfo">我的订单</a>';
		    $("#pleaseLoginDiv").empty();
		    $("#pleaseLoginDiv").show();
		    $("#noLoginDiv").hide();
		    $("#pleaseLoginDiv").html(urlhtml);
	    } else { // 未登录用户
		    $("#pleaseLoginDiv").hide();
		    $("#noLoginDiv").show();
	    }
	    var COOKIE_CITY="mallcity";
		//切换地区
		var arr=new Array(2);
		if($.cookie("mallcity")){
			arr=$.cookie("mallcity").split("|");
			var provincecodeLink =  arr[0];
			if(arr[0]){				
				var flag = $("a[provincecode='"+arr[0]+"']").text();
				if('79'==provincecodeLink){
					$('#OldUserLink').attr("href","http://www.10010.com/static/homepage/subjectpage/57900000251770.html");
			  	}else{
			  		$('#OldUserLink').attr("href","http://www.10010.com/pushpage/59800000169773.html");
			  	}
				if("" == flag || flag == null) {
					$('#my_area').text($("a[provincecode='"+arr[0]+"']").text());					
				} else {					 
					$('#my_area').text($("a[provincecode='"+arr[0]+"']").text());
				}
			}
		}else{
			if('79'==provincecodeLink){
				$('#OldUserLink').attr("href","http://www.10010.com/static/homepage/subjectpage/57900000251770.html");
		  	}else{
		  		$('#OldUserLink').attr("href","http://www.10010.com/pushpage/59800000169773.html");
		  	}
			Set_Cookie(COOKIE_CITY, "11|110", 3, "/", ".10010.com");
		}
	    
		//导航切换地区		
		$(".media-body a").on('click',function(){
			var text = $(this).text();
			$("#my_area").text(text)
			var province = $(this).attr('provinceCode');
			if('79'==province){
				$('#OldUserLink').attr("href","http://www.10010.com/static/homepage/subjectpage/57900000251770.html");
		  	}else{
		  		$('#OldUserLink').attr("href","http://www.10010.com/pushpage/59800000169773.html");
		  	}
			var cityCode = $(this).attr('cityCode');
			Set_Cookie(COOKIE_CITY, province + "|" + cityCode, 3, "/", ".10010.com");
			$(this).parents(".dropdown-menu").hide();
		})
