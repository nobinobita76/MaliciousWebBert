var newWindow = null; 
var urlT ="//inf.10010.com/serviceOnline/handLogin.action?accessType=014";
function openWindow(){ 
if(newWindow == null){ 
newWindow = open(urlT, "_blank", ""); 
}else{ 
newWindow.close(); 
newWindow = open(urlT, "_blank", ""); 
} 
} 

