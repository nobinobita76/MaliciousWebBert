		$(this).attr('style','border:0 none;vertical-align: bottom ;padding-top:17px ;padding-left:16px;');
	}); */
	$(".login_list_ul li a").live("click",function(){
		if ($(this).attr('href').indexOf("upay.10010.com/npfweb/npfcellweb/phone_recharge_fill.htm") > -1) {
			$(this).attr('target','_blank');
		} else {
			if ($(this).attr('href').indexOf("transact/index.html") < 0) {
				if(!_BaseTools.isLogin()){
					return false;
				}
			}
		}
	
	});
});
