 <script>

 function submitAdvice(){

	var value=$("#telephone").val();

	var adviceText=$("#OtherText").val();
	 var isMobile=/^1[3|4|5|7|8][0-9]\d{4,8}$/;   
	/* var isMobile=/^(1(3[0-2]|5[56]|8[56])\d{8})$/; */
	var isPhone=/^((0\d{2,3})-)?(\d{7,8})(-(\d{3,}))?$/;
	var advice=removeAllSpace($("#OtherText").val());
	var title=removeAllSpace($("#OtherText").attr("title"));
	if(value !=null && value !=""){
		if(value.match(isMobile)==null &&value.match(isPhone)==null && "如方便，请留下您的联系电话" !=value  ){
			
			$("#errorTips").html("请输入正确格式的联系电话！");
			$("#errorTips").attr("name","show");
			 _BaseTools.showid('tipsWrong');
			
		} else if(advice==null ||advice.length<=0 || advice ==title){
			$("#errorTips").html("对不起，内容不能为空。说点什么吧！");
			$("#errorTips").attr("name","show");
			 _BaseTools.showid('tipsWrong');
		
			
		}else{
			 var adviceType=$(".search_up_city p").html();
	        	if("如方便，请留下您的联系电话" ==value){
	        		value=" ";
	        	}
		      
	        	$.ajax({
	    			type:"post",
	    			data:{adviceText:adviceText,adviceType:adviceType,advicePhone:value},
	    			url:E3CommonsVariables.RequestPrefix() + "/tucao",
	    			success:function(data){
	    				$(".closeTransactConmit").parents('.tucao').fadeOut();
	    				$('div#layer').remove();
	    		
	    				 _BaseTools.showid('wrongTc22');
	    			}

	    		});	
			
		}
		}

	else if(value ==null || value ==""){
		if(advice==null ||advice.length<=0 || advice ==title){
			$("#errorTips").html("对不起，内容不能为空。说点什么吧！");
			$("#errorTips").attr("name","show");
			 _BaseTools.showid('tipsWrong');
			
		}else{
	    var adviceType=$(".search_up_city p").html();
	        	
	    if("如方便，请留下您的联系电话" ==value){
    		value=" ";
    	}
	        	$.ajax({
	    			type:"post",
	    			data:{adviceText:adviceText,adviceType:adviceType,advicePhone:value},
	    			url:E3CommonsVariables.RequestPrefix() + "/tucao",
	    			success:function(data){
	    				$(".closeTransactConmit").parents('.tucao').fadeOut();
	    				$('div#layer').remove();
	    				
	    				 _BaseTools.showid('wrongTc22');
	    			}

	    		});	
	
	}
	
	}
} 
