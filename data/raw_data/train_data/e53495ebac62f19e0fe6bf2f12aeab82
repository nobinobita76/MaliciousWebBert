        <script type="text/javascript">
            $(document).ready(function() {
                $(".left_w3").mouseout(function() {
                    $(".money_w3").hide()
                });
                $(".left_w3,.money_w3").mouseover(function() {
			    $.getJSON((isHttps?"https://upay.10010.com":"//www.10010.com") + "/mall-web/LoginGetOrderAction/initJson?callback=?", function(data) {
				   initOrderInfo(data);
				});
                    $(".money_w3").show()
                });
                $("#searchType").mouseout(function() {
                    $(".li-two").hide()
                });
                $("#searchType").mouseover(function() {
                    $(".li-two").show()
                });
                $("#shortcut ul li").mouseout(function() {
                    $(this).find("dd").css({
                        "display": "none"
                    })
                });
                $("#shortcut ul li").mouseover(function() {
                    $(this).find("dd").css({
                        "position": "absolute",
                        "display": "block",
                        "top": "28px",
                        "z-index": "9999",
                        "background-color": "#fff"
                    })
                });
                $("#searchType").on("click",".div", function(event) {
                     var TEXT= $(this).text();
                     if (TEXT=="商品") {
                       $(".li-one").text(TEXT);
                        $(".li-two").text("服务").hide();
                        $("#hotSearchBox_service").hide();
                        $("#hotSearchBox_product").show();
                     }else {
                       $(".li-one").text(TEXT);
                        $(".li-two").text("商品").hide();
                        $("#hotSearchBox_service").show();
                        $("#hotSearchBox_product").hide();
                     }
                })
            });
        </script>
