    <!--QQ客服JS------> 
    <script language="javascript" type="text/javascript">
    var tips; var theTop = 145
    var old = theTop;
    function initFloatTips() {
        tips = document.getElementById("divwww_hlxx");
        moveTips();
    }; 
    function moveTips() {
        var tt = 50;
        if (window.innerHeight) {
            pos = window.pageYOffset
        }
        else if (document.documentElement && document.documentElement.scrollTop) {
            pos = document.documentElement.scrollTop
        } 
        else if (document.body) {
            pos = document.body.scrollTop;
        }
        pos = pos - tips.offsetTop + theTop;
        pos = tips.offsetTop + pos / 10;
        if (pos < theTop) pos = theTop;
        if (pos != old) {
            tips.style.top = pos + "px";
            tt = 10;
            
        }
        old = pos;
        setTimeout(moveTips, tt);
    } 
    initFloatTips();
    function OnlineOver() {
        document.getElementById("hlxx").style.display = "none";
        document.getElementById("_hlxx").style.display = "block";
        document.getElementById("divwww_hlxx").style.width = "145px";
    }
    function OnlineOut() {
        document.getElementById("hlxx").style.display = "block";
        document.getElementById("_hlxx").style.display = "none";
    }
    if (typeof (HTMLElement) != "undefined") 
    {
        HTMLElement.prototype.contains = function (obj) {
            while (obj != null && typeof (obj.tagName) != "undefind") {
                if (obj == this) return true;
                obj = obj.parentNode;
            }
            return false;
        };
    }  
    function hideMsgBox(theEvent) { 
        if (theEvent) {
            var browser = navigator.userAgent; 
            if (browser.indexOf("Firefox") > 0) {
                if (document.getElementById("_hlxx").contains(theEvent.relatedTarget)) { 
                    return; 
                }
            } 
            if (browser.indexOf("MSIE") > 0) {
                if (document.getElementById("_hlxx").contains(event.toElement)) { 
                    return; 
                }
            }
        } 
       
        document.getElementById("hlxx").style.display = "block";
        document.getElementById("_hlxx").style.display = "none";
    }
</script> 
