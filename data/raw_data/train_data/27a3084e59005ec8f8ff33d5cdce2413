	<script>
		var copyTxt=document.getElementById("text");
		copyTxt.innerHTML="http://m.ku6.com/show/"+vid+".html";
		/*评论列表*/
		var isContent1=true,isContent2=false,curComPage=1,comListTotal,comPageNum,comPageSize=10;
		Comment(vid,curComPage,comPageSize,cb,false);      //初始化
		$(".tab_menu li").mouseover(function(){
			$(this).removeClass('tab_li').addClass('cur_tab');
			$(this).siblings().removeClass('cur_tab').addClass('tab_li');
			var curContent=$(this).attr("tabName");
			$('.'+curContent+'').show();
			$('.'+curContent+'').siblings().hide();
			if(curContent=="content1") {
				isContent1=true;
				isContent2=false;
			}
			if(curContent=="content2") {
				isContent1=false;
				isContent2=true;
				/*获取评论列表*/
				curComPage=1;
				Comment(vid,curComPage,comPageSize,cb,true);
			}
		})

		/*刷新验证码*/
		$(".check_refresh a").click(function(){
			$(this).parent().parent().find(".check_img img").attr("src","http://m.ku6.com/checkcode/index.php?token="+Math.random(new Date()));
		})

		/*列表翻页*/
		function pageUp() {
			if(isContent1) {
				get_relation('up');
			}
			if(isContent2) {
				curComPage--;
				if(curComPage<1) {
					curComPage=1;
					alert("这已经是第一页了！");
				}
				Comment(vid,curComPage,10,cb,true);
			}
		}

		function pageDown() {
			if(isContent1) {
				get_relation('down');
			}
			if(isContent2) {
				curComPage++;
				if(curComPage>comPageNum) {
					curComPage=comPageNum;
					alert("这已经是最后一页了！");
				}
				Comment(vid,curComPage,10,cb,true);
			}
		}

		/*分享弹层显示、隐藏*/
		function shareShow_Hide() {
			if($('.nav_share').hasClass('hide')) {
				$('.nav_share').removeClass('hide').addClass('show');
			}else {
				$('.nav_share').removeClass('show').addClass('hide');
			}
		}

		/*评论弹层显示、隐藏*/
		function contentShow_Hide(bl) {
			if(bl) {
				if(!isLogined) {
					loginShow_Hide(true);
					return;
				}
				$('.nav_content_box').removeClass('hide').addClass('show');
				$('#video video').addClass('hideImport');
			}else {
				$('.nav_content_box').removeClass('show').addClass('hide');
				$('#video video').removeClass('hideImport');
			}
		}

		/*微信分享复制链接*/
		function shareWei() {
			document.getElementById("text").select();
            document.execCommand("copy",false,null);
            if($('.weiTip').hasClass('hide')) {
            	$('.weiTip').removeClass('hide').addClass('show');
            	setTimeout(function() {$('.weiTip').removeClass('show').addClass('hide');},2000);
            }
		}

		/*视频详情显示下拉*/
		var isMoreShowed=false;
		//显示视频播放数
		function v_counts(par1,par2) {
			$('#play_num').html("播放次数："+par1[0]['count']);
		}
		function showMore() {
			$('#more_desc').toggleClass('show');
			if(!isMoreShowed) {
				$('.abstract_title').css('white-space','normal');
				$('.abstract_txt').css('border-bottom','#c0bebf 1px solid');
				$('.btnMore').css('transform','rotate(0deg)');
				isMoreShowed=true;
				var stat_sid = getRand(8,1);
				$.getScript("http://v"+stat_sid+".stat.ku6.com\/dostatv.do?method=getVideoPlayCount&n=v_counts&cp=0&v="+vid);
			}else {
				$('.abstract_title').css('white-space','nowrap');
				$('.abstract_txt').css('border-bottom','none');
				$('.btnMore').css('transform','rotate(180deg)');
				isMoreShowed=false;
			}
		}
		
		//取随机值
		function getRand(arr, len) {
			
			if(toString.apply(arr) === '[object Number]') {
				
				var new_arr = new Array();
				for(var i = 0; i < arr; i++) { 
					new_arr[i] = i;
				}
				arr = new_arr;
			}
			arr.sort(function () { 
				return Math.random()-0.5; 
			}); 
			return arr.slice(0, len); 
		}

		/*点赞按钮点击*/
		var isZaned=false;
		var zanBtn=document.getElementById("zanBtn");
		var new_element=document.createElement("script");
		function zanFun() {
			if(!isLogined) {
				loginShow_Hide(true);
				return;
			}
			new_element.setAttribute("type","text/javascript");
			var zanUrl="http://v0.stat.ku6.com/dostatv.do?method=setVideoUp&v="+vid+"&o=10000&n=zanBack&cp=1";
			if(isZaned) {
				new_element.setAttribute("src","");
				removeElement(new_element);
				$('#zanBtn img').attr("src","http://css.ku6cdn.com/www/2016/0826/images/zan_pre.png");
				isZaned=false;
			}else {
				$('#zanBtn img').attr("src","http://css.ku6cdn.com/www/2016/0826/images/zan_cancel.png");
				isZaned=true;
				var spanObj=document.createElement("span");
				spanObj.className="fadeOutUp";
				spanObj.innerHTML="+1";
				zanBtn.appendChild(spanObj);
				new_element.setAttribute("src",zanUrl);
				document.body.appendChild(new_element);
			}
			
		}

		function zanBack(data) {
			console.log(data);
		}
		
		/*加载时判断登录*/
		login_user();
		function login_user(){
		  	user_id = getcookie("sysUserId");
			systemPPCNonIBWSign = getcookie("systemPPCNonIBWSign");
			if(user_id == null || user_id == "" || systemPPCNonIBWSign == "" || systemPPCNonIBWSign == ""){return;};

		  	var systemPPCLoginUserNickName = decodeURIComponent(getcookie("systemPPCLoginUserNickName"));
		  	var systemPPCLoginUserHeadFace = decodeURIComponent(getcookie("systemPPCLoginUserHeadFace")).split(";");
		  	isLogined=true;
			check_favorite();
			check_follow('27413508');
	  		if(!!systemPPCLoginUserHeadFace[1] && systemPPCLoginUserHeadFace[1] !=""){
		   		$("#login_user").attr("src",systemPPCLoginUserHeadFace[1]);
			}else {
				$("#login_user").attr("src","http://img.ku6.com/images/common/defcover/both_small.gif");
			}
		}

		/*登录弹层显示、隐藏*/
		var redirect=window.location.href;
		function loginShow_Hide(bl) {
			if(bl) {
				if(isLogined) {
					window.open('http://m.ku6.com/member');
					return;
				}
				$('.nav_login').removeClass('hide').addClass('show');
				$('#video video').addClass('hideImport');
				
				//刷新验证码
				$(".check_img img").attr("src","http://m.ku6.com/checkcode/index.php?token="+Math.random(new Date()));
				
				cookie('_ulg_rd', redirect, {path:'/',domain: 'ku6.com'} );
				cookie('_ulg_owin', 0, {path:'/',domain: 'ku6.com'} );
			}else {
				$('.nav_login').removeClass('show').addClass('hide');
				$('#video video').removeClass('hideImport');
			}
		}

		/*提交登录*/
		function loginFun() {
			var checkUrl=$('.check_img img').attr('src');
			var token=checkUrl.split("token=")[1];
			var checkbox = document.getElementById('rem-me');
			var checkNum=0;
			var loginName=$('#loginName')[0].value;
			var loginPassword=$('#loginPassword')[0].value;
			var loginCheck=$('#loginCheck')[0].value;
			checkNum=checkbox.checked?2592000:0;
			var loginUrl="http://passport.ku6.com/v3-ajaxLogin.htm?callback=loginBack&loginNamePar="+loginName+"&passwordPar="+loginPassword+"&checkcodePar="+loginCheck+"&expirePar="+checkNum+"&checkcodeTokenPar="+token;
			console.log(loginUrl);
			$.ajax({
			   async:false,
			   url: loginUrl,
			   type: "GET",
			   dataType: 'jsonp',
			   callback: 'loginBack',
			   timeout: 3000,
			   success: function (data) {
			   	if(data.status=='-1') {
			   		alert(data.msgInfo);
			   	}
			   	if(data.status=='1') {
			   		loginShow_Hide(false);
			   		login_user();
			   	}
			   },
			   error: function(xhr){
			   	alert("请求出错(请检查相关度网络状况.)");
			   }
			});
		}

		function loginBack(data) {
			
		}

		/*提交评论*/
		function submitComment() {
			var contentUrl=$('#textInput').val();
			var commentUrl="http://comment.ku6.com/api/web_save.jhtm?vtype=111&id="+vid+"&comment.commentContent="+contentUrl;
			if(contentUrl.length=0) return;
			$.ajax({
			   async:false,
			   url: commentUrl,
			   type: "GET",
			   dataType: 'jsonp',
			   callback: 'commentBack',
			   timeout: 3000,
			   success: function (data) {
			   		if(data.status=='300') {
						alert(data.msgText);
					}
					if(data.status=='200') {
						contentShow_Hide(false);
						Comment(vid,curComPage,comPageSize,cb,true);      //刷新评论
						document.getElementById("textInput").value = "";
					}
				   },
				   error: function(xhr){
						alert("请求出错(请检查相关度网络状况.)");
				   }
			});
		}
		
		//获取评论列表数据
		function cb(v) {
			comListTotal=v.data.count;
			comPageNum=Math.ceil(comListTotal/comPageSize);
			if(comListTotal==0) {
				$('.commentSubmit a img').attr("src","http://css.ku6cdn.com/www/2016/0826/images/commentSumit_none.png");
				$('.commentSubmit a img').css("height","1.4em");
			}else {
				$('.commentSubmit a img').attr("src","http://css.ku6cdn.com/www/2016/0826/images/commentSumit.png");
				$('.commentSubmit a img').css("height","2em");
				$('.comment_num').removeClass('hide').addClass('show');
				$('.comment_num')[0].innerHTML=comListTotal;
			}
		}

		function commentBack(data) {
			
		}

		function removeElement(_element){
	         var _parentElement = _element.parentNode;
	         if(_parentElement){
                _parentElement.removeChild(_element);
	         }
		}
		
		//打开APP
		document.getElementById('openApp').onclick = function(e){
			e.preventDefault();
			var ua = window.navigator.userAgent.toLowerCase();
			var openUrl="ku6://video?vid="+vid;
			var downUrl="http://m.ku6.com/app.php";
			
			//微信  
			if(ua.match(/MicroMessenger/i) == 'micromessenger'){  
				window.location.href=downUrl; 
			}else{//非微信浏览器  
				if (navigator.userAgent.match(/(iPhone|iPod|iPad);?/i)) {
					document.location.href = downUrl;
					/*setTimeout(function() {
						document.location.href = downUrl;
					}, 1000);*/
				}else if (navigator.userAgent.match(/android/i)) {
					var ifrSrc = openUrl ;
					if (!ifrSrc) {
						return;
					}
					var ifr = document.createElement('iframe');
					ifr.src = ifrSrc;
					ifr.style.display = 'none';
					document.body.appendChild(ifr);
					setTimeout(function() {
						document.body.removeChild(ifr);
						document.location.href = downUrl;
					}, 1000);
				}
			}
		}

		window.onload=function() {
			var redirect = encodeURIComponent("http://m.ku6.com/index.php?m=member");
			$(".login_other a.th-qq").click(function(){
				cookie('ckl_hao', "QQ", {path:'/',domain: 'ku6.com'} );
				window.open('http://passport.ku6.com/qzone-loginQZone.htm?entry='+redirect, '' ,'height=500,width=600, toolbar=no, menubar=no, scrollbars=no, status=no, location=yes, resizable=yes');
				return;
			});
			$(".login_other a.th-weibo").click(function(){
				cookie('ckl_hao', "SINA", {path:'/',domain: 'ku6.com'} );
				window.open('http://passport.ku6.com/tsina-loginTsina.htm?entry='+redirect, '' ,'height=500,width=600, toolbar=no, menubar=no, scrollbars=no, status=no, location=yes, resizable=yes');
				return;
			});
			$(".login_other a.th-qqwb").click(function(){
				cookie('ckl_hao', "QZONE", {path:'/',domain: 'ku6.com'} );
				window.open('http://passport.ku6.com/qq-loginQQ.htm?entry='+redirect, '' ,'height=500,width=600, toolbar=no, menubar=no, scrollbars=no, status=no, location=yes, resizable=yes');
				return;
			});
			$(".login_other a.th-kaixin").click(function(){
				cookie('ckl_hao', "KX", {path:'/',domain: 'ku6.com'} );
				window.open('http://passport.ku6.com/kaixin-loginKaiXin.htm?entry='+redirect, '' ,'height=500,width=600, toolbar=no, menubar=no, scrollbars=no, status=no, location=yes, resizable=yes');
				return;
			});
			$(".login_other a.th-renren").click(function(){
				cookie('ckl_hao', "RR", {path:'/',domain: 'ku6.com'} );
				window.open('http://passport.ku6.com/renren-loginRR.htm?entry='+redirect, '' ,'height=500,width=600, toolbar=no, menubar=no, scrollbars=no, status=no, location=yes, resizable=yes');
				return;
			});
			
			if(window.location.href.indexOf("ucbrowser") == -1) {
				//推荐视频统计ST参数
				for(var i=0;i<document.getElementById("video_list").getElementsByClassName("newztst").length;i++){
					var ele_href=document.getElementById("video_list").getElementsByClassName("newztst")[i].getElementsByTagName("a")[0].href;
					if(ele_href.indexOf("?")>0){
						document.getElementById("video_list").getElementsByClassName("newztst")[i].getElementsByTagName("a")[0].href=ele_href+"&st=2_1_"+(i+1);
					}else{
						document.getElementById("video_list").getElementsByClassName("newztst")[i].getElementsByTagName("a")[0].href=ele_href+"?st=2_1_"+(i+1);
					}
				}
			}
		}

		var cookie = function (key, value, options) {

		    // key and value given, set cookie...
		    if (arguments.length > 1 && (value === null || typeof value !== "object")) {
		        options = jQuery.extend({}, options);

		        if (value === null) {
		            options.expires = -1;
		        }

		        if (typeof options.expires === 'number') {
		            var days = options.expires, t = options.expires = new Date();
		            t.setDate(t.getDate() + days);
		        }

		        return (document.cookie = [
		            encodeURIComponent(key), '=',
		            options.raw ? String(value) : encodeURIComponent(String(value)),
		            options.expires ? '; expires=' + options.expires.toUTCString() : '', // use expires attribute, max-age is not supported by IE
		            options.path ? '; path=' + options.path : '',
		            options.domain ? '; domain=' + options.domain : '',
		            options.secure ? '; secure' : ''
		        ].join(''));
		    }
			
		    options = value || {};
		    var result, decode = options.raw ? function (s) { return s; } : decodeURIComponent;
		    return (result = new RegExp('(?:^|; )' + encodeURIComponent(key) + '=([^;]*)').exec(document.cookie)) ? decode(result[1]) : null;
		};
	</script>
