		function logjs(value) {
			if (window.sdoLogger) {
				sdoLogger.authId = '624eeba8a615b068';
				sdoLogger.clickLog('adtype=' + value);
			}
		}
		
		/**加订阅*/
		function add_follow(uid) {
			if (!uid) return false;
			if(!isLogined) {
				loginShow_Hide(true);
				return;
			}
			$.get('http://m.ku6.com/apiv2/subscribe.php', {
				a: 'add',
				uid: uid
			},
			function(data) {
				if (data == 1) {
					alert('订阅成功！');
					$('#add_follow').html('已订阅');
					$('#add_follow').removeAttr("onclick");
					$('#add_follow').attr("onclick", "del_follow('27413508')");
				} else if (data == 0) {
					alert('订阅失败！');
				} 
			});
		}
		/**取消订阅*/
		function del_follow(uid) {
			if (!uid) return false;
			$.get('http://m.ku6.com/apiv2/subscribe.php', {
				a: 'del',
				uid: uid
			},
			function(data) {
				if (data == 1) {
					alert('取消成功！');
					$('#add_follow').html('+订阅');
					$('#add_follow').removeAttr("onclick");
					$('#add_follow').attr("onclick", "add_follow('27413508')");
				} else if (data == 0) {
					alert('取消订阅失败！');
				}
			});
		}
		
		/*判断是否已经订阅*/
		function check_follow(uid) {
			if (!uid) return false;
			if(!isLogined) {
				return;
			}
			$.get('http://m.ku6.com/apiv2/subscribe.php', {
				a: 'is',
				uid: uid
			},
			function(data) {
				if (data == 1) {
					//已经订阅
					$('#add_follow').html('已订阅');
					$('#add_follow').removeAttr("onclick");
					$('#add_follow').attr("onclick", "del_follow('27413508')");
				} else if (data == 0) {
					//尚未订阅
					$('#add_follow').html('+订阅');
					$('#add_follow').removeAttr("onclick");
					$('#add_follow').attr("onclick", "add_follow('27413508')");
				}
			});
		}

		/**加收藏*/
		function add_favorite(vid) {
			if (!vid) return false;
			if(!isLogined) {
				loginShow_Hide(true);
				return;
			}
			$.get('http://m.ku6.com/apiv2/favorite.php', {
				a: 'add',
				vid: vid
			},
			function(data) {
				if (data == 1) {
					alert('收藏成功！');
					$('#add_favorite img').attr('src','http://css.ku6cdn.com/www/2016/0826/images/shoucang_cancel.png');
					$('#add_favorite').removeAttr("onclick");
					$('#add_favorite').attr("onclick", "del_favorite('" + vid + "')");
				} else if (data == 0) {
					alert('收藏失败！');
				}
			});
		}
		/**取消收藏*/
		function del_favorite(vid) {
			if (!vid) return false;
			$.get('http://m.ku6.com/apiv2/favorite.php', {
				a: 'del',
				vid: vid
			},
			function(data) {
				if (data == 1) {
					alert('取消成功！');
					$('#add_favorite img').attr('src','http://css.ku6cdn.com/www/2016/0826/images/shoucang_pre.png');
					$('#add_favorite').removeAttr("onclick");
					$('#add_favorite').attr("onclick", "add_favorite('" + vid + "')");
				} else if (data == 0) {
					alert('取消收藏失败！');
				}
			});
		}
		
		/*判断是否已经收藏*/
		function check_favorite() {
			if (!vid) return false;
			if(!isLogined) {
				return;
			}
			var jsonP=document.createElement("script");
			jsonP.type="text/javascript";
			jsonP.src="http://new.ku6.com/mobile/isWatchLater?vid="+vid+"&cb=favoriteBack";
			document.getElementsByTagName("head")[0].appendChild(jsonP);
		}
		
		function favoriteBack(result) {
			if(result.data) {     //已经收藏
				$('#add_favorite img').attr('src','http://css.ku6cdn.com/www/2016/0826/images/shoucang_cancel.png');
				$('#add_favorite').removeAttr("onclick");
				$('#add_favorite').attr("onclick", "del_favorite('" + vid + "')");
			}
		}
		
		/*加密提交*/
		function pwSubFun() {
			var password=$('.videoPw').val();
			var pwUrl="http://m.ku6.com/api.php?op=json&action=check_password&vid="+vid+"&password="+password;
			if(password.length == 0) {
			 	alert("请输入密码！");
				return;
			}
			
			$.get('http://m.ku6.com/api.php', {
				op: 'json',
				action: 'check_password',
				vid: vid,
				password: password
			},
			function(data) {
				var obj=JSON.parse(data);
				if(obj.code=="1") {
					$('.checkvideo').removeClass('show').addClass('hide');
					subPW=password;
					v.player_param.password=subPW;
					var ctrolBar = document.getElementById("KP_control_bar");
					if (ctrolBar != null) ctrolBar.parentNode.removeChild(ctrolBar);
					if(v.player_param.env.video_src_type == 'm3u8'){
						v.load_m3u8_player();
					}else{
						v.load_mp4_player();
					}
				}else {
					alert("您输入的密码错误！");
				}
			});
		}
		
		function pwLoadPlayer() {
			if(isEncrypted && v) {
				v.player_param.password=subPW;
				v = new Ku6HTML5Player('video', vid, poster,param,function(callback){
					if(vid_arr.length > 0){
						vid = vid_arr.shift();
						param = param_arr.shift();
						if(vid_arr.length==0) {
							get_relation('down');
						}
						get_video(vid,function(){
							return callback(vid,param);
						});
						
					} else {
						get_relation('down');
					}
				});
			}
		}
	</script>
