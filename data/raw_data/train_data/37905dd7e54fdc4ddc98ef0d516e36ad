(function(){
		if((navigator.userAgent.match(/iPhone/i)) || (navigator.userAgent.match(/iPod/i)) || (navigator.userAgent.match(/iPad/i))) {
	  	var regxs=['http://\\w+\.youku\.com/player\.php/sid/(\\w+)/v\.swf','http://\\w+\.youku\.com/player\.php/sid/(\\w+)\.swf','http://www\.tudou\.com/v/(\.+)/(\.+)/v\.swf'];
		function html5Player(embed){
			var result;
			for(var i=0;i<regxs.length;i++){
				result=new RegExp(regxs[i],'ig').exec(embed.src);
				if(result){
					var site = result[0];
					if(site.match(/youku/)) {
						var vid=result[1];
						jQuery(embed).replaceWith('<video width="'+embed.width+'" height="'+embed.height+'" x-webkit-airplay="allow" preload controls autoplay src="http://v.youku.com/player/getRealM3U8/vid/'+vid+'/type/mp4/v.m3u8"></video>');
			      		return true;
					}else if(site.match(/tudou/)) {
						var vid=result[1];
						jQuery(embed).replaceWith('<iframe width="'+embed.width+'" height="'+embed.height+'" frameborder="0" src="http://www.tudou.com/programs/view/html5embed.action?code='+vid+'"></iframe>');
						return true;
					}
				}
		  	}
		  	return false;
		}
	  	var embeds = jQuery('embed[src*=swf]');
	  	jQuery.each(embeds, function(i, v) {
	  		html5Player(embeds[i]);
	  	});
	}
}());
</script>

