    <input name="hid_cityid" type="hidden" id="hid_cityid">
    <input name="hid_provinceid" type="hidden" id="hid_provinceid">
    <input name="hid_TotalPage" type="hidden" id="hid_TotalPage" value="113">
    <script type="text/javascript">
        (function ($) {
            $.goodList = {};
            $.extend($.goodList, {
                TotalPage: $("#hid_TotalPage").val(),
                PageSize: 6,
                PageIndex: 1,
                IsRuning: false,
                btnMore: $("#more"),
                Init: function () {
                    this.GetMore();
                    this.BindEvent();
                    this.SetPages();
                },
                GetGoodsData: function (pageIndex1, loadTypoe) {
                    $.goodList.IsRuning = true;
                    var that = this;
                    $("#loading").show();
                    $.ajax({
                        type: "GET",
                        dataType: "jsonp",
                        jsonpCallback: "aa",
                        url: "/Ajax/GetGoodsInfo.ashx?type=json",
                        data: that.GetPara(pageIndex1),
                        success: function (data) {
                            $.goodList.IsRuning = false;
                            $("#loading").hide();
                            if (data.IsSuccess) {
                                that.TotalPage = data.TotalPage;
                                that.PageIndex = pageIndex1;
                                that.IsShowMore();
                                $("#c-list ").children().remove();
                                if (data.GoodsList.length > 0) {
                                    $("#c-list").append(that.GetGoodInfoForHtml(data.GoodsList));
                                }
                                if (loadTypoe == 1) {
                                    that.SetPages();
                                    that.BindEvent();
                                }

                            }
                        },
                        err: function () {
                            $("#loading").hide();
                        }
                    });
                },
                GetPara: function (pageIndex) {
                    var priceType = 0;
                    var brandId = 0;
                    var cityId;
                    var provinceId;
                    brandId = $(".brands li.dq").attr("bid");
                    priceType = $(".prices li.active").attr("pid");
                    cityid = $("#hid_cityid").val();
                    provinceId = $("#hid_provinceid").val();
                    if (!priceType)
                        priceType = 0;
                    if (!brandId)
                        brandId = 0;
                    var para = { "price": priceType, "cityid": cityid, "provinceid": provinceId, "bsid": brandId, "pageindex": pageIndex };
                    return para;
                },
                IsShowMore: function () {
                    if (this.TotalPage <= this.PageIndex)
                        this.btnMore.hide();
                    else
                        this.btnMore.show();
                },
                GetGoodInfoForHtml: function (datalist) {
                    var carlisthtml = "";
                    for (var i = 0; i < datalist.length; i++) {
                        var data = datalist[i];
                        carlisthtml += "<div class=\"car-item\" id=\"car" + (i + 1) + "\"><a href='http://m.qiche4s.cn/" + data.DealerId + "/price_order_" + data.CarId + ".html?opsource=mtm" + "'>"
                                        + "<img src=\"" + data.ImagePath + "\">"
                                        + "<span class=\"car-type\">" + data.Cs_Name + "</span>"
                                        + "<span class=\"car-price\">" + data.MinPromot.toFixed(2) + "万<small>起</small></span>"
                                        + "<span class=\"car-guide-price\">指导价<del>" + data.MinPrice.toFixed(2) + "万元</del></span>"
                                        + "</a></div>";
                    }
                    return carlisthtml;
                },
                GetMore: function () {
                    var that = this;
                    that.btnMore.click(function () {
                        $.goodList.GetGoodsData($.goodList.PageIndex + 1, 0);
                    });
                },
                SetPages: function () {
                    if ($.goodList.TotalPage > 0) {
                        this.ShowPages();
                        this.HideDivNone()
                    }
                    else {
                        this.HidePages();
                        this.ShowDivNone();
                    }
                    $("#pageSelect").html("");
                    for (var i = 1; i <= $.goodList.TotalPage ; i++) {
                        $("#pageSelect").append("<option value=" + i + ">" + i + "</option>");
                    }
                    this.SetPageSelect();
                    this.ControlPageButtonClass();
                },
                BindEvent: function () {
                    $("#btnPrePage").click(function () {
                        if ($.goodList.IsRuning)
                            return;
                        if ($.goodList.PageIndex > 1) {
                            $.goodList.PageIndex = parseInt($.goodList.PageIndex) - 1;
                            $.goodList.SetPageSelect();
                            $("#divPageInfo").html($.goodList.PageIndex + "/" + $.goodList.TotalPage);
                            $.goodList.GetGoodsData($.goodList.PageIndex, 0);
                            //$('html, body').animate({ scrollTop: 0 }, 'fast');
                        }
                        $.goodList.ControlPageButtonClass();
                    });
                    $("#btnNextPage").click(function () {
                        if ($.goodList.IsRuning)
                            return;
                        if ($.goodList.PageIndex < $.goodList.TotalPage) {
                            $.goodList.PageIndex = parseInt($.goodList.PageIndex) + 1;
                            $.goodList.SetPageSelect();
                            $("#divPageInfo").html($.goodList.PageIndex + "/" + $.goodList.TotalPage);
                            $.goodList.GetGoodsData($.goodList.PageIndex, 0);
                            //$('html, body').animate({ scrollTop: 0 }, 'fast');
                        }
                        $.goodList.ControlPageButtonClass();
                    });
                    $("#pageSelect").change(function () {
                        $.goodList.PageIndex = $("#pageSelect").val();
                        $.goodList.SetPageSelect();
                        $("#divPageInfo").html($.goodList.PageIndex + "/" + $.goodList.TotalPage);
                        $.goodList.GetGoodsData($.goodList.PageIndex, 0);
                        $.goodList.ControlPageButtonClass();
                        //$('html, body').animate({ scrollTop: 0 }, 'fast');
                    });
                },
                SetPageSelect: function () {
                    $("#pageSelect").val($.goodList.PageIndex);
                    $("#divPageInfo").html($.goodList.PageIndex + "/" + $.goodList.TotalPage);
                    // $("#pageSelect option[value=" + $.goodList.PageIndex + "]").attr("selected", "selected");
                },
                HidePages: function () {
                    $("#divPages").hide();
                },
                ShowPages: function () {
                    $("#divPages").show();
                },
                ShowDivNone: function () {
                    $("#divNone").show();
                },
                HideDivNone: function () {
                    $("#divNone").hide();
                },
                ControlPageButtonClass: function () {
                    $("#btnPrePage").removeClass("m-pages-none");
                    $("#btnNextPage").removeClass("m-pages-none");
                    if ($.goodList.PageIndex == 1) {
                        $("#btnPrePage").addClass("m-pages-none");

                    }
                    if ($.goodList.PageIndex == $.goodList.TotalPage) {
                        $("#btnNextPage").addClass("m-pages-none");
                    }
                }
            })
        })(jQuery);
        $.goodList.Init();
        $(".tabs-pane-item .brands li").click(function () {
            $(".tabs-pane-item .brands li").removeClass("dq");
            $(this).addClass("dq");
            $("#lbrand").html("品牌：" + $(this).text() + "<a>×</a>").show().parent().show();
            $("#lbrand").click(function () {
                $(".tabs-pane-item .brands li").removeClass("dq");
                $("#lbrand").hide();
                if ($("#lprice").is(":hidden"))
                    $("#lclear").parent().hide();
                $.goodList.GetGoodsData(1, 1);
            });
            $.goodList.GetGoodsData(1, 1);
        });

        $(".tabs-pane-item .prices li").click(function () {
            $(".tabs-pane-item .prices li").removeClass("active");
            $(this).addClass("active");
            $("#lprice").html("价格：" + $(this).text() + "<a>×</a>").show().parent().show();
            $("#lprice a").click(function () {
                $(".tabs-pane-item .prices li").removeClass("active");
                $("#lprice").hide();
                if ($("#lbrand").is(":hidden"))
                    $("#lclear").parent().hide();
                $.goodList.GetGoodsData(1, 1);
            });
            $.goodList.GetGoodsData(1, 1);
        });
        $("#lclear").click(function () {
            $(".tabs-pane-item .prices li").removeClass("active");
            $(".tabs-pane-item .brands li").removeClass("dq");
            $("#lbrand").hide();
            $("#lprice").hide();
            $("#lclear").parent().hide();
            $.goodList.GetGoodsData(1, 1);
        });

    </script>
