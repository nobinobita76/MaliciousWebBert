    require(['juicer', 'alertify', 'app/shop/tpl/main'], function (juicer, alertify, tpl) {
//        $.getJSON( '/tag/list.json?callback=?', {tag_type: 11}, function (result) {
//            if (result.status !== '200') {
//                alertify.error(result.statusText);
//            } else {
//                $(".artistListBox").append(juicer(tpl.artistsTagTpl, result));
//            }
//        });
        var params = {};
        params.tag_type = 11;
        params.number = 10;
        var url_topten = Y.domains.shopSite + '/tag/list.json';
        $.ajax({
            type: 'get',
            dataType: 'json',
            jsonp: 'callback',
            url: url_topten,
            data: params,
            success:function( result ){
                $(".artistListBox").append(juicer(tpl.artistsTagTpl, result));
            }
        });
    });
</script>

