					$(document).ready(function(){
						$(".webarea").mouseover(function(){
							$(".cityList").css({"display":"block"});
						}).mouseout(function(){
							$(".cityList").css({"display":"none"});
						});
						
						$(".titleChar li").each(function(i){
							$(this).click((function(k){
								var _index = k;
								return function(){
									$(".titleChar li").removeClass("on");
									$(this).addClass("on");
									$(".cityListBox dl").hide();
									$(".cityListBox dl:eq(" + _index + ")").show();
								}
							})(i));
						});
				    });
					
