  $('.small_type li').bind('mouseover',function(){
   $(this).addClass('current');
});
$('.small_type li').bind('mouseout',function(){
   $(this).removeClass('current');
});
var ishover=false,ishover1=false;
$('#category_type').hover(function(){
     ishover1=true;
     $('#category_type').show();
},function(){
     ishover1=false;
     setTimeout(function(){
	 if(!ishover1&&!ishover)
	 $('#category_type').hide();
	 },50);
})
$('#navtype').hover(function(){
     ishover=true;
     $('#category_type').show();
},function(){
     ishover=false;
	 setTimeout(function(){
	 if(!ishover1&&!ishover)
	  $('#category_type').hide();
	  },50);
})

