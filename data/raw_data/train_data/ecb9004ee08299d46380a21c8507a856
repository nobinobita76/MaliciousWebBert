        (function sticky(){
            var aim = $('[data-fixed=sticky]');
            window.addEventListener('scroll', function(){
                var scrollTop = $(window).scrollTop();
                var coords = aim.offset().top;
                if (scrollTop >= coords){
                    aim.addClass('sticky');
