         GetViewUrl: function (picClassId,picid) {
             var self = this;
             if(self.specId!=0)
             {
                 if (self.colorId > 0) {
                     if( self.classId==1 ||  self.classId==12 ||  self.classId==0)
                     {
                         return "#!/spec/" + self.specId + "/t" + self.classId +"/c" + self.colorId + "/i" + picid;
