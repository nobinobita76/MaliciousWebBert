        function setAdMarginRight(setting) {
            var index = 0;
            var info = document.getElementById(setting.BoxId).getElementsByTagName("li");
            for (var i = 0; i < info.length; i++) { //循环
                if ((index + 1) % setting.columnCount != 0) {
                    info[i].style.marginRight = setting.marginRight;
                }
                else {
                    info[i].style.marginRight = "0px";

                }
                index++;

            }

        }
        setAdMarginRight({ BoxId: "hoCompany", columnCount: 8, marginRight: "9px" });
