	//阻止重复提交
	function PreventRepeatSubmit(options){
		this.defaults = {
			form: $(""),
			submitButton: $(""),
			submitHint: "提交中..."
		};
		this.opts = $.extend(this.defaults, options);
		if(typeof this.opts.form == "string"){
			this.opts.form = $(this.opts.form);
		}
		if(typeof this.opts.submitButton == "string"){
			this.opts.submitButton = $(this.opts.submitButton);
		}
		if(!(this.opts.form.length && this.opts.submitButton.length)){
			return false;
		}
		var self = this;
		this.opts.form.submit(function(){
			self.opts.submitButton.attr("disabled", "disabled");
			if($.inArray(self.opts.submitButton[0].tagName.toLowerCase(), ["input","button"]) != -1){
				self.opts.submitButton.val(self.opts.submitHint);
			}else{
				self.opts.submitButton.html(self.opts.submitHint);
			}
		});
	}
	new PreventRepeatSubmit({
		form: "#ticket_form",
		submitButton: "#ticket_form [type=\"submit\"]"
	});



		var word;
		$("#TicketForm_title").focusout(function(){
			var title = $("#TicketForm_title").val();
			if(word != title) {
				word = title;
				$(".article-suggestion").remove();
				$.post("/hc/request/suggest/",{word:title},function(data){
					if(data){
						//$("#TicketForm_title").after("<ul class=\"article-suggestion\"></ul>");
						//$(".article-suggestion").html(data);
						$("#TicketForm_title").after("<dl class=\"article-suggestion\"><dt>为您的问题推荐的帮助文档：</dt></dl>");
						$(data).appendTo(".article-suggestion");
					}
				});
			}
		});
	
