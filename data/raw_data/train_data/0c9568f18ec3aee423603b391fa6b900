		<script>
			var myScroll,t_img,isLoad = true;
			
			function loaded () {
				myScroll = new IScroll('#wrapper', { mouseWheel: true, tap: true ,click: true });
			}
			document.addEventListener('touchmove', function (e) { e.preventDefault(); }, false);
	
	 		isImgLoad(function(){ $('#pageLoadingLayer').hide();});
	        function isImgLoad(callback){	  
	            $('.content img').each(function(){if(this.height === 0){isLoad = false;return false;}});
	            if(isLoad){clearTimeout(t_img);callback()}else{isLoad = true;t_img = setTimeout(function(){isImgLoad(callback);},500)}
	        }
