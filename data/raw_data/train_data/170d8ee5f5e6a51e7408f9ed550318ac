					/*鼠标移过，左右按钮显示*/
					$(".buy_right").hover(function() {
						$(this).find(".prev,.next").fadeTo("show", 0.1);
					}, function() {
						$(this).find(".prev,.next").hide();
					})
					/*鼠标移过某个按钮 高亮显示*/
					$(".prev,.next").hover(function() {
						$(this).fadeTo("show", 0.7);
					}, function() {
						$(this).fadeTo("show", 0.1);
					})
					$(".buy_right").slide({
						titCell : ".num ul",
						mainCell : ".buy_rightpic",
						effect : "fold",
						autoPlay : true,
						delayTime : 700,
						autoPage : true
					});
