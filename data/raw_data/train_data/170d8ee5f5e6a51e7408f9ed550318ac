  <script type="text/javascript">

function Num(obj, num,blon) {
	var MyNum = obj.value;
	blon ? MyNum++ : MyNum--;
	MyNum = MyNum <= 1 ? 1 : MyNum;
	if(MyNum>num){
		alert("现产品剩余数量为"+num+"下单数量不能超过该数量");
		return;
	}
	obj.value = MyNum;
	MyNum > 1 ? document.getElementById('jian').disabled = false
			: document.getElementById('jian').disabled = true;
	
	sumVal();
}
function sumVal(){
var discountprice = $("#discountprice").val();
var num = $("#num").val();
var sum = discountprice * num;
$(".showTotal").html("￥"+sum);
}
	$(document).ready(function (){
		//初始化导航条CSS
		set_cur('.tab1');
		//滚动定位JS
		$.fn.smartFloat = function() {
		    var position = function(element) {
		        var top = element.position().top, pos = element.css("position");
		        $(window).scroll(function() {
		            var scrolls = $(this).scrollTop();
		            //滚动时保持导航置顶
		            if (scrolls > top) {
		                if (window.XMLHttpRequest) {
		                    element.css({
		                        position: "fixed",
		                        top: 0
		                    });    
		                } else {
		                    element.css({
		                        top: scrolls
		                    });    
		                }
		            }else {
		                element.css({
		                    position: pos,
		                    top: top
		                });    
		            }
		            //导航卡动态切换
		            var tab1_top = $('#tab1').offset().top;
		            var tab2_top = $('#tab2').offset().top;
		            var tab3_top = $('#tab3').offset().top;
		            var tab4_top = $('#tab4').offset().top;
		            if(scrolls >= tab4_top-40){
		            	set_cur('.tab4');
		            } else if(scrolls >= tab3_top-40) {
		            	set_cur('.tab3');
		            } else if(scrolls >= tab2_top-40) {
		            	set_cur('.tab2');
		            } else if(scrolls >= tab1_top) {
		            	set_cur('.tab1');
		            }
		        });
		    };
		    return $(this).each(function() {
		        position($(this));                         
		    });
		};
		//绑定
		$("#navigation").smartFloat();
		//风琴
		var aMenuOneLi = $(".menu-one > li");
		var aMenuTwo = $(".menu-two");
		$(".menu-one > li > .header").each(function (i) {
			$(this).click(function () {
				if ($(aMenuTwo[i]).css("display") == "block") {
					$(aMenuTwo[i]).slideUp(300);
					$(aMenuOneLi[i]).removeClass("menu-show")
				} else {
					for (var j = 0; j < aMenuTwo.length; j++) {
						$(aMenuTwo[j]).slideUp(300);
						$(aMenuOneLi[j]).removeClass("menu-show");
					}
					$(aMenuTwo[i]).slideDown(300);
					$(aMenuOneLi[i]).addClass("menu-show")
				}
			});
		});
	});
	
	//切换导航菜单
	function set_cur(n) {
		if($('.present_top a').hasClass('on')) {
			$('.present_top a').removeClass('on');
		}
		$('.present_top a' + n).addClass('on');
	}
	//页面定位
	function goTo(id){
		var top = $("#tab"+id).offset().top;
		if($("#navigation").css('position') == "fixed"){
			top = top - 40;
		}else{
			top = top - 82;
		}
		if(id>1){
			$("body,html").animate({ 
				scrollTop: top
			}, 0);
		}
		window.setTimeout("changeBtn("+id+")", 100);
	}
	function changeBtn(id){
		$('.present_top a').removeClass('on');
		$(".tab"+id).toggleClass("on");
	}
	
	//收藏产品
	function collection(proid){
		var userid = '';
		if(userid==''){
			$("div[class=touch_m]").show();
			$("#touch_bj").show();
		}else{
			var data={
				'userid':userid,
				'proid':proid
			};
			$.ajax({
				url:'saveCollection',
				data:data,
				type:'POST',
				success:function(data){
					if(data==2){
						alert("您已收藏该产品");
					}else if(data==1){
						alert("收藏成功");
					}else if(data==0){
						alert("连接超时");
					}
				}
			});
		}
	}
	
	//登录
	function loginCheck(){
		var loginName = $("#loginName").val();
		var loginPwd = $("#loginPwd").val();
		var yzm = $("#yzm").val();
		if( loginName.indexOf('@')<0){
			if(loginName.length!=11){
				alert('请输入正确的手机号！');
				return
			}
		}else{
			var myreg = /^([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+@([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+\.[a-zA-Z]{2,3}$/;
	        if(!myreg.test(loginName)){
	        	alert('请输入正确的邮箱！');
	            return false;
	        }
		}
		
		if(loginPwd==null || loginPwd==''){
			alert('请输入密码！');
			return;
		}
		if(yzm==null || yzm==''){
			alert('请输入验证码！');
			return;
		}
		
		var param= {
			"account":loginName,
			"password":loginPwd,
			"yzm":yzm,
		}
		
		$.ajax({
			type:'POST',
			url:"http://www.qlxcly.com/Login/login",
			dataType:'json',
			data:param,
			success:function(d){
				if(d.flag){
					window.location.href="http://www.qlxcly.com/product/p2020_0.html";
				}else{
					changeVery();
					alert(d.msg);
				}
			}
		})
	}
	
	function hideDiv(){
		$("div[class=touch_m]").hide();
		$("#touch_bj").hide();
		
	}
	function changeVery(){
		$("#veryImg").attr("src","http://www.qlxcly.com/Login/very/rand/"+Math.random());
	}
</script> 
