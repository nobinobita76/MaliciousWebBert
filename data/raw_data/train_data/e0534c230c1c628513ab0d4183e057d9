    $(document).ready(function(){
        //换进度
        $('.j_changeSchedule').off('tap').on('tap', function(){
            var segmentId = $(this).parents('li').prev().children('ul.minutia').last().attr('segment_id');
            var subjectId = $(this).attr('subject_id');
            var userId = $('.recommend_content').attr('user_id');
            window.location.href = "/customize/plan/video/schedule/"+userId+"/"+subjectId+"/"+segmentId+"?frompage=mesg#J_head";//跳转到具体的详情页面
        });
    });
</script>
