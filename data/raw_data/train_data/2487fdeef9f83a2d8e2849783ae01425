<script type="text/javascript">
$(function(){
	$("#jump").click(function(){
		var pageNum=$("#pageNumber").val();
		if(pageNum<=1){
			 window.location.href='http://wyc.gzjt.gov.cn/hygs/index.html';
		}
		else if(pageNum>=1){
			window.location.href='http://wyc.gzjt.gov.cn/hygs/index.html';
		}
		else if(pageNum>1&&pageNum<1){
			var link='http://wyc.gzjt.gov.cn/hygs/index.html';
			var prefix=link.substring(0,link.indexOf(".html"));
			var suffix="_"+pageNum+".html";
			window.location.href=prefix+suffix;
		}
	});
});
</script>
