                    var commentCountUrl ='http://comment.ifeng.com/get.php?job=1&format=js&p=1&pagesize=5&docurl='+_docUrl +'&field=a';
                }
                if (typeof __commentCount !== "undefined") {
                    renderCommentCount(__commentCount[0], __commentCount[1]);
                } else {
                    F.jsonp({
                        url: commentCountUrl,
                        data: {},
                        cache:true,
                        callback: "callbackGetFastCommentCount1",
                        success: function (result) {
                            renderCommentCount(result.count, result.join_count);
                        }
                    });
                }
                //console.log('__commentCount:' + __commentCount);
            }, "next", "获取评论数");
