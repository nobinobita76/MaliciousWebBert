      <script type="text/javascript">
		var i_hot = 0;
		function hot_turn() {
			var $content = $(".hot_content_contaner>div")
			var $dot = $(".hot_dot_contaner>span")
			var count = $content.length - 1;
			if (i_hot >= count) {
				i_hot = 0;
			} else {
				i_hot = i_hot + 1;
			}
			$content.eq(i_hot).fadeIn().siblings("div").hide();
			$dot.eq(i_hot).addClass("sel_span").siblings("span").removeClass("sel_span");
		}
 </script>
