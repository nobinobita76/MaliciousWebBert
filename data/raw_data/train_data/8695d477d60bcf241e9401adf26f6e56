function ietester() {
			var undef,ie,v = 3,div = document.createElement('div'),all = div.getElementsByTagName('i');
			while (
				div.innerHTML = '<!--[if gt IE ' + (++v) + ']><i></i><![endif]-->',all[0]
			);
			v > 4 ? ie = v : ie = undef;
			return ie;
		}
