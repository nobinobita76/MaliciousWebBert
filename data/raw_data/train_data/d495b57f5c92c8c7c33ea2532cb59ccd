$(function(){
	$('.message_data_list li').hover(function(){
		$(this).addClass("h");
	},function(){
		$(this).removeClass("h");
	});
	var navTimer=[];
	$("#userinfo_box").find("em,.UIbox").mouseover(function(){
		$("#userinfo_box").addClass("h");
		$("#userinfo_box").find(".UIbox").show();
	}).mouseleave(function(){
		$("#userinfo_box").removeClass("h");
		$("#userinfo_box").find(".UIbox").hide();
	});
	$(".n_message").find("em,.message_wrap").mouseover(function(){
		$(".n_message").addClass("h");
		$(".n_message").find(".message_wrap").show();
	});
	$(".n_message").mouseleave(function(){
		$(".n_message").removeClass("h");
		$(".n_message").find(".message_wrap").hide();
	});
	var tnav=$(".message_tabs dd");
	var tcon=$(".message_data");
	tnav.click(function(e) {
		var index = tnav.index(this);
		$(this).siblings().removeClass("on");
		$(this).addClass("on");
		if(!tcon==""){
			tcon.hide();
			tcon.eq(index).show();
		}
	});
});
