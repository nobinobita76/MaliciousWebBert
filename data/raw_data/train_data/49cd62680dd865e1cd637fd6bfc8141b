    <!-- 左右方向键翻页 -->
    <script type="text/javascript">
        function keydownJumpPge(e) {
            e = (e) ? e : event;
            var code = (e.charCode) ? e.charCode : ((e.keyCode) ? e.keyCode : ((e.which) ? e.which : 0));
            var tagSrc = e.srcElement ? e.srcElement : e.target;
            if ((code == 37 || code == 39) && tagSrc.tagName != 'INPUT' && tagSrc.tagName != 'TEXTAREA') {
                var the_pages = jQuery('.pagination a');
                if (the_pages.length > 0) {
                    var index = 0;
                    if (code == 39) {
                        index = the_pages.length - 2;
                    }
                    var pageUrl = the_pages[index].href;
                    if (code == 39 && pageUrl == "") {
                        pageUrl = the_pages[index + 1].href;
                    }
                    if (pageUrl != "") {
                        window.location.href = pageUrl;
                    }
                }
            }
        };
    </script>
