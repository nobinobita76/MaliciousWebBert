            $(function(){
                $('.art_info img').each(function(){
                	if($(this).width()==0 || $(this).height()==0){
                		return false;
                	}
                    var x = 630; 
                    var w=$(this).width(), h=$(this).height();
                    if (w > x) { 
                        var w_original=w, h_original=h;
                        h = parseInt(h * (x / w));
                        w = x; 
                    }
                    $(this).attr({width:w,height:h});
                });
            });
