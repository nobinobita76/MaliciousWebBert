   <script type="text/javascript">
	var bt = baidu.template; //百度模板
 	$(function(){
 		/****搜索关键词查询****/
 		$.ajax({
 			url : "/getKeyword",
 			type : "post",
 			dataType : "json",
 			success : function(data) {
 				var i = 0,
 					length = data.length,
 					html = "";
 				for (; i < length; i++) {
	 				html += '<a onclick="searchHotKey(\''+data[i].name+'\');" href="javascript:void(0);"';
 					if (data[i].highlight) {
 						//高亮
 						html +=	'style="color: #ff7900;"'; 
 					}		
 					html +=	'>'+data[i].name+"</a>"; 						
 				}
 				$("#searchKeyword").html(html);
 			},
 			error: function() {
 				//错误弹窗
 			}
 		});
 		
        updateCartShortcut();
 	    
        $('#inputKeyWords').bind('keypress',function(event){
            if(event.keyCode == "13"){
            	searchKeyWords();
            }
        });
        
        
        $.post("/findParentAndChildren", function (date) {
            var html = bt("category", date);
            $("#subNavCategory").html(html);
//             $("#subNavCategory #thirdTop").perfectScrollbar();
//             $("#subNavCategory #thirdBottom").perfectScrollbar();
//             $("img.lazy").lazyload({effect: "fadeIn"});
          /*   $(".subNavCategory").html(html);
            //三级导航溢出
            $(".subNavCategory #thirdLeft").perfectScrollbar();

            $(".subNavCategory #thirdRight").perfectScrollbar();
            $("img.lazy").lazyload({effect: "fadeIn"}); */
            
            $(".head .top .topNavWarp .nav .allWarp dd .allItem").each(function(){
        		$(this).hover(function(){
        			$(this).addClass("on");
        		},function(){
        			$(this).removeClass("on");
        		});
        	});
            $(".head .top .topNavWarp .nav .allWarp dt").mouseenter(function(){
            	$(".head .top .topNavWarp .nav .allWarp dd .allItem").eq(0).addClass("on");
            });
            //updated by qinzhongliang 2016/12/9：导航左右两侧高度一致，此方法定义在common.js中
            calcNavHeight();
        });
        
        
    	
        
        
 	});
	
	function updateCartShortcut(){
		 $.ajax({
	    		url : '/cart/updateCartShortcut',
	    		type:'post',
	        	async: false,
	        	success: function(data){
	        		if(null != data && data.items != null){
	        			var content ='';
	        			$.each(data.items,function(i,item){
	        				content +=
	        					'<dl class="clearfix">'
	        					+'<dt><a href="javascript:;"><img src="/'+item.image+'" width=68 height=69/></a></dt>'
	        					+'<dd>'
	        					+'<a href="javascript:void(0);">'
	        					+'<div class="shoppingInfo">'
	        					+'<p>'+item.title+'</p>'
	        					+'</div>'
	        					+'<div class="shoppingMoney">'
	        					+'<span class="money">￥<span>'+item.price+'</span></span><span class="amounts">x'+item.qty+'</span>'
	        					+'</div>'
	        					+'</a>'
	        					+'<a class="deletes"><input type="hidden" value="'+item.id+'"/><img src="/images/_delete.png" /></a>'
	        					+'</dd>'
	        					+'</dl>';
	        				
	        			})
	        			$('#cart_shortcut_div .shoppings .shopList').html(content);
	        			$('#cartGoodsNum').html(data.goodsCount);
	        			$('#cart_shortcut_div .shoppings .shopTotal .amountsTotal span').html(data.goodsCount);
	        			$('#cart_shortcut_div .shoppings .shopTotal .moneyTotal .money span').html(data.zprice);
	        		}else{
	        			
	        		}
	        	}
	        });
	}
 	
	function searchKeyWords(){
		$("#headKeyWords").val(encodeURI($("#inputKeyWords").val()));
		$("#searchKeyForm").submit();
	}
	
    function searchHotKey(hotKey){
    	$("#headKeyWords").val(encodeURI(hotKey));
    	$("#searchKeyForm").submit();
    }
    
    function goCatSearch(id, name){
		$("#headCat").val(id);	
		$("#headCatName").val(encodeURI(name));
		$("#searchCatForm").submit();
	}

    
    function searchOrder(type, order){
    	//clearAllSortField();
    	if(type == "complexTab"){
    		//$("#.....").val(order) ; //综合
    		//综合排序需要综合考虑,在后端做修改
    		$("#complexOrder").val(order)
    	}
    	else if(type == "saleTab"){
    		$("#saleOrder").val(order)  //销量
    	}
    	else if(type == "priceTab"){
    		$("#priceOrder").val(order)  //价格
    	}
    	else if(type == "clickTab"){
    		//点几率
    		$("#clickOrder").val(order)
    	}
    	else if(type == "reviewTab"){
        	 //评论数
    		$("#reviewOrder").val(order)
    	}
    	
    	$("#sortField").val(type);  //当前排序字段
    	var keywords = $('#inputKeyWords').val();
    	$('#keyWords').val(keywords);
    	$("#searchForm").submit(); 
    }
</script> 
