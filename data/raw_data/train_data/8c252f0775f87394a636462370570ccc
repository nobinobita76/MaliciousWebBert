   <script type="text/javascript">
		var contextPath = '';
		var areaBox = new AreaSelect({
			className:"areaSelect",
			queryStock : true
		});
		areaBox.init();
		$(function() {
			$('img.lazy').lazyload();
		})
        //配件加入购物车
        function addShoppingCar(obj){
        	var arr = [$("#goodsId").val()];
        	var count = 1;
        	var $li = $(obj).closest("li.clearfix");
        	$li.find("input[type='checkbox']").each(function(i){
        		if ($(this).is(':checked')) {
        			arr[count] = $(this).val();
        			count++;
        		}
        	})
        	$.ajax({
        		url:"/cart/add",
        		type:"post",
        		data:{"goodsId":arr},
        		traditional:true,
        		dataType:"json",
        		success:function(data){
        			$(".mask").show();
        			$(".succeeWarp").show();
        		}
        	})
        }
        
        //立即购买套餐
        function buyCombo(id){
        	$.ajax({
                url: '/user/isLogin',
                cache: false
            }).done(function (data) {
            	 if ('error' == data) {
                     $('.InformationPrompt').show();
                     $('.login').show();
                 } else {
                	 //window.location.href = '/order/orderConfirm?combineId='+id;
                	 var $form = $('<form method="post"></form>');
                     $('<input type="hidden" name="combineId">').val(id).appendTo($form);
                     $form.attr('action', '/order/orderConfirm').appendTo(document.body).submit().remove();
                 }
            });
        	
        }
        
        //提交发表咨询
        function saveAdvisory(){
        	$.ajax({
                url: '/user/isLogin',
                cache: false
            }).done(function (data) {
            	 if ('error' == data) {
                     $('.InformationPrompt').show();
                     $('.login').show();
                 } else {
                	var isValidate = $("#form1").validate();
         			if (!isValidate.checkForm()) {
         				isValidate.showErrors();
         				return false;
         			}
                	 
                	 $.ajax({
                		 url:"/member/advisory/saveAdvisory",
                		 type:"post",
                		 data:$("#form1").serialize(),
                		 dataType:"json",
                		 success:function(data){
                			 if (data.success) {
                				 window.location.reload();
                			 }
                		 }
                	 })
                 }
            });
        }
        
        //立即购买配件
        function buyAccessory(obj){
        	var arr = [$("#goodsId").val()];
        	var count = 1;
        	var nums = [1];
        	var $li = $(obj).closest("li.clearfix");
        	$li.find("input[type='checkbox']").each(function(i){
        		if ($(this).is(':checked')) {
        			arr[count] = $(this).val();
        			nums[count] = 1;
        			count++;
        		}
        	})
        	
        	$.ajax({
                url: '/user/isLogin',
                cache: false
            }).done(function (data) {
            	 if ('error' == data) {
                     $('.InformationPrompt').show();
                     $('.login').show();
                 } else {
                	 //window.location.href = '/order/orderConfirm?combineId='+id;
                	 var $form = $('<form method="post"></form>');
                     $('<input type="hidden" name="ids">').val(arr.join(',')).appendTo($form);
                     $('<input type="hidden" name="nums">').val(nums.join(',')).appendTo($form);
                     $form.attr('action', '/order/orderConfirm').appendTo(document.body).submit().remove();
                 }
            });
        	
        	
        	/* $.ajax({
        		url:"/order/orderConfirm",
        		type:"post",
        		data:{"ids":arr},
        		traditional:true,
        		dataType:"json",
        		success:function(data){
//         			$(".mask").show();
//         			$(".succeeWarp").show();
        		}
        	}) */
        }
        
        $(".choose input[type='checkbox']").click(function(){
        	//计算选择的商品价格
        	reset(this);
        })
        
        function reset(obj){
        	var count = 0;
        	var price = $("#goodsPrice").val() *1;
        	$(".choose").find("input[type='checkbox']").each(function(){
        		if ($(this).is(':checked')) {
        			count += 1;
        			price = (price) + ($(this).parent().find("input[name='price']").val() *1);
        		}
        	})
        	$(obj).closest("li.clearfix").find("div[class='selected'] span").text(count);
        	$(obj).closest("li.clearfix").find("div[class='p-price'] span").text(price.toFixed(2));
        }
        
        function loadAddress(){
        	//当前默认配送地址
        	var cookieAreaId = '310106';
        	var addressDetailName = "请选择";
        	$.ajax({
     			url : "/area/getAddressNameByAreaId",
     			type : "post",
     			data : {"areaId": cookieAreaId},
     			dataType : "json",
     			success : function(data){
     				addressDetailName = data.addressDetailName;
        			$(".areaBox .checked").text(addressDetailName);
		            var stockInfo = queryGoodsStock(cookieAreaId);
		            var stock = stockInfo.stock;
                 	if(stock == -999){
                 		stock = 999; //专属管家
                 	}
                 	
               	 	var msg = stockInfo.msg || "";
               	 	$("#stockNum").html(stock);
               	 	$("#stockMsg").css({"display":"inline","color":"#939393"}).html(msg);
     			}
     		});
        }
        
            //图片放大镜效果
            $(function () {
