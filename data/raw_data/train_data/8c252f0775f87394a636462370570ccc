   <script type="text/javascript">
	 $(function () {
		 var aurl = window.location.href ;
		 if(aurl.indexOf("goods/search") != -1||aurl.indexOf("goods/goodsDetail") != -1||aurl.indexOf("main") != -1) {
				$(".mainNav>ul li:eq(1)").addClass("on");
		 }else if(aurl.indexOf("releaseProject/index") != -1  ||aurl.indexOf("releaseProject/detail") != -1) {
				$(".mainNav>ul li:eq(2)").addClass("on");
	     }else if(aurl.indexOf("purchaseServiceHome/index") != -1  
	    		 ||aurl.indexOf("purchaseServiceHome/detail") != -1
	    		 ||aurl.indexOf("purchaseService/dispatch") != -1
	    		 ||aurl.indexOf("purchaseService/orderConfirm") != -1
	    		 ||aurl.indexOf("purchaseService/findNotSerOrder") != -1
	    		 ||aurl.indexOf("purchaseService/dispatchWaitOrder") != -1
	    		 ||aurl.indexOf("annualDemand/dispatch") != -1
	    		 ||aurl.indexOf("annualDemand/confirm") != -1
	    		 ||aurl.indexOf("annualDemand/dispatchWaitOrder") != -1
	    		 ||aurl.indexOf("protocolDispatch/dispatch") != -1
	    		 ||aurl.indexOf("protocolDispatch/orderConfirm") != -1
	    		 ||aurl.indexOf("protocolDispatch/dispatchWaitOrder") != -1
	    		 ||aurl.indexOf("residentService/dispatch") != -1
	    		 ||aurl.indexOf("residentService/orderConfirm") != -1
	    		 ||aurl.indexOf("residentService/dispatchWaitOrder") != -1) {
				$(".mainNav>ul li:eq(3)").addClass("on");
	     }else if(aurl.indexOf("member/payForMembership/") != -1) {
				$(".mainNav>ul li:eq(4)").addClass("on");
	     }else if(aurl.indexOf("recharge/flowPackage") != -1  
	    		 ||aurl.indexOf("recharge/paymentSuccess") != -1
	    		 ||aurl.indexOf("recharge/paymentFail") != -1) {
				$(".mainNav>ul li:eq(5)").addClass("on");
	     }else {
	    	  $(".mainNav>ul li:eq(0)").addClass("on");
	     }


	 });

</script> 
