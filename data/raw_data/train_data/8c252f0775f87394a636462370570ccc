                $(".jqzoom").jqueryzoom({ xzoom: 470, yzoom: 470 });
                
                //点击小图更变放大镜
                $(".smallImages").each(function(i){
                	$(this).on("click", function(){
                		var imgPath = $(this).attr("data");
                		$("#magnifier").attr("jqimg",imgPath);
                	});
                });
                
