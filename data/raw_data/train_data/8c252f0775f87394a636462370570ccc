            $(function () {
            	//updated by qinzhongliang 2017/1/20：注释掉安装服务
            	//是否显示安装
            	/* if('false' == 'true'){
            		$("#installDiv").show(); 
            	} */
            	
            	//隐藏规格参数
             	if('{主体={品牌=华三（H3C）, 型号=SecPath F100-A-SI , 类型=防火墙&VPN}}' == ''){ 
            		$("#paramTab").hide();
            	}
            	
                //产品详细页轮换
                $("#Pro_Slide").Slide({
                    effect: "scroolLoop",
                    autoPlay: false,
                    speed: "normal",
                    timer: 3000,
                    steps: 1
                });

                //第一个缩略图加on
                $("#Pro_Slide li:first").addClass("on");
                //取第一张图片显示
                $("#Pro_Slide li:first img").load(function () {
                    $(".ProDetail_Pic dt img").attr({ "src": $("#Pro_Slide li:first img").attr("data"), "width": 470 });
                });
                $("#Pro_Slide li").click(function () {
                    $(this).addClass("on").siblings().removeClass("on");
                    var src = $(this).find("img").attr("data");
                    $(".ProDetail_Pic dt img").attr("src", src);
                });
                var sum = $("#Pro_Slide").find("li").size();
                if (sum < 6) {
                    $("#JQ-slide-nav").hide();
                }
                //产品详细页轮换 end

                //购物数量加减
                var sn = $(".count-number");
                $(".countup").click(function () {
                    sn.val(parseInt(sn.val()) + 1);
                });
                $(".countdown").click(function () {
                    if (sn.val() > 1) {
                        sn.val(parseInt(sn.val()) - 1);
                    }
                });
                //配件及套餐 选项卡
                $(".comboWarp ul.comboWarpNav li").each(function () {
                    $(this).click(function () {
                        $(this).addClass("on").siblings("li").removeClass("on");
                        var index = $(this).index();
                        $(this).parent("ul").siblings(".combo").eq(index).show().siblings(".combo").hide();
                        $('img.lazy').lazyload();
                    });
                });
                //套餐一二 选项卡
                $(".comboWarp ul.comboNav li").each(function () {
                    $(this).click(function () {
                        $(this).addClass("on").siblings("li").removeClass("on");
                        var index = $(this).index();
                        $(this).parent(".comboNav").siblings(".comboBox").find("li").eq(index).show().siblings("li").hide();
                        //清空配件选中及价格
                        var $t = $(this).parent(".comboNav").siblings(".comboBox").find("li[style='display: none;']").find("input[type='checkbox']");
                        $t.removeAttr("checked");
                        reset($t);
                        $('img.lazy').lazyload();
//                         $t.parent().parent().parent().find("div[class='selected'] span").text(0);
//                         $t.parent().parent().parent().find("div[class='p-price'] span").text($("#goodsPrice").val());
                    });
                });
                //触发路由器排行榜
                $(".m2.comparison ul.tab li").each(function () {
                    $(this).mouseenter(function () {
                        $(this).addClass("curr").siblings("li").removeClass("curr");
                        var index = $(this).index();
                        $(this).parent("ul").siblings(".tabcon").eq(index).show().siblings(".tabcon").hide();
                        $('img.lazy').lazyload();
                    });
                });
                //商品详情 规格参数 商品评价 安装指导 选项卡
                $(".selectTab li").each(function () {
                    if ($(".selectTab li").eq(2).hasClass("on")) {
                        $(".commentWarp .mt").hide();
                    }
                    $(this).click(function () {
                        $(this).addClass("on").siblings("li").removeClass("on");
                        var index = $(this).index();
                        $(this).parent("ul").siblings(".tabWarp").find("li").eq(index).show().siblings("li").hide();
                        if ($(".selectTab li").eq(2).hasClass("on")) {
                            $(".commentWarp .mt").hide();
                        } else {
                            $(".commentWarp .mt").show();
                        }
                    });

                });
                
                //joinBtn 点击出现添加成功弹窗
       /*          $(".joinBtn").on("click", function () {
                    popShow("succeeWarp");
                }); */
                
                
                
                //立即购买
                $("#immediately").on("click",function(){
                	  $.ajax({
                          url: '' + '/user/isLogin',
                          cache: false
                      }).done(function (data) {
                          if ('error' == data) {
                              $('.InformationPrompt').show();
                              $('.login').show();
                          } else {
                        	  //已登录
                             /* var stockNum = $("#stockNum").html();
                             if(stockNum < 1){
                             	$("#stockMsg").css("color","red");
                          		$("#immediately").css("cursor","not-allowed");
                            	return false;
                             }else{
	                       		$("#immediately").css("cursor","pointer");
                             } */
                             //updated by qinzhongliang：若当地区域库存不足（数据库中实际库存，而非页面展示的库存），则不能跳转到“提交订单”页面
                             var realStock = queryStockByArea();
                             var selectStock = $("#goodCountId").val();
                             if(realStock < 1 || realStock < selectStock){
                             	/* $("#stockMsg").css("color","red");
                           		$("#immediately").css("cursor","not-allowed"); */
                           		layer.msg('库存不足');
                             	return false;
                              } else{
 	                       		 $("#immediately").css("cursor","pointer");
                              }
                             
                        	  
		                	 var $form = $('<form method="post"></form>');
		                     $('<input type="hidden" name="ids">').val('959').appendTo($form);
		                     $('<input type="hidden" name="nums">').val($("#goodCountId").val()).appendTo($form);
		                     $('<input type="hidden" name="isInstall">').val($("#isInstall").val()).appendTo($form);
		                     $form.attr('action', "/order/orderConfirm").appendTo(document.body).submit().remove();
                          }
                      })
                });
                
                //单品 加入购物车
                $("#addCart").on("click", function(){
                	var stockNum = $("#stockNum").html();
                	if(stockNum < 1){
                		$("#stockMsg").css("color","red");
                		return false;
                	}
                	
                	var id = '959';
                	var num = $("#goodCountId").val();
                	var isInstall = $('#isInstall').val();
                	$.ajax({
             			url : "/cart/add",
             			type : "post",
             			data : {"goodsId": id, "goodsNum" : num,"isInstall" : isInstall},
             			dataType : "json",
             			success : function(data){
             				popShow("succeeWarp");
             				updateCartShortcut();
             			}
             		});
                	
                });
                
                //点击关闭 弹窗关闭
                $(".closes").on("click", function () {
                    popHide("succeeWarp");
                    popHide("collectWarp");

                });
                
                //点击继续购物 弹窗关闭
                $(".continue").on("click", function () {
                    popHide("succeeWarp");
                });
                
                $(".selectBox").find("a").each(function(){
                	var spec = $(this).attr("data-value"); //显示的规格ID
                	var svStr = '';
   					var spec_Array = svStr.split(";"); //该商品所拥有的所有规格id
   					if(spec_Array.indexOf(spec) != -1){
   						$(this).addClass("on");
   					}
                });
                
                
                $(".selectInfoWarp .selectWarp .selectBox").find("a").each(function () {
                    $(this).click(function () {
                        if ($(this).hasClass("on")) {
                            //点击当前已选中的，无变化
                        } else {
                        	$(this).addClass("on").siblings().removeClass("on");
                			var goodsId = findGoodsBySpec();
                			window.location.href='/goods/goodsDetail?goodsId=' + goodsId;
                        }
                    });
                });
                
                //点击安装
                $(".serviceWarp").find("a").each(function () {
                    $(this).click(function () {
                    	var goodsId = $('#goodsId').val();
                    	
                    	/* $ajax({
                    		url: 'findInstall',
                            type: 'post',
                            async: false,
                            data: {"specStr" : spec,"productId" : productId},
                            dataType: 'json',
                            success: function(data) {
                               goodsId = data;
                            }
                    	}) */
                    	var goodsPrice = $('#goodsPrice').val();
                    	var installPrice = $('#installPrice').val();
                    	var totalPrice = (parseFloat(goodsPrice) + parseFloat(installPrice)).toFixed(2);
                    	var $isInstall = $("#isInstall");
                        if ($(this).hasClass("on")) {
                            $(this).removeClass("on");
                            $('.memberPrise .member span').text('￥'+goodsPrice);
                            $isInstall.val(0);
                        } else {
                            $(this).addClass("on");
                            $('.memberPrise .member span').text('￥'+totalPrice);
                            $isInstall.val(1);
                        }
                        
                        
                    });
                });
                
                //去掉评论最下面的线
                $(".com-table-main .comments-item:last-child").css("border-bottom", "none");
                //点击发表咨询出现发表内容
                $(".consultAsk a").click(function () {
                    $(".reviewWarp").show();
                });
                
            	$('.InformationPrompt .close3').click(function(){
        			$('.InformationPrompt').hide();
        		});
            	
            	//加载库存与地址
            	loadAddress();
            	
            });
            
            
            function findGoodsBySpec(){
    			var selectSpecArray = new Array(); //已选中的
            	$(".selectInfoWarp .selectWarp .selectBox").find("a").each(function(){
            		//遍历所有，取选中值
            		if ($(this).hasClass("on")) {
                        var specValue = $(this).attr("data-value");
                        selectSpecArray.push(specValue);
                    } 
            	});
            	
            	var spec = selectSpecArray.join(";"); 
            	var productId = '484'; 
            	var goodsId = null;
                $.ajax({
                    url: 'findGoodsBySpec',
                    type: 'post',
                    async: false,
                    data: {"specStr" : spec,"productId" : productId},
                    dataType: 'json',
                    success: function(data) {
                       goodsId = data;
                    }
                });
                
                return goodsId;
            }
            
			
			function queryGoodsStock(areaId){
				var goodsId = $("#goodsId").val();
				var isInstall = $("#isInstall").val() == 1 ? true : false;
				var stockInfo = null;
				$.ajax({
                     url : '' + "/dealerGoods/queryStockByAddress",
                     type : 'post',
                     data: {"goodsId": goodsId , "isInstall":isInstall ,"areaId":areaId},
                     async: false,
                     success: function(data){
                    	 stockInfo = data;
                         var stock = stockInfo.stock;
                      	 if(stock <= 0){
                      		//无货时改变"加入购物车"鼠标样式
                      	 	$("#addCart").css("cursor","not-allowed");
                      	 	$("#immediately").css("cursor","not-allowed");
                      	 }else{
                      		$("#addCart").css("cursor","pointer");
                      		$("#immediately").css("cursor","pointer");
                      	 }
                     }
                });
				return stockInfo;
			}
			
			//updated by qinzhonglaing 2016/1/3：获取当前区域下的库存
			function queryStockByArea(){
				var cookieAreaId = '310106';
				var goodsId = $("#goodsId").val();
				var isInstall = $("#isInstall").val() == 1 ? true : false;
				var stock = 0;
				$.ajax({
                     url : '' + "/dealerGoods/queryStockByAddress",
                     type : 'post',
                     data: {"goodsId": goodsId , "isInstall":isInstall ,"areaId":cookieAreaId},
                     async: false,
                     success: function(data){ stock = data.stock; }
                });
				return stock;
			}
			
			
			
			function queryIsInstall(areaId){
				var isQuery = 'false';
				if(isQuery){
					//仅商品支持安装服务时查询
					var goodsId = $("#goodsId").val();
					$.ajax({
	                     url : '' + "/dealerGoods/queryIsInstall",
	                     type : 'post',
	                     data: {"goodsId": goodsId ,"areaId":areaId},
	                     async: false,
	                     success: function(data){
	                     	var isInstall = data.isInstall;
	                        if(isInstall){
	                        	$("#installDiv").show();
	                        }else{
	                        	$("#installDiv").hide();
	                        }
	                     }
	                });
				}
			}
			
			function reflushGuessULike(){
				var bt=baidu.template;
                //设置左分隔符为 <!
                baidu.template.LEFT_DELIMITER='<!';
                //设置右分隔符为 <!
                baidu.template.RIGHT_DELIMITER='!>';
               
                    $.ajax({
                        url:"/goods/guessYouLove",
                        type: "POST",
                        data: {},
                        success: function(data){
                           // var data = page.rows;
                            if(data){
                            	$("#ul_guLike").html("");
                                $.each(data,function(i,e){//debugger;
                                    var a={
                                        id: e.id,
                                        title: e.title,
                                        image: e.image,
                                        price:e.price
                                    //    startDate:new Date(e.startDate).Format("yyyy.MM.dd"),
                                       // endDate: new Date(e.endDate).Format("yyyy.MM.dd"),
                                    }
                                    var html=bt('t_gULike', a);
                                    
                                    $("#ul_guLike").append(html);
                                    $('img.lazy').lazyload();
                                });
                               
                            }
                            /* if(page.lastPage){
                                $(".more_btn").hide();
                            }else{
                                page+=1;
                            } */
                        },
                         error:function(page){
                            alert("获取失败.请稍候再试");
                        } 
                    })
                    
			}
			
			function gobrandSearch(id, name){
				$("#brandId").val(id);	
				$("#searchBrandForm #brandName").val(name);
				$("#searchBrandForm").submit();
			}
			function gocategorySearch(id, name){
				$("#categoryId").val(id);	
				$("#categoryName").val(encodeURI(name));
				$("#searchCategoryForm").submit();
			}
        </script> 
  </div> 
