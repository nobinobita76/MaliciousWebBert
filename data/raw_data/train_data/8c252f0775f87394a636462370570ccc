    <script type="text/javascript">
    	$(function(){
    		$.ajax({
    			url: "/foot/link",
    			type: "post",
    			dataType: "json",
    			success: function(data) {
    				var icp = data.icp ? data.icp.value : false,
    					telephone = data.telephone ? data.telephone.value : false;
    					aboutList = data.aboutList ? data.aboutList : false;
    					websiteLogo = data.websiteLogo ? data.websiteLogo.value : false;
    				if (icp) {
    					$("#icp").html("<a style='color: #8a8a8a;' href='http://www.miitbeian.gov.cn/state/outPortal/loginPortal.action' target='_blank'>" + icp + "</a>");					
    				}
    				if (telephone) {
    					$("#footTel").text(telephone);
    					//将headBar中的电话也改掉
    					$("#headTel").text(telephone);
    				}
    				if (websiteLogo) {
    					$("#websiteLogoId").attr("src","/"+websiteLogo);
    				}
    				if(aboutList) {
    					var htmlStr = "";
    					for(var i = 0;i < aboutList.length; i++) {
    						var id = aboutList[i].id;
    						htmlStr += "<a href='/foot/link/about/" + id + "'>"+ aboutList[i].title +"</a> |";
    					}
    					$(".footLink").prepend(htmlStr);
    				}
    			},
    			error: function() {
    				//错误弹窗
    			}
    		});
    	});
    </script>
   </div> 
