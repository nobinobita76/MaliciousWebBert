   <script type="text/javascript">
	$(function(){
		$.ajax({
			url : "/help/getFootHelp",
			type: "post",
			dataType: "json",
			success: function(data) {
				var i = 0,
					helpCategory = data.helpCategory,
					wechatQRCode = data.wechatQRCode ? data.wechatQRCode.value : false,
					qqQRCode = data.qqQRCode ? data.qqQRCode.value : false,
					defaultPic = data.defaultPic ? data.defaultPic.value : false,
					html = "",
					categoryLength = helpCategory.length || 0;
				for(; i < categoryLength; i++) {
					var j = 0,
						category = helpCategory[i].articleList,
						length = category.length;
					html += "<dl>"
			             +   	"<dt>"+helpCategory[i].name+"</dt>";
					for(; j < length; j++) {
						html += 	"<dd>"
				             +          "<a href='/help/help/"+category[j].id+"'>"+category[j].title+"</a>";
				             +		"</dd>"
					}
					html +=  "</dl>"	
				}
				$("#footHelp").html(html);
				//图片问题，如果有二维码，显示二维码，否则如果有默认图片，显示默认图片，否则显示现在的图片
				if (wechatQRCode) {
					$("#wechatQRCode").attr("src", "/"+wechatQRCode);					
				} else if (defaultPic) {
					$("#wechatQRCode").attr("src", "/"+defaultPic);	
				}
				if (qqQRCode) {
					$("#qqQRCode").attr("src", "/"+qqQRCode);					
				} else if (defaultPic) {
					$("#qqQRCode").attr("src", "/"+defaultPic);
				}
			},
			error: function() {
				//错误弹窗
			}
		});
	});
</script>
