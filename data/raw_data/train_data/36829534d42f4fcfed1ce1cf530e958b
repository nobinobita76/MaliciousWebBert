            });

            $('.downimgupword-open-modal').click(function(e){
                e.stopPropagation();
                $('.panel-download h3').html($(this).data('title'));
                $('.panel-download p').html($(this).data('desc'));
                $('.panel-download p').attr('data-artId',$(this).data('artid'));
                $('.panel-download p').attr('data-artTag',$(this).data('arttag'));
                $('.panel-download p').attr('data-type',$(this).data('type'));
                $('.panel-wrap').css({
