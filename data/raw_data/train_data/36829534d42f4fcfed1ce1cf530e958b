            $('.leftImgRightWord-modal').click(function(e){
                e.stopPropagation();
                var artId = $(this).find(".righttxt").attr('data-artId');
                var artTag = $(this).find(".righttxt").attr('data-artTag');
                var downType = $(this).find(".righttxt").attr('data-type');
                var artType = $('#artType').val();

                var pageId = $('#pageId').val();
                var myUrl = 'http://qdtg.eastday.com/pages/download?type='+artType+'&pageid='+pageId+'&tag='+artTag+'&qid='+qid+'&downType='+downType;
                sendLog(artId,'tips',myUrl);
                setTimeout(function(){
                    location.href = myUrl;
                }, 100);
            });

            var newid = request.QueryString('newid');
            if (newid) {
                var browser_type = 'html';
                var agent =navigator.userAgent.toLowerCase();
                if (agent.indexOf("micromessenger") > 0) {
                    browser_type = "WeChat";
                }

                $("#tempListContainer,#mainListContainer").each(function() {
                    $(this).hide();
                });
                $.get('http://qdtg.eastday.com/union/getnew?id='+newid+'&type='+browser_type,function(data,status){
                    $('header').after(data);
                    var has = 0;
                    $(".download-tip").each(function(){
                        has++;
                        if (has > 1) {
                            $(this).remove()
                        }
                    });
                    $('title').html($('.newHtml h2').text());

                    $('.images-list-box').on('click', function(){
                        $(this).next().show();
                        $(this).parent().siblings().children('.content').hide();
                    });
                    $('.images-list-box').eq(0).parent().siblings().children('.content').hide();

                });
            }
        }

        $(document).ready(function () {
            try {
                onDocumentReady();
            } catch (e) {
                console.log(e);
            }
        });
