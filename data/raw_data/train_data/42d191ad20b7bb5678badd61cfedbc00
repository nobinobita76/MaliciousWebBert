    <script type="text/javascript">
        var nGapTime = 1491086931788 - new Date().getTime(),
        isGlobalExpand = !!'';
        var ghtRmcPlayerControlOption = {
            visible : {
                logo : false
                , expand : true
                , scrap : false
            }
            , clicked : {
                expand : !!''
            }
        };
        var nowPlayClipNo = "1552522";
        var isMT = Boolean(false);
        var isUHD = Boolean(false);
        var TechType = nhn.rmcnmv.TechType.AUTO;
        
        
			
        
			
        

        if (isMT) {
            TechType = nhn.rmcnmv.TechType.FLASH;
        }

        

        var NFM_cookie = jindo.$Cookie().get("NFM_TV");
        var rmcPlayer = new nhn.rmcnmv.RMCVideoPlayer("C61D8B9F582ED7599E11216FDDAA4E7CE478", "V126a2bcc120e3ce62fe1d9da3dc67eb9f2d61c698180175ef94a218278996e967eb0d9da3dc67eb9f2d6", 0, 0, "2010", TechType, "http://serviceapi.rmcnmv.naver.com");
        rmcPlayer.properties.wmode = "window";
        
        
        
        rmcPlayer.properties.autoPlay = "true";
        
        


        rmcPlayer.properties.skinName = "tvcast_white";
        rmcPlayer.properties.coverImageURL = 'http://tvcast1.phinf.naver.net/20170328_3/FVgrg_1490697638832mIfLW_JPEG/50000615500.jpg?type=mfullfill_880_495';
        rmcPlayer.properties.isResizableCoverImage = true;
        rmcPlayer.properties.hasRelativeMovie = true;
        rmcPlayer.properties.isP2P = "false";
        
        rmcPlayer.properties.callbackHandler = "initCallbackHandler";
        rmcPlayer.properties.cassiodServiceID = "2010";
        rmcPlayer.properties.controlBarMovable = true;
        rmcPlayer.properties.nsc = "tvcast.end";
        rmcPlayer.properties.jsCallable="true";
        rmcPlayer.properties.apiAd="http://ad.nmv.naver.com/ad";
        rmcPlayer.properties.protocol="rtmpe";
        rmcPlayer.properties.controls = ghtRmcPlayerControlOption;
        rmcPlayer.properties.advertiseInfo='[{"adSystem":"AMS"}]';
        rmcPlayer.properties.advertiseUrl="http://ams.rmcnmv.naver.com/item/create/2010/SMR?ams_ctgr=CULTU&ams_chnl=live_2&ams_videoId=C61D8B9F582ED7599E11216FDDAA4E7CE478&ams_cp=216&navervodtype=RMC&programid=S01_V2000007270&clipid=S01_80000322422&adlink=cpid%3DC1%26channelid%3DS01%26category%3D01%26section%3D04%26programid%3DS01_V2000007270%26clipid%3DS01_80000322422%26contentnumber%3D0%26targetnation%3D%26isonair%3DY%26ispay%3DN%26vodtype%3DC%26broaddate%3D%26playtime%3D154%26starttime%3D0%26endtime%3D0";
        rmcPlayer.properties.vendor = 'SMR';
        rmcPlayer.properties.countryCode = 'CN';
        rmcPlayer.properties.interfaceLang = 'en_US';

        var milliTime = parseInt((new Date().getTime() - nGapTime)/1000, 10);
        for ( var i = milliTime + 2, nLen = milliTime - 3 ; i > nLen ; i-- ){
            try{
                if( window.localStorage.getItem("searchTime") == i ){
                    rmcPlayer.properties.continuedInfo = window.localStorage.getItem("contInfo");
                    window.localStorage.removeItem("contInfo");
                    window.localStorage.removeItem("searchTime");
                    break;
                }
            }catch (e) {}
        }
        
        rmcPlayer.properties.externalTrackingData = {"type":"SMR","params":{"adlink":"cpid=C1&channelid=S01&category=01&section=04&programid=S01_V2000007270&clipid=S01_80000322422&contentnumber=0&targetnation=&isonair=Y&ispay=N&vodtype=C&broaddate=&playtime=154&starttime=0&endtime=0","uuid":"","clipid":"S01_80000322422","programid":"S01_V2000007270"}};
        


        
        if (false) {
            if(false) {
                rmcPlayer.setNotAllowPlay('해당 기기에선 재생이 지원되지 않습니다.<br/>양해 부탁드리며 PC에서 감상해주시기 바랍니다.');
            }
        }

        
        rmcPlayer.properties.wmode = "window";
        rmcPlayer.displayRMCPlayer(document.getElementById("player"));
        

        
        if  (false) {
            var videoPlayer = new VideoPlayer(rmcPlayer, {
                p2p : true,
                isGlobal : true,
                playerLayer : "player",
                installLayer : "install",
                notSupportedLayer : "notSupported",
                macOsNotSupportedLayer : "macOsNotSupported",
                globalNotSupportedLayer : "globalNotSupported",
