        function diagnosisPopup() {
            window.open('/special/p2p/help/cassiodDiagnosis','cassiodDiagnosisPopup','width=620,height=502, scrollbars=yes, resizable=yes');
        }

        function helpPopup() {
            window.open('https://help.naver.com/support/contents/contents.nhn?serviceNo=705&categoryNo=6030','cassiodHelpPopup','width=1008,height=800, scrollbars=yes, resizable=yes');
        }

        

        var playLoggerOptions = {
            apiDomain : 'http://logapi.tvcast.naver.com',
            clipNo : '1552522',
            playlistNo : '109254',
            beforeClipNo : jindo.$Cookie().get("NTC_BEFORE_CLIP"),
            device : 'PC'
        };

        var oPlaybackOrderController = {};

        window.tvcast = (typeof window.tvcast !== "undefined") ? window.tvcast : {};
        tvcast.PlayLogger = new PlayLogger(playLoggerOptions);

        tvcast.pc.PlayTimeLogger.logPlayTime({
            apiDomain: 'http://logapi.tvcast.naver.com',
            rmcObjId: rmcPlayer.properties.objId,
            clipNo : '1552522',
            playlistNo: '109254'
        });     
        jindo.$Cookie().remove("NTC_BEFORE_CLIP");

        function initCallbackHandler(sCallbackType, pid, id, data) {
            var playTypeCookie = "",
                    autoPlayFlag = false;

            try {
                oPlaybackOrderController = tvcast.pc.ClipEnd.getClipEndPlaylist().getPlaybackOrderController() || null;
                // 후광고 완료 시
                /**
                 *  arguments
                 *      0 : [String] callbackType
                 *      1 : [String] flashID
                 *      2 : [Boolean] isFullscreen
                 */
                if (sCallbackType === 'ad_complete') {
                    playTypeCookie = oPlaybackOrderController.getCurrentType().sCookie;
                    
                    if(oPlaybackOrderController.isRepeat()){
                 	   jindo.$Cookie().set("NTC_BEFORE_CLIP", ghtEnv.nClipNo, 0);
                	}
                    
                    autoPlayFlag = arguments && arguments[2];

                    if ( autoPlayFlag && playTypeCookie == oPlaybackOrderController.TYPE.REPEAT_PLAYLIST.sCookie ) {
                        getNextClip();
                    }
                    else if ( autoPlayFlag && playTypeCookie == oPlaybackOrderController.TYPE.REPEAT_CLIP.sCookie) {
                        playOtherClip('1552522');
                    }
                    else {
					    oPlaybackOrderController.movePage();				   
                    }
                }

                if(sCallbackType == "beginfullscreen"){
                    if(window.ghtEnv){
                        window.ghtEnv.beginFullscreen = true;
                    }
                }
                // 다시 보기 선택 시
                if (sCallbackType === 'replay') {
                    oPlaybackOrderController.increasePlayCountCookie();
                    oPlaybackOrderController.moveCurrentPage();
                }


            } catch(e) { }

            if(sCallbackType == "nclick"){
                data && clickcr && clickcr("", data, "", "", "");
            }

            // 전체화면 종료
            /**
             *  arguments
             *      0 : [String] callbackType
             *      1 : [String] flashID
             *      2 : [Boolean] continuedInfo     페이지 갱신 후 플래시쪽에 전달해야할 값.
             */
            if (sCallbackType === 'endfullscreen'){
                if( arguments && arguments[2] ){

                    try {
                        var timeMilli = parseInt((new Date().getTime() - nGapTime) / 1000, 10);
                        window.localStorage.setItem("contInfo", arguments[2]);
                        window.localStorage.setItem("searchTime", timeMilli);
                    }catch(e){}
                    
                    if( ghtEnv.nClipNo != nowPlayClipNo){
                        location.href= "/v/"+nowPlayClipNo+"/list/"+ghtEnv.nPlaylistNo;
                    }

                    if(window.ghtEnv) {
                        window.ghtEnv.beginFullscreen = false;
                    }
                }
            }

            // 재생 완료
            if (sCallbackType === 'complete') {
                tvcast.PlayLogger.logCompletePlay();
            }

            // 재생 시작
            if (sCallbackType === 'play') {
                tvcast.PlayLogger.logStartPlay();
                MyPage.addMyPlaylist('http://tv.naver.com', 'recent', '1552522');
            }
        }

        function getNextClip(){
            continuousPlayList.playIndex++;
            if(continuousPlayList.list.length <= continuousPlayList.playIndex){
                continuousPlayList.playIndex = 0;
            }

            ghtEnv.nNextClipNo = continuousPlayList.list[continuousPlayList.playIndex].clipNo;
            // ghtEnv.nClipNo=ghtEnv.nNextClipNo;

            if(continuousPlayList.list[continuousPlayList.playIndex].isAdult == "Y" && ghtEnv.isPlayableAuth){
                rmcPlayer.getElement().closeVideo();
                location.href= "/v/"+ ghtEnv.nNextClipNo +"/list/"+ghtEnv.nPlaylistNo;
                return;
            }

            playOtherClip();
        }
        function playOtherClip(clipNo){
            var clipNo = clipNo || (ghtEnv && ghtEnv.nNextClipNo),
                    playList = ghtEnv && ghtEnv.nNextPlaylistNo;

            if(clipNo){
                new jindo.$Ajax("/api/clipPlayInfo/"+ clipNo +"/list/" + playList, {
                    type : "xhr",
                    method : "get",
                    timeout : 2,
                    onload : function(e){
                        var retData = e.json();
                        nowPlayClipNo = clipNo;
                        
                        tvcast.PlayLogger.reset(nowPlayClipNo, playList, jindo.$Cookie().get("NTC_BEFORE_CLIP"));
                        jindo.$Cookie().remove("NTC_BEFORE_CLIP");

                        if (!!retData.smrTrackingData) {
                            rmcPlayer.properties.externalTrackingData = jindo.$Json(retData.smrTrackingData).toObject();
                        } else {
                            rmcPlayer.properties.externalTrackingData = null;
                        }

                        rmcPlayer.updatePlayInfo(retData.videoId, retData.inKey, {
                            "advertiseInfo" : retData.advertiseInfo,
                            "advertiseUrl": retData.advertiseUrl
                        });
                        rmcPlayer.getElement().playNewVideo({
                            "playapi" : rmcPlayer.getPlayAPIUrl(),
                            "coverImageURL": retData.coverImageUrl
//                            "advertiseInfo": retData.advertiseInfo,
//                            "advertiseUrl": retData.advertiseUrl
                        });
                    },
                    onerror : function(){
                        oPlaybackOrderController && oPlaybackOrderController.movePage();
                    }
                }).request();

            }else{
                oPlaybackOrderController && oPlaybackOrderController.movePage();
            }
        }

        
        function expandPlayer(sIsExpand) {
            var bIsExpand = isGlobalExpand = sIsExpand === 'true';
            var ClipEnd = tvcast.pc.ClipEnd;
            var ClipEndExpand = ClipEnd.getClipEndExpand();

            ClipEndExpand.setExpand(bIsExpand);
        }

        /**
         *  미니플레이어 처리.
         */
        var videoArea = jindo.$Element(jindo.$$.getSingle(".video_area")),
            pipCloseBtn = "",
            hideInterval = 0,
            clipInfo = jindo.$Element("clipInfoArea"),
            clipEndDa = jindo.$Element(jindo.$$.getSingle("#banner ._banner")),
            playerArea = jindo.$Element("playerArea"),
            clipInfoTop = 0,
            isCloseByBtn = false,
            isFixed = false,
            hideFlag = false,
            isPlayerDown = isPlayerMove = false,
            scroll = 0,
            playerPosGap = {},
            daMargin = 50,
            nowPos = {
                posX : 92,
                posY : 22
            },
            mousePos = {},
            closeBtnSize = 29,
            tmpPos = {
                posX : 92,
                posY : 22
            },
            playerSize = {
                width : 384,
                height : 216
            },
            browserSize = jindo.$Document().clientSize(),
            headerHeight = parseInt(jindo.$Element("header").height(), 10),
            oCookie = jindo.$Cookie(),
            playerEl = "",
            isMiniView = false;
        
        if (!!videoArea) {
            pipCloseBtn = jindo.$Element('<a href="#" class="btn_close" style="display:none;">닫기</a>');
            videoArea.append(pipCloseBtn);
            pipCloseBtn.attach("click", function(e){
                e.stopDefault();
                isCloseByBtn = true;
                closeMiniPlayer();
                clickcr(e.srcElement, "pip.close", "", "", e._event);
            });
        }

        var resizeBrowser = function(){
                browserSize = jindo.$Document().clientSize();
                setPos();
                nowPos = {
                    posX : tmpPos.posX,
                    posY : tmpPos.posY
                };

                setPosCookies();
        };


        var attachMouseDown = function(e){
            mousePos = e.pos();
            isPlayerDown = true;
        };

        var setPosCookies = function(){
            oCookie.set("EndPlayer_PosX", ""+nowPos.posX);
            oCookie.set("EndPlayer_PosY", ""+nowPos.posY);
        };

        var setPos = function(){
            if(playerPosGap.posX && playerPosGap.posX != 0){
                tmpPos = {
                    posX : nowPos.posX + playerPosGap.posX < 0 ? 0 : nowPos.posX + playerPosGap.posX,
                    posY : nowPos.posY + playerPosGap.posY < 0 ? 0 : nowPos.posY + playerPosGap.posY
                };
            }

            var sumPlayer = {
                width : browserSize.width - playerSize.width - closeBtnSize,
                height : browserSize.height - headerHeight - playerSize.height
            };

            tmpPos = {
                posX : tmpPos.posX > sumPlayer.width || tmpPos.posX < 0 ? sumPlayer.width : tmpPos.posX,
                posY : tmpPos.posY > sumPlayer.height || tmpPos.posY < 0 ? sumPlayer.height : tmpPos.posY
            };

            videoArea.css({
                "position": "fixed",
                "right" : tmpPos.posX,
                "bottom" : tmpPos.posY,
                "zIndex" : 3001,
                "paddingRight" : "29px",
                "background" : "none",
                "width" : playerSize.width + "px",
                "height" : playerSize.height + "px",
                "opacity" : 1
            });

        };
        var attachMouseMove = function(e){
            if(isPlayerDown){
                isPlayerMove = true;
                e.stop();
                playerPosGap = {
                    posX : mousePos.clientX - e.pos().clientX,
                    posY : mousePos.clientY - e.pos().clientY
                };

                setPos();

            }
        };
        var attachMouseUp = function(e){
            if(isPlayerDown && isPlayerMove){
                e.stop();
                nowPos = {
                    posX : tmpPos.posX,
                    posY : tmpPos.posY
                };
                playerPosGap = {
                    posX : 0,
                    posY : 0
                }

                setPosCookies();
            }
            isPlayerDown = isPlayerMove = false;
        };

        function hideUiPlayer(){
            if(rmcPlayer.toggleIconDisplay) {
                rmcPlayer.toggleIconDisplay("expand", false);
                rmcPlayer.toggleIconDisplay("resolution", false);
                hideFlag = true;

                rmcPlayer.updateLayout();
            }
            else if(rmcPlayer.playerType == "flash" && playerEl && playerEl.hideUi) {
                playerEl.hideUi("expand", false);
                playerEl.hideUi("resolution", false);
                hideFlag = true;
            }

            if(hideInterval && hideFlag){
                clearInterval(hideInterval);
                hideInterval = 0;
            }
        }

        function closeMiniPlayer(){
            videoArea.detach({
                "mousedown": attachMouseDown,
                "mousemove": attachMouseMove,
                "mouseup": attachMouseUp
            });
            jindo.$Element(window).detach("resize", resizeBrowser);

            clipInfo.css("marginTop", "");

            if(pipCloseBtn) {
                pipCloseBtn.hide();
            }

            videoArea.css({
                "position": "",
                "right" : "",
                "bottom" : "",
                "paddingRight" : "",
                "zIndex" : "",
                "width" : "",
                "height" : "",
                "left" : "",
                "background" : "#000"
            });

            playerArea.css({
                height : "",
                backgroundColor:""
            });
        }

		/**
         * 미니 뷰 적용 관련 로직
         * 멀티트랙, VR360, 채널 홈 - 적용 안함
         *
         */
        jindo.$Element(window).attach('scroll', function(e){
            if (!!isMT) {
                return;
            }

            if(isGlobalExpand || false){
                return;
            }

            if(typeof ghtEnv != "undefined" && ghtEnv.sPageName != "clipEnd") {
                return;
            }

            scroll = (document.documentElement && document.documentElement.scrollTop) || document.body.scrollTop;

            if(!playerEl){
                playerEl = rmcPlayer.getElement();
            }

            if(!clipInfo){
                clipInfo = jindo.$Element("clipInfoArea");

                clipInfoTop = clipInfo.offset().top;
            }
            if(!clipEndDa){
                clipEndDa = jindo.$Element(jindo.$$.getSingle("#banner ._banner"));
            }

            if(!clipInfo || !clipEndDa){
                return;
            }

            
                
                
            if (scroll >= clipInfoTop && !isFixed && !isCloseByBtn) {
                
            

                isFixed = true;
                clipInfo.css("marginTop", videoArea.height() + "px");
                hideInterval = 0;
                hideFlag = false;

//                    if(playerEl && playerEl.hideUi) {
//                        hideUiPlayer();
//                    }else{
                        hideInterval = setInterval(function(){
                            hideUiPlayer();
                        }, 10);
//                    }

                videoArea.attach({
                    "mousedown": attachMouseDown,
                    "mousemove": attachMouseMove,
                    "mouseup": attachMouseUp
                });

                nowPos = {
                    posX : parseInt(oCookie.get("EndPlayer_PosX"), 10) || nowPos.posX,
                    posY : parseInt(oCookie.get("EndPlayer_PosY"), 10) || nowPos.posY
                };
                tmpPos = {
                    posX : nowPos.posX,
                    posY : nowPos.posY
                };

                videoArea.css({
                    "position": "fixed",
                    "right" : nowPos.posX,
                    "bottom" : nowPos.posY,
                    "paddingRight" : "29px",
                    "background" : "none",
                    "zIndex" : 3001,
                    "width" : playerSize.width + "px",
                    "height" : playerSize.height + "px",
                    "opacity" : 1
                });

                if(!isMiniView){
                    // 미니플레이어 페이지상 처음 보여질때 한번만 로그수집
                    clickcr && clickcr("", "pip.open", "", "", "");
                }

                isMiniView = true;

                if(!pipCloseBtn) {
//                    pipCloseBtn = jindo.$Element('<a href="#" class="btn_close">닫기</a>');
//                    videoArea.append(pipCloseBtn);
//                    pipCloseBtn.attach("click", function(e){
//                        e.stopDefault();
//                        isCloseByBtn = true;
//                        closeMiniPlayer();
//                        clickcr(e.srcElement, "pip.close", "", "", e._event);
//                    })
                }else{
                    pipCloseBtn.show();
                }
                playerArea.css({
                    height : playerSize.height + "px",
                    backgroundColor:"#000"
                });

                jindo.$Element(window).attach("resize", resizeBrowser);

            
                
                
            } else if (scroll < clipInfoTop && isFixed) {
                
            

                isFixed = false;
                closeMiniPlayer();
                if(rmcPlayer.toggleIconDisplay){
                    rmcPlayer.toggleIconDisplay("expand", true);
                    rmcPlayer.toggleIconDisplay("resolution", true);

                    rmcPlayer.updateLayout();
                }
                else if(rmcPlayer.playerType == "flash" && playerEl && playerEl.hideUi){
                    playerEl.hideUi("expand", true);
                    playerEl.hideUi("resolution", true);
                }
            }
        });

    </script>

    
    


