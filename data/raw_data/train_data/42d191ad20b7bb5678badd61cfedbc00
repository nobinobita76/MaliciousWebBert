        , sAccessKey : ''
        , nPlaylistNo : 109254
        , sPlaylistType : 'GENERAL'
        , nNextClipNo : '1543902'
        , nNextPlaylistNo : '109254'
        , sContinuousPlayCookieKey : 'NTC_ContPL'
        , sClipboardSwfUrl : '/resources/swf/clipboard.swf'
        , sCurrentClipUrl : 'http://tv.naver.com/v/1552522/list/109254'
        , sNextClipUrl : 'http://tv.naver.com/v/1543902/list/109254'
        , bIsDescriptionAvailable : true
        , bIsDescriptionOpened : true
        , bCommentOpen : false
        , bIsSmr : true
        , htComment2Option : {"domain":"http://static.cbox.naver.net","apiDomain":"http://apis.naver.com/commentBox/cbox","login":false,"ticket":"channel01","templateId":"default","objectId":"1552522","language":"ko","country":"KR","pageType":"default","pageSize":20,"indexSize":5,"replyPageSize":5,"cssId":"tvcast","cssUrl":"http://tv.naver.com/resources/release/css/comment/pc_20170323051955.css","id":"cbox_module","likeItId":"1552522","help":"down","groupId":"live_2"}
