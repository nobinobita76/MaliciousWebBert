// 			    		$("#o_ask")[0].style.display="none";
 			    		$("input[name='o1']").each(function() {
 				    		if($(this).val() == "null")
 				    		{
 				    			$(this).attr("checked",true);
 				    		}});
// 			    		$("input[name='o1'][value='time'").attr("checked",true);
						$("#codeSelector")[0].style.display="none";
						$("#codeSelector2")[0].style.display="none";
