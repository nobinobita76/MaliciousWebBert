		    );
		    SyntaxHighlighter.all();
		    /* add by zw end 20160622 */
		    
			var $href = $( '.rel-search' );
			$href.each( function ( i, item ) {
				var href = item.getAttribute( 'href', 2 );
				var reg = /q=(.*?)&/g;
				item.href = href.replace( reg, function ( all, sub ) {
					return 'q=' + encodeURIComponent( sub ) +"&";
				});
			});
			//console.log( href );
		});
