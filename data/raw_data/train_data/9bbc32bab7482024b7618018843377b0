			function changeOrder(value) {
				$("#ot")[0].value = value;
	    		$("#ob")[0].value = value;
	    		
	    		document.search_form_1.submit();
			}
			function changeDocOrder(value) {
				$("#st")[0].value = value;
	    		$("#sb")[0].value = value;
	    		
	    		document.search_form_1.submit();
			}
			function changeLan(type) {
				var value = "";
				if ("1" == type)
					value = $("#dropdownMenu").val();
				else
					value = $("#dropdownMenu2").val();
				$("#lant")[0].value = value;
	    		$("#lanb")[0].value = value;
	    		
	    		document.search_form_1.submit();
			}
