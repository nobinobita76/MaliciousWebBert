<script language="javascript" type="text/javascript">

   function setSbqxTsxx() {
    var today = new Date();
    var monthx = today.getMonth() + 1;
    var str = "";
    for (var i = 0 ; i < NSSB_SBQX.length ; i ++) {
      var tmp = NSSB_SBQX[i];
      if (tmp[0] == monthx) {
          str = monthx+"月"+tmp[1];
        break;
      }
    }

    if (str != "") {
        str = "<a href='#' onclick='window.open(\"http://www.xm-l-tax.gov.cn/xmds/bainianli_1.htm\",\"detail\",\"toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=auto,resizable=no,width=410,height=455,left=10,top=10\");'>本月纳税申报截止日期是 <font style='color: red; font: bold;'><b>" + str + "日</b></font></a>" ;
      var _SBQXTS = document.getElementById("_SBQXTS_TOP");
      if(_SBQXTS != null && typeof(_SBQXTS)=="object") {
        _SBQXTS.innerHTML = str;
      }
    }
  }
  
  //申报截止日期
  setSbqxTsxx();

    //重新加载在线人数及访问总数
  function parseCount(){
    if(typeof(siteinfo)=="undefined" || siteinfo==null) 
      return;
    document.getElementById("nowDayinfo").innerText=siteinfo.nowDay;
    document.getElementById("onlineCount").innerText=siteinfo.onlineCount;
    document.getElementById("accessCount").innerText=siteinfo.indexAccessCount;
  };
  
  

  if (typeof(document.all)!="undefined"){
    window.attachEvent("onload", parseCount);
  }else{
    window.addEventListener('load',parseCount,false);//firefox
  }

  //加入收藏
     function addFavorite(sURL, sTitle){
   try {
       window.external.addFavorite(sURL, sTitle);
          }catch (e){
      try{
         window.sidebar.addPanel(sTitle, sURL, "");
      }catch (e){
        alert("加入收藏失败，请使用Ctrl+D进行添加");
      }
    }
  }
  
  //搜索
  function goSearch(key){
    if(key=='' || key=='-请输入关键字-') {
      alert("请输入关键字");
      keysearch.focus();
      return;
    }
  
    var ch;
    var search_key='';
    for (i=0;i<=key.length-1;i++) {
      ch=key.charAt(i);
      if (ch=='+') {
        search_key=search_key+"%2b";
      } else {
        search_key=search_key+ch;
      }
    }
  
    
      var mainptd="default";
    var searchstr=Cfg.searchBaseUrl+"&keyWord="+encodeURI(search_key);
    window.open(searchstr, "newsearch");
    return true;
  }

  
</script><!-- main -->
