      <script>
                            var specialTab = (function(){
                                var tab_wrap = $('#specialTab'),
                                        tab_btn = tab_wrap.find('.title').find('span'),
                                        tab_cont = tab_wrap.find('.mod-pic--video'),
                                        index = Math.round(Math.random());
                                var init = function(){
                                    tab_btn.eq(0).addClass('current-nav');
                                    tab_cont.eq(0).addClass('current');
                                    tab_btn.on('hover',function(){
                                        var t = $(this).index();
                                        $(this).addClass('current-nav').siblings().removeClass();
                                        tab_cont.eq(t).addClass('current').siblings().removeClass('current')
                                    })
                                };
                                return{
                                    init:init
                                }
                            })();
                            specialTab.init();
                        </script>
      
