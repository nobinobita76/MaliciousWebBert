<script>
    (function(){
        var btns = document.getElementById('face').getElementsByTagName('i');
        var len = btns.length;
        var select = function(){
            for (var i = 0; i < len; i++) {
                btns[i].className = 'vote-system__btn';
            }
            this.className += ' vote-system__btn--selected';
            var radio = this.nextElementSibling||this.nextSibling;
            radio&&radio.click&&radio.click();
        }
        for (var i = 0; i < len; i++) {
            btns[i].onclick = select;
        }
    })();
