	function isDel(url)
	{
		var x = window.confirm('是否删除？');
		if (x)
		{
			document.getElementById("ifr_delarticle").contentWindow.location=url;
			return true;
		}
		else
		{
			return false;
		}
	}
	
	function bookmarkIt(blogName,articleID,title)
	{
		t=title;
		u="http://"+blogName+".blog.hexun.com/"+articleID+"_d.html";
		e="";
		void(open('http://bookmark.hexun.com/post.aspx?title='+escape(t)+'&url='+escape(u)+'&excerpt='+escape(e),'HexunBookmark','fullscreen=yes,scrollbars=yes,status=yes,resizable=yes,toolbar=yes,menubar=yes,location=yes'));	
	}
	function ToggleVisible(targetID, imageID, linkImage, linkImageCollapsed)
	{
		if (document.getElementById){
  		target = document.getElementById(targetID);
  		if (target.style.display == "none") {
  			target.style.display = "";
  		} else {
  			target.style.display = "none";
  		}
  		
  		if (linkImageCollapsed != "") {
  			image = document.getElementById(imageID);
  			if (target.style.display == "none") {
  				image.src = linkImageCollapsed;
  			} else {
  				image.src = linkImage;
  			}
  		}
  		}
	}
	function sms(brief)
	{
	    window.open('http://comment.blog.hexun.com/i/sendsms.aspx?blogname=29261884&brief='+brief,'blogsms','toolbar=no,menubar=no,scrollbars=no,resizable=no,location=no,status=no,width=380,height=180,left=' + (screen.width-380)/2 + ',top=' + (screen.height-180)/2);
	}
