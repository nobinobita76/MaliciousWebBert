<script type="text/javascript">
    $(function () {
        $(".top-category li:last").css({ "border-right": "0" });
        var _moreBtn = $(".top-tips");
        _moreBtn.bind("mouseover", function () {
            $(this).find(".more-item").show();
        });
        _moreBtn.bind("mouseout", function () {
            $(this).find(".more-item").hide();
        });

        $(".wap-link").bind("mouseover", function () {
            $(this).find(".show-code").show();
        });
        $(".wap-link").bind("mouseout", function () {
            $(this).find(".show-code").hide();
        });
//        $(".top-category li:last").css({ "border-right": "0" });

        //控制极力推荐（编辑推荐）显示两条与四条时的不同样式
        var _getRecommendLi = $(".top-recommendList");
        //_getRecommendLi.find("li:even").css({ "width": "295" });
        if (_getRecommendLi.find("li").length > 2) {
            $(".top-recommendTit").height(67).find("p").addClass("pad-t");
            _getRecommendLi.find("li:gt(1)").css({ "border-bottom": "0" });
        } else {
            _getRecommendLi.find("li:lt(2)").css({ "border-bottom": "0" });
        }
        //end

        //控制文章相关推荐样式
        var _getNewRecommend = $(".new-recommendList");
        var _getNewRecommendLi = $(".new-recommendList dl");
        _getNewRecommend.find("dl:odd").css({ "width": "165" });
        for (var i = 0; i < _getNewRecommendLi.length; i++) {
            if (i % 4 == 0) {
                _getNewRecommendLi.eq(i).find("dt").css({ "border-left": "none" });
            }
        }
        //end

        //$(".fashion-rank li:first, .headline-recommend li:first").addClass("first"); //娱乐排行 & 集萃阁 & 头条推荐 第一条添加样式
        $(".fashion-rank li").eq(2).addClass("first");
        $(".headline-recommend li").eq(2).addClass("first");

        /* 返回顶部 */
        var _windowHeight = $(window).height();
        var _getBackTop = $(".float-slidebar");
        _getBackTop.css({ "right": parseInt(($(window).width() - 1000) / 2 - 113) });
        $(window).resize(function () {
            _getBackTop.css({ "right": parseInt(($(window).width() - 1000) / 2 - 113) });
        });

        $(window).scroll(function () {
            var _scrollTop = $(document).scrollTop();
            if (_scrollTop >= parseInt(_windowHeight / 2)) {
                _getBackTop.fadeIn();
            } else {
                _getBackTop.fadeOut();
            }
        });

        _getBackTop.find(".back-top").click(function () {
            $("body, html").animate({ scrollTop: 0 }, 500);
        });

        /* 顶部显示与隐藏二维码 */
        $(".wap-app").bind("mouseover", function () {
            $(this).addClass("wap-app-on").find("i").show();
        });
        $(".wap-app").bind("mouseout", function () {
            $(this).removeClass("wap-app-on").find("i").hide();
        });
    });
    //设为首页
    function SetHome(obj, url) {
        try {
            obj.style.behavior = 'url(#default#homepage)';
            obj.setHomePage(url);
        } catch (e) {
            if (window.netscape) {
                try {
                    netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
                } catch (e) {
                    alert("抱歉，此操作被浏览器拒绝！\n\n请在浏览器地址栏输入'about:config'并回车然后将[signed.applets.codebase_principal_support]设置为'true'");
                }
            } else {
                alert("抱歉，您的浏览器不支持自动设置首页，请使用浏览器菜单手动设置！");
            }
        }
    }
    //加入收藏
    function AddFavorite(title, url) {
        try {
            window.external.addFavorite(url, title);
        }
        catch (e) {
            try {
                window.sidebar.addPanel(title, url, "");
            }
            catch (e) {
                alert("抱歉，您所使用的浏览器无法完成此操作。\n\n加入收藏失败，请使用Ctrl+D进行添加");
            }
        }
    }
    function shutters(showContent, way, currentClass) {
        showContent.bind(way, function () {
            var _this = $(this);
            _this.addClass(currentClass).siblings().removeClass(currentClass);
        });
    }
    $(function () {
        $(".project-rec dl:first").addClass("hover");
        shutters($(".project-rec dl"), "mouseover", "hover");
    });
</script>
