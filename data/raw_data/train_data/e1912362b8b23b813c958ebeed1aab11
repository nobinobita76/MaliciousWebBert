     <script type="text/javascript">
		// cookies
		var div1=document.getElementById("cookies");
		var b=document.getElementById("b");
		var arr = document.cookie.split(';');
		var  flag=true ;
		for(var i=0;i<arr.length;i++){
			arr[i]=arr[i].trim();
			if(arr[i]=='NoneDisplay=true'){
				div1.style.display='none';
				b.style.background='transparent';
				flag=false;
				checkToggle();
				break;
			}
		};

		// toggle
		$(".checktoggle").on("touchstart",function(){
			if(flag==true){
				var d = new Date();
				d.setHours(d.getHours()+3);
				document.cookie="NoneDisplay=true;expires="+d.toUTCString()+";path=/";
				flag=false
				$(".bottom-nav").css({
					"background":" transparent"
				})
				checkToggle()
				$("#cookies").animate({
					"width":"0",
				},300,function(){
					$("#cookies").hide()
				})
				flag=false
			}
			else {
				$("#cookies").show()
				checkToggle1();
				$(".bottom-nav").css({
					"background":"#f0f8ff "
				})
				$("#cookies").animate({
					"width":"83%"
				})
				flag=true
			}
		})

		function checkToggle(){
			$(".checktoggle").attr("src","http://www.nbd.com.cn/corp/2016app/images/exit.jpg")
		}
		function checkToggle1(){
			$(".checktoggle").attr("src","http://www.nbd.com.cn/corp/2016app/images/shou.png")
		}
		var imgs = $(".article-ul a img");
		for(var i=0;i<imgs.length;i++){
			//imgs[i].attr('src',this.attr('data-aload'));
			var url = imgs[i].getAttribute('data-aload');
			if( url != null && url.length > 0){
				imgs[i].setAttribute('src',url);
			}
		}
</script> 
     <script type="text/javascript">
  //评论点赞
   var liLens=$(".comment-ul li").length;
   for(var i=0;i<liLens;i++){
     $(".like").eq(i).attr("data-id",i)
    }
    var article_ids = 1072807 ;
    var flag=true ;
    function setCookie(c_name,value){
        var exdate=new Date()
         var newDate = new Date();
         newDate.setHours(newDate.getHours()+24);
         document.cookie=c_name+ "=" +value;expires="+d.toUTCString()+";
      }
     function getCookie(c_name) {
        if (document.cookie.length>0)
          {
          c_start=document.cookie.indexOf(c_name + "=")
          if (c_start!=-1)
            {
            c_start=c_start + c_name.length+1
            c_end=document.cookie.indexOf(";",c_start)
            if (c_end==-1) c_end=document.cookie.length
            return unescape(document.cookie.substring(c_start,c_end))
            }
          }
          return ""
        }
          $(".like").click(function(obj){
            var $this=$(this);
            var review_id = this.getAttribute("reveiw-id");
            $(this).removeClass("like").addClass("like-active") ;
            if (getCookie(review_id) !== ""){
                 $("#msg2").show();
                  $("#msg2").text("您已经赞过了")
                  setTimeout(function(){
                   $("#msg2").hide()
                 },1000);
                return false;
            }else{
              $.ajax({
                      type: 'POST',
                      url:"/reviews/"+review_id+"/support",
                      success: function(data) {
                          var num = data.data.support_num;
                          $this.siblings(".like-b").text(num);
                          $this.attr("upvote","yes");
                          setCookie(review_id,"yes");
                      }
                });
            }

      })
</script> 
