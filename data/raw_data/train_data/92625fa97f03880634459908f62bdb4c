        function login(iplogin) {
            var fm = $("#fmlogin"), um = fm.find("#nm").val().Trim(), pw = fm.find("#pw").val(), cc = fm.find("#pin").val();
            if ((um == "" || pw == "") && iplogin != true) {
                if (um == "") {
                    showNewTips("用户名不能为空！", null, "");
                    fm.find("#nm").focus();
                    return;
                }
                if (pw == "") {
                    showNewTips("密码不能为空！", null, "");
                    fm.find("#pw").focus();
                    return;
                }
            }
            if ($("#checkHolder").css("display") == "block") {
                if (cc == "" || (cc || '').length != 4) {
                    showNewTips("请输入正确的验证码！", null, "");
                    fm.find("#pin").focus();
                    return;
                }
            }
            // cw modify 2012-11-06
            $("#divLogining").show();
            $("#divLogin").hide();

            var actionstr = "ajxlogin";
            if (iplogin == true)
                actionstr = "ajxiplogin";
            var prms = "action=" + actionstr + "&um=" + um + "&pw=" + pw + "&cc=" + cc, p = unescape(GetURLParam("p"));
            $.ajax({
                url: "/datacenter/DoLoginHandler.ashx",
                type: "post",
                data: prms,
                cache: false,
                success: function (result) {
                    var m = result;
                    if (m.startsWith("success") > 0) {
                        if (p.length > 0) { window.location.href = p; return; }
                        $("#loginarea").html(m.split("|")[0].substring("success".length + 1));
                        if (m.split("|")[5] != "1") {
                            $("#GRUL").hide();
                            $("#JGUL").show();
                            $("#createGR").attr("href", "http://" + CurrentServer + "/gerenguan1.aspx?unitname=" + m.split("|")[3]);
                            $("#createJG").attr("href", "http://" + CurrentServer + "/Register/WebPetition.aspx?UserName=" + m.split("|")[2]);
                            UID = m.split("|")[4];
                            $(".popover-tr").hide();
                            $("#ppdiv").css("display", "none");
                        }
                        else {
                            $("#JGUL").hide();
                            $("#GRUL").show();
                            var arcicon = "";
                            if (m.split("|")[1] != "")
                                arcicon = "▼";
                            $("#GRUL").find(".nav_menu").append("<a class=\"menu_head\" href=\"javascript:var a=toMyOrg('" + m.split("|")[2] + "');\">我的机构馆" + arcicon + "</a>" + m.split("|")[1]);
                            UID = m.split("|")[4];
                            InitHeadList();
                        }
                        $(".gbt").find(".fr").show();
                    }
                    else {
                        fm.find("#nm").val("");
                        fm.find("#pw").val("");
                        fm.find("#pin").val("");
                        fm.find("#nm").focus();
                        if (result == "fail") {
                            showTips("电子商务登陆接口异常！");
                            $("#divLogining").hide();
                            $("#divLogin").show();
                            return false;
                        }
                        if (result) {
                            var obj = eval('(' + result + ')');
                            if (obj.fail === "true") {
                                showNewTips(obj.errorMsg, null, "");
                            }
                            if (obj.showcheck === "true") {
                                //显示并加载 验证码
                                $("#checkImg").attr("src", "/DataCenter/CheckCodeHandler.ashx?t=" + Math.random());
                                $("#checkHolder").css("display", "block");
                            }
                        }

                        // cw modify 2012-11-06
                        $("#divLogining").hide();
                        $("#divLogin").show();
                    }
                }
            });
            //$.ajax({
            //    type: "get",
            //    url: LoginDigitalUrl + "/DataCenter/DoLoginHandler.ashx?jsoncallback=?",
            //    data: prms + "&tp=grid20",
            //    dataType: "jsonp",
