        function getCookie1(objName) {//获取指定名称的cookie的值
            var arrStr = document.cookie.split("; ");
            for (var i = 0; i < arrStr.length; i++) {
                var temp = arrStr[i].split("=");
                if (temp[0] == objName) return unescape(temp[1]);
            }
        }
        function AddMypPaperPrompt() {
            var claimLeft = $("#GRUL").find("li:eq(3)").offset().left + 15;
            var claimTop = $("#GRUL").find("li:eq(3)").offset().top + 35;
            var pp = "<div id=\"ppdiv\" class=\"popover-tl\" style=\"top: " + claimTop + "px; left : " + claimLeft + "px; \">";
            pp += "<a class=\"close\" href=\"#1\" style=\"\" data-dismiss=\"alert\">×</a>";
            pp += "<div class=\"popover-caret\">";
            pp += "<div class=\"popover-caretInner\"></div>";
            pp += "</div><strong>新产品推荐</strong><br />收集互联网信息、剪藏文章、收藏网址，请点击！</div>";
            $("body").append(pp);
            $(".close").bind("click", "", Pclose);
        }
        //关闭提示POP
        function Pclose() {
            $(".popover-tr").hide();
            $("#ppdiv").css("display", "none");
            setCookie1("MyPaperPrompt", "1", 190);
            return false;
        }
        GetLoginStatus();
        // 获取登录信息
        function GetLoginStatus() {
            var MyPaperP = getCookie1("MyPaperPrompt"), p = unescape(GetURLParam("p"));
            // 获取登录信息
            $.ajax({
                url: "/datacenter/DoLoginHandler.ashx",
                type: "get",
                data: "action=ajxstatus",
                cache: false,
                success: function (result) {
                    if (result.length < 10)
                        return;
                    var jsonResult = window.eval(result);
                    if (jsonResult[0].status.startsWith("on") > 0) {
                        if (p.length > 0) { window.location.href = p; return; }
                        $("#loginarea").html(jsonResult[0].status.substring("on".length + 1));
                        if (jsonResult[0].LibType != "1") {
                            $("#GRUL").hide();
                            $("#JGUL").show();
                            $("#createGR").attr("href", "http://" + CurrentServer + "/gerenguan1.aspx?unitname=" + jsonResult[0].CurUnitName);
                            $("#createJG").attr("href", "http://" + CurrentServer + "/Register/WebPetition.aspx?UserName=" + jsonResult[0].UserName);
                        }
                        else {
                            $("#JGUL").hide();
                            $("#GRUL").show();
                            var arcicon = "";
                            if (jsonResult[0].JoinUnitName != "")
                                arcicon = "▼";
                            $("#GRUL").find(".nav_menu").append("<a class=\"menu_head\" href=\"javascript:var a=toMyOrg('" + jsonResult[0].UserName + "');\">我的机构馆" + arcicon + "</a>" + jsonResult[0].JoinUnitName);
                            UID = jsonResult[0].IdenID;
                            InitHeadList();
                        }
                        $(".gbt").find(".fr").show();
                    }
                }
            });
        }
        $(document).ready(function () {
            var index1 = 0;
            var index2 = 0;
            var timer1;
            var timer2;
            var len1 = $("#logo1.imgContainerInner ul li").length;
            $("#logo1.imgContainerInner ul li").hide();
            $("#logo1.imgContainerInner ul li").eq(index1).show();
            $("#logo1 .sliderCtr a").eq(index1).addClass("active");
            $("#logo1 .sliderCtr a").mouseover(function () {
                $("#logo1 .sliderCtr a").removeClass("active");
            });
            $("#logo1 .sliderCtr a").click(function () {
                index1 = $("#logo1 .sliderCtr a").index(this);
                showfast1(index1);
            });
            $("#logo1 .sliderCtr").mouseleave(function () {
                clearInterval(timer1);
                $("#logo1 .sliderCtr a").eq(index1).addClass("active");
            });
            function show1(index) {
                $("#logo1 .sliderCtr a").eq(index).addClass("active");
                $("#logo1.imgContainerInner ul li").fadeOut(500).delay(100).eq(index).fadeIn(500);
                $("#logo1 .sliderCtr a").removeClass("active").eq(index).addClass("active");
            }
            function showfast1(index) {
                $("#logo1 .sliderCtr a").eq(index).addClass("active");
                $("#logo1.imgContainerInner ul li").fadeOut(200).delay(100).eq(index).fadeIn(200);
                $("#logo1 .sliderCtr a").removeClass("active").eq(index).addClass("active");
            }
            function autoplay1() {
                timer1 = setInterval(function () {
                    if (index1 == len1 - 1) { index1 = -1 }
                    show1(index1 + 1)
                    index1++;
                    if (index1 == len1 - 1) { index1 = -1 }
                }, 8000);
            };
            $("#logo1.imgContainerInner").hover(function () {
                clearInterval(timer1);
            }, function () {
                autoplay1();
            }).trigger("mouseleave");

            var len2 = $("#logo2.imgContainerInner ul li").length;
            $("#logo2.imgContainerInner ul li").hide();
            $("#logo2.imgContainerInner ul li").eq(index2).show();
            $("#logo2 .sliderCtr a").eq(index2).addClass("active");
            $("#logo2 .sliderCtr a").mouseover(function () {
                $("#logo2 .sliderCtr a").removeClass("active");
            });
            $("#logo2 .sliderCtr a").click(function () {
                index2 = $("#logo2 .sliderCtr a").index(this);
                showfast2(index2);
            });
            $("#logo2 .sliderCtr").mouseleave(function () {
                clearInterval(timer2);
                $("#logo2 .sliderCtr a").eq(index2).addClass("active");
            });
            function show2(index) {
                $("#logo2 .sliderCtr a").eq(index).addClass("active");
                $("#logo2.imgContainerInner ul li").fadeOut(500).delay(100).eq(index).fadeIn(500);
                $("#logo2 .sliderCtr a").removeClass("active").eq(index).addClass("active");
            }
            function showfast2(index) {
                $("#logo2 .sliderCtr a").eq(index).addClass("active");
                $("#logo2.imgContainerInner ul li").fadeOut(200).delay(100).eq(index).fadeIn(200);
                $("#logo2 .sliderCtr a").removeClass("active").eq(index).addClass("active");
            }
            function autoplay2() {
                timer2 = setInterval(function () {
                    if (index2 == len2 - 1) { index2 = -1 }
                    show2(index2 + 1)
                    index2++;
                    if (index2 == len2 - 1) { index2 = -1 }
                }, 8000);
            };
            $("#logo2.imgContainerInner").hover(function () {
                clearInterval(timer2);
            }, function () {
                autoplay2();
            }).trigger("mouseleave");
            //绑定检索按钮事件
            $(".searchInBody a").click(function () {
                $(this).parents("ul").find("li[class='active']").removeClass("active");
                $(this).parent().addClass("active");
                $(".is-inp").val("");
            });
            $("#search-btn").click(function () {
                var i = $(this).parents(".searchInBody").find("li[class='active']").index();
                var _val = $(".is-inp").val();
                if (!CheckChar(_val)) {
                    showTips(message_Back['SearchLib6'], null, "");
                    return;
                }
                switch (i) {
                    case 0://文献
                        var scholarip = CnkiScholarIP == "" ? "http://scholar.cnki.net" : CnkiScholarIP;
                        var url = scholarip + "/Result.aspx?uid=" + UID + "&rt=" + "" + "&q=" + escape(_val);
                        window.open(url, "学术搜索", "", "");
                        break;
                    case 1://学者
                        window.open("http://xuezhe.cnki.net/Search/Search.aspx?ac=result&nmt=nm&nmv=" + _val);
                        break;
                    case 2://期刊
                        window.open("http://epub.cnki.net/kns/in/");
                        break;
                    case 3://科研项目
                        window.open("http://projects.cnki.net/search.aspx?q=" + _val);
                        break;
                    case 4://学术趋势
                        window.open("http://trend.cnki.net/TrendSearch/trendshow.htm?searchword=" + escape(_val));
                        break;
                }
            });
        });
        function scholarSearch() {
            var _val = $("#searchtext").val();
            if (!CheckChar(_val)) {
                showTips(message_Back['SearchLib6'], null, "");
                return;
            }
            var scholarip = CnkiScholarIP == "" ? "http://scholar.cnki.net" : CnkiScholarIP;
            var url = scholarip + "/Result.aspx?uid=" + UID + "&rt=" + "" + "&q=" + escape(_val);
            window.open(url, "学术搜索", "", "");
        }
        function showTips(message, dom, parameter) {
            if (dom == null || dom == undefined || dom == "")
                dom = document;
            if (message == null || message == undefined || message == "")
                message = "";
            var windowWidth = dom.documentElement.clientWidth;
            var time = 3; //默认显示3秒
            $("#smarttips", dom).html("");
            var tipsDiv = $("#smarttips", dom).text(message);
            tipsDiv.css({
                'left': (windowWidth / 2) - (message.length * 13 / 2) + 'px',
                'z-index': '9999'
            });
            tipsDiv.addClass("fixed").show();
            setTimeout(function () { $('#smarttips', dom).fadeOut(); }, (time * 1000));
        }
        function CheckChar(mvalue) {
            var charary = "~,!,@,#,$,%,^,&,*,_,+,;,',\",:, |,<,>,[,],{,},\\,/".split(',');
            for (var i = 0; i < charary.length; i++) {
                if (mvalue.indexOf(charary[i]) != -1) {
                    return false;
                }
            }
            return true;
        }
        // 不显示label内容
        function ForNotDisp(id) {
            $("#" + id).css("display", "none");
        }
        var ForCount = 1;
        // 还原label内容
        function ForDisp(text, id) {
            if ($(text).length > 0 && $("#" + id).length > 0 && ForCount < 100) {
                if ($(text).val() == "") {
                    $("#" + id).css("display", "block");
                }
            }
            else {
                setTimeout(function () { ForCount += 1; ForDisp(text, id); }, 100);
            }
        }
        //个人馆 机构馆头部控件调用方法
        function InitHeadList() {
            var IsShow = false, a = $(".nav_menu"), b = $(".menu_body"), c = $(".more_menu"), d = $(".more_body");
            a.mouseover(function () {
                var TabTop = parseInt($(this).offset().top) + 20;
                b.css("top", TabTop + "px").css("left", $(this).offset().left + "px");
                b.show();
            }).mouseleave(function () { b.hide(); });
            c.mouseover(function () {
                var TabTop = parseInt($(this).offset().top) + 20;
                d.css("top", TabTop + "px").css("left", $(this).offset().left + "px");
                d.show();
            }).mouseleave(function () { d.hide(); });
            //镜像
            $("#DIV_help, #liMyOrg").hover(function () {
                $(this).find(".menu_body").slideDown('fast').show();

                $(this).hover(function () {
                }, function () {
                    $(this).find(".menu_body").slideUp('slow');
                });
            }).hover(function () {
                $(this).css({ color: "#F4AE34" });
            }, function () {
                $(this).css({ color: "#000" });
            });
        }
        //个人馆头部脚本
        function toMyOrg(tname) {
            var sourceType = "M028";
            setCookie1("SettingItem", sourceType, 1);
            var a = window.open('/manage/myidl.aspx');
            a.focus();
        }
        //设置cookie
        function setCookie1(name, value, expireDays) {
            if (expireDays == undefined || expireDays == null) {
                expireDays = 10;
            }
            var str = name + "=" + escape(value);
            var dd = new Date();
            dd.setDate(dd.getDate() + expireDays); //获取明天 
            str += ";expires=" + dd.toGMTString();
            document.cookie = str;
        }
        //登陆提示
        function CheckloginEnter(paras, rebacktype, lan) {
            $.ajax({
                url: "/datacenter/DoLoginHandler.ashx",
                type: "get",
                data: "action=ajxstatus",
                cache: false,
                success: function (result) {
                    var jsonResult = window.eval(result);
                    if (jsonResult[0].status.startsWith("on") > 0) {
                        document.location.href = "/" + jsonResult[0].UserName + "/";
                    }
                    else {
                        var func = null;
                        if (rebacktype != null && rebacktype == "Switch") {
                            func = {
                                WClose: function () {
                                    var elem = $("#loginfunc").data(rebacktype);
                                    if (elem != null && elem.click != null)
                                        $("#loginfunc").data(rebacktype).click();
                                }
                            };
                        }
                        var loginheight = 290;
                        WinBox(window.eval("btnTitle[\"011\"]"), 'iframe:/login/language/' + lan + '/AlertLogin.htm?' + paras, 400, loginheight, 'true', '', 'true', '', func);
                    }
                }
            });
        }
        function MyPaperJumpSelf(type) {
            //馆内我发表的文献跳转，自跳转
            var _url = "";
            switch (type) {
                case "newPush": //新推送跳转
                    _url = "/MyPaper/Push.aspx";
                    break;
                case "bacth": //批量导入成果
                    _url = "/MyPaper/Add.aspx?module=bathchadd";
                    break;
                case "yxlfx": //学术影响力分析
                    _url = "/PaperCenter.aspx?module=analysis";
                    break;
                case "index": //跳向首页
                    _url = "/PaperCenter.aspx?module=paper&login=1";
                    break;
                default: //查看更多
                    _url = "/PaperCenter.aspx";
                    break;
            }
            window.open("http://" + MyPaperUrl + "/LoginDigital.aspx?action=single&uid=" + UID + "&p=" + escape(_url), "成果库");
        }
        function MyWebCollectionJump(url) {
            window.open("http://" + WebCollectionUrl + "/LoginDigital.aspx?action=single&uid=" + UID + "&p=" + url, "常用网址栏目");
        }
        function MyXueZheJump() {
            window.open("http://" + XueZheUrl + "/LoginDigital.aspx?action=single&uid=" + UID + "&p=" + escape("MyCenter.aspx"), "学者圈");
        }
        //跳向我的科研项目
        function MyKeyanJump(url) {
            window.open(ProjectsUrl + "LoginDigital.aspx?action=single&UID=" + UID + "&p=" + escape(url), "_blank");
        }
        //馆内跳转
        function jumpInLib(type) {
            CheckloginStatus(type, "GB");
        }
        //登陆提示
        function CheckloginStatus(paras, lan) {
            $.ajax({
                url: "/datacenter/DoLoginHandler.ashx",
                type: "get",
                data: "action=ajxstatus",
                cache: false,
                success: function (result) {
                    if (result == "off") {
                        var func = null;
                        var loginheight = 290;
                        WinBox(window.eval("btnTitle[\"011\"]"), 'iframe:/login/language/' + lan + '/AlertLogin.htm', 400, loginheight, 'true', '', 'true', '', func);
                        return false;
                    }
                    var jsonResult = window.eval(result);
                    if (jsonResult[0].status.startsWith("on") > 0) {
                        window.open("/" + jsonResult[0].UserName + "/" + "#lib=" + paras, "_self");
                    }
                }
            });
        }
        //重新获取验证码
        function GetCodeImg() {
            $("#checkImg").attr("src", "/DataCenter/CheckCodeHandler.ashx?t=" + Math.random());
        }
