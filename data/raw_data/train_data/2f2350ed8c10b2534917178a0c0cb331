<script language="javascript" type="text/javascript">
chart.size=11;
chart.pagesize = 30;
var CPName = "重庆时时彩";
var tKey = {
    'h1': {                // 选号位key值
        filter: ':lt(10)',  // 筛选<td/>对象的jQuery表达式
        ball: 'q1'          // 选号球样式
    }
}

// 选号验证
function balls_match(balls){
    var checked = false;
	if(balls[0].length > 0){
	   checked = true;
	}
    return checked;
}

// 预测区单行清空选号（行号，是否清除工具箱）
function yc_clear($tds,n,clear){
	var $ball = $tds.filter('.q1,.q2');
	if($ball.length > 0){
		$ball.each(function(){
			var i = $tds.index(this);
			var cName = 'y1_1';
			if ((i >= 10 && i <= 14)||(i >= 20 && i <= 24)||(i >= 30 && i <= 34)||i >= 40)
				cName = 'y2_1';
			this.className = cName;
		});
		delete yc.ball[n]
		delete yc.hm_str[n];
		if(clear || false) tbox.clear(n);
	}
}

// 折线样式
function reset_cLine() {
	var $z2 = $('#z2');
	var $z1 = $('#z1');
	return {
	'h1': {
		size: $z1.css('size'),
		color: $z1.css('color'),
		indent: $z1.css('zindex')
	},
	'dx': {
		size: $z2.css('size'),
		color: $z2.css('color'),
		indent: $z2.css('zindex')
	},
	'jo': {
		size: $z1.css('size'),
		color: $z1.css('color'),
		indent: $z1.css('zindex')
	},
	'zh': {
		size: $z2.css('size'),
		color: $z2.css('color'),
		indent: $z2.css('zindex')
	},
	'012': {
		size: $z1.css('size'),
		color: $z1.css('color'),
		indent: $z1.css('zindex')
	},
	'spj': {
		size: $z2.css('size'),
		color: $z2.css('color'),
		indent: $z2.css('zindex')
	},
	'dzx': {
		size: $z1.css('size'),
		color: $z1.css('color'),
		indent: $z1.css('zindex')
	},
	'zf': {
		size: $z2.css('size'),
		color: $z2.css('color'),
		indent: $z2.css('zindex')
	}
	}
}

$(function(){
	var line = reset_cLine();
	init('ssc', '/ssc/php/wxzxh.php', ['h1','zf'],line);
});
</script>


