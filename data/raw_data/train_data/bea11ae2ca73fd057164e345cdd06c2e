/**************摘要****************/
function show_abs(dbid,an,show_key){
	var newan=an.replace(/\./g,"-"); 
	newan=newan.replace(/\+/g,"-");
	if($('#abs_on'+newan).attr('class')!=undefined){
		//$("#abs_"+newan).toggle();
	}else{
		$('#abs_on'+newan).addClass('abs_on');
		//$("#abs_"+newan).show();
		$.ajax({ 
			type:"get", //提交类型 
			url:"?h=show_abs&db="+dbid+'&an='+an+'&s_query='+show_key, //提交页面 
			success:function(msg) { 
				$('#abs_'+newan).html('<strong>Abstract: </strong>'+msg);
			}
		});
	}
}

function show_hide(num,url){
	if(url!=''){
		var google_url='http://demo.findplus.cn/QRcode.php?url='
		$(".img"+num).attr("src",google_url+url);
	}
	var temp= $('.code_'+num).is(":hidden");//是否隐藏
	if(temp==true){
		$('.hidcon').hide();
	}
	$('.code_'+num).toggle();
}

function show_txthtml(content,class_name,an,type){
	var txt_html='';
	if(content.length>1){
		var num=0;
		for(var i=0; i<content.length; i++ ){
			if(an!=content[i]['An'] && ((type=='Book' || type=='eBook') && (content[i]['PubType']=='Book' || content[i]['PubType']=='eBook') || (type=='Academic Journal' || type=='Periodical') && (content[i]['PubType']=='Academic Journal' || content[i]['PubType']=='Periodical'))){
			num=num+1;
			txt_html+='<table><tr>';
			txt_html+='		<td style="vertical-align:top;width:20px;">'+num+'.</td> ';
			txt_html+='		<td style="vertical-align:top;width:100px">';
			txt_html+='			<span title="'+content[i]['PubType']+'" class="pubtype-icon pt-'+content[i]['PubTypeId']+'" style="width:60px;"></span>';
			txt_html+='		</td>';
			txt_html+='		<td style="vertical-align:top">';
			txt_html+='			<div class="titleM">';
			txt_html+='				<a href="?h=articles&db='+content[i]['DbId']+'&an='+content[i]['An']+'" target="_blank">'+content[i]['Title']+'</a>';
			txt_html+='			</div>';
			if(content[i]['author']!=undefined){
				txt_html+='<div><strong>By</strong> : '+content[i]['author']+'</div>';
			}
			if(content[i]['source']!=undefined){
				txt_html+='<div><strong>Source</strong> : '+content[i]['source']+'</div>';
			}
			if(content[i]['subject']!=undefined){
				txt_html+='<div><strong>Subjects</strong> : '+content[i]['subject']+'</div>';
			}
			if(content[i]['pdf_link'] == '1'){
				txt_html+='<div><span><a class="icon link_c" href="?h=down_pdf&an='+content[i]['An']+'&db='+content[i]['DbId']+'" target="_blank"> 阅读全文</a></span></div>';
			}
			if(content[i]['pdf_link'] == '11'){
				txt_html+='<div><span><a class="icon linkdoi" href="?h=down_pdf&an='+content[i]['An']+'&db='+content[i]['DbId']+'" target="_blank"> 链接全文</a></span></div>';
			}
			
			txt_html+='		</td>';
			txt_html+='</tr></table>';
			}
		}
		if(txt_html==''){
			$('.'+class_name).html('系统未收录该文献的其它版本');
		}else{
			$('.'+class_name).html(txt_html);
		}
	}else{
		$('.'+class_name).html('系统未收录该文献的其它版本');
	}
}

//checkbox 全选
$(function(){
	$("#checkall,#checkal2").click(function() {
		$('input[name="selTag[]"]').attr("checked",this.checked);
	});
	var $subBox = $("input[name='selTag[]']");
	$subBox.click(function(){
		$("#checkAll").attr("checked",$subBox.length == $("input[name='selTag[]']:checked").length ? true : false);
	});
});


//获取checkbox值
function getSelect(names)
{
    var vals = "";
    var eles = document.getElementsByName(names);
    for(var i=0;i<eles.length;i++)
    {
		if(eles[i].checked){vals+=eles[i].value+",";}
    }
    vals = vals.length>0?(vals.substring(0,vals.length-1)):("");
	return vals;
}

//导出
function file_export(type,tag){
	if(tag== undefined || tag== 'undefined'){tag='en';}
	var ids=getSelect('selTag[]');
	if(ids==''){
		alert('请选择需要导出的文章');
	}else{
		var url='control.html?control_type=export_list&types='+type+'&pt_tag='+tag+'&selTag='+ids
		window.open(url,'_self');
	}
}
