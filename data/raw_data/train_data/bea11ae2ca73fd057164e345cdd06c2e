  <script>
function do_selects(tag_name){
    var vals ="http://en.ruc.findplus.cn/search_list.html?h=search_list&query=AR:\&quot;Sicari, Rosa\&quot;";
	var action_str='[]';
	var action_arr=eval("("+action_str+")");
	if(action_arr[tag_name]){
		for(var item  in action_arr[tag_name]) {
			var replace_str='&action['+tag_name+'][]='+action_arr[tag_name][item];
			vals=vals.replace(replace_str,'');
		}
	}  
	var select_list=getSelect_zdy('f_check[]',vals);
	open_url(select_list);
	parent.tb_remove();
}
//获取checkbox值 
function getSelect_zdy(names,vals)
{
    var eles = document.getElementsByName(names);
    for(var i=0;i<eles.length;i++)
    {
		if(eles[i].checked){
			vals+=eles[i].value;
		}
    }
	return vals;
}
</script> 
