	$.ajax({
		type:"GET",
		url:"http://52.1.187.113/getip.php",
		dataType: "jsonp",
		jsonp:"callbackparam",
		jsonpCallback:"success_show_ip",
		success:function(json){
		   save_session(json);
		}
	});
	function save_session(ip){
		$.ajax({
			type	:	'GET',
			url		:	'control.cx',
			data	:	{'control_type':'save_session','client_ip':ip,rand:Math.random()},
			success: function(msg){}
		});
	}
</script> 
