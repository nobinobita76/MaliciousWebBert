/***期刊检索**/
var wiki_url='http://primobe.lib.sjtu.edu.cn:8991/cgi-bin/wiki.cgi?q=AR%3ASicari%2C+Rosa';
function send_url_message(url)
{
	var a=document.createElement("script");
	a.setAttribute("type","text/javascript");
	a.setAttribute("src",url);
	document.body.appendChild(a);
}

function showiki(wikcon)
{
	if(strlen(wikcon)>160){					  
		var wikicon= '<fieldset id="wiki_con"><legend><b>AR:\"Sicari, Rosa\"</b></legend>';
		wikicon+='<div style="display:none;" class="wikicon" id="full-wikicon">'+wikcon+' <a href="javascript:;" onclick="hide_full()" id="full-wikicon-plug">[收起]</a></div>'; 
		var wikcon=getInterceptedStr(wikcon,160)+'...';
		wikicon+='<div style="display:block;" class="wikicon" id="wikicon">'+wikcon+' <a href="javascript:;" onclick="show_full()" id="wikicon-plug">[更多]</a></div>';  
		wikicon+='</fieldset>';
	}else{
		var wikicon='<fieldset id="wiki_con"><legend><b>AR:\"Sicari, Rosa\"</b></legend><div style="display:block;" class="wikicon" id="wikicon">'+wikcon+'</div></fieldset>';					  
	}             
	$('#divwiki_con').append(wikicon);
}


function get_qk_message(title,ids){
	$.ajax({
		type	:	'get',
		url	:	'?h=get_qk_message',
		data	:	{title:encodeURIComponent(title),id:ids,rand:Math.random()},
		success: function(msg){
			var data=eval('('+msg+')');
			if(data.contents != ''){	  
				var html_s= '<fieldset id="wiki_con"><legend><b>'+data.title+' &nbsp;期刊导览</b></legend><div class="wikicon" id="wikicon">'+data.contents+'</div></fieldset>';
				$('#divwiki_con').html(html_s);
			}else{
				send_url_message(wiki_url);
			}
		}
	});
}

function click_m(obj){
	if($(obj).html()=='更多&gt;&gt;'){
		$(obj).html('收起&gt;&gt;');
		$('.sh_div').toggleClass('hide');
	}else{
		$(obj).html('更多&gt;&gt;');
		$('.sh_div').toggleClass('hide');
	}
}
/***期刊检索**/	$(document).ready(function(){					get_qk_message('ContentProvider','8337');				//get_atr_cated(); //加载被引次数	})/*** 处理输入年代参数*/function do_yearurl1(){	var fromtext=$('#fromtext').val()	var tromtext=$('#tromtext').val()	fromtext=fromtext.replace(/^\s+|\s+$/g,"");			var pattern = /\d{4}/;	if(pattern.test(fromtext)==false || pattern.test(tromtext)==false)	{		alert('年代输入有误！')	}	else	{		if(fromtext>tromtext)		{			open_url("?h=search_list&query=AR:\&quot;Sicari, Rosa\&quot;&limiter="+tromtext+"-"+fromtext);		}		else		{			open_url("?h=search_list&query=AR:\&quot;Sicari, Rosa\&quot;&limiter="+fromtext+"-"+tromtext);		}		}}	</script> 
