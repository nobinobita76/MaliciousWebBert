                <div class="instance_list2_box">
                    <div class="instance_list2" style="display: block;">
                    </div>
                </div>
                <!-- 购物说明结束 -->

                <!-- 推荐项目开始 -->
                <div class="instance_list2_box">
                        <div class="instance_list2" style="display: block;">
                        </div>
