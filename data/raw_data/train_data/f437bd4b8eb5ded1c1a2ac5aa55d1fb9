<script>
	//通过点击行程天数页面过来的，进行统计
	//获取外部链接
	var getReferrer=function() {
	    var referrer = '';
	    try {
	        referrer = window.top.document.referrer;
	    } catch(e) {
	        if(window.parent) {
	            try {
	                referrer = window.parent.document.referrer;
	            } catch(e2) {
	                referrer = '';
	            }
	        }
	    }
	    if(referrer === '') {
	        referrer = document.referrer;
	    }
	    return referrer;
	};
	var getReferrerStr = getReferrer();
	$(function(){
		$(".travelDays").click(function(){
			sessionStorage.setItem('clicked','true');
			var url = "http://dujia.lvmama.com/package/"+$(this).attr("data-value");
			$(this).removeClass("active");
			$(this).parent().children(":first").addClass("active");
			$(this).parent().children(":first").append("<i class='line_icon icon_gou'></i>");
			cmCreateElementTag("categoryName_行程天数","行程天数link点击");
			//ie浏览器需要自己加上refer
			if (document.all || !!window.ActiveXObject || "ActiveXObject" in window){ //兼容ie11的判断
    		var referLink = document.createElement('a');
    		referLink.href = url;
    		referLink.target = '_blank';
    		document.body.appendChild(referLink);
    		referLink.click();
    		sessionStorage.setItem('clicked','false');
			} else {
    		//location.href = url;
    		window.open(url);
    		sessionStorage.setItem('clicked','false');
			}
		});
	});
	
	//解决刷新统计问题
	if(sessionStorage.clicked == undefined || sessionStorage.clicked == null){
	}else{
		if(sessionStorage.clicked == 'true'){
			//如果是通过点击行程天数页面过来的，将进行统计
			var isGetReferrer = (getReferrerStr.indexOf("dujia.lvmama.com/group/")==-1?false:true) || (getReferrerStr.indexOf("dujia.lvmama.com/freetour/")==-1?false:true) || (getReferrerStr.indexOf("dujia.lvmama.com/local/")==-1?false:true) || (getReferrerStr.indexOf("dujia.lvmama.com/package/")==-1?false:true);
			if(isGetReferrer){
				cmCreateElementTag("categoryName_行程天数","行程天数link点击");
			}
		}else{
		}
	}
	sessionStorage.setItem('clicked','false');
	
	//门票有变化时
$('.ticket-select').live('change', function (e) {
	var checkArray = new Array(); 
	var isCombStructName = '';
	$('.active_xuan').each(function(){
		 var obj1 = $(this);
		 var dataType = obj1.attr('data-type');
		//如果是非结构化套餐
		 if('packageComb'==dataType){
			 var hotelComb = new Object(); 
			 hotelComb.typeName = '酒店套餐';
			 hotelComb.goodsName=obj1.attr('showGoodsName');
			 hotelComb.quantity=obj1.attr('data-quantity')+"份";
			 checkArray.push(hotelComb);
		 }	
		 //结构化套餐
		  if('comb_struct_ticket'==dataType || 'comb_struct_ticket'==dataType){
			  if(isCombStructName == ''){  
			    isCombStructName = $('#currentgoodsname').text();
			    var comb_struct = new Object(); 
			    comb_struct.typeName = '酒店套餐';
			    comb_struct.goodsName=isCombStructName;
		        var comb_quantity = $("#preorder-quantity").val();
		        comb_struct.quantity=comb_quantity+"份";
		        checkArray.push(comb_struct);
			  }
		  }		 	 
		 //如果是酒店
		 if('hotel'==dataType){
			 var hotel = new Object();
			 hotel.typeName = '房型';
			 hotel.goodsName=obj1.attr('showGoodsName');
			 var hotelQuantity = obj1.parent().prev().find('.hotel-select').find("option:selected").text()+"间";
			 hotel.quantity = hotelQuantity;
			 checkArray.push(hotel);
		 }
		 
	  });
		  
	//门票buy_number ticket-select
    $('.buy_number.ticket-select').each(function(){
       	var ticket = $(this);
    	ticket.typeName="门票";
    	ticket.goodsName=ticket.attr('showGoodsName');
    	ticket.quantity=ticket.find("option:selected").text()+"张";
    	checkArray.push(ticket); 
    });

    
     var htmlStr = "";
   $.each(checkArray, function(index,obj){      
       var li_1 ="<li>";
	   var typeName = '<span class="ysi-type">'+obj.typeName+'：</span>';
	   var goodsName = '<span class="ysi-name" title='+obj.goodsName+'>'+obj.goodsName+'</span>';
	   var quantity = '<span class="ysi-amount">'+obj.quantity+'</span>';
	   var li_2 ="</li>";
	   htmlStr = htmlStr+li_1+typeName+goodsName+quantity+li_2;
	   console.log(typeName);
	   console.log(goodsName); 	
	   console.log(quantity); 
	   //yh-slc-items		   
   });
    if(htmlStr!=""){
       $('.yh-slc-items').html(htmlStr)
       $('.yh-slc-selected').show();
       //yh-slc-none
       $('.yh-slc-none').hide();
    }
    
});


//酒店变化
$('.hotel-select').live('change', function (e) {
	var checkArray = new Array(); 
	var hotelObj = $(this);
	var isCombStructName = '';
	$('.active_xuan').each(function(){
		 var obj1 = $(this);
		 var dataType = obj1.attr('data-type');
		//如果是非结构化套餐
		 if('packageComb'==dataType){
			 var hotelComb = new Object(); 
			 hotelComb.typeName = '酒店套餐';
			 hotelComb.goodsName=obj1.attr('showGoodsName');
			 hotelComb.quantity=obj1.attr('data-quantity')+"份";
			 checkArray.push(hotelComb);
		 }	
		  //结构化套餐
		  if('comb_struct_ticket'==dataType || 'comb_struct_ticket'==dataType){
			  if(isCombStructName == ''){  
			    isCombStructName = $('#currentgoodsname').text();
			    var comb_struct = new Object(); 
			    comb_struct.typeName = '酒店套餐';
			    comb_struct.goodsName=isCombStructName;
		        var comb_quantity = $("#preorder-quantity").val();
		        comb_struct.quantity=comb_quantity+"份";
		        checkArray.push(comb_struct);
			  }
		  }		 	 
		 //如果是酒店
		 if('hotel'==dataType){
		     var hotel = new Object();
		     hotel.typeName = '房型';
		     if(hotelObj.attr('data-groupid')==obj1.attr('data-groupid')){
		        hotel.goodsName=hotelObj.attr('showGoodsName');
		        hotel.quantity = hotelObj.find("option:selected").text()+"间";
		        checkArray.push(hotel);
		     }else{			 
			   hotel.goodsName=obj1.attr('showGoodsName');
			   var hotelQuantity = obj1.parent().prev().find('.hotel-select').find("option:selected").text()+"间";
			   hotel.quantity = hotelQuantity;
			   checkArray.push(hotel);
		     }
		 }
		 
	  });
		  
	//门票buy_number ticket-select
    $('.buy_number.ticket-select').each(function(){
       	var ticket = $(this);
    	ticket.typeName="门票";
    	ticket.goodsName=ticket.attr('showGoodsName');
    	ticket.quantity=ticket.find("option:selected").text()+"张";
    	checkArray.push(ticket); 
    });

     var htmlStr = "";
   $.each(checkArray, function(index,obj){      
       var li_1 ="<li>";
	   var typeName = '<span class="ysi-type">'+obj.typeName+'：</span>';
	   var goodsName = '<span class="ysi-name" title='+obj.goodsName+'>'+obj.goodsName+'</span>';
	   var quantity = '<span class="ysi-amount">'+obj.quantity+'</span>';
	   var li_2 ="</li>";
	   htmlStr = htmlStr+li_1+typeName+goodsName+quantity+li_2;
	   //yh-slc-items		   
   });
    if(htmlStr!=""){
       $('.yh-slc-items').html(htmlStr)
       $('.yh-slc-selected').show();
       //yh-slc-none
       $('.yh-slc-none').hide();
    }
});	


//选择事件
$('.btn_xuan').live('click',function(){
      var checkArray = new Array(); 
      var isCombStructName = '';
	  //获取当前选择对象
	  var obj = $(this);	    
	  //判断是否从更换页面选择
	  if(typeof(obj.attr('data-changeType'))!='undefined')	  
	  {
	     return false;
	  }
	  //选择商品，更换商品
	  $('.active_xuan').each(function(){		  
		  var obj1 = $(this);
		  var dataType = obj1.attr('data-type');
		  //如果是非结构化套餐
		  if('packageComb'==dataType){
			 var hotelComb = new Object(); 
			 hotelComb.typeName = '酒店套餐';
			 if(obj.attr('data-type')==obj1.attr('data-type')){
			  hotelComb.goodsName=obj.attr('showgoodsname');
			  hotelComb.quantity=obj.attr('data-quantity')+"份";
			  
			  checkArray.push(hotelComb);
			 }else{
			  hotelComb.goodsName=obj1.attr('showgoodsname');
			  hotelComb.quantity=obj1.attr('data-quantity')+"份";
			  checkArray.push(hotelComb);
			 }
		  }	
			 
		  //结构化套餐
		  if('comb_struct_ticket'==dataType || 'comb_struct_ticket'==dataType){
			  if(isCombStructName == ''){  
			    isCombStructName = $('#currentgoodsname').text();
			    var comb_struct = new Object(); 
			    comb_struct.typeName = '酒店套餐';
			    comb_struct.goodsName=isCombStructName;
		        var comb_quantity = $("#preorder-quantity").val();
		        comb_struct.quantity=comb_quantity+"份";
		        checkArray.push(comb_struct);
			  }
		  }		 	 
		 //如果是酒店
		 if('hotel'==dataType){
			 var hotel = new Object();
			 hotel.typeName = '房型';
			 if(obj.attr('data-groupid')==obj1.attr('data-groupid')){
			     console.log('---groupid 相等---');
				 hotel.goodsName=obj.attr('showGoodsName');
				 var hotelQuantity = obj.parent().prev().find('.hotel-select').find("option:selected").text()+"间";
				 hotel.quantity = hotelQuantity;
				 checkArray.push(hotel); 				 
			 }else
			 {				 
				 hotel.goodsName=obj1.attr('showGoodsName');
				 var hotelQuantity = obj1.parent().prev().find('.hotel-select').find("option:selected").text()+"间";
				 hotel.quantity = hotelQuantity;
				 checkArray.push(hotel); 
			 }
		 }
		  
	  });
	   
		//遍历门票
	    $('.buy_number.ticket-select').each(function(){
	       	var ticket = $(this);
	    	ticket.typeName="门票";
	    	ticket.goodsName=ticket.attr('showGoodsName');
	    	ticket.quantity=ticket.find("option:selected").text()+"张";
	    	checkArray.push(ticket);
	    });
	  var htmlStr = "";
	  $.each(checkArray, function(index,obj){      
	     var li_1 ="<li>";
	  	 var typeName = '<span class="ysi-type">'+obj.typeName+'：</span>';
	  	 var goodsName = '<span class="ysi-name" title='+obj.goodsName+'>'+obj.goodsName+'</span>';
	  	 var quantity = '<span class="ysi-amount">'+obj.quantity+'</span>';
	  	 var li_2 ="</li>";
	  	 htmlStr = htmlStr+li_1+typeName+goodsName+quantity+li_2;	   
	  });
	    if(htmlStr!=""){
	       $('.yh-slc-items').html(htmlStr);
	       $('.yh-slc-selected').show();
	       $('.yh-slc-none').hide();
	     }
	  	  
});

</script>
<script>
function pyRegisterCvt(){
	var w=window,d=document,e=encodeURIComponent;
	var b=location.href,c=d.referrer,f,g=d.cookie,h=g.match(/(^|;)\s*ipycookie=([^;]*)/),i=g.match(/(^|;)\s*ipysession=([^;]*)/);
	if (w.parent!=w){f=b;b=c;c=f;};u='//stats.ipinyou.com/cvt?a='+e('Rds.9ps.e7t6CKVu8aDDv6A4ka6sRP')+'&c='+e(h?h[2]:'')+'&s='+e(i?i[2].match(/jump\%3D(\d+)/)[1]:'')+'&u='+e(b)+'&r='+e(c)+'&rd='+(new Date()).getTime()+'&e=';
	(new Image()).src=u;
}
</script>


