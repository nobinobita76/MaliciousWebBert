        result : null,
        init: function() {
            this.loadCM();
		    var params = {
		    	productId: 608009,
		    	businessType : 'DestBu'
		    };
		    $.ajaxSettings.async = false;
		    $.getJSON('http://dujia.lvmama.com/package/data.json', params, function(response) {
		    	result = response;
		        result.forEach(function(data){
		            if(data.price >=0 && data.surplus != 0) {
		                dateArr[datePos] = data.date;
		                dateStampFlags[datePos] = data.stampFlag;
						datePos++;
		            }
				});
		    });
		    $.ajaxSettings.async = true;
            this.loadCalendar();
            this.loadDepartureDate();//加载出发日期
          
             //跳转到最近有时间价格表的月份
