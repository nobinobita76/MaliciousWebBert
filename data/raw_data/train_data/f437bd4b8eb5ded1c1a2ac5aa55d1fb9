<script type="text/javascript">
window.onload = function() {
	$.searchType.showType('SCENICTOUR');
}

$(function(){

        $("a.lineRecommend").click(function() {
		    cmCreateElementTag("酒店套餐_线路推荐产品点击","线路推荐产品点击");
		});
		$("p.lineRecommend").click(function() {
		    cmCreateElementTag("酒店套餐_线路推荐产品点击","线路推荐产品点击");
		});

	var fv={};
	fv.startCheckFVstatus=function(){
         $.getJSON(
            "http://login.lvmama.com/nsso/ajax/checkLoginStatus.do?jsoncallback=?",
            function(data) {
                if (data.success) {
                  fv.checkFavorite();
                }
                else {
                    fv.bindFVclickEvent();
                }
            }
        );
	};
	fv.checkFavorite= function(){
        var objectId=608009
		$.ajax({
		    type: "get",
			contentType: 'application/json;charset=utf-8',
            url: "destbu/checkFavoriteStatus",
            cache: false,
            data: {
                objectId:objectId
            },
            success: function(data) {
                if(data.success == "true"){
                    $(".btn_collect.clearfix").attr('class','btn_collect clearfix current');
                    var collection = $(".btn_collect.clearfix").attr('class');    
                    if(collection=="btn_collect clearfix current"){
                       fv.bindFVclickEvent();
                     }
                } else {
                    fv.bindFVclickEvent();
                }
            },
            error : function(){
                //do nothing
                fv.bindFVclickEvent();
            }
        });
	};
	
	fv.deleteFavorite=function(){
			 var objectId=608009
            $.ajax({
		    type: "get",
			contentType: 'application/json;charset=utf-8',
            url: "destbu/checkFavoriteStatus",
            cache: false,
            data: {
                objectId:objectId
            },
            success: function(data) {
                if(data.success == "true"){
                    $.ajax({
                            type: "get",
                        	contentType: 'application/json;charset=utf-8',
                            url: "destbu/deleteFavorite",
                            cache: false,
                            data: {
                                objectId:objectId
                            },
                            success: function(data) {
                                if(data.success == "true"){
                                   $(".btn_collect.clearfix").attr('class','btn_collect clearfix');
                                }else if(data.success == "false"){
		                             pandora.dialog({
								        content: "取消收藏失败",
										okValue: "确定",
								        okClassName:"btn btn-small btn-orange",
								        ok: function () {
								        },
										cancelClassName:"btn btn-small",
										cancelValue: "关闭",
										cancel: true
								     });
                                }
                            },
                            error : function(){
                            }
                        });
                    
                    } else {
                     $(".btn_collect.clearfix").attr('class','btn_collect clearfix');

                    var collection = $(".btn_collect.clearfix").attr('class');    
                    if(collection=="btn_collect clearfix current"){
                       fv.bindFVclickEvent();
                     }
                }
            },
            error : function(){
                //do nothing
                fv.bindFVclickEvent();
            }
        });           
                       
		}
		
		
		
	
	fv.addFavorite=function(){
         var objectId=608009;
        $.ajax({
            type: "get",
        	contentType: 'application/json;charset=utf-8',
            url: "destbu/addFavorite",
            cache: false,
            data: {
                objectId:objectId,
                objectName:encodeURI("【阳光明媚海泉湾踏春季】住珠海海泉湾客栈1晚＋享双人早餐＋享受无限次海洋温泉＋无限次神秘岛主题乐园＋康乐设施任选两项")
            },
            success: function(data) {
                if(data.success == "true"){
                    $(".btn_collect.clearfix").attr('class','btn_collect clearfix current');
                }else if(data.success == "false"){
                    $(".btn_collect.clearfix").attr('class','btn_collect clearfix current');
                }
            },
            error : function(){
                pandora.dialog({
						        content: "收藏失败",
								okValue: "确定",
						        okClassName:"btn btn-small btn-orange",
						        ok: function () {
						        },
								cancelClassName:"btn btn-small",
								cancelValue: "关闭",
								cancel: true
						});
            }
        });
    };
   
   	fv.bindFVclickEvent=function(){
			$(".btn_collect.clearfix").live("click",function(){
			    var collection = $(this).attr('class');    
		 		if(collection=="btn_collect clearfix current"){
			 		fv.deleteFavorite();
			 	}else{
				 	$.getJSON(
			                "http://login.lvmama.com/nsso/ajax/checkLoginStatus.do?jsoncallback=?",
			                {},
			                function(data) {
			                    if (data.success) {
			                      	fv.addFavorite();
			                    }else{ 
			                     showLogin(function(){
			                              //$("#submitDataForm").submit();
			                              fv.addFavorite();
			                              $('.dialog-close').click();//如果没有关闭当前窗口，则调用这个click。
			                           });
			                        //cmCreatePageviewTag("登陆浮动层-酒店套餐-国内","虚拟浏览");
			                    }
			                }
			        );}
			      });
    	};
    //invoke fv statu check
    fv.startCheckFVstatus();
});
</script>
