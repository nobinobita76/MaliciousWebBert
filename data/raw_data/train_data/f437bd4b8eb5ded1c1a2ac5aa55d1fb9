 <!--360聚效开始  by 王永方 -->
 <script>
 var categoryId_new = 17; 
var  productUrl = "";//产品url
var categoryname = "";

 //是否可售
 var saleFlag =  "Y";
if(saleFlag == 'Y'){
     saleFlag = "1";
 }else if(saleFlag == 'N'){
     saleFlag = "0";
 }else{
 }

 var saleprice = "580.0";
 if(saleprice == ""){
     saleprice = 0;
 }else{
 
 }
 
 var imgurl = "";
 
     imgurl = "/uploads/pc/place2/2015-10-26/a0f5debc-5284-44a7-af7a-2f7735b05764.jpg";
 
 if(imgurl == ""){
     saleFlag = "0";
     imgurl = "http://s1.lvjs.com.cn/img/logo/lv_logo.png";
 }else{
    var photoLen = imgurl.length-4;
    var phototype = imgurl.substring(photoLen,photoLen+4);//图片类型
    var prhotourl = imgurl.substring(0,photoLen);
    imgurl = "http://s2.lvjs.com.cn" + prhotourl+"_480_320"+phototype;//带尺寸完整url    
 }
 
  //判断品类名称
      if(categoryId_new == 18){
           categoryname ="自由行";
           productUrl  = "http://dujia.lvmama.com/freetour/608009";
       }else if(categoryId_new == 17) {
           categoryname ="酒店套餐";
           productUrl  = "http://dujia.lvmama.com/package/608009";
       }else{
          categoryname ="自由行";
          productUrl  = "http://dujia.lvmama.com/freetour/608009";
       }
 
 if(saleFlag == "1"){  
var _mvq = window._mvq || []; 
		window._mvq = _mvq;
		_mvq.push(['$setAccount', 'm-193368-0']);
		_mvq.push(['$setGeneral', 'goodsdetail', '', '', '']);
		_mvq.push(['$logConversion']);
		_mvq.push(['setPageUrl',  productUrl]);
		_mvq.push(['$addGoods', '',  '', /*产品名称*/ '【阳光明媚海泉湾踏春季】住珠海海泉湾客栈1晚＋享双人早餐＋享受无限次海洋温泉＋无限次神秘岛主题乐园＋康乐设施任选两项',/*产品ID*/ '608009',/*产品售价*/ saleprice, /*产品图片url*/ imgurl, '', '', /*产品库存状态1在售或是0售完或下架*/ saleFlag, '', '', '', /*品类,productType_categoryName*/ categoryname, '', '']);
		_mvq.push(['$addPricing', '']);
		_mvq.push(['$logData']);

}else if(saleFlag == "0"){
		var _mvq = window._mvq || []; 
		window._mvq = _mvq;
		_mvq.push(['$setAccount', 'm-193368-0']);
		_mvq.push(['$setGeneral', 'goodsdetail', '', '', '']);
		_mvq.push(['$logConversion']);
		_mvq.push(['setPageUrl',  productUrl]);
		_mvq.push(['$addGoods', '',  '', /*产品名称*/ '【阳光明媚海泉湾踏春季】住珠海海泉湾客栈1晚＋享双人早餐＋享受无限次海洋温泉＋无限次神秘岛主题乐园＋康乐设施任选两项',/*产品ID*/ '608009',/*产品售价*/ saleprice, /*产品图片url*/ imgurl, '', '', /*产品库存状态1在售或是0售完或下架*/ saleFlag, '', '', '', /*品类,productType_categoryName*/ categoryname, '', '']);
		_mvq.push(['$addPricing', '']);
		_mvq.push(['$logData']);

}	 

$(".js_tip").poptip({
            offsetX: 0
        });
</script>
