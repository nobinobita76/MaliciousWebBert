    $('.calendar-info').click(function(e){
        $('.JS_calendar').trigger('click');
        e.stopPropagation();
    });
    
    var dateArr = new Array(); // 记录时间价格的日期
    var dateStampFlags = new Array(); //stampFlags Array
    var datePos = 0; // 时间价格日期的数组坐标位
    
     //出发日期 获取默认日期的星期值
    var $selectDay = $('.selectDay');
    var aDayOfWeek = ['日','一','二','三','四','五','六'];
    
    
    
    var publicCallBack = {};
    var DestBu = window.DestBu = {
        calendarLastDate: null, //上次日历展示的日期
        calendarEndDate: null,  //日历数据截止日期
        isFirstLoadCalendar: true, //是否第一次加载日
        departureCalendar : null ,
