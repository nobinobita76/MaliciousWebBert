         loadDepartureDate: function() {
            var self = this;
           
            // 出发日期日历
		    departureCalendar = lv.calendar({
		        trigger: ".JS_calendar",
		        monthNext: 12,
		       
		        zIndex: 10,
		        bimonthly: true,
		        titleTip: "<dfn>{{year}}</dfn>年{{month}}月",
		        template:  "small",
		        autoRender: false,
		        sourceFn:function(obj){
		        	
	        		// 处理没有时间价格的日期
		            var tdElement=$(".ui-calendar-mini .caltable").find("td");
					$(tdElement).each(function(t,v){
						var date=$(v).attr("data-date-map");
						var isNodate = true;
						for(var i=0; i<datePos; i++){
							if(dateArr[i]==date && dateStampFlags[i] != '1' && dateStampFlags[i] != '2'){
							  	isNodate = false;
							  	break;
							}
						}
						$(v).find("div:eq(0)").addClass("caldisabled");
						if(!isNodate){
							var dateParam = { year: date.substring(0,4), month: date.substring(5,7), day: date.substring(8,10) };
							
							$(v).find("div:eq(0)").removeClass("caldisabled");//.addClass(obj.getClass(dateParam)); 
						}
					});
					
					var tdElementSecond, tdElementFirst;
					$($(".ui-calendar-mini .caltable")[1]).find("td").each(function(){
						if($(this).attr("data-date-map") !=undefined){
							tdElementSecond = this;
						}
					});
					
					$($(".ui-calendar-mini .caltable")[0]).find("td").each(function(){
						if($(this).attr("data-date-map") !=undefined){
							tdElementFirst = this;
							return false;
						}
					});
					
					/*if($(tdElementSecond).attr("date-map")>=dateArr[dateArr.length-1]){
						$(".ui-calendar-mini .month-next").toggle(false);//不显示下一页
					}else{
						$(".ui-calendar-mini .month-next").toggle(true);//显示下一页
					}
					
					if($(tdElementFirst).attr("date-map")<=dateArr[0]){
						$(".ui-calendar-mini .month-prev").toggle(false);//不显示上一页
					}else{
						$(".ui-calendar-mini .month-prev").toggle(true);//显示上一页
					}*/
					
		        },
		        selectDateCallback: function(obj,element){	
		     
		        	calendar.toMonth(obj.getSelect()[0].substring(0,4),obj.getSelect()[0].substring(5,7));
		         	$(".calendar_box_new .caltable td").removeClass("td-selected");
		        	//日历框联动
		        	var el = $(".calendar_box_new .caltable td[data-date-map='" + obj.getSelect()[0] + "']").addClass("td-selected");
		        	
		        	//选择后设置日期
		        
		            var week = parseInt(element.attr('data-week'));
		            $selectDay.html('星期' + aDayOfWeek[week]);
		            
                    var routeId = el.find(".calactive").attr("routeId");
                    if(routeId) { //时间价格表和行程联动
                        $(".xingcheng_all .instance_tab li[data-line='" + routeId + "'").trigger("click");
                    }else{// 单行程，取默认
                    	routeId = $("#defaultLineRouteId").val();
                    }
                    
	                window.DestGoods.javaCallback.loadingSuppGoodsCallBack(obj.getSelect()[0],routeId);
		        }
		    });
        },
        loadCalendar: function() {
            var self = this;
           calendar = lv.calendar({
           		autoRender: true,  //自动渲染日历
                trigger: ".calendar_box_new",//渲染容器
                monthNext: 12,
                isTodayClick: true,
                selectDateCallback:  function(that,element){//点击选择日期后的回调函数
                	
                	$(".JS_calendar").val(that.getSelect()[0]);
                	//判断下个月是否有可售日期
                	var selectedMonth =  parseInt(that.getSelect()[0].substring(5,7));
                	var selectedYear =  parseInt(that.getSelect()[0].substring(0,4));
                	var nextMonthDate,curMonthFirstDate;
                	if(selectedMonth == 12){
						nextMonthDate = (selectedYear+1) + '0101';
					}else{
						var nextMonth =selectedMonth+1+'';
						if(nextMonth.length == 1){
							nextMonthDate = selectedYear + '0' + nextMonth + '01';
						}else{
							nextMonthDate = selectedYear + '' + nextMonth + '01';
						}
					}
					curMonthFirstDate = selectedYear + '' + selectedMonth + '01';
                	var maxEffectiveDate = dateArr[dateArr.length-1].replace(/-/g,'');
                	var minEffectiveDate = dateArr[0].replace(/-/g,'');
                	
                	if(maxEffectiveDate >= nextMonthDate &&  curMonthFirstDate > minEffectiveDate){
                		departureCalendar.toMonth(selectedYear,selectedMonth);
                	}else if(nextMonthDate > maxEffectiveDate){
                		departureCalendar.toMonth(selectedYear,selectedMonth - 1);
                	}else if(minEffectiveDate >= curMonthFirstDate){
                		departureCalendar.toMonth(selectedYear,selectedMonth + 1);
                	}
                	       
                	//选择后设置日期
		            var week = parseInt(element.attr('data-week'));
		            $selectDay.html('星期' + aDayOfWeek[week]);
		            $(".caltable td[data-date-map]").removeClass("td-selected");
                    var el = $(".caltable td[data-date-map='" + that.getSelect()[0] + "']").addClass("td-selected");
                    var routeId = el.find(".calactive").attr("routeId");
                    if(routeId) { //时间价格表和行程联动
                        $(".xingcheng_all .instance_tab li[data-line='" + routeId + "'").trigger("click");
                    }else{// 单行程，取默认
                    	routeId = $("#defaultLineRouteId").val();
                    }
                    
	                window.DestGoods.javaCallback.loadingSuppGoodsCallBack(that.getSelect()[0],routeId);
                },//点击选择日期后的回调函数
                completeCallback: function () {//数据加载完成并显示出日历后的回调函数,点击上下月回调函数。
                    $('.calsale').poptip();               
                },
                sourceFn:function(){
                
            var self = this;
              /**
         * 获取剩余HTML
         * @param inventory 剩余数量
         * @returns {string} 生成的HTML
         */
        function getStoreHTML(inventory) {
            var html = "";
            if (inventory == 0) {
                html = "售罄";
            } else if (0<inventory && inventory < 10) {
                html = "余" + inventory
            }
            return html;
        }

        /**
         * 创建浮动框
         * @returns {*|jQuery|HTMLElement}  被创建的浮动框jQuery对象
         */
        function createHover() {
            var $hover = $(".calhover");
            if ($hover.length <= 0) {
                $hover = $('<div class="calhover"><div class="triangle"></div></div>');
            } else {
                $hover.html('<div class="triangle"></div>');
            }
            $("body").append($hover);
            $hover.removeClass("calhover-right");
            return $hover;
        }   
             /**
             * 退化的情况，无任何返回值
             */
            if (!result) {
                return false;
            }

            var $allTd = self.wrap.find('td[data-date-map]');  //所有的日历单元格
            $allTd.children().addClass("caldisabled");  //先禁用所有的日历日期

            //对json对象进行迭代处理
            result.forEach(function (row) {
			    var el = $($($(".caltable")[0]).find("td[data-date-map='" + row.date + "']"));
                if(row.stampFlag=='1'){	                  
                    var htmlStr = el.children('div').html();
                    var str = '<span class="calinfo tagsback noborder" style="color: #999" tip-content="可现在先买预售券，再预约出游日期~">正在预售</span>';
                    el.children('div').html(htmlStr+str);
                    el.children('div').addClass('nodate');		                   
                }else if(row.stampFlag=='2') {
                	var htmlStr = el.children('div').html();
                    var str = '<span class="calinfo tagsback noborder" style="color: #999" tip-content="如果您之前购买过该日期段的预售券，现在可预约出游日期哦~">预售预约</span>';
                    el.children('div').html(htmlStr+str);
                    el.children('div').addClass('nodate');	
                }else{
                var jsonDateStr = row.date;  //json单元-日期

                //将json单元日期字符串转化为JS日历对象(new Date())
                var date = lv.calendar.getDateFromFormattedString(jsonDateStr, self.options.dateFormat);

                //将日历对象转换为字符串(只保留参数dateFormat设定的数据，默认值为年月日)
                var dateStr = lv.calendar.dateFormat(date, self.options.dateFormat);

                //json单元-价格
                var price = row.price;

                //价格-浮点数
                var inventory = parseFloat(row.surplus);

                //json单月-是否促销
                var sale = row.detail;

                //json单元-日期对应文档中的td单元格
                var $td = self.wrap.find('td[data-date-map=' + dateStr + ']');

                //如果json中的数据有td单元格相对应，则显示数据信息
                if ($td && price) {

                    //促销等元素的显示位置
                    var $calActive = $td.find(".calactive");
                    //显示库存
                    $td.find(".calinfo").html(getStoreHTML(inventory));

                    //是否售罄
                    if (inventory == 0) {
                        $td.find(".calinfo").addClass("sellout");
                    } else {
                         $td.children().removeClass("caldisabled");
                         //显示价格
                   		 $td.find(".calprice").html('<i>¥</i><em>' + price + '</em>起');
               		    //是否促销
	                    if (sale) {
	                        var $sale = $('<div class="calsale">促</div>');
	                        $calActive.append($sale);
	                    }
				        if(row.lineRouteName) {
				            $calActive.attr("routeId", row.lineRouteId).html(row.lineRouteName);
				        }
                    }

                }
                }
            });
            
            
                   // 处理没有时间价格的日期
		            var tdElement=$(".caltable").find("td");
					$(tdElement).each(function(t,v){
						var date=$(v).attr("data-date-map");
						if(date == $(".JS_calendar").val()){
							$(v).addClass("td-selected");
						}
						var isNodate = true;
						for(var i=0;i<datePos;i++){
							if(dateArr[i]==date){
							  	isNodate = false;
							  	break;
							}
						}	
						if(isNodate){
							$(v).find("div:eq(0)").removeClass().addClass("caldisabled"); 
						}
                    });
            
            
            
            //显示促销/线路/休假浮动框
            (function () {

                var festival;  //节日
                var route;  //线路
                var sale;  //促销

                //鼠标移开，隐藏浮动框
                self.wrap.on("mouseleave", "[data-date-map]", function () {
                    var $hover = $(".calhover");
                    $hover.hide();
                    $hover.css({
                        left: 0,
                        top: 0
                    });
                });

                //鼠标移入，显示浮动框
                self.wrap.on("mouseenter", "[data-date-map]", function () {
                    var hasOnce = false;

                    //td单元格
                    var $this = $(this);

                    //sting 当前单元格日期字符串
                    var date = $this.attr("data-date-map");

                    //创建浮动框jQuery DOM对象
                    var $hover = createHover();

                    //休假
                    var $calfestival = $('<p class="calfestival"><i>休</i><span></span></p>');
                    var $calfestivalContent = $calfestival.find("span");

                    //线路
                    var $calroute = $('<p class="calroute"><i> </i><span></span></p>');
                    var $calrouteTitle = $calroute.find("i");
                    var $calrouteContent = $calroute.find("span");

                    //促销
                    var $calsale = $('<p class="calsale"><i>促</i><span></span></p>');
                    var $calsaleContent = $calsale.find("span");

                    //显示坐标
                    var left = $this.offset().left;
                    var top = $this.offset().top + $this.outerHeight();

                    //节日
                    var thatFestival = self.options.festival[date];
                    festival = thatFestival;
                    if (thatFestival) {
                        hasOnce = true;
                        $calfestivalContent.html(thatFestival.vacationName);
                        $hover.append($calfestival);
                    }

                    //获取json数据填充到页面中
                    result.forEach(function (row) {
                        if (row.date == date) {
                            var route = row.lineRouteName;
                            if (row.sale) {
                                var sale = row.sale.replace(/\n/g, '<br/>');
                            }

                            if (route) {
                                hasOnce = true;
                                $calrouteTitle.html(route);
                                $calrouteContent.html("线路");
                                $hover.append($calroute);
                            }
                            if (sale) {
                                hasOnce = true;
                                $calsaleContent.html(sale);
                                $hover.append($calsale);
                            }
                        }
                    });

                    //页面右侧处理，如果屏幕过小，则显示在左侧
                    var width = $hover.outerWidth();

                    var $table = $this.parents(".caltable");
                    var tableLeft = $table.offset().left;
                    var tableWidth = $table.outerWidth();
                    if (width + left - tableLeft > tableWidth) {
                        left = tableLeft + (tableWidth - width);
                        $hover.addClass("calhover-right");
                    }

                    //显示
                    if (hasOnce) {
                        $hover.show();
                        $hover.css({
                            left: left,
                            top: top
                        });
                    }

                });

            })();
            }
           });
        },
        loadCM: function() {
            _LVMAMA_COREMETRICS.init(document.domain);
            cmCreatePageviewTag("国内度假_产品详情页_酒店套餐_608009_国内", "JDTC001", null, null);
            cmCreateProductviewTag('608009', '【阳光明媚海泉湾踏春季】住珠海海泉湾客栈1晚＋享双人早餐＋享受无限次海洋温泉＋无限次神秘岛主题乐园＋康乐设施任选两项', '酒店套餐','目的地BU');
        },
        formatDate: function(date) {
            return date.getFullYear() + '-' + (date.getMonth() < 9 ? '0' + (date.getMonth() + 1) : date.getMonth() + 1)
                    + "-" + (date.getDate() < 10 ? '0' + date.getDate() : date.getDate());
        },
        loadComment: function() {
            var self = this;
            /**
             *相关点评翻页
             * @param type          点评类别
             * @param currentPage   当前页
             * @param totalCount    总数
             * @param placeId       POI
             * @param productId     产品ID
             * @param placeIdType   POI类型
             * @param isPicture     晒图
             * @param isBest        精华
             * @param isPOI         是不是POI
             * @param isELong       是不是艺龙点评
             * @param productRef    相关点评产品id串,逗号分隔
             * @param vstType       产品品类
             * @param isRef         相关点评类型
             */
            window.newLoadPaginationOfCommentRef = function(type,currentPage,totalCount,placeId,productId,placeIdType,isPicture,isBest,isPOI,isELong,productRef,vstType,isRef){
              var paramsVal = {};
	          paramsVal.currentPage = currentPage;
	          paramsVal.type = type;
	          paramsVal.totalCount = totalCount;
	          paramsVal.placeId = placeId;
	          paramsVal.placeIdType = placeIdType;
	          paramsVal.isPicture=isPicture;
	          paramsVal.isBest=isBest;
	          paramsVal.isPOI = isPOI;
	          paramsVal.isELong=isELong;
	          paramsVal.productRef=productRef;
	          paramsVal.vstType=vstType;
	          paramsVal.isRef = isRef;
	          var params = JSON.stringify(paramsVal);	          
              $.ajax({
                   url: "/comment/destNewPaginationOfComments",
                   type:"POST",
                   data:{jsonData: params},
                   dataType: 'html',
                   success: function(data) {
                       $("#"+type+"CmtComment").html(data);
                       $(window).scrollTop($('#product-customer-review').offset().top);
                   }
              });
            };
        },
        bindEvent: function() {
            //背书链接
            $(".bz_list li").live("click", function(e){
            	e.stopPropagation();
                window.open('http://www.lvmama.com/public/user_security#zby');
            });

            $('#preorder-confirm-button').live('click',function(e){//form提交
            	if($(this).attr("data-disable")){
            		$(window).scrollTop($('div.calendar_box_new').offset().top);
            		$(".JS_calendar").click();
            		return false;
                }else{
                	if($(".buy_number.ticket-select.disney").length > 0) {
	                	var totalDisney = 0;
	                	$(".buy_number.ticket-select.disney").each(function() {
	                		totalDisney += parseInt($(this).find("option:selected").text());
	                	});
	                	
	                	if(totalDisney > 5) {
	                		$.alert("一笔订单中迪士尼门票最多选购5张，超过5张订单提交失败，如订购数量超过5张，请分开下单。");
	                		$(".buy_number.ticket-select.disney").get(0).focus();
	                		return false;
	                	}
	                }
                	// 驴妈妈-点击立即抢购&开始预订触发  品友dsp
	                pyRegisterCvt();
               		e.stopPropagation();
                	window.DestGoods.javaCallback.preorderStart(e);
                	return false;
                }
            });
            
            //$('#preorder-confirm-button').live('mouseover',function(){
            //	if($(this).attr("data-disable")){
            		//btnTips($('.btn-big'),'请选择出行日期'); //提示的时候调用
            		//$(".JS_calendar").click();
            //	}
            //});
            
            //$('#preorder-confirm-button').live('mouseout',function(){
            //	$('.js_btnTips').remove(); //删除提示
            //});
            
            $('.product-select-quantity').live('change',function(e){//份数选择
            	e.stopPropagation();
            	var baseAdultQuantity = $("#adult-count").val();
	            var baseChildQuantity = $("#children-count").val();
	            var quantity = $("#preorder-quantity").val();
            	var quantityHiddenFlag = $("#preorder-quantity").data("hidden");
            	
              	if(quantityHiddenFlag == 'N'){//按份售卖
                  	$("#adultCounts").text(baseAdultQuantity*quantity);
                  	$("#childCounts").text(baseChildQuantity*quantity);
              	}
                window.DestGoods.javaCallback.loadingSuppGoodsCallBack(null);
                return false;
            });
        }
    };
    DestBu.init();
      $("#taocan_Cmt").bind("click",function(){
        var count = $("#allCmtComment").find(".comment-li").length;
        if(count <= 0){
            Comment.newLoadPaginationOfComment({type:"all",currentPage:1,totalCount:210,productId:'608009',isPicture:"N",isBest:"N",isPOI:"Y",isELong:"N"});
        }
    });

    $("#allCmt").bind("click",function(){
        var count = $("#allCmtComment").find(".comment-li").length;
        if(count <= 0){
            Comment.newLoadPaginationOfComment({type:"all",currentPage:1,totalCount:210,productId:'608009',isPicture:"N",isBest:"N",isPOI:"Y",isELong:"N"});
        }
    });

    $("#bestCmt").bind("click",function(){
        var count = $("#bestCmtComment").find(".comment-li").length;
        if(count <= 0){
            Comment.newLoadPaginationOfComment({type:"best",currentPage:1,totalCount:21,productId:'608009',isPicture:"N",isBest:"Y",isPOI:"Y",isELong:"N"});
        }
    });

    $("#picCmt").bind("click",function(){
        var count = $("#picCmtComment").find(".comment-li").length;
        if(count <= 0){
            Comment.newLoadPaginationOfComment({type:"pic",currentPage:1,totalCount:93,productId:'608009',isPicture:"Y",isBest:"N",isPOI:"Y",isELong:"N"});
        }
    });
