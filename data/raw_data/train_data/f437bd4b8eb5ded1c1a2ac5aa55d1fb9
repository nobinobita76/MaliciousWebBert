<script src="http://s3.lvjs.com.cn/min/index.php?f=/js/new_v/top/header-air_new.js"></script>
<script type="text/javascript">
	var loadTripsNumDefault = "no";
	var dataArray = [];//本地存储数据
	$(document).ready(function(){
		init();
		addLoscHref();
	});
	
	//初始化数据
	function init(){
		//直接加载层对应数据(加载缓存数据)
		//console.info(dataArray.length);
		if(dataArray.length<=0){
			loadPaginationOfTripsDefault(1);
			ajaxData(1, 1);//精华
		}
		//var index = getIndex();
		//dataArray[index] = $(".tripListBox").html();
	}

	//全部事件
	$("#allTabTrip").bind("click",function(){
		$("#elite").val(0);//全部
		refresh();
	});
	
	//精华事件
	$("#bestTabTrip").bind("click",function(){
		$("#elite").val(1);//精华
		refresh();
	});
	
	/**
	*游记部分(分页)
	*@param currentPage 页数
	*/
	function loadPaginationOfTrips(currentPage){
		var elite = getIndex(getElite());
		ajaxData(currentPage, elite, function(data){
			$(".tripListBox").html(data);
		});
	}
	/**
	*Ajax请求数据
	*@param elite 全部/精华
	*@param callFn 回调函数
	*/
	function ajaxData(currentPage, elite, callFn){
		var categoryCode  = $("#category_code").val()!='' ? $("#category_code").val() : "";
		
		$.ajax({
	         url: "/freetour/destbu/loadPaginationOfTrips",
	         data: {
	             currentPage:currentPage,
	             destId:$("#destId").val(),
	             elite:getElite(elite),
	             categoryCode:categoryCode
	         },
	         dataType: 'html',
	         success: function(data) {
	        	 var index = getIndex(elite);
	        	 dataArray[index] = data;
	        	 if(callFn) callFn(data);
	         },
	         error:function(res){
	        	 //console.info("ajaxData failure["+ res.status +"]");
	         }
	    });
	    
	}
	/**
	*第一次加载游记返现游记，需要修改相关游记的数量标签
	*@param currentPage 页数
	*/
	function loadPaginationOfTripsDefault(currentPage){
		var elite = getIndex(getElite());
		ajaxDataDefault(currentPage, elite, function(data){
			$(".tripListBox").html(data);
			 if(data){
	        		var categoryId = '17';
					addLoscHref2();
	        	 }
		});
	}
	/**
	*Ajax请求数据
	*@param elite 全部/精华
	*@param callFn 回调函数
	*/
	function ajaxDataDefault(currentPage, elite, callFn){
		var categoryCode  = $("#category_code").val()!='' ? $("#category_code").val() : "";
		
		$.ajax({
	         url: "/freetour/destbu/loadPaginationOfTrips",
	         data: {
	             currentPage:currentPage,
	             destId:$("#destId").val(),
	             elite:getElite(elite),
	             categoryCode:categoryCode
	         },
	         dataType: 'html',
	         success: function(data) {
	        	 var index = getIndex(elite);
	        	 dataArray[index] = data;
	        	 if(callFn) callFn(data);
	        	 changeDestOrder();
	         },
	         error:function(res){
	        	 //console.info("ajaxData failure["+ res.status +"]");
	         }
	    });
	    
	}
	//刷新div内容
	function refresh(){
		var index = getIndex();
		$(".tripListBox").html("");
		$(".tripListBox").html(dataArray[index]);
		//景酒增加losc
		var categoryId = '17';
		addLoscHref2();
	}

	/**
	*根据elite获取div选项索引
	*/
	function getIndex(elite){//直接加载层对应数据
		var elite = getElite(elite);
		if(!elite) return 0;//默认显示全部层
		return elite;
	}
	function getElite(elite){
		if(!elite) return $("#elite").val();
		return elite;
	}

	function addLoscHref(){

		var jz_tic_losc = '';
		var categoryId = '17';
		//var packType = 'SUPPLIER';
		if(categoryId == 18){
			jz_tic_losc = '?losc=056659&ict=i';
		}else if(categoryId == 17){// && packType=='LVMAMA'
			jz_tic_losc = '?losc=056661&ict=i';
		}else if(categoryId == 15){//packType=='SUPPLIER'
			 jz_tic_losc = '?losc=056662&ict=i';
		}else if(categoryId == 16){
			 jz_tic_losc = '?losc=162285&ict=i';
		}/* else if(categoryId == 11){
			 jz_tic_losc = '?losc=058172&ict=i';
		} */
		if(jz_tic_losc!=''){
			$('.trip-listPic').each(function(){
				var herf = $(this).attr('href');
				var anchor = $(this).attr('data');
			    $(this).attr('href',herf+jz_tic_losc+ "#p" +anchor);
			});
			$('.trip-list_info_content').each(function(){
				var herf = $(this).attr('href');
				var anchor = $(this).attr('data');
			    $(this).attr('href',herf+ jz_tic_losc+ "#p" +anchor);
			});
		}

	}
	function changeDestOrder(){
		$totalTrip = $("#destorder").find("li[date_id='xgtrip']").children("span");
		//if($totalTrip.attr("load")=="no"){
		if(loadTripsNumDefault=="no"){
			var totalTrip = $("#totalTrip").val();
			if((totalTrip >=0) && (totalTrip != null && totalTrip != undefined && totalTrip != '') ){
				$totalTrip.html("("+totalTrip+")");
			}
			//$totalTrip.attr("load","yes");
			loadTripsNumDefault=="yes";
		}
	}
	function addLoscHref2(){
		var jz_tic_losc = '';
		var categoryId = '17';
		if(categoryId == 18){
			jz_tic_losc = '?losc=056659&ict=i';
		}else if(categoryId == 17){
			jz_tic_losc = '?losc=056661&ict=i';
		}else if(categoryId == 15){
			 jz_tic_losc = '?losc=056662&ict=i';
		}
		if(jz_tic_losc!=''){
			$('.trip-listPic').each(function(){
				var herf = $(this).attr('href');
			    $(this).attr('href',herf+jz_tic_losc);
			    
			});
			$('.trip-list_info_content').each(function(){
				var herf = $(this).attr('href');
			    $(this).attr('href',herf+ jz_tic_losc);
			});	
		}
	}
</script>
			
        </div><!--product_detail结束-->

    </div><!--产品详情左侧栏--开始-->

</div><!--产品详情模块--结束-->

<script>
	//losc 增加
	$(function(){
		if($("#scenic_recommendation") && $("#scenic_recommendation")!=null){
			addLosc($("#scenic_recommendation"),'054087');
		}
		if($("#line_recommend") && $("#line_recommend")!=null){
			addLosc($("#line_recommend"),'054085');
		}	
	});

</script>    </div>
