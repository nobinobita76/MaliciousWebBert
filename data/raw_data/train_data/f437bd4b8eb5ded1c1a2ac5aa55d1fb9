<script type="text/javascript">

	$(function(){
		$(document).on("click", ".JS_hav_got_it",function () {
	        //知道了
	        havDialog.close(); 
	        clickClose = 1;
	    });
	    $(document).on("click", ".JS_hav_think_again",function () {
	        //再想想
	         clickClose = 2;
	        havDialog.close();
	    });
	});
    function dealtotalMoney(e) {
	    var type = $(e).data("type");
	
		var price = 0;
	    $(".lvmama-price-flag").each(function() {
	        var select_value = $(this).val();
	        if (select_value == null) {
	            select_value = 0;
	        }
	
	        price += parseInt(select_value);
	    });
	    var priceValue = 0;
	    if (price % 100 == 0) {
	        priceValue = price / 100;
	    } else {
	        priceValue = (price / 100).toFixed(2);
	    }
	    $("#total-price-value").html("<dfn>¥</dfn>" + priceValue);
	}
	
	$('.sale_by_people').children().each(function(){
		//自驾游按人，默认选择1个人
		var selfDrivingSaleType="";
		if(selfDrivingSaleType=="PEOPLE"){
			if($(this).val()==1){
				$(this).attr("selected","selected");
			}   
		}else{
			if($(this).val()==2){
				$(this).attr("selected","selected");
			}
		}
	});
	
	
    function checkGoodsIsChange(data){
    	if(data != ""){
		    $("#loadingGoodsContext").html(data);
			var oldLoadingGoods = $("#preorder-adjust");
			var newLoadingGoods = $("#loadingGoodsContext");
	    	var defrentHotelData = getHotelDefrentGoods(oldLoadingGoods,newLoadingGoods);
	    	var defrentTicketGoodsData = getTicketDefrentGoods(oldLoadingGoods,newLoadingGoods);
	    	var defrentTotelPackagelData = getHotelPackageDefrentGoods(oldLoadingGoods,newLoadingGoods);
	    	if(defrentHotelData != "" || defrentTicketGoodsData != "" || defrentTotelPackagelData != "" ){
	        	if(defrentHotelData != ""){
	        		$("#defrentHotelData").find(".hav-detail").html(defrentHotelData);
	        		$("#defrentHotelData").show();
	        	}else{
	        		$("#defrentHotelData").hide();
	        	}
	        	if(defrentTicketGoodsData != "" ){
	        		$("#defrentTicketGoodsData").find(".hav-detail").html(defrentTicketGoodsData);
	        		$("#defrentTicketGoodsData").show();
	        	}else{
	        		$("#defrentTicketGoodsData").hide();
	        	}
	        	if(defrentTotelPackagelData != ""){
	        		$("#defrentTotelPackagelData").find(".hav-detail").html(defrentTotelPackagelData);
	        		$("#defrentTotelPackagelData").show();
	        	}else{
	        		$("#defrentTotelPackagelData").hide();
	        	}
    		    return false;
	    	}else{
	    		return true;
	    	}
    	}else{
    		return true;
    	}
    	
    }
    
    var needChangeDefaultHotelPackage = false;
    var needChangeDefaultHotelPackageId = [];
    function getHotelPackageDefrentGoods(oldLoadingGoods,newLoadingGoods){
    	var defrentHotelData = "";
    	//结构化的酒套餐
    	oldLoadingGoods.find("#currentgoodsname").each(function(index,element1){
	    	var oldSelectTicket = $(element1);
	    	var isTrue = false; 
	    	var newHotelSelectName = "";
    		newLoadingGoods.find("#currentgoodsname").each(function(index,element){
    			if($(element1).attr("data-groupid") == $(element).attr("data-groupid")){
			    	var newSelectTicket = $(element);
			    	if(oldSelectTicket.attr("data-goodsId") && newSelectTicket.attr("data-goodsId")){
			    		newHotelSelectName = newSelectTicket.attr("data-name");
			    		if(oldSelectTicket.attr("data-goodsId") == newSelectTicket.attr("data-goodsId")){
			    			isTrue = true;
			    		}
			    	}
    			}
    		});
	    	if(!isTrue && newHotelSelectName != ""){
				defrentHotelData+= "<p>"+newHotelSelectName+"</p>";
	    	}
    	});
    	//非结构化的酒套餐
    	oldLoadingGoods.find(".active_xuan[data-type='packageComb']").each(function(index,element1){
			var newHotelSelectName = "";
			var isTrue = false; 
			var oldSelectHotelId = $(element1).attr("data-suppgoodsid");
			var oldGroupId=$(element1).attr("data-groupid");
			newLoadingGoods.find(".packagehouse-type-select[data-type='packageComb']").each(function(index,element){
				var thishotelId = $(element).attr("data-suppgoodsid");
				if(oldGroupId == $(element).attr("data-groupid")){
					if($(element).hasClass("active_xuan")){
						newHotelSelectName = $(element).attr("showGoodsName");
						if(thishotelId == oldSelectHotelId){
			    			isTrue = true;
						}
					}else{
						if(thishotelId == oldSelectHotelId){
							if(oldLoadingGoods.find("#hotelPackageDefaultBySystem[data-groupid='"+oldGroupId+"']").val() != oldSelectHotelId){
			    				isTrue = true;
			    				needChangeDefaultHotelPackage  = true;
			    				var dataMap={"hotelGroupId":oldGroupId,"hotelId":oldSelectHotelId};
			    				needChangeDefaultHotelPackageId.push(dataMap);
							}
						}
					}
				}
			});
			if(!isTrue && newHotelSelectName != ""){
				defrentHotelData+= "<p>"+newHotelSelectName+"</p>";
			}
    	});
	   return defrentHotelData;
    }
    
    function getTicketDefrentGoods(oldLoadingGoods,newLoadingGoods){
    	var defrentHotelData = "";
    	newLoadingGoods.find(".lvmama-price-flag[data-type='ticket']").each(function(index,element1){
	    	var  newSelectTicket= $(element1);
	    	var isTrue = false; 
	    	var newHotelSelectName = newSelectTicket.attr("showgoodsname");
    		oldLoadingGoods.find(".lvmama-price-flag[data-type='ticket']").each(function(index,element){
		    	var oldSelectTicket = $(element);
		    	if(newSelectTicket.attr("data-groupid") == oldSelectTicket.attr("data-groupid")){
			    	if(oldSelectTicket.attr("data-suppgoodsid") && newSelectTicket.attr("data-suppgoodsid")){
			    		if(oldSelectTicket.attr("data-suppgoodsid") == newSelectTicket.attr("data-suppgoodsid")){
			    			isTrue = true;
			    		}
			    	}
		    	}
    		});
	    	if(!isTrue && newHotelSelectName != ""){
				defrentHotelData+= "<p>"+newHotelSelectName+"</p>";
	    	}
    	});
    	
	   return defrentHotelData;
    }
    
    var needChangeDefaultHotel = false;
    var needChangeDefaultHotelId = [];
    function getHotelDefrentGoods(oldLoadingGoods,newLoadingGoods){
    	var defrentHotelData = "";
    	oldLoadingGoods.find(".active_xuan[data-type='hotel']").each(function(index,element1){
			var newHotelSelectName = "";
			var isTrue = false; 
			var oldSelectHotelId = $(element1).attr("data-suppgoodsid");
			var oldGroupId=$(element1).attr("data-groupid");
			newLoadingGoods.find(".house-type-select[data-type='hotel']").each(function(index,element){
				var thishotelId = $(element).attr("data-suppgoodsid");
				if(oldGroupId == $(element).attr("data-groupid")){
					if($(element).hasClass("active_xuan")){
						if(thishotelId == oldSelectHotelId){
			    			isTrue = true;
						}
						newHotelSelectName = $(element).attr("showgoodsname");
					}else{
						if(thishotelId == oldSelectHotelId){
							if(oldLoadingGoods.find("#hotelDefaultBySystem[data-groupid='"+oldGroupId+"']").val() != oldSelectHotelId){
			    				isTrue = true;
			    				needChangeDefaultHotel  = true;
			    				var dataMap={"hotelGroupId":oldGroupId,"hotelId":oldSelectHotelId};
			    				needChangeDefaultHotelId.push(dataMap);
							}
						}
					}
				}
			});
			if(!isTrue && newHotelSelectName != ""){
				defrentHotelData+= "<p>"+newHotelSelectName+"</p>";
			}
    	});
    	return defrentHotelData;
    }
    
    
    var isCanNotSell = false;
    function showGoodsInfo(routeId,data,quantity,adultQuantity,childQuantity,selectDate){
          thisSelectDate = selectDate;
      	  thisQuantity=quantity;
		  thisadultQuantity=adultQuantity;
		  thischildQuantity=childQuantity;
    	  $("#preorder-adjust").html(data);
    	  $("#loadingGoodsContext").html("");
	      $("#preorder-adjust").show();
	      $("#preorder-adjust-loading").hide();
    	  $("#preorder-adjust").html(data);
    	  var $preorderConfirmButton = $("#preorder-confirm-button");
	      if($("<div>"+data+"</div>").find(".do-not-modify-me").size() == 0){
	      	   if(routeId){
	           		var feeIncludeHtml = $("#line_route_"+routeId).html();
	           		$("#preorder-adjust").append(feeIncludeHtml);
	           }
	      }
	      // 商品不可售
	      if(data.indexOf("do-not-modify-me") > -1 || 'N' == "Y"){
	          $preorderConfirmButton.html("开始预订");
	          $preorderConfirmButton.removeAttr("data-lock");
	          isCanNotSell = true;
	          return;
	          // 商品可售
	      }
	      // 按钮恢复正常
	      $preorderConfirmButton.removeAttr("data-disable");
	      $("#preorder-confirm-button").removeClass("btn_stop");
	      $preorderConfirmButton.attr("data-lock", true);
	      $preorderConfirmButton.html("立即预订");
	      //不再使用提示
	      //$('.js_btnTips').remove(); //删除提示
	      //第一次加载算一次总价
	      $(".lvmama-price-flag").each(function(e) {
	         dealtotalMoney(this);
	      });
	      pandora.selectModel({'autoWidth': false, 'selectElement': $('.buy_number')});
	      getAllBuyInfo();
    	  if(needChangeDefaultHotel || needChangeDefaultHotelPackage){
    	  	for(var i = 0;i<needChangeDefaultHotelId.length;i++){
	    	  	$("#preorder-adjust").find(".house-type-select[data-groupid='"+needChangeDefaultHotelId[i].hotelGroupId+"']").each(function(e) {
			         if($(this).attr("data-suppgoodsid") == needChangeDefaultHotelId[i].hotelId){
			    	  	$(this).click();
			    	  	var js_btn_other = $(this).parent().parent().parent().next(".js_btn_other");
			    	  	if(js_btn_other){
			    	  		$(js_btn_other).click();
			    	  	}
			         }
			     });
    	  	}
    	  	needChangeDefaultHotel = false;
		  	needChangeDefaultHotelId = [];
		  	
    	  	for(var i = 0;i<needChangeDefaultHotelPackageId.length;i++){
	    	  	$("#preorder-adjust").find(".packagehouse-type-select[data-groupid='"+needChangeDefaultHotelPackageId[i].hotelGroupId+"']").each(function(e) {
			         if($(this).attr("data-suppgoodsid") == needChangeDefaultHotelPackageId[i].hotelId){
			    	  	$(this).click();
			    	  	var js_btn_other = $(this).parent().parent().parent().next(".js_btn_other");
			    	  	if(js_btn_other){
			    	  		$(js_btn_other).click();
			    	  	}
			         }
			     });
    	  	}
    	  	needChangeDefaultHotelPackage = false;
		  	needChangeDefaultHotelPackageId = [];
    	  }
    }
    
    var thisQuantity=0;
    var thisadultQuantity=0;
    var thischildQuantity=0;
    var thisSelectDate=-1;
    var clickClose = 1;
    var havDialog = null;
