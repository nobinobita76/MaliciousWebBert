        productId: 608009,
        notSell:'N',
        orderE: null, 		//缓存下单点击事件
	    javaCallback: {
			init: function(){
				this.bindEvent();
			},
	    	loadingSuppGoodsCallBack: function($date,$routeId){
				var self = this;
	    		var selectDate = null;
	    		var routeId = $routeId;
	    		var startDate = $("#preorder-start-time").val();
	            if($date === undefined || $date == null || $date == ""){
					 if(startDate != null && startDate != ""){
                        selectDate = startDate;
                        
                        if(!routeId){// 行程
	                        var el = $(".caltable td[date-map='" + selectDate + "']");
	                    	routeId = el.find(".caltips").attr("routeId");
		                    if(!routeId) {// 单行程，取默认
		                    	routeId = $("#defaultLineRouteId").val();
		                    }
	                    }
                     }else{
                        // 啥都没有，返回
                        return;
                     }
	            }else{
	            	selectDate = $date;
	            }
	            var adultQuantity = $("#adult-count").val();
	            var childQuantity = $("#children-count").val();
	            var quantity = $("#preorder-quantity").val();
	            
	            var ajaxData = {
	                 selectDate1:selectDate,
	                 adultQuantity:adultQuantity,
	                 childQuantity:childQuantity,
	                 quantity:quantity,
	                 productId:window.DestGoods.productId
	            };
	            
	            // 禁用掉预定按钮，及设置预订时间
	            var $preorderConfirmButton = $("#preorder-confirm-button");
	            $preorderConfirmButton.attr("data-disable",true);
                $preorderConfirmButton.attr("data-lock", true);
                $("#preorder-confirm-button").removeClass("btn_stop").addClass("btn_stop");
                $("#total-price-value").html("<dfn>¥</dfn>--");
                $("#preorder-start-time").val(selectDate);
               
	            // 显示loading界面
	             $("#preorder-adjust").hide();
	             $("#preorder-adjust-loading").show();
	            $("#preorder-adjust-loading").html('<div class="loading_box"><i class="icon_loading"></i><span>请稍候，正在为您查询...</span></div>');
	
	            var loadingGoodsUrl = "destbu/loadingGoods";
	            if('N' == "Y"){
	                 //loadingGoodsUrl = "/route/destbu/loadingGoods";
                }
			    $.ajax({
	                  url:loadingGoodsUrl,
	                  type:"get",
	                  contentType:'application/json;charset=utf-8', //设置请求头信息
	                  data: ajaxData,
	                  success: function (data) { 
	                  	
	                  	   showGoodsInfo(routeId,data,quantity,adultQuantity,childQuantity,selectDate);
	                  }
	              });
	    	
	    	},
	        //更换产品
	        replaceCallback: function(e) {
	            var groupid = $(e.currentTarget).data("groupid");
	            var currentproductbranchid = $(e.currentTarget).attr("data-currentproductbranchid");
	            var haveChangeButton = $(e.currentTarget).data("havechangebutton"); //是否有更换button的按钮
	            var adultquantity = $(e.currentTarget).data("adultquantity");
	            var childquantity = $(e.currentTarget).data("childquantity");
	            var quantity = $(e.currentTarget).data("quantity");
	            var selectdate = $(e.currentTarget).data("selectdate");
	            var changeType = $(e.currentTarget).data("changetype");
	            var startDistrictId = $(e.currentTarget).attr("data-startDistrictId");
	            if (!startDistrictId) {
	                startDistrictId = -1;
	            }
	
	            //这个产品ID是被打包的产品ID 解决升级问题的
	            var productId = $(e.currentTarget).data("productid");
	            //自主打包的产品ID 若为供应商打包就和上面的productId是一样的
	            var lvmamaProductId = window.DestGoods.productId;
	            var changeTitle = "";
	            if(changeType == "hotelPackage") {
	                changeTitle = "更换酒店套餐";
	            }else if (changeType == "hotel") {
	                changeTitle = "更换酒店";
	            }else if (changeType == "ticket") {
	                changeTitle = "更换景点门票";
	            }else {
	                changeTitle = "更换其他套餐";
	            }
	
	            var changeProductUrl = "destbu/changeProduct";
	
	            if ('N' == "Y") {
	                changeProductUrl = "/route/destbu/changeProduct";
	            }
	            winLoading(true,'正在努力加载中...');
	            $.ajax({
	                url: changeProductUrl,
	                type: "get",
	                contentType: 'application/json;charset=utf-8',
	                //设置请求头信息
	                data: {
   		                 lvmamaProductId: lvmamaProductId,
	                     selectDate: selectdate,
                         adultQuantity: adultquantity,
	                     childQuantity: childquantity,
	                     quantity: quantity,
	                     productId: productId,
	                     groupId: groupid,
	                     currentProductBranchId: currentproductbranchid,
	                     changeType: changeType,
	                     startDistrictId: startDistrictId,
	                     haveChangeButton: haveChangeButton
	                },
	                success: function(data) {
	               		  winLoading(false);
	                	  $(e.currentTarget).removeAttr("data-disable");
	                      $.dialog({
	                          title: changeTitle,
	                          width:1030,
							  height:570,
	                          content: data
	                      });
	                      if (changeType == "hotel") {
		                     $(".change_list").each(function(){
						  		 //重新计算差价
						         refreshHotelGapPrice($(this),$('.selected_change_hotel_type'));
						  	 });
					  	  }
	                },
	                error:function(){
	                	winLoading(false);
                        $(e.currentTarget).removeAttr("data-disable");
                    }
	           });
	        },
	        //替换选择的商品和相对变化的附件信息
            doSelectAction: function(e) {
			    var b = false;
			    var startDistrictId = $(e.currentTarget).attr("data-startDistrictId");
			    if (!startDistrictId) {
			        startDistrictId = -1;
			    }
			    var productId = $(e.currentTarget).data("productid");
			    //自主打包的产品ID 若为供应商打包就和上面的productId是一样的
			    var lvmamaProductId = window.DestGoods.productId;
			    var groupId = $(e.currentTarget).data("groupid");
			    var productBranchId = $(e.currentTarget).data("currentproductbranchid");//被选中的产品规格
			    var currentSuppGoodsId = $(e.currentTarget).data("suppgoodsid"); //选中的商品ID
			    var selectedProductBranchId = $(e.currentTarget).data("selectedcurrentproductbranchid"); //默认被选中的产品规格
			    var adultquantity = $(e.currentTarget).data("adultquantity");
			    var childquantity = $(e.currentTarget).data("childquantity");
			    var quantity = $(e.currentTarget).data("quantity");
			    var selectdate = $(e.currentTarget).data("selectdate");
			    var changeType = $(e.currentTarget).data("changetype");
			
			    var changeSuppGoodsUrl = "destbu/changeSuppGoods";
			    if ('N' == "Y") {
			        changeSuppGoodsUrl = "/route/destbu/changeSuppGoods";
			    }
			    winLoading(true,'正在努力更换中...');
			    $.ajax({
			        url: changeSuppGoodsUrl,
			        type: "get",
			        contentType: 'application/json;charset=utf-8',
			        //设置请求头信息
			        data: {
			            lvmamaProductId: lvmamaProductId,
			            productId: productId,
			            groupId: groupId,
			            productBranchId: productBranchId,
			            currentSuppGoodsId: currentSuppGoodsId,
			            selectDate: selectdate,
			            adultQuantity: adultquantity,
			            childQuantity: childquantity,
			            quantity: quantity,
			            changeType: changeType,
			            startDistrictId: startDistrictId
			        },
			        success: function(data) {
			        	winLoading(false);
			        	if($("<div>"+data+"</div>").find(".do-not-modify-me").size() == 0){
			        		$(".dialog-close").trigger("click");
				            $("#group-" + groupId + "_" + selectedProductBranchId).replaceWith(data);
				            //更换商品之后再算一次价格
				            $(".lvmama-price-flag").each(function(e) {
				                dealtotalMoney(this);
				            });
				            if(changeType == "hotel"){
					            pandora.selectModel({'autoWidth':false, 'selectElement': $('.buy_number')});
						  		//获取酒店默认商品DIV
						  	    var houseTypeItem = $("#group-" + groupId + "_" + productBranchId).find(".default-hotel");
						  		//重新计算差价
						        refreshHotelGapPrice($("#group-" + groupId + "_" + productBranchId),houseTypeItem);
					  		}
					  		pandora.selectModel({'autoWidth': false, 'selectElement': $('.buy_number')});
				  		}else{
				  			alert("系统异常，更换商品失败！");
                        	$(".dialog-close").trigger("click");
				  		}
			        },
			        error:function(){
			        	winLoading(false);
			        	alert("系统异常，更换商品失败！");
                        $(".dialog-close").trigger("click");
                    }
			    });
			},
	        //酒店图片详情
	        getHotelDetailCallback: function(e,productId) {
				var getHotelDetailUrl = "destbu/hotelDetail/" + productId;
				if ('N' == "Y") {
					getHotelDetailUrl = "/route/destbu/hotelDetail/" + productId;
				}
				winLoading(true,'正在努力加载中...');
				$.ajax({
					url: getHotelDetailUrl,
					success: function(data) {
						winLoading(false);
						pandora.dialog({
							title: "酒店信息",
							width:1000,
							height:570,
							content: data
						});
						$(e.currentTarget).removeAttr("data-disable");
					},
                    error:function(){
                        winLoading(false);
                        $(e.currentTarget).removeAttr("data-disable");
                    }
				});
	        },
	        //门票图片详情
	        getTicketDetailCallback: function(e,productId) {
				var getTicketDetailUrl = "destbu/scenicDetail/" + productId;
				if ('N' == "Y") {
					getTicketDetailUrl = "/route/destbu/scenicDetail/" + productId;
				}
				winLoading(true,'正在努力加载中...');
				$.ajax({
					url: getTicketDetailUrl,
					success: function(data) {
						winLoading(false);
						pandora.dialog({
							title: "门票信息",
							width:1000,
							height:570,
							content: data
						});
						$(e.currentTarget).removeAttr("data-disable");
					},
					error:function(){
                        winLoading(false);
                        $(e.currentTarget).removeAttr("data-disable");
					}
				});
			},
			bindEvent: function(){
				var self = this;
				var yudingBox = $(".yuding_box");
                yudingBox.on("click", ".show-hotel-detail-flag", function(event){
					var targetId = $(this).attr("productId");
					if(targetId) {
						self.getHotelDetailCallback(event, targetId);
					}
					return false;
				});
                yudingBox.on("click", ".show-ticket-detail-flag", function(event){
					var targetId = $(this).attr("productId");
					if(targetId) {
						self.getTicketDetailCallback(event, targetId);
					}
					return false;
				});
			},
            preorderStart: function (e) {
                e.stopPropagation();
                // alert("提交校验！");
                if ($('#preorder-confirm-button').hasClass('btn_stop')) {
                    return;
                }

                var dlgPre = '<div class="continue-order-dialog"><span class="nova-icon-outline-lg nova-icon-warning"></span>';
                var dlgPost = '</div>';
                var msg = "";
                $.each($("input[name=aperTicketTimeValidMsg]"), function (index, element) {
                    if ($(element).val() != null && $(element).val().length > 0) {
                        msg += $(element).val();
                        return false; //跳出$.each，只需要一个提示
                    }
                });
                if (msg.length > 0) {
                    window.DestGoods.orderE = e; 	//缓存点击参数
                    nova.dialog({
                        title: '',
                        content: dlgPre + msg + dlgPost,
                        width: 500,
                        height: 219,
                        zIndex: 51,
                        wrapClass: 'continue-order',
                        okCallback: function () {
                            window.DestGoods.javaCallback.preorderContinue(window.DestGoods.orderE)
                        },
                        cancelCallback: true,
                        okText: "继续预订",
                        cancelText: "重新选择",
                        okClassName: "continue-btn-ok",
                        cancelClassName: "continue-btn-cancel"
                    });
                    return;
                } else {
                    window.DestGoods.javaCallback.preorderContinue(e);
				}
            },
            preorderContinue: function (e) {
				var $currentTarget = $(e.currentTarget);
				if ($currentTarget.attr("data-disable")) {
					return;
				}
				if ($currentTarget.attr("data-lock")) {
					window.DestGoods.javaCallback.submitData(e);
					return;
				}
			},
	        submitData: function(e) {
	        	var itemList = [];
                var breakFlag = "1";
                var maxAdultQuantity =0,maxChildQuantity = 0;
                $(".need-submit-data").each(function(){
                    var groupType = $(this).attr("data-group");
                    var item = {};
                    var hotelAdditation = {};//酒店的时候使用
                    var goodsId=$(this).attr("data-goodsId");
                    if(typeof(goodsId) != 'undefined'){
                        item.goodsId = goodsId;
                    }
                    var goodType=$(this).attr("data-goodType");
                    if(typeof(goodType) != 'undefined'){
                        item.goodType = goodType;
                    }
                    var mainItem=$(this).attr("data-mainItem");
                    if(typeof(mainItem) != 'undefined'){
                        item.mainItem = mainItem;
                    }
                    var routeRelation=$(this).attr("data-routeRelation");
                    if(typeof(routeRelation) != 'undefined'){
                        item.routeRelation = routeRelation;
                    }
                    var detailId=$(this).attr("data-detailId");
                    if(typeof(detailId) != 'undefined'){
                        item.detailId = detailId;
                    }
                    var quantity=$(this).attr("data-quantity");
                    if(typeof(quantity) != 'undefined'){
	                    if(groupType == "addition"){
	                        item.adultQuantity = quantity;
	                        item.quantity = quantity;
	                    }else{
	                        item.quantity = quantity;
	                    }
	                 }
	                 
	                 if(groupType  == "ticket"){
	                 	 //增加品类判断
		                 var childQuantity = $(this).attr("data-childQuantity");
		                 if(typeof(childQuantity) != 'undefined'){
	                        item.childQuantity = childQuantity*quantity;
	                        if(childQuantity*quantity > maxChildQuantity){
	                        	maxChildQuantity = childQuantity*quantity;
	                        }
	                     }
		                 
		                 var adultQuantity = $(this).attr("data-adultQuantity");
		                 if(typeof(adultQuantity) != 'undefined'){
	                        item.adultQuantity = adultQuantity*quantity;
	                        if(adultQuantity*quantity > maxAdultQuantity){
	                        	maxAdultQuantity = adultQuantity*quantity;
	                        }
	                    }
	                 }
	                 
                    var visitTime = $(this).attr("data-visitTime");
                    if (typeof(visitTime) != 'undefined') {
                        item.visitTime = visitTime;
                    }
                    if (groupType == "ticket") {
                        var markSelect_1 = detailId + "-" + goodsId;
                        $("select").each(function () {
                            var markSelect_2 = $(this).attr("data-detailid-suppgoodsid");
                            if (typeof(markSelect_2) != 'undefined' && markSelect_1 == markSelect_2) {
                                var selectOptionLength_new = $(this).find("option:selected").text();
                                if (selectOptionLength_new == '请选择') {
                                    $(this)[0].focus();
                                    $(this).css("background-color", "#ffcccc");
                                    breakFlag = 0;
                                    return false;
                                }
                            }
                        })
                    }
                    var displayTime = $(this).attr("data-displayTime");
                    if (typeof(displayTime) != 'undefined') {
                        item.displayTime = displayTime;
                    }
                    var detail = $(this).attr("data-detail");
                    if (typeof(detail) != 'undefined' && detail && (groupType == 'selectPackage' || groupType == 'hotelPackage')) {
                        item.hotelcombOptions = JSON.parse(detail);
                    }
                    if($(".disney").length>0){
                    	 item.isDisneyGood = true; 
                    }
                    var leaveTime = $(this).attr("data-leaveTime");
                    if (typeof(leaveTime) != 'undefined' && groupType == "hotel") {
                        hotelAdditation.leaveTime = leaveTime;
                    }
                    var arrivalTime = $(this).attr("data-arrivalTime");
                    if (typeof(arrivalTime) != 'undefined' && groupType == "hotel") {
                        hotelAdditation.arrivalTime = arrivalTime;
                    }
                    var stayDays = $(this).attr("data-stayDays");
                    if (typeof(stayDays) != 'undefined' && groupType == "hotel") {
                        hotelAdditation.stayDays = stayDays;
                    }

                    //如果是酒店才会有这个对象
                    if (groupType == "hotel") {
                        item.hotelAdditation = hotelAdditation;
                    }
                    //过滤掉份数为0的,而且所有下单的东西一定是会有份数的
                    if ("quantity" in item && item.quantity != ""  && item.quantity != "0") {
                        itemList.push(item);
                    }

              	});
              	if(breakFlag == 0){
                 	return false;
             	}
              	//输入的参数也要传过去
              	var adultQuantity = $("#adult-count").val();
             	var childQuantity = $("#children-count").val();
             	var quantity = $("#preorder-quantity").val();
              	var quantityHiddenFlag = $("#preorder-quantity").data("hidden");
           		// alert(adultQuantity+":"+childQuantity+":"+quantity);
           
              	var form = {};
              	form.itemList = itemList;
              	//如果没有酒店套餐的,按人售卖
              	if(quantityHiddenFlag == 'Y'){
              		if(adultQuantity > maxAdultQuantity){
              			maxAdultQuantity = adultQuantity;
              		}
              		if(childQuantity>maxChildQuantity){
              			maxChildQuantity = childQuantity;
              		}
                  	form.adultQuantity=adultQuantity;
                  	form.childQuantity=childQuantity;
                  	//如果有酒店套餐的，按份售卖
              	}else if(quantityHiddenFlag == 'N'){
              		if((adultQuantity*quantity) > maxAdultQuantity){
              			maxAdultQuantity = adultQuantity*quantity;;
              		}
              		if((childQuantity*quantity)>maxChildQuantity){
              			maxChildQuantity = childQuantity*quantity;;
              		}
                  	form.adultQuantity=adultQuantity*quantity;
                  	form.childQuantity=childQuantity*quantity;
              	}
              	form.maxAdultQuantity = maxAdultQuantity;
              	form.maxChildQuantity = maxChildQuantity;
              	
              	form.quantity=quantity;
              	form.productId=608009;
              	form.startDistrictId = 
              	form.visitTime = $("#preorder-start-time").val();
             	if($("#lineRouteId"))
              	{
                   form.lineRouteId=$("#lineRouteId").val();
                }
                // 游玩人数
                var travPersonQuantity = maxAdultQuantity + maxChildQuantity;
		        form.travPersonQuantity = travPersonQuantity;
                
                $("#orderJson").val(encodeURI(JSON.stringify(form)));
                //alert( $("#orderJson").val());
                //改为免登录
                 $("#submitDataForm").submit();
                /*$.getJSON(
                   "http://login.lvmama.com/nsso/ajax/checkLoginStatus.do?jsoncallback=?",
                   {},
                   function(data) {
                       if (data.success) {
                           $("#submitDataForm").submit();
                       } else {
                           showLogin(function(){
                               $("#submitDataForm").submit();
                           });
                           cmCreatePageviewTag("登陆浮动层-酒店套餐-国内","虚拟浏览");
                      }
                  }
               );*/
	       }
	   }
    };

	$(function(){
        DestGoods.javaCallback.init();
        getAllBuyInfo();
	});
	
	/*$(function(){
		var params = {productId: 608009,};
		$.getJSON('http://dujia.lvmama.com/package/data.json', params, function(response) {
			var flag = false;
			for(i=0;i<response.length;i++){
				if(typeof(response[i].price)!==undefined && response[i].price!==null && response[i].price!==''){
					flag = true;
					break;
				}
			}
			if(flag){
				// 按钮恢复正常,锁住立即预订按钮，不让其跳转
	    		//$preorderConfirmButton.removeAttr("data-disable");
	    		$("#preorder-confirm-button").removeClass("btn_stop");
	    		//$preorderConfirmButton.attr("data-lock", true);
	    		$preorderConfirmButton.html("立即预订");
			}
		});
	});*/

 function getAllBuyInfo(){
    var categoryId = 17;
    if(categoryId!=17){
      	var checkArray = new Array();
    var isCombStructName = '';
	//遍历酒店  和 酒店套餐
   $('.active_xuan').each(function(){
	 var obj1 = $(this);
	 var dataType = obj1.attr('data-type');
	 //如果是非结构化套餐
	 if('packageComb'==dataType){
		 var hotelComb = new Object(); 
		 hotelComb.typeName = '酒店套餐';
		 hotelComb.goodsName=obj1.attr('showgoodsname');
		 hotelComb.quantity=obj1.attr('data-quantity')+"份";
		 checkArray.push(hotelComb);
	 }	
	 
	 //如果是套餐结构化门票
	 if('comb_struct_ticket'==dataType && isCombStructName == ''){
	    isCombStructName = $('#currentgoodsname').text();
	     var comb_struct = new Object(); 
	     comb_struct.typeName = '酒店套餐';
	     comb_struct.goodsName=isCombStructName;
	     var comb_quantity = $("#preorder-quantity").val();
	     comb_struct.quantity=comb_quantity+"份";
	     checkArray.push(comb_struct);
	 }
	 //套餐结构化酒店
	 if('comb_struct_hotel'==dataType && isCombStructName == ''){
	    isCombStructName = $('#currentgoodsname').text();
	     var comb_struct = new Object(); 
	     comb_struct.typeName = '酒店套餐';
	     comb_struct.goodsName=isCombStructName;
	     var comb_quantity = $("#preorder-quantity").val();
	     comb_struct.quantity=comb_quantity+"份";
	     checkArray.push(comb_struct);
	 }	 	 
	 //如果是酒店
	 if('hotel'==dataType){
		 var hotel = new Object();
		 hotel.typeName = '房型';
		 hotel.goodsName=obj1.attr('showGoodsName');
		 var hotelQuantity = $('select[data-suppgoodsid='+obj1.attr('data-suppgoodsid')+']').find("option:selected").text()+"间";
		 hotel.quantity = hotelQuantity;
		 checkArray.push(hotel);
	 }
   });
	//遍历门票
    $('.buy_number.ticket-select').each(function(){
       	var ticket = $(this);
    	ticket.typeName="门票";
    	ticket.goodsName=ticket.attr('showGoodsName');
    	ticket.quantity=ticket.find("option:selected").text()+"张";
    	checkArray.push(ticket);
    });
    
    var htmlStr = "";
   //遍历数组，组装内容显示
   $.each(checkArray, function(index,obj){      
       var li_1 ="<li>";
	   var typeName = '<span class="ysi-type">'+obj.typeName+'：</span>';
	   var goodsName = '<span class="ysi-name" title='+obj.goodsName+'>'+obj.goodsName+'</span>';
	   var quantity = '<span class="ysi-amount">'+obj.quantity+'</span>';
	   var li_2 ="</li>";
	   htmlStr = htmlStr+li_1+typeName+goodsName+quantity+li_2;		   
   });
     //console.log(htmlStr); 

    if(htmlStr!=""){
       $('.yh-slc-items').html(htmlStr)
       $('.yh-slc-selected').show();
       $('.yh-slc-none').hide();
     }
    }
}
</script><!--产品详情模块--开始-->
