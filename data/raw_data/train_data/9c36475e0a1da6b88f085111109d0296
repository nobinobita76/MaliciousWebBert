    <script>
    function tjas(){
		var lc_name=$("input[name=lc_name]").val();
        var lc_email=$("input[name=lc_email]").val();
		var lc_tel=$("input[name=lc_tel]").val();
        if(!lc_name){
            layer.msg('请填写姓名');
            return false;
        }
        if(!lc_email){
            layer.msg('请填写邮箱');
            return false;
        }else{
            if(!/^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(.[a-zA-Z0-9_-])+/i.test(lc_email))
            {
              layer.msg('邮箱格式不正确');
              return false;
            }
        }
        if(!lc_tel){
            layer.msg('请填写正确的手机号');
            return false;
        }else{
            if(!/^(13[0-9]|14[0-9]|15[0-9]|18[0-9])\d{8}$/i.test(lc_tel))
            {
              layer.msg('手机格式不正确');
              return false;
            }
        }
		
		$.post("?a=user_reg",{lc_name:lc_name,lc_tel:lc_tel,lc_email:lc_email},function (data){
			    data=eval(data);
				if(data.state=='yes')
				{
					layer.confirm('报名成功', {
					  btn: ['确定'] //按钮
					}, function(){
					  window.location.reload();
					}, function(){
					});
					return false;
				}
				if(data.state=='no')
				{
					layer.confirm('报名成功', {
					  btn: ['确定'] //按钮
					}, function(){
					  window.location.reload();
					}, function(){
					});
					return false;
				}
				 
			},'json')
        
    }
</script> 
