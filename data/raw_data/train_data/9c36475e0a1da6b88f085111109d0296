  <script>
    function tjss(){
		var lc_title=$("textarea[name=lc_titles]").val();
        var lc_name=$("input[name=lc_names]").val();
        var lc_tel=$("input[name=lc_tels]").val();
		var lc_company=$("input[name=lc_company]").val();
		var lc_age=$("input[name=lc_age]").val();
		var lc_lanmu=$("input[name=lc_lanmu]").val();
        
        if(!lc_title){
            layer.msg('请填写问题！');
            return false;
        }
        if(!lc_name){
            layer.msg('请填写姓名');
            return false;
        }
        if(!lc_tel){
            layer.msg('请填写手机号码');
            return false;
        }else{
            if(!/^(13[0-9]|14[0-9]|15[0-9]|18[0-9])\d{8}$/i.test(lc_tel))
            {
              layer.msg('手机格式不正确');
              return false;
            }
        }

        if(!lc_company){
            layer.msg('请填写意向课程');
            return false;
        }
        
        if(!lc_age){
            layer.msg('请填写年龄');
            return false;
        }
		$.post("?a=gbook_save",{lc_name:lc_name,lc_tel:lc_tel,lc_title:lc_title,lc_company:lc_company,lc_age:lc_age,lc_lanmu:lc_lanmu},function (data){
			    data=eval(data);
				if(data.state=='yes')
				{
					layer.confirm('注册成功', {
					  btn: ['确定'] //按钮
					}, function(){
					  window.location.reload();
					}, function(){
					});
					return false;
				}
				if(data.state=='no')
				{
					layer.confirm('注册成功', {
					  btn: ['确定'] //按钮
					}, function(){
					  window.location.reload();
					}, function(){
					});
					return false;
				}
				 
			},'json')
        
    }
</script> 
  <script type="text/javascript">
$(function(){
	$('.foot-contact').click(function(){
		$('.index-fixed-bj').css('display','block');
	    });
	});
	
$(function(){
	$('.index-fixed-none').click(function(){
		$('.index-fixed-bj').css('display','none');
	    });
	});
</script> 
  <script>
function lodex()
{
	var w = document.body.clientWidth;  
	var h = document.documentElement.clientHeight;
    var hs =(h-495)/2;
	var ws =(w-935)/2;
	
	$(document).ready(function(){
	  $(document).find('.index-fixed-bj').css('top',hs);
	  $(document).find('.index-fixed-bj').css('left',ws);
	});
}

window.onload=lodex;
window.onresize=lodex;
</script> 
