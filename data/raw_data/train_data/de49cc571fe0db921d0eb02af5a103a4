      $(document).ready(function() {
           $(".mengclose").click(function() {
              $(".mengleft").hide();
                $(".mengright").hide();
         });
         $(".mengbut").click(function() {
              $(".mengleft").show();
         });
         $(".mengleftclose").click(function() {
              $(".mengleft").hide();
         });
      }); 
     
</script>
