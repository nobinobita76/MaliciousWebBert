        jQuery(function () {
            jQuery("#AddComments").dialog({
                autoOpen: false,
                bgiframe: false,
                resizable: false,
                width: 580,
                height: 460,
                modal: true,
                open: function () {
                    jQuery("select").hide();
                },
                close: function () {
                    jQuery("select").show();
                }
            });
            jQuery(".btnComemnts").click(function () {
                if (ZTravle.IsLogined()) {
                    var id = jQuery(this).attr("id");
                    var title = jQuery(this).attr("title");
                    var url = "/Comment/sight.aspx?id=" + id + "&title=" + title;
                    jQuery("#frm_comments").attr("src", url);
                    jQuery("#AddComments").dialog("open");
                } else {
                    ZTravle.OpenLogin(location.href);
                }
            });
            jQuery(".approve").live("click", function () {
                var id = jQuery(this).attr("id");
                var code = ZTravle.commentor.ApproveOpprove(id, 1);
                if (code) {
                    var s = jQuery(this).parent().find("b");
                    jQuery(s).text(parseInt(jQuery(s).text()) + 1);
                }
            });
            jQuery(".oppose").live("click", function () {
                var id = jQuery(this).attr("id");
                var code = ZTravle.commentor.ApproveOpprove(id, 2);
            });
            jQuery('.img6 > a').lightBox();
        });
</script> 
