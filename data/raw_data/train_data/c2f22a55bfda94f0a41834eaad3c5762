    <script>
        //localStorage.clear();


        //设置时间匹对，在关闭未满12小时不开放
        var  timeCheck = function(){
            var nd = Date.parse(new Date());
            if(localStorage.glmj){
                var o = parseInt((nd-localStorage.glmj)/1000/60/60);
                if(o >= 12){
                    return true;
                }else{
                    return false;
                }
            }else{
                return true;
            }
        }
        if(timeCheck()){
            $(".glmj").show();
            $("#glmj_ad_top").show();
            $(".glmj_ad_close").bind("click",function(){
                var nd = Date.parse(new Date());
                if(localStorage.glmj){
                    var o = parseInt((nd-localStorage.glmj)/1000/60/60);
                    if(o >=12){
                        localStorage.glmj = nd;
                    }
                }else{
                    localStorage.glmj = nd;
                }
                $(".glmj").fadeOut(300);
            })
        }
    </script>

			
			
			
			
			
