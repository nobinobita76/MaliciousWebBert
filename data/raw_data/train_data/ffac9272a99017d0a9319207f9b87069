function CheckForm2(){
    var flag=true,msg='请正确填写以下信息：\n',sp=Array(30).join(" ");
    if($('#reg_cate2').val()>0);else{ msg+='\n\t- 想要购买的车型'+sp;$('#reg_cate2').focus();flag= false;}
    if($('#province2').val()>0);else{ msg+='\n\t- 您的省'+sp; flag && $('#province2').focus();flag= false;}
    if($('#city2').val()>0);else{ msg+='\n\t- 您的城市'+sp; flag && $('#city2').focus();flag= false;}
    if($.trim($('#reg_name2').val()).length<2){ msg+='\n\t- 您的姓名'+sp; flag && $('#reg_name2').focus();flag= false;}
    if(!isMobile($.trim($('#reg_mb2').val()))){ msg+='\n\t- 您的手机号码(11位)'+sp; flag && $('#reg_mb2').focus();flag= false;}
    if(!flag){alert(msg);}else{return flag;}
}

</script>
