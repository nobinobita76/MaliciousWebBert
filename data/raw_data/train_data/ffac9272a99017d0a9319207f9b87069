      <script type="text/javascript">$(document).ready(function(){
	getBC("reg_brand2","reg_cate2",0,98);
	getCity("province2","city2",430002);
    $('#btnReg2').click(function(){ if(CheckForm2()===true){ $.post("/reg",$("form.reg2").serialize(),function(data){if(data.status){location.href="/ok/"+data.data.m+"/"+data.data.n+"/"+data.data.t+"/"+data.data.b+"/"+data.data.c+"/"+data.data.msg;}else{alert(data.info);}},'json');};});	

