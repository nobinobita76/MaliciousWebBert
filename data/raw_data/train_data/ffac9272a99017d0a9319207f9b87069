function CheckForm(){
    var flag=true,msg='请正确填写以下信息：\n',sp=Array(30).join(" ");
    if($('#reg_cate').val()>0);else{ msg+='\n\t- 想要购买的车型'+sp;$('#reg_cate').focus();flag=false;}
    if($('#province').val()>0);else{ msg+='\n\t- 您的省'+sp;flag && $('#province').focus();flag=false;}
    if($('#city').val()>0);else{ msg+='\n\t- 您的城市'+sp;flag && $('#city').focus();flag=false;}
    if($.trim($('#reg_name').val()).length<2){ msg+='\n\t- 您的姓名'+sp;flag && $('#reg_name').focus();flag= false;}
    if(!isMobile($.trim($('#reg_mb').val()))){ msg+='\n\t- 您的手机号码(11位)'+sp;flag && $('#reg_mb').focus();flag= false;}
    if(!flag){alert(msg);}else{return flag;}
}
