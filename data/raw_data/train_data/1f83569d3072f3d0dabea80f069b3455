<script type="text/javascript">
// 4s 经销商轮换logo
 	(function($) {
		
		var switch_logos = new Array(), curIndex = 0;
					switch_logos.push('http://img1.xcarimg.com/PicLib/logo/bl166_160s.png');
				if (switch_logos.length > 1) { // 只有一个就不在轮换
			function switch_logo() {
				
				if (switch_logos.length == curIndex) {
					curIndex = 0;
				}
				$('#b_img').attr('src', switch_logos[curIndex]);
				curIndex++;
			}
			
			switch_logo();
			window.setInterval(switch_logo, 1000);
		} else {
			$('#b_img').attr('src', switch_logos[0]);
		}
	})(jQuery)
	</script>
