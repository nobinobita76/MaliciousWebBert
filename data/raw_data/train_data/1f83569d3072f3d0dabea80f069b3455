<script>
    //测试使用JS
    var tabTimer=null;
	var tabTimeh=null;
	//选车导航条
    $(".Headnav .tit,.Hnav_view .Hnav_con").hover(function () {
		_this=this;
		if(tabTimeh){
         clearTimeout(tabTimeh);
         }
        tabTimer=setTimeout(function(){
        if ($(".Headnav .tit").index(_this) >= 0) {
            var i = $(".Headnav .tit").index(_this);
        } else {
            var i = $(".Hnav_view .Hnav_con").index(_this);
        }
        $(".Headnav .tit").removeClass("active").eq(i).addClass("active");
        $(".Hnav_view .Hnav_con").hide().eq(i).show();
        },200)
    }, function () {
        if(tabTimer){
         clearTimeout(tabTimer);
         }
		 tabTimeh=setTimeout(function(){
           $(".Headnav .tit").removeClass("active");
           $(".Hnav_view .Hnav_con").hide();
		 },200)
    });
    //SUV展开
    $(".Hsuv_tb").parent().css("position", "relative").hover(function () {
        $(this).addClass("active");
    }, function () {
        $(this).removeClass("active");
    });
	//顶部下拉
    $(".menubox").hover(function () {
        $(this).addClass("hover").find(".menutb").addClass("active");	
		if($(this).hasClass("muser")){
		   $(this).find('.muser_show').width($(this).width())	
		}
    }, function () {
        $(this).removeClass("hover").find(".menutb").removeClass("active");
    });
	
	
	

    function setCookierl(name,value,expires,path,domain,secure) {
    	var exdate=new Date()
    	exdate.setDate(exdate.getDate()+expires)
    	var cookieText = encodeURIComponent(name) + "=" + encodeURIComponent(value);
    	cookieText += ";expires=" + exdate.toGMTString();
    	if(path){
    		cookieText += ";path=" + path;
    	}

    	if(domain){
    		cookieText += ";domain=" + domain;
    	}

    	if(secure){
    		cookieText += ";secure";
    	}

    	document.cookie = cookieText;

    }
    
    
    function getCookierl(name){
    	var cookieName  = encodeURIComponent(name) + "=",
    		cookieStart = document.cookie.indexOf(cookieName),
    		cookieValue = null;
    	if(cookieStart>-1){
    		var cookieEnd = document.cookie.indexOf(";",cookieStart);
    		if(cookieEnd == -1){
    			cookieEnd = document.cookie.length;
    		}
    		cookieValue = decodeURIComponent(document.cookie.substring(cookieStart + cookieName.length,cookieEnd));
    	}
    	return cookieValue;
    }
    
    
    $('.H_news').parent().mousemove(function(){
    	$('.H_news').css('display','none');
    	setCookierl('cms_H_news','OK',30,'/');
    })
    
    $(function(){
	if(!getCookierl('cms_H_news')){
		$('.H_news').css('display','block');
	}else{
		$('.H_news').css('display','none');
	}
});
    
    

  (function($){
      var cid = getCookierl('place_crid');
      if (null == cid) return;
      var list=$('div.Hnav_recommended a');
      var reg=/(http:\/\/price\.xcar\.com\.cn.*)(city0)(.*)/g;
      var href,newhref;
      for (var i in list) {
          href=list[i].href;
          if (null != href && '' != href) {
              newhref = href.replace(reg, "$1city"+cid+"$3");
              list[i].href    = newhref;
          }
      }
  })($);
  //end
</script>
