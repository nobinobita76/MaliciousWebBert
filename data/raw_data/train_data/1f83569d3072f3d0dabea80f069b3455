    var price_totle = "11.38";

            
                    //询底价js
                    // 加载询低价信息
                    (function() {
                        function getCookie(name)//取cookies函数
                        {
                            var arr = document.cookie.match(new RegExp("(^| )" + name + "=([^;]*)(;|$)"));
                            if (arr != null)
                                return unescape(arr[2]);
                            return null;
                        }

                        function renderConsultBox(data) {

                            var oPro = $('#province_id');
                            init_province()
                            oPro.val(data.province_id);
                            change_province_dd();

                            if (data.city_id > 0) {
                                $('#city_id').val(data.city_id);
                            }
                            $('#sname').val(data.name);
                            $('#mobile').val(data.mobile);
                        }

                        // 填充询低价数据
                        var _LimitPlace = getCookie('newcar_dealer_apply');
                        if (!_LimitPlace) {
                            _LimitPlace = {
                                'province_id': 6 || 1,
                                'city_id': 76|| 475,
                                'mobile': '',
                                'name': ''
                            }
                        } else {
                            _LimitPlace = eval('(' + _LimitPlace + ');');
                        }

                        renderConsultBox(_LimitPlace);
                    })();
                    $("#but").click(function() {
                        var mname = $("#mname").val();
                        //var province_id = $("#province_id").val();
                        //var city_id = $("#city_id").val();
						var province_id  = 6;
						var city_id      = 76
                        var sname = $("#sname").val();
                        var ra = $('input[name="ra"]:checked').val();
                        var email = $("#email").val();
                        var mobile = $("#mobile").val();
                        var intro = $("#intro").val();
                        var uuid = $("#uuid").val();
                        var uv   = $("#uv").val();
						var refer   = $("#refer").val();
                        var patrn = /^[0-9]{11}$/;
                        if (intro === $('#intro').attr('default')) {
                            intro = '';
                        }
                        regex = new RegExp("^[\u4e00-\u9fa5_a-zA-Z]{1,4}$"); 
                    	if(sname == ' ' || typeof(sname) == undefined || sname == '' )
                    	{
                    		alert('姓名必须填写');
                    	}else{
                    		if(!regex.test(sname)) {
                			 alert("请输入姓名(1-4字)");
                			}
                    	}
                        if (province_id == '0') {
                            alert('省必须选择');
                        } else if (city_id == '0') {
                            alert('市必须选择');
                        } else if (!ra) {
                            alert('性别必须选择');
                        } else if (mobile == '') {
                            alert('手机号必须填');
                        }
                        else if (!patrn.exec(mobile))
                        //else if(!preg_match('/^1(3|5)\d{9}$/',mobile))
                        {
                            $("#mob").html("手机格式错误！");
                            $("#mob").show();
                            $("#mobile").focus();
                        }

                        else {
                            //var url  = "news_detail_cuxiao.php?do=province_id&mname="+mname+"&province_id="+province_id+"&city_id="+city_id+"&sname="+encodeURI(sname)+"&ra="+ra+"&email="+email+"&mobile="+mobile+"&intro="+encodeURI(intro)+"&did="+uuid;

                            $.getJSON("/xcar/dealer/news_detail_cuxiao.php?do=province_id&mname=" + mname + "&province_id=" + province_id + "&city_id=" + city_id + "&sname=" + encodeURI(sname) + "&ra=" + ra  + "&mobile=" + mobile + "&intro=" + encodeURI(intro) + "&did=" + uuid+"&is_cms=601"+ "&uv=" + uv+"&uv_firstv_refer=" + refer, function(myJSON) {
                                //alert(myJSON);return false;
                                if (myJSON == '2') {
                                    alert('信息提交成功！');
                                    $("#but").attr('disabled', true);
                                    return false;

                                } else {
                                    alert("不能重复提交！");
                                    $("#but").attr('disabled', true);
                                    return false;
                                }

                            })
                        }

                    })


</script>
