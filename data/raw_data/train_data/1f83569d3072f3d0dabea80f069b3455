<script language="javascript">
//预估购车总价
var price_totle = "11.38";  //  经销商报价	
var seat_num= "5"; // 座位数
var m_disl  = "1.2"; // 排量
var country = "1";// 国家

var BuyCarObj = new XcarBuyCar();
var t_price = BuyCarObj.Calculate(price_totle*10000,seat_num,m_disl,country);//price,seat,disl,country
$("#price_totle").text(split_price(String(t_price/10000)));
</script>

