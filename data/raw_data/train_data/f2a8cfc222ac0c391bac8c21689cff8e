<script type="text/javascript">
var uslogin=$("#userislogin").val();
var num1 = 0;
function getCoupon(t){
    if (uslogin != "n") {
            if (num1==0) {
             getCouponTimeout(num1,t);
             }else{alert(num1+"秒后重试");
             getCouponTimeout1(num1,t);
             }
    }else{
      window.location.href='userinfo.aspx';
    }
};
  
  function getCouponTimeout(count,t) {
    setTimeout(function () {
        count--;
        num1=count;
        if (count > 0) {
          getCouponTimeout(count,t);
        }else{
           jQuery.ajax({
                 cache:false,
                 type:"get",
                 url:"ajax.aspx?_action=GetCoupon&t="+t,
                 dataType:"text",
                 timeout:10*1000,
                 contentType: "application/json; charset=utf-8",
                  success:function(data){
                     alert(data);
                  }
            });

          getCouponTimeout2(10);
        }
    }, 1000);
};
function getCouponTimeout1(count,t){
    setTimeout(function () {
        count--;
        num1=count;
        if (count > 0) {
          getCouponTimeout1(count,t);
        }
      },1000);
};
  function getCouponTimeout2(count){
    setTimeout(function () {
        count--;
        num1=count;
        if (count > 0) {
          getCouponTimeout2(count);
        }
      },1000);
};
</script>
