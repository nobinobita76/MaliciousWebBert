  <script>




function menu_toogle(a,s){

$("#mcbox").fadeIn(0); 		
$("#"+a).siblings(0).fadeOut(0);
$("#"+a).fadeToggle(0);

if($("#main_menu").css('display')=='none'){
$("#main_menuid").attr("src","./template/strong_say/touch/images/s_plus.png");			
}else{
$("#main_menuid").attr("src","./template/strong_say/touch/images/s_minus.png");	
}
if($("#"+a).css('display')=='none' && $("#"+s).css('display')=='none'){warpmc();} 
}



function warpmc(){ $("#mcbox").fadeOut(0); $("#main_menu").fadeOut(0);$("#user_menu").fadeOut(0); 
if($("#main_menu").css('display')=='none'){
$("#main_menuid").attr("src","./template/strong_say/touch/images/s_plus.png");			
}else{
$("#main_menuid").attr("src","./template/strong_say/touch/images/s_minus.png");	
} 
}
