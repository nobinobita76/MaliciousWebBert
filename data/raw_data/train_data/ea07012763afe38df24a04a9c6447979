function e_postf(replyid){

if (document.getElementById("vf"+replyid).score.value == ""){
alert("请输入你的评分!");
document.getElementById("vf"+replyid).score.focus();
return false;
}
if (document.getElementById("vf"+replyid).comment.value.length < 3 || document.getElementById("vf"+replyid).comment.value.length > 500 ){
alert("评价内容长度不能小于3大于500!,目前长度:"+document.getElementById("vf"+replyid).comment.value.length);
document.getElementById("vf"+replyid).comment.focus();
return false;
}
var ajax=new AJAXRequest({
		method: "POST",
		charset: "gb2312"
	});


ajax.postf(
	        document.getElementById("vf"+replyid), //要发送的表单对象
	         //回调函数
	        function(obj) { document.getElementById("evalue"+replyid).innerHTML=obj.responseText;}
          );
}

function unitprice(replyid,flag)
{
	price=document.evaluezu.price.value
	
	if (isNaN(price)|(price==""))
   {
    alert ("您好，奖金格式不对");
    document.evaluezu.price.focus();
    return false;
  }
	
	if(price <= 0)
	{
		alert("您好，奖金格式不对.");
		document.getElementById("evalueOK"+replyid).innerHTML="";
	}else
	{
		if ((flag-price)<0 || price > flag)
		{
			alert("余额不足!");
			document.getElementById("evalueOK"+replyid).innerHTML="";}
	 }

}



function e_postfZ(replyid,flag)
{

	price=document.evaluezu.price.value
	
	if (isNaN(price)|(price==""))
   {
    alert ("您好，奖金格式不对");
    document.evaluezu.price.focus();
    return false;
  }
	
	
	if(price <= 0)
	{
		alert("奖金不能为负!");
		document.getElementById("evalueOK"+replyid).innerHTML="";
	}else
	{
		if ((flag-price)<0 || price > flag)
		{
			alert("余额不足!");
			document.getElementById("evalueOK"+replyid).innerHTML="";
			
		}else
		{
		
			var ajax=new AJAXRequest({
					method: "POST",
					charset: "gb2312"
				});
			
			ajax.postf(
				
				document.getElementById("vfz"+replyid), //要发送的表单对象
				//回调函数
				function(obj) { document.getElementById("evalueOK"+replyid).innerHTML="<font color=#e60000>稿件合格，奖金已支付</font>";}
				
			);
		}
	}
}

function chkno(replyid)
{if (document.getElementById("unitfalse"+replyid).comment.value.length < 3 || document.getElementById("unitfalse"+replyid).comment.value.length > 200 )
{alert("评价内容长度不能小于3大于200!,目前长度:"+document.getElementById("unitfalse"+replyid).comment.value.length);
document.getElementById("unitfalse"+replyid).comment.focus();
return false;}
}


function e_postfalse(replyid){
var ajax=new AJAXRequest({
		method: "POST",
		charset: "gb2312"
	});

ajax.postf(
	        document.getElementById("unitfalse"+replyid), //要发送的表单对象
	         //回调函数
	        function(obj) { document.getElementById("evaluefalse"+replyid).innerHTML=obj.responseText;}
          );
}


function evalueOK(replyid,imageid){
document.getElementById("evalue"+replyid).innerHTML = '<form id="vf'+replyid+'" method="post" action="ajaxevalue.asp?act=evalue">评分：<input type=text class=tx name="score" value="68" size=12 onKeyDown="onlyNum();">(1~100分之间)<br />评价：<TEXTAREA CLASS="tx" NAME="comment" ROWS="3"  style="width:99%;table-layout:fixed;word-wrap:break-wrod;overflow:hidden" ></TEXTAREA><input name="repid" type="hidden" id="repid" value="'+replyid+'" /><input type="hidden" name="imageid1" id="imageid1" value="'+imageid+'" /><input type="button" onclick="evalueC('+replyid+');" value="取消" /><input style="margin-left:15px" type="button" onclick="e_postf('+replyid+');" value="提交" /></form>';
}
function evalueZoo(replyid,imageid,flag,unit){
document.getElementById("evalueOK"+replyid).innerHTML = '<form name="evaluezu" id="vfz'+replyid+'" method="post" action="ajaxevalue.asp?act=evalueZ">支付奖金:<input type=text class=tx name="price" value="'+unit+'" maxlength="3" size=12 onkeyup="value=value.replace(/[^\\d\\.]/g,\'\')" onblur="unitprice('+replyid+','+flag+')"><br /><input name="repid" type="hidden" id="repid" value="'+replyid+'" /><input type="hidden" name="imageid1" id="imageid1" value="'+imageid+'" /><input type="button" onclick="evalueCZ('+replyid+');" value="取消" /><input style="margin-left:55px" type="button" onclick="e_postfZ('+replyid+','+flag+');buttonfalse('+replyid+')" value="提交" /></form>';
}

