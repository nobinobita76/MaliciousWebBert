function replacestr(str){
var str1
str1 = str.replace("fbpf:","发布者评分：");
str1 = str1.replace("fbpj:","发布者评价：");
replacestr = str1;
}


function remaining_sum(RowCount,unitchkS) 
{ var xx
  xx = RowCount-1-unitchkS ;
  if ( RowCount-1-unitchkS>0)
    {alert ("您好，稿件没有评完。全部评完才能结束任务。未评稿件："+xx+"个");
      return false;
    }
}

