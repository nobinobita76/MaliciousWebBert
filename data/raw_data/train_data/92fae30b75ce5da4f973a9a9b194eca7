  <script language="javascript">
	function notnull()
	{
		keyword=document.getElementById("keyword").value;
		if(keyword=="" || keyword=="请输入产品名称")
		{
			alert("请输入产品名称");
			return false;
		}
		return true;
	}
	function setnull()
	{
		document.getElementById("keyword").value="";
	}
</script> 
