function pro_bx(YKTabid_num,YKTabnum){

 for(var i=0;i<2;i++){document.getElementById("bxcot_"+YKTabid_num+i).style.display="none";}

 for(var i=0;i<2;i++){document.getElementById("bxtit_"+YKTabid_num+i).className="a_bx";}

 document.getElementById("bxtit_"+YKTabid_num+YKTabnum).className="a_sj";

 document.getElementById("bxcot_"+YKTabid_num+YKTabnum).style.display="block";

} 

</script>


