<script language="javascript"> 

function top_bar(YKTabid_num,YKTabnum){

 for(var i=0;i<8;i++){document.getElementById("topcot_"+YKTabid_num+i).style.display="none";}

 for(var i=0;i<8;i++){document.getElementById("toptit_"+YKTabid_num+i).className="a_top";}

 document.getElementById("toptit_"+YKTabid_num+YKTabnum).className="a_hov";

 document.getElementById("topcot_"+YKTabid_num+YKTabnum).style.display="block";

} 

</script>


<script>

function out_bar(YKTabid_num,YKTabnum){

	document.getElementById("topcot_"+YKTabid_num+YKTabnum).style.display = "none";

	document.getElementById("toptit_"+YKTabid_num+YKTabnum).className="a_top";

	}

</script>


