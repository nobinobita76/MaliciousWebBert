       $(".app-close").click(function(){
				$(".app-main").hide();
				$(this).hide();
			});
       //右边app的js
       $(document).ready(function(){
           var timer = null,point = $('.dimensional-new'),box = $('#dimensional-new');
           point && point.on({
               mouseenter:function(){
                   box.show();
                   timer && clearTimeout(timer);
               },
               mouseleave:function(){
                   timer = setTimeout(function(){
                       box.hide();
                   },500);
               }
           });
           box.on({
               mouseenter:function(){
                   timer && clearTimeout(timer);
               },  
               mouseleave:function(){
                   box.hide(); 
               }
           })
       });
       $(window).scroll(function(){
           var navi_scroll_top = document.body.scrollTop + document.documentElement.scrollTop;
           if (navi_scroll_top > 800) {
               $(".fixednav").show()
           } else {
               $(".fixednav").hide()
           }
       });


           //选项卡
           $(".o_nav a").each(function(){
        	   if($(this).hasClass('cur')){
        		   $('.'+$(this).attr('relative')).show();
        	   }
        	   $(this).on('click',function(){
        		   $(this).addClass('cur').siblings().removeClass('cur');
        		   $('.'+$(this).attr('relative')).show().siblings().hide();
        	   })
           })
	//pic
	
   });
          
</script>
<script type="text/javascript">
 $('.recommend-tag').tab();
 $('.project-list').goSlide();
</script>



