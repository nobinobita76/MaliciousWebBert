<script type="text/javascript">
	//网易热分享   add by wangjiajia 2015-05-12
    //获取当前页面的url
    var url			= encodeURIComponent(window.location.href);	
    //获取当前页面的标题
    var title		= encodeURIComponent(document.title);
    //获取文章区域的所有图片
    var imgs		= $(".acticle_txt img");

    if(imgs[0]){
       image	= imgs[0].src;
       param	= "&image="+encodeURIComponent(image);
    }else{
       param	= '';
    }

$(".wyire").click(function(){
	window.open("http://hot.163.com/share.html?title="+title+param+"&url="+url,'_blank');
});

$(".page_go").click(function(){

	documentstr = $('.docIdArr').val();
	documentArr = documentstr.split(',');
	num = $('.page_num').val();
	currentId = documentArr[num-1];
	if(!currentId){
		alert('请输入正确的跳转页码~');
		return false;
	}
	currenturl = window.location.pathname;
	currenthost = window.location.host;
	urlArr = currenturl.split('/');
	newUrl = 'http://'+currenthost +'/'+urlArr[1]+'/'+urlArr[2]+'/'+currentId+'.shtml';
	window.location.href = newUrl;
});
</script>

