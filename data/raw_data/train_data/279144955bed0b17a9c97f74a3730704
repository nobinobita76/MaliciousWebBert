jQuery.noConflict();
jQuery(function (){
var max= 10.9,min =0.1;
price =  Math.random()*(max-min)+min+"";
jQuery('.input_wrap input').val(price.substring(0,4));
jQuery('.ico_shang').click(function (){
jQuery('.Reward_pop,.reward_overlay').show();
});
jQuery('.return,.reward_overlay').click(function (){
jQuery('.Reward_pop,.reward_overlay').hide();
});
jQuery('em').click(function (){
var num = 0;
var setnum = setInterval(function (){
num ++;
var price =  Math.random()*(max-min)+min+"";
jQuery('.input_wrap input').val(price.substring(0,4));
if(num>10){
clearInterval(setnum);
}
},100);
})
jQuery('.btn_confirm').click(function (){
if(jQuery('.input_wrap input').val()==''){
jQuery('.tip_error').show();return false;
}
jQuery('[num]').val(jQuery('.input_wrap input').val());
jQuery('#reward_form').submit();
});

