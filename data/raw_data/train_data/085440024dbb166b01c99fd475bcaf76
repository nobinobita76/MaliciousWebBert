<script style="zoom: 0; ">
    $(function(){
        var domain='http://carth5.jumei.com/'
      function resize(){
        var size=Math.min(screen.width, document.documentElement.getBoundingClientRect().width) / 375 * 16;
        var fontSize= size>=32 ? 32 :size
        document.documentElement.style.fontSize = fontSize + 'px';
        //20px 0.625
        //18px 0.5625
        if(window.chrome){
          $(".footer_transform").css({'-webkit-transform':'scale(1)'})
          $(".footer_tip_span").css({'-webkit-transform':'scale(1)','position':'static','width':'100%','margin-left':'0'})
          if(fontSize*0.5625<12){
            $(".footer_tip_span").css({'-webkit-transform':'scale(' + ((fontSize*0.5625) / 12) + ')'
              ,'position':'absolute','width':12/(fontSize*0.5625)*100+'%','margin-left':-(12/(fontSize*0.5625)-1)/2*100+'%'})
          }
          if(fontSize*0.625<12){
            $(".footer_transform").css({'-webkit-transform':'scale(' + (fontSize*0.625 / 12) + ')'})
          }
        }
      }
      resize()
      window.onresize=function(){
        resize()
      }
      $(".footer_carth5").attr('href',domain+'cart/show');
      $.ajax({
        url: domain+'/api/cart/get_qty?_ajax_=1',
        dataType: "jsonp",
        success: function(res) {
          if(res.code*1==0){
          	  var quantity=(res.data.quantity).toString()
              if(quantity.length==2){
                $('.footer_tip').addClass('footer_tip1')
              }else if(quantity.length>2){
                quantity="99+";
                $('.footer_tip').addClass('footer_tip2')
              }
              $('.footer_tip_span').html(quantity)
              if (quantity != 0) {
                $('.footer_tip').show()
              }
          }
        },
        error:function(xhr){
        }
      });
    })
  </script>
