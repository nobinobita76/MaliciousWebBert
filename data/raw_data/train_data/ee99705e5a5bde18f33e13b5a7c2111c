$(function(){
	//右侧多彩内容块滑动效果
	var rtimer=[];
	$(".block_win8_list li").mouseenter(function(){
		var _this=$(this);
		_this.find("span").animate({opacity:"0"},300);
		rtimer[1]=window.setTimeout(function(){
			_this.find("span").hide();
		},300);
		return false;
	});
	$(".block_win8_list li").mouseleave(function(){
		
		var _this=$(this);
		_this.find("span").show().animate({opacity:"1"},300);
		window.clearTimeout(rtimer[1]);
		return false;
	});
});
</script>
