	},function(){
		rTstimer[1]=window.setTimeout(function(){
			$(".rTsearch").find(".rTs_box").stop(true,true).animate({width:"0"},250);
		},250);
	});
	//搜索按钮触发
	$("#gotoSearch").bind("click",function(){
