<script>

  $('#server_right').hide();
  $('.stepImg').hide();
  $('.stepImg:eq(0)').show();

  var earn_cash = 0;
  var is_one = 0;
  var is_new = 0;
  var first_show_num = 3;
  var showeq = 1;
  var ex = 0;
  if(ex){
    cpa_change(2);
  }else{
    cpa_change(0);
  }
  var flag = (earn_cash>=30000 || is_one);

  if(!flag){
    $('.mast02-game').hide();
    $('.mast01').show();
  }else{
    $('.mast02-game').show();
    $('.mast01').hide();
  }

  if(!is_new){
    console.log('not new');
    $('.mast01').hide();
    $('.mast02-game').show();
    $('#game2').hide();
    $('#2game').hide();
  }

  $(function(){
    $('#2game').click(function(){
      $('.mast01').hide();
      $('.mast02-game').show();
      $('#game2').show();
    });
    $('#game2').click(function(){
      $('.mast02-game').hide();
      $('.mast01').show();
    });

    //1.3.10.20.66
    $('.termB-nav a').click(function(){
      var cname_click = $(this).attr("class");
      var click_num = cname_click.substring(4,5); console.log(click_num);
      var min = $(this).data("min");
      var need = $(this).data("need");
      var cash = $(this).data("cash");//hongbao
      if(earn_cash >=min || (click_num == 2 && flag) ){
        console.log('enter');
        if(click_num!=1){
          $('.mast01').hide();
          $('.mast02-game').show();
          $('#game2').hide();
        }else{
          if( !$(".mast02-game").is(":hidden") && is_new ){
            $('#game2').show();
          }
        }
        console.log(min);
        var eq;
        $('.termB-nav a').each(function(i){
          if( $(this).attr("class").indexOf("select")>0 )  eq=i;
        });
        console.log(click_num);
        var index=eq+1;
        //点击当前
        if(index ==click_num) return;
        $('#need_earn').html(need/10000);
        $('#hongbao').html(cash);
        $('.termB-nav a:eq('+eq+')').removeClass();
        $('.termB-nav a:eq('+eq+')').addClass('nav0'+index);
        var ee = $('.'+cname_click);
        ee.removeClass();
        ee.addClass(cname_click+'-select');
      }else{
        alert('请先完成当前任务');
      }
    });






    //领取1 3 10 20 66红包
    $('#lingqu').click(function(){
      var cash_val = $('#hongbao').html();
      $.getJSON("",{cash_val: cash_val},function(ret){
        alert(ret.msg);
      });
    });

    //wx+竞猜10
    $('#rmb1').click(function(){
      $.getJSON("",{rmb1: 1},function(rst){
        alert(rst.msg);
      });
    });

    //新手赚钱
    var myindex=0;
    $(".tcArrowR").click(function(){
      if(myindex==6) return;
      $('.stepImg').hide();
      myindex++;
      $('.stepImg:eq('+ myindex +')').show();
    });
    $(".tcArrowL").click(function(){
      if(!myindex) return;
      $('.stepImg').hide();
      myindex--;
      $('.stepImg:eq('+ myindex +')').show();
    });

    //新手赚钱 必看
    $(".mustSee img").click(function(){
      $(".tc").show();
      $('#messageBoxBack').show();
    });
    //关闭
    $('.Close').click(function(){
      $('#messageBoxBack').hide();
      $(".tc").hide();
    });

    //完善信息鼠标覆盖
    $(".task01").hover(function(){
      $(".wsMess").show();
    },function(){
      $(".wsMess").hide();
    });
  });

/*  $('.refresh a').click(function(){
    cpa_change();
  });*/

  function cpa_change(how){
    $(".tjGame a").hide();
    if(how==1){
      if(showeq==first_show_num){
        showeq=1;
      }else{
        showeq++;// = parseInt(5*Math.random());
      }
      $.getJSON("",{adid: $('#cpa'+showeq).data('adid')},function(ret){
        console.log(ret.msg);
      });
    }else if(how==2){
      showeq=0;
    }else if(how==0){
      showeq=1;
    }
    console.log(showeq);
    $('#cpa'+showeq).show();
    var adid = $('#cpa'+showeq).data('adid');
    $("#gotogame").attr("href","http://www.youzhuan.com/html/"+adid);
    if(adid==3589){
      $("#gotogame").attr("href","http://www.youzhuan.com/wrw.php");
    }
  }
</script>



