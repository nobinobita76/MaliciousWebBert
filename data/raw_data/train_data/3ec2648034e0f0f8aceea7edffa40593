  <script>
      // 展开
      function zxkfShow() {

      	$('#divFloatToolsView').animate({
      				width : 'show',
      				opacity : 'show'
      			}, 'normal', function() {
      				$('#divFloatToolsView').show();
      				kf_setCookie('RightFloatShown', 0, '', '/', 'www.istudy.com.cn');
      			});
      	$('#aFloatTools_Show').attr('style', 'display:none');
      	$('#aFloatTools_Hide').attr('style', 'display:block');
      }

      // 收缩

      function zxkfHide() {

      	$('#divFloatToolsView').animate({
      				width : 'hide',
      				opacity : 'hide'
      			}, 'normal', function() {
      				$('#divFloatToolsView').hide();
      				kf_setCookie('RightFloatShown', 1, '', '/', 'www.istudy.com.cn');
      			});
      	$('#aFloatTools_Show').attr('style', 'display:block');
      	$('#aFloatTools_Hide').attr('style', 'display:none');
      }

      $(function(){
      	$("#floatTools").mouseenter(function(){
      	  zxkfShow();
      	});
      	$("#aFloatTools_Hide").click(function(){
      	  zxkfHide();
      	});
      	
      })

      </script> 
