  <script>
      $(document).ready(function(){
    		//展开特定选项卡
    		choosetab();
    	});
      //选项卡切换 
      function tabc(tag,key,num,uncheckcss,checkcss)
      {	
      	for(var i=1;i<=num;i++)
      	{
      		if(i==tag)
      		{
      			$("#"+key+i).attr("class",checkcss);
      			$("#d"+key+i).show();
      		}
      		else
      		{
      			$("#"+key+i).attr("class",uncheckcss);
      			$("#d"+key+i).hide();
      		}
      	}
      }
      </script> 
