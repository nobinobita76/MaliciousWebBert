$(function () {
	var navLi=$(".menu .nav li");
	navLi.mouseover(function () {
		$(this).find("li").addClass("current");
		$(this).find(".box").stop();
		$(this).find(".box").slideDown();
	})
	navLi.mouseleave(function(){
		$(this).find("li").removeClass("current");
		$(this).find(".box").stop();
		$(this).find(".box").slideUp();
	})
})
</script> 
