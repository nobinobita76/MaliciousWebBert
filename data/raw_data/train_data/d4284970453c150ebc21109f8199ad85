    (function(win){
        var search = location.search.toLowerCase();
        win.ENVIRONMENT = /environment=(\w*)/.exec(search);
        win.App={};
        win.App.setDeviceInfo = function(param){
            if(!param) return;
            if(typeof param == "string"){
                win.MAIN_ACCOUNT_DEVICE_INFO = param;
            }else{
                win.MAIN_ACCOUNT_DEVICE_INFO = param.deviceInfo;
            }
        };
    })(window)
</script><script type="text/javascript" charset="utf-8" async="" data-requirecontext="_" data-requiremodule="main" src="./app/main.js"></script></head>

