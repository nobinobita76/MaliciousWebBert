    $(function () {
        $.post("/ask/Ajax/GetInfo", function (result) {
            if (result.nickName.length > 0) {
                //$("#loginContainer").html("【<a href='/ask/user\'>" + result.nickName + "</a>】【<a href='//user.jianke.com/account/logout'>退出</a>】");

                $("#myMessageCount").html(result.messageCount);
                $("#myTotalIntegral").html(result.totalIntegral);
                var type = result.memberType;
                
                if (type == "6" || type == "2") {//inline  none
                    $("#Div1").css("display", "none");
                    $("#Div2").css("display", "block");
                    $("#loginContainer").html("【<a href='/ask/doctor/center/index\'>" + result.nickName + "</a>】【<a href='//user.jianke.com/account/logout'>退出</a>】");
                } else {
                    $("#loginContainer").html("【<a href='/ask/user\'>" + result.nickName + "</a>】【<a href='//user.jianke.com/account/logout'>退出</a>】");
                }
            }
            else {
                var loginUrl = "//user.jianke.com/Account/login?backurl=" + escape(document.URL);
                var registerUrl = "//user.jianke.com/Account/register?backurl=" + escape(document.URL);
