    if ($("#kw").attr("value") == "") {
        $("#icon_remove").hide();
    }
    function removeText() {
        $("#kw").attr("value", "");
        $("#icon_remove").hide();
    }
    function submitMobile() {
        var frm = document.getElementById('bdcs-search-form');
        frm.submit();
    }

    function backRefer() {
        var refer = document.referrer;
        if (refer && refer.indexOf('cse/search') == -1) {
            window.location.href = refer;
        } else {
            history.go(-1);
        }

    }

</script> 
