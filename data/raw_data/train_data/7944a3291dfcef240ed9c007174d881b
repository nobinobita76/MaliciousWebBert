  <script type="text/javascript" src="http://m.ithao123.cn/statics/wap/js/flipsnap.js"></script> 
  <script>

    var marginLeft = "";
    if (marginLeft == undefined || marginLeft == null || marginLeft == "") {
        marginLeft = "0";
    }
    $("#mobileChannel-ul").css("left", marginLeft + "px");
    var outWidth;
    if ($.browser.mozilla) {
        outWidth = screen.width;
    } else {
        outWidth = $("#mobileChannel").width();
    }
    var ul = $("#mobileChannel").find("ul"), ul_w = 0;
    ul.find("li").each(function (i, item) {
        ul_w = ul_w + $(item).width() + 20;
    });
    ul.width(ul_w + 10);
    var ulReal = ul_w - 10;
    if (marginLeft < 0) {
        document.getElementById("channel-left").style.display = "block";
    }
    if (ulReal > outWidth) {
        document.getElementById("channel-right").style.display = "block";
        var leftX = ulReal - outWidth + 5;
        Flipsnap('#mobileChannel-ul', {
            leftX: leftX,
            marginLeft: -marginLeft
        });
    }
    function setBackStyle(index) {
        var iden = "no-selected" + index;
        document.getElementById(iden).style.background = "#F0F0F0";
    }
    function clickChanel(url) {
        //var iden = "no-selected" + index;
        //var left = $("#mobileChannel-ul").offset().left;
        window.location.href = url;
    }

</script> 
