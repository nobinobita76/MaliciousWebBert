    <script type="text/javascript">
        (function () {
            var hm = document.createElement("script");
            hm.src = "https://hm.baidu.com/hm.js?af904f771e48a048be5a22f1a6c4a8b0";
            var s = document.getElementsByTagName("script")[0];
            s.parentNode.insertBefore(hm, s);
        })();
    </script>
    <script type="text/javascript">
        //返回上一页
        $("#header").on("touchend", ".public-headleft", function () {
            if (isMove) {
                history.go(-1);
            }
        });
        //返回顶部
        $(document).on("touchend", ".BackToTop", function (e) {
            if (isMove) {
                e.preventDefault();
                e.stopPropagation();
                $("html,body").animate({ scrollTop: 0 }, 300);
            }
        });
        //滚动到底部时加载
        $(window).scroll(function () {
            var scrollTop = $(window).scrollTop(); //滚动高度
            if (scrollTop > document.documentElement.clientHeight) { //到达底部时,加载新内容
                $("#BackToTop").show();
            } else {
                $("#BackToTop").hide();
            }
        });

        //触发百度统计代码
        function getBaiduClick() {
            $.ajax({
                type: "POST",
                url: "/Resume/GetAccountInfoByJobSeeker?t=" + Math.random(),
                dataType: "json",
                async:false,
                success: function (data) {
                    if (data.Succeded == true) {
                        if (data.ExtraData.CN_ResumeFinishedStatus >= 100 && ($.cookie("NoComplete") == "yes")) {
                            if ($.cookie("IsClickComplete") == "") {
                                $.cookie("IsClickComplete", true);
                                $.cookie("NoComplete", "no");
                                $.ajax({
                                    type: "POST",
                                    url: "/Resume/EditActiveIdByJobSeekerId?t=" + Math.random(),
                                    data: { "activeId": $.cookie("activeId") },
                                    async:false,
                                    success: function (e) {
                                        if (e.Succeded == true) {
                                            $.cookie("activeId", '', { 'path': '/', 'expires': -1 });
                                            $("#m_finish")[0].click();
                                        }
                                    }
                                });

                            } else {
                                $("#m_finish").unbind('click');
                            }
                        } else {
                            $("#m_finish").unbind('click');
                        }


                    }
                },
                error: function (data) {
                    alert(data.Message);
                }
            });
        }

    </script>
  


