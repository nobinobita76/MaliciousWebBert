     <script type="text/javascript">
var ssid = 1490645323;
function updateImg(){
  ssid = new Date().getTime();
  $('#yz_img').attr("src",'/index.php?c=ajax&m=verify&ssid='+ssid); 
}
function putly() {
  if ($("#comment_name").val()=='') {
    alert('姓名不能为空');
    return false;
  }
  if ($("#comment_mobile").val()=='') {
    alert('手机号码不能为空');
    return false;
  }
  if ($("#comment_verify").val()=='') {
    alert('验证码不能为空');
    return false;
  }
  if ($("#comment_content").val()=='') {
    alert('留言内容不能为空');
    return false;
  }
  $.post("/index.php?c=ajax&m=comment", {mobile:$("#comment_mobile").val(),name:$("#comment_name").val(),estateid:'6688',content:$("#comment_content").val(),verify:$("#comment_verify").val(),ssid:ssid},function(data){
    alert(data.c);
    if(data.s=='2'){
      window.location.reload();
    }
  },'json');
  return true;
}
function showMessage()
{
    var winW = $(window).width();
    var winH = $(window).height();
    var sroT = $(window).scrollTop();
    var sroL = $(window).scrollLeft();
    var objW = $("#centerWin").width();
    var objH = $("#centerWin").height();
    var left = (winW - objW) / 2 + sroL;
    var top = (winH - objH) / 2 + sroT;
    $("#centerWin").css("left", left).css("top", top).fadeIn(300);
    $("#group_name").val('');
    $("#group_mobile").val('');
    $("#centerWin").show();
}
function checkmyform(name,mobile,type) {
  if ($("#"+name).val()=='') {
    alert('姓名不能为空');
    return false;
  }
  if ($("#"+mobile).val()=='') {
    alert('手机号码不能为空');
    return false;
  }
  $.post("/index.php?c=ajax&m=group", {mobile:$("#"+mobile).val(),name:$("#"+name).val(),type:type,estateid:'6688'},function(data){
    alert(data.c);
    if(data.s=='2'){
      window.location.reload();
    }
  },'json');
  return true;
}
$.fn.smartFloat = function() {
  var position = function(element) {
    var top = element.position().top, pos = element.css("position");
    $(window).scroll(function() {
      var scrolls = $(this).scrollTop();
      if (scrolls > top) {
        if (window.XMLHttpRequest) {
          element.css({
            position: "fixed",
            top: 0
          }); 
        } else {
          element.css({
            top: scrolls
          }); 
        }
      }else {
        element.css({
          position: pos,
          top: top
        }); 
      }
    });
};
  return $(this).each(function() {
    position($(this));             
  });
};
$("#building-float").smartFloat();
function magic(n,menu,div,degree){
  for(i=1;i<=parseInt(degree);i++){
    $('#'+menu+i).removeClass("on");
    $('#'+div+i).hide();
  }
  $('#'+menu+n).attr("class",'on');
  $('#'+div+n).show();
}
</script> 
