   <script>
    function submitto() {
        var c = document.getElementsByName("Inscode");
        var res = false;
        for (var i = 0; i < c.length; i++) {
            if (c[i].checked) {
                res = true;
                break;
            }
        }
        if (res) {
            document.forms[0].submit();
        }
        else {
            alert("请选择通道！");
            return false;
        }
    }
</script> 
