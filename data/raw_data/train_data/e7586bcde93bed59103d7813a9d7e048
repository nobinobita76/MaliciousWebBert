if(userAgent_str.indexOf("Firefox")!=-1){
	Use_Firefox=1;
}
if (!Use_Firefox){
	od=document.o1;
	od.t2.value=t2 + "-" + t3 + "-" + t4;
}

function f10(obj){
	var t1 = new Date(), t2 = t1.getMinutes(), t3 = t1.getSeconds(), t4 = t1.getMilliseconds();
	od = document.o1;
	od.t1.value = t2 + "-" + t3 + "-" + t4;
