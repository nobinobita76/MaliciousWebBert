		f9();
	}
}

function f9(){
	if (mg == 0){return ;}
	mk = mi - 1;
	if (mk < 0){
		mk = 0;
	}
	
	if ((mx[mk] - event.clientX > 1 || event.clientX - mx[mk] > 1) || (my[mk] - event.clientY > 1 && event.clientY - my[mk] > 1)){
		mx[mi] = event.clientX;
		my[mi] = event.clientY;
		mi++;
	}
	if (mi > 30){
		mg = 0;
	}
}

//f3(网站主id, 广告主id, 广告id, 广告地址, 扣金额, 加金额)

function f3(my_id, userid, aid, website, rm, am){
  var t1 = new Date(), t3 = t1.getMinutes(), t4 = t1.getSeconds(), t5 =t1.getMilliseconds(), t;
  try
  {
      ref=parent.location.href;
	  if (ref == null)
	  {
		  ref=document.referrer;
	  }
  }
  catch(e){
      ref=document.referrer;
  }
  
  t = document.o1; 
  t.t3.value = t3 + "-" + t4 + "-" + t5;  
  t.wzzid.value = my_id;
  t.ggzid.value = userid;
  t.aid.value = aid; 
  t.s.value = screen.width + '-' + screen.height;  
  t.c.value = screen.colorDepth;
  t.website.value = website;
  t.ref_url.value = escape(ref); 
  t.xx.value = mx;  
  t.yy.value = my;
