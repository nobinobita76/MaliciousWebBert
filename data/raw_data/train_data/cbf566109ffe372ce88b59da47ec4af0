<script type="text/javascript">
/**
 * search
 */
jQuery(function(){
    var p_fast_search = $('.p_searinp')
    p_fast_search.focus(function(e) {
        p_fastval = $(this).val();
        $(this).css('color','#333');
        if (p_fastval=='输入关键词') {
            $(this).val("");
        }
    });
    p_fast_search.blur(function(e) {
        p_fastval = $(this).val();
        if (!p_fastval) {
            $(this).val('输入关键词');
            $(this).css('color','#bbbbbb');
        }
    });
});

function searchSub()
{
    var v = $.trim($(".p_searinp").val());
    if(v == '' || v == '输入关键词')
        alert('谢谢您的支持，请填写搜索内容！');
    else
       $("#search").submit();
}

function goAskPost(){
    window.open('http://www.120ask.com/ask_qus.php?title='+$.trim($('.p_searinp').val()).replace('输入关键词', '')+'&fromurl=tag_jingyan');
}

function goHisTask(){
    window.open('http://tag.120ask.com/jingyan/task/0/');
}

