            else if (url.search("yanxiu") != -1) { selectNva("_nav_yanxiu"); $("[id=nav_yanxiu]").addClass("selected"); }
            else { selectNva("_nav_zaizhi"); $("[id=nav_zaizhi]").addClass("selected"); }
        }
        else if (url.search("mba") != -1) { selectNva("_nav_mba"); $("[id=nav_mba]").addClass("selected"); }
        else if (url.search("yanxiu") != -1) { selectNva("_nav_yanxiu"); $("[id=nav_yanxiu]").addClass("selected"); }

        else { selectNva("_nav_zaizhi"); $("[id=nav_zaizhi]").addClass("selected"); }

    });
</script> 
 </body>
</html>