function showhide(selector,type){
    if(type=='show')
        $(selector).show();
    else if(type=='hide')
        $(selector).hide();
}
function delete_comment(comment_id,user_id){
if(confirm('确定删除吗？') == true) {
        if ($.browser.msie && $.browser.version == 6) {
            setTimeout(function() {
                location.href =
                    'http://www.babytree.com/user/reply.php?type=c&rid=' + comment_id +
                    '&pointopuid=' + user_id +
                    '&action=remove&owner=u100624680&url=' + encodeURIComponent(window.location.href);
            }, 50);
        } else {
            location.href =
                'http://www.babytree.com/user/reply.php?type=c&rid=' + comment_id +
                '&pointopuid=' + user_id +
                '&action=remove&owner=u100624680&url=' + encodeURIComponent(window.location.href);
        }
    }
    return false;
}    
