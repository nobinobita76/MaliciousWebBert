$("a[class='like-button']").click(function() {
    var is_login_like = isLoginVal();
    if ( !is_login_like ) {
        return false;
    }
    if (is_liked) {
        return false;
    }
    var count_container = $('#like_count_container');
    var count_like = parseInt(count_container.html(),10);
    var id = $(this).attr('like');
    count_container.html("喜欢中");
    if (typeof from_source == 'undefined') {
        var source = '';
    } else {
        var source = from_source;
    }
    $.getJSON(
        'http://www.babytree.com/common/digg.php?source=' +source+ '&jsoncallback=?',
        {
            'id': id,
            'type': 'journal',
            'v':Math.random()
        },
        function(data) {
            if (data['count'] != 0) {
                var count = parseInt(count_container.html(),10);
                count_container.html((count_like + 1)+'人' );
                is_liked = true;
                $(".like-button").attr("title", "已喜欢");
                $(".like-button").attr("class","like-button-done");
                var $userIcon=$('<li><a href="'+data.user_data.home_url+'" target="_blank"><img title="'+data.user_data.nick_name+'" src="'+data.user_data.photo_url+'" /></a><span class="ico-heart"></span></li>');
                $("div.i-like-list ul").prepend($userIcon.fadeIn('slow'));
            	$("a[class='like-button']").unbind();

            }
        }
    );
    return false;
});

$('a[fav]').click(function() {
    var id = $(this).attr('fav');
    var container = $('span.global-utility-fav');
    container.html('收藏中');
    $.getJSON(
        'http://www.babytree.com/user/newfav.php?jsoncallback=?',
        {
            'action': 'add',
            'id': id,
            'type': 'journal',
            'title': $(this).attr('journal-title'),
            'tags': '日记'
        },
        function(data) {
            if (data['success']) {
                var container = $('span.global-utility-fav');
                container.html('已收藏');
				container.tooltip("show");
            }
        }
    );
    return false;
});
