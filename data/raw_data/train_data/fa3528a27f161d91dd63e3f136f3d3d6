<script type="text/javascript">
$(function (){
    var htmlad = "";
    $('#mytree-wrapper').append(
        '<div id="mytree-bottom-slide-nav" class="mytree-bottom-slide-nav" style="_position: absolute;">'+ htmlad +'<p class="feedback"><a href="#feedbackPopup" data-toggle="show.overlay" title="建议反馈"><i class="ico ico-feedback"></i>建议反馈</a></p>' +
        '<p class="back-to-top" id="mytree-bottom-slide-nav-top" style="display: none;"><a title="回到顶部" href="javascript:void(0)" onclick="scroll(0,0);return false;"><i class="ico ico-arrow-up"></i>回到顶部</a></p>' +
        '</div>'
    );
    var doscroll = (function(){
        var $doc = $(document),
            $win = $(window),
            $nav = $('#mytree-bottom-slide-nav'),
            $top = $('#mytree-bottom-slide-nav-top'),
            bottom = 50,
            height = $nav.height();

        if($.browser.msie && $.browser.version <= 7){
            $nav.css('top', $win.height()-80);
            return function(){
                var offset = $doc.scrollTop();
                var top = offset + $win.height() - height - bottom;
                if(offset < bottom+height){
                    $top.hide();
                    $nav.css('top', $win.height() - 80);
                }
                else{
                    $top.show();
                    $nav.css('top', top);
                }
            };
        }
        else{
            $nav.css('bottom', 80 - height);
            return function(){
                var offset = $doc.scrollTop();
                if(offset < bottom + height){
                    $top.hide();
                    $nav.css('bottom', Math.min(bottom, 80 - height + offset));
                }
                else{
                    $top.show();
                    $nav.css('bottom', bottom);
                }
            };
        }
    }());
    window.onscroll=doscroll;
    window.onresize=doscroll;
});
$(function () {
    $('input[placeholder], textarea[placeholder]').placeholder();
    });
</script>

