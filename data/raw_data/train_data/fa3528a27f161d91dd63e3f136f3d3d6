//初始化蒙层
function appTuiguangAd() {
    var flow = bui.getCookie('app_tuiguang_ad'),
        parts = flow && flow.split('|');
    if (!parts || parts.length !== 4 || (parts && parts[2] === '0')) {
        return;
    }
    parts[2] = '0';
    bui.setCookie('app_tuiguang_ad', parts.join('|'), null, '/', bui.webSite.getRootDomain());

    /*
    if (parts[3] === '2') {
        // 小时光
    }
    else if (parts[3] === '1') {
        // 快乐孕期
    }
    */

    $("a#pregnancyLayerImg").fancybox({
        'transitionIn'  :   'elastic',
        'transitionOut' :   'elastic',
        'speedIn'       :   0, 
        'speedOut'      :   0, 
        'overlayShow'   :  true
    }); 

    try{
        setTimeout(function(){
            $("a#pregnancyLayerImg").trigger('click.fb')
        },500)
    }catch(e){
        console.log(e.message);
    }
};
