                    <div class="actions">
                        <!-- fave and share -->
                        <div class="like" style="display: inline;">
                            <a target="_self" id="addfav" class="fave js-ilike ilike" rel-book-id="71551" href="javascript:void(0);"><em>收藏</em></a><!--em class="sep">|</em-->
