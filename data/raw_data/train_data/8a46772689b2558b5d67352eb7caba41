<script type="text/javascript">
      var PREM = "?from=404";
      $(function(){
        $("a").click(function(){
          var $this = $(this),
              uri   = $this.attr("href"),
              tar   = $this.attr("target");
          if(tar === "_blank"){
            window.open(uri+PREM);
          } else {
            location.href = uri+PREM;
          }
          return false;
        })
      })
</script>

