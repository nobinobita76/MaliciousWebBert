function noPoint(){
	 if($('#noPoint').is(':checked') == true){
		 $.cookie("noPoint", "y", {expires:365});
	 }
	 $("#Layer1").hide();
}

function MM_showHideLayers() { //v9.0
  var i,p,v,obj,args=MM_showHideLayers.arguments;
  for (i=0; i<(args.length-2); i+=3) 
  with (document) if (getElementById && ((obj=getElementById(args[i]))!=null)) { v=args[i+2];
    if (obj.style) { obj=obj.style; v=(v=='show')?'visible':(v=='hide')?'hidden':v; }
    obj.visibility=v; }}
	function resizeFrame() {
		try {
			var frame = document.getElementById("mainFrame");
			height = frame.contentWindow.document.body.scrollHeight;
			$("#mainFrame").height(height);
		} catch (e) {
		}
	
		if (($(window).height() < 685) || ($(document).height() > 689)) {
			$('html').css("overflow-y", "auto");
		} else {
			$('html').css("overflow-y", "hidden");
		}
	}
	
	
	function goSearch() {
		var keyWord = $("#keyword").val();
		if (keyWord == "") {
			Message.alert({
				title : '提示',
				message : "请输入关键词！",
				slideShowHide : true,
				showMask : true
			});
		} else {
			$("#Skeyword").val(encodeURIComponent(encodeURIComponent(keyWord)));
			$("#searchForm").submit();
		}
	}
	function handleUncompleteFun() {
		Message.alert({
			message : '此项功能正在建设中!',
			title : '山东省地方税务局-网上办税服务厅',
			maskAlphaColor : '#000',
			maskAlpha : 0
		});
	}
	var checkMainFrame = function() {
		var mainSrc = $('#mainFrame')[0].contentWindow.document.location.href;
		var width = 0;
		try {
			if (window.screen.width) {
				width = window.screen.width;
			} else {
				width = document.body.clientHeight;
			}
		} catch (e) {
	
		}
		/** 注释咨询的税企通
		if (mainSrc.indexOf("consulation") != -1) {
			var left = $("#index01").width()
					+ (getViewSizeWithScrollbar().width - 977) / 2 + 10;
			if (parseInt(left) + 99 > width) {
				left = left - 99;
			}
			$("#sqtLogo").css({
				position : "fixed",
				top : getViewSizeWithScrollbar().height / 2 - 50,
				left : left,
				zIndex : 10000
			});
			try {
				$("#sqtLogo").unbind("click");
			} catch (e) {
	
			}
			$("#sqtLogo").bind("click",
							function() {
								window.open('/etax/sqt/checkSqt.jsp',
												'newwindow',
												'height=260,width=410,top=0,left=0,toolbar=no,menubar=no,scrollbars=no, resizable=no,location=no, status=no');
							});
			$("#sqtLogo").show();
		} else {
			$("#sqtLogo").hide();
		}
		**/
	};
	var getViewSizeWithScrollbar = function() {
		if (window.innerWidth) {
			return {
				width : window.innerWidth,
				height : window.innerHeight
			};
		} else if (document.documentElement.offsetWidth == document.documentElement.clientWidth) {
			return {
				width : document.documentElement.offsetWidth,
				height : document.documentElement.offsetHeight
			};
		} else {
			return {
				width : parseInt(document.documentElement.clientWidth),
				height : parseInt(document.documentElement.clientHeight)
			};
		}
	};
	$().ready(function() {
		window.setInterval("resizeFrame()", 100);
		window.setInterval(function() {
			checkMainFrame();
		}, 1000);
