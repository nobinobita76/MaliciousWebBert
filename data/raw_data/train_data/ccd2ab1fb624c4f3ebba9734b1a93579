	function onkeypressSearch(){
		if(window.event.keyCode == '13'){
			goSearch();
		}
	}
	function oldUserW(){
		window.open('/etax/security/nsr/oldUser/goOldSysUser.do','_blank')
	}
	
	$(function(){
		
		$.ajax({ 
			type : "get",
			url : "dlqsytg.html", 
			dataType :"html",
			success : function(result){ 
				$("#Layer1").show();
				$("#scdsDlqTxymZw").html(result);;
				$("#scdsDlqTxymZw").css("top", 0);
				$("#scdsDlqTxymZw").css("left", 0);
				$("#scdsDlqTxymZw").css("position", "absolute");
				$("#scdsDlqTxymZw").show();
				var height = $("#scdsDlqTxymZw").find("div").height();
				var width = $("#scdsDlqTxymZw").find("div").width()
				if(height>700){
					$("#scdsDlqTxymZw").css("top", 0);
					$("#scdsDlqTxymZw").css("left", ($(window).width() - width) / 2);
					$("#bgall").css("height", height);
					
				}else{
					$("#scdsDlqTxymZw").css("top", ($(window).height() - height) / 2);
					$("#scdsDlqTxymZw").css("left", ($(window).width() - width) / 2);
				}
				
				$("#scdsDlqTxymZw").css("z-index", 999);
			},
			error : function(){
				 $("#Layer1").hide();
				 $("#scdsDlqTxym").hide();
			}
		});
		
		 
	});
	$(function ()  
			{  
			function test(divObj,aobj) {  
			var myDiv = $(divObj);  
			  
			$(aobj).mouseover(function (event)   
			{  
			$(divObj).fadeIn(200);;//调用显示DIV方法  
			$(document).one("mouseover", function ()   
			{//对document绑定一个影藏Div方法  
			$(divObj).hide(200);  
			});  
			   
			event.stopPropagation();//阻止事件向上冒泡  
			});  
			  
			$(divObj).mouseover(function (event)   
			{  
			event.stopPropagation();//阻止事件向上冒泡  
			});  
			}  
			  
			test("#tanni", "#dianwo");  
			});  
