    <div class="analyticbox-body">
    <% 
        var answer_html = null;
        if (q.question_type == '4') {
            answer_html = _.map(q.answer_json, function(v, k) {
                return "【第" + (k+1) + "空】" + v.toString();
            }).join("<br>");
        }
        else if (q.question_type == '6') {
            answer_html = _.map(q.answer_json, function(v, k) {
                return "<span>（" + (k+1) + "）、" + v.join('') + "</span>";
            }).join('');
        }
         else if(q.question_type == '3'){
            answer_html = q.answer == 'A' ? '正确':'错误';
        }
        else {
            answer_html = q.answer;
        }

    %>
    <%= answer_html %>
    </div>
</div>
<% } %>
