<script>
function user_news() {
    var remain = -1; // 剩余到期天数
    var $usernews = $('.user-news');
    var p = $usernews.parent();
    var h = p.height();
    p.css('position', 'relative');
    $usernews.css('top', h + 'px');

    var vip_overtip_close = OT2.Cookie.get('vip_overtip_date');
    if (remain == 0 && vip_overtip_close == 'end') {
        OT2.Cookie.set('vip_overtip_date', 'end', new Date(new Date().getTime() + 30 * 24 * 3600 * 1000)); // 更新cookie有效期
        return false; // 结束不显示
    }
    var d = new Date();
    var v = [d.getFullYear(), d.getMonth(), d.getDate()].join('/');
    if (!vip_overtip_close || vip_overtip_close != v) $usernews.show();
    $usernews.find('.user-news-close').on('click', function() {
        $usernews.hide();
        if (remain == 0) {
            OT2.Cookie.set('vip_overtip_date', 'end', new Date(new Date().getTime() + 30 * 24 * 3600 * 1000));
            return false;
        }
        OT2.Cookie.set('vip_overtip_date', v, new Date(new Date().getTime() + 24 * 3600 * 1000));
    });
}
