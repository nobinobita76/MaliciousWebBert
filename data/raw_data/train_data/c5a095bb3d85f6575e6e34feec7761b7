        map_size=3;
    }
    else if(dest_type=='PROVINCE'){
        map_size=7;
    }
    else if(dest_type=='CITY'){
        map_size=9;
    }else if(dest_type=='COUNTY'){
        map_size=10
    }else{
        map_size=11;
    }
            // 百度地图API功能
    var bmap = new BMap.Map("allmap");    // 创建Map实例
    bmap.centerAndZoom(new BMap.Point("120.86154200", "30.40341000"), map_size);  // 初始化地图,设置中心点坐标和地图级别
    bmap.addControl(new BMap.MapTypeControl());   //添加地图类型控件
    bmap.setCurrentCity("金良羊肉面店");          // 设置地图显示的城市 此项是必须设置的
    bmap.enableScrollWheelZoom(true);     //开启鼠标滚轮缩放
        function addUsefulCount(varUsefulCount, varCommentId) {
        $.ajax({
            type: "get",
            async: false,
            dataType: "jsonp",
            jsonp: "jsoncallback",
            jsonpCallback: "success_jsonpCallback",
            url: "http://www.lvmama.com/comment/ajax/addUsefulCountOfCmt.do",
            data: {
                commentId: varCommentId
            },
            success: function(jsonList, textStatus) {
                if (!jsonList.result) {
                    alert("已经点过一次");
                } else {
                    var newUsefulCount = parseFloat(varUsefulCount) + parseFloat(1);
                    $("#userfulCount_"+varCommentId).html("<i class=\"iconcom iconcom-enjoy\"></i>有用<em>" + newUsefulCount + "</em>");
                }
            }
        });
    }

</script>
<script>

    $('.com-answer-submit').live('click',function(){
        var _this = this;
        var res = $.ajax({
            type: "POST",
            url: siteurl+'ajax/ajaxAuth',
            dataType:'text',
            async: false,
            success: function(json){
                return true;
            }
        }).responseText;
        eval('var json = '+res+';');
        switch (json.code) {
            case 200:
                break;
            case 201:
                $(UI).ui("login");
                return false;
                break;
            default:
                alert(json.message);
                break;
        }
        var content = $(this).prev().val();
        var commentId = $(this).attr('com_id');
        if(content == ''){
            alert('评论内容不能为空!');
            return false;
        }
        $.ajax({
            dataType:"jsonp",
            type: "POST",
            jsonp: "jsoncallback",
            url: "http://www.lvmama.com/comment/ajax/addReplyForPhp.do",
            data:{
                userId:json.data.uid,
                content:content,
                commentId:commentId
            },
            success: function(data){
                if(data.success == true){
                    alert("您的回复已经发布成功，请等待审核！");
                    $(_this).prev().val('');
                }
                else{
                    alert("您的回复发布失败，请重新尝试!");
                }
            }
        });
    })

</script>
