<script>
    var siteurl = 'http://www.lvmama.com/lvyou/', dest_id = "10532108";
    function getPage(page){
        var page=page?page:1;
        $.ajax({
            dataType:"json",
            type: "POST",
            url: siteurl+'dest_food/AjaxGetRestraunt',
            data:{
                page:page,
                dest_id:dest_id,
                food_id:'687'
            },
            success: function(data){
                if(data.code==200){
                    $('#restaurant').html(data.data.html);
                    $('html,body').animate({scrollTop:$(".wy_state_model").offset().top},500);
                    getHasBeenComment(siteurl+"dest_index/ajaxGetCommentNum");
                }
            }
        });
    }
    function getFoodHaveRestNum(){
        var food_ids=new Array();
        $('.recom_food').each(function(){
            food_ids.push($(this).attr('data-id'));
        });
        $.ajax({
            dataType:"json",
            type: "POST",
            url: siteurl+'dest_food/ajaxFoodhaverest',
            data:{
                dest_id:dest_id,
                food_ids:food_ids
            },
            success: function(data){
                if(data.code==200){
                    var rec_rest_num=data.data.html;
                    for(food_id in rec_rest_num){
                        if(rec_rest_num[food_id]){
                            $("#recom_rest_"+food_id).html(rec_rest_num[food_id]+'家推荐餐厅');
                        }
                    }
                }
            }
        });
    }
    getFoodHaveRestNum();
</script>

