<script>
    /*页面链接losc添加*/
    var menpiaolosc = '?losc=049835&ict=i'; //losc代码；
    var jiudianlosc="?losc=0545621&ict=i";
    var menpiao= '.menpiaolosc';
    var jiudian=".jiudianlosc";
    var jinzhuilosc='?losc=054553&ict=i';
    var jinzhui='.jinzhuilosc';
    function bindlosc(loscclass,losccode){
        $(loscclass).each(function(){
            var href = $(this).attr('href');
            $(this).attr('href',href+losccode);
        });
    }
    bindlosc(menpiao,menpiaolosc);
    bindlosc(jiudian,jiudianlosc);
    bindlosc(jinzhui,jinzhuilosc);
</script>
