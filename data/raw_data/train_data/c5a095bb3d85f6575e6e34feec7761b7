        function getDestCount(){
        $.ajax({
            dataType:"json",
            type: "POST",
            url: "http://www.lvmama.com/lvyou/ajax/AjaxGetCount",
            data:{
                dest_type: "RESTAURANT",
                dest_id  : "10532108"
            },
            success: function(data){
                if(data.code==200){
                    $(".count_been").html(data.data.count_been?data.data.count_been:0);
                    $(".count_want").html(data.data.count_want?data.data.count_want:0);
                    $(".icon_Gone").attr('data-sum',data.data.count_been?data.data.count_been:0);
                    $(".icon_wantGo").attr('data-sum',data.data.count_want?data.data.count_want:0);
                }
            }

        });
    }
    getDestCount();
    function addCount(className){
        $(className).one('click',function(){
            $(this).parent().addClass('active')
            $(this).siblings('.addone').show().animate({
                bottom : 40,
                opacity: 0
            },'slow');
            var type=$(this).attr('_type');
            var sum = parseInt($(this).attr('data-sum'));
            $(this).next('p').find('span').html(sum+1);
            $.ajax({
                url: siteurl+'ajax/AjaxNewAddCount',
                type: 'POST',
                dataType: 'json',
                data: {type:type,dest_id:"10532108"},
                success: function(data){
                    getDestCount();
                }
            });
        });
    }
    addCount(".icon_wantGo");
    addCount(".icon_Gone");
