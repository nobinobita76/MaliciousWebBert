    $(function(){
        moretag('res_area');
        moretag('res_theme');
    })
    function moretag(tagclass){
        $('.'+tagclass).children('span').each(function(i){
            if($(this).attr('class')=='active'){
                if(i>5){
                    $('.'+tagclass).parent().parent().css('height','auto');
                }
            }
            if(i>11){
                $('.'+tagclass+'_more').show();
            }
        });
    }
</script></div>
