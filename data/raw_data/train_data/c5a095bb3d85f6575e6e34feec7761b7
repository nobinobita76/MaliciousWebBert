<script>
    $(function(){
        if (getLOSCCookie("orderFromChannel") == 'qqcb') {
            var arrStr = document.cookie.split("; ");
            var temp;
            var HeadShow = "";
            for(var i = 0,l=arrStr.length;i < l;i++){
                temp=arrStr[i].split("=");
                if(temp[0] == "HeadShow") {
                    HeadShow = decodeURIComponent(temp[1]).replace(new RegExp("\\+","gm"), " ");
                    break;
                }
            }
            var qqcbHtml='<div class="caibei-wrap" id="caibei-wrap"><div class="wrap"><div class="caibei-info"><i class="lv_icon icon_caibei"></i>' + HeadShow + '</div></div></div>';
            $("body").prepend(qqcbHtml);
        }
    });
	var search_result = [];
	var history_kw = '';
    function searchFnajax(){
		var kw = $('#search_php').val();
		if(!$.trim(kw) || history_kw == kw){//内容没有变就不要去请求
			return false;
		}
		if(typeof search_result[kw] != 'undefined'){//已经搜索过就不需要再去请求
			show_searchList(search_result[kw]);
			return false;
		}
		history_kw = kw;
        $.ajax({
            type: 'POST',
            url: 'http://www.lvmama.com/lvyou/ajax/ajaxGetHeaderSearch',
            data: {
                word:kw
            },
            dataType:'jsonp',
            jsonp: "callback",
            success: function(res){
                search_result[kw] = res;
				show_searchList(res);
            }
        })
    }
	var show_searchList = function(res){
		$('#lv_searchList').show();
		searchListIndex = -1;
		var htmlCode = '';
        for(var i=0; i<res.length; i++){
			if(res[i].dest_url != ''){
					htmlCode+='<a href="'+res[i].dest_url+'" target="_blank"><span>'+res[i].parent_name+'</span><i class="icon_type '+res[i].icon_type+'"></i>'+res[i].dest_name+'</a>';
			}
		};
		$('#lv_searchCon').html(htmlCode);
	};
