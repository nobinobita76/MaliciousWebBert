<!-- 加载点评代码 -->
<script type="text/javascript">
/**
 * 点评分页
 * @param page
 * @param list_type
 * @param type
 * @param dest_id
 */
function toPage_comment(page, list_type, type, dest_id){
    $.ajax({
        dataType:"json",
        type: "POST",
        url: "http://www.lvmama.com/lvyou/home/ajaxGetCommentPage",
        data:{
        page:page,
            list_type:list_type,
            type:type,
            dest_id:dest_id
        },
        success: function(data){
        if(data.data.type == 'Y'){
            $("#commentbest").html(data.data.html).find(".ufeed-content").each(function(){
                var h = $(this).height();
                if( h <= 70){
                    $(this).find("span").css('display','none');
                }
                $(this).removeClass('showmore');
            });
            $('html,body').animate({scrollTop:$("#c001").offset().top},100);
            }else if(data.data.type == 'N'){
            $("#commentnormal").html(data.data.html).find(".ufeed-content").each(function(){
                var h = $(this).height();
                if( h <= 70){
                    $(this).find("span").css('display','none');
                }
                $(this).removeClass('showmore');
            });
            $('html,body').animate({scrollTop:$("#c001").offset().top},100);
            }else if(data.data.type == 'P'){
            $("#commentpic").html(data.data.html).find(".ufeed-content").each(function(){
                var h = $(this).height();
                if( h <= 70){
                    $(this).find("span").css('display','none');
                }
                $(this).removeClass('showmore');
            });
            $('html,body').animate({scrollTop:$("#c001").offset().top},100);
            }
    }
    });
}

//取点评数据
$.ajax({
    type: 'POST',
    url: 'http://www.lvmama.com/lvyou/ajax/ajaxGetComDataNew',
    data: {
        d_id:"10532108"
    },
    dataType:'json',
    success: function(res){
        if(!res){
            $('#commend_3').show();
            return 0;
        }
        if(res.com_dimen[4])
            var dimen = (res.com_dimen[4]['formatAvgScore']=='')?'100':res.com_dimen[4]['formatAvgScore'];
        else var dimen = '100';
//            console.log(dimen);
        // 景点娱乐点 幻灯片右测 评分和数量
        $('.cityView_score').find('strong').html(dimen+'<em>%</em>');
        $('.cityView_score').find('a').html(res.comdata_pub.commentCount);
        $('.com-count').find('a').html(res.comdata_pub.commentCount);
//        $('#dscore_0').html(res.com_dimen[4]?res.com_dimen[4]['formatAvgScore']:'00.0');
        $('#dscore_0').html(res.com_dimen[4]?res.com_dimen[4]['formatAvgScore']:'100');

        var levelNum1 = res.com_dimen[3]?res.com_dimen[3]['avgScoreForTwoDP']:'5';
        $('#dscore_1').css('width', levelNum1*20+"%");
        $('#dscore_m_1').html(res.com_dimen[3]?res.com_dimen[3]['avgScoreForTwoDP']:'5.0');

        var levelNum2 = res.com_dimen[2]?res.com_dimen[2]['avgScoreForTwoDP']:'5';
        $('#dscore_2').css('width', levelNum2*20+"%");
        $('#dscore_m_2').html(res.com_dimen[2]?res.com_dimen[2]['avgScoreForTwoDP']:'5.0');

        var levelNum3 = res.com_dimen[1]?res.com_dimen[1]['avgScoreForTwoDP']:'5';
        $('#dscore_3').css('width', levelNum3*20+"%");
        $('#dscore_m_3').html(res.com_dimen[1]?res.com_dimen[1]['avgScoreForTwoDP']:'5.0');

        var levelNum4 = res.com_dimen[0]?res.com_dimen[0]['avgScoreForTwoDP']:'5';
        $('#dscore_4').css('width', levelNum4*20+"%");
        $('#dscore_m_4').html(res.com_dimen[0]?res.com_dimen[0]['avgScoreForTwoDP']:'5.0');

        // 景点娱乐点 幻灯片右测内容
                if(res.comdata_best){
            if(res.comdata_best.commentCount > 0){
                var countAll = (res.comdata_best.commentCount > 10)?10:res.comdata_best.commentCount;
                var dex = Math.floor(Math.random()*countAll+1)-1;
                $('.cityView_userCommit').html(res.comdata_best.data[dex]['userName']+'对景点的评价：').next().html(res.comdata_best.data[dex]['contentDelEnter'].substring(0,160)+'...'+'<a href="javascript:;" class="viewCommit" id="lookdianping">查看点评</a>');
                $("#poi_dianpin").attr('title',res.comdata_best.data[dex]['contentDelEnter']);
                $('#commend_1').show();
            }else{
                if(res.comdata_pub){
                    if(res.comdata_pub.commentCount > 0){
                        $('#commend_2').show();
                    }else{
                        $('#commend_3').show();
                    }
                }else{
                    $('#commend_3').show();
                }
            }
        }else{
            if(res.comdata_pub){
                if(res.comdata_pub.commentCount > 0){
                    $('#commend_2').show();
                }else{
                    $('#commend_3').show();
                }
            }else{
                $('#commend_3').show();
            }
        }
        
        // 点评具体数据
        // 全部
        if(res.html_pub){
            // tab 标签显示的条数
            $('#com_pub').find('span').html('('+res.count_pub+')');
            // 单条内容
            $('#commentnormal').html(res.html_pub).find(".ufeed-content").each(function(){
                var h = $(this).height();
                if( h <= 70){
                    $(this).find("span").css('display','none');
                }
                $(this).removeClass('showmore');
            });
        }else{
            $('#dptop').hide();
            $('#dptab').hide();
        }

        // 精华
        if(res.html_best){
            $('#com_best').find('span').html('('+res.count_best+')');
            $('#commentbest').html(res.html_best);
        }else{
            $('#com_best').hide();
        }
        // 有图
        if(res.html_pic){
            $('#com_pic').find('span').html('('+res.count_pic+')');
            $('#commentpic').html(res.html_pic);
        }else{
            $('#com_pic').hide();
        }
    }
});


$(function () {
    /**
     * 点评tab切换
     * @param tab_nav
     * @param tab_con
     * @param selected
     * @param tri_type
     * @constructor
     */
    function JS_tab_nav(tab_nav, tab_con, selected, tri_type) {
        $tab_obj = $(tab_nav);
        $tab_obj.bind(tri_type, function () {
            var tab_li_index = $(tab_nav).index(this);
            console.log(tab_li_index);
            console.log(tab_con);
            $(this).addClass(selected).siblings().removeClass(selected);
            $(tab_con).eq(tab_li_index).show().siblings(tab_con).hide();
            $(tab_con).eq(tab_li_index).find('.ufeed-content').each(function(){
                $(this).addClass('showmore');
                var h = $(this).height();
                if( h <= 70){
                    $(this).find('span').css('display','none');
                }
                $(this).removeClass('showmore');
            });
        });
    };
    JS_tab_nav('.JS_tabnav>li', '.JS_com-content .tabcon', 'active', 'click');
});
$("#lookdianping").live('click',function(){
    jump_to('comment_point');
});
</script>

