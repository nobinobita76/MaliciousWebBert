    <script type="text/javascript">
    var saleId = 246962436,
        lastPost = '',
        lastPostTime = 0,
        saleOpenTimeArr = {},
        saleEndTimeArr = {};

    //格式化数字
    function number_format(n) {
        var ns = n.toString();
        var mm = ns.match(/\d{1,3}(?=(\d{3})+$)/g);
        if (mm == null) return ns;
        mm.push(ns.replace(mm.join(""), ""));
        return mm.join(",");
    }

    //获取对应尺寸头像 flag = 0、46、64、96、132
    function headimgurl(headimgurl, flag) {
        return headimgurl.replace(/\w\/\/\d+$/, '/w/' + flag);
    }

    //拍卖开始倒计时
    function saleOpenTimeout(key) {
        var openTime = saleOpenTimeArr['time_' + key] - saleOpenTimeArr[key];
        saleOpenTimeArr[key] += 1;
        var hour = parseInt(openTime / 3600);
        var minute = parseInt((openTime - hour * 3600) / 60);
        var sec = openTime - hour * 3600 - minute * 60;

        var saleOpenBtn = $("." + key);
        var updateBid = saleOpenBtn.siblings('.updateBid');
        var bidBtn = saleOpenBtn.siblings('.bidBtn');
        if (openTime <= 0) {
            var saleId = key.replace('saleOpenTime_', '');
            $.getJSON('/wpt/checkSale?saleId=' + saleId, function(res){
                if(res.errcode == 0) {
                    clearInterval(saleOpenTimeArr["setInterval_" + key]);
                    saleOpenBtn.parent().siblings(".stateInfo").removeClass("hide");
                    saleOpenBtn.siblings('.bidBtn').removeClass("hide");
                    saleOpenBtn.siblings('.updateBid').removeClass("hide");
                    saleOpenBtn.remove();
                } else if(res.errcode == 101) {
                    saleOpenTimeArr[key] -= parseInt(res.remainingOpenTime);
                } else {
                    clearInterval(saleOpenTimeArr["setInterval_" + key]);
                    $('.bidBtns').find('.saleOpenBtn').css('backgroundColor', '#CCC').html(res.errmsg);
                }
            });
        } else {
            saleOpenBtn.html('距离开拍 <em>' + hour + '</em>时<em>' + minute + '</em>分<em>' + sec + '</em>秒');
        }
    }

    //拍卖倒计时
    function saleEndTimeout(key) {
        var endTime = saleEndTimeArr['time_' + key] - saleEndTimeArr[key];
        saleEndTimeArr[key] += 1;
        var hour = parseInt(endTime / 3600);
        var minute = parseInt((endTime - hour * 3600) / 60);
        var sec = endTime - hour * 3600 - minute * 60;
        if (endTime <= 0) {
            var saleId = key.replace('saleEndTime_', '');
            refreshBid(saleId, function(lastBid){
                if(lastBid) {
                    return;
                }
                clearInterval(saleEndTimeArr["setInterval_" + key]);
                var stateInfo = $("." + key).parent();

                var multiWins = 1;
                var multiWinsBtnObj = stateInfo.siblings('.bidBtns').find(".multiWinsBtn");
                if(multiWinsBtnObj.length > 0) {
                    multiWins = multiWinsBtnObj.attr("multiWins");
                }
                var _html = '<div class="saleEndBtn' + (multiWins > 1 ? " multiWins" : "") + '">拍卖已结束</div>';
                if(multiWins > 1) {
                    _html += '<div class="multiWinsBtn saleEnd" multiWins="' + multiWins + '">' + multiWins + '人中拍</div>';
                }
                stateInfo.siblings('.bidBtns').html(_html);
                stateInfo.remove();
            });
            return;
        }else if(endTime < 300){
            $("." + key).parent().find('.countdownTip').show();
        }else{
            $("." + key).parent().find('.countdownTip').hide();
        }
        $("." + key + " span").html('<em>' + hour + '</em>时<em>' + minute + '</em>分<em>' + sec + '</em>');
    }

    //出价后显示
    function prependBid(data, sid) {
        var bidTemplate = $("#bidTemplate").html();
        var bidList = $(".sale" + sid + " .bidList");
        var multiWinsNum = bidList.data('multi-wins');
        multiWinsNum = !multiWinsNum ? 1 : multiWinsNum;
        var bidLength = data.bidList.length;
        var localNeedChangeNum = multiWinsNum - bidLength;

        var bidLeader = $(".sale" + sid + " .bidLeader");
        bidLeader.removeClass("bidLeader").addClass("bidOut");

        if(localNeedChangeNum > 0) {
            bidList.find('.bidOut').each(function (i, n) {
                if (i < localNeedChangeNum) {
                    $(n).removeClass("bidOut").addClass("bidLeader");
                } else {
                    return false;
                }
            });
        }

        for (var index in data.bidList) {
            (function (index) {
                setTimeout(function () {
                    var bidArr = data.bidList[index];
                    var bidLeader = $(".sale" + sid + " .bidLeader");

                    var bidHtml = bidTemplate;
                    bidHtml = bidHtml.replace(/\{0\}/g, bidArr.createTime.replace(/^\d*(\d\d)-/, "$1-")).replace(/\{1\}/g, 'style="background-image: url(' + bidArr.headimgurl + ')"');
                    if(bidArr.userinfoUri){
                        bidHtml = bidHtml.replace(/\{4\}/g, bidArr.userinfoUri + "?autoRedirect=1");
                    }else{
                        bidHtml = bidHtml.replace(/\/user\/\{4\}/g, 'javascript:void(0);');
                    }
                    bidHtml = bidHtml.replace(/\{2\}/g, bidArr.nickname).replace(/\{3\}/g, number_format(bidArr.price));
                    bidHtml = bidHtml.replace(/\{5\}/g, 'lv' + bidArr.buyerLevel);
                    bidHtml = bidHtml.replace(/\{6\}/g, bidArr.id);

                    if(bidLength - index > multiWinsNum) {
                        bidHtml = bidHtml.replace("bidLeader", "bidOut");
                    }

                    bidList.prepend(bidHtml);
                    $(".sale" + sid + " .bidList .ddli:nth-child(1)").animate({height: "50px"}, 200);
                }, index * 400);
            })(index);
        }
    }

    //用户出价
    function bid(nowBid, lastBid) {
        if (!nowBid) {
            wptAlert("价格输入错误!");
            return;
        }
        if (nowBid < lastBid) {
            wptAlert("出价不能低于领先价!");
            return;
        }

        //防止短时间重复出价
        var post_url = "/wpt/bid?bid=" + nowBid + "&saleId=" + saleId + "&lastBid=" + lastBid;
        var post_time = Date.now();
        if (post_time - lastPostTime > 3500 * 1) {
            lastPost = '';
        }
        if (post_url == lastPost) {
            return;
        }
        lastPost = post_url;
        lastPostTime = post_time;

        $.getJSON(post_url, function (data) {
            var errcode = parseInt(data.errcode);
            if (errcode > 0) {
                if(errcode == 40000) {
                    $(document).off('wptConfirm_view:sure').one('wptConfirm_view:sure', function () {
                        wptRedirect('/pay/bzj_buyer?showwxpaytitle=1&uri=' + data.uri + '&lastBid=' + data.bid);
                    });
                    wptConfirm('本商家已设置出价保护，您本次出价需支付保证金。<br />如违约，保证金将扣除并赔偿给卖家。', {text: '支付保证金', class: "btn-green"});
                } else if(errcode == 4000) {
                    $(document).off('wptConfirm_view:sure').one('wptConfirm_view:sure', function () {
                        wptLoading('去支付保证金', function(){
                            wptRedirect('/pay/bzj_buyer?showwxpaytitle=1&uri=' + data.uri + '&lastBid=' + data.bid, 0);
                        }, 250);
                    });
                    wptConfirm('由于您违约过多，本次出价需支付保证金。<br />如违约，保证金将扣除并赔偿给卖家。', {text: '支付保证金', class: "btn-green"});
                } else if (errcode == 400) {
                    wptRedirect('/pay/bzj_buyer?showwxpaytitle=1&uri=' + data.uri + '&lastBid=' + data.bid);
                } else if (errcode == 114) {
                    $(document).off("verifyTelephone_view:success").one("verifyTelephone_view:success", function() {});
                    $(document).trigger('verifyTelephone_view:show', ['my', 'all', data.errmsg, "验证通过后才能继续出价"]);
                } else {
                    if (data['lastBid']) {
                        refreshBid(saleId);
                    }
                    wptAlert(data.errmsg);
                }
                return;
            }

            saleEndTimeArr['saleEndTime_' + saleId] = 1;
            if (data['lastTime']) {
                saleEndTimeArr['time_saleEndTime_' + saleId] = data.lastTime;
            }else{
                saleEndTimeArr['time_saleEndTime_' + saleId] = 0;
            }

            $(document.body).trigger("fixednum_view:hide");
            prependBid(data, saleId);

            if (hasTelephone == 1) {
                $(document).off("verifyTelephone_view:success").one("verifyTelephone_view:success", function(){});
                setTimeout(function(){
                    $(document).trigger('verifyTelephone_view:show', ['my', 'all', '出价成功，请完善你的联系电话，便于卖家联系', '验证号码后才能继续出价']);
                }, 800);
            }

            //采集数据，3：出价
            $(document).trigger("wptStatistic:sendData", ['{"data":{"u":"' + data.uri + '","uu":"","su":"' + data.su + '","uc":"' + data.uc + '","usc":"' + data.usc + '"},"route":"bid"}']);
            //采集数据，1：关注
            if (typeof data.isAttention != 'undefined' && data.isAttention == -1) {
                $(document).trigger("wptStatistic:sendData", ['{"data":{"u":"' + data.uri + '","uu":"","su":"' + data.su + '","uc":"' + data.uc + '","usc":"' + data.usc + '"},"route":"attention"}']);
            }
        });
    }

    //刷新最新出价
    function refreshBid(sid, cb) {
        cb = typeof cb == "undefined" ? function () {} : cb;
        var lastBid = $(".sale" + sid + " .bidList .ddli .price").attr("money");
        if (lastBid != undefined) {
            lastBid = parseInt(lastBid.replace(/,/g, ''));
        } else {
            lastBid = 0;
        }

        var nowTime = new Date();
        nowTime = nowTime.getHours() + ':' + (nowTime.getMinutes() < 10 ? 0 : '') + nowTime.getMinutes() + ':' + (nowTime.getSeconds() < 10 ? 0 : '') + nowTime.getSeconds();
        $(".sale" + sid + " .newbidTM").html(nowTime);

        var post_url = "/wpt/refreshBid?saleId=" + sid + "&lastBid=" + lastBid;
        $.getJSON(post_url, function (data) {
            if (parseInt(data.errcode) == 0) {
                //倒计时
                saleEndTimeArr['saleEndTime_' + sid] = 1;
                if (data.endTime) {
                    saleEndTimeArr['time_saleEndTime_' + sid] = data.endTime;
                }else{
                    saleEndTimeArr['time_saleEndTime_' + sid] = 0;
                }

                $(document.body).trigger("fixednum_view:lastNum", data.lastBid);
                prependBid(data, sid);
                (data.endTime > 0) && cb(data.lastBid);
            } else {
                cb();
            }
        });
    }

    function likeBoxSlide(html, likeBox) {
        likeBox.css({display: 'block', overflow: 'hidden'});
        likeBox.html(html);
        likeBox.animate({maxHeight: 316}, 300, function () {
            likeBox.css({display: 'table', overflow: 'visible'});
        });
    }

    var fixednum_extend = {
        tip: function(txt){
            txt = parseInt(txt);
            if(!txt){
                return '';
            }
            var fixedPriceObj = $('.fixednumMain .fixedPrice');
            if(fixedPriceObj.length > 0){
                var fixedPrice = fixedPriceObj.attr('fixedPrice');
                if(txt >= fixedPrice) {
                    return '一口价立即成交！';
                }
            }
            var clearPriceObj = $('<div class="clearPrice"></div>');
            clearPriceObj.one("touchend", function() {
                $(this).parent().parent().find(".editTxt span").html('');
                $(this).remove();
            });
            return clearPriceObj
        }
    };

    $(function () {
        //出价按钮
        $(".saleMain").on("click", ".bidBtn", function () {
            var self = $(this);

            if(WPT.userinfo.uri != '') {
                //有保证金的拍卖不需要关注就能出价
                if (!isSubscribe && self.parents(".saleItem").find(".hasBzj").length == 0) {
                    wptSubscribe('', 'http://cdn01.weipaitang.com/res/img/subscribe/saleBid.png', self.attr("uri") + "#bid");
                    return false;
                }

                var needTelephone = self.attr("needTelephone") == "Y" ? true : false;
                if (!hasTelephone || (needTelephone && hasTelephone == 1)) {
                    $(document).off("verifyTelephone_view:success").one("verifyTelephone_view:success", function () {
                        self.trigger("click");
                    });
                    $(document).trigger('verifyTelephone_view:show', ['my', 'all', '请输入你的电话号码，方便卖家联系', '验证号码后才能继续出价']);
                    return false;
                }
            }

            saleId = self.attr("saleId");
            if ($(".saleEndTime_" + saleId).length == 0) {
                wptAlert($(".sale" + saleId + " .stateInfo .state").text());
                return;
            }

            var type = self.attr("type");
            var fixedPrice = self.attr("fixedPrice");
            var increase = self.attr("increase");
            var bidbzj = self.attr("bidbzj");
            var multiWins = self.attr("multiWins");
            var bidMoney = self.siblings(".price").html();

            refreshBid(saleId, function (lastBid) {
                if (typeof lastBid == "undefined") {
                    lastBid = $(".sale" + saleId + " .bidList .ddli .price").attr("money");
                    if (lastBid == undefined) {
                        lastBid = "0";
                    }

                    lastBid = parseInt(lastBid.replace(/,/g, ''));
                }
                $(document.body).trigger("fixednum_view:show", [lastBid, parseInt(type), parseInt(fixedPrice), parseInt(increase), parseInt(bidMoney), parseInt(bidbzj), parseInt(multiWins)]);
            });
        });

        //更新出价按钮
        $(".saleMain").on("click", '.updateBid', function (e) {
            var sid = $(this).attr('saleId');
            refreshBid(sid);
        });

        //查看更多出价
        $(".saleMain").on("click", ".bidMore", function () {
            var self = $(this);
            var page = parseInt(self.attr("page"));
            if (isNaN(page)) return;

            var id = self.attr("id");
            var sid = id.replace("sale", "");

            $(this).html("加载中...");
            $.get("/wpt/getMoreBid?saleId=" + sid + '&page=' + page, function (html) {
                if (html) {
                    self.before(html);
                    var ddli = $(html).filter('.ddli').length;
                    if (ddli < PAGE_NUM) {
                        self.remove();
                    } else {
                        self.html("查看更多");
                    }
                    self.attr("page", parseInt(page) + 1);
                } else {
                    self.remove();
                }
            });
        });

        //拍品删除
        $(".saleMain").on("click", ".saleDel", function () {
            var uri = $(this).attr("uri");
            $(document).off('wptConfirm_view:sure').one('wptConfirm_view:sure', function () {
                $.getJSON("/auction/sale_del?uri=" + uri, function (res) {
                    if (res.errcode > 0) {
                        wptAlert(res.errmsg);
                    } else {
                        wptAlert(res.errmsg, function () {
                            wptRedirect('/my/saleList/draft');
                        });
                    }
                });
            });
            wptConfirm('您确认要下架此拍品？', {text: '确认下架'});
        });

        //拍品隐藏/显示
        $(".saleMain").on("click", ".saleToggle", function () {
            var self = $(this);
            var uri = self.attr("uri");
            var action = self.text();

            $(document).off('wptConfirm_view:sure').one('wptConfirm_view:sure', function () {
                $.getJSON("/auction/sale_toggle?uri=" + uri, function (res) {
                    if (res.errcode > 0) {
                        wptAlert(res.errmsg);
                    } else {
                        self.text(action == "隐藏" ? "显示" : "隐藏");
                    }
                });
            });
            wptConfirm('您确认要' + action + '此拍品？', {text: '确认' + action});
        });

        //图片预览
        $(".saleMain").on("click", ".imgList>div", function (e) {
            var self = $(this);
            // 如果是视频预览，就不触发图片预览
            if($(this).hasClass('video-play')) {
                return;
            }
            e.preventDefault();
            self.toggleClass("clickImg");
            var imgdiv = self.parent('.imgList').find("div");
            var urls = [];
            if (imgdiv) {
                imgdiv.forEach(function (v, k) {
                    if($(v).hasClass('video-play')) {
                        var url = $(v).css('backgroundImage').replace(/url\(\"?(.*?)\"?\)/ig, "$1");
                    } else {
                        var url = $(v).css('backgroundImage').replace(/url\(\"?(.*?)\/w\/\d{2,3}\"?\)/ig, "$1/w/640");
                    }
                    urls.push(url);
                });
            }
            setTimeout(function () {
                if (urls) {
                    wx.previewImage({
                        current: urls[self.index()],
                        urls: urls
                    });
                }
                self.toggleClass("clickImg");
            }, 100);
        });

        $('.saleMain').on('click', '.video-play', function(){
            WPT && WPT.playVideo && WPT.playVideo($(this), WPT.getVideoSrc($(this)));
        });

        //证书图片预览
        $(".saleMain").on("click", ".cert", function (e) {
            e.preventDefault();
            var certImg = $(this).attr('cert');

            setTimeout(function () {
                wx.previewImage({
                    current: certImg,
                    urls: [certImg]
                });
            }, 100);
        });

        
        //分享按钮
        $(".saleMain").on("click", ".shareBtn, .shareIt", function () {
            var self = $(this);
            if (!hasTelephone && self.hasClass('shareBtn')) {
                $(document).off("verifyTelephone_view:success").one("verifyTelephone_view:success", function(){
                    self.trigger("click");
                });
                $(document).trigger('verifyTelephone_view:show', ['my', 'all', '请输入您的手机号码', '验证号码后才能分享']);
                return false;
            }

            //赋值saleId, 回调时候 用到
            saleId = self.attr("saleId");

            wxShare.title = self.attr("title");
            wxShare.summary = self.attr("summary");
            wxShare.desc = self.attr('desc');
            wxShare.link = self.attr("link");
            wxShare.imgUrl = self.attr("imgUrl");
            wxShare.pics = JSON.parse(self.attr('pics'));

            _statisticData = JSON.parse(self.attr('statisticData'));
            _statisticData.source = 'sale/list';

            WPT.Model.share(wxShare);
        });

        //群发通知
        $(".saleMain").on("click", ".sendMultiMsgBtn", function () {
            var uri = $(this).attr("uri");
            wptLoading('群发通知', function () {
                wptRedirect('/auction/sendSaleMsg?uri=' + uri, 0);
            });
        });

        //绑定出价事件
        $(document).off('fixednum_view:confirm').on('fixednum_view:confirm', function (e, data) {
            if((data.fixedPrice > 0 && data.num - data.fixedPrice > 3 * data.increase)
                || (data.fixedPrice > 0 && data.num < data.fixedPrice && data.num - data.lastNum > 3 * data.increase)
                || (data.fixedPrice == 0 && data.num - data.lastNum > 3 * data.increase)
                || (data.multiWins > 1 && data.num - data.lastNum > 3 * data.increase)
                || (data.bidbzj == 0 && !(data.fixedPrice > 0 && data.num >= data.fixedPrice) && ((buyerLevel == 2 && (data.num - (data.lastNum == 0 ? data.bidMoney : data.lastNum)) > 3 * data.increase) || (buyerLevel == 1 && (data.num - (data.lastNum == 0 ? data.bidMoney : data.lastNum)) > 1 * data.increase)))) {
                if (data.bidbzj == 0 && !(data.fixedPrice > 0 && data.num >= data.fixedPrice) && ((buyerLevel == 2 && (data.num - (data.lastNum == 0 ? data.bidMoney : data.lastNum)) > 3 * data.increase) || (buyerLevel == 1 && (data.num - (data.lastNum == 0 ? data.bidMoney : data.lastNum)) > 1 * data.increase))) {
                    wptAlert("请逐手出价");
                } else {
                    if(data.multiWins == 1 && (buyerLevel > 1 || data.bidbzj > 0 || data.increase < 30)) {
                        $(document).off('wptConfirm_view:sure').one('wptConfirm_view:sure', function () {
                            bid(data.num, data.lastNum);
                        });
                        if (data.fixedPrice > 0 && data.num - data.fixedPrice > 3 * data.increase) {
                            wptConfirm('您的出价已远远高于一口价 <br/> 您确认要出价？', {text: '确认出价'});
                        } else {
                            wptConfirm('您的出价已超加价幅度的3倍 <br/> 您确认要出价？', {text: '确认出价'});
                        }
                    } else {
                        wptAlert("任性的亲，出价太高了，考虑下重新出价！");
                    }
                }
            } else {
                bid(data.num, data.lastNum);
            }
        });

        //围观
        $('.saleMain').on('click', '.likeIt', function () {
            var self = $(this);
            var uri = self.attr('saleuri');
            if (!uri || self.hasClass('liked')) {
                return false;
            }

            var url = '/wpt/likeIt/' + uri + '?t=1491039004';
            $.getJSON(url, function (res) {
                if (typeof res.errcode != 'undefined') {
                    if (res.errcode == 0) {
                        self.addClass('liked');

                        var likeBox = $('#likeBox_' + uri);
                        var likeIt = $('#likeIt_' + uri);
                        likeBox.show();
                        var likeTotal = parseInt(likeIt.find('span').text());
                        if (!likeTotal) {
                            likeTotal = 0;
                        }
                        var _likeAvatar = '<a href="/user/' + WPT.userinfo.uri + '" class="likeAvatar" style="background-image: url(' + headimgurl(WPT.userinfo.headimgurl, 64) + ')"></a>';
                        likeBox.prepend(_likeAvatar);

                        likeIt.find('span').html(++likeTotal);

                        var popularity = likeIt.siblings('.popularity');
                        var popularityNum = popularity.find('span').text() || 0;
                        popularity.find('span').html(popularityNum * 1 + 10);

                        if (!isSubscribe) {
                            wptSubscribe('', 'http://cdn01.weipaitang.com/res/img/subscribe/saleLike.png', '/uri/'+ uri +'#like_'+ uri);
                        }

                        //采集数据 关注
                        if (typeof res.isAttention != 'undefined' && res.isAttention == -1) {
                            $(document).trigger("wptStatistic:sendData", ['{"data":{"u":"' + uri + '","uu":"' + WPT.userinfo.uri + '","su":"' + res.su + '","uc":"' + res.uc + '","usc":"' + res.usc + '"},"route":"attention"}']);
                        }
                        //采集数据 点赞
                        $(document).trigger("wptStatistic:sendData", ['{"data":{"u":"' + uri + '","uu":"' + WPT.userinfo.uri + '","su":"' + res.su + '","uc":"' + res.uc + '","usc":"' + res.usc + '"},"route":"like"}']);
                    } else if(res.errcode == 100) {
                        self.addClass('liked');
                        WPT.wechat && wptAlert(res.errmsg);
                    } else if (res.errcode != 401) {
                        wptAlert(res.errmsg);
                    }
                }
            });
        });

        //获取所有点赞列表
        $('.saleMain').on('click', '.getMoreLike', function () {
            var self = $(this);
            var uri = self.attr('saleuri');
            if (!uri) {
                return;
            }
            var likeBox = $('#likeBox_' + uri);
            var tmpLikeBox = $('#tmpLikeBox_' + uri);
            if (!self.html()) {
                var html = tmpLikeBox.html();
                tmpLikeBox.html(likeBox.html());
                likeBox.html(html);
                likeBox.css({maxHeight: 62});
            } else {
                var html = tmpLikeBox.html();
                tmpLikeBox.html(likeBox.html());
                if (html) {
                    likeBoxSlide(html, likeBox);
                } else {
                    var url = '/wpt/getMoreLike/' + uri;
                    $.getJSON(url, function (res) {
                        if (!res.errcode) {
                            var html = '';
                            res.likeList.forEach(function (e) {
                                if(e.headimgurl.indexOf("/res/img/nohead.jpg") == -1) {
                                    html += '<a href="' + e.userinfoUri + '" class="likeAvatar" style="background-image: url(' + e.headimgurl + ')"></a>';
                                }
                            });
                            html += '<div class="getMoreLike" saleuri="' + uri + '"><span class="wptFi fi-stack"><i class="wptFi icon-arrowup fi-stack-1x"></i></span></div>';
                            likeBoxSlide(html, likeBox);
                        } else {
                            wptAlert(res.errmsg);
                        }
                    });
                }
            }
        });

        $('.saleMain').on('click', '.follow', function(e){
            e.preventDefault();
            var self = $(this);
            var u = self.attr("u");
            var uc = self.attr("uc");
            var usc = self.attr("usc");
            var su = self.attr('userinfoUri');

            if (!isSubscribe) {
                wptSubscribe('', 'http://cdn01.weipaitang.com/res/img/subscribe/saleAttention.png', '/uri/'+ u + "#attention_" + su);
                return false;
            }

            var data = 'userinfoUri=' + su + '&act=attention&ajax=1';
            $.getJSON('/user/attention', data, function (res) {
                if(res.errcode >= 400){
                    wptAlert(res.errmsg);
                    return false;
                }
                self.animate({"opacity": 0}, 300, function() {
                    if(res.errcode == 0) {
                        //关注及点赞
                        self.parents(".saleItem").find(".likeIt").trigger("click");

                        //统计数据
                        $(document).trigger("wptStatistic:sendData", ['{"data":{"u":"' + u + '","uu":"","su":"' + su + '","uc":"' + uc + '","usc":"' + usc + '"}, "route":"attention"}']);
                    }
                    self.remove();
                });
            });
        });

        //私信
        $('.saleMain').on('click', '.chat', function(e){
            var url = '/message/messages/' + $(this).attr('userinfoUri') + "?saleUri=" + $(this).attr("saleUri");
            if(WPT.pc || WPT.app) {
                $.getJSON('/scanScope/getRedirectUrl?url=' + url, function(res) {
                    if(res.status.code == 0) {
                        wptRedirect(res.data.url);
                    }
                });
            } else {
                wptRedirect(url);
            }
        });

        //多人中拍说明
        $('.saleMain').on('click', '.multiWinsBtn', function(){
            var N = $(this).attr('multiWins');
            wptAlert("<div style='text-align: left;width: 90%;margin: 0 auto;'>a. 如只有 1 人参拍，则该人中拍<br />b. 有 " + N + " 人以上参拍，则 " + N + " 人以第 " + N + " 个高的出价中拍<br />c. 每人只成交一件</div>");
        });

        //PC版微信退出
        if(WPT.pc) {
            $('.head .logout').on('click', function () {
                $.getJSON('/scanScope/logout', function (data) {
                    if (data.status.code == 0) {
                        window.location.reload();
                    }
                });
            });
        }
    });
</script>

