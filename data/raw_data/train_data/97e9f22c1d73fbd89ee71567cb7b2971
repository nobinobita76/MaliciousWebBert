<script type="text/javascript" src="http://cdn01.weipaitang.com/res/js/zepto.wptStatistic.min.js?v=0.9.1.48"></script>
<script language="javascript">
    var reg = new RegExp("(^|&)sc=([^&]*)(&|$)", "i");
    var matches = window.location.search.substr(1).match(reg);
    var scene = "";
    if (matches != null) {
        scene = unescape(matches[2]);
        sessionStorage.setItem('sc', scene);
    }

    var _statisticData = _behaviorData = {};
    try {
        _statisticData = JSON.parse('{"u":"1703311657og0od5","uc":"1","usc":"1001","su":"1607060015peb6H6"}');
        _behaviorData = JSON.parse('{"u":"1703311657og0od5","uc":"1","usc":"1001","su":"1607060015peb6H6","ui":"http:\/\/cdn01.weipaitang.com\/img\/20170331WKu81_FsrW90yDYCJv-XfyWByEBf-gFht84wefMcAcBNXgELMtpdkA17CDQep53Q-W480H640\/w\/320","ud":"\u653e\u6f0f\u5566\uff01\u653e\u6f0f\u5566\uff01\u5929\u7136\u548c\u7530\u78a7\u7389&nbsp;&nbsp;\u677e\u4e0b","uet":1491058859,"si":"http:\/\/cdn01.weipaitang.com\/certify\/20170223STRegaxWLohigG1Weizuv_OAzfH6umeOdGbKn3X7zxw6LVsyoIUYi0PTnj7XU_hR-W640H640\/w\/96","sn":"\u7f8e\u7389\u4e4b\u5bb6158"}');
    } catch(e) {}

    var wptStatisticData = null;
    $(function () {
        //统计初始化
        wptStatisticData = $.wptStatistic();
        _statisticData = $.extend(_statisticData, {
            uu: "",
            ref: ""
        });
        $(document).trigger(
            "wptStatistic:sendData",
            ['{"data":' + JSON.stringify(_statisticData) + ', "route": "visit"}']
        );
        //触发曝光统计
        $(document).trigger("wptStatistic:sendExposureData");

        //千人千面数据采集
        if(!$.isEmptyObject(_behaviorData) && WPT && WPT.userinfo && WPT.userinfo.uri) {
            _behaviorData = $.extend(_behaviorData, {
                uu: WPT.userinfo.uri
            });
            setTimeout(function () {
                $(document).trigger(
                    "wptStatistic:sendData",
                    ['{"data":' + JSON.stringify(_behaviorData) + ', "route": "behavior"}']
                );
            }, 3000);
        }
    });
</script>

