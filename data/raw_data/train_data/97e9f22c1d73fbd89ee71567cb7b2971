<script type="text/javascript">
    var keyDownFun = null;
    var verify_telephone_focus = null;
    var verify_telephone_options = '{}';
    try {
        verify_telephone_options = JSON.parse(verify_telephone_options);
    } catch (e) {
        verify_telephone_options = {};
    }

    var verify_telephone_type = 'telephone';
    var checkTelephone = false;
    var customServices_telephone = '0571-81110039';
    var verify_telephone_fun = {
        format: function (num) {
            num = num.replace(/[\D]/g, '');
            if(num.length > 0) {
                verify_telephone_focus.find('span').removeClass('hasValue').addClass('hasValue');
            }else{
                verify_telephone_focus.find('span').removeClass('hasValue');
            }
            if(verify_telephone_focus.hasClass('telephoneInput')) {
                num = num.replace(/(\d{3})-?(\d{4})-?(\d{1,4}).*/, "$1-$2-$3");
            } else if(verify_telephone_focus.hasClass('secretCodeInput')) {
                num = num.substr(0, 4);
            }
            return num;
        },
        check: function(t) {
            var _emp = /^\s*|\s*$/g;
            t = t.replace(_emp, "");
            var _d = /^1[3578][01379]\d{8}$/g;
            var _l = /^1[34578][01256]\d{8}$/g;
            var _y = /^(134[012345678]\d{7}|184\d{8}|1[34578][012356789]\d{8})$/g;
            if (_d.test(t)) {
                return 3;
            } else if (_l.test(t)) {
                return 2;
            } else if (_y.test(t)) {
                return 1;
            }
            return 0;
        }
    };

    function showVerifyTelephone_view(type, checkType, title, tipsText) {
        verify_telephone_type = type;
        checkTelephone = false;

        checkType = typeof checkType == 'undefined' ? 'all' : checkType;
        if(checkType == "all"){
            $(".checkBtnBanner .btn").addClass('show');
        } else {
            $(".checkBtnBanner .btn[type='" + checkType + "']").addClass('show');
        }

        if(title){
            $(".telephoneBox .title").text(title);
        }

        if(tipsText){
            $(".tips .tipText").text(tipsText);
        }

        $(".verifyTelephoneMain .telephoneBox").show();
        $(".verifyTelephoneMain .secretCodeBox").hide();
        $(".btnBanner .confirmBtn").text("下一步");
        $(".tips .checkResult").text("");
        verify_telephone_focus = $(".telephoneInput");

        maskIsHide = false;
        pushStateEvent('verifyTelephone_view:hide');
        $(".verifyTelephoneMain").show().animate({
            bottom: '0px'
        }, 100, 'ease-in-out');
    }

    function showCheckTelephone_view(type, checkType, tipsText, telephone, send) {
        verify_telephone_type = type;
        checkTelephone = true;

        checkType = typeof checkType == 'undefined' ? 'all' : checkType;
        if(checkType == "all"){
            $(".checkBtnBanner .btn").addClass('show');
        } else {
            $(".checkBtnBanner .btn[type='" + checkType + "']").addClass('show');
        }

        if(tipsText){
            $(".tips .tipText").text(tipsText);
        }

        if(telephone) {
            $(".secretCodeBox .title label").text('将发送至');
            $(".secretCodeBox .title .telephone").text(telephone);
            $(".verifyTelephoneMain .telephoneInput span").text(telephone);
        }

        $(".verifyTelephoneMain .telephoneBox").hide();
        $(".verifyTelephoneMain .secretCodeBox").show();
        $(".btnBanner .confirmBtn").text("确定");
        $(".tips .checkResult").text("");
        verify_telephone_focus = $(".secretCodeInput");

        maskIsHide = false;
        pushStateEvent('verifyTelephone_view:hide');
        $(".verifyTelephoneMain").show().animate({
            bottom: '0px'
        }, 100, 'ease-in-out', function(){
            if(send) {
                var sendIndex = 0;
                if(checkType == "all"){
                    sendIndex = send == 'call' ? 1 : 0;
                }
                $(".checkBtnBanner .btn.show").eq(sendIndex).trigger("click");
            }
        });
    }

    function hideVerifyTelephone_view() {
        maskIsHide = true;
        $(".verifyTelephoneMain").animate({
            bottom: '-400px'
        }, 200, function () {
            $(".verifyTelephoneMain").hide();
        });
    }

    $(function () {
        $(document).on('verifyTelephone_view:hide', function () {
            hideVerifyTelephone_view();
        });

        $(document).on('verifyTelephone_view:show', function (e, type, checkType, title, tipsText) {
            showVerifyTelephone_view(type, checkType, title, tipsText);
        });

        $(document).on('showCheckTelephone_view:show', function (e, type, checkType, tipsText, telephone, send) {
            showCheckTelephone_view(type, checkType, tipsText, telephone, send);
        });

        //关闭
        $(".verifyTelephoneMain .wptMask").on("click", function (e) {
            e.preventDefault();
            e.stopPropagation();
            if ($.os.android && ignorePhoneModel.indexOf(getPhoneModel()) == -1 && pushState && pushState.backevent && pushState.backevent == "verifyTelephone_view:hide") {
                setTimeout(function() {
                    history.back();
                }, 0);
            } else {
                $(document.body).trigger("verifyTelephone_view:hide");
            }
        });

        $(".verifyTelephoneMain .numkey ul li").on("touchstart", function () {
            if ($(this).hasClass("othernum") || $(this).html() == '' || $(this).html() == '.') {
                $(this).css({"background-color": "#FFF"});
            } else {
                $(this).css({"background-color": "#D1D5DA"});
            }
        });

        $(".verifyTelephoneMain .numkey ul li.num").on("touchend", function (e) {
            e.preventDefault();
            e.stopPropagation();
            if($(this).hasClass('othernum')){
                $(this).css({"background-color": "#D1D5DA"});
                if ($.os.android && ignorePhoneModel.indexOf(getPhoneModel()) == -1 && pushState && pushState.backevent && pushState.backevent == "verifyTelephone_view:hide") {
                    setTimeout(function() {
                        history.back();
                    }, 0);
                } else {
                    $(document.body).trigger("verifyTelephone_view:hide");
                }
                return false;
            }
            var clickNum = $(this).find("div").text();
            if (clickNum == null) {
                clickNum = $(this).html();
            }
            var content = verify_telephone_focus.find('span').html() + clickNum;
            content = verify_telephone_fun.format(content);

            verify_telephone_focus.find('span').html(content);
            if (clickNum.length == 1 && clickNum != ".") {
                $(this).css({"background-color": "#FFF"});
            } else {
                $(this).css({"background-color": "#D1D5DA"});
            }
        });

        //删除
        $(".verifyTelephoneMain .numkey ul li.delete").on("touchend", function (e) {
            e.preventDefault();
            e.stopPropagation();
            var content = verify_telephone_focus.find('span').html();
            var content = content.substr(0, content.length - 1);
            content = verify_telephone_fun.format(content);

            verify_telephone_focus.find('span').html(content);
            $(this).css({"background-color": "#D1D5DA"});
        });

        //确认提交
        $(".verifyTelephoneMain .confirmBtn").on("touchend", function (e) {
            e.preventDefault();
            var checkResultTxtObj = $(".tips .checkResult");
            checkResultTxtObj.text("");
            var telephone = $(".verifyTelephoneMain .telephoneInput span").html();
            if(verify_telephone_focus.hasClass('telephoneInput')) {
                var oldTelephone = $(".secretCodeBox .title .telephone").text();
                if(verify_telephone_fun.check(telephone.replace(/-/g, '')) == 0) {
                    checkResultTxtObj.text("电话号码有误");
                    return false;
                }

                if(telephone == oldTelephone) {
                    checkResultTxtObj.text("");
                    $(".verifyTelephoneMain .telephoneBox").hide();
                    $(".verifyTelephoneMain .secretCodeBox").show();
                    $(".btnBanner .confirmBtn").text("确认");
                    verify_telephone_focus = $(".secretCodeInput");
                    return false;
                }

                clearInterval(keyDownFun);
                $(".secretCodeBox .checkBtnBanner .btn.keyDown").removeClass('keyDown').text($(this).attr('txt')).show();
                $(".secretCodeBox .secretCode .secretCodeInput span").text('');

                //检查电话号码
                $.post(
                    '/' + verify_telephone_type + '/verify_telephone',
                    {
                        tel: telephone.replace(/-/g, '')
                    },
                    function(res){
                        if(res.errcode == 0){
                            checkResultTxtObj.text("");
                            $(".verifyTelephoneMain .telephoneBox").hide();
                            $(".verifyTelephoneMain .secretCodeBox").show();
                            $(".secretCodeBox .title .telephone").text(telephone);
                            $(".btnBanner .confirmBtn").text("确认");
                            verify_telephone_focus = $(".secretCodeInput");
                            $(".checkBtnBanner .btn.show").eq(0).trigger("click");
                        }else{
                            checkResultTxtObj.text(res.errmsg);
                        }
                    },
                    'json'
                );
            } else if(verify_telephone_focus.hasClass('secretCodeInput')) {
                var secretCodeInput = $(".secretCodeInput span").text();
                if(secretCodeInput.length != 4){
                    checkResultTxtObj.text("验证码错误");
                    return false;
                }

                var _data = {
                    tel: telephone.replace(/-/g, ''),
                    code: secretCodeInput,
                    checkTelephone: checkTelephone ? 1 : 0
                };
                $.each(verify_telephone_options, function(i,n) {
                    _data[i] = n;
                });
                $.post(
                    '/' + verify_telephone_type + '/verify_telephone_return',
                    _data,
                    function(res){
                        if(res.errcode == 0) {
                            hasTelephone = 11111111111;
                            hideVerifyTelephone_view();
                            setTimeout(function(){
                                if(checkTelephone) {
                                    $(document.body).trigger("checkTelephone_view:success", telephone);
                                } else {
                                    $(document.body).trigger("verifyTelephone_view:success", telephone);
                                }
                            }, 500);
                        }else if(res.errcode == 302){
                            $('.verifyTelephoneMain .verifyMain').hide();
                            $('.verifyTelephoneMain .errBox').show();
                            $('.verifyTelephoneMain .errBox .errTitle').html(res.errtitle);
                            
                            if(res.errdata){
                                $('.verifyTelephoneMain .errBox .errContent').html(res.errdata);
                            }
                        } else {
                            checkResultTxtObj.text(res.errmsg);
                        }
                    },
                    'json'
                );
            }
        });

        $(".smsCheck, .callCheck").on('click', function(e){
            var self = $(this);
            if(self.hasClass("keyDown")){
                return;
            }
            self.addClass('keyDown').text('59 秒后可重新获取');
            self.siblings(".show").hide();
            keyDownFun = setInterval(function () {
                var seconds = parseInt(self.text());
                self.html((--seconds) + ' 秒后可重新获取');
                if (seconds <= 0) {
                    clearInterval(keyDownFun);
                    self.removeClass('keyDown').text(self.attr('txt'));
                    self.siblings(".show").show();
                }
            }, 1000);

            //发送验证码
            var telephone = $(".verifyTelephoneMain .telephoneInput span").text();
            $(".secretCodeBox .title label").text('已发送至');

            $.getJSON(
                '/my/secret_code/'+ self.attr("type"),
                {
                    tel:telephone.replace(/-/g, '')
                },
                function(res){
                    if(res.errcode == 1000) {
                        wptAlert(res.errmsg + "60秒后请重新再试");
                    } else if (res.errcode == 999) {
                        wptAlert(res.errmsg);
                    } else if (res.errcode > 0) {
                        wptAlert("验证码发送失败，60秒后请重新再试");
                    } else {
                        if(self.hasClass('callCheck')){
                            $(".secretCodeBox").find('.callNotice').show();
                        }
                    }
                    $(".tips .tipText").html('<span>收不到验证码？</span>');
                }
            );
        });

        //人工验证
        $(".tips .tipText").on("click", "span", function(){
            $(this).parent().html('请拨 <a href="tel:'+ customServices_telephone +'">'+ customServices_telephone +'</a> 进行人工验证！');
        });

        //点击知道了
        $(".verifyTelephoneMain .errBox .errButton").on("click", "button", function(){
            hideVerifyTelephone_view();
        });

        //键盘输入
        if(typeof WPT != 'undefined' && WPT.pc) {
            $(document).on('keyup', function (e) {
                if ($('.verifyTelephoneMain').css('display') != 'block') {
                    return;
                }
                if ([8, 13, 27, 37, 38, 39, 40].indexOf(e.keyCode) > -1) {
                    // backspace
                    if (e.keyCode == 8) {
                        var content = focusEditTxt.find('span').html();
                        content = content.substr(0, content.length - 1);

                        if (typeof verify_telephone_fun != 'undefined') {
                            //tip函数直接返回需要显示的描述，无描述需返回''
                            if (typeof verify_telephone_fun.tip == 'function') {
                                $(".verifyTelephoneMain .tips").html(verify_telephone_fun.tip(content));
                            }

                            if (typeof verify_telephone_fun.format == 'function') {
                                content = verify_telephone_fun.format(content);
                            }
                        }
                        focusEditTxt.find('span').html(content);
                    } else if (e.keyCode >= 37 && e.keyCode <= 40) {
                        return;
                    } else if (e.keyCode == 13 || e.keyCode == 27) {
                        // Enter Key
                        var _funName = e.keyCode == 13 ? 'finish' : 'cancel';
                        if (typeof verify_telephone_fun != 'undefined') {
                            if (typeof verify_telephone_fun[_funName] == 'function') {
                                if (!verify_telephone_fun[_funName]()) {
                                    return;
                                }
                            }
                        }
                        hideVerifyTelephone_view();
                    }
                    return;
                }

                if(e.keyCode < 48 || (e.keyCode > 57 && e.keyCode < 96) || e.keyCode > 105) {
                    return;
                }

                var clickNum = e.keyCode - 48;
                if(e.keyCode >= 96) {
                    clickNum = e.keyCode - 96;
                }
                var content = focusEditTxt.find('span').html() + clickNum;
                if (typeof verify_telephone_fun != 'undefined') {
                    if (typeof verify_telephone_fun.tip == 'function') {
                        $(".verifyTelephoneMain .tips").html(verify_telephone_fun.tip(content));
                    }

                    if (typeof verify_telephone_fun.format == 'function') {
                        content = verify_telephone_fun.format(content);
                    }
                }
                focusEditTxt.find('span').html(content);
            });
        }
    });
</script>

