</style><script type="text/javascript">
    var pageloadOver = false;
    var scrollFun = null;

    //登录成功回调
    WPT.loginCallback = function() {
        $('.head.home .logout').addClass('show');
    };

    function loadNextPage() {
        if($(".loadNextPage").length > 0) {
            if ($(".loadNextPage").attr("lastSaleUri")) {
                loadNextPageByLastSaleId();
            } else if ($(".loadNextPage").attr("page")) {
                loadNextPageByPage();
            }
        }
    }

    function loadNextPageByPage() {
        if (pageloadOver) return;

        var page = parseInt($(".loadNextPage").attr("page"));
        if (isNaN(page)) return;

        pageloadOver = true;
        $(".loadNextPage").css("visibility", "visible");
        $.get('/wpt?ajax=true&page=' + page, function (html) {
            if (html.trim()) {
                $($(".loadNextPage").attr("html2append")).append(html);
                $(".loadNextPage").attr("page", page + 1);
                pageloadOver = false;

                //触发曝光统计
                $(document).trigger("wptStatistic:sendExposureData");
            } else {
                $(".loadNextPage").remove();
            }
        });
    }

    function loadNextPageByLastSaleId() {
        if (pageloadOver) return;

        var lastSaleUri = $(".loadNextPage").attr("lastSaleUri");
        if (!lastSaleUri) return;

        pageloadOver = true;
        $(".loadNextPage").css("visibility", "visible");
        $.get('/wpt?ajax=true&lastSaleUri=' + lastSaleUri, function (html) {
            if (html.trim()) {
                $($(".loadNextPage").attr("html2append")).append(html);
                $(".loadNextPage").attr("lastSaleUri", $(html).last().attr('saleUri'));
                pageloadOver = false;

                //触发曝光统计
                $(document).trigger("wptStatistic:sendExposureData");
            } else {
                $(".loadNextPage").remove();
            }
        });
    }

    var wxShare = {
