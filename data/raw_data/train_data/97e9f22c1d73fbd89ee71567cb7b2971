<script type="text/javascript">
    var focusEditTxt;

    //格式化数字
    function number_format(n) {
        var ns = n.toString();
        var mm = ns.match(/\d{1,3}(?=(\d{3})+$)/g);
        if (mm == null) return ns;
        mm.push(ns.replace(mm.join(""), ""));
        return mm.join(",");
    }

    function showFixednum_view(txt, type, fixedPrice, increase, bidMoney, bidbzj, multiWins, targetMoney) {
        maskIsHide = false;
                    if(isNaN(parseInt(txt))) {
                $(".tipBanner .title").html(txt);
                $(".fixednumMain .last").hide();
                txt = 0;
            } else if (type == 1) {
                $(".tipBanner .title").html("一元拍");
                $(".fixednumMain .last").hide();
            } else {
                $(".fixednumMain .last").html(number_format(txt) + "元").show();
            }
            $(".fixednumMain .editTxt").attr("lastNum", txt);
            $(".fixednumMain .editTxt").attr("type", type);
            $(".fixednumMain .editTxt").attr("fixedPrice", fixedPrice);
            $(".fixednumMain .editTxt").attr("increase", increase);
            $(".fixednumMain .editTxt").attr("bidMoney", bidMoney);
            $(".fixednumMain .editTxt").attr("bidbzj", bidbzj);
            $(".fixednumMain .editTxt").attr("multiWins", multiWins);
            $(".fixednumMain .editTxt").attr("targetMoney", targetMoney);

            var editPrice = 1 * txt;
            if(editPrice == 0 && bidMoney > 0) {
                editPrice = bidMoney;
            } else {
                editPrice = fixedPrice == 0 ? (editPrice + increase) : ((editPrice + increase) > fixedPrice ? fixedPrice : (editPrice + increase));
            }

            $(".fixednumMain .editTxt").find('span').html(editPrice);
            if(typeof fixednum_extend != 'undefined') {
                if(typeof fixednum_extend.tip == 'function'){
                    $(".fixednumMain .tips").html(fixednum_extend.tip(editPrice));
                }
            }

            $(".fixednumMain .fixedPrice").remove();
            if(fixedPrice){
                $(".fixednumMain .confirmBtn").before('<div class="fixedPrice" fixedPrice="' + fixedPrice + '">一口价￥' + number_format(fixedPrice) + '</div>');
            }

            $(".fixednumMain .changePrice").remove();
            if(type == 1) {
                $(".fixednumMain .confirmBtn").before('<div class="changePrice sub"></div>');
                $(".fixednumMain .confirmBtn").text("出价");
                $(".fixednumMain .confirmBtn").after('<div class="changePrice add"></div>');

                $(".fixednumMain .agreeBanner").html('出价即表示同意<span url="//mp.weixin.qq.com/s/CHDGAXybNLM_AXvn7EohUg">《微拍堂一元拍服务协议》</span>')
            }
                $(".fixednumMask").show().animate({
            opacity: 0.382
        }, 100);

        $(".fixednumMain").show().animate({
            bottom: '0px'
        }, 100, 'ease-in-out');
    }

    function hideFixednum_view() {
        maskIsHide = true;
        $(".fixednumMain .tips").html('');
        $(".fixednumMain").animate({
            bottom: '-400px'
        }, 150, function () {
            $(".fixednumMain, .fixednumMask").hide();
        });
    }

    $(function () {
        $(document).on('fixednum_view:focus', function (e, editObj) {
            focusEditTxt = $(editObj);
        });

        $(document).on('fixednum_view:hide', function (e) {
            hideFixednum_view();
        });

        $(document).on('fixednum_view:show', function (e, txt, type, fixedPrice, increase, bidMoney, bidbzj, multiWins, targetMoney) {
            setTimeout(function() {
                showFixednum_view(txt, type, fixedPrice, increase, bidMoney, bidbzj, multiWins, targetMoney);
            }, 0);
        });

                $(document).on('fixednum_view:lastNum', function (e, txt) {
            var editTxtObj = $(".fixednumMain .editTxt");
            editTxtObj.attr("lastNum", txt); 
            editTxtObj.find('span').html(parseInt(txt) + parseInt(editTxtObj.attr("increase")));
	    
            $(".fixednumMain .last").html(number_format(txt) + "元");
        });
        
        $(".fixednumMain").on("click", function (e) {
            e.preventDefault();
        });

        $(".fixednumMain").on("touchend", ".agreeBanner span", function () {
            if($(this).attr("url")) {
                wptRedirect($(this).attr("url"));
            }
        });

        $(".fixednumMain .close, .tipBanner .finish, .fixednumMask").on("touchend", function (e) {
            e.preventDefault();
            if($(this).hasClass("finish")) {
                if (typeof fixednum_extend != 'undefined') {
                    if (typeof fixednum_extend.finish == 'function') {
                        if(!fixednum_extend.finish()) {
                            return;
                        }
                    }
                }
            } else {
                if (typeof fixednum_extend != 'undefined') {
                    if (typeof fixednum_extend.cancel == 'function') {
                        if(!fixednum_extend.cancel()) {
                            return;
                        }
                    }
                }
            }
            hideFixednum_view();
        });

        $(".fixednumMain .numkey ul li").on("touchstart", function () {
            if ($(this).html() == '00' || $(this).html() == '' || $(this).html() == '.') {
                $(this).css({"background-color": "#FFF"});
            } else {
                $(this).css({"background-color": "#D1D5DA"});
            }
        });

        $(".fixednumMain .numkey ul li.num").on("touchend", function () {
            var clickNum = $(this).find("div").html();
            if (clickNum == null) {
                clickNum = $(this).html();
            }

            var content = focusEditTxt.find('span').html() + clickNum;
            if(typeof fixednum_extend != 'undefined'){
                if(typeof fixednum_extend.tip == 'function'){
                    $(".fixednumMain .tips").html(fixednum_extend.tip(content));
                }

                if(typeof fixednum_extend.format == 'function'){
                    content = fixednum_extend.format(content);
                }
            }
            focusEditTxt.find('span').html(content);

            if (clickNum.length == 1 && clickNum != ".") {
                $(this).css({"background-color": "#FFF"});
            } else {
                $(this).css({"background-color": "#D1D5DA"});
            }
        });

        if(typeof WPT != 'undefined' && WPT.pc) {
            $(document).on('keyup', function (e) {
                if ($('.fixednumMain').css('display') != 'block') {
                    return;
                }
                if ([8, 13, 27, 37, 38, 39, 40].indexOf(e.keyCode) > -1) {
                    // backspace
                    if (e.keyCode == 8) {
                        var content = focusEditTxt.find('span').html();
                        content = content.substr(0, content.length - 1);

                        if (typeof fixednum_extend != 'undefined') {
                            //tip函数直接返回需要显示的描述，无描述需返回''
                            if (typeof fixednum_extend.tip == 'function') {
                                $(".fixednumMain .tips").html(fixednum_extend.tip(content));
                            }

                            if (typeof fixednum_extend.format == 'function') {
                                content = fixednum_extend.format(content);
                            }
                        }
                        focusEditTxt.find('span').html(content);

                    } else if (e.keyCode >= 37 && e.keyCode <= 40) {
                        return;
                    } else if (e.keyCode == 13 || e.keyCode == 27) {
                        // Enter Key
                        var _funName = e.keyCode == 13 ? 'finish' : 'cancel';
                        if (typeof fixednum_extend != 'undefined') {
                            if (typeof fixednum_extend[_funName] == 'function') {
                                if (!fixednum_extend[_funName]()) {
                                    return;
                                }
                            }
                        }
                        hideFixednum_view();
                    }
                    return;
                }

                if(e.keyCode < 48 || (e.keyCode > 57 && e.keyCode < 96) || e.keyCode > 105) {
                    return;
                }

                var clickNum = e.keyCode - 48;
                if(e.keyCode >= 96) {
                    clickNum = e.keyCode - 96;
                }
                var content = focusEditTxt.find('span').html() + clickNum;
                if (typeof fixednum_extend != 'undefined') {
                    if (typeof fixednum_extend.tip == 'function') {
                        $(".fixednumMain .tips").html(fixednum_extend.tip(content));
                    }

                    if (typeof fixednum_extend.format == 'function') {
                        content = fixednum_extend.format(content);
                    }
                }
                focusEditTxt.find('span').html(content);
            });
        }

        $(".fixednumMain .numkey ul li.delete").on("touchend", function () {
            var content = focusEditTxt.find('span').html();
            var content = content.substr(0, content.length - 1);

            if(typeof fixednum_extend != 'undefined'){
                //tip函数直接返回需要显示的描述，无描述需返回''
                if(typeof fixednum_extend.tip == 'function'){
                    $(".fixednumMain .tips").html(fixednum_extend.tip(content));
                }

                if(typeof fixednum_extend.format == 'function'){
                    content = fixednum_extend.format(content);
                }
            }
            focusEditTxt.find('span').html(content);

            $(this).css({"background-color": "#D1D5DA"});
        });

                $(".fixednumMain .editTxt").on("click", function (e) {
            e.preventDefault();
            $(this).find('span').removeClass("hover").addClass("hover");
            focusEditTxt = $(this);
        });

        $(".fixednumMain .confirmBtn").on("touchend", function (e) {
            e.preventDefault();
            var num = parseInt(focusEditTxt.find('span').html());
            var lastNum = focusEditTxt.attr("lastNum");
            var type = focusEditTxt.attr("type");
            var fixedPrice = focusEditTxt.attr("fixedPrice");
            var increase = focusEditTxt.attr("increase");
            var bidMoney = focusEditTxt.attr("bidMoney");
            var bidbzj = focusEditTxt.attr("bidbzj");
            var multiWins = focusEditTxt.attr("multiWins");
            var targetMoney = focusEditTxt.attr("targetMoney");
            if(lastNum == 0 && bidMoney > 0) {
                lastNum = bidMoney;
            }

            $(document.body).trigger("fixednum_view:confirm", {'num': num, 'lastNum': lastNum, 'type': type, 'fixedPrice': fixedPrice, 'increase': increase, 'bidMoney': bidMoney, 'bidbzj': bidbzj, 'multiWins': multiWins, 'targetMoney': targetMoney});
        });

        focusEditTxt = $(".fixednumMain .editTxt");

        $(".fixednumMain").on("click", ".fixedPrice", function (e) {
            e.preventDefault();
            e.stopPropagation();
            var fixedPrice = $(this).attr('fixedPrice');
            focusEditTxt.find('span').html(fixedPrice);
            $(".fixednumMain .tips").html(fixednum_extend.tip(fixedPrice));
        });

        $(".fixednumMain").on("touchend", ".changePrice", function (e) {
            e.preventDefault();
            e.stopPropagation();
            var _num = parseInt(focusEditTxt.find('span').html());
            _num = isNaN(_num) ? 0 : _num;
            var targetMoney = focusEditTxt.attr("targetMoney");
            var addUnit = targetMoney > 3000 ? 10 : 1;
            if ($(this).hasClass("add")) {
                focusEditTxt.find('span').html(_num + addUnit);
            } else if ($(this).hasClass("sub")) {
                if (_num >= 2 * addUnit) {
                    focusEditTxt.find('span').html(_num - addUnit);
                } else {
                    focusEditTxt.find('span').html(addUnit);
                }
            }
        });
            });
</script>

