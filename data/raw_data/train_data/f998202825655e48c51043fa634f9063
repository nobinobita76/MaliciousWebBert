<!--
function LTrim(s)
{
    for(var i=0;i<s.length;i++)
        if(s.charAt(i)!=' ')
         return s.substring(i,s.length);
     return "";
}

function RTrim(s)
{
    for(var i=s.length-1;i>=0;i--)
        if(s.charAt(i)!=' ')
            return s.substring(0,i+1);
        return "";
}

function Trim(s)
{
    return RTrim(LTrim(s));
}

function notice(s)
{
	alert(s);
	return false;
}


function getCatalogSql(catalog)
{
		if(catalog=="0")
		{
			return "";
		}
		else if(catalog.length==3)
		{
			return "Catalog1="+catalog;
		}
		else if(catalog.length==6)
		{
			return "Catalog2="+catalog;
		}
		else
		{
			return "Catalog="+catalog+"%";
		}
		
}

function getsearchword()
{
	var _searchword = ""; 
	var _sAndOr = " and "
	if(Trim(document.form1.title.value) != "")
	{
	
	   if(document.form1.domain.value =="1")
	   {
		  if(_searchword != "") 
		    {
		     _searchword = _searchword + _sAndOr;
		    }
		 _searchword = _searchword + "DOCTITLE =" + Trim(document.form1.title.value);
	   }
	   else
	   {
	   _searchword = _searchword + Trim(document.form1.title.value);
	   }
	   document.form1.searchword.value = _searchword;
		}
	
	
	
}

function validWord(_keyword)
{
	if(_keyword.length>0)
	{
		if(_keyword.indexOf("+")>=0)
		{
			return notice("搜索词中不能含有关键字'+'");
		}
		else if(_keyword.indexOf("*")>=0)
		{
			return notice("搜索词中不能含有关键字'*'");
		}
		else if(_keyword.indexOf("-")>=0)
		{
			return notice("搜索词中不能含有关键字'-'");
		}
	}
	return true;
}

function submitsearch()
{
	if(validWord(Trim(document.form1.title.value)))
	{
		getsearchword();
		if(document.form1.searchword.value == "")
		{
			alert("搜索词不为空，请输入搜索词");
			return false;
		}
		else
		{
		if(document.form1.domain.value=="3002")
		{
		document.form1.action="http://210.75.211.170/bjgov/topsortsearch";
		}
		else
		{
		document.form1.action="http://www.bjcz.gov.cn/was40/search?channelid=18369";
		}
		document.form1.submit();
			return true;
		}
	}
	else
	{
		return false;
	}
}

//点击非采购公告
function djfcggg(){
	 document.getElementById('bjsfzfcggg').style.color='#CC6600';
	 document.getElementById('bjszfcggg').style.color='#0044BB';
	 document.getElementById('zcd'). bgColor='#0099FF';
	 document.getElementById('zbgg').href='http://www.bjcz.gov.cn/zfcgcs/bjsfzfcgggcs/syfzbggcs/index.htm';
	 document.getElementById('zgysgg').href='http://www.bjcz.gov.cn/zfcgcs/bjsfzfcgggcs/syfzgysggcs/index.htm';
	 document.getElementById('zbcjgg').href='http://www.bjcz.gov.cn/zfcgcs/bjsfzfcgggcs/syfzbcjggcs/index.htm';
	 document.getElementById('cghtgg').href='http://www.bjcz.gov.cn/zfcgcs/bjsfzfcgggcs/syfcghtggcs/index.htm';
	 document.getElementById('ysjggg').href='http://www.bjcz.gov.cn/zfcgcs/bjsfzfcgggcs/syfysjgggcs/index.htm';
	 document.getElementById('dylygs').href='http://www.bjcz.gov.cn/zfcgcs/bjsfzfcgggcs/syfdylygscs/index.htm';
	 document.getElementById('qtgg').href='http://www.bjcz.gov.cn/zfcgcs/bjsfzfcgggcs/syfqtggcs/index.htm';
	 document.getElementById('cgtztg').href='http://www.bjcz.gov.cn/zfcgcs/bjsfzfcgggcs/syftztgcs/index.htm';
	 return true;
}

