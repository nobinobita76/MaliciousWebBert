function validate_form() {
 if (event.keyCode!=13) {
	 validity = true; // assume valid
	 if (document.websrch.searchword.value=="") {
	  validity = false;
	  alert('您输入的搜索关键字为空！');
	 }
	 if( validity ) {
	  document.websrch.action=document.websrch.select.value;
	  document.websrch.submit();
	 }
	 return validity;
 }
}

function KeyDown()
	{
	   if (event.keyCode==13) {
	  	 validity = true; // assume valid
		 if (document.websrch.searchword.value=="") {
		  validity = false;
		  alert('您输入的搜索关键字为空！');
		  return false;
		 }
		 if( validity ) {
		  document.websrch.action=document.websrch.select.value;
		  document.websrch.submit();
		 }
		 return false;
	   }
	}
