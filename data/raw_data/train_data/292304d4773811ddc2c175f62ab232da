<script>
	var map;
	$(function () {
		
		var mapX = 115.053084;
		var mapY = 35.794003;
		
		var mPoint = new BMap.Point(mapX, mapY);
		map = new BMap.Map("allMap");
		map.centerAndZoom(mPoint, 15);
		map.addControl(new BMap.MapTypeControl({mapTypes: [BMAP_NORMAL_MAP, BMAP_HYBRID_MAP]}));
		map.addControl(new BMap.NavigationControl());
		map.disableScrollWheelZoom();
		
		// 复杂的自定义覆盖物
	    function ComplexCustomOverlay(point, text){
	      this._point = point;
	      this._text = text;
	    }
	    
	    ComplexCustomOverlay.prototype = new BMap.Overlay();
	    ComplexCustomOverlay.prototype.initialize = function(map){
	      this._map = map;
	      var div = this._div = document.createElement("div");
	      div.style.position = "absolute";
	      div.style.zIndex = BMap.Overlay.getZIndex(this._point.lat);
	      div.style.backgroundColor = "#EE5D5B";
	      div.style.border = "1px solid #BC3B3A";
	      div.style.color = "white";
	      div.style.height = "18px";
	      div.style.padding = "2px";
	      div.style.lineHeight = "18px";
	      div.style.whiteSpace = "nowrap";
	      div.style.MozUserSelect = "none";
	      div.style.fontSize = "12px"
	      var span = this._span = document.createElement("span");
	      div.appendChild(span);
	      span.appendChild(document.createTextNode(this._text));      
	      var that = this;
	
	      var arrow = this._arrow = document.createElement("div");
	      arrow.style.background = "url(http://map.baidu.com/fwmap/upload/r/map/fwmap/static/house/images/label.png) no-repeat";
	      arrow.style.position = "absolute";
	      arrow.style.width = "11px";
	      arrow.style.height = "10px";
	      arrow.style.top = "22px";
	      arrow.style.left = "10px";
	      arrow.style.overflow = "hidden";
	      div.appendChild(arrow);
	     
	      map.getPanes().labelPane.appendChild(div);
	      
	      return div;
	    }
	    
	    ComplexCustomOverlay.prototype.draw = function(){
	      var map = this._map;
	      var pixel = map.pointToOverlayPixel(this._point);
	      this._div.style.left = pixel.x - parseInt(this._arrow.style.left) + "px";
	      this._div.style.top  = pixel.y - 30 + "px";
	    }
	    
		$("#distanceBtn").click (function () {
		
			var myDis = new BMapLib.DistanceTool(map);
			myDis.open();  //开启鼠标测距
		});
		
		//周边楼盘
		$(".roundFloorTag").click(function () {
		
			$("#roundFloorList").show();
			$("#roundResult").hide();
			
			$(".tags ul li").removeClass("cur");
			$(this).addClass("cur");
						
			$(".roundFloor:eq(0)").click();
		});
		
		$(".roundFloor").click(function () {
			
			//显示或隐藏界面
			$(".roundFloor").removeClass("cur");
			$(".roundContent").removeClass("cur");
			
			$(this).addClass("cur");
			
			var index = $(this).index();
			$(".roundContent:eq(" + index + ")").addClass("cur");
			
			//添加地图标注
			map.clearOverlays();
			var aryPos = $(".roundFloorPos:eq(" + index + ")").val().split(";");
			for (var i=0; i<aryPos.length; i++) {
			
				map.addOverlay(new ComplexCustomOverlay(new BMap.Point(aryPos[i].split(",")[0], aryPos[i].split(",")[1]), $(".roundContent.cur tr:eq(" + i + ") th a").html()));
			}
			
			map.addOverlay(new ComplexCustomOverlay(mPoint, "香榭里"));
		});
		
		//其他周边
		$(".tags ul li:not(.roundFloorTag)").click(function () {
			
			$("#roundFloorList").hide();
			$("#roundResult").show();
			
			$(".tags ul li").removeClass("cur");
			$(this).addClass("cur");
			
			map.clearOverlays();
			map.addOverlay(new ComplexCustomOverlay(mPoint, "香榭里"));
			
			var pageCap = 2;
			var type = $(this).children("a").html();
			if (type == "综合商场") {
				
				type = new Array("商场", "");		
			}
			else if (type == "宾馆酒店") {
			
				type = new Array("宾馆", "酒店");	
				pageCap = 1;
			}
			else if (type == "娱乐餐饮") {
			
				type = new Array("娱乐", "餐饮");	
				pageCap = 1;
			}
			else if (type == "学校") {
			
				type = new Array("学校", "");
			}
			else if (type == "医院") {
			
				type = new Array("医院", "");
			}
			
			var local = new BMap.LocalSearch(map, {renderOptions: {map: map, panel: "roundResult", autoViewport: false, selectFirstResult: false}, pageCapacity: pageCap});
			local.searchNearby(type, mPoint, 2000);
		});
		
		$(".tags ul li:eq(0)").click();
	});
</script>
