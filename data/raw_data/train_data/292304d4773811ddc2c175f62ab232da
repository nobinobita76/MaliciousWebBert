<script>
	$(function(){
		
	    $("#float_bottom_close").click(function() {
	    
	        $("#float_bottom").hide();
	    });
	    
	    var max_kf_nums = 5;
        var btn_kf_nums = $(".btn_kfs").children("dl").children("dd").length + 1;
        var btn_kf_height = $(".btn_kfs").children("dl").children("dt").children("a").children(".btn_kf").outerHeight();
        $(".btn_kfs").children("dl").children('.bg').width($(".btn_kfs").children("dl").outerWidth());

        $(".btn_kfs").hover(function() {
            if(btn_kf_nums > 1){
                $(this).addClass('show_more');
            }
            if(btn_kf_nums <= max_kf_nums){
                $(".btn_kfs").children("dl").height(btn_kf_height*btn_kf_nums);
            }else{
                $(".btn_kfs").children("dl").height(btn_kf_height*max_kf_nums);
            }
            $(".btn_kfs").children("dl").children('.bg').height(btn_kf_height*btn_kf_nums + 8);
            // console.log(btn_kf_nums + "*" + btn_kf_height + "*" + max_kf_nums);
            console.log($(".btn_kf").height());
            /* Stuff to do when the mouse enters the element */
        }, function() {
            if(btn_kf_nums > 1){
                $(this).removeClass('show_more');
            }
            $(".btn_kfs").children("dl").height(btn_kf_height);
            /* Stuff to do when the mouse leaves the element */
        });
	    
	   $("#float_submit").click(function () {
	    	
	    	$("#float_submit").attr("disabled", true);
	    	
			var name = $("#uhui_name").val();
			var phone = $("#uhui_phone").val();
			var address = $.trim($("#uhui_address").val());
			
			if (name == "" || name == "姓名：" || !isChineseName(name)) {
			
				alert("请输入您的真实姓名！");
				$("#uhui_name").focus();
				$("#float_submit").attr("disabled", false);
				return ;
			}
		
			if (phone == "" || phone == "手机：" || !isMobil(phone)) {
			
				alert("请输入您的手机号码！");
				$("#uhui_phone").focus();
				$("#float_submit").attr("disabled", false);
				return ;
			}
			
			var reg = /[1-9][0-9]{4,}/;
			if (address != "" && address != "QQ号码：" && !reg.test(address)) {
				
				alert("请输入正确的QQ号码！");
				$("#uhui_address").focus();
				$("#float_submit").attr("disabled", false);
				return ;
			}
						
			$.ajax({
			    
		    	url:"http://newhouse.py.fccs.com/newhouse/floor/houseCar.do?seckey=HaLaRuYa&nd=" + new Date().getTime(),
		    	type: "post",
		        data:{
		        	
		        	issueId : 3270001,
		        	name : escape(escape(name)),
		        	phone : phone,
		        	explain : escape(escape("香榭里")),
		        	address : address == "QQ号码：" ? "" : address,
		        	etoken : $("#kanfangToken").val()
		        },
		    	dataType:'jsonp',  
		        jsonp:'callback',
		        success:function(result) {
		        	
		        	if (result) {
	        			
	        			alert (result.msg);
	        			
	        			$("#uhui_name").val("");
	        			$("#uhui_phone").val("");
	        			$("#uhui_address").val("");
		        	}
		        	else {
		        		
		        		alert ("申请提交出现问题，请稍后重新提交");
		        	}
		        	
		        	$("#float_submit").attr("disabled", false);
		        },
		        error:function() {
		        
		        	alert("申请提交出现问题，请稍后重新提交");
		        	$("#float_submit").attr("disabled", false);
		        }
		    });
		});
	});
</script>
