    <div class="t_w_0">
        <div class="wrap">
        
<!-- 当前位置 -->
<style>
	.topActive {color: #ff5200; text-decoration: underline;}
</style>
<script>
	$(function () {
	
		//收藏功能
		//默认调用ajax查看是否已经收藏
		
		$.ajax({
			type : "post",
			dataType : "jsonp",
			url : "http://second.py.fccs.com/sale/collectUpload.do?callback=?"
				+"&saleId=3270001&sortId=5",
			success : function(data){
				
				if(data.type==1){
					
					$(".collect").html("已收藏");
					$("#scImg").removeClass("i4");
					$("#scImg").addClass("ysc");
					$("#scImg2").removeClass("i1");
					$("#scImg2").addClass("ysc");
				}
			},
			error:function(data){
				
			}
		});
		
		$(".collect, #scImg").hover(function(){
			
			$(".collect").addClass("topActive");
			if($(".collect").html() == "已收藏"){
				
				$(".collect").html("取消收藏");
			}
		},function(){
			
			$(".collect").removeClass("topActive");
			if($(".collect").html() == "取消收藏"){
				
				$(".collect").html("已收藏");
			}
		});
		
		$("#appraiseTopPic").hover(function () {
			
			$("#appraiseTop").css({"color": "#ff5200", "text-decoration": "underline"});
		},function () {
		
			$("#appraiseTop").removeAttr("style");
		});
		
		$("#groupBuyTopPic").hover(function () {
			
			$("#groupBuyTop").css({"color": "#ff5200", "text-decoration": "underline"});
		},function () {
		
			$("#groupBuyTop").removeAttr("style");
		});
		
		//点击   取消或者收藏
		$(".collect, #scImg").click(function(){
			
			collectFloor();
		});
	});
	
	function collectFloor () {
		
		$.ajax({
			type : "post",
			dataType : "jsonp",
			url : "http://second.py.fccs.com/sale/collect.do?callback=?"
				+"&saleId=3270001"
				+"&city=414"
				+"&cityTable=A"
				+"&urladdress=" + window.location.href
				+"&title="+	encodeURIComponent("香榭里")
				+"&sortId=5",
			success : function(data){
				if(data.type==0){
					window.location.href='http://member.fccs.com/login.jhtml?forwardUrl='+encodeURIComponent(window.location);
				}
				if(data.type==1){
					$(".collect").html("已收藏");
					
					$("#scImg").removeClass("i4");
					$("#scImg").addClass("ysc");
					
					$("#scImg2").removeClass("i1");
					$("#scImg2").addClass("ysc");
				}
				if(data.type==2){
					$(".collect").html("收藏");
					
					$("#scImg").removeClass("ysc");
					$("#scImg").addClass("i4");
					
					$("#scImg2").removeClass("ysc");
					$("#scImg2").addClass("i1");
				}
			},
			error:function(data){
			}
		});
	}
</script>
