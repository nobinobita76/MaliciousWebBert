						}
						$('#globalNavLogUl').html(html);
					} 
					else { 
						$.post('/remoteUserService/login.do?nd=' + new Date().getTime(), function(data) {
							if(data=='1') {
								$.post('/newIndex/glb/cookieCheck.do?nd=' + new Date().getTime(), function(data) {
									if(data.indexOf(':') > -1) {
									
