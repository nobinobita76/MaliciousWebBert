<script>
	<!-- 免费通话js -->
	$(function () {
	
		//获取验证码
		changeVerificationCode();
		
		//换验证码
		$('.btnNewVerify').click(function() {
			
			changeVerificationCode();
		});
		
		//看房专车界面的关闭按钮操作
		$("#kanfangClose").click(
			function(){
				
				$(this).parents(".kfzc_y").addClass("dn");
				$("#kanfangTitle").html('看房专车 <span class="f16">第一时间联系您安排看房！</span>');
			}
 		);
	});

	function changeVerificationCode() {
	
		var time = new Date().getTime();
		$('.verifyImg').attr("src", "http://newhouse.py.fccs.com/newhouse/floor/getNewVerify.do?date=" + time + "&type=2578cbf872b480c9ae8774c82a4bfb5f");
	}
	
	function IsNum(e) {
	
		var k = window.event ? e.keyCode : e.which;
		if (((k >= 48) && (k <= 57)) || k == 8 || k == 0) {
		
		} else {
		
			if (window.event) {
				window.event.returnValue = false;
			} else {
				e.preventDefault(); //for firefox 
			}
		}
	}
	
	function changeNum(strId, strVal) {
	
		document.getElementById(strId).value = strVal.replace(/\D/gi, "");
	}
	
	//免费通话，表单提交
	function callPhone(phoneId, validateId) {
		
		$(".btn_style_y1").attr("disabled", true);
		var phone = document.getElementById(phoneId).value;
		if((validateId.indexOf("mobileValidate") != -1 && isMobil(phone)) || (validateId.indexOf("telValidate") != -1 && isPhone(phone))) {
			
			var validate = document.getElementById(validateId).value;
		    
		    $.ajax({
		    
		    	url:"http://newhouse.py.fccs.com/newhouse/floor/callPhone.do",
		    	type: "get",
		        data:{
		        	"phone": phone,
		        	"floorId": 3273681,
		        	"veriCode": validate
		        },
		    	dataType:'jsonp',  
		        jsonp:'callback',
		        success:function(result) {
		        
		        	if(result.flag == 1) {
		        		
		    			alert("申请提交成功！请保持电话畅通！");
		    			document.getElementById(validateId).value = "";
		    			changeVerificationCode();
			        } else {
			        
			        	alert(result.msg);
			        }
			        
			        $(".btn_style_y1").attr("disabled", false);
		        },
		        error:function() {
		        
		        	alert("申请提交出现问题，请稍后重新提交");
		        	$(".btn_style_y1").attr("disabled", false);
		        }
		    });
		} else {
		
			alert("请输入正确的联系号码！");
			$(".btn_style_y1").attr("disabled", false);
		}
	}
	
	function isMobil(s){ 
	
		var patrn = /(^0{0,1}1[3|4|5|6|7|8|9][0-9]{9}$)/; 
		if (!patrn.exec(s)){ 
			return false; 
		} 
		return true; 
	}
	
	function isPhone(s){ 
		var patrn = /^(([0\+]\d{2,3})?(0\d{2,3}))?(\d{7,8})((\d{3,}))?$/; 
		if (!patrn.exec(s)){ 
			return false; 
		} 
		return true; 
	}
	
	<!-- 看房专车js -->
	function kanfangSubmit (nameId, phoneId, explainId, btnId) {
		
		$("#" + btnId).attr("disabled", true);
		
		var name = $.trim($("#" + nameId).val());
		var phone = $.trim($("#" + phoneId).val());
		var explain = $("#" + explainId).val();
		var etoken = $("#kanfangToken").val();
		
		if (name == "" || !isChineseName(name)) {
			
			alert("请输入您的真实姓名！");
			$("#" + nameId).focus();
			$("#" + btnId).attr("disabled", false);
			return ;
		}
		
		if (phone == "" || !isMobil(phone)) {
		
			alert("请输入您的手机号码！");
			$("#" + phoneId).focus();
			$("#" + btnId).attr("disabled", false);
			return ;
		}
		
		if (explain == "请输入你的购房需求") {
		
			explain = "";
		}
		
		$.ajax({
		    
	    	url:"http://newhouse.py.fccs.com/newhouse/floor/houseCar.do?seckey=HaLaRuYa&nd=" + new Date().getTime(),
	    	type: "post",
	        data:{
	        	
	        	issueId : 3270001,
	        	name : escape(name),
	        	phone : phone,
	        	explain : escape(explain),
	        	etoken : etoken
	        },
	    	dataType:'jsonp',  
	        jsonp:'callback',
	        success:function(result) {
	        	
	        	if (result) {
        			
        			alert (result.msg);
        			
        			if (nameId == "kanfangName") {
        			
	        			$("#kanfangClose").click();
        			}

        			$("#" + nameId).val("");
        			$("#" + phoneId).val("");
        			$("#" + explainId).val("");

        			if (nameId == "kanfangName2") {
						
						$("#" + nameId).focus();
						$("#" + nameId).blur();
	        			$("#" + phoneId).focus();
	        			$("#" + phoneId).blur();
	        			$("#" + explainId).focus();
	        			$("#" + explainId).blur();
        			}
	        	}
	        	else {
	        		
	        		alert ("申请提交出现问题，请稍后重新提交");
	        	}
	        	
	        	$("#" + btnId).attr("disabled", false);
	        },
	        error:function() {
	        
	        	alert("申请提交出现问题，请稍后重新提交");
	        	$("#" + btnId).attr("disabled", false);
	        }
	    });
	}
	
	function isChineseName(name) {
	
		if((/[\u4e00-\u9fa5]+/).test(name)) {
		
			return name.length >= 2 ? (name.length <= 4 ? true : false) : false;
		} else {
		
			return false;
		}
	}
	
	//获取优惠界面显示
	function showBenefit () {
		
		$(".mfkfzc").removeClass("dn");
		$("#kanfangTitle").html("我要优惠");
	}
	
	$(function () {
	
		//搜索框楼盘下拉列表
		var keyword = "";
		$("#keyword").autocomplete("http://py.fccs.com/newhouse/search/floorList.do", {
			extraParams : {
				keyword : function() {
					
					keyword = $.trim($("#keyword").val());
					return keyword;
				}
			},
	
			parse : function(data) {
				
				var parsed = [];
				var rows = data;
	
				for (var i = 0; i < rows.length; i++) {
					var row = rows[i];
					if (row) {
						parsed[parsed.length] = {
							value : row.floor,
							data : row,
							result : row.floor
						};
					}
				}
				return parsed;
			},
	
			formatItem : function(row) {
				return row.floor;
			},
	
			scrollHeight : 480,
			width : 393,
			left : -25,
			dataType:'jsonp',  
        	jsonp:'callback'
	
		}).result(function(event, data, formatted) {
			
			var url = data.url;
			if (url.indexOf("http://") == -1) {
				
				url = "http://py.fccs.com" + url;
			}
			
			$("#keyword").val(keyword);
			window.open(url);
		});
		
		//搜索框搜索
		$("#keywordSubmit").click(function() {
			
			var text = $.trim($("#keyword").val());
			
			if (text != "" && text != "请输入楼盘名称、地址、开发商或学校") {
				
				window.open("http://py.fccs.com/newhouse/search/p1_k" + escape(escape(text)) + ".html");
			}
		});
	});
</script>
