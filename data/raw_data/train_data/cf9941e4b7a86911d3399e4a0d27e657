       <script type="text/javascript">
	//下拉框与文本框切换
	function num_change(a)
	{
		move_flag=3;
		if(document.getElementById(a).value==-1)
		{
			document.getElementById(a).style.display="none";
			document.getElementById(a+"_text").style.display="";
			document.getElementById(a+"_text").focus()
		}
		else
		{
			document.getElementById(a).style.display="";
			document.getElementById(a+"_text").style.display="none"
		}
	}
</script> 
