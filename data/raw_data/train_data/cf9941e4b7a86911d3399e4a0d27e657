						function showCalendar(lid,date){
							$.ajax({
								cache:false,
								url:'/abcalendar/get_calendar/?lid='+lid+'&date='+date,
								success:function(data){
									$('#cal_show').html(data);
									
								}
							})
						}
						function next_cal(date){
							showCalendar(279,date);
						}
						function past_cal(date){
							showCalendar(279,date);
						}
						showCalendar(279,'');
						
						function hideDeparture(a){
							document.getElementById(pub_price_info).style.display="none"
						}
						function findPosX(a){
							var b=0;
							if(a&&a.offsetParent){
								while(a.offsetParent){
									b+=a.offsetLeft;a=a.offsetParent
								}
							}else{
								if(a&&a.x){
									b+=a.x
								}
							}
							return b
						}
						function findPosY(b){
							var a=0;
							if(b&&b.offsetParent){
								while(b.offsetParent){
									a+=b.offsetTop;b=b.offsetParent
							}
							}else{
								if(b&&b.y){
									a+=b.y
								}
							}
							return a
						}
						function showOtherpriceleft(b,g,e,h,a){
							
							$.ajax({
								cache:false,
								url:'/abcalendar/line_order/?ltime='+b+'&lid='+279,
								success:function(data){
									var g=data;
									g=g.replace(/form_order/,"form_order_cal");
									g=g.replace(/<select/i,'<div style="border: 1px solid #FFC267; padding: 2px; background: rgb(255, 255, 255); float: right;"><img  src="/templates/front/default//images/cross.gif" alt="关闭" style="cursor: pointer;" onclick="hideDeparture(20);"></div><select');
									g=g.replace(/<div class="order_div">/i,'<div class="order_div" style="margin:0">');
									g=g.replace(/<div class=order_div>/i,'<div class="order_div" style="margin:0">');
									g=g.replace(/relative/g,"");
									g=g.replace(/route_order_bn/g,"route_order_b1n"+e);
									document.getElementById("frame_show").innerHTML=g;
									pub_price_span="other_price_span_left"+e;
									pub_price_info="form_order_cal";
									var d=document.getElementById(pub_price_span);
									var b=findPosX(d)-100;
									var a=findPosY(d)-650;
									if(b>260) b=260;
									if(b<0) b=0;
									document.getElementById("frame_show").style.left=b+"px";
									document.getElementById("frame_show").style.top=a+"px";
									document.getElementById("frame_show").style.position="absolute";
									document.getElementById("frame_show").style.display="block"
								}
							});
						}
