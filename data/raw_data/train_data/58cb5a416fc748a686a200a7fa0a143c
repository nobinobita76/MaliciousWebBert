jQuery(document).ready(function () {

	$("a[name='miurl']").each(function() {
	    var href = $(this).attr("href");
	    var filename = $(this).attr("filename");
	    href = "http://d.miwifi.com/d2r/?url=" + Base64.encodeURI(href)+ "&name="+filename
	    $(this).attr("href",href)
	})

	$("img[ectype='zmdownload']").click(function(){
		var engname = $(this).attr('name');
    	       $(this).attr('src','/Application/Home/View/Public/static/images/loading.gif');
		$.ajax({
		            url:"/index.php/zimu/index.html",
		            data:"engname="+engname,
		            type:'post',
		            cache:false,
		           	success:function(data){
								$("img[ectype='zmdownload']").attr('src','/Application/Home/View/Public/static/images/mi.png');
								if(data){
									var href = "http://d.miwifi.com/d2r/?url=" + Base64.encodeURI(data)+ "&name="+engname
									//window.open(href, "_blank");
									//window.location.href = href;
									var openLink = $("#zimulink");
									openLink.attr('href', href);
									openLink[0].click();
								}else{
									alert("字幕文件暂不存在!");
								}
		           }
		});
  	})

	$("img[ectype='downnode']").click(function(){
		var seedid = $(this).attr('name');
		$.ajax({
		            url:"/index.php/seed/ajaxclick.html",
		            data:"seedid="+seedid,
		            type:'post',
		            cache:false,
		           /* success:function(data){
						alert(data);
		            }*/
		});
   })

	$("img[ectype='clicknode']").click(function(){
		var seedid = $(this).attr('name');
		var nodetype = $(this).attr('nodetype');
        var w = window.open();		
		$.ajax({
		            url:"/index.php/seed/nodeclick.html",
		            data:"seedid="+seedid+"&nodetype="+nodetype,
		            type:'post',
		            cache:false,
                    success:function(data){
                      	w.location = data;
                    },
                    error: function(){
                         w.close();
                    }
		}); 
  	});
   
});
