function ElementPosition(element){var actualLeft=element.offsetLeft;var actualTop=element.offsetTop;var current=element.offsetParent;while(current!==null){actualLeft+=current.offsetLeft;actualTop+=current.offsetTop;current=current.offsetParent}var position=new Array(actualLeft,actualTop);return position}



	document.domain = "cheshi.com";
	var uid = glob_userid;
	var id = 713518;
	var type ='ask';
	var box = new LightBox("collectBox");

	 cheshi.ready(function() {
			window.setTimeout(function(){
        		cheshi.show(cheshi.$('collect'));
				get_collect();
            },2000);	
	 });

					//获取是否收藏信息 
	                function get_collect(){               
	                	cheshi.getJSON('http://my.cheshi.com/user/ajax/ajaxCollect.php?act=select&id='+id+'&type='+type,function(data){
							if(data > 0){
								cheshi.$('favid_collect').value = data;
								cheshi.show(cheshi.$('yes_collect'));
								cheshi.hide(cheshi.$('no_collect'));
							}else{
	    						cheshi.show(cheshi.$('no_collect'));
								cheshi.hide(cheshi.$('yes_collect'));	
	    					}
	    				}); 

	                	cheshi.$('yes_collect').href = "javascript:del_collect()";
	                	cheshi.$('no_collect').href = "javascript:add_collect()";
	                }

					//添加收藏 
					function add_collect(){
						if(!uid){
							login_box();
								
						}else{
							cheshi.getJSON("http://my.cheshi.com/user/ajax/ajaxCollect.php?act=add&id="+id+"&type="+type,function(data){
								if(data == -2){
									login_box();
								}else if(data > 0){
									cheshi.$('favid_collect').value = data;
									cheshi.show(cheshi.$('success_collect'));
		    						cheshi.hide(cheshi.$('no_collect'));
									cheshi.show(cheshi.$('yes_collect'));
									window.setTimeout(function(){
										cheshi.hide(cheshi.$('success_collect'));
										},2000); 	
		    					}
		    				}); 

						}
					} 
					
					//取消收藏 
					function del_collect(){
						 var id = cheshi.$('favid_collect').value;
						if(id){
							cheshi.getJSON("http://my.cheshi.com/user/ajax/ajaxCollect.php?act=del&id="+id+"&type="+type,function(data){
								if(data > 0){
									cheshi.$('favid_collect').value = 0;
									cheshi.show(cheshi.$('bad_collect'));
		    						cheshi.show(cheshi.$('no_collect'));
									cheshi.hide(cheshi.$('yes_collect'));
									window.setTimeout(function(){
										cheshi.hide(cheshi.$('bad_collect'));
										},2000); 
		    					}
		    				}); 
							
						}
					}

					//登陆框 
					function login_box(){
						  document.domain = "cheshi.com";
						  
						  box.Fixed = true;
						  box.Center = true;
						  box.Show();						
					}

					//登陆成功 		
					function loginOk(){
					    parent.box.Close();
						parent.window.location.reload();
					}

</script>
<script>

function logout(){var a=location.href;location.href="http://service.cheshi.com/user/logout.php?backurl="+a}
function login(){var a=location.href;location.href="http://service.cheshi.com/user/login.php?backurl="+a}
var search_text=document.getElementById("search_k");
var search_botton=document.getElementById("btn_search");
function trim_str(a){return a.replace(/^\s+|\s+$/,"")}
function go_search(){
	var a=trim_str(search_text.value);
	if(a==""){
		a.value = "请输入关键字...";
		return false;
	}
	if(a=="请输入关键字..."){
		return false;
	}
	var b="http://search.cheshi.com/ask.php?q="+encodeURIComponent(a);
	window.open(b);return false
}
function key_enter(b){var a=window.event?b.keyCode:b.which;if(a==13){go_search();return false}}
search_text.onfocus=function(){if(trim_str(this.value)=="请输入关键字..."){search_text.value=""}};
search_text.onblur=function(){if(trim_str(this.value)==""){search_text.value="请输入关键字..."}};
search_botton.onclick=function(){go_search()};

</script>
