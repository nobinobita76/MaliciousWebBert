					var x = 0;
					var Index = 0;
					var y = 0;
					$.ajax({
						url:"http://bbs.tiexue.net/hotthread_1.html",
						dateType:"json",
						asyns:false,
						success:function(data){
							data = JSON.parse(data);
							var html = "";
							html='<ul class="newsList">'
							for(var j=0;j<10;j++)
							{		
								var url1=data[x].URL.replace(/\/touch\/thread/,'http://bbs.tiexue.net/post2');
								if(data[x].Imgs.length==1)
								{
									Index++;
									html+='<li class="onePic"><a href="'+url1+'" target="_black" onclick="fnMonitor(15005601,ThreadID)"><img src="'+data[x].Imgs[0].replace(/itiexue.net\//g,'itiexue.net/middle/')+'"/> <h3>'+decodeURIComponent(data[x].Title)+'</h3><p> <span class="time">'+data[x].DateCreated+'</span> <span class="replyNum">'+data[x].TotalReplies+'</span></p> </a></li>';
