            <script>
                $("#postHead").load('http://bbs.tiexue.net/post_head.htm ,#topBar',function(){
				    //登录框状态
				    $(".form-list input").each(function () {
					    $(this).focus(function () {
						    $(this).css("opacity", 1);
						    $(this).parent().css("border", "solid 1px #ff7000");
					    }).blur(function () {
						    if (this.value == "") {
							    $(this).css("opacity", 0.5);
						    } else {
							    $(this).css("opacity", 1);
						    }
						    $(this).parent().css("border", "solid 1px #ddd");
					    }).triggerHandler("blur");
				    });
				    ResetUserLoginStatus();
			    });
            </script>


