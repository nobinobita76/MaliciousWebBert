 <!--右侧悬浮开始-->
 <script>
   $(function(){
		var screenH=$(window).height();			
		$(window).scroll(function(){
			var domH=$(document).height();
			var t=$(".adfixedBox").offset().top;
			var st=$(window).scrollTop();
			if(t>=st && t < (st+$(window).height())) 
			{				
				var top=t-st;
				if(t<0){t=0;}				
				$(".moveBox").css({"top":top}).show();
			}
			else
			{
				if(st>t)
				{
					if(st+$(window).height()>=domH-$(".footer").outerHeight())
					{$(".moveBox").css({"top":-230}).show();}
					else
					{$(".moveBox").css({"top":0}).show();}
				}					 
			}			
		})
   })
</script>
<!--右侧悬浮结束-->

