								}
								 
								x++;
							}
							html+="</ul>";				
							$("#bomNum0").append(html);	
							 
						},
						error:function(){
							console.log("请求失败");
						}
					})
				})
				$(function(){
					//滚动加载
					var num = 2;
					var i = 0,isGet=true;
					var adIndex=0; 
					var n = 0;
					$(window).on("scroll",getDataList);
					function getDataList()
					{						
						var bot = 160;
						var scrolltop = $(window).scrollTop();
						var scrollHeight = $(document).height();
						var windowHeight = $(window).height();						
						if (scrolltop >= scrollHeight - windowHeight-500) {
							scrolltop = $(window).scrollTop();
							scrollHeight = $(document).height();	
							if(isGet)
							{
							    if(num>=9){$(window).off("scroll",getDataList);}isGet=false;
								$.ajax({
									url:"http://bbs.tiexue.net/hotthread_"+num+".html",
									dateType:"json",
									async: false,
									success:function(data){	
										jsonData=data;	
										if(data==null||data==""){$(window).off("scroll",getDataList);return;}
										jsonData=JSON.parse(data);
										isGet=true;
										i=0;
										num++;
									},
									error:function(){
										console.log("请求失败");
									}
								})						
							}							
							var html='';
							html='<ul class="newsList">'
							if(jsonData==null||jsonData==""){return;}
							for(var j=0;j<10;j++)
							{		
								var url1=jsonData[i].URL.replace(/\/touch\/thread/,'http://bbs.tiexue.net/post2');
								if(jsonData[i].Imgs.length==1)
								{
									adIndex++;
									html+='<li class="onePic"><a href="'+url1+'" target="_black" onclick="fnMonitor(15005601,ThreadID)"><img src="'+jsonData[i].Imgs[0].replace(/itiexue.net\//g,'itiexue.net/middle/')+'"/> <h3>'+decodeURIComponent(jsonData[i].Title)+'</h3><p> <span class="time">'+jsonData[i].DateCreated+'</span> <span class="replyNum">'+jsonData[i].TotalReplies+'</span></p> </a></li>';
								}else if(jsonData[i].Imgs.length==3){
								adIndex++;	html+='<li class="threePic"><a href="'+url1+'" target="_black" onclick="fnMonitor(15005601,ThreadID)"><h3>'+decodeURIComponent(jsonData[i].Title)+'</h3><img src="'+jsonData[i].Imgs[0].replace(/itiexue.net\//g,'itiexue.net/middle/')+'"/><img src="'+jsonData[i].Imgs[1].replace(/itiexue.net\//g,'itiexue.net/middle/')+'"/><img class="last" src="'+jsonData[i].Imgs[2].replace(/itiexue.net\//g,'itiexue.net/middle/')+'"/><p> <span class="time">'+jsonData[i].DateCreated+'</span> <span class="replyNum">'+jsonData[i].TotalReplies+'</span></p> </a></li>';
								}
								 
								i++;
							}
							html+="</ul>";
							if(i==20){isGet=true;}					
							$("#bomNum0").append(html);	
							
							 

							return;
						}
					} 
				})
