        //通过id获取当前页面的flash
        function getFlash(movieName)
        {
            if (navigator.appName.indexOf("Microsoft") != -1)
            {
                return window[movieName];
            }
            else
            {
                return document[movieName];
            }
        }
	   //添加到收藏夹
		function addBookmark()
		{
			setTimeout(addBookmarkDelay,50);
		}
    
    
    	function addBookmarkDelay()
		{
			if (document.all){  
			try{  
				window.external.addFavorite("http://xiuxiu.web.meitu.com/?channel=bookmark","美图秀秀网页版");  
			}catch(e){  
				alert( "对不起！添加到收藏夹失败，请使用Ctrl+D进行添加！" );  
			}  
			  
			}else if (window.sidebar){  
				window.sidebar.addPanel("美图秀秀网页版", "http://xiuxiu.web.meitu.com/?channel=bookmark", "");  
			 }else{  
				alert( "对不起！添加到收藏夹失败，请使用Ctrl+D进行添加！" );  
			}  
		}
		
        window.onbeforeunload = onbeforeunload_handler;   
        window.onunload = onunload_handler;   
        var DispClose = true;
        function onbeforeunload_handler(){ 
            if(DispClose){
              var change = getFlash("Main").getPhotoChange();
               if(change)
                {
                    return "您辛苦编辑的照片还未保存呢！";
                }
                else
                {
                    return void (0);
                }
                DispClose=false;
            }
        }         
        function onunload_handler(){   
            if(DispClose){
                var change = getFlash("Main").getPhotoChange();
               if(change)
                {
                    return "您辛苦编辑的照片还未保存呢！";
                }
                else
                {
                    return void (0);
                }
                DispClose=false;
            }
        } 
        var xmlHttp;
        function S_xmlhttprequest() {
            if(window.ActiveXObject) {
                xmlHttp = new ActiveXObject('Microsoft.XMLHTTP');
            } else if(window.XMLHttpRequest) {
                xmlHttp = new XMLHttpRequest();
            }
        }
        function submitdata(data){
            S_xmlhttprequest();	
            xmlHttp.open("GET","/data.php?content="+data,true)
            xmlHttp.send(null);
            return true;
        }
		function closeWindow(url)
        {
			parent.location.href=url;	
        }	
