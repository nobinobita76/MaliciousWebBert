      function CajReadXlst(filename, page, tmpYear)
      {
          var murl = 'http://mall.cnki.net/onlineread/Mall/MallIndex?sourceid=10&fName={0}*{1}*{2}';
          if (page.indexOf("+") > 0)
              page = page.substr(0, page.indexOf("+"));
          OpenCnkiViewPage(String.Format(murl, filename, page, ""), tmpYear);
      }

      function GetFreeMagezine(uid, url, subuser)
      {
          if (uid == "")
          {
              alert("您还未登录，请先登录！");
          }
          else if (subuser == 1)
          {
              alert("请您登录主账户领取该杂志！");
          }
          else
          {
              window.location.href = url;
          }
          return false;
      }
