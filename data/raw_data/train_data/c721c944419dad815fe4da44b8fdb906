function is_ipad(url){
   var url_r = url;
       var arr = new Array(); 
        arr = url_r.split("output").reverse();
      var url_ok=arr[0];
      url_ok = url_ok.replace('_800k.flv', '_500k.mp4');
var ipadUrl= "http://videostream.chinadaily.com.cn:1935/app_1/_definst_/mp4:ugc"+url_ok+"mp4/playlist.m3u8";
//var androidUrl = "rtsp://videostream.chinadaily.com.cn:1935/app_1/_definst_/mp4:ugc"+url_ok+"mp4";
var androidUrl = "http://videostream.chinadaily.com.cn:1935/app_1/_definst_/mp4:ugc"+url_ok+"mp4/playlist.m3u8";
var winphoneUrl = "http://videostream.chinadaily.com.cn:1935/app_1/_definst_/mp4:ugc"+url_ok+"mp4/Manifest";
if((navigator.userAgent.match(/iPhone/i)) || (navigator.userAgent.match(/iPad/i))) {
//alert(ipadUrl);
$("#play-logo").hide();
$("#openclose").hide();
$("#player").html('<video width="100%" height="100%" controls="controls"><source src="'+ipadUrl+'" type="video/mp4"></video>');
return false; 
}else if((navigator.userAgent.match(/Android/i))){ 
$("#play-logo").hide();
$("#openclose").hide();
$("#player").html('<video width="100%" height="100%" controls="controls"><source src="'+androidUrl+'" type="video/mp4"></video>');
//$("#player").html('<a href="'+androidUrl+'"><image src="http://uploadmedia.chinadaily.com.cn/2016/0513/1463119074286.jpg" width="640" height="480" /></a>');
return false;
}else if((navigator.userAgent.match(/Windows Phone/i))){ 
$("#play-logo").hide();
$("#openclose").hide();
return false;
}
return true;
 
}	
//开关灯
function op_clo_light(){

	open_close_deng('chtop-bottom,mainNav,mainr-ch,chfooter,comment');
	 $(".openlit").show();
	 $(".closelit").hide();
} 
