 <script type="text/javascript">
     $().ready(function () {
         $(window).scroll(function () {
             var t = $("#js-conditionbox");
             $("#js_conditionParent").height($("#js_conditionParent").height());
             $(window).scrollTop() >= $('.list-wom').position().top ? (t.addClass("wom-sift-fixed"), $('#js_loading').removeClass('fn-hide')) : reset();
         })
         function reset() {
             'use strict';
             var wrap = $('.wom-sift');
             var options = $('.wom-sift .options');
             wrap.removeClass("wom-sift-fixed wom-sift-all");
             options.find('.icon-arrow-css').attr('class', 'icon-arrow-css arrow-top');
             $('.wom-sift').off('touchmove');
             //$('#js_conditionParent .icon-load').addClass('fn-hide');
             $('#js_loading').addClass('fn-hide');
             $('body').css({
                 height: '',
                 overflow: ''
             });
         }
         $('.wom-sift-first .updown').on('click', function () {
             'use strict';
             var el = $(this);
             el.parent('.wom-sift-first').toggleClass('fn-hide').siblings('.wom-sift-first').toggleClass('fn-hide');
             $("#js_conditionParent").height($(".wom-sift-con").height());
         })
         $('.wom-sift-con span').on('click', function () {
             'use strict';
             if ($(this).hasClass('slected') || $(this).hasClass('updown')) {
                 return false;
             } else {
                 if ($(this).parent().hasClass('wom-sift-first')) {
                     $('.wom-sift-first span').removeClass('selected');
                     $(this).parent('.wom-sift-first').siblings('.wom-sift-first').find('span').eq($(this).index()).addClass('selected');

                     //如需刷新二级选项，代码写这里
                     $('.wom-sift-secondary').hide();
                     $('#second' + $(this).attr('group')).show();
                     $("#js_conditionParent").height($(".wom-sift-con").height());
                     if ($('.wom-sift-all').length > 0 && $("#second" + $(this).attr("group")).length == 0) {
                         $("#js_conditionParent").height($(".wom-sift-con").height() - 15);
                         $(".options a[class=updown]").click();
                         $('body').scrollTop($('.list-wom').position().top);
                     }

                     //初始化选中项目,保证选中的二级印象词与命中段落保持一致
                     var group = $(this).attr("group");
                     if ($('.wom-sift-all').length == 0 || window.currentValue != $('#second' + group + ' .selected').attr('Value')) {
                         $('#second' + group + ' .selected').removeClass('selected');
                         $('#second' + group + ' span:first-child').addClass('selected');
                     }

                     //点击遮罩层的一级类目，增加点击PV签
                     try {
                         var group = $(this).attr("group"),
                             seriesid = requestobejct.isSeries ? requestobejct.Id : requestobejct.serialId,
                             specid = requestobejct.isSeries ? "0" : requestobejct.Id,
                             skey = "0",
                             a = (new Date).valueOf();
                         var eid = "";
                         if (requestobejct.isSeries && requestobejct.yearId == 0) {
                             eid = "2|1211030|366|0|203262|300728";
                         }
                         else if (requestobejct.isSeries && requestobejct.yearId > 0) {
                             eid = "2|1211030|367|0|203263|300729";
                         }
                         else {
                             eid = "2|1211030|368|0|203264|300730"
                         }
                         1 == _trackEvent.push({
                             eid: eid,
                             val: 1,
                             a1: skey,
                             a2: seriesid,
                             a3: specid,
                             a4: group
                         });
                     } catch (i) { }
                 }
                 else {
                     $('.options .selected').removeClass('first');
                     if ($(this).index() == 0 && $(this).children().length == 0) { $('.options .selected').addClass('first'); };
                     //如果只有二级筛选，筛选结果操作代码写这里
                     if ($('.wom-sift-all').length > 0) {
                         $(".options a[class=updown]").click();
                         $('body').scrollTop($('.list-wom').position().top);
                     }
                 }
                 $(this).addClass('selected').siblings('span').removeClass('selected');
                 if ($('.wom-sift-all').length == 0 || $("#second" + $(this).attr("group")).length == 0) {
                     if ($(this).html() == '全部') {
                         $('.options .selected').html($(this).parent().attr('data_forgropn'));

                         //点击全部，增加点击PV签
                         try {
                             var group = $(this).parent().attr("data_forgrop"),
                                 seriesid = requestobejct.isSeries ? requestobejct.Id : requestobejct.serialId,
                                 specid = requestobejct.isSeries ? "0" : requestobejct.Id,
                                 skey = "0",
                                 a = (new Date).valueOf();
                             var eid = "";
                             if (requestobejct.isSeries && requestobejct.yearId == 0) {
                                 eid = "2|1211030|366|0|203262|300728";
                             }
                             else if (requestobejct.isSeries && requestobejct.yearId > 0) {
                                 eid = "2|1211030|367|0|203263|300729";
                             }
                             else {
                                 eid = "2|1211030|368|0|203264|300730"
                             }
                             1 == _trackEvent.push({
                                 eid: eid,
                                 val: 1,
                                 a1: skey,
                                 a2: seriesid,
                                 a3: specid,
                                 a4: group
                             });
                         } catch (i) {
                         }
                     }
                     else {
                         $('.options .selected').html($(this).html());
                     }
                     //控制浮动框的样式：一级类目和二级类目不一致，有浮层的和无浮层的不一致
                     if ($(this).attr('group') != null || $(this).html() == '全部') {
                         $('.options .selected').addClass('first');
                     }
                 }
                 //如果一级二级都筛选，筛选结果操作代码写这里
             }
         })
         $('.options .updown').on('click', function () {
             'use strict';
             var el = $(this);
             if (el.find('.icon-arrow-css').hasClass('arrow-top')) {
                 el.find('.icon-arrow-css').attr('class', 'icon-arrow-css arrow-bottom');
                 $('.wom-sift .mask').css({
                     'height': Math.max(document.documentElement.clientHeight, $('body').height()) + 'px'
                 })
                 $('body').css({
                     height: document.documentElement.clientHeight,
                     overflow: 'hidden'
                 });
                 $('.wom-sift').on('touchmove', function (e) {
                     e.preventDefault();
                     e.stopPropagation();
                 });
             } else {
                 el.find('.icon-arrow-css').attr('class', 'icon-arrow-css arrow-top');
                 $('body').css({
                     height: '',
                     overflow: ''
                 });
                 if ($("#second" + $('.wom-sift-first .selected').attr("group")).length == 0) {
                     $("#js_conditionParent").height($(".wom-sift-con").height() - 15);
                 } else {
                     $("#js_conditionParent").height($(".wom-sift-con").height());
                 }

                 $('.wom-sift').off('touchmove');
             }
             $('.wom-sift-first').eq(0).addClass('fn-hide');
             $('.wom-sift-first').eq(1).removeClass('fn-hide');
             $('.wom-sift').toggleClass('wom-sift-all');
         })
         $('.wom-sift .mask').on('click', function () {
             'use strict';
             $('.options .icon-arrow-css').attr('class', 'icon-arrow-css arrow-top');
             $('.wom-sift').removeClass('wom-sift-all');
             $('.wom-sift').off('touchmove');
             $('body').css({
                 height: '',
                 overflow: ''
             });
         });
     })
</script>



