<script type="text/javascript">!function(){    (function () {
        var currentOverlay, currentButton;

        $('.button-area a').on('click', function (e) {
            if(!$(this).hasClass('btn-category')) {
                return true;
            }

            e.preventDefault();

            var $thisButton = $(this),
                $thisOverlay = $($thisButton.attr('data-associated-overlay'));

            if (currentOverlay && currentOverlay[0] != $thisOverlay[0]) {
                currentOverlay.hide();
            }

            if (currentButton && currentButton[0] != $thisButton[0]) {
                currentButton.removeClass('ui-state-active');
            }

            if ($thisButton.hasClass('ui-state-active')) {
                $thisButton.removeClass('ui-state-active');
                $thisOverlay.hide();
            } else {
                $thisButton.addClass('ui-state-active');
                $thisOverlay.show();
                currentButton = $thisButton;
                currentOverlay = $thisOverlay;
            }
        });

        // 点击背景直接关闭弹层（曹宇添加）
        $('.nav-overlay').click(function(e) {
            e.stopPropagation();
            // console.log($(e.target).hasClass('nav-overlay'));
            if($(e.target).hasClass('nav-overlay')) {
                currentButton.removeClass('ui-state-active');
                currentButton = null;
                currentOverlay.hide();
                currentOverlay = null;
            }
        });
    }());
    }();
!function(){        var resizeEventName = 'onorientationchange' in window ? 'orientationchange' : 'resize';
        //mainTabs = new $.hao123.Tabs('#main_tabs');

        (function () {
            // 生成剧集列表
            var episodes = window._episodes,
                    episodeCount = episodes.length,
                    pageSize = 100,
                    pageCount = Math.ceil(episodeCount / pageSize),
                    scrollContainer = $('#episode_list .js-iscroll ul').hide(),
                    episodeContainer = $('#episode_list .js-episodes'),
                    scroll,
                    timer,
                    activeItem,
                    finish = window._finish,
                    pages = [];

            function _drawEpisodes(pageNum) {
                var from = pageNum * pageSize,
                        to = Math.min(from + pageSize, episodeCount),
                        episode,
                        html;

                episodeContainer.hide();
                episodeContainer.empty();
                for (; from < to; from++) {
                    episode = episodes[from];
                    html = '<li><a href="' + episode.ori_link + '">' + episode.episode + '</a></li>';
                    if (finish === 0) {
                        episodeContainer.prepend(html);
                    } else {
                        episodeContainer.append(html);
                    }
                }
                episodeContainer.show();
            }

            for (var i = 0; i < pageCount; i++) {
                var page = $('<li></li>').attr('data-page-num', i);

                if (i + 1 === pageCount) {
                    page.html(i * pageSize + 1 + '-' + episodeCount);
                } else {
                    page.html(i * pageSize + 1 + '-' + (i + 1) * pageSize);
                }

                pages.push(page);
            }

            if (finish === 0) {
                pages = pages.reverse();
            }

            for (var i = 0, len = pages.length; i < len; i++) {
                if (i === 0) {
                    _drawEpisodes(pages[i].attr('data-page-num'));
                    activeItem = pages[i].addClass('ui-state-active');
                }
                scrollContainer.append(pages[i]);
            }


            scrollContainer.show();

            scrollContainer.delegate('li', 'click', function () {
                var pageNum = $(this).attr('data-page-num');

                _drawEpisodes(pageNum);
                activeItem.removeClass('ui-state-active');
                activeItem = $(this).addClass('ui-state-active');
            });

            setTimeout(function () {
                scroll = new IScroll('#episode_list .js-iscroll', {
                    scrollX: true,
                    scrollY: false,
                    eventPassthrough: true
                });
            }, 0);

            $(window).on(resizeEventName, function () {
                clearTimeout(timer);
                timer = setTimeout(function () {
                    scroll.refresh();
                }, 0);
            });

            // mainTabs.on('activate', function () {
            //     if (this.activeIndex === 0) {
            //         clearTimeout(timer);
            //         timer = setTimeout(function () {
            //             scroll.refresh();
            //         }, 0);
            //     }
            // });
        }());
        }();</script></body></html>