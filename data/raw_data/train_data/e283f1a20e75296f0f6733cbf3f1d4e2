<script type="text/javascript">
    window.addEvent('domready', function(){
        var p_c_pos =  $('photo_container').getPosition();
        var cursor_flag = true;
        $('photo_container').addEvents({
            'mouseover': function(e) {
                if (e.page.x > (p_c_pos.x + 400)) {
                    cursor_flag = false;
                }
            },
            'mousemove': function(e){
                if (e.page.x < (p_c_pos.x + 400)) {
                    if (cursor_flag) {
                        this.setStyle('cursor','url(/img/tugo/cursor_left.cur), auto');
                        cursor_flag = false;
                    }
                } else {
                    if (!cursor_flag) {
                        this.setStyle('cursor','url(/img/tugo/cursor_right.cur), auto');
                        cursor_flag = true;
                    }
                }
            },
            'click':function(e) {
                if (e.page.x < (p_c_pos.x + 400)) {
                    _gaq.push(['_trackPageview', '/click/photoview/next/']);
                    window.location.href ='http://v.yupoo.com/photos/chico88/965268113/';
                } else {
                    _gaq.push(['_trackPageview', '/click/photoview/prev/']);
                    window.location.href ='http://v.yupoo.com/photos/chico88/965268081/';
                }
            }
        });
    });

    window.addEvent('load', function () {
        $$('.PostContent').each(function(element) {
            var content = element.innerHTML;
            
            var regex = /&lt;blockquote&gt;/igm;
            var matchs = content.match(regex);
            if (matchs == null || matchs.length < 4) {
                content = content.replace(/&lt;blockquote&gt;/ig,"<blockquote>");
                content = content.replace(/&lt;\/blockquote&gt;/ig,"<\/blockquote>");
            } else {
                if (matchs.length < 10) {
                    content = content.replace(/&lt;blockquote&gt;/igm,"<blockquote style='margin:2px; padding:5px 0; text-indent:2em;'>");
                } else {
                    content = content.replace(/&lt;blockquote&gt;/igm,"<blockquote style='margin:0; padding:5px 0; text-indent:2em;'>");
                }
                content = content.replace(/&lt;\/blockquote&gt;/igm,"<\/blockquote>");
            }
            content = content.replace(/&lt;blockquote/igm,"<blockquote");
            content = content.replace(/&gt;/gim,">");
            
            element.innerHTML = content;
        });
    });
    function cite(id,who) {
        var content = $(id).innerHTML;
        content = content.replace(/<br>/ig,"\r\n");
        
        content = content.replace(/<img(.*?)>/ig,"<img$1\/>");
        $('comment_content').value += ('<blockquote>' + who + '说:\r\n' + content +'<\/blockquote>');
    }
</script>
