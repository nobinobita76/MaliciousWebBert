<script src="http://upzip.b0.upaiyun.com/website/jsonp.js"></script>
<script type="text/javascript">
    var username = '';
    var uri = '/show/';
    var go = false;
    var path = window.location.pathname;
    if (!path || path === '/') {
        var host = window.location.host;
        var matches = host.match(/([^\.]+)/);
        if (matches && matches.length) username = matches[0];
        uri += username;
        go = true;
    } else {
        if (path[path.length - 1] === '/') {
            path = path.substr(1, path.length - 2);
        }
        path = path.split('/');
        if (path.length > 2) {
            username = path[1];
            uri += username;
            var len = path.length;
            go = true;

            if (path[len - 1] === 'albums') { // 相册列表
                uri += '?tab=gallery';
            } else if (len > 3 && path[2] === 'albums') { // 相册详情
                uri += '/' + path[len - 1];
            } else if (len === 3 && Number(path[len - 1])) { // 图片页
                uri = path.join('/');
            } else if (path[len - 1] === 'contact') {
                go = false;
            }
        }
    }
    if (go && username) {
        var url = 'http://api.x.yupoo.com/migration/state';
        // JSONP.init({callbackName: 'redirect'});
        JSONP.get(url, {username: username}, function (response) {
            if(response.code === 0) {
               window.location.href = 'http://x.yupoo.com' + uri;
            }
        });
        
    }
</script>


