var ajaxQueue=function(data,fn){
  var run=function(list){
    var item=list[0];
    if(item){
      fn&&fn(item,function(){
        setTimeout(function(){
          run(list.slice(1));
        },10);
      });
    }
  }
  run(data);
};

define('init',function(require, exports, module){
  var $=jQuery,_=require('underscore');


  productData.sort(function(a,b){
    return a.LocationID-b.LocationID;
  });

  //$('#mainContainer').html(_.template($('#mainTemplate').html())({productData: _.filter(productData, function(item){return item.GroupNum === 1;})}));

  var mainTemplate = _.template($('#mainTemplate').html());
        $('#mainContainer').html(_.map(_.filter(productData, function(item) {
                return item.GroupNum == 1
            }), function(item) {
                return mainTemplate({
                    item: item
                });
            }).join(''));

  var $subContainer = $('#subContainer'),
      subTemplate = _.template($('#subTemplate').html())
      ;

  _.each($subContainer.children(), function(item, index){
    $(item).html(_.map(_.filter(productData, function(item){return item.GroupNum == index+2}), function(item){
        return subTemplate({item: item});
      }).join(''));
  })

  $('.pro_tab_box').on('click', 'li', function(){
    var $this =$(this),
        index = $this.index();
    $this.addClass('current').siblings().removeClass('current');
    $subContainer.children().hide().eq(index).show();
    $('.more_link').children().hide().eq(index).show();
  })


  $('#mainContainer').on('click', 'a', function(){
        var $a = $(this);
        if(!$a.is('.es_btn01_disabled,.link_disabled')){
            var href = $a.attr('data-href');
            window.open(href)
        }
    })

  var params=[];
    $(".es_main_push").each(function(){//考虑不会有相同的班期
      var $this =  $(this);
      var voyaid=$this.attr('data-voyaid'),sailingid=$this.attr('data-sailingid');
      if(voyaid&&sailingid){
        params.push({
          sailingid:sailingid,
          voyaid:voyaid
        });
      }
    });

    var refresh=function(param,data){
      var product=null,maxRebate=1;//筛选出最大折扣 意味着最小折率的 产品 并且有售卖房型
      var CategoryTypeList=data.CategoryTypeList||[];
      for (var i = CategoryTypeList.length-1; i >= 0; i--) {
        var CategoryType=CategoryTypeList[i],CategoryList=CategoryType.CategoryList||[];
        if(!CategoryList.length)continue;

        _.each(CategoryList,function(category){
          var price=category.Price,rebate=price.InCludeMinPrice/price.OriPrice;
          if(isNaN(rebate)||!isFinite(rebate)||rebate>maxRebate)return;
          else if(rebate==maxRebate&&product&&price.InCludeMinPrice>=product.minprice)return;


          maxRebate=rebate;
          product={
            typename:CategoryType.TypeName,
            rebate:rebate,
            oriprice:price.OriPrice,
            minprice:price.InCludeMinPrice
          }
        });
      };

      if(product){
        var showRebeat = Math.round(product.rebate*100)/10;
        var $item=$(".es_main_push[data-voyaid='"+param.voyaid+"'][data-sailingid='"+param.sailingid+"']");
        if(showRebeat < 10){
          $item.find(".es_sale_label").html(showRebeat +'折').show();
          $item.find(".es_del_price").html((product.typename||'')+'原价：'+Math.ceil(product.oriprice)).css({'color': '#999'});
          $item.find(".es_main_push_price_num").html(Math.ceil(product.minprice)).css({opacity:0}).animate({opacity:1},400);  
        }
      }
    }

    ajaxQueue(params,function(param,callback){

      $.ajax({ type:"post", url: '/Cruise-Booking-Online/CrystalProject/Booking/GetSailingDetailV3', dataType: "json", async: true,
        data:param,
        success: function (data) {
              if(data&&data.Ack==1){
                refresh(param,data.Data);
              }
              callback();
          },
          error: function () {
            callback();
          }
      });

    });
});

define('promotion',function(require, exports, module){
  var $=jQuery,_=require('underscore');

    //优惠券事件绑定
    $('.es_hongbao a').on('click', _.debounce(function() {
        self.checkPromotion('spring2017');
    }, 1000, true))

  var self={
    checkPromotion:function(id){
        self.showLoading();
        $.ajax({
          url:"/Cruise-Booking-Online/WebAPI/GetClientInfo",
          dataType:"jsonp",
          data:{"info":"VacationOrderCount"},
          success:function(data){
              self.hideLoading();
              try{
                data = typeof data === 'string' ? $.parseJSON(data) : data;
                if(!data.IsLogin){
                  __SSO_CALLBACK = function(){
                    self.getPromotion(id);    
                  }
                  return self._login();
                }
                if(id=='summerxkhzx'&&data.VacationOrderCount>0){
                  return self.showMessage("新客专享券，仅适用首单预订旅游产品的客户");
                }
                self.getPromotion(id);

              }
              catch(e){
                self.showMessage("优惠券领取异常");
              }
          },
          error: function(){
              self.hideLoading();
              self.showMessage('网络异常');
          }
        })
    },
    getPromotion:function(id){
        var self = this;
        self.showLoading();
        //领取优惠券ajax
        // Ack = 0 未登錄
        // Ack = 1 领取成功
        // Ack = 2 領取失敗
        $.ajax({
                url:'/Cruise-Booking-Online/Activity/qihubox/GetHaoliPromo?PromotionUniqueId=' + id,
                beforeSend: function(xhr){
                    xhr.setRequestHeader("If-Modified-Since","0");
                },
                success:function(data){
                    self.hideLoading();
                    data = typeof data === 'string' ? $.parseJSON(data) : data;

                    switch(data.Ack){
                        case 0:
                            self._login();
                            return;
                        case 2:
                            data.Data = '抱歉，您已领取过优惠券了';
                        default:
                            self.showMessage(data.Data)
                            break;
                    }
                },
                error: function(){
                    self.hideLoading();
                    self.showMessage('网络异常');
                }
            })
    },
    //登陆弹层
    _login:function(){
        __SSO_loginShow_1('login', true, '0', true);
    },
    showMessage: function(msg){
        cQuery('#message').mask().find('#text').html(msg);
    },
    closeMessage: function(){
        cQuery('#message').unmask();
    },
    showLoading: function(){
        cQuery('#loading').mask();
    },
    hideLoading: function(){
        cQuery('#loading').unmask();
    }
  };


    //优惠券事件绑定
  $('.hb_click').on('click', _.debounce(function(){
      var $this = $(this),
          id = $this.data('id');
      if(id)
        self.checkPromotion(id);
  }, 1000, true))

    $('.pop_rule_x').on('click', function() {
        self.closeMessage();
    });

    $('.btn_base01').on('click', function() {
        self.closeMessage();
    });
});

(function($){
  seajs.use('init');
  seajs.use('promotion');
  seajs.use("http://webresource.c-ctrip.com/ResCruiseOnline/R4/online/js/app/page/ui/sidebar_v2.js", function (main) {
    main.init({},'body');
    seajs.use("http://webresource.c-ctrip.com/ResCruiseOnline/R4/online/js/app/page/ui/fastbooking_v3.js", function (FastBooking) {
      FastBooking.init('#sidebar_fastbooking')
    });
  });

  $('img[data-src]').each(function(){
      var $this = $(this);
      $this.attr('src', $this.attr('data-src'));
  });


})(jQuery);


</script>


