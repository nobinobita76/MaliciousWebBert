<script type="text/javascript">
    var dimgConfig = {
        PreUrl: "https://images4.c-ctrip.com/target/",
        Replace: "https://dimg04.c-ctrip.com/images/"
    };

    function GetDimgMapSize(url, width, height) {
        if (!url || width < 0 || height < 0) return url;
        if (url.indexOf("https://dimg02.c-ctrip.com") > -1 || url.indexOf("https://dimg03.c-ctrip.com") > -1 || url.indexOf("https://dimg04.c-ctrip.com") > -1) {
            var sizeType = width + "_" + height;
            var r = url.lastIndexOf("_R_"),
                lastPoint = url.lastIndexOf(".");
            if (r < 0) r = lastPoint;
            return url.substring(0, r) + "_R_" + sizeType + url.substring(lastPoint, url.length);
        }
        if (url.indexOf(dimgConfig.PreUrl) < 0) return url;
        var lastPoint = url.lastIndexOf(".");
        if (lastPoint < 0) return url;
        return (url.substring(0, lastPoint) + "_R_" + width + "_" + height + url.substring(lastPoint, url.length)).replace(dimgConfig.PreUrl, dimgConfig.Replace);
    } 


function getStartDate(strDate){/*因为节点本身就已经是yyyy-MM-dd 格式了*/
        try{
          strDate=strDate.substring(0,10);
          return strDate.replace(/-/g,'.');

        }
        catch(e){
          return "";
        }
}
function getUrl(is6381, productid, departure, promotionCode, productSourceType) {
        /*这个正常情况下 不能为空*/
        if (is6381 && productid) {
            return "http://vacations.ctrip.com/cruise/p" + productid + "s1.html" + (departure ? ("#selectDate=" + departure) : "");
           
        } else if (productid){
           var isCruisePackage =  productSourceType == 2 ? true : false;
           var host =  isCruisePackage ? 'http://cruise.ctrip.com/p/' : 'http://cruise.ctrip.com/c/';
           return host + productid + ".html" + (promotionCode ? '?promotionCode=' + promotionCode + '&distributechannel=StaffOnly' : '') + (departure ? ("#departure=" + departure) : "");
        } 

        return "javascript:;";
    }

