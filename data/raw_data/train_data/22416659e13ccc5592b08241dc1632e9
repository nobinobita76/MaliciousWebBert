        cabins=_.map(_.groupBy(cabins,function(item){return item.SalesCode.Category.CategoryCode;}),function(groupList){
            return _.min(groupList,function(item){return item.PriceInfo.InCludeMinPrice;});
        });
        cabins = _.map(cabins, function(t){
