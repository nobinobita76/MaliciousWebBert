            var MGE = function (lab, act, isDelay) {

                if(typeof lab === 'string') {
                    lab = {
                        cpt: lab
                    }
                }

                lab.cat = window.eventCat;

                var pid = MT.pageData['pid'];
                if (pid > 200000) {pid -= 200000}

                var toReport = {
                    nm: 'mge',
                    val: {
                        act: act,
                        cid: 'firework_' + pid,
                        lab: lab,
                    }
                };

                if(act === 'show' || act === 'click') {
                    // 添加bid, 如果是deal和poi那么不上报deal 或者 poi 的 id, 这些动态信息在发布页面无法录入。
                    // deal和poi id逻辑应该另行安排。
                    if(lab.cpt) {
                        // 普通组件
                        toReport.val.bid = 'firework_' + global.eventId + '_' + lab.cpt;
                    }

                    //deal 和 poi
                    if(lab.dealid) {
                        toReport.val.bid = 'firework_' + global.eventId + '_deal';
                    }

                    if(lab.poiid) {
                        toReport.val.bid = 'firework_' + global.eventId + '_poi';
                    }
                }

                if(act === 'show') {
                    toReport.event_type = 'view';
                    toReport.val.bid += '_view';
                }

                if(act === 'click') {
                    toReport.event_type = 'click';
                    toReport.val.bid += '_click';
                }

                if (window.Analytics) {
                    Analytics('event', toReport, {
                        isDelay: !!isDelay,
                        delay: 5
                    });
                } else {
                    anaback.push([event,  toReport, {
                        isDelay: !!isDelay,
                        delay: 5
                    }]);
                }
                if (location.hostname == 'tpl.dianping.com' || location.hostname == 'tpl.51ping.com') {
                    _hip && _hip.push(['mv', {
                        module: lab,
                        action: act
                    }]);
                }
            }

