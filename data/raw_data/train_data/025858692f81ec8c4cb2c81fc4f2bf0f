<script>
    require(["zepto.js", 'urltransform.js'], function ($, MobileUrlManager) {
         $('.deallink').attr('data-stid', stid);
         $('.poilink').attr('data-ctpoi', stid);

        //绑定链接转换
        var envConfig = {
             autoScan: true,
             vars: {
                 citypinyin: $("body").data('citypinyin')
             },
             paramAttrPrefix: "data-ep-"
         };
         if (global.isMeituan && global.isIos){
             envConfig.hostName = "mt-iphone";
         } else if (global.isMeituan && global.isAndroid){
             envConfig.hostName = "mt-android";
         } else if (global.isDianping) {
             envConfig.hostName = "dp-app";
         } else if (global.isMaoyan) {
             envConfig.hostName = "my-app";
         } else {
             envConfig.hostName = "mt-h5";
         }
         var mobileUrlManager = new MobileUrlManager(envConfig);

        /**
         * 统一链接处理
         * @param e a标签点击事件
         * 1、拼接参数
         * 2、只处理http/https协议的链接
         * 3、微信端调起客户端，转化为综合链接打开
         * 4、客户端中，转化i版链接为客户端协议链接，其他的直接打开
         */
        function transformLink (e) {
            var curnode = e.currentTarget,
                chref = curnode.getAttribute('href');
            var gid = $(e.target).closest('[gid]').attr('gid');
            e.preventDefault();

            /**
             * 获取必要的参数，stid,ct_poi,ste,stid_ext
             */
            var extra = [];

            //stid
            if (curnode.getAttribute('data-stid') && chref.indexOf('stid') == -1) {
                extra.push('stid=' + curnode.getAttribute('data-stid'));
            }
            // ct_poi
            if (curnode.getAttribute('data-ctpoi') && chref.indexOf('ct_poi') == -1) {
                extra.push('ct_poi=' + curnode.getAttribute('data-ctpoi'));
            }
            // query.ste
            if (window.global && global.queryParam.ste && curnode.href.indexOf('ste') == -1) {
                extra.push('ste=' + global.queryParam.ste + 'p' + gid + 'q' + window.global.eventId);
            }

            // query.stid_ext
            if (window.global && global.queryParam.stid_ext && curnode.href.indexOf('stid_ext') == -1) {
                extra.push('stid_ext=' + global.queryParam.stid_ext + 'p' + gid + 'p' + window.global.eventId);
            }

            /**
             * 给链接拼参数
             * @param initHref
             * @returns {string}
             */
            function joinLink(initHref){
                var _href = '';
                if (extra.length == 0) {
                    _href = initHref;
                } else if (initHref.indexOf('#') == -1) {
                    _href = ~initHref.indexOf('?') ? initHref + '&' + extra.join('&') :initHref + '?' + extra.join('&');
                } else {
                    _href = initHref;
                }
                return _href;
            }

            //如果这个链接没有被转化过，做链接转换
            if (!$(curnode).attr("data-transform")) {
                var transformedHref = mobileUrlManager.transform(chref, curnode);
            }

            var targetHref = joinLink(transformedHref || chref);

            function openUrl() {

                if (global.isMeituan) {
                    if (targetHref.indexOf("http://") == 0 || targetHref.indexOf("https://") == 0) {
                        location.href = "imeituan://www.meituan.com/web?url=" + encodeURIComponent(targetHref);
                    } else {
                        location.href = targetHref;
                    }
                } else if (global.isDianping) {
                    if (targetHref.indexOf("http://") == 0 || targetHref.indexOf("https://") == 0) {
                        location.href = "dianping://web?url=" + encodeURIComponent(targetHref);
                    } else {
                        location.href = targetHref;
                    }
                } else {
                    location.href = targetHref;
                }
            }

            if ($(curnode).closest(".firwork-document").attr("data-invokeapp")) {
                if (window.global && window.global.isWeixin) {

                    var linkHandle = {
                        mtPage: function () {
                            if ($(curnode).attr("data-mthref")) {
                                var mtHref = joinLink($(curnode).attr("data-mthref"));
                            } else {
                                var mtHref = "imeituan://www.meituan.com/web?url=" + encodeURIComponent(targetHref);
                            }

                            location.href = 'http://tpl.dianping.com/firework/callApp?type=meituan&appurl='+ encodeURIComponent(mtHref) +'&weburl=' + encodeURIComponent(targetHref);
                        },
                        dpPage: function () {
                            if ($(curnode).attr("data-dphref")) {
                                var dpHref = joinLink($(curnode).attr("data-dphref"));
                            } else {
                                var dpHref = "dianping://web?url=" + encodeURIComponent(targetHref);
                            }

                            location.href = 'http://tpl.dianping.com/firework/callApp?type=dianping&appurl='+ encodeURIComponent(dpHref) +'&weburl=' + encodeURIComponent(targetHref);
                        }
                    };

                    //检测，如果已经配置了综合链接，直接打开
                    if (targetHref.indexOf('//tpl.dianping.com/firework/callApp') != '-1'){
                        location.href = targetHref;
                        return;
                    }

                    //mt、dp活动做不同处理
                    if (global.base == 'meituan') {
                       linkHandle.mtPage();
                    } else if (global.base == 'dianping') {
                       linkHandle.dpPage();
                    } else {
                        location.href = targetHref;
                    }

                } else {
                    openUrl();
                }
            } else {
                openUrl();
            }

        }
        //监听链接点击事件，做链接转换
        $('body').on('click', 'a', function (e) {
            if((e.isDefaultPrevented && e.isDefaultPrevented()) || e.defaultPrevented) return;
            var curnode = e.currentTarget, chref;
            if(
                'href' in curnode && // is a link
                (chref=curnode.getAttribute('href')) &&
                !/javascript:|tel:|mail:|sms:/.test(chref) &&   //不是这几种类型
                chref.replace(location.href,'').indexOf('#')    //不是锚点
            ){
                transformLink(e);
            }

        });

        window.gopage = function (pn, gid) {
            if (gid) {
                var page = $(".slider[gid='"+gid+"']")
            } else {
                var page = $('.slider').data('pageable')
            }
            page && page.goPage(pn - 1);
        }

        var perScreen = $(window).height();
        var recorded = {
            '0.5': false,
            '1': false,
            '1.5': false,
            '2': false,
            '2.5': false,
            '3': false,
            '4': false,
            '5': false
        };
        var postDep = function (d) {
            if(recorded[d]) {
                return;
            }
            recorded[d] = true;
            MGE('dep' + d, 'durationdeps');
        };

        $(window).on('scroll', function () {
            //记录用户在页面滑动的最大深度
            if(typeof MT.scrollData != 'undefined'){
                if(window.scrollY > MT.scrollData){
                    MT.scrollData = window.scrollY;

                    var pages = window.scrollY / perScreen;
                    if(pages > 0.5 && pages < 1) {
                        postDep(0.5);
                    } else if(pages > 1 && pages < 1.5) {
                        postDep(1);
                    } else if(pages > 1.5 && pages < 2) {
                        postDep(1.5);
                    } else if(pages > 2 && pages < 2.5) {
                        postDep(2);
                    } else if(pages > 2.5 && pages < 3) {
                        postDep(2.5);
                    } else if(pages > 3 && pages < 4) {
                        postDep(3);
                    } else if(pages > 4 && pages < 5) {
                        postDep(4);
                    } else if(pages > 5) {
                        postDep(5);
                    } else {

                    }
                }
            }else{
                MT.scrollData = window.scrollY;
            }
        })

        window.imgErrorHandler = function() {
            console.log('errored');
            mta('count', 'page.imgeerr');
        }

        $('img').on('error', window.imgErrorHandler);

    });
</script>
