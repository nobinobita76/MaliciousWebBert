require([
    'zepto.js',
    'util/openapp.js',
    'util/cookie.js',
    'util/url.js'
], function ($, openApp, cookie, URL) {
    var param = global.queryParam;
    var weburl = param.weburl;
    var appurl = param.appurl;
    var type = param.type || "meituan";
    
    var sitest = cookie.get('testLCH');
    var passParams = param.psparams;
    
    var downloadUrl = {
        meituan: {
            android: 'http://www.meituan.com/mobile/download/meituan/android/imeituan/waputm_wxfuwuhao?from=touch',
            ios: "https://itunes.apple.com/cn/app/mei-tuan-tuan-gou-tuan-gou/id423084029?mt=8"
        },
        maoyan: {
            android: 'http://www.meituan.com/mobile/download/movie/android',
            ios: 'http://itunes.apple.com/cn/app/id504274740'
        },
        dianping: {
            android: 'http://android.dianping.com/m/',
            ios: 'http://itunes.apple.com/cn/app/da-zhong-dian-ping-mei-shi/id351091731?mt=8'
        },
        waimai: {
            android: 'http://waimai.meituan.com/download?channel=1052',
            ios: 'https://itunes.apple.com/cn/app/mei-tuan-wai-mai/id737310995'
        }
    };
    function addAppPassParam(url){
        var allParams = URL.parseQueryString(url);
        for(var key in allParams){
            if(key.indexOf("url") != -1){
                var _href = allParams[key];
                _href = decodeURI(_href);
                if(_href.indexOf('?') != -1){
                    _href = _href + '&' + passParams;
                }else{
                    _href = _href + '?' + passParams;
                }
                return URL.replace(url, {"url": _href});
            }
        }
        return url;
    }

    if (appurl) {
        setTimeout(function () {
            var q = URL.parseQueryString(document.referrer || "");
            if (appurl.indexOf('lch') == -1) {
                if (q.lch) {
                    if (appurl.indexOf('?') == -1) {
                        appurl += "?lch=" + q.lch
                    } else {
                        appurl += "&lch=" + q.lch
                    }
                }
            }
            if (appurl.indexOf('traffic_source') == -1) {
                if (q.traffic_source) {
                    if (appurl.indexOf('?') == -1) {
                        appurl += "?traffic_source=" + q.traffic_source
                    } else {
                        appurl += "&traffic_source=" + q.traffic_source
                    }
                }
            }
            if(passParams){
                appurl = addAppPassParam(appurl);
            }
            if (cookie.get('openid')) {
                var q = URL.parseQueryString(appurl);
                appurl = URL.replace(appurl, {
                    lch: q.lch + ":" + cookie.get('openid')
                });
            }
            if (sitest) {
                var p = URL.parseQueryString(appurl);
                if (p.lch) {
                    var dom = $('<div style="background:#afa">URL:'+appurl+'<div>LCH:'+p.lch+'</div></div>')
                } else {
                    var dom = $('<div style="background:#faa">URL:'+appurl+'<div>LCH:<b>未设置</b></div></div>')
                }
                $('.modelWrapper').prepend(dom);
            } else {
                openApp.callProtocal(appurl);
            }
        }, 100);
    }
    function addWebPassParam(url){
        if(!url){
            return;
        }
        var _href = decodeURI(url);
        if(_href.indexOf('?') != -1){
            _href = _href + '&' + passParams;
        }else{
            _href = _href + '?' + passParams;
        } 
        return _href;
    }
    if (weburl) {
        if(passParams){
            weburl = addWebPassParam(weburl);
        }
        $('.iopen').show().find('a').attr('href', weburl);
    }
    function changeSource(url){
        var _sourceValue = param.utm_source || '';
        if (passParams) {
        var _params = passParams.split("&");
        for(var i = 0, len = _params.length; i < len; i++){
            var _key = _params[i].split("=")[0];
            if(_key == "utm_source"){
                _sourceValue = _params[i].split("=")[1];
                break;
            }
        }}
        if(_sourceValue){
            return downloadUrl['meituan'].android.replace("waputm_wxfuwuhao", _sourceValue);
        } else {
            return downloadUrl['meituan'].android
        }
    }
    // 下载点击
    $('.downloadbtn').on('click', function (e) {
        if (global.isIphone) {
            location.href = param.iosdownloadurl || downloadUrl[type].ios;
        } else {
            location.href = param.androiddownloadurl || changeSource(downloadUrl[type].android);
        }
    });
});
