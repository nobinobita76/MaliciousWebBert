<script type="text/javascript">
    GJ.use('jquery', function(){
        var total = 1;
        var needBb0s = $('.bn-add-bb-1000063');
        GJ.each(needBb0s, function(needBb0, idx) {
            if ($.inArray(idx, ['context','prevObject', 'selector', 'length']) == -1) {
                // 最后一个广告加 bb0 样式,去边线
                if (idx == total - 1) {
                    $(needBb0).addClass('bb0');
                } else {
                    $(needBb0).removeClass('bb0');
                }
                
                // 鼠标移动效果
                $(needBb0).mouseover(function() {
                    var isImg = $(this).find('.kR-link').data('isimg');
                    if (isImg == 'img') {
                        $(this).addClass('hover-a');
                    } else {
                        var titleHeight = $(this).find('.kR-link').height();
                        if (titleHeight > 20) {
                            $(this).addClass('hover-d2');
                        } else {
                            $(this).addClass('hover-d1');
                        }
                    }
                });
                $(needBb0).mouseout(function() {
                    var isImg = $(this).find('.kR-link').data('isimg');
                    if (isImg == 'img') {
                        $(this).removeClass('hover-a');
                    } else {
                        if ($(this).hasClass('hover-d1')) {
                            $(this).removeClass('hover-d1');
                        } else if ($(this).hasClass('hover-d2')) {
                            $(this).removeClass('hover-d2');
                        }
                    }
                });
            }
        });
    });
</script>
