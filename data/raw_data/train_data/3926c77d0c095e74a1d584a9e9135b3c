	<script>
	
		function hiddenme(obj){
			$(obj).parent().hide(150);
			$('.ads_123').show(150);
		}
        $(function(){
        	$(".ntc-btn").click(function(){
				$(".notice").hide(500);
			});
            //$('#wrapper').removeAttr('style');
            if(document.URL.indexOf('openlink') > 0){
            	$('a').attr('target','_blank');
            }
        });

		if (navigator.userAgent.match(/IEMobile\/10\.0/)) {
		  var msViewportStyle = document.createElement('style')
		  msViewportStyle.appendChild(
			document.createTextNode(
			  '@-ms-viewport{width:auto!important}'
			)
		  )
		  document.querySelector('head').appendChild(msViewportStyle)
		}
		$(function(){
			setTimeout(function(){
				$.getScript("/ads/data.js?t=201602012").done(function(r,s){
					showAds();
				});
			},1500);
			setTimeout(function(){
				if(ismobile() == 0){
					$.getScript("/templates/frontend/frontend-default/js/scoll_bg.js?t=2016020392136");
				}
			},3000);
		});
		function createCloseBtn(){
			var $btn = $('<div />');
			$btn.css({'position':'absolute','font-size':'30px','top':'10px','color':'rgb(6, 255, 207)','cursor':'pointer'}).html('X');
			$btn.click(function(){
				$(this).parent().hide();
				$('#nyhiddenme').parent().show(150);
			});
			return $btn;
		}
		function showAds(){
			if(ads){
				var cWidth = parseInt($('.container').css('width').replace('px',''));
				var pcContainerWidth = 1150;
				var ismo = ismobile();
				$.each(ads,function(i,r){
					var width = parseInt(r.width) === 0 ? '100%' : (r.width * (cWidth / pcContainerWidth))+'px';
					var height = parseInt(r.height)===0 ? '100%' : r.height+'px';
					var advs = $('.ads_'+i);
					if(r.ads && advs.length > 0){
						var url = '';
						var adv = r.ads[0];
						if(ismo != parseInt(adv.ismobile)) return;
						$img = $('<img />');
						$img.attr('src', (adv.media != '') ? adv.media : adv.relogopic);
						$img.attr('title', adv.name);
					
						$img.attr('style', "width:"+width+";height:"+height+";");

						$a = $('<a />');
					
						if(adv.id && adv.relname != '' && adv.relogopic != '') {
							url = '/tiaozhuan.php?id=' + adv.id;
						}else{
							url = adv.url;
						}
						$a.attr('href', url);
						$a.attr('target', '_blank');
						var id = 0;
						if(adv.id){
							id = adv.id;
						}
						var name =  "";
						if(adv.name){
							name =  adv.name;
						}
						var zone_name = "";
						if(adv.zone_name){
							zone_name = adv.zone_name;
						}
						$a.on({
							'click':function(){
								$.post('/ajax/adv_count',{'id':id,'title':name,'zone_name':zone_name},function(resutl){});
							}
						});
						var margin = '0 0 10px 0';
						if(adv.margin && adv.margin != ''){
							margin = adv.margin;
						}
						if(adv.isbtn && parseInt(adv.isbtn) === 1){
							var btn = createCloseBtn();
							$(advs).append(btn);
						}
						$(advs).css({'width':width,'margin':margin}).append($a.append($img));
					}
				});
			}
		}
		function HideFoot() {
            var botm = $('.QQ_S').css("top");
            var screenH = $(window).height();
            var len = botm.length;
            num = Number(botm.substr(0, len - 2));
            var time = setInterval(function () {
                if (num <= screenH - 212) {
                    num = num + 40;
                    botmt = num + "px";
                    $('.QQ_S').css("top", botmt)
                } else {
                    clearInterval(time);
                    $('.QQ_S').css("display", "none");
                    $('.QQ_S1').css("display", "block");
                }
            }, 10);
        }
        function ShowFoot() {
            $('.QQ_S').css("display", "block");
            $('.QQ_S1').css("display", "none");
            var botm = $('.QQ_S').css("top");
            var screenH = $('.QQ_none').css("top");
            var len = botm.length; lena = screenH.length;
            num = Number(botm.substr(0, len - 2));
            numa = Number(screenH.substr(0, lena - 2));
            var time = setInterval(function () {
                if (num > numa + 30) {
                    num = num - 30;
                    botmt = num + "px";
                    $('.QQ_S').css("top", botmt);
                } else {
                    $('.QQ_S').css("top", numa + "px");
                    clearInterval(time);
                }
            }, 10);
        }
        
	
	var back_img = '/templates/frontend/frontend-default/img/ZL_LL.jpg?t=1490963593';
	var wrapper_left_bg_top = parseInt('440');
	var wrapper_right_bg_top = parseInt('450');
	</script>
