function checkformcn(theForm)
{
  if (theForm.cLinkMan.value == "")
  {
    alert("请输入联系人姓名！");
    theForm.cLinkMan.focus();
    return (false);
  }  
  if (theForm.cTel.value == "")
  {
    alert("请输入联系电话！");
    theForm.cTel.focus();
    return (false);
  } 
  if (theForm.cDetails.value == "")
  {
    alert("请输入订单内容！");
    theForm.cDetails.focus();
    return (false);
  }
  return (true);
}
