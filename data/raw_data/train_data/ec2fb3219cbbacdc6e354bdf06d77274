  <script type="text/javascript">
/*********箭头控制滚动方向、加速及鼠标拖动***************/
/*********箭头控制滚动方向、加速及鼠标拖动***************/
var MarqueeDiv2Control=new Marquee("MarqueeDiv2");		//箭头控制滚动方向、加速及鼠标拖动实例
MarqueeDiv2Control.Direction="left";
MarqueeDiv2Control.Step=1;
MarqueeDiv2Control.Width=870;
MarqueeDiv2Control.Height=100;
MarqueeDiv2Control.Timer=20;
MarqueeDiv2Control.ScrollStep=1;				//若为-1则禁止鼠标控制实例
MarqueeDiv2Control.Start();
MarqueeDiv2Control.BakStep=MarqueeDiv2Control.Step;
getObj("LeftButton2").onmouseover=function(){MarqueeDiv2Control.Direction=2};
getObj("LeftButton2").onmouseout=getObj("LeftButton2").onmouseup=function(){MarqueeDiv2Control.Step=MarqueeDiv2Control.BakStep};
getObj("LeftButton2").onmousedown=getObj("RightButton2").onmousedown=function(){MarqueeDiv2Control.Step=MarqueeDiv2Control.BakStep+3};
getObj("RightButton2").onmouseover=function(){MarqueeDiv2Control.Direction=3};
getObj("RightButton2").onmouseout=getObj("RightButton2").onmouseup=function(){MarqueeDiv2Control.Step=MarqueeDiv2Control.BakStep};
</script> 
