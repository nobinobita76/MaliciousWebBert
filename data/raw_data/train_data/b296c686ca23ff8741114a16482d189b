	<script type="text/javascript">
		/*add by lbh 14/03/31 start ie6下公共导航兼容处理*/
		function hover (tab,hoverName) {
			jQuery(tab).bind('mouseover',function(){
				jQuery(tab).addClass(hoverName);
				hoverhidescope();
			});
			jQuery(tab).bind('mouseleave',function(){
				jQuery(tab).removeClass(hoverName);
				hoverhidescope();
			});
		};
		hover('#news-nav','news-mod-hover');
		hover('#brand-nav','brand-mod-hover');
		hover('#use-nav','nav-use-hover');
		hover('#message-nav','nav-message-hover');
		hover('#bbs-nav','nav-bbs-caption-hover');
		/*add by huzhiqiang 2014.5.29*/
		function livebutton(tab,tabs,hoverName,hoverNames){
			jQuery(tab).live({
				   mouseenter:
				   function()
				   {
					   jQuery(tab).addClass(hoverName);
					   jQuery(tabs).addClass(hoverNames);
				   },
				   mouseleave:
				   function()
				   {
					   jQuery(tab).removeClass(hoverName);
					   jQuery(tabs).removeClass(hoverNames);
				   }
				});
		}
		livebutton('.app-caption','.app-options','app-caption-hover','app-options-hover');
		livebutton('.app-options','.app-caption','app-options-hover','app-caption-hover');



		//处理搜索全文 scope select 样式问题
		function hoverhidescope(){
			var isIE6 = /msie 6/i.test(navigator.userAgent);
			if(isIE6){
				if(jQuery(".nav-bbs-options").css('display') != 'none'){
					jQuery("#scope").css('display','none');
				}else{
					jQuery("#scope").css('display','inline');
				}
			}
		}
		/*add by lbh 14/03/31 over*/


		function pageWH(){
			var h= document.body.scrollHeight ? document.body.scrollHeight : document.documentElement.clientHeight;
			var w= document.body.scrollWidth ? document.body.scrollWidth : document.documentElement.clientWidth;
			return {width:w,height:h};
		}
		var pageobj = pageWH() ;
		var c_pageWidth = getcookie("55bbs_pageWidth");
		var headerWidth = jQuery("#mainbox").width();
		var localUrl = window.location.href,reg = /forum/;
		if(!c_pageWidth){
			setcookie('55bbs_pageWidth', pageobj.width+'|'+pageobj.height, 20*3600*1000,'','.55bbs.com');
		}
		//窄屏隐藏APP入口

		if(pageobj.width<=1200&&!localUrl.match(reg)){
			jQuery(".bbs-reply-app").css('display','none');
			jQuery(".bbs-list-reply-app").css('display',"");
		}
		if(pageobj.width>=1200){
			$("header").parentNode.style.width="1200px";
			$("sub_footer").style.width="1200px";
			$("cbs_link").style.width="1200px";
			var script = 'viewthread';
			if(script=="forumdisplay"){
				$("content_list").style.width="1200px";
				$("list_left").style.width="973px";
				$("list_right").style.display="block";
				$("wrap_r").style.display="block";

				$("jc_recommend").style.display="block";
				$("pro_activity").style.display="block";
				$("wrap_r2").style.display="block";
			}
		}
</script>
<!--yll新分栏1200,980-->

