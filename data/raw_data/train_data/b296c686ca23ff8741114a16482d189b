		//提交求链
		function addLink(img,aid,pid,to_uid,to_username,to_avatar,dateline){
			var addpost = $('addpost').checked ? 1 : 0, message = $('talkBox').value, linksubject = '第一次用了“春药” 后果很严重';
			var data = '&fid=37&tid=1708355&pid='+ pid +'&aid='+ aid +'&addpost='+ addpost +'&img='+ img + '&message='+ encodeURIComponent(encodeURIComponent(message)) +'&subject='+encodeURIComponent(encodeURIComponent(linksubject))+'&to_uid='+ to_uid +'&to_username='+ encodeURIComponent(encodeURIComponent(to_username))+'&to_avatar='+ to_avatar+'&dateline='+dateline;
			ajaxresponse('inajax=1&action=addlink'+ data);
			$('talkBox').value='';
		}
		//JS ajax
		function ajaxresponse(data) {
			var x = new Ajax('XML'),uid = 0;
				x.post('ajax.php', data, function(s){
					if(s == 'succeed') {
						cancel();
						var popNotAnswer = '<div class="con" style="height:200px;"><h5><strong>提示</strong><i onclick="colse(this)"></i></h5><p class="popup-tip" style="padding-top:80px;">亲，楼主暂未回复宝贝链接，楼主回复后我们会站短通知您！</p></div>';
						showAlert(popNotAnswer, 'popNotAnswer', 2000);
					}else if(s == 'isreply'){
					//已回复
					cancel();
					var popAnswer = '<div class="con"><h5><strong>提示</strong><i onclick="colse(this)"></i></h5><p class="popup-tip">亲，楼主已回复宝贝链接，<a target="_blank" href="http://my.55bbs.com/linkout-'+uid+'-2-1.html">马上查看</a>！</p></div>';
					showAlert(popAnswer, 'popAnswer', 0);
					}else if(s == 'not_login'){
						cancel();
						var poplogin = '<div class="con"><h5><strong>亲，请先登录哦</strong><i onclick="colse(this)"></i></h5><div class="popup-btn"><a href="http://my.55bbs.com/login/index.php" class="login-btn">登录</a><a href="http://my.55bbs.com/reg/index.php">立即注册</a></div><p class="login-more">您也可以用其他账号登陆</p><div class="popup-btn"><a href="#" class="qq-btn"></a><a href="#" class="sina-btn"></a></div></div>';
						showAlert(poplogin, 'popLogo', 0);
					}else{
						cancel();
						var error = '<div class="con"><h5><strong>提示</strong><i onclick="colse(this)"></i></h5><p class="popup-tip">请求失败，请重试！</p></div>';
						showAlert(error, 'popError', 2000);
					}
				});
		}
		//获取求链次数
		function getLinkTimes(aid, uid){
			if($('linktimes_'+aid).innerHTML == ''){
				ajaxget('/ajax.php?inajax=1&action=getlinktimes&aid='+aid+'&uid='+uid, 'linktimes_'+aid);
			}
		}
		//弹窗输出
		function showAlert(info, infoSign, setTime) {
			$(infoSign).innerHTML = info;
			$(infoSign).style.display = 'block';
			//if(setTime){
				setTimeout(function(){$(infoSign).style.display ='none';},2500);
			//}
		}
		//关闭窗口
		function colse(obj){
			if (obj == null) {
				return;
			}
			obj.parentNode.parentNode.parentNode.style.display = 'none';
		}
		//取消求链
		function cancel(){
			$('popupLink').style.display = 'none';
		}
		//添加表情
		function showSmily(){
			$('smily').onclick=function (){
				$('smilies').style.display = 'block';
				ajaxget('post.php?action=smilies&link=1', 'smilies_list');
			};
		}
		function insertLinkSmiley(smilieid) {
			obj = $('talkBox');
			obj.focus();
			var src = $('linksmilie_' + smilieid).src;
			var code = $('linksmilie_' + smilieid).code;
			code += ' ';
			obj.value += code;
			$('smilies').style.display = 'none';
		}
		//快速求链
		function chanTxt(i){
			var f 	= $('fastList'),
				t  	= $('talkBox'),
				l 	= f.getElementsByTagName('li');
				var txt = l[i].innerHTML;
				t.value = txt;
		}
	</script>
<script type="text/javascript">
	jQuery(document).ready(function() {
		jQuery(".down_up_btn").toggle(function(){
        jQuery(this).parent().find(".autograph_div").css({height:"auto"});
			  jQuery(this).find("i").css({background:"url(http://oss-attachments.55-img.com/newbbs/images/seeall_btn.png)no-repeat center right"})
       }, function(){
        jQuery(this).parent().find(".autograph_div").css({height:60});
			  jQuery(this).find("i").css({background:"url(http://oss-attachments.55-img.com/newbbs/images/seeall_btn1.png)no-repeat center right"})

    });

    /*
		jQuery(".down_up_btn").click(function() {
			jQuery(".autograph_div").slideToggle();
		});
		*/
		jQuery("#goTop").click(function() {
			jQuery('body,html').animate({
				scrollTop : 0
			}, 300);
		});
		jQuery(window).scroll(function() {
			var scroll_top = jQuery(document).scrollTop();
			if (scroll_top >= 500) {
				jQuery(".stick_top").show();
			} else if (scroll_top <= 500) {
				jQuery(".stick_top").hide();
			}
		});
		var r_item_h = jQuery(".card_content_main_right").height(),
			 jl_top_h = Math.abs(jQuery(".card_content_main_right").offset().top),
			 windowh = jQuery(window).height(),
			 main_h = jQuery(".card_content_main").height(),
		main_top_h = jQuery(".card_content_main").offset().top;

    jQuery(window).scroll(function(){
      var winscrolltop = jQuery("body").scrollTop();
        if(r_item_h+jl_top_h <= windowh){
          if(winscrolltop >= (windowh + jl_top_h) &&  winscrolltop <= (main_h+main_top_h)){
        		jQuery(".card_content_main_right").css({position:"fixed",left:"50%",bottom:"0px",marginLeft:"202px"});
          }else{
						jQuery(".card_content_main_right").css({position:"",marginLeft:"0px"});
					}
        }else{
          if(winscrolltop >= (r_item_h-windowh+jl_top_h)  &&  winscrolltop <= (main_h+main_top_h-windowh)){
		            jQuery(".card_content_main_right").css({position:"fixed",left:"50%",bottom:"0px",marginLeft:"202px"});
		      }else{
				    jQuery(".card_content_main_right").css({position:"",marginLeft:"0px"});
				  }
			 }
		 });

		// for(var ii=1;ii<=13;ii++){
			// jQuery(".user_phone").mouseover(function() {
			// 	jQuery(".user_pop").show();
			//
			// });
			// jQuery(".user_pop").mouseleave(function() {
			// 	jQuery(".user_pop").hide();
			// });

		// }
/*
jQuery(".user_img1").mouseover(function(){
	jQuery(this).find(".user_pop").show();
});
jQuery(".user_img1").mouseleave(function(){
	jQuery(this).find(".user_pop").hide();
});
*/
jQuery(".louzhuinfo").mouseover(function(){

	jQuery(this).find(".user_pop").css({top:"-276px", left:"-156px"});
	jQuery(this).find(".user_pop").show();
});
jQuery(".louzhuinfo").mouseleave(function(){
	jQuery(this).find(".user_pop").hide();
});

jQuery(".user_center_name_f").mouseover(function(){

	jQuery(this).find(".user_pop").css({top:"-275px",left:"-168px"});
	jQuery(this).find(".user_pop").show();

});


jQuery(".user_center_name_f").mouseleave(function(){
jQuery(this).find(".user_pop").hide();
});

		jQuery(".up_down_btn_pop").toggle(function() {
			jQuery(".up_down_btn_pop").removeClass("up1").addClass("down1")
			jQuery(".looks_div").animate({
				height : '120px'
			})
		}, function(){
			jQuery(".up_down_btn_pop").addClass("up1").removeClass("down1")
			jQuery(".looks_div").animate({
				height : '60px'
			})
		})
	});
</script>


