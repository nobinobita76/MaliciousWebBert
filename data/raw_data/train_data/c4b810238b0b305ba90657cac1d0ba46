$(window).load(function(){
    (function (){
        var pageCount = 0;
        var pageIndex = 1;
        var pager = '';
        var baseUrl = '',prevUrl='',nextUrl='';
        var img;
        var width,height,top,left;
        var btnHTML = '';

        pager = $(".pager").text();
        pageCount = $(".pager a").size();

        if(pageCount>4){
            if(pager.indexOf('上一页')!=-1){
                pageCount -= 1; // 去除上一页
            }
            if(pager.indexOf('下一页')!=-1){
                pageCount -= 1; // 去除下一页
            }
        }

        if(pageCount>4){
            pageIndex = $("a.curpage").text();
            baseUrl = $("a.curpage").attr("href");
            baseUrl = baseUrl.replace(/(_\d{0,2})?.htm/,'');
                
            img = $(".news img");
            width = img.width()+10; // padding:5
            height = img.height()+10;
            top = img.offset().top;
            left = img.offset().left;

            if(pageIndex==1){
                btnHTML = '';
            }
            else{
                if(pageIndex==2){
                    prevUrl = baseUrl + '.htm';
                }
                else{
                    prevUrl = baseUrl + '_' + (pageIndex-2) + '.htm';
                }
                
                btnHTML = '<div id="btnPrevPage" style="position:absolute;display:block;width:'+width/2+'px;height:'+ height + 'px;left:'+left+'px;top:' + top + 'px; z-index:100;font-size:72px;color:#fff;line-height:' + height + 'px;cursor:pointer;background:#fff;opacity:0.01;filter:alpha(opacity=1);"></div><div style="position:absolute; display:block; width:80px;height:80px;left:' + (left+10) + 'px;top:' + (top+height/2-40) + 'px; z-index:90; text-align:left; font-size:72px; color:#fff; line-height:80px;" >&lt;</div>';
            }

            if(pageIndex==pageCount){
                
            }
            else{
                nextUrl = baseUrl + '_' + pageIndex + '.htm';
                btnHTML += '<div id="btnNextPage" style="position:absolute; display:block; width:' + width/2 + 'px;height:' + height + 'px;left:' + (left+width/2) + 'px;top:' + top + 'px; z-index:100; text-align:right; font-size:72px; color:#fff; line-height:' + height + 'px;cursor:pointer;background:#fff;opacity:0.01;filter:alpha(opacity=1);" ></div><div style="position:absolute; display:block; width:80px;height:80px;left:' + (left+width-90) + 'px;top:' + (top+height/2-40) + 'px; z-index:90; text-align:right; font-size:72px; color:#fff; line-height:80px;" >&gt;</div>';
            }
        
            $(".news").append(btnHTML);
        }

        $('#btnPrevPage').click(function(){window.location = prevUrl; });
        $('#btnNextPage').click(function(){window.location = nextUrl; });

    })();    


});



    

function addSearch(){
    var html=[];
    /*
    html.push('<style>');
    html.push('#lpsearch{float:right;margin:3px 0 0; display:inline-block;}');
    html.push('#lpsearch .textBox {width:90px; height:18px; color:#777;border:1px solid #ccc; float:left; line-height:18px; text-indent:3px;}');
    html.push('#lpsearch .btn {border:1px solid #ccc;background:#efefef; width:40px; height:20px; float:left; line-height:18px; font-size:12px; cursor:pointer;}');
    html.push('</style>');
    */
    html.push('<div id="lpsearch">');
    html.push('<form action="http://s.rednet.cn/" accept-charset="utf-8" onsubmit="document.charset=\'utf-8\';">');
    html.push('<input name="q" id="searchinput" type="text" value="请输入关键词" class="textBox" onfocus="javascript:if(this.value==\'请输入关键词\'){this.value=\'\';this.style.color=\'#000\'}" onblur="javascript:if(this.value==\'\'){this.value=\'请输入关键词\';this.style.color=\'#7B7C7D\'}"  size="20">');
    html.push('<input name="scope" value="1" type="hidden">');
    html.push('<input name="title" value="0" type="hidden">');
    html.push('<input type="submit" class="btn" value="搜索" onclick="javascript:var input=document.getElementById(\'searchinput\');if(input.value==\'请输入关键词\'){input.value=\'\';input.focus();input.style.color=\'#000\';return false;} return true;"/> ');
    html.push('</form></div>');
    $('.path').append(html.join(''));
}

function loadBanner(){
	var sd = document.domain.split('.')[0];
	var hb = {'hn':974,'news':975,'gov':976,'china':977,'globe':978,'av':979,
	'ent':980,'hlj':981,'tax':982,'ny':983,'yq':984,'jj':985,'fdc':986,
    'auto':987,'money':988,'health':989,'edu':990,'jt':991,'tour':992,
    'life':993,'qy':1161,'aq':1194,'pf':1214,'3c':1203,'zscq':1241,'yz':1247};
	var url = 'http://ad.rednet.cn/Code/974.htm';
	if(hb[sd]>0){url = url.replace('974',hb[sd]);};
	loadScript(url,function(){});
}

function ifr(iUrl,iWidth,iHeight){
    return '<iframe src="' + iUrl + '" width="'+iWidth+'" height="'+iHeight+'" frameborder="no" border="0" marginwidth="0" marginheight="0" scrolling="no" ></iframe>';
}







