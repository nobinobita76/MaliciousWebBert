function PushData_videoList(){
				jQuery.ajax({
		             url:"http://search.boosj.com/video/relate/2625270?size=7",
		             dataType:"jsonp",
		             jsonp:"callback",
		             success:function(data){
						if(data.code==200){
							var re =[];
							jQuery(data.videos).each(function(i,item){
								re.push('<dl class="pri_piclist"><dt><a href="'+item.url+'" title="'+item.title+'">');
								if(i < 3){
									re.push('<img alt="'+item.title+'" src="'+item.imageUrl+'"  width="123" height="80" />');	
								}else{
									re.push('<img alt="'+item.title+'" datasrc="'+item.imageUrl+'"  width="123" height="80" />');
								}
								re.push('</a></dt><dd><h4><a href="'+item.url+'"  title="'+item.title+'">'+item.title+'</a></h4><span class="haseplay">播放：'+item.playtimes+'</span>');
								re.push('<span class="time">时长：'+item.duration+'</span></dd></dl>');
							});
							jQuery("#AutoVideoList .overview dl:eq(1)").after(re.join("")).find("img").slice(0, 5).each(function(){//--显示前4张图片
										if($(this).attr("datasrc")){
										$(this).attr("src",$(this).attr("datasrc")).removeAttr("datasrc");
									}
							});
						}
						return false; 
		             }
					});
		}
