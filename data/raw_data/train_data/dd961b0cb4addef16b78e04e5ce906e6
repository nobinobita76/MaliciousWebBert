     <script src="template/v2_mbl20121009/mobile_plus/js/ajaxpage.js?x6j" type="text/javascript"></script> 
     <script type="text/javascript">
var pages=1;
var allpage=3;
function ajaxpage(url){
jq("ajaxld").style.display='block';
jq("ajnt").style.display='none';
var x = new Ajax("HTML");
pages++;
url=url+'&page='+pages;
x.get(url, function (s) {
s = s.replace(/\\n|\\r/g, "");//alert(s);
s = s.substring(s.indexOf("<div id=\"alist\""), s.indexOf("<div id=\"ajaxshow\"></div>"));//alert(s);
jq('ajaxshow').innerHTML+=s;
jq("ajaxld").style.display='none';
if(pages==allpage){							
jq("a_pg").style.display='none';
}else{
jq("ajnt").style.display='block';
}
if(post_new){
post_new.parentNode.removeChild(post_new);						
var newMask = document.createElement('div');
newMask.id ='post_new';
post_ajax.appendChild(newMask); 
}
});
return false;
}
</script> 
