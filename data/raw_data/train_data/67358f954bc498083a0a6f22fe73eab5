<script type="text/javascript" language="javascript">
document.getElementById("TUserName").focus();
$("#theform").unbind("submit");
$("#theform").on("submit", function (e) {
if ( $("#TUserName").val().length<=0)
{
alert('请输入有效的帐号。');
$("#TUserName").focus();
return false;
}
if ($("#TPassword").val().length <= 0) {
alert('请输入正确的密码。');
$("#TPassword").focus();
return false;
}
if ($("#TVerifyCode").val().length < 4) {
alert('请输入正确的验证码，验证码是4位中文。');
$("#TVerifyCode").focus();
return false;
}
});
</script>  


