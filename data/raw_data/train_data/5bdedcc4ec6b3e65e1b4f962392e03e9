<script language="JavaScript" type="text/javascript">
var n,c=0,wid;

function autoPlay() {
    c++; //指向下一图
    if (c >= n) c = 0; //循环
    $($("#num li")[c]).addClass("current");
    $($("#num li")[c]).siblings().removeClass("current");
    $($("#scrollArea li")[c]).siblings().hide();
    $($("#scrollArea li")[c]).fadeTo(500, 1);
}
$(function() {
    var framTime = "3";
    var time;
    if (framTime != null && framTime != '0') {
        time = framTime * 1000;
    }
    n = $("#num li").size();
    $("#num li:first").addClass("current");
    $("#num li").css("right", function(i) {
        return 10 * (n - i);
    })
    $("#scrollArea li:gt(0)").hide();
    $("#num li").hover(function() {
        window.clearInterval(wid); //鼠标进入 清除定时序列
        c = $(this).index();
        $(this).addClass("current");
        $(this).siblings().removeClass("current");
        $($("#scrollArea li")[c]).siblings().hide();
        $($("#scrollArea li")[c]).fadeTo(500, 1);
    }, function() {//鼠标移开 重新启动定时序列
        wid = window.setInterval("autoPlay()", time);
    }
    )
    wid = window.setInterval("autoPlay()", time); // 启动定时序列
})

$.getScript("/tplresource/taiping/script/statistics.js",function () {
	advertisingView("tsggzx");
});
</script>


