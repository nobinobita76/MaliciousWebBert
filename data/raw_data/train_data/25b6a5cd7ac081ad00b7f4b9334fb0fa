        o.displayExpert = function (ids) {
            for (var i = 0; i < ids.length; i ++) {
                $(".user_meg"+ids[i]).after("<div class=\"zjIcon\"><a href=\"http://bbs.pcauto.com.cn/expert/"+ids[i]+"/\"  target=\"_blank\">车专家</a><\/div>");
                if(ids[i] == Topic.bestAnswerAuthorId){
                    $("#basExpert .info-main #askToHimButton_"+ids[i]).css("display","none");
                    $("#basExpert a").attr("href", "http://bbs.pcauto.com.cn/expert/"+ids[i]+"/");
                    var basExpertHtml = "<a id=\"askToHimButton\" href=\"#\" target=\"_blank\" onclick=\"location.href='http://bbs.pcauto.com.cn/new/post.do?fid=21205&type=question&askTo="+ids[i]+"';return false;\" class=\"askForHer\">向他提问<\/a>";
                    basExpertHtml += "<div class=\"zhuanjia\"><\/div>";
                    $("#basExpert .info-main").append(basExpertHtml);
                }
            }
        }
        o.run = function(){
            var onlineIds = [];
            var vipIds = [];
            var re = /user-([\d]+).*/ig;
            var re2 = /vip_ul([\d]+).*/ig;
            $('.needonline').each(function() {
                onlineIds.push(this.className.replace(re,'$1'));
            });

            $(".user_car").each(function(i, ele){

                // vipIds.push($(ele).attr("uid"));
                vipIds.push(this.className.replace(re2,'$1'));

            });

            if(!!$.cookie("common_session_id")){
                //loaddynamic
                $.getJSON("http://bbs.pcauto.com.cn/topic/loadDynamicInfos.ajax", {fid:Topic.forumId,
                    tid:Topic.topicId,
                    authorId:Topic.byAuthorId,
                    pageNo:Topic.pageNo,
                    t:new Date().getTime(),
                    c:$.cookie("common_session_id")},
                o.loadDynamic);
            }else{
                //获取帖子浏览数
                getViews(0);
                //remove elements when anonymous
                $(".view_ip").remove();
                // $(".signature").remove();
                $("fix_reply_wrap").remove();

                $('.post_wrap').live('mouseenter', function(e){
                    $(this).find(".askForHer").css('display','block');
                });
                $('.post_wrap').live('mouseleave', function(e){
                    $(this).find(".askForHer").css('display','none');
                });

                $("#activeAction").html("<span class=\"tip\"><i class=\"icon\"></i>您需要登录后才可以参加活动<a href=\"javascript:window.location.href='http://my.pcauto.com.cn/login.jsp?return='+encodeURIComponent(window.location.href)\">立即登录</a></span>");
            }

            $.getJSON("http://bbs.pcauto.com.cn/topic/loadStaticInfos.ajax?isBrandForum=true", {tid:Topic.topicId, fid:Topic.forumId, vids:unique(vipIds).join(',')}, o.loadStatic);
            $.getScript("http://bbs.pcauto.com.cn/topic/loadExpertUsers.do?cb=loadTopicInfos.displayExpert&ids="
                + unique(onlineIds).join(','));
            if(false == true){
                $.getJSON("http://autosem.pc.com.cn/seo/xueche/topicToXuecheTag.do?callback=?", {topicId:Topic.topicId}, function(data){
                    if(!!data.items && data.items.length > 0){
                        var xcitems = data.items;
                        var xchtml = "<div class=\"carseris-b\"><p>相关提问：";
                        for(var i in xcitems){
                            if(i<5){
                                xchtml+="<a href=\""+xcitems[i].url+"\" target=\"_blank\">"+xcitems[i].name+"<\/a>";
                            }
                        }
                        xchtml+="</p></div>";
                        $(".mnks:first").after(xchtml);
                    }
                });
            }
        };
        return o;
    }());
</script>
        
        





<script type="text/javascript">
        function getViews(uid){
            // 查看数
           $.getJSON("http://bbs.pcauto.com.cn/intf/topic/counter.ajax?tid=12180537&fid=21205&agent=0&uid="+uid+"&currentUrl=&currentReferer=&" + new Date().getTime(),function(result) {
               if (!!result) {
                   $("#views").html(result.views);
               }
           });
        }
           var visitedfid = $.cookie("visitedfid");
           if (visitedfid == null || visitedfid == '') {
               visitedfid = '21205';
           } else {
               var fids = visitedfid.split('D');
               for (var i = 0; i < fids.length; i++) {
                   if(fids[i] == '21205') {
                       fids.splice(i, 1); break;
                   }
               }
               fids.unshift('21205');
               if (fids.length > 10) fids.pop()
               visitedfid = fids.join("D");
           }
           $.cookie('visitedfid', null);
           $.cookie('visitedfid', visitedfid, { expires: 100, domain: 'pcauto.com.cn'});

           window.doReply = function(){
               goCreateTopic("http://bbs.pcauto.com.cn/reply.do?fid=21205&tid=12180537&action=reply");
          };
          window.doReply = function(channel){
              if (channel) {
                  goCreateTopic("http://bbs.pcauto.com.cn/reply.do?fid=21205&tid=12180537&action=reply&ad="+channel);
              } else {
                  goCreateTopic("http://bbs.pcauto.com.cn/reply.do?fid=21205&tid=12180537&action=reply");
              }
          };
          
           /* function doReply(){
               var returnURL = encodeURIComponent("http://bbs.pcauto.com.cn/reply.do?fid=21205&tid=12180537&action=reply");
               var tips = '您还没有登录<div style="margin-top:10px;"><a href="javascript:goCreateTopic(\'http://my.pcauto.com.cn/passport/login.jsp?return='+returnURL+'\')" style="color:#336797;">去登录/注册</a></div>';
               if (jQuery.browser.msie6) {
                   tips = "您还没有登录,请先登录";
               }
               showMessage(tips);
           } */
            function showGradeDiv(obj){
                        $(".fixFloatDiv").hide();
                        $("#"+obj).css("top",($(window).height()-$("#"+obj).outerHeight())/2).css("position","fixed").show(250);
                    }
                    function showAllScore (id) {
                        $("#pflist_"+id).show(250);
                    }
                    function closeAllpf(id){
                        $("#pflist_"+id).hide(250);
                    }
        function checkAllManagerBox(obj){
                if($("#manage_check_all").attr("checked")){
                    $("input[name='moderate[]']").each(function(i) {
                        $("#checkAllEvent").val("true");
                        if (!$(this).attr("checked")) {
                            $(this).attr("checked",true);
                            pidchecked(this);
                            modclick(this);
                        }
                    });
                    $(obj).attr("checked", false);
                    $(obj).attr("checked", true);
                    $("#manage_check_all").attr("checked",true);
                    $("#checkAllEvent").val("false");
                }else{
                    $("input[name='moderate[]']").each(function(i) {
                        if ($(this).attr("checked")) {
                            $(this).attr("checked", false);
                            pidchecked(this);
                            modclick(this);
                        }
                    });
                }

            }
            var lastSelectedPid = 0;
            function pidchecked(obj) {
                if(obj.checked) {
                    lastSelectedPid = $(obj).val();
                    $("<input name='topiclist[]'/>").attr("id", 'topiclist_' + obj.value).
                        val(obj.value).css("display", "none").appendTo("#modactions");
                } else {
                    $('#topiclist_'+ obj.value).remove();
                }
            }
            var modclickcount = 0;
            function modclick(obj, pid) {
                if(obj.checked) {
                    modclickcount++;
                } else {
                    modclickcount--;
                }
                $('#modcount').html(modclickcount);
                $('.modlink').attr("title","选中" + modclickcount+ "篇");

                if(modclickcount > 0) {
                    if($("#checkAllEvent").val()!="true"){
                        var offset = $(obj).offset();
                        $('#topic_admin_layer').css('top', offset.top + 20);
                        $('#topic_admin_layer').css('left', offset.left - 180);
                    }
                    $("#checkAllEvent").val("false");
                    $("#manage_check_all").attr("checked",false);
                    $('#topic_admin_layer').show();
                } else {
                    $('#topic_admin_layer').hide();
                }
            }
            function restore(ids) {
                $.ajax({
                    type:"post",
                    url:"http://bbs.pcauto.com.cn/action/topic/restore.do?tids=" + ids,
                    success:function(data) {
                        eval("var result=" + data);
                        if(result.status == 0){
                            alert(result.desc);
                            window.location.href = window.location.href;
                        } else {
                            alert(result.desc);
                        }
                    }
                });
            }

            function setEmot(id, group, num) {
            	if(!group){
            		group = 1;
            	}
            	if(!num){
            		num = 12;
            	}

            	num = num > smilies_array[1][group].length ? smilies_array[1][group].length : num;
            	var html = "";
            	for (var i = 0; i < num; i++) {
            		var arr2 = new Array();
            		arr2 = smilies_array[1][group][i];
            		if (typeof arr2 !== "undefined") {
            			html += "<a class=\"e" + (i+1) + "\" title=\"" + arr2[1] + "\" href=\"javascript:insertSmileyImage('" + id + "','" + arr2[1] + "');\"><img src=\"http://www1.pcauto.com.cn/autobbs/2013/images/emot/"+arr2[0]+".gif\"></a>";
            		}
            	}

            	if(num > 54  && typeof(window.iScroll) != 'undefined') html = '<div class="iscroll"><div class="a3-m-ul" id="inner-content_'+ id +'_'+ group +'"><div>'+ html + '</div></div><div class="tool"><i class="tool-bar-top"></i><span id="tool-bar_'+ id +'_'+ group +'" class="tool-bar"></span><i class="tool-bar-bottom"></i></div></div>';

            	$("#emotion_" + id+"_"+group).html(html);

            	if (num > 54 && typeof(window.iScroll) != 'undefined') {
            		setTimeout(function(){
            			window.iScroll.init({
            				el: document.getElementById('inner-content_'+ id +'_'+ group ),
            				scrollBar: document.getElementById('tool-bar_' + id + '_'+ group)
            			});
            		},10);
            	}

            	//鼠标进入表情2 不滚动页面
            	function scrollFunc(evt) {
            		evt = evt || window.event;
            		if(evt.preventDefault) {
            			// Firefox
            			evt.preventDefault();
            			evt.stopPropagation();
            		} else {
            			// IE
            			evt.cancelBubble=true;
            			evt.returnValue = false;
            		}
            		return false;
            	}

            	function disabledMouseWheel(obj) {
            		if (document.addEventListener) {
            			obj.addEventListener('DOMMouseScroll', scrollFunc, false);
            		}//W3C
            		obj.onmousewheel  = scrollFunc;//IE/Opera/Chrome
            	}
            	$('.tabctn').each(function(){
            		if($(this).find('.iscroll').length<1) disabledMouseWheel(this);
            	});
           	}

            /**
             * 改变粉丝数目
             * act 操作标志 add表示粉丝数+num，del粉丝数-num,show仅仅显示num
             * id 用户的id
             */
            function changeFanNum(act,id,num) {
                if(act == "add") {
                    $(".fan"+ id ).find('a').html(parseInt($(".fan" + id).html()) + num);
                } else if(act == "del") {
                    $(".fan" + id ).find('a').html(parseInt($(".fan" + id).html()) - num);
                } else if(act == "show") {
                    $(".fan" + id ).find('a').html(num);
                }
            }

            /**
             *  改变按钮状态
             *  status 按钮的状态
             *  id 用户的id
             */
            function changeBtnStatus(id,status) {
                //status：0 没有关系，1 我关注的人，2 我的粉丝，3 互相关注
                if(status == 1 || status ==3) {
                    $(".attendA"+id).hide();
                    $(".attendB"+id).show();
                } else if(status == 2 || status == 0) {
                    $(".attendA"+id).show();
                    $(".attendB"+id).hide();
                }
            }
            function changeFocusNum(act,id,num) {
                if(act == "add") {
                    $(".follow" + id).find('a').html(parseInt($("#follow" + id).html()) + num);
                } else if(act == "del") {
                    $(".follow" + id).find('a').html(parseInt($("#follow" + id).html()) - num);
                } else if(act == "show") {
                    $(".follow" + id).find('a').html(num);
                }
            }

            function changeFocusStatus(id) {
                $.getJSON("http://bip.pcauto.com.cn/intf/focus.jsp?callback=?",
                {"act":"getNum","accountIds":id},
                function(data){
                    if(data.code == undefined) {
                        $.each(data,function(id,status) {
                            changeFanNum("show",id,status.fsNum);
                            changeFocusNum("show",id,status.fcNum);
                            changeBtnStatus(id,status.status);
                        });
                    }
                });
            }

