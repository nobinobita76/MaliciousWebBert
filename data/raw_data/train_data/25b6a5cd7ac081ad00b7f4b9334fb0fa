            <script type="text/javascript">
                ubb.common.smiliesShow2('smiliesdiv', 9, editorid + '_');
                var smilietabmenu = $(".smiliesdiv").find(".tabmenu");
                var smilietabctn = $(".smiliesdiv").find(".tabctn");
                smilietabmenu.click(function(){
                    var tindex = smilietabmenu.index(this);
                    $(this).addClass("tabmenu-current").siblings().removeClass("tabmenu-current");
                    smilietabctn.eq(tindex).show().siblings().hide();
                });
            </script>

        </div>
    </div>
</div>

<script type="text/javascript">
    var fastPostFlag = false;
    var sendingFlag = false;
    var advancePost = false;
    var postTextArea;

    var editorid = 'f';
    var editorcurrentheight = 103;
    var editorminheight = 103;
    var editormaxheight = 103;
    var textobj = textobj || $$(editorid + '_textarea');
    ubb.editor.newEditor(1, ubb.bbcode.bbcode2html(textobj.value));
    ubb.editor.setCaretAtEnd();
    $(document).ready(function () {
        var rapitSubmit = !!$.cookie('rapitSubmit');
        $('#rapitSubmit').attr('checked', rapitSubmit);
    });

    function setCheckboxCookie () {
        var value = "";
        if ($("#rapitSubmit").attr('checked')) {
            value = "1";
        }
        $.cookie("rapitSubmit", value, {expires: 30});
    }

    //fast_post
    if(!!$.cookie("common_session_id")){
        var iframe = document.getElementById("f_iframe").contentWindow.document.body;
        var iframeObj = document.getElementById("f_iframe").contentWindow;
        window.bindEvent=function(){
            sendingFlag = false;
            //$("#sendBtn").attr("src", "http://bbs.pcauto.com.cn/images/auto/btn_rapitreply.png");
            $("#sendA").removeClass("btn-sending").html("发表回复");
            $("#sendA").bind("click", fastpost);
            _attachEvent(iframe, "keydown", postWithEvent);
            if (document.getElementById("fast_post_title")) {
                $("#fast_post_title").bind("keypress",postWithEvent);
            }
            if (document.getElementById("fast_post_captcha")) {
                $("#fast_post_captcha").bind("keypress",postWithEvent);
            }
        };
        window.unbindEvent=function(){
            sendingFlag = true;
            if (!advancePost) {
                //$("#sendBtn").attr("src", $("#sendingBtn").attr("src") === "" ? "http://bbs.pcauto.com.cn/images/auto/btn_sending.png" : $("#sendingBtn").attr("src"));
                $("#sendA").addClass("btn-sending").html("回复中...");
            }
            $("#sendA").unbind("click", fastpost);
            _detachEvent(iframe, "keydown", postWithEvent);
            if (document.getElementById("fast_post_title")) {
                $("#fast_post_title").unbind("keypress",postWithEvent);
            }
            if (document.getElementById("fast_post_captcha")) {
                $("#fast_post_title").unbind("keypress",postWithEvent);
            }
        };
        window.postWithEvent=function(e) {
            if(e.ctrlKey && e.keyCode == 13 || e.keyCode == 10){
                _detachEvent(iframe, "keydown", postWithEvent);
                fastpost();
            }
        };
        window.fastpost=function(){
            if (!sendingFlag) {
                postTextArea = iframe;
                unbindEvent();
                setValue ();
                jiankong(676);
                $("#fast_post_form").submit();
            }
        };
        window.setValue=function() {
            fastPostFlag = true;
            $("#message_hidden").val(ubb.editor.getEditorData());
            if ($("#sendMsg")[0]) {
                $("#sendMsg_hidden").val($("#sendMsg").attr("checked") === "checked" ? "true" : "false");
            }
            if (document.getElementById("fast_post_title") && document.getElementById("fast_post_title").value !== "请输入标题") {
                $("#title_hidden").val($("#fast_post_title").val());
            }
            if (document.getElementById("fast_post_captcha")) {
                $("#captcha").val($("#fast_post_captcha").val());
            }
        };
        window.showFastRequest=function(formData,jqForm,options){
            var formValue =  $("#message_hidden").val().replace(/(^\s*)/g,'');
            var tip = $("#topicFlag").val() === "" ? "发帖内容" : "回复内容";
            if(formValue === ""){
                bindEvent();
                showMessage(tip + "必须填写！");
                return false;
            } else {
                var minLen = 1;
                var maxLen = 500000;
                if(formValue.length < minLen){
                    bindEvent();
                    showMessage(tip + "长度不得小于"+minLen);
                    return false;
                }
                if(formValue.length > maxLen){
                    bindEvent();
                    showMessage(tip + "长度不得大于"+maxLen);
                    return false;
                }
            }
            if (document.getElementById("captcha_span").style.display == "block") {
                if($("#fast_post_captcha").val() === ""){
                    bindEvent();
                    showMessage("请输入验证码！");
                    $("#fast_post_captcha").focus();
                    return false;
                }
            }
        };
        window.showFastResponse=function(responseText,statusText){
            var fastResult = new Function("return ("+responseText+");")();
            if(fastResult.status==0){
                var rapitSubmit = $("#rapitSubmit").attr('checked');
                if(rapitSubmit){
                    var redirectUrl = "http://bbs.pcauto.com.cn/topic-" + fastResult.tid + "--1.html#post_" + fastResult.pid;
                    redirectUrl = encodeURIComponent(redirectUrl);
                    window.location.href="http://bbs.pcauto.com.cn/redirect.do?url=" + redirectUrl;
                }else{
                    var request = $.ajax({
                        url: "reply_floor.do",
                        type: "POST",
                        data: {tid:fastResult.tid,pid:fastResult.pid},
                        success : function(msg){
                            editdoc.body.innerHTML = "";
                            $("#post_list").append(msg);
                            $body = (window.opera) ? (document.compatMode == "CSS1Compat" ? $('html') : $('body')) : $('html,body');// 这行是 Opera 的补丁, 少了它 Opera 是直接用跳的而且画面闪烁
                            $body.animate({scrollTop: $("#post_"+fastResult.pid).offset().top}, 500);
                            bindEvent();

                            if(fastResult.need_captcha){
                                initCaptcha2();
                            }
                        }
                    });
                }
                $.cookie('fast_post_form'+Topic.topicId, null);
            }else if(fastResult.status==-3){

                var date = new Date();
                date.setTime(date.getTime() + (5 * 60 * 1000));
                var inputMsg =  $("#message_hidden").val().replace(/(^\s*)/g,'');
                $.cookie('fast_post_form'+Topic.topicId, inputMsg, { expires: date });

                var returnURL = encodeURIComponent("http://bbs.pcauto.com.cn/topic-" + Topic.topicId + ".html");
                window.location.href= 'http://my.pcauto.com.cn/passport/opens/bbs_bind.jsp?return=' + returnURL;
            }else if(fastResult.status==6){

                var date = new Date();
                date.setTime(date.getTime() + (5 * 60 * 1000));
                var inputMsg =  $("#message_hidden").val().replace(/(^\s*)/g,'');
                $.cookie('fast_post_form'+Topic.topicId, inputMsg, { expires: date });

                var returnURL = encodeURIComponent("http://bbs.pcauto.com.cn/topic-" + Topic.topicId + ".html");
                window.location.href= 'http://my.pcauto.com.cn/passport/login.jsp?return=' + returnURL;
            }else{
                bindEvent();
                showMessage(fastResult.desc);
            }
            postTextArea = null;
        };
        $(document).ready(function(){
            var fastPostForm = $.cookie("fast_post_form"+Topic.topicId);
            if(fastPostForm!=null){
                document.getElementById("f_iframe").contentWindow.document.body.innerHTML= ubb.bbcode.bbcode2html(fastPostForm);
            }
        })
        bindEvent();
        $("#fast_post_form").ajaxForm({
            beforeSubmit:showFastRequest,
            success:showFastResponse,
            error:function(){showMessage("请求超时，请刷新页面查看最新回复")},
            timeout:5000});
    }

    $.ajaxSetup({
        type: 'POST',
        timeout: 100000,
        error: function(xhr) {
            $.unblockUI();
        }
    });

    function login (type) {
        if (type === 1) {
            location.href = "http://my.pcauto.com.cn/passport/opens/authorize.jsp?type=qzone_auto&return=" + location.href;
        } else if (type === 2) {
            location.href = "http://my.pcauto.com.cn/passport/opens/authorize.jsp?type=sina_auto&return=" + location.href;
        } else if (type == 3){
            location.href = "http://my.pcauto.com.cn/passport/opens/authorize.jsp?type=weixin_auto&return=" + location.href;
        }
    }
    function toAdvancePost (id, textareaId) {
        var postUrl = "";
        var postUrl = "http://bbs.pcauto.com.cn/reply.do?fid=21205&tid=12180537&action=reply&advance=true";
        var msgcookie;
        if(!!textareaId){
            msgcookie =  $("#"+textareaId).val();
        }else{
            msgcookie = ubb.editor.getEditorData()
        }
        if (msgcookie == '请输入内容') {
            msgcookie = '';
        }
        $.cookie("adMessage", msgcookie, {expire:1});
        if (document.getElementById("fast_post_title")) {
            if($("#fast_post_title").val() !== "请输入标题"){
                $.cookie("adTitle", $("#fast_post_title").val(), {expire:1});
            } else {
                $.cookie("adTitle", "", {expire:1});
            }
        }
        location.href = postUrl;
    }

    function initCaptcha2(){
        document.getElementById("captcha_span").style.display = "block";
        var imgv = $("#imgv");
        var src = imgv.attr("src");
        if(src == null || src == ""){
            changeCaptcha2();
        }
    }
    function changeCaptcha2() {
        var imgv = $("#imgv");
        if(imgv.css("display") == "none"){
            imgv.show();
        }
        document.getElementById("imgv").src = 'http://captcha.pcauto.com.cn/captcha/v.jpg?' + new Date().getTime();
    }
</script>

