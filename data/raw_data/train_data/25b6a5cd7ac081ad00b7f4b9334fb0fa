    <script type="text/javascript">
        $("#espl_fix").click(function(){
            var _this=$(this),
            sTop = document.documentElement.scrollTop || document.body.scrollTop;
            sTop+=document.documentElement.clientHeight;
            espbox = $("#espo_fix");
            if ((_this.offset().top+espbox.height)<=sTop){
                espbox.css({left:_this.offset().left,top:_this.offset().top,display:'block'})
            }else{
                espbox.css({left:_this.offset().left,top:_this.offset().top-espbox.height()-_this.height(),display:'block'})
            }
        });
        $("#espo_fix").find("td img").click(function(){
        	insertSmileyImage('fix118175990',this.getAttribute('title'));
            $("#espo_fix").hide();
        });
        document.body.onclick=function(event){closeEspo(event);};
        var closeEspo=function(e){
            var e=e||window.event;
            var obj=e.target||e.srcElement;
            if ('espl_fix'!=obj.id){
                document.getElementById("espo_fix").style.display="none";
            }
        };

        $("#msgfix118175990").keypress(function(e){
            if(e.ctrlKey && e.which == 13 || e.which == 10){
                $(".fix_reply").find(".fix_reply_con").removeClass("complete").addClass("simple");
                fixbox_place();
                postForm('fix118175990');
            }
        });

        $("#submitfix").click(function(e){
            $(".fix_reply").find(".fix_reply_con").removeClass("complete").addClass("simple");
            fixbox_place();
            jiankong(674);
            postForm('fix118175990');
        });

        $(".popupEmo").each(function(){
            var popupemoTime = null;
            var _this = $(this);
            _this.click(function(e){
                e.stopPropagation();
                if(_this.hasClass('popupEmo-open')){
                    _this.removeClass('popupEmo-open');
                    return;
                }
                if( typeof(window.iScroll) != 'undefined') _this.addClass('smiliesdiv2');
                _this.addClass("popupEmo-open");
            });
            $(document).click(function(){
                if(_this.hasClass('popupEmo-open')){
                    _this.removeClass('popupEmo-open');
                }
            });
            _this.bind("mouseEnter",function(){
                clearTimeout(popupemoTime);
            });
            _this.bind("mouseLeave",function(){
                clearTimeout(popupemoTime);
                popupemoTime = setTimeout(function(){
                    _this.removeClass("popupEmo-open");
                }, 200);
            })
            var tabmenu = _this.find(".tabmenu");
            var tabctn = _this.find(".tabctn");
            tabmenu.click(function(e){
                var tindex = tabmenu.index(this);
                $(this).addClass("tabmenu-current").siblings().removeClass("tabmenu-current");
                tabctn.eq(tindex).show().siblings().hide();
                e.stopPropagation();
            })
            tabctn.click(function(e){
                _this.removeClass("popupEmo-open");
                e.stopPropagation();
            })
        });
    </script>
