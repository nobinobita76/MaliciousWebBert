            function del_topic(tid){
                showMsgObj.html(showConfirmMsg);
                tb_confirm('删除自己的帖子会扣分,确定要删除此帖吗？',function(){
                    $.post("http://bbs.pcauto.com.cn/action/topic/delete.do",{tid:tid},function(data){
                        eval("var delResult=" + data);
                        if(delResult.status == 0){
                            showMsgObj.html(showSuccessMsg.replace('[:MSG]',"删除成功"));
                            tb_show_2('','#TB_inline?inlineId=showMsg&width=340',false);
                            tb_fadeOut(3000);
                            setTimeout("tb_redirect_forum()",2500);
                        }else{
                            showMsgObj.html(showErrorMsg.replace('[:MSG]',delResult.desc));
                            tb_show_2('','#TB_inline?inlineId=showMsg&width=310',false);
                        }
                    });}
            );
            }
            function del_post(tid,pid){
                showMsgObj.html(showConfirmMsg);
                tb_confirm('删除自己的帖子会扣分,确定要删除此帖吗？',function(){
                    $.post("http://bbs.pcauto.com.cn/action/post/delete.do",{tid:tid,pid:pid},function(data){
                        eval("var delResult=" + data);
                        if(delResult.status == 0){
                            showMsgObj.html(showSuccessMsg.replace('[:MSG]',"删除成功"));
                            tb_show_2('','#TB_inline?inlineId=showMsg&width=340',false);
                            tb_fadeOut(3000);
                            setTimeout("tb_redirect(window.location.href)",2500);
                        }else{
                            showMsgObj.html(showErrorMsg.replace('[:MSG]',delResult.message));
                            tb_show_2('','#TB_inline?inlineId=showMsg&width=310',false);
                        }
                    });}
            );
            }
            function tb_redirect_forum(){
                window.location.href="http://bbs.pcauto.com.cn/forum-21205.html";
            }
            
                function tb_redirect(href){
                    //window.location.href="http://bbs.pcauto.com.cn/redirect.jsp?url="+encodeURIComponent(href);
                    window.location.reload();
                }
                function showResponse(responseText,statusText){
                    var result = new Function("return ("+responseText+");")();
                    if(result.status==0){
                        window.location.href= window.location.href;
                    }else{
                        setTimeout(function(){showMessage(result.desc);},300);
                    }
                }
                function clickAutoLoadImg(on){
                    if(on){
                        $.cookie('autoloadimg','1',{path:'/',expires:30});
                        var selected = $('#chkAutoLoadImg').attr('checked');
                        if(!selected){
                            $('#chkAutoLoadImg').attr('checked',true);
                        }
                        jiankong(673);
                    }else{
                        $.cookie('autoloadimg','',{path:'/',expires:30});
                    }
                }
                (function(){
                    var autoloadimg = !!$.cookie('autoloadimg');
                    $('#chkAutoLoadImg').attr('checked',autoloadimg);
                })();

                function switchPage(pageNo){
                    var str = 'tid=12180537';
                    str = str.replace(/(^|&)pageNo=-?\d+/ig, '');
                    str = (str != '' ? str + '&' : '') + 'pageNo=' + pageNo;
                    return "http://bbs.pcauto.com.cn/topic.do?" + str;
                }
                function btnSurePress(o,e){
                    o.value=o.value.replace(/[^\d]/g,'');
                    var pNum = o.value;
                    if(pNum == null || pNum == '' || pNum == undefined || pNum == 0) {
                        pNum = 1;
                    }
                    pNum = pNum> 1 ? 1 : pNum;
                    if((window.event && e.keyCode == 13) || (e.which && e.which == 13)) {
                        window.location =switchPage(pNum);
                    }
                }

                /**
                 *   更新是否接受消息
                 */
                function changeStatus(sendMsgStatus){
                    var topicId = 12180537;
                    $.ajax({
                        url: "http://bbs.pcauto.com.cn/action/topic/change_sendmsg.ajax",
                        type: "POST",
                        data: {sendMsg : sendMsgStatus,tid:topicId},
                        success : function(msg) {
                            var data = eval("(" + msg + ")");
                            if(data.status == 0){
                                if (sendMsgStatus == 'true') {
                                    $("#cancelSendMsg").show();
                                    $("#noticeSendMsg").hide();
                                } else {
                                    $("#cancelSendMsg").hide();
                                    $("#noticeSendMsg").show();
                                }
                            }
                        }
                    });
                }

                function initCaptcha(id){
                    var imgv = $("#imgv" + id);
                    var src = imgv.attr("src");
                    if(src == null || src == ""){
                        changeCaptcha(id);
                    }
                }
                function changeCaptcha(id){
                    var imgId = "imgv" + id;
                    var imgv = $("#" + imgId);
                    if(imgv.css("display") == "none"){
                        imgv.show();
                    }
                    document.getElementById(imgId).src = 'http://captcha.pcauto.com.cn/captcha/v.jpg?' + new Date().getTime();
                }

                function recommend(fid) {
                    $.post("http://bbs.pcauto.com.cn/plugin/recommend/userRecommend.do?tid=12180537&fid="+fid, null, function(data){
                        eval("var result = " + data);
                        if (result.status == 0) {
                            showMessage("推荐成功");
                        } else {
                            showMessage(result.desc);
                        }
                    });
                }
                function redrawPager(pageNo, pageSize, totalSize, tid, byAuthorId){
                        var totalPage = Math.floor((totalSize-1)/pageSize) +1;
                        var showPage = 8;
                        var begin = 2;
                        var end = totalPage - 1;
                        var pagerHtml = "";
                        if(totalPage > 1) {
                            if(totalPage > showPage){
                                if(pageNo >= totalPage - 3){
                                    begin = totalPage - showPage + 1;
                                    end = totalPage - 1;
                                } else if(pageNo <= 4){
                                    begin = 2;
                                    end = showPage;
                                } else {
                                    begin = pageNo - 4 <= 1 ? 2 : pageNo - 4;
                                    end = pageNo + 3;
                                }
                            }

                            if(pageNo >1){
                                pagerHtml+=anchor(tid, byAuthorId, pageNo>1?pageNo-1:1 , '上一页', 'prev');
                            }
                            if(pageNo > 6 && totalPage > 9){
                                pagerHtml+=anchor(tid, byAuthorId, 1 , '1...', '');
                            }else{
                                if(pageNo != 1){
                                    pagerHtml+=anchor(tid, byAuthorId, 1 , '1', '');
                                }else{
                                    pagerHtml+='<span>1<\/span>';
                                }
                            }
                            for(var n = begin;n<=end;n++){
                                if(pageNo != n){
                                    pagerHtml+=anchor(tid, byAuthorId, n , n, '');
                                } else {
                                    pagerHtml+='<span>'+n+'<\/span>';
                                }
                            }
                            if(pageNo < totalPage-4 && totalPage > 9){
                                pagerHtml+=anchor(tid, byAuthorId, totalPage, '...'+totalPage, '');
                            }else{
                                if(pageNo != totalPage){
                                    pagerHtml+=anchor(tid, byAuthorId, totalPage , totalPage, '');
                                }else{
                                    pagerHtml+='<span>'+totalPage+'<\/span>';
                                }
                            }
                            if(totalPage > 1){
                                pagerHtml+='<i class=\"iNum\"><input type=\"text\" class=\"txt\" name=\"pageNo\" style=\"width:18px;\" value=\"'+pageNo+'\" onkeyup=\"btnSurePress(this,event);\"  title=\"输入页码，按回车跳转\"\/> /'+totalPage+'页<\/i>';
                            }
                            if(pageNo < totalPage){
                                pagerHtml+=anchor(tid, byAuthorId, pageNo < totalPage ? pageNo+1 : totalPage , '下一页', 'next');
                            }
                        }
                        $("#pager").html(pagerHtml);
                        $("#pagerBottom").html(pagerHtml);
                    }

                    function anchor(tid,byAuthorId,pageNo,text,c){
                        var topic_url = "http://bbs.pcauto.com.cn/topic";
                        if(!!byAuthorId && byAuthorId > 0){
                            topic_url += "_"+byAuthorId;
                        }
                        topic_url += "-"+tid;
                        if(!!pageNo && (pageNo > 1 || pageNo == -1)){
                            topic_url += "-"+pageNo;
                        }
                        topic_url+=".html";
                        return '<a class=\"'+c+'\" href=\"'+topic_url+'\">'+text+'</a>';
                    }

                    function setCMS(data){
                        var dlength = data.length - 1;
                        for (var i = 0, length = Topic.postLength; i < length; i++){
                            if(i%2 == 0){
                                var id = Math.floor(Math.random() * dlength);
                                var obj = data[id];
                                $("#middle_ad_" + i).html(obj);
                            }
                        }
                    }
                    function setTopics (data) {
                        if (data == undefined || data == null || data.length <= 0) {
                            $(".post_middle_block").attr("style","border:0");
                        } else {
                            var dlength = data.length;
                            for (var i = 0, length = Topic.postLength; i < length; i++){
                                if(i % 2 != 0){
                                    var id = Math.floor(Math.random() * dlength);
                                    var obj = data[id];
                                    var content = "<span class='middle_ad_title'>&nbsp;<a href='"+ obj.topicUrl
                                        + "' title='"
                                        + obj.title
                                        + "' target='_blank'>"
                                        + obj.title
                                        + "</a>";
                                    $("#middle_ad_" + i).html(content);
                                }
                            }
                        }
                    }

                    var showMsgObj = $("#showMsg");
                    function showMessage(message){
                        if(jQuery.browser.msie6){
                            alert(message);
                        }else{
                            showMsgObj.html(showErrorMsg.replace('[:MSG]',message));
                            setTimeout(function(){tb_show_2('','#TB_inline?inlineId=showMsg&width=340',false);},50);
                        }
                    }

                    function saveSuccess(){
                        var save=$('.save_success')
                        save.css({'display':'block'}).animate({
                            opacity:"1",
                            bottom: '+=30'
                        }, 500, function() {
                            setTimeout(function(){save.animate({
                                    opacity:"0",
                                    bottom: '-=30'
                                }, 300, function() {
                                    save.css({'display':'none'})
                                }); },800);

                        });
                    }
                    function priceAds(data) {
                        if(!!data){
                            var img = "<h4><a href=\""+data.url+"\" target=\"_blank\">"+data.name+"<\/a><\/h4>";
                            img+="<dl id=\"J_abdDl\">";
                            img+="<dt><a href=\"http:\/\/price.pcauto.com.cn\/sg"+data.id+"\/\" target=\"_blank\"> <img src=\""+data.fronPage+"\" alt=\""+data.name+"\"><\/a><\/dt>";
                            img+="<dd id=\"J_autoTitleBg\" class=\"autoTitleBg\"><\/dd>";
                            img+="<dd id=\"J_autoTitle\" class=\"autoTitle\"> <a href=\"http:\/\/price.pcauto.com.cn\/sg"+data.id+"\/\" target=\"_blank\"> "+data.name+"<\/a><\/dd>";
                            img+="<dd class=\"abd_dd3\"> <a href=\""+data.priceUrl+"\" target=\"_blank\">[报价]<\/a>&nbsp; <a href=\""+data.picUrl+"\" target=\"_blank\">[图库]<\/a>&nbsp; <a href=\""+data.configUrl+"\" target=\"_blank\">[参数]<\/a><\/dd><\/dl>";
                            $("#J_abdLeft").html(img);
                            var html = "<a href=\""+data.brandUrl+"\" target=\"_blank\">"+data.brandName+"品牌专区<\/a> ";
                            html += "<a href=\""+data.url+"\" target=\"_blank\">车系频道<\/a> ";
                            html += "<a href=\""+data.dealerUrl+"\" target=\"_blank\">经销商<\/a>";
                            html += "&nbsp; | &nbsp;";
                            $("#abd_ads_li").prepend(html);
                        }
                  }

            // 发放悬赏
            function doReward(pid) {
                $('.rewardBtn').attr('disabled', 'disabled'); // 禁用

                var rewardsParams = '';
                $('.rewardBtn').each(function(i){
                    var $thisPrev = $(this).prev();
                    var $pid = $thisPrev.attr('name').replace('reward', '');
                    var $point = $thisPrev.val();

                    rewardsParams += $pid + '-' + $point + ',';
                });

                var tid = '12180537';
                $.post('http://bbs.pcauto.com.cn/action/reward.do?tid='+tid+"&rewardsParams="+rewardsParams, {},
                function(data) {
                    if (data.status == 0) {
                        location.reload();
                    }
                }, 'json');
            }
        var userIdArray = [];
        var postIdArray = [];
        // 用户加入的车友会
        $('div.user_belong').each(function() {
            var idValue = $(this).attr('id').split('-'); // myClub--{uid}
            postIdArray.push(idValue[1]);
            userIdArray.push(idValue[2]);
        });
        $.getScript("http://club.pcauto.com.cn/bbsappapi/gf/1.0/userJoinClubs/get.do?source_agent=2&userIds=" + userIdArray.join(",") + "&callback=showAutoClub");
        function showAutoClub(resp) {
            if (!!resp) {
                for(var i = 0;i<userIdArray.length;i++){
                    var ja = resp[userIdArray[i]];
                    if(!!ja && ja.length > 0){
                        var clubs = '<span class="tit">所属：</span><div class="belong">';
                        for(var j =0;j<ja.length;j++){
                            var club = ja[j];
                            clubs+='<a class="dblue ';
                            if(club.isGf == 1){
                                clubs+='guan';
                                clubs+='" href="'+club.gfClubUrl+'" target="_blank">'+club.gfClubName+'</a>';
                            }else {
                                clubs+='" href="'+club.clubUrl+'" target="_blank">'+club.clubName+'</a>';
                            }

                        }
                        clubs+='</div>';
                        $('#myClub-'+postIdArray[i]+'-'+userIdArray[i]).css('display', '').html(clubs);
                    }
                }
            }
        }
        function applyPick() {
            $.post("http://bbs.pcauto.com.cn/action/topic/applyPick.do", {
                "fid": '13373',
                "tid": '12180537',
                "pid": ''
            }, callBack, "json");
        }

        function callBack(data) {
            if (data != null) {
                var status = data.status;
                var desc = data.desc;
                if (status == 1) { // 申请通过
                    if (desc == 'true') { // 楼主
                        tb_show_2('','#TB_inline?inlineId=author_applyPick_success&width=360',false);
                    } else {
                        tb_show_2('','#TB_inline?inlineId=applyPick_success&width=360',false);
                    }
                } else if (status == 2) { // 帖子未达标
                    tb_show_2('','#TB_inline?inlineId=applyPick_wrong&width=360',false);
                } else if (status == 3) { // 超过申请限制
                    if (desc == 'true') {
                        tb_show_2('','#TB_inline?inlineId=author_applyPick_warm&width=360',false);
                    } else {
                        tb_show_2('','#TB_inline?inlineId=applyPick_warm&width=360',false);
                    }
                } else if (status == 4) { // 已经申请，审核中
                    if (desc == 'true') {
                        tb_show_2('','#TB_inline?inlineId=author_applyPick_success&width=360',false);
                    } else {
                        tb_show_2('','#TB_inline?inlineId=applyPick_success_2&width=380',false);
                    }
                } else {
                    tb_show_2('','#TB_inline?inlineId=sys_note&width=360',false);
                    $('#tipbd-msg').text(desc);
                }
            }
        }
        
        (function(){
            var applypicktime = null;
            $('.applyPicklink').live('hover', function(event){
                if (event.type == 'mouseenter') {
                    var _this = $(this);
                    clearTimeout(applypicktime);
                    applypicktime = setTimeout(function(){
                        _this.addClass("applyPicklink-show");
                    }, 200);
                } else if (event.type == "mouseleave") {
                    var _this = $(this);
                    clearTimeout(applypicktime);
                    applypicktime=setTimeout(function(){
                        _this.removeClass("applyPicklink-show");
                    }, 200);
                }
            })
            var serialGroupName = "吉利博瑞论坛".replace("论坛", "");
            $("#baike").html(serialGroupName + "专区");
        })();
       var reg = "(#(.*?)#)";
        function jumpSubject(){
            var subjectTitle = "博瑞的一锤子解决右前轮异响呢？";
            var v = subjectTitle.match(reg);
            if(v && v.length>0){
                $.getJSON("http://club.pcauto.com.cn/subject/getSubjectByTitle.do?callback=?",{title:v[2].trim()},function(data){
                    if(data.code==0){
                        $("#subjectTitle").html(subjectTitle.replace(v[1],"<a href=\"http://club.pcauto.com.cn/topic/"+data.subjectId+".html\" target=\"_blank\">"+v[1]+"</a>"));
                    }
                });
            }
        }
        var uidArray =[];
        $(".banz").each(function(){
                uidArray.push($(this).attr("data-userId"));
        });
        $.getScript("http://bbs.pcauto.com.cn/getUserModeratorRole.do?ids=" + uidArray.join(",") + "&callback=showBanz");
        function showBanz(data){
            if(data.status==0){
                var array  = data.data;
                for(var i = 0,len=array.length; i<len; i++){
                    var html = "";
                    var result = array[i];
                    for(var j = 0 ,jlen = result.roleList.length; j < jlen ;j++){
                       html+="<i class=\"user-type\">"+result.roleList[j].forumName+result.roleList[j].roleName+"</i>";
                    }
                    $(".banz[data-userId=\""+result.uid+"\"]").each(function(){
                            if($(this).attr("hasload")=="false"){
                                $(this).append(html);
                                $(this).attr("hasload",true);
                            }

                    });
                }
            }
        }


