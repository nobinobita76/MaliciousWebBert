<script type="text/javascript">
    var loadTopicInfos = (function(){
        var o = {};
        var unique = function(s){
            if (!s && typeof s !== 'Array') return [];
            var newArray=[];
            var temp = {};
            var item = {};
            for (var i = 0 ; (item= s[i]) != null; i++) {
                if (!temp[item]) {
                    newArray.push(item);
                    temp[item] = true;
                }
            }
            return newArray;
        };
        var textsub = function(text,limitlen){
            if(typeof text === 'string'){
                if(text.length > limitlen){
                    return (text.substr(0,limitlen) + "…");
                }else{
                    return text;
                }
            }
            return "";
        };
        var face = function(uid, width){
            uid = uid+"";
            var f = "http:\/\/i6.3conline.com\/images\/upload\/upc\/face\/";
            for (var i = 0, c = uid.length; i < c; i += 2) {
                f+=uid.charAt(i);
                if (i < c - 1) {
                    f+=uid.charAt(i + 1);
                }
                f+='\/';
            }
            f+=uid+"_"+width+"x"+width;
            return f;
        };
        o.loadStatic = function(data){
            //vip showcar
            if(!!data.vipInfo){
                $.each(data.vipInfo, function(i, vip) {

                    var temp = "<li><i class=\"vip"+vip.vipid+" autobrand\" onmouseover=\"this.className+=' show'\" onmouseout=\"this.className = 'autobrand'\">";
                    if(vip.modelname !== '' && vip.fid > 0){
                        temp += "<a target=\"_blank\" class=\"dblue\" href=\"http://bbs.pcauto.com.cn/forum-"+vip.fid+".html\">";
                        temp += textsub(vip.serialname,4);
                        temp += "</a>";
                    }else{
                        temp += textsub(vip.serialname,4);
                    }
                    if (vip.serialname===''&& vip.modelname==='') {
                        temp += textsub(vip.brandname,4);
                    }
                    temp += "<span class=\"user_title\">";
                    temp += "<nobr>"+vip.serialname+" "+vip.modelname+ (vip.serialname===''&& vip.modelname==='' ? vip.brandname : "") + "</nobr>";
                    temp += "<ins></ins>";
                    temp += "</span></i>";
                    temp += "<img src=\"http://www1.pcauto.com.cn/bbs/2016/images/rz.png\" class=\"rz\" title=\"认证车主\">";
                    temp += "</li>";
                    $(".vip_ul" + vip.vipid).append(temp);
                });
            }

            //topic seo block
            if(!document.getElementsByClassName){
                document.getElementsByClassName = function(className, element){
                    var children = (element || document).getElementsByTagName('*');
                    var elements = new Array();
                    for (var i=0; i<children.length; i++){
                        var child = children[i];
                        var classNames = child.className.split(' ');
                        for (var j=0; j<classNames.length; j++){
                            if (classNames[j] == className){
                                elements.push(child);
                                break;
                            }
                        }
                    }
                    return elements;
                };
            }

            //展示粉丝数
            var postIds = [];
            $('.postids').each(function() {
                postIds.push(this.className.replace(/postid_([\d]+).*/ig,'$1'));
            });
            showFanNum(unique(postIds).join(','));
            function showFanNum(id) {
                $.getJSON("http://bip.pcauto.com.cn/intf/focus.jsp?callback=?",
                {"act":"getNum","accountIds":id},
                function(data){
                    if(data.code == undefined) {
                        $.each(data,function(id,status) {
                            changeFanNum("show",id,status.fsNum);

                        });
                    }
                });
            }

            //check pager
            if(!!data.totalSize && data.totalSize != 7 && true){
                redrawPager(1,19, 7,Topic.topicId, 0);
            }
            tb_init(".thickboxS");//enable tb on new anchors
            //广告表示样式
            if(data.isNotice){
                $("#subjectTitle").addClass("ADtitle");
            }
        };
        o.loadDynamic = function(data){
            if(data.status!=0){
                return ;
            }
            //reward&grade stat
            if(!!data.prgStats){
                var prgStats = data.prgStats;
                for(var i in prgStats){
                    var tempPost = prgStats[i];
                    var div = $("#post_operation_"+tempPost.pid);
                    div.attr("pid", tempPost.pid);
                    div.attr("aid", tempPost.aid);
                    div.attr("reward", tempPost.reward);
                    div.attr("gradeCount", tempPost.gradeCount);
                    div.attr("hasGrade", tempPost.hasGrade);
                    div.attr("floor", tempPost.floor);

                    var td = $("#post_msg_td_right_"+tempPost.pid);
                    td.attr("pid", tempPost.pid);
                    td.attr("aid", tempPost.aid);
                    td.attr("reward", tempPost.reward);
                    td.attr("gradeCount", tempPost.gradeCount);
                    td.attr("hasGrade", tempPost.hasGrade);
                    td.attr("floor", tempPost.floor);
                }
            }

            //uid>0
            if(data.user.uid > 0){
                if(data.user.uid == Topic.bestAnswerAuthorId){
                    $("#askToHimButton").hide();
                }

                $(".user_info").each(function(i, ele){
                    var div = "<div class=\"user-focus dblue\">";
                    var aid = $(ele).attr("authorId");
                    var ann = $(ele).attr("authorNickname");
                });


                if(data.user.uid == Topic.authorId || !!data.rights.allowSetBestAnswer){

                    if(true == true){
                        window.setBestReply=function(tid, pid) {
                            showMsgObj.html(showConfirmMsg);
                            tb_confirm('确定要设置此帖为最佳回复吗？', function () {
                                $.post("http://bbs.pcauto.com.cn/action/post/best_reply.do", {pid:pid, tid:tid}, function (data) {
                                    eval("var bestResult=" + data);
                                    if (bestResult.status == 0) {
                                        showMsgObj.html(showDoneMsg.replace('[:MSG]', "设置成功"));
                                        tb_show_2('', '#TB_inline?inlineId=showMsg&width=310', false);
                                        tb_fadeOut(2000);
                                        setTimeout("tb_redirect(window.location.href)", 2500);
                                    } else {
                                        showMsgObj.html(showErrorMsg.replace('[:MSG]', bestResult.desc));
                                        tb_show_2('', '#TB_inline?inlineId=showMsg&width=310', false);
                                    }
                                });
                            }
                        )
                        };
                    }
                    if(true==true){
                        window.setBestAnswer = function(tid, pid){
                            showMsgObj.html(showConfirmMsg);
                            tb_confirm('确定要设置此帖为推荐答案吗？', function () {
                                $.post("http://bbs.pcauto.com.cn/action/post/best_answer.do", {pid:pid, tid:tid}, function (data) {
                                    eval("var bestResult=" + data);
                                    if (bestResult.status == 0) {
                                        showMsgObj.html(showDoneMsg.replace('[:MSG]', "设置成功"));
                                        tb_show_2('', '#TB_inline?inlineId=showMsg&width=310', false);
                                        tb_fadeOut(2000);
                                        setTimeout("tb_redirect(window.location.href)", 2500);
                                    } else {
                                        showMsgObj.html(showErrorMsg.replace('[:MSG]', bestResult.desc));
                                        tb_show_2('', '#TB_inline?inlineId=showMsg&width=310', false);
                                    }
                                });
                            }
                        )
                        };
                        window.cancelBestAnswer = function(tid){
                            showMsgObj.html(showConfirmMsg);
                            tb_confirm('确定要取消此帖的推荐答案吗？', function () {
                                $.post("http://bbs.pcauto.com.cn/action/post/best_answer.do", {pid:0, tid:tid}, function (data) {
                                    eval("var bestResult=" + data);
                                    if (bestResult.status == 0) {
                                        showMsgObj.html(showDoneMsg.replace('[:MSG]', "设置成功"));
                                        tb_show_2('', '#TB_inline?inlineId=showMsg&width=310', false);
                                        tb_fadeOut(2000);
                                        setTimeout("tb_redirect(window.location.href)", 2500);
                                    } else {
                                        showMsgObj.html(showErrorMsg.replace('[:MSG]', bestResult.desc));
                                        tb_show_2('', '#TB_inline?inlineId=showMsg&width=310', false);
                                    }
                                });
                            }
                        )
                        };
                        if(false == true){
                            $("#setBestAnswerTips").show();
                        }
                    }
                }
                /* window.doReply = function(){
                             goCreateTopic("http://bbs.pcauto.com.cn/reply.do?fid=21205&tid=12180537&action=reply");
                        }; */
                window.openDiv = function(obj){
                    showDiv(obj);
                };
                window.addFocus = function(focusId,focusName,name){
                    if (addingFlag){
                        return;
                    }
                    addingFlag = true;
                    postCORS("http://bip.pcauto.com.cn/intf/focus.jsp",function(o){
                        o = new Function("return ("+$.trim(o)+");")();
                        var code = o.code;
                        if(typeof code != 'undefined'){
                            if(code == 0) {
                                showInfo('关注成功!<div style="margin-top:10px;"><a href="http://my.pcauto.com.cn" style="color:#336797;" target="_blank">进入个人中心看看</a></div>');
                                //   changeFanNum("add",focusId,1);
                                changeFocusStatus(focusId);
                                changeFocusNum("add", data.user.uid, 1);
                            } else if(code==-1){
                                //showInfo(data.message);
                                location.href = "http://my.pcauto.com.cn/login.jsp?return=http://bbs.pcauto.com.cn/topic.do?tid=12180537";
                                return;
                            } else {
                                showInfo(o.message);
                            }
                        }
                        addingFlag = false;
                    },
                    {
                        'act':'add',
                        'friendId':focusId,
                        'name':name,
                        'friendName':focusName,
                        "common_session_id":$.cookie("common_session_id")
                    },
                    {
                        'withCookie':true
                    }
                );
                };
                window.delFocus = function(focusId){
                    postCORS("http://bip.pcauto.com.cn/intf/focus.jsp?",function(o){
                        o = new Function("return ("+$.trim(o)+");")();
                        var code = o.code;
                        if(typeof code != 'undefined'){
                            if(code == 0) {
                                changeFocusStatus(focusId);
                                changeFocusNum("del", data.user.uid, 1);
                            } else if(code==-1){
                                location.href = "http://my.pcauto.com.cn/login.jsp?return=http://bbs.pcauto.com.cn/topic.do?tid=12180537";
                                return;
                            }
                        }
                    },
                    {
                        'act':'delete',
                        'friendId':focusId,
                        "common_session_id":$.cookie("common_session_id")
                    },
                    {
                        'withCookie':true
                    }
                );
                };

                //fast_post
                var faceImg=face(data.user.uid, 120);
                if(!!$.cookie("headpic")){
                    faceImg+='?ts='+new Date().getTime();
                }
                $("#fast_post_avatar").html("<a href=\"http://my.pcauto.com.cn\/"+data.user.uid+"\/\" target=\"_blank\"><img width=\"120\" height=\"120\" alt=\""+data.user.nickname+"\" src=\""+faceImg+"\" onload=\"this.width=120;this.height=120\"\/><\/a>");
