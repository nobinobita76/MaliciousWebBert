<script>
    function goPost(id,callback){
        addCount(id);
        if(callback){
            callback();
        }
    }
    function goPostByUrl(id,url){
        addCount(id);
        if(url){
            location.href=url;
        }
    }
    function addCount(id){
         tagid=id;
        var img = new Image();
        img.src="http://count.pcauto.com.cn/count.php?channel="+tagid+"&screen="+screen.width+"*"+screen.height+"&refer="+encodeURIComponent(document.referrer)
                +"&anticache="+new Date().getTime()+"&url="+encodeURIComponent(location.href)+"&from=event";
    }
    function goCreateTopic(url){
        goPostByUrl("195",url);
    }
</script>

