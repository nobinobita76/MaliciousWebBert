                            html+="<a class=\"editpost\" href=\"http://bbs.pcauto.com.cn/reply.do?action=edit&fid=21205&tid=12180537&pid="+pid+"\">修改<\/a>"
                        }
                    }
                    if(data.user.uid == Topic.authorId && floor > 1 && Topic.bestReplyId == 0 && aid != Topic.authorId && Topic.rewardAmount >0){
                        html+="<a class=\"setBestReply\" href=\"javascript:;\" onclick=\"setBestReply(12180537, "+pid+")\">设为最佳回复<\/a>";
                    }
                    html+="<a class=\"fastreply\" id=\"fastReply"+pid+"\" href=\"javascript:;\" onclick=\"Topic.toReplyForm('"+pid+"', '"+floor+"')\">回复本楼<\/a>";
                    //-viewpost
                    if(!!rights.allowGrade && data.user.uid != aid){
                        html+="<a href=\"http://bbs.pcauto.com.cn/toGrade.do?fid=21205&tid=12180537&pid="+pid+"&action=grade&height=208&width=310&page=topic\" class=\"thickboxD score\" title=\"评分\">评分<\/a>";
                    }
                    if(((!!rights.allowCancelGrade && !!data.hasGrade) || !!rights.allowCancelOthersGrade) && gradeCount > 0 && hasGrade){
                        html+="<a href=\"http://bbs.pcauto.com.cn/toCancelGrade.do?fid=21205&tid=12180537&pid="+pid+"&action=grade&height=400&width=620&page=topic\" title=\"\" class=\"thickboxD modlink\">撤销评分<\/a>";
                    }
                    if(!!rights.allowReportAbuse && data.user.uid > 0 && data.user.uid != aid){
                        html+="<a href=\"http://bbs.pcauto.com.cn/toReport.do?fid=21205&tid=12180537&pid="+pid+"&action=reportAbuse&height=208&width=310&page=topic\" class=\"thickboxD report\" title=\"举报\">举报<\/a>";
                    }
                    if(rights.admin && (!!rights.allowDeleteTopic || !!rights.allowGrade)){
                        html+="&nbsp;<label for=\"manage"+pid+"\"><input type=\"checkbox\" id=\"manage"+pid+"\" name=\"moderate[]\" onclick=\"pidchecked(this);modclick(this, "+pid+");\" value=\""+pid+"\" class=\"vc\"/>管理<\/label>";
                    }
                    $(ele).html(html);
                });


                //post button
                if ('21665' != '21205') {
                    $("ul.btn_ul_up").append("<li class=\"question\"><a href=\"javascript:goCreateTopic('http://bbs.pcauto.com.cn/new/post.do?type=question&fid=21205#ad=10769')\">发提问帖</a></li>");
                    $("ul.btn_ul_bottom").append("<li class=\"question\"><a href=\"javascript:goCreateTopic('http://bbs.pcauto.com.cn/new/post.do?type=question&fid=21205#ad=10770')\">发提问帖</a></li>");
