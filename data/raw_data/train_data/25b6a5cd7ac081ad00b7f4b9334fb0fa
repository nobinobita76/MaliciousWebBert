                    }
                    if(!!rights.allowPostRewardTopic){
                        $("ul.btn_ul_up").append("<li class=\"reward\"><a href=\"javascript:goCreateTopic('http://bbs.pcauto.com.cn/post.do?fid=21205&hasReward=true#ad=10769')\">发悬赏</a></li>");
                        $("ul.btn_ul_bottom").append("<li class=\"reward\"><a href=\"javascript:goCreateTopic('http://bbs.pcauto.com.cn/post.do?fid=21205&hasReward=true#ad=10770')\">发悬赏</a></li>");
