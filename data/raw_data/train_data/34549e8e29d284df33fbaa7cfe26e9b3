                                var _tmpl = jQuery("#tmplRankRow").html();
                                function renderRank(period) {
                                    var data = _wow_vRank[period];
                                    if(data){
                                        var con = jQuery("#rank_" + period).html("");
                                        jQuery(data).each(function(i,article){
                                            if(i>=6) return false;
