<script>
function chanEpRank() {
    var elem = document.getElementById("chanRank"),
        _html = '',
        _title;
    if ( !elem ) { return false };
    if ( typeof(rank_top) != "object" ) { return false };

    for ( var i=0; i<10; i++ ) {
        if ( !rank_top[i] ) { break };
        _title = unescape(rank_top[i].title).replace("(图)", '<span class="title_blue">图</span>');
        _html += '<li><a href="'+ rank_top[i].location +'" target="_blank">'+ _title +'</a></li>';
    };
    elem.innerHTML = _html;
};
chanEpRank();
</script> -->
    <div class="side_dvs padtop10">
