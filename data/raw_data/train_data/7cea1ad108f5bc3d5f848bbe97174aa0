  (function(cid){
	var cid = cid;
	var hotData = null;
	var newDomain="";
	var uiHotBook = $("#uiHotBook");
	var ulHotBook = $("#ulHotBook");
        var btMore = $("#btHotMore");
        btMore.on("click", function(e){getHotData();});
        function fill(){
	    if(!hotData)return;
            var coll = hotData.bookcoll;
            var aaa = new Array();
            for (var i = 0; i < coll.length; i++) {
                aaa.push("<li>");
                aaa.push("<a href=\""+newDomain+"/book/"+coll[i].bookid+".html\">");
		aaa.push("<div class=\"portrain-con\">");
                aaa.push("<div class=\"zw_img\"><img src=\"" + coll[i].bookcover + "\" alt=\""+coll[i].bookname+"\"/></div>");
                aaa.push("<p>" + coll[i].bookname + "</p>");
                aaa.push("</div></a></li>");
            }
            ulHotBook.html(aaa.join(""));
        };
        function getHotData(){
            var url = "/json/hotbook_json.aspx?cid=" + cid;
            $.get(url,function(res) {
                hotData = $.parseJSON(res);
		fill();
            });
        };})(2);
  </script>

