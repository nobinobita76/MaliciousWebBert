<script>
    ;$(function(){
        changeHref($("#zyb"));
        function changeHref($element){
            //element是jQuery对象
            if(!$element){
                return
            }
            $element.find("a").bind("click",function(){
                var $ele=$(this);
                if($ele.attr("data-init")!=="true")
                {
                    var href="http://go.focus.cn/url="+$ele.attr("href");
                    $ele.attr("href",href);
                    $ele.attr("data-init","true");
                }
            });
        }
        var action = "detail";
        var form_id = "";
        if(action == 'preview'){
            $( '#j-pagination-news a').bind('click', function(event){
                var $a_link = $(this);
                var href = $a_link.attr('data-href');
                if( form_id.length == 0 || href.indexOf('news.focus.cn') == -1){
                    return true;
                }
                event.preventDefault();
                event.stopPropagation();
                var $form = $('#' + form_id);
                $form.attr('action', href);
                $form[0].submit();
                return false;
            });
        }

    });
</script>
