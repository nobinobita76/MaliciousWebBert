<script>

	// 반응형 웹의 모바일 최대 가로길이
	var limitWidthOfMobile = 800;
	
			var isAndroidMobile = false;
		
	var resize = {
		executeWhenWindowResize : function(execFunc) {
			var curWidth = $(window).width();
			$(window).resize(function() {
				var nowWidth = $(window).width();
				if (curWidth != nowWidth) {
					execFunc();
				}
			});
		},
		executeResizeImmediately : function(execFunc) {
			$(window).resize(function() {
				execFunc();
			});
		}
	};
	
</script>

