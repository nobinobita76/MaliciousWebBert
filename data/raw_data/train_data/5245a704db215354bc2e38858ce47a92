    <SCRIPT type="text/javascript">

        function chcolor() {
            $('.txt-focus').css('color', ($(this).value == '请输入关键词') ? '#999' : '#000');
        }

        function checkserAgent() {
            var userAgentInfo = navigator.userAgent;
            if ((userAgentInfo.match(/Android/i))) { return true; }
            if ((userAgentInfo.match(/iPhone/i))) { return true; }
            if ((userAgentInfo.match(/SymbianOS/i))) { return true; }
            if ((userAgentInfo.match(/Windows Phone/i))) { return true; }
            if ((userAgentInfo.match(/iPad/i))) { return false; }
            if ((userAgentInfo.match(/iPod/i))) { return true; }
            if ((userAgentInfo.match(/MQQBrowser/i))) { return true; }
            return false;
        }

        function getParameter(paraStr, url) {
            var result = "";
            //获取URL中全部参数列表数据
            var str = "&" + url.split("?")[1];
            var paraName = paraStr + "=";
            //判断要获取的参数是否存在
            if (str.indexOf("&" + paraName) != -1) {
                //如果要获取的参数到结尾是否还包含“&”
                if (str.substring(str.indexOf(paraName), str.length).indexOf("&") != -1) {
                    //得到要获取的参数到结尾的字符串
                    var TmpStr = str.substring(str.indexOf(paraName), str.length);
                    //截取从参数开始到最近的“&”出现位置间的字符
                    result = TmpStr.substr(TmpStr.indexOf(paraName), TmpStr.indexOf("&") - TmpStr.indexOf(paraName));
                }
                else {
                    result = str.substring(str.indexOf(paraName), str.length);
                }
            }
            else {
                result = "无此参数";
            }
            return (result.replace("&", ""));
        }

        var r = getParameter("fr", window.location.href);
        if (!(r == "fr=mb") && checkserAgent()) window.location.href = "http://m.cankaoxiaoxi.com";

        function slideFunc(json) {
            //外框
            var oDiv = json.oDiv;
            //图片列表
            var oUl = json.oUl;
            //序列点
            var oOl = json.oOl || null;

            if (oOl) {
                var oLi = oOl.children;
            }

            var oImgLi = oUl.getElementsByTagName("li");
            var oPrev = json.oPrev || null;
            var oNext = json.oNext || null;

            var nWidth = oImgLi[0].offsetWidth;
            var now = 0;

            oUl.style.width = oImgLi.length * nWidth + "px";
            var timer = null;

            //timer = setInterval(slide, 3000);

            oDiv.onmouseover = function () {
                clearInterval(timer);
                if (oPrev) {
                    oPrev.style.display = "block";
                    oNext.style.display = "block";
                }

            };
            oDiv.onmouseout = function () {
                //timer = setInterval(slide, 3000);
                if (oPrev) {
                    oPrev.style.display = "none";
                    oNext.style.display = "none";
                }
            };

            if (oOl) {
                for (var i = 0; i < oLi.length; i++) {
                    oLi[i].index = i;

                    oLi[i].onmouseover = function () {
                        now = this.index;
                        oUl.style.left = -now * nWidth + "px";
                        var j = 0;
                        for (j = 0; j < oLi.length; j++) {
                            oLi[j].className = "";
                        }
                        oLi[now].className = "on";
                        clearInterval(timer);
                    }
                }
            }

            if (oPrev) {
                oNext.onclick = function () {
                    slide();
                    return false;
                }

                oPrev.onclick = function () {
                    var num = oImgLi.length;

                    if (now == 0) {
                        now = num - 1;
                    } else {
                        now--;;
                    }
                    oUl.style.left = -now * nWidth + "px";
                    if (oOl) {
                        for (i = 0; i < oLi.length; i++) {
                            oLi[i].className = "";
                        }
                        oLi[now].className = "on";
                    }
                    return false;
                }
            }

            function slide() {
                var num = oImgLi.length;
                if (now >= (num - 1)) {
                    now = 0;
                } else {
                    now++;
                }
                oUl.style.left = -now * nWidth + "px";
                if (oOl) {
                    for (i = 0; i < oLi.length; i++) {
                        oLi[i].className = "";
                    }
                    oLi[now].className = "on";
                }
            }
        };

        //幻灯片
        $(function () {

            $(".favorite").click(function () {
                $('.addfav_pop_p0').show();
            });

            $('.addfav_close').click(function () {
                $('.addfav_pop_p0').hide();
            });

            var oDiv = document.getElementById("slide");
            var oUl = oDiv.getElementsByTagName("ul")[0];
            var oImgLi = oUl.getElementsByTagName("li");
            var oPrev = document.getElementById("btn_prev");
            var oNext = document.getElementById("btn_next");

            oUl.style.width = oImgLi.length * oImgLi[0].offsetWidth + "px";
            var timer = null;

            timer = setInterval(slide, 3000);

            oDiv.onmouseover = function () {
                clearInterval(timer);
                //$('.blk_jdt_nextbtn').show();
                //$('.blk_jdt_prebtn').show();
            };
            oDiv.onmouseout = function () {
                timer = setInterval(slide, 3000);
                //$('.blk_jdt_nextbtn').hide();
                //$('.blk_jdt_prebtn').hide();
            };


            oNext.onclick = function () {
                slide();
                return false;
            }

            oPrev.onclick = function () {
                var left = oUl.offsetLeft;
                var index = Math.ceil(-left / 320);
                var num = Math.ceil(oUl.offsetWidth / 320);
                if (index == 0) {
                    index = num - 1;
                } else {
                    index -= 1;
                }
                oUl.style.left = -index * 320 + "px";

                return false;
            }

            function slide() {
                var i = 0;
                var left = oUl.offsetLeft;
                var index = Math.ceil(-left / 320);
                var num = Math.ceil(oUl.offsetWidth / 320);
                if (index >= (num - 1)) {
                    index = 0;
                } else {
                    index += 1;
                }
                oUl.style.left = -index * 320 + "px";

            }
            YlMarqueeFn('scroll-verticale');

            slideFunc({
                "oDiv": $('#slide_quanqiu_wrap')[0],
                "oUl": $('#slide_quanqiu')[0],
                "oOl": $('#quanqiu_index')[0],
                "oPrev": $('#btn_prev_quan')[0],
                "oNext": $('#btn_next_quan')[0]
            });

            slideFunc({
                "oDiv": $('#slide_tuwen_wrap')[0],
                "oUl": $('#slide_tuwen')[0],
                "oOl": $('#tuwen_index')[0],
                "oPrev": $('#btn_prev_tw')[0],
                "oNext": $('#btn_next_tw')[0]
            });

            slideFunc({
                "oDiv": $('#slide_rui_wrap')[0],
                "oUl": $('#slide_rui')[0],
                "oOl": $('#rui_index')[0],
                "oPrev": $('#btn_prev_rui')[0],
                "oNext": $('#btn_next_rui')[0]
            });

            slideFunc({
                "oDiv": $('#slide_lbsm_wrap')[0],
                "oUl": $('#slide_lbsm')[0],
                "oOl": $('#lbsm_index')[0],
                "oPrev": $('#btn_prev_lbsm')[0],
                "oNext": $('#btn_next_lbsm')[0]
            });

            slideFunc({
                "oDiv": $('#slide_milweek_wrap')[0],
                "oUl": $('#slide_milweek')[0],
                "oOl": $('#milweek_index')[0],
                "oPrev": $('#btn_prev_milweek')[0],
                "oNext": $('#btn_next_milweek')[0]
            });

            slideFunc({
                "oDiv": $('#slide_war_wrap')[0],
                "oUl": $('#slide_war')[0],
                "oOl": $('#war_index')[0],
                "oPrev": $('#btn_prev_war')[0],
                "oNext": $('#btn_next_war')[0]
            });

            slideFunc({
                "oDiv": $('#slide_office_wrap')[0],
                "oUl": $('#slide_office')[0],
                "oOl": $('#office_index')[0],
                "oPrev": $('#btn_prev_office')[0],
