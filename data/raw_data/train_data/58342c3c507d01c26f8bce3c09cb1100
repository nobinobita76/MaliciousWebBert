        'dianying' : '1e4dba3bcd0ba732d24e88da011b7927',
        'dongman' : 'a9688c7c918f13b06b9a21d94d464c6a',
        'tv' : 'af091c04eda0bbcafac6d8b29c0e4943',
        'v' : '38dbd999e98c658cb904bf28503bc253'
    };
    (function(){
        var host = window.location.host;
        if(typeof host == 'undefined' || !host) return;
        var type = host.split('.');
        if(typeof TONGJI_CONFIG[type[0]] == 'undefined' || !TONGJI_CONFIG[type[0]]) return;
        var src = document.location.protocol +'//js.passport.qihucdn.com/11.0.1.js?' + TONGJI_CONFIG[type[0]];
        document.write('<script src="' + src + '" id="sozz"><\/script>');
    })();
</script>
