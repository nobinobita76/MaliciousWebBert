  <script>
    (function () {
        var cur = 1,
            total = 25;

        new Pagination({
            totalPage: total,
            curPage: cur
        }, function (pageNum) {
            var url = location.href;

            if (url.indexOf('?pn=') > -1 || url.indexOf('&pn=') > -1) {
                url = url.replace(/([?&]pn=)[^&]*/, '$1' + pageNum);
            } else {
                url += (url.indexOf('?') == -1 ? '?' : '&') + 'pn=' + pageNum
            }

            location.href = url;
        });
    })();
</script> 
