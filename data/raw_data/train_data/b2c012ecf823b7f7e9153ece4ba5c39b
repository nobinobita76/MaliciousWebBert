    <script type="text/javascript">
    var qianbaoLogin = "http://passport.wanmei.com/accounts/serviceLogin?service=ewallet&continue=https://www.xunbaozhifu.com/Login&location=cross";
    $('#searchBox').keyup(function(e) {
		if(e.keyCode == 13) {
			var keyWord = $.trim($('#searchBox').val());
			if(!keyWord)
				return;
   			var url = location.href; 
	   		if(url.indexOf("?") > 0) {
	   			url = url.substring(0,url.indexOf("?"));
	   		}
   			location.href = url + "?keyWord=" + encodeURI(keyWord);
   			return false;
		}
	});
	$('#btnSearch').click(function(e) {
		var keyWord = $.trim($('#searchBox').val());
		var url = location.href; 
  		if(url.indexOf("?") > 0) {
  			url = url.substring(0,url.indexOf("?"));
  		}
		location.href = url + "?orderBy=unitprice-asc&keyWord=" + encodeURI(keyWord);
		return false;
	});

	if($('#confirmForbid').val() == 'true') {
		var pop = new Pop("您的账号被封禁了", $('#forbidText').val().replace(/\\n/g, '<br>'), $('#hiddenPop'), function() {$.post("confirmForbid.do");});
		pop.alert();
	}

	 var isLogin = false;
		var comfirmInfo = "登录才能进行该操作                                 \n您要登录吗？";
		if(!isLogin) {
			$('a').click(function(e) {
				var href = $(this).attr('href');
				if(href.indexOf(".do")>-1){
					var pop = new Pop("请您登录", "登录后才能进行该操作，您要登录吗？", $('#hiddenPop'), function() {location.href = href;});
					pop.confirm();
					return false;
				}
				
				//if(!confirm(comfirmInfo)) {
				//	return false;
				//}
			});
		}

		//$('#ewallet, .ewallet').click(function() {
   		//	$.getScript(qianbaoLogin+'&t='+ new Date().getTime() , function() {
       	//		location.href="https://www.xunbaozhifu.com";
