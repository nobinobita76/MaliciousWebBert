	{
		document.getElementById("mation5").style.position = "fixed";
	}
	else if(document.getElementById("mation6"))
	{
		document.getElementById("mation6").style.position = "fixed";
	}
}

if(document.getElementById("mation5"))
{
	var ww=document.documentElement.clientWidth / 2-290 ;
