<script type="text/javascript">
  	var _superArea = $("#js_super_area")/*.find("ul")*/;
  	var _superList = $("#js_super_list").find("li");
  	var _newSuperNum = 0,_isMove=false;
  
    //移动超级专区位置
    function MoveSuperAreaFunction(location){
      	/*if(_superList && _newSuperNum >= 0) _superArea.stop(true,false).animate({"top":location+"px"},1000,ChangeSuperListFunction);*/
      if(_superList && _newSuperNum >= 0) _superArea.stop(true,false).animate({"scrollTop":(parseInt(_newSuperNum) * 500)+"px"},1000,function(){_isMove = false;ChangeSuperListFunction();});
    }
  
  	//改变超级之处列表样式
    function ChangeSuperListFunction(){
        if(_superList && _superList.length > 0 && _newSuperNum >= 0 && _newSuperNum < _superList.length){
          	_superList.removeClass("cur");
          	$(_superList[_newSuperNum]).addClass("cur");
        }
    }
  
    function MouseWheelFunction(event){
      	event.stopPropagation();
      	event.preventDefault();
        var detail = 0;
        event = event || window.event;
        if(event.wheelDelta){
            //当前是IE、谷歌浏览器
            detail = event.wheelDelta;
        }
        else if(event.detail){
            //当前是火狐浏览器
            detail = -event.detail;
        }
        
        if(!_isMove){
          	_isMove = true;
          	if(detail > 0){
          		//当前是向上移动
              	_newSuperNum--;
              	if(_newSuperNum < 0) _newSuperNum = 0;
              	MoveSuperAreaFunction(parseInt(_newSuperNum) * 500);
        	}
        	else{
          		//当前是向下移动
              	_newSuperNum++;
              	if(_superList && _newSuperNum >= _superList.length) _newSuperNum = _superList.length - 1;
              	MoveSuperAreaFunction(parseInt(_newSuperNum) * 500);
        	}
        }
      
        return false;
    }
  
    $(window).ready(function(){
      	//得到数据
      	getData();

      	//初始化当前超级之处列表位置
      	_newSuperNum = 0;
      
      	var super_area = document.getElementById("js_super_area");
        if(super_area.addEventListener) super_area.addEventListener("DOMMouseScroll",MouseWheelFunction,false);
      	super_area.onmousewheel = MouseWheelFunction;

       /* 
       _superArea.scroll(function(event){
       		var scrollTop = $(this).scrollTop();
          	_newSuperNum = parseInt(scrollTop / 500) + ((scrollTop % 500) > 0 ? 1 : 0);
          
            if(!_isMove){
                _isMove = true;
                MoveSuperAreaFunction();
            }
          
            ChangeSuperListFunction();
        });
        */
      
        _superList.bind("click",function(event){
          	_isMove = true;
          	_newSuperNum =  $(this).attr("list-num");
          	MoveSuperAreaFunction();
        });
      
        $("#js_phone_type").find("li").on({
            "mouseenter":function(event){
                $("#js_phone_type").find("li").removeClass("cur");
                $(this).addClass("cur");
              	if($(this).attr("type-value") == "le_2"){
                  	if($("#js_le_2_product").hasClass("hidden"))$("#js_le_2_product").removeClass("hidden");
                  	if(!$("#js_le_1_product").hasClass("hidden"))$("#js_le_1_product").addClass("hidden");
                }else{
                  	if($("#js_le_1_product").hasClass("hidden"))$("#js_le_1_product").removeClass("hidden");
                  	if(!$("#js_le_2_product").hasClass("hidden"))$("#js_le_2_product").addClass("hidden");

                }
            }
        });
    });
</script>
