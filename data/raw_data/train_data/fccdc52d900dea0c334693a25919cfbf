//構建百度分享URL
function createbdShareUrl(url){
	if(typeof url != "undefined" && url.length > 0){
		if(url.indexOf("http") >= 0){
			return url;
		}else{
          	return url.indexOf("/") == 0 ? ("http://" + window.location.host + url ) : ("http://" + window.location.host + url);
		}
	}
	return url;
}

