<script type="text/javascript">
  	var _friendly_one_element,_friendly_two_element,_friendly_one_top = 0,_friendly_two_top = 0,_isMoveFriendly = true;
    /**
     ** 檢查鼠標事件
     ** 功能：用於鼠標事件沖突的兩個元素
     ** by wangsheng
     **/
    function CheckMouseEvent(){
        /**
         ** 檢查觸發鼠標事件的對象
         ** 功能：檢查當前觸發鼠標事件的元素，與鼠標監聽對象的關系
         **
         ** 參數：
         ** mouseEvent 	當前鼠標事件
         ** now_mc		添加此事件的元素
         **
         ** 返回值： Bollon  true 同壹元素/上下級關系；false 兩者沒有關系
         **/
        this.checkMouseEventElement = function(mouseEvent,new_event_mc){
            //出發事件的對象
            var eventMc;
    
            //得到當前事件
            mouseEvent = mouseEvent || window.event;
    
            //得到當前觸發鼠標事件的對象
            if(mouseEvent.type == "mouseover"){
                eventMc = mouseEvent.formElement || mouseEvent.relatedTarget;
            }
            else if(mouseEvent.type == "mouseout"){
                eventMc = mouseEvent.toElement || mouseEvent.relatedTarget;
            }
    
            //檢查元素之間的關系
            //判斷當前瀏覽器是否為IE，如果document.all存在，則為IE；否則是其他瀏覽器
            if(document.all){
                //判斷添加此鼠標事件監聽的元素(new_event_mc)，是否與當前觸發鼠標事件的元素(eventMc)，存在包含關系。
                return new_event_mc.contains(eventMc);
            }
            else{
                /**
                 ** a.compareDocumentPosition(b)有五個值：
                 ** 若為零，表示相同節點；
                 ** 若為2 ，表示a位於b後面；
                 ** 若為4 ，表示a位於b前面；
                 ** 若為10，表示a為b的後代；
                 ** 若為20，表示a為b的祖級。
                 ** 註：返回值為零和20，表示有包含關系，其他值為沒有包含關系。
                 **/
                var reg = new_event_mc.compareDocumentPosition(eventMc);
                if(reg == 0 || reg == 20) return true;
            }
    
            return false;
        }
    }
  
  	function CheckDataFun(data){
    	if(data != null && data != undefined && data != "" && data != " " && data != "null" && data != "undefined"){
          	return true;
        }
    	return false;
  	}
  
  	$(window).load(function(){
      	_isMoveFriendly = true;
      	//得到友情鏈接節點數據
       	_friendly_one_element = $("#js_friendly_one");
  		_friendly_two_element = $("#js_friendly_two");
      
      	var checkmouse = new CheckMouseEvent();
      
      	//初始化友情鏈接位置
      	if(CheckDataFun(_friendly_one_element)) _friendly_one_element.css("top","0");
        if(CheckDataFun(_friendly_two_element)) _friendly_two_element.css("top",_friendly_two_element.height());

      	$("#js_friendly_bg").on({
          	"mouseover":function(event){
                //停止滾動友情鏈接
                if(!checkmouse.checkMouseEventElement(event,this)) StopMoveFriendlyFun();
            },
            "mouseout":function(event){
                //繼續滾動友情鏈接
                if(!checkmouse.checkMouseEventElement(event,this)) StratMoveFriendlyFun();
            }
        });
      
      	//友情鏈接初始位置
      	_friendly_one_top = _friendly_one_element.position().top;
        _friendly_two_top = _friendly_two_element.position().top;
      
      	//滾動友情鏈接
      	StratMoveFriendlyFun();
  	});
  
  	//開始移動友情鏈接
    function StratMoveFriendlyFun(){
      	_isMoveFriendly = true;
      	_friendly_one_element.stop(true,false).animate({"top" : _friendly_one_top - 20},2000,"linear");
      	_friendly_two_element.stop(true,false).animate({"top" : _friendly_two_top - 20},2000,"linear",CompleteMoveFriendlyFun);
  	}
  
  	//停止移動友情鏈接
  	function StopMoveFriendlyFun(){
      	_isMoveFriendly = false;
      	_friendly_one_element.stop(true,false);
      	_friendly_two_element.stop(true,false);
  	}
  
  	//友情節點位置移動完成
  	function CompleteMoveFriendlyFun(){
    	if(_friendly_one_element.position().top + _friendly_one_element.height() <= 0) _friendly_one_element.css("top",_friendly_one_element.height());
   	 	if(_friendly_two_element.position().top + _friendly_two_element.height() <= 0) _friendly_two_element.css("top",_friendly_two_element.height());
      	
      	_friendly_one_top = _friendly_one_element.position().top;
      	_friendly_two_top = _friendly_two_element.position().top;
      
    	//繼續移動友情節點位置
    	if(_isMoveFriendly) StratMoveFriendlyFun();
  	}
</script>
	<!-- /* 页尾模块 结束 */ -->
      
