<script type="text/javascript">
  var timeOutList = {};
  $(document).ready(function () {
      //刷新頭部登陸狀態	 
    //refreshHeader();
     userLogin.refreshHead();
  });

  function refreshHeader() {
        if (Js.login.getAuth()) {
            var a = getCookie("COOKIE_NICKNAME");
            //處理cookie用戶名異常
            try{
              a = 10 < a.length ? a.substring(0, 10) + "..." : a;}
            catch(e){}
            $("#loginMess").html('<span class="gray">Hi,</span><span class="pl5 pr5">' + a + '</span><a rel="nofollow" webtrekkparam="{ct:"3.0sy_dh1_out"}" id="logout"><span class="pl5 pr15">[退出]</span></a>');
            $("#logout").click(function () {
                Js.login.logout(function () {
                    window.location.href = "/"
                })
            })
        } else $("#login").click(function () {
          top.location.href = "/login.html?from=" + encodeURIComponent(location.href);
          //Js.login.hasLogin(function () {
          //location.reload()
          //})
        })
    };
</script>
