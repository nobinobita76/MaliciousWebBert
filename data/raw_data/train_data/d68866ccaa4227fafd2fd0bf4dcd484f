<script type="text/javascript">
    function platformClick(url, isGN) {
        var mhost = location.host;
        if (isGN == false)
        {
            mhost = "international";
        }
        var str = location.href;
        var num = str.indexOf(mhost);
        var strParameter = str.substring(num + mhost.length);
        if (strParameter.indexOf("?") > 0) {
            location.href = url + strParameter + "&platform=pc";
        }
        else {
            location.href = url + strParameter + "?platform=pc";
        }
    }
</script>
