<script type="text/javascript">

//获取推广参数
function getUrlParamIfNum (name) {
  var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)"); //构造一个含有目标参数的正则表达式对象
  var r = window.location.search.substr(1).match(reg);  //匹配目标参数
  if (r != null && !isNaN(r[2])) return r[2]; 
  else return null;
}

//FastClick.attach(document.body);

;(function(){	          

	//选中的页面
	var url = window.location.href;
	var locale = url.substring(url.lastIndexOf("/") + 1);
	if(locale.indexOf("travels") != -1) {
		var p_type = new RegExp('[-=](type_\\d*).*').exec(locale);
		if(p_type && p_type[1]) {
			$(".sideNav ." + p_type[1]).addClass("on");
		}
	} else if(locale.indexOf("userCenter") != -1) {
		$(".sideNav .t_uc").addClass("on");
	} else if(locale.indexOf("aboutUs") != -1) {
		$(".sideNav .t_au").addClass("on");
	} else {
		$(".sideNav .t_ix").addClass("on");
	}

	//isMain
	$.cookie('isMain', 1, {path: '/', expires: 7 });
	
	//渠道参数
	var fm = getUrlParamIfNum('fm');
	if(fm) {
		$.cookie('fm', fm, {path:'/', expires:7});
	}
	var insidesource = getUrlParamIfNum('insidesource');
	if(insidesource) {
		$.cookie('insidesource', insidesource, {path:'/', expires:7});
	}
	var outsidesource = getUrlParamIfNum('outsidesource');
	if(outsidesource) {
		$.cookie('outsidesource', outsidesource, {path:'/', expires:7});
	}

	// 通过COOKIE获取定位信息
  var siteInfo = $.cookie('WONIU_USER_SITE_INFO');
  if(siteInfo){
    siteInfo = JSON.parse(siteInfo);
    $("#cityBtn span").text(siteInfo.districtName);
  }

  var tss = getUrlParamIfNum('tss');
	if(tss) {
		if(tss == -1) {
			if(!!siteInfo && siteInfo.siteId != 0){
				$.cookie('tss', siteInfo.siteId, {path:'/', expires:7});
			}
		} else {
			$.cookie('tss', tss, {path:'/', expires:7});
		}
	}

  //当入口来源是fm=11和fm=12，需要隐藏首页的banner图及导航栏中的个人中心入口、标题
  fm = $.cookie('fm');
	if(fm == 11 || fm == 12){
		$(".indexFocus").hide();
		$('.ucdiv').hide();
	}
})(); 

</script>

