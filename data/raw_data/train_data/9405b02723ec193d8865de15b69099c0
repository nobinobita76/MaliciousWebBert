  <div class="gwt-tip">通知：<span id="notice-message"></span></div>
  <a href="javascript:;" class="close" id="global-warn-close">关闭</a>
</div>

<script type="text/javascript">
  (function() {
    function $$(selector) {
      return document.querySelector(selector);
    }

    var req = new XMLHttpRequest();

    req.onreadystatechange = function() {
      if (req.readyState == 4 && req.status >= 200 && req.status < 400) {
        var res = JSON.parse(req.responseText);

        if (res.data && res.data.noticeId) {
          $$('#notice-message').innerText = res.data.message;
          $$('#global-warn-tip').style.display = 'block';

          function handler() {
            $$('#global-warn-tip').style.display = 'none';

            var xhr = new XMLHttpRequest();

            //成不成功 就不用管了
            xhr.open('post', '/main/notice/read', true);
            xhr.setRequestHeader('Content-Type', "application/x-www-form-urlencoded; charset=UTF-8");        
            xhr.send(encodeURIComponent('noticeId') + '=' + encodeURIComponent(res.data.noticeId));

            $$('#global-warn-close').removeEventListener('click', handler);
          }

          $$('#global-warn-close').addEventListener('click', handler);          
        }

        req.onreadystatechange = null;
      } 
    }

    req.open('get', '/main/notice', true);
    req.send(null);
  })();
</script>
