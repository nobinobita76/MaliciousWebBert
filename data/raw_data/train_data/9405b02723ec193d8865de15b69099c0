    	// 根据ua判断APP来源，并在分享的url上加上fm参数
		var url = window.location.href;
		var ua = navigator.userAgent.toLowerCase();
		if(ua.indexOf("mmq") != -1){
			// 如果有参数
			if(url.indexOf("?") != -1 && url.indexOf("=") != -1){
				url += "&fm=5";
			}else{
				url += "?fm=5"
			}
		}else if(ua.indexOf("pregnancyhelper") != -1){
			// 如果有参数
			if(url.indexOf("?") != -1 && url.indexOf("=") != -1){
				url += "&fm=7";
			}else{
				url += "?fm=7";
			}
		}else if(ua.indexOf("gzq") != -1){
			// 如果有参数
			if(url.indexOf("?") != -1 && url.indexOf("=") != -1){
				url += "&fm=9";
