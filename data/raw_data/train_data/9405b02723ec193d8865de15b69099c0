<script type="text/javascript">
;(function() {

  if (!$('.calling').length) {
    return;
  }
  
  $.get('/main/controller/echat/encryptstr', {}, null, 'json')
  .done(function(res) {
    if (res.code == 100) {
      var hm = document.createElement("script");
      hm.id = 'echatmodulejs';
      hm.src = "//es.echatsoft.com/visitor/echat.js?companyId=44&metaData=" + res.data;
    
      hm.onload = hm.onreadystatechange = function() {
        if (!hm.readyState || /loaded|complete/.test(hm.readyState)) {
          hm.onload = hm.onreadystatechange = null;
          hm = null;
          
          $(".calling").on("click",function(){
            WoniuTrip.Widget.callingPop();
          });             
        }
      };
    
      document.head.appendChild(hm);    
    }   
  });

  WoniuTrip.Widget.callingPop(function() {
    ECHAT.customMiniChat('lywx');
  });    
})();  
