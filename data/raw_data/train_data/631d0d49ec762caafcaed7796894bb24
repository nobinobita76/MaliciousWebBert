<script language="javascript">
function turnpage(page)
{
	var f1 = document.freshlist;
	if(isNaN(page) || page<=0 || page>29)
	{
		alert("请正确输入页码！");
		return false;
	}
	f1.page.value = page;
	f1.submit();
}
function eturnpage(page)
{
	var f1 = document.freshlist;
	if(isNaN(page) || page<=0 || page>29)
	{
		alert("Please input correct number!");
		return false;
	}
	f1.page.value = page;
	f1.submit();
}
</script>


