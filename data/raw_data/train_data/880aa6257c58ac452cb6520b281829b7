        function funcSearch() {
            var keywords = $("#keywords").val();
            if (keywords == null ||  keywords == "输入产品关键字") {
                alert("请输入要查找信息的关键字！");
                document.getElementById("keywords").focus();
                return false;
            } else {
                window.location.href = "http://zhenzhen0087.kuyibu.com/search.aspx?keywords=" + keywords;
            }
        }
