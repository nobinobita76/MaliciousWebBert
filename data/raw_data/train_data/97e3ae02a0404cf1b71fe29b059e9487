<script type="text/javascript">
if( $('.mod-page-switcher').length == 0 ){
    var eye = '<br/><a href="http://www.17173.com/" target=_blank><img src="http://ue3.17173cdn.com/a/news/final/2014/img/eye02.gif"></a>&nbsp;&nbsp;<a href="http://'+window.location.host+'">返回专区首页&gt;&gt;</a>&nbsp;&nbsp;&nbsp;<a href="http://www.17173.com/?pos_ref=zq_b">返回17173首页&gt;&gt;</a>';
    if( $('.content p:last:has(img)').length != 0 ){
        if( $('.content p:last').text() == '' ){
            eye = '<br/>'+eye;
        }else{
            if( $('.content p:last').find('img').parent().is('a') ){
                $('.content p:last').find('img').parent().after('<br/>');
            }else{
                $('.content p:last').find('img').after('<br/>');
            }
        }
    }
    
    $('.content p:last').append( eye );
}
</script>
