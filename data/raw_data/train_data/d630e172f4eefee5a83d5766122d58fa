		        		$(function(){
							var casLetGoal = "-1.0";
							casLetGoal = casLetGoal.toString();
							if(casLetGoal=="0"){
								casLetGoal = "0.0";
							}
							if(casLetGoal.indexOf("-") != -1){
								casLetGoal = casLetGoal.substring(1);
