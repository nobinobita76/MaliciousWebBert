            	<script type="text/javascript">
				//自定义下拉列表
					function DropDown(el) {
						this.dd = el;
						this.placeholder = this.dd.children('span');
						this.opts = this.dd.find('.dropdown li a');
						this.chg = this.dd.find('#matchChg a');
						this.val = '';
						this.index = -1;
						this.initEvents();
					}
					DropDown.prototype = {
						initEvents : function() {
							var obj = this;
		
							obj.dd.on('click', function(event){
								$(this).toggleClass('active');
								return false;
							});
		
							obj.opts.on('click',function(){
								var opt = $(this);
								obj.val = opt.text();
								obj.index = opt.index();
								obj.placeholder.text(obj.val);
								if($(this).attr("location")!=undefined){
								 window.location.href=$(this).attr("location");
								}
							})
						},
						getValue : function() {
							return this.val;
						},
						getIndex : function() {
							return this.index;
						}
					}
					$(function() {
						var dd = new DropDown( $('#dropdown') );
						  $(document).click(function() {
							  // all dropdowns
							  $('.wrap-dropdown').removeClass('active');
						});
					})	
		
					</script>
