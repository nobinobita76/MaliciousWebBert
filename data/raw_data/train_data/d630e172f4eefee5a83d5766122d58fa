             	$(function() {
             		var comValue1 = 0;
             		var comValue2 = 0;
	             	$(".rank tr td:nth-child(11)").each(function() {
	             		var a = $(this).text();
	             		if(a.indexOf("%") != -1){
	             		    var tmp = parseFloat(a.substring(0,a.indexOf("%")));
		             		if(tmp>comValue1){
		             			comValue1 = tmp;
		             		}
	             		}
				    });
				    $(".rank tr td:nth-child(11)").each(function() {
