                <script type="text/javascript">

                    var lang;
                    if (false) {
                        lang = "zh";
                    } else {
                        lang = 'zh';
                    }
                    var appShare = "";
                    var siteNums = 0;
                    if (lang != "zh") {
                        appShare = "twitter,digg,fb,googleplus,linkedin,delicious";
                        siteNums = 6;
                    } else {
                        appShare = "qzone,tsina,weixin,tqq";
                        siteNums = 4;
                    }
                    var urlName = document.URL;
                    var jiathis_config = {
                        data_track_clickback: true,
                        siteNum: siteNums,
                        sm: appShare,
                        url: urlName,
                        summary: "#",
