      function phoneredirect(f) {
        var c = window.location.hash;
        if (!c.match("fromapp")) {
          if ((navigator.userAgent.match(/(iPhone|iPod|Android|ios|Mobile)/i)) && navigator.userAgent.toLowerCase().indexOf("ipad") < 0 && navigator.userAgent.toLowerCase().indexOf("ipd") < 0) {
            location.replace(f + window.location.pathname)
          }
        }
      }
