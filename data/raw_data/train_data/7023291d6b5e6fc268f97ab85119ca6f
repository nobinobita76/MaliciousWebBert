  <script type="text/javascript">
$(document).ready(function(){
	$(".box-index,.box").attr("id","zoom");
	$("#tpbclose").css({"cursor":"pointer"}).bind("click",function(){
		$("#tpbrowser").hide();
		$.cookie("tp_browser_close",true,{path: "/"})
	})
	In("zoom",function(){
		$(".zoom_in").live("click",function(){
			zoomInOut('zoom','in')
		})
		$(".zoom_out").live("click",function(){
			zoomInOut('zoom','out')
		})
		$(".zoom_return").live("click",function(){
			zoomInOut('zoom','return')
		})
		initZoom("zoom");
	});
	$("#backToTop").live("click", function(){
		var _this = $(this);
		$('html,body').animate({ scrollTop: 0 }, 500 ,function(){
		   // _this.hide();
		});
	});

	$(".box-hdb-tb").bind("mouseover",function(){
		var img=$(this).find("img");
		img.css({"cursor":"pointer"})
		var src=img.attr("src");
		var tosrc=img.attr("tosrc");
		img.attr("src",tosrc);
		img.attr("tosrc",src);
	}).bind("mouseleave",function(){
		var img=$(this).find("img");
		img.css({"cursor":"pointer"})
		var src=img.attr("src");
		var tosrc=img.attr("tosrc");
		img.attr("src",tosrc);
		img.attr("tosrc",src);
	})
	
	var userAgent = navigator.userAgent.toLowerCase();
    var browserId = "";
	if(userAgent.match(/(firefox|chrome|safari|opera|msie)/) != null){
    	browserId = userAgent.match(/(firefox|chrome|safari|opera|msie)/)[1];
	}
    var browserVersion = (userAgent.match(new RegExp('.+(?:version)[\/: ]([\\d.]+)')) || userAgent.match(new RegExp('(?:'+browserId+')[\/: ]([\\d.]+)')) || [0,'0'])[1];
    var isIe6 = (browserId + browserVersion == "msie6.0");
    var isIe7 = (browserId + browserVersion == "msie7.0");
    var isFirefox = (browserId  == "firefox");
    var isClose = $.cookie("tp_browser_close");
    if(isIe6 || isIe7 || isFirefox){
    	In("capacityFixed",function(){
    		$("#tptools").capacityFixed({
    			right : 0,
    	        top:100,
    	        rightOnScreen:true,
    	        fixed:"right"
    		});
    	    $("#tptopbar").capacityFixed({
    			right : 0,
    	        top:0,
    	        fixed:"top"
    		});
    	    if((isIe6 || isIe7)&& isClose!="true"){
    	    	$("#tpbrowser").show().capacityFixed({
    	 			right : 0,
    	 			fixed:"bottom"
    	 		});
    	    }
    	    $(window).trigger("scroll")
    	})
    }
})
</script>  
