    <script src="http://css.safetree.com.cn/shengpingtai/js/csee.searcher.js" type="text/javascript"></script>
    <script type="text/javascript">
        var webName = "福建省";
        var userid = "";
        var trueName = "";
        var RegionalAuthority = "0";
        var EplatLogoName = "";
        var cid = "96";
        var contentid = "0";


        //收藏页面
        function SavePage() {
            var title = document.title
            var url = document.location.href
            if (window.sidebar) {
                window.sidebar.addPanel(title, url, "");
            }
            else {
                window.external.AddFavorite(url, title);
            }
        }

        $(function () {
            var html = "";
            if (userid != "" && userid > -1) {
                html = '<span>欢迎你，' + trueName + '</span>| <a href="javascript:;" onclick="CSEE.exit(function(){window.location.reload();})">退出</a>';
            } else {
                html = '<a href="javascript:show(\'message_box\');">登录</a>';
            }
            $("#isLogin").html(html);
            $("#cont_" + "0").show();
            window.location.hash = "h2_" + "0";

            $("#LoginButton").click(function () {
                LoginFunc();
            })

            $("#PassWord").keydown(function (e) {
                if (e.keyCode == 13) {
                    $("#LoginButton").click();
                }
            })

        })


        //登录方法
        function LoginFunc() {
            var UName = $("#UName").val();
            var PassWord = $("#PassWord").val();
            if ($.trim(UName).length <= 0) {
                $("#UName").focus();
                alert("请输入帐号");
                return;
            }
            else if ($.trim(PassWord).length <= 0) {
                $("#PassWord").focus();
                alert("请输入密码");
                return;
            }
            $("#LoginButton").unbind(); //移除登录事件
            $.getJSON("/LoginHandler.ashx?jsoncallback=?", { userName: UName, password: PassWord, type: 'login', checkcode: 1, loginType: '1', r: Math.random() }, function (json) {

                if (json.ret == 1) {
                    window.location.href = "/HelpSystem/PhoneHelpEplat.aspx";

                }
                else {
                    if (json.msg) {
                        $("#LoginButton").click(function () {
                            LoginFunc();
                        })
                        switch (json.msg) {
                            case "帐号不属于平台":
                                alert("帐号不属于平台");
                                break;
                            //                        case "您相同账户在其它地方登录，请重新登录！":                     
                            //                            alert("您相同账户在其它地方登录，请重新登录！");                     
                            //                            break;                     
                            default:
                                alert("帐号或密码错误!");
                                break;
                        }
                        return;
                    }
                }
            });
        }

    </script>
