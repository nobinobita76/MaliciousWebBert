				'mouseenter':function(){
					this.addClass('current');
					var li = this.getSize();
					popup.setStyles({
						'display':'block',
						'margin': '-'+0+'px 0 0 '+(li.x-0)+'px'
					});
				},
				'mouseleave':function(){
					this.removeClass('current');
					popup.setStyle('display','none');
				}
