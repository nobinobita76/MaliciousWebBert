        $('.btn').click(function () {
            var pageUrl = window.location.href;
            var paraValue = "";
            if (pageUrl.indexOf("?") > -1) {
                paraValue = pageUrl.split('?')[1];
            }
            $('#my-login2').attr('src', "registered.aspx?" + paraValue);
            $('#my-login2').show().css('display','block');
            $('body').css('overflow', 'hidden');
        });

        $(document).scroll(function()
        {
           if($(this).scrollTop()>600)
           {
              $(".calculator").show();
           }else
           {
              $(".calculator").hide();
           }
        });

        $(".calculator").click(function () {
            $(this).hide();
            $(".cal").show();
        });
        $(".guan").click(function () {
            $(".cal").hide();
            $(".calculator").show();
        });
        $(".cal li").click(function () {
            $(".cal li").removeClass("current");
            $(this).addClass("current");
        });
        var rate = 13;
        $("#calculate").click(function () {
            if ($('.yue1').hasClass('current')) {
                $(".earnings span").html(($("#money").val() * rate / 100 / 365 * 7).toFixed(2));
            } else {
                $(".earnings span").html(($("#money").val() * rate / 100 / 12 * $(".cal li.current").val()).toFixed(2));
            }

        });
        $("#reset").click(function () {
            $("#money").attr("value", '');
            $(".earnings span").html(0);
        });
        $(function () {
            /*JQuery 限制文本框只能输入数字*/
            $("#money").keyup(function () {
                $(this).val($(this).val().replace(/\D|^0/g, ''));
            }).bind("paste", function () {  //CTR+V事件处理
                $(this).val($(this).val().replace(/\D|^0/g, ''));
            }).css("ime-mode", "disabled"); //CSS设置输入法不可用
            /*JQuery 限制文本框只能输入数字和小数点*/
        });

