        $('#box').Picture();
        boxShow();
        function boxShow() {
            $('#phone').focus(function () {
                $('.box .font1').hide();
            }).blur(function () {
                if ($(this).val() == '') {
                    $('.box .font1').show();
                }
            });
            $('#passWord').focus(function () {
                $('.box p.font2').hide();
            }).blur(function () {

                var a = $(this).val();

                if (a == '' || a.length == 0) {
                    $('.box p.font2').show();
                }
            });

            $('#txtValidNumber').focus(function () {
                $('.box .font3').hide();
            }).blur(function () {
                if ($(this).val() == '') {
                    $('.box .font3').show();
                }
            });
            $('#messCode').focus(function () {
                $('.box .font4').hide();
            }).blur(function () {
                if ($(this).val() == '') {
                    $('.box .font4').show();
                }
            });
            $('.box p.font1').click(function () {
                $(this).hide();
                $('#passWord,#txtValidNumber,#messCode').blur();
                $('#phone').focus();
            });
            $('.box p.font2').click(function () {
                $(this).hide();
                $('#phone,#txtValidNumber,#messCode').blur();
                $('#passWord').focus();
            });
            $('.box p.font3').click(function () {
                $(this).hide();
                $('#passWord,#phone,#messCode').blur();
                $('#txtValidNumber').focus();
            });
            $('.box p.font4').click(function () {
                $(this).hide();
                $('#passWord,#phone,#phone').blur();
                $('#messCode').focus();
            });

            $('#phone').click(function () {
                $('#passWord,#txtValidNumber,#messCode').blur();
                $('.box p.font1').hide();
                $(this).focus();
            });
            $('#passWord').click(function () {
                $('#phone,#txtValidNumber,#messCode').blur();
                $('.box p.font2').hide();
                $(this).focus();
            });
            $('#txtValidNumber').click(function () {
                $('#passWord,#phone,#messCode').blur();
                $('.box p.font3').hide();
                $(this).focus();
            });
            $('#messCode').click(function () {
                $('#passWord,#phone,#txtValidNumber').blur();
                $('.box p.font4').hide();
                $(this).focus();
            });


            $('.box input').focus(function () {
                $(this).css('outline-color', '#f00');
            });
            $('.p2 .p2-down .t7').hover(function () {
                if (!$(this).hasClass('over')) {
                    $(this).css('background-position', '-10px -172px');
                }
            }, function () {
                if (!$(this).hasClass('over')) {
                    $(this).css('background-position', '-10px -121px');
                }
            })
        }


