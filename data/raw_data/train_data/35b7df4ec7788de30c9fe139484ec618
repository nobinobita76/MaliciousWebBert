    <script type="text/javascript">
        $(function() {
            var ss_id = 196;
            var actor_id = window.STAR.id;
            $('#JS_commentMain').on('click', '.disucz-page-box li a', function () {
                var _this = $(this);
                if (_this.parent('li').hasClass('cur')) {
                    return false;
                }

                var url = 'http://w.star.kankan.com/accomplishment/comments.html';
                $.getJSON(url, {ss_id: ss_id, p: _this.attr('data-page')}, function (data){
                    if (data != '') {
                        $('#JS_commentMain').html(data);
                    };
                });
                return false;
            });

            $('#reference-main').on('click', '.star-pages li a', function (){
                var _this = $(this);
                if (_this.parent('li').hasClass('on')) {
                    return false;
                }

                var url = 'http://w.star.kankan.com/accomplishment/references.html';
                $.getJSON(url, {ss_id: ss_id, id: actor_id, p: _this.attr('data-page')}, function (data){
                    if (data != '') {
                        $('#reference-main').html(data);
                        $('#reference-main').find('.star-name').append(window.STAR.name);
                    };
                });
                return false;
            });

            $('.js-share').click(function() {
                var _this = this;
                dialog({
                    id: 'share',
                    title: '才艺分享',
                    quickClose: true,
                    content: '正在载入……',
                    onshow: function () {
                        var query = 'url=http%3A%2F%2Fw.star.kankan.com%2Faccomplishment%2F1005711477%2F196.html&description=张凯源：让人看呆的机械舞&pic=http%3A%2F%2Fimage.star.kankan.com%2FVcover%2F2015%2F12%2F15%2F20151215141603_21785.jpg'
                        this.content('<iframe src="http://w.star.kankan.com/accomplishment/share.html?' + query + '" width="400" height="300" frameBorder="0"></iframe>');
                    },
                    fixed: true,
                }).showModal();
            });

        });
    </script>

    
    
