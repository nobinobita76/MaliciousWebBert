<script language="javascript" type="text/javascript">
         function Search()
	     {
	        var keyword=document.getElementById("txtKeyword").value;
	        if(keyword =='输入你要查找的关键字')
	        {
	            alert('输入你要查找的关键字');
	            return;
	        }
		if (keyword.indexOf('<') > -1) {
	            alert('您输入的关键字带有敏感词!');
	            return;
	        }
	        keyword=encodeURI(keyword);
	        //document.URLUnencoded.
	        var url=window.location.href.substring(0,window.location.href.indexOf('/',8)+1) ;
	        var charList= window.location.href.split('/');
	        if(window.location.href.indexOf('http://localhost:',0)>-1)//如果为本地开发
	        {
	            url=charList[0]+'//'+charList[2]+'/';//+charList[3]+'/';
	        }
           //var selectSearch=document.getElementById("selectSearch");
	        var searchType = 'Course'; //搜索类型
    	   //searchType=selectSearch.value;
    	   var course="course";                 
	       if(searchType=="Course")
	       {
	           window.location.href = url + searchType + "/Default.aspx?selecttype=keyword&txtvalue=" + keyword; //课程名称
	       }
	       else
	       {
	           window.location.href = url + searchType + "/Default.aspx?KeyWord=" + keyword;
	       }
	    }
	    document.onkeypress=docKeyPress;
	    function docKeyPress()
        {
            if (event.keyCode==13)
            {
                if (event.srcElement.id=='txtKeyword')
                {
                    Search();
                }
            }
        }
</script>


