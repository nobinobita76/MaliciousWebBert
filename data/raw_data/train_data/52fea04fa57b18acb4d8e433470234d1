<script type="text/javascript">
	$(function() {
		$('#slides').slidesjs({
			width: 1000,
			height: 372,
			play: {
				active: false,
				auto: true,
				interval: 5000,
				swap: false,
				pauseOnHover: true,
				restartDelay: 2500,
				generatePagination:true,
				paginationClass: 'pagination',
				currentClass:'current'
			},
			navigation: {
				active: false
			}
		});
		
		$(window).manhuatoTop({
			showHeight : 100,
			speed : 500
		});
		
	});
	
	/**
	 * 登陆
	 */
	var oldUrl = window.location.href;
	initPage(oldUrl);
	
	function login(ctl) {
		var currUrl = window.location.href;
//		$.removeCookie('unipayUser',{path:'/'});
		return chkLogin(currUrl,ctl);
	}
	
	function chkLogin(oldUrl,ctl){
		var loginUrlPrefix = $("#loginUrlPrefix").val();
		oldUrl=oldUrl.replace(new RegExp("&","gm"), "~");
		var redirectUrl = loginUrlPrefix+"?redirectUrl="+oldUrl;
		var returnFlag = false;
			$.ajax({
				async:false,
				type:"POST",
				url:"/join/chkLogin",
				data:{"oldUrl":redirectUrl},
				success:function(data){
					if(data.flag && data.loginUrl != ""){
						$(".thumb1").hide();
						$(".thumb").show();
						$("#login-frame").attr("src",data.loginUrl);
						$("#fade").show();
						$("#loginDiv").show();
					}else if(data.flag && data.loginUrl == ""){
						oldUrl = "/join/error/error";
						returnFlag=true;
					}else{
						returnFlag=true;
					}
				}
			});
			if(!returnFlag){
				return returnFlag;
			}
			$(ctl).attr("href",oldUrl);
	}
	
	function logout(logoutUrl,ctl){
//		$.cookie("unipayUser","",{expires:-1});
//		$.removeCookie('unipayUser',{path:'/'});
		$(ctl).attr("href",logoutUrl)
	}
	
	$("#d-close").click(function(){
		$("#fade").hide();
		$("#loginDiv").hide();
		$(".thumb1").show();
		$(".thumb").hide();
		
	})
	
	
	function chkLoginForm(oldUrl){
//		oldUrl = oldUrl+"?mchntTaskRecId="+$("#mchntSignRecId").val();
		oldUrl=oldUrl.replace(new RegExp("&","gm"), "~");
		var loginUrlPrefix = $("#loginUrlPrefix").val();
		var redirectUrl = loginUrlPrefix+"?redirectUrl="+oldUrl;
		var returnFlag = false;
		$.ajax({
			async:false,
			type:"POST",
			url:"/join/chkLogin",
			data:{"oldUrl":redirectUrl},
			success:function(data){
				if(data.flag && data.loginUrl != ""){
					$(".thumb1").show();
					$(".thumb").hide();
					$("#login-frame").attr("src",data.loginUrl);
					$("#fade").show();
					$("#loginDiv").show();
				}else if(data.flag && data.loginUrl == ""){
					oldUrl = "/join/error/error";
					location.href = oldUrl;
				}else{
					returnFlag=true;
				}
			}
		});
		return returnFlag;
	}
	
	function initPage(oldUrl){
		oldUrl=oldUrl.replace(new RegExp("&","gm"), "~");
		var loginUrlPrefix = $("#loginUrlPrefix").val();
		var redirectUrl = loginUrlPrefix+"?redirectUrl="+oldUrl;
		$.ajax({
			async:false,
			type:"POST",
			url:"/join/chkLogin",
			data:{"oldUrl":redirectUrl},
			success:function(data){
				if(data.flag && data.loginUrl != ""){
					$(".thumb1").show();
					$(".thumb").hide();
					$("#login-frame1").attr("src",data.loginUrl);
					//$("#loginDiv1").show();
				}else if(data.flag && data.loginUrl == ""){
					oldUrl = "/join/error/error";
					location.href = oldUrl;
				}
			}
		});
	}
	
</script>


</body></html>