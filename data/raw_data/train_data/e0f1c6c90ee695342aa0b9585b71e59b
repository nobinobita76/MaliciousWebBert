<script type="text/javascript" src="http://api.map.baidu.com/api?v=1.4"></script><script type="text/javascript" src="http://api.map.baidu.com/getscript?v=1.4&amp;ak=&amp;services=&amp;t=20150522093217"></script>
<script type="text/javascript">
$(function () {

    //展开路线
    $("#routeExpand").click(function(e){
        e = e || window.event;
        e.preventDefault();
        $("#routeCollapseLayer").hide();
        $("#routeExpandLayer").show();
    })

    //收起路线
    $("#routeCollapse").click(function(e){
        e = e || window.event;
        e.preventDefault();
        $("#routeCollapseLayer").show();
        $("#routeExpandLayer").hide();
    })

    //打开地图
    $(".openMap").click(function (e) {
        e = e || window.event;
        e.preventDefault();
        $("#map_overlay,#mapWrapper").show();
        
        var mapArr = { "lnt": $(this).attr("lnt"), "lat": $(this).attr("lat"), "name": $(this).attr("name"),"telephone":$(this).attr("telephone"),"address":$(this).attr("address") };

        createBaiduMap(mapArr);
    })

    //关闭地图
    $("#closeMap").click(function () {
         $("#map_overlay,#mapWrapper").hide();
    })

    window.onscroll = function () {
        $("#mapWrapper").css("top", ($(window).height() - $('#mapWrapper').outerHeight()) / 2 + $(document).scrollTop());
    }    
})

function createBaiduMap(mapArr) {

    $("#hospitalName").text(mapArr.name);
    var map = new BMap.Map("map");
    var top_left_navigation = new BMap.NavigationControl();  //左上角，添加默认缩放平移控件
    map.addControl(top_left_navigation);
    map.enableScrollWheelZoom();
    var point = new BMap.Point(mapArr.lnt, mapArr.lat);
    var marker = new BMap.Marker(point);
    map.addOverlay(marker);
    map.centerAndZoom(point, 16);
    var content="<div>";
    if(mapArr.address.length>0){
        content+="地址："+mapArr.address+"<br />";
    }
    if(mapArr.telephone.length>0){
         content+="电话："+mapArr.telephone+"<br />";
    }
    content += "</div>";
    
    var opts = { width: 350, title: mapArr.name,mapArr:content };
    var infoWindow = new BMap.InfoWindow(content, opts);
    map.openInfoWindow(infoWindow, point);
    marker.addEventListener("click", function () {
        map.openInfoWindow(infoWindow, point);
    });
}
</script>


<script>
