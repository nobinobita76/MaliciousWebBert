<!-- 地图弹出层开始-->
<div id="map_overlay"></div>
<div id="mapWrapper">
    <ul class="winopen_title">
        <span id="hospitalName"></span>
        <img src="//static.fh21.com.cn/images/hospital/close01.gif" id="closeMap">
    </ul>
    <div id="map">
