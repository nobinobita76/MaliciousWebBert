<script>
(function($,D){
	if(D){
		var gurl="http://partners.fh21.com.cn/partners/showcodejsonp?callback=?";
		var placeholders={},pids=[];
		D.condition.forEach(function(item){
			pids.push(item.pid);
			placeholders[item.pid]=item.placeholder;

			placeholders[item.pid+'_exec']=item.extra_exec;
			delete item.placeholder;
		});
		var req_data={
						kw:encodeURIComponent(D.keyword),
						pid:pids.join(',')
		}
		console.log(D,req_data);
		$.getJSON(gurl,req_data,function(resp){
		//console.log(resp);
		if(resp.errno === 0){
			var ads= JSON.parse(resp.result),
				defaults=[];
			
			$.each(ads, function(posID,adHtmlStr) {
				if(adHtmlStr){
					if(placeholders[posID]){
						$(placeholders[posID]).html(adHtmlStr)
					}else{
						defaults.push(adHtmlStr);
					}
				}else if(placeholders[posID+"_exec"]){
					placeholders[posID+"_exec"]();
				}
			});
			$("body").append(defaults.join("")); 
		}else{
			console.log("ad api error");
		}
		});
	}
})($,typeof AD_REQ_DATA === "undefined"?null:AD_REQ_DATA);
</script>



