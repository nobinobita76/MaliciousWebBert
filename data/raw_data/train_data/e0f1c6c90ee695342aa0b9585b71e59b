u91cd\u75c7\u76d1\u62a4\u5ba4","parent_id":"1996"},"120970":{"name":"\u7279\u8272\u533b\u7597\u79d1","parent_id":"1996"},"120971":{"name":"\u4f53\u68c0\u4e2d\u5fc3","parent_id":"1996"},"70091":{"name":"\u836f\u5242\u79d1","parent_id":"1996"}}'; //所有科室
var objDepartAll=$.parseJSON(strDepartAll);
var commentDefaultVal="您的评价对其他患者是很重要的参考";
$(function () {
	var strDepartSecond='{"1973":{"121669":"\u76ae\u80a4\u6027\u75c5\u79d1","121679":"\u5987\u79d1","121730":"\u666e\u5916\u79d1","121711":"\u813e\u80c3\u75c5\u79d1","121713":"\u80be\u75c5\u79d1","123009":"\u80be\u5916\u79d1","121701":"\u8033\u9f3b\u5589\u79d1","123007":"\u7537\u79d1","1983":"\u7efc\u5408\u95e8\u8bca","121699":"\u9aa8\u79d1","121712":"\u5185\u5206\u6ccc\u79d1","121726":"\u4e73\u817a\u79d1","121682":"\u80bf\u7624\u79d1","121720":"\u547c\u5438\u75c5\u79d1","121724":"\u75ae\u75a1\u8840\u7ba1\u5916\u79d1","121707":"\u5fc3\u8840\u7ba1\u79d1","121710":"\u98ce\u6e7f\u75c5\u79d1","1984":"\u9488\u7078\u4e2d\u5fc3","123010":"\u6025\u8bca\u4e13\u5bb6\u95e8\u8bca","123008":"\u5fc3\u8eab\u533b\u5b66\u79d1","121675":"\u513f\u79d1","123011":"\u75bc\u75db\u79d1","120967":"\u63a8\u62ff\u79d1","121703":"\u611f\u67d3\u79d1\uff08\u809d\u75c5\uff09","123004":"\u773c\u79d1","123005":"\u53e3\u8154\u79d1","121731":"\u809b\u80a0\u79d1","123006":"\u8001\u5e74\u75c5\u79d1","123012":"\u6742\u75c5\u95e8\u8bca"},"1996":{"121696":"\u9ebb\u9189\u79d1","120969":"\u91cd\u75c7\u76d1\u62a4\u5ba4","120970":"\u7279\u8272\u533b\u7597\u79d1","120971":"\u4f53\u68c0\u4e2d\u5fc3","70091":"\u836f\u5242\u79d1"}}'; //二级科室
	var objDepartSecond=$.parseJSON(strDepartSecond);
	var hospital_id='62';
	var pagefirst='http://yyk.fh21.com.cn/hospital/commentget?page=1&id='+hospital_id;
	getCommentList(pagefirst);
	//加载一级科室
	var strParentDepart="<option value='0'>请选择科室</option>";
	$.each(objDepartAll,function(index,item){
		if(item.parent_id=='0'){
			strParentDepart+="<option value="+index+">"+item.name+"</option>";
		}
	});
	$("#departmentid1").html(strParentDepart);

	//加载二级科室
	$("#departmentid1").change(function(){
		var id=$(this).val();
		if(id!="0"){
			var childDepart=objDepartSecond[id];
			if (childDepart!=undefined) {
				var strChildDepart="<option value='0'>请选择二级科室</option>";
				$.each(childDepart,function(index,item){
					strChildDepart+="<option value="+index+">"+item+"</option>";
				});
				$("#departmentid2").html(strChildDepart).show();
			}else{
				$("#departmentid2").html("").hide();
			}
		}else{
			$("#departmentid2").html("").hide();
		}
	});

	//评论支持placeholder效果
    $("#comment").focus(function () {
        if ($(this).val() == commentDefaultVal) {
            $(this).val("").removeClass("blur").addClass("focus");
        }
    }).blur(function () {
        if ($(this).val().length == 0) {
            $(this).val(commentDefaultVal).removeClass("focus").addClass("blur");
        }
    });

	//关闭评论成功遮罩层
    $(".closeMasker").click(function () {
        $(".success_masker").hide();
        $("#commentForm")[0].reset();
    });

	$("#comment").keyup(function(){
		checkCommentForm();
	});

	 //提交评论
    $("#submitComment").click(function () {
    	var serverMsg={
    		"hospital_not_exist":"医院不存在！",//-1
    		"depart_not_exist":"科室不存在！",//-2
    		"depart_error":"该医院没有该科室！",//-3
    		"illegal_word":"包含非法字符！",//10004
    		"success":"评论成功！",//0
    		"fail":"评论失败！",//-4
    		"commented":"已经评论！"//-5
    	};
    	//检查发送数据 
    	if(checkCommentForm()){
	        //准备发送数据
	        var commentTip = $("#commentTip");
	        var content = $.trim($("#comment").val());
	        var departmentid1 = $("#departmentid1 option:selected").val();
	        var departmentid2 = $("#departmentid2 option:selected").val();
	        var degree = $("input[name='degree']:checked").val();
	        //发送数据
	        var url='http://yyk.fh21.com.cn/hospital/commentsend';
	        var data={hospital_id:hospital_id,departmentid1:departmentid1,degree:degree,content:content};
	        if(departmentid2!=undefined){
	        	data={hospital_id:hospital_id,departmentid1:departmentid1,departmentid2:departmentid2,degree:degree,content:content};
	        }
	        $(this).attr("disabled","disabled");
	        $.post(url,data,function(result){
	        	if(result.errno==0){
	        		$(".success_masker").show().fadeOut(3000,function(){
	        			$("#commentForm")[0].reset();
	        			$("#comment").addClass("blur");
	        		});
	        	}else if(result.errno=='-1'){
	        		commentTip.show().text(serverMsg.hospital_not_exist).fadeOut(3000);
	        	}else if(result.errno=='-2'){
	        		commentTip.show().text(serverMsg.depart_not_exist).fadeOut(3000);
	        	}else if(result.errno=='-3'){
	        		commentTip.show().text(serverMsg.depart_error).fadeOut(3000);
	        	}else if(result.errno=='-4'){
	        		commentTip.show().text(serverMsg.fail).fadeOut(3000);
	        	}else if(result.errno=='-5'){
	        		commentTip.show().text(serverMsg.commented).fadeOut(3000);
	        	}else if(result.errno=='10004'){
	        		commentTip.show().text(serverMsg.illegal_word).fadeOut(3000);
	        	}
	        	$("#submitComment").removeAttr("disabled");
	        });
        }
    });
});

//绑定事件
function bindCommentEvent(){
	//截取分页
	$("#pager a").on("click",function(e){
		e = e || window.event;
		e.preventDefault();
		var url=$(this).attr("href");
		getCommentList(url);
	})
	//展开
	$(".expand").click(function(e){
		e = e || window.event;
		e.preventDefault();
		$(this).parent().hide().next().show();
	})
	//折叠
	$(".collapse").click(function(e){
		e = e || window.event;
		e.preventDefault();
		$(this).parent().hide().prev().show();
	})
}

//获取评论列表
function getCommentList(pageurl){
	$.get(pageurl,function(data){
		var html="";
		if(data.hospitalComment.commentList.length>0){
			$.each(data.hospitalComment.commentList,function(index,item){
				html+="<dl><dt>";
				if(objDepartAll[item.content.departmentid]){
					if(objDepartAll[item.content.departmentid].name){
						html+="<p>就诊科室：<a href='http://yyk.fh21.com.cn/department_intro_"+item.content.departmentid+".html' target='_blank' class='link08'>"+objDepartAll[item.content.departmentid].name+"</a></p>";
					}
				}
				html+="满意度：<span>"+item.content.degreeDesc+"</span></dt><dd>";
				html+="<ul>"+processContent(item.content.content)+"</ul>";
				html+="<ol>"+item.frontend_nickname+"<span>|</span>"+item.cdate+"</ol>";
				html+="</dd></dl>";
			})
			$("#commentList").html(html);
			if(data.hospitalComment.pageHtml.length>0){
				$("#pager").show().html(data.hospitalComment.pageHtml);
			}else{
				$("#pager").hide();
			}
		}else{
			$("#commentWrapper").hide();
		}
		bindCommentEvent();
	});
}

function processContent(content){
	var maxlength=150;
	content=$.trim(content);
	if (content.length<=maxlength) {
		return content;
	}else{
		return "<span class='comment_expand'>"+content.substring(0,maxlength)+"...<a href='#' class='link03 expand' style='font-size:12px;''>展开↓</a></span><span class='comment_collapse' style='display:none;'>"+content+"<a href='#' class='link03 collapse' style='font-size:12px;'>收起↑</a></span>";
	}
}

function checkCommentForm() {
    var commentcontrol = $("#comment");
    var commentTip = $("#commentTip");
    var comment = $.trim(commentcontrol.val());
    var departmentid2 = $("#departmentid2 option:selected").val();

    //验证提交数据
 	if (departmentid2=='0') {
		  commentTip.show().text("请选择二级科室！");
		  return false;
    };

    if (comment.length <= 0||comment==commentDefaultVal) {//是否输入
        commentTip.show().text("请输入评价！");
        commentcontrol.focus();
        return false;
    }
    if (comment.length > 0 && comment.length < 5) {//最大长度
        commentTip.show().text("评价最少为5个字！");
        commentcontrol.focus();
        return false;
    }
    if (comment.length > 200) {//最大长度
        commentTip.show().text("评价最多为200个字！");
        commentcontrol.focus();
        return false;
    }

    var pattern = /<|>/;
    if (pattern.test(comment)) {
        commentTip.show().text("包含敏感字符！");
        commentcontrol.focus();
        return false;
    };
    //清除状态
    commentTip.show().text("");
    return true;
}
