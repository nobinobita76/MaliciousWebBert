  <script type="text/javascript">
    myFocus.extend({
	mF_sd_tqq:function(par,F){
		var box=F.$(par.id),msgs=F.$c('msgs',box),n=F.$$_('li',msgs).length;
		//PLAY
		eval(F.switchMF(function(){
			var last=F.$$_('li',msgs)[n-1],lastH=last.offsetHeight;
			F.slide(msgs,{marginTop:lastH},750,'easeOut',function(){
				msgs.insertBefore(last,msgs.firstChild);
				F.setOpa(last,0);
				msgs.style.marginTop='0px';
				F.fadeIn(last);
			});
		}));
	}
});
//判断是不是第一页
var page=QueryString("page");
var str=QueryString("str");
if((page==""&& str=="")||(page=="2"&& str=="previous")||(page=="1"&& str=="number")){
    myFocus.set({id:'qetions',pattern:'mF_sd_tqq',time:30});
}

function bqshow(){ 
//$("#kemu1infoa").slideUp("slow");
   // document.getElementById("kemu1infoa").style.display="none";
    $("#kemu1infoa").hide();
}
function bqdown(){ 
//$("#kemu1infoa").slideDown("slow");
   // document.getElementById("kemu1infoa").style.display="block";
    $("#kemu1infoa").toggle(300);
}
function biaoqing(bq){
    document.getElementById("qution").value+=bq;
    document.getElementById('qution').focus();
    bqshow();
}
function submitForm() {
    $("#form2").ajaxSubmit({
        url: "DoUpLoad.aspx",
        beforeSubmit: checkform,
        datatype: "json",
        success: function(data) { SubmitSucceed(data); },
        error: function(ex) { alert('服务器正忙，请稍后提交...' + ex.status); }
    });
}

//检查客户端用户输入
function checkform() {
    var _file = document.getElementById('fillName');
    if (_file.value == "") {
        alert("请选择文件！");_file.focus();return false;
    }
    return true;
}
var imgstr="";
//提交成功后回调
function SubmitSucceed(data) {
    if(data=="文件太大"){alert("文件要小于512kb");
    }else if (data == "文件格式非法") {alert(data);
    }else if (data != "") {document.getElementById("ppp").innerHTML = data+"<img style=\"cursor:hand;\" src=\"../image/del.gif\" onclick=\"DelFile()\" />"; imgstr=data;
    }else {alert("上传失败！");}
}
function DelFile(){
    document.getElementById("ppp").innerHTML="";imgstr="";
}
//计算输入的字数
function jisuan(){
    var qution=document.getElementById("qution").value;
    var num = document.getElementById("num");
    num.innerHTML=140-parseInt(qution.length);
    if(num.innerHTML<0){
        var abc=parseInt(-num.innerHTML);
       document.getElementById("shuru").innerHTML="超出<span id=\"num\" style=\"font-size:16px; font-weight:bold;\">"+abc+"</span>字";
    }
}
