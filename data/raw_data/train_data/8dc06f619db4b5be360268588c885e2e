  <script language="JavaScript" type="text/javascript">
function rightBottomAd(){
    var abc = document.getElementById("rbtnad");
    abc.style.top = document.documentElement.scrollTop+document.documentElement.clientHeight-183+"px"; 
    setTimeout(function(){rightBottomAd();},50);
}
function rightBottomClose() {
    var abc = document.getElementById("rbtnad");
    abc.style.display = "none";
}
rightBottomAd();
