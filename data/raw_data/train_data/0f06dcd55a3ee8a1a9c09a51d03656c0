<html><head></head><body><pre style="word-wrap: break-word; white-space: pre-wrap;">function Pager(d,e,c){if(c){this.pageName=c}else{this.pageName="page"}this.perpage=10;this.total=d;this.pagenum=this.getPagenum();this.offset=Math.floor((this.pagenum-1)*this.perpage);this.totalpages=Math.ceil(this.total/this.perpage);this.clickFunc=function(){};this.navContainer={};this.Container=e}Pager.prototype.reset=function(){this.pagenum=this.getPagenum();this.offset=Math.floor((this.pagenum-1)*this.perpage);this.totalpages=Math.ceil(this.total/this.perpage);if(this.pagenum&gt;this.totalpages){this.setPagenum(this.totalpages);this.pagenum=this.getPagenum();this.offset=Math.floor((this.pagenum-1)*this.perpage);this.totalpages=Math.ceil(this.total/this.perpage)}};Pager.prototype.getPagenum=function(){var f=location.href;var e=new RegExp("[#]"+this.pageName+"=([0-9]+)","g");var d=e.exec(f);if(null!=d){return(parseInt(d[1])&gt;0)?parseInt(d[1]):1}else{return 1}};Pager.prototype.setPagenum=function(f){var h=location.href;var e;var g=new RegExp("([#]"+this.pageName+"=)[0-9]*","g");if(g.test(h)){e=h.replace(g,"$1"+f)}else{e=h+"#"+this.pageName+"="+f}location.href=e};Pager.prototype.bulitUrl=function(e){var d=location.href;var f=new RegExp("([#]"+this.pageName+"=)[0-9]*","g");if(f.test(d)){return d.replace(f,"$1"+e)}else{return d+"#"+this.pageName+"="+e}};Pager.prototype.bulitLink=function(e,g,f){var h=document.createElement("a");h.innerHTML=f;h.href=this.bulitUrl(e);(function(a,b){h.onclick=function(){location.href=h.href;window.scrollTo(0,b.Container.offsetTop);b.reset();b.clickFunc(a);b.reechoPageNav();return false}})(e,this);return h};Pager.prototype.reechoPageNav=function(){this.echoPageNav(this.navContainer,this.clickFunc)};Pager.prototype.echoPageNav=function(w,q){this.navContainer=w;this.clickFunc=q;if(this.total==0){this.navContainer.style.display="none";return false}w.innerHTML="";if(this.pagenum==1){var m=document.createElement("span");m.className="next_page";m.innerHTML="\u4e0a\u4e00\u9875";w.appendChild(m)}else{m=this.bulitLink(this.pagenum-1,q,"\u4e0a\u4e00\u9875");w.appendChild(m);FirstPageNod=this.bulitLink(1,q,"1");w.appendChild(FirstPageNod)}var p=new Array();var r=0;for(i=1;i&lt;4&amp;&amp;((this.pagenum-i)&gt;1);i++){o=this.bulitLink(this.pagenum-i,q,this.pagenum-i);r=this.pagenum-i;p.push(o)}if((r-1)&gt;1){var v=document.createElement("span");v.innerHTML="...";w.appendChild(v)}var u=p.length;for(i=0;i&lt;u;i++){w.appendChild(p.pop())}var o=document.createElement("strong");o.innerHTML=this.pagenum;w.appendChild(o);var n=this.totalpages;for(i=1;i&lt;5&amp;&amp;((this.pagenum+i)&lt;this.totalpages);i++){o=this.bulitLink(this.pagenum+i,q,this.pagenum+i);n=this.pagenum+i;w.appendChild(o)}if(n+1&lt;this.totalpages){var s=document.createElement("span");s.innerHTML="...";w.appendChild(s)}if(this.pagenum&gt;=this.totalpages){var t=document.createElement("span");t.className="next_page";t.innerHTML="\u4e0b\u4e00\u9875";w.appendChild(t)}else{LastPageNod=this.bulitLink(this.totalpages,q,this.totalpages);w.appendChild(LastPageNod);t=this.bulitLink(this.pagenum+1,q,"\u4e0b\u4e00\u9875");w.appendChild(t)}};</pre></body></html>