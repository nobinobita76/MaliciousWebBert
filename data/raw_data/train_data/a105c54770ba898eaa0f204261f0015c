<script type="text/javascript">
    $(function(){     
        $('#t__cp').keydown(function(e){
            if(e.keyCode==13){
                verifyListPages();
            }
        });
        $('#click_get_page').click(function(){
            verifyListPages();
        });
    });

    function verifyListPages(){
        var page=$.trim($('#t__cp').val());
        var max_page=7;
        re = /^[1-9]*[0-9]*[0-9]$/i; 
        if(!re.test(page)||page==0){                  
            alert('您好，请输入正确页码。');
            return false;                   
        }else{
            if(parseInt(page)>100){
                alert('您好，请输入正确页码。');
                return false;
            }else{       
                if(parseInt(page)>max_page){
                    
                    alert('您好，请输入正确页码。');
                    return false;
                    //                    var href_url = '/pgnone-cid4008154-srsort_score_desc-a1000016%3A27.html';
                    //                                            //                        end_url = href_url;
                        //                                        //                    
                    //                    window.location.href=end_url;
                    //                    return false;
                }else{
                    var href_url = '/pgnone-cid4008154-srsort_score_desc-a1000016%3A27.html';
                        if (page == 1) {
                            end_url = href_url.replace('pgnone-','');
                        }else{
                            end_url = href_url.replace('pgnone','pg'+page);
                        }
                    
                    window.location.href=end_url;
                }
            }
        }
    }
</script>





