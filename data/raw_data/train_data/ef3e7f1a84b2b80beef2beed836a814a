            $("body").on("click", "[data-trace-label]", function() {
                var t = $(this), trace_category = t.data('trace-category'), trace_label = t.data('trace-label');
                kaq('send', 'event', trace_category, 'click', trace_label);
            });
        }
        kaq('send', 'pageview');
        kaq('send', 'timing', 5);
        z.kaq = kaq;
    })(window.SOHUZ);
</script>    
</body></html>