    <script type="text/html" id="schedule-item-template">
    <@
    function formatImg(url,size){
        if(!url){
        return 'http://i1.letvimg.com/lc03_iscms/201601/11/15/15/81939676d8af46c49976f18a54f3140b.png';
        }else{
        var len= url.lastIndexOf('.');
        return url.substring(0,len)+size+url.substring(len,url.length);
        }
    }
    function getStatus(status){
        var arrStatus=['未开始', '直播中','已结束'];
        return arrStatus[status];
    }
    function getScores(status,lscore,rscore){
         if(status==0){
          return 'VS';
    }else{
         lscore = lscore?lscore:'0';
         rscore = rscore?rscore:'0';
         return lscore + ':' + rscore;
    }
    }

    function getStatusCls(status){
        var arrStatus=['nostart', 'ing','end'];
        return arrStatus[status];
    }
    function formatTime(t){
        if(t){
            return t.substring(4,6)+'-'+t.substring(6,8)+' '+ t.substring(8,10)+':'+ t.substring(10,12);
        }
    }
    function formatName(name){
        if(name.length==6){
            return name.substring(2,6);
        }else{
            return name;
        }
    }
    function formatScore(score){
        if(score){
            return score;
        }else{
            return 0;
        }
    }
  @>

    <a href="http://sports.letv.com/match/<@=item.mid @>.html" target="_blank">
        <div class="status-top">
            <span class="status <@= getStatusCls(item.status)@>"><@= getStatus(item.status)@></span>
            <span class="start-time"><@= formatTime(item.startTime)@></span>
        </div>

        <div class="logos-mid">
            <i class="left logo"><img src="<@=formatImg(item.competitors[0].logo,'/11_50_50')@>"></i>
            <span class="score nostart"><@= getScores(item.status,item.competitors[0].score,item.competitors[1].score)@></span>
            <i class="right logo"><img src="<@=formatImg(item.competitors[1].logo,'/11_50_50')@>"></i>
        </div>
        <div class="names-bottom">
            <span class="team-name"><@=item.competitors[0].name @></span>
            <span class="team-name"><@=item.competitors[1].name @></span>
        </div>
    </a>
</script>

