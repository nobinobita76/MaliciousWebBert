    <script type="text/html" id="date-item">
        <@ for (var i = 0, len = list.length; i < len; i++) { @>
        <@ if (list[i].name) { @>
        <li data-time="<@=list[i].day@>" data-type="day" data-today="true" class="today">
            <em>
                <@=list[i].name@>
            </em>
            <b></b>
            <i>
                <span class="day-txt"><@=list[i].day.substr(5)@></span>
                (<span class="chang">0场</span>)
            </i>
            <span class="arrow"></span>
        </li>
        <@ } else { @>
        <li data-time="<@=list[i]@>"  data-type="day">
            <i>
                <span class="day-txt"><@=list[i].substr(5)@></span>
                (<span class="chang">0场</span>)
            </i>
            <span class="arrow"></span>
        </li>
        <@ } @>
        <@ } @>
    </script>

    <script type="text/html" id="match-list">
        <@ if (data && data.length > 0) { @>
        <div class="content-item">
            <@ for (var i = 0, len = data.length; i < len; i++) { @>
            <@ if (i % 2 == 0) { @>
            <ul>
                <@ } @>
                <li data-period="<@=data[i].timeSection@>">
                    <@if (data[i].mid != 0) {@>
                    <div class="star-bg">
                        <span data-id="<@=data[i].id@>" class="star"></span>
                    </div>
                    <@}@>
                    <@if (data[i].isTextLive == 1 && data[i].isLive == 0) {@>
                    <a href="<@=data[i].tLiveLink@>" target="_blank">
                        <div class="live-type">
                            <span class="text-icon"></span>
                            <span>图文</span>
                        </div>
                        <@ } else { @>
                        <a href="<@=data[i].playLink@>" target="_blank">
                            <div class="live-type">
                                <span class="video-icon"></span>
                                <span>视频</span>
                            </div>
                            <@ } @><@if (data[i].isTextLive == 1 && data[i].isLive == 0) {@>
                            <a href="<@=data[i].tLiveLink@>" target="_blank">
                                <div class="live-type">
                                    <span class="text-icon"></span>
                                    <span>图文</span>
                                </div>
                                <@ } else { @>
                                <a href="<@=data[i].playLink@>" target="_blank">
                                    <div class="live-type">
                                        <span class="video-icon"></span>
                                        <span>视频</span>
                                    </div>
                                    <@ } @>
                                    <div class="item-top">
                                        <span class="clock"></span>
                                        <@var h = new Date(data[i].startTimeStamp * 1000).getHours()@>
                                        <@var hour = h >= 10 ? h : '0' + h@>
                                        <@var m = new Date(data[i].startTimeStamp * 1000).getMinutes()@>
                                        <@var minutes = m >= 10 ? m : '0' + m@>
                                        <i class="time"><@=hour@>:<@=minutes@></i>
                                    </div>
                                    <@if (data[i].showName){@><div class="item-type"><@=data[i].showName@></div><@}@>
                                    <div class="team-name">
                                        <div class="t-home">
                                            <@ if (data[i].vs == 1 && data[i].competitors.length > 0) { @>
                                            <@var oneLogo = data[i].competitors[0].logo@>
                                            <@if (oneLogo) {@>
                                            <@var oneIndex = oneLogo.lastIndexOf('.')@>
                                            <@var ologoUrl = oneLogo.substring(0,oneIndex) + '/11_70_70' + oneLogo.substring(oneIndex)@>
                                            <@ } else {@>
                                            <@var ologoUrl = 'http:\/\/i3.letvimg.com/lc07_iscms/201702/16/16/54/afe25b8e3c4e4e8188158af8cdb84926.png'@>
                                            <@}@>
                                            <img class="team-icon" src="<@=ologoUrl@>" alt="<@=data[i].competitors[0].name@>"/>
                                            <p class="name"><@=data[i].competitors[0].name@></p>
                                            <@ } @>
                                            <@ if (data[i].vs == 0) { @>
                                            <@var oneLogo = data[i].logo@>
                                            <@if (oneLogo) {@>
                                            <@var oneIndex = oneLogo.lastIndexOf('.')@>
                                            <@var ologoUrl = oneLogo.substring(0,oneIndex) + '/11_70_70' + oneLogo.substring(oneIndex)@>
                                            <@ } else {@>
                                            <@var ologoUrl = 'http:\/\/i3.letvimg.com/lc07_iscms/201702/16/16/54/afe25b8e3c4e4e8188158af8cdb84926.png'@>
                                            <@}@>
                                            <img class="team-icon" src="<@=ologoUrl@>" alt="<@=data[i].cname@>" />
                                            <@ } @>
                                        </div>
                                        <@ if (data[i].vs == 1 && data[i].competitors.length > 0) { @>
                                        <div class="score">
                                            <@ if (data[i].status == 0) { @>
                                            <div class="bf no-bf"></div>
                                            <i class="m-vs">vs</i>
                                            <em class="no-start">未开始</em>
                                                <@ if(data[i].isEpisodePay == 1) { @>
                                                    <em class="no-member">会员</em>
                                                <@ } @>


                                            <@ } @>
                                            <@ if (data[i].status == 1 || data[i].status == 2) { @>
                                            <@var oneScore = parseInt(data[i].competitors[0].score, 10)@>
                    <@var twoScore = parseInt(data[i].competitors[1].score, 10)@>
                    <@if (!isNaN(oneScore) && !isNaN(twoScore)) { @>
                    <div class="bf">
                        <@ if (oneScore > twoScore) { @>
                        <span><@=oneScore@></span> : <@=twoScore@>
                        <@ } else if (oneScore < twoScore) { @>
                        <@=oneScore@> : <span><@=twoScore@></span>
                        <@ } else { @>
                        <@=oneScore@> : <@=twoScore@>
                        <@ } @>
                    </div>
                    <@ } else { @>
                       <i class="m-vs">vs</i>
                    <@ } @>
                                            <@ } @>
                                            <@ if (data[i].status == 1) { @>
                                            <em class="is-live">比赛中</em>
                                            <@ if(data[i].isEpisodePay == 1) { @>
                                                    <em class="no-member">会员</em>
                                                <@ } @>
                                            <@ } @>
