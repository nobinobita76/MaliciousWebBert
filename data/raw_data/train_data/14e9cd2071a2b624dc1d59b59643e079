    <script type="text/javascript">
    function selectHot(kind) {
        if (kind == 1) {
            document.getElementById("select_f").className = "qon";
            document.getElementById("ul_football").style.display = "";
            document.getElementById("select_b").className = "qoff";
            document.getElementById("ul_basketball").style.display = "none";
        }
        else {
            document.getElementById("select_f").className = "qoff";
            document.getElementById("ul_football").style.display = "none";
            document.getElementById("select_b").className = "qon";
            document.getElementById("ul_basketball").style.display = "";
        }
    }
</script>
