<script language="javascript">
    $(function () {
        var i = 0; //当前行计数器
        function newsScroll() {
            if (i < (($('#news').height() - 10) / 25) - 1)//判断行数
            {
                $("#news").animate({ top: '-=25px' }, "slow");
                i++;
            } else {
                $("#news").animate({ top: '0px' }, "slow");
                i = 0;
            }
        }
        timer1 = setInterval(newsScroll, 1000)//值越小，速度越快。

    });

        var t, n = 0, count = $("#play_list a").size();
        var play_map = new Array();
        try {
            $(function () {
                settitle();
                $("#play_list a:not(:first-child)").hide();

                $("#play_text li:first-child").addClass("now");

                $("#play_info").click(function () { window.open($("#play_list a:first-child").attr('href'), "_blank") });
                $("#play_text li").click(function () {
                    var i;
                    for (i = 0; i < 4; i++)//
                    {
                        if ((i + 1) == $(this).text()) {
                            break;
                        }
                    }
                    n = i;
                    if (i >= count) return;
                    $("#play_info").html($("#play_list a").eq(i).find("img").attr('alt'));
                    $("#play_info").unbind().click(function () { window.open($("#play_list a").eq(i).attr('href'), "_blank") })
                    $("#play_list a").filter(":visible").fadeOut(200).parent().children().eq(i).fadeIn(400);
                    $(this).addClass("now").siblings().removeClass("now");
                });


                t = setInterval("showAuto()", 6000);
                $("#play").hover(function () { clearInterval(t) }, function () { t = setInterval("showAuto()", 6000); });
            })
        } catch (e) { }
        function settitle() {
            for (var i = 0; i < 4; i++) {
                $('#play_text li').eq(i).text(i + 1);//获取标题*/
            }
        }
        function showAuto() {
            n = n >= (count - 1) ? 0 : ++n;
            $("#play_text li").eq(n).trigger('click');
        }
	</script>
