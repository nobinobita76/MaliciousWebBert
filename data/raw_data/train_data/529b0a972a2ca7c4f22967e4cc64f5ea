   <script type="text/javascript">
$('.deletefavbtn').on('click', function() {
var obj = $(this);
$.ajax({
type:'POST',
url:obj.attr('href') + '&handlekey=favbtn&inajax=1',
data:{'deletesubmit':'true', 'formhash':'3e7c720f'},
dataType:'xml',
})
.success(function(s) {
window.location.reload();
})
.error(function() {
window.location.href = obj.attr('href');
popup.close();
});
return false;
});
</script> 
