		var zhoubian = "y";
		var IsHasEsf = zhoubian=='y'?true:false;
		var communityInfoCont=document.getElementById("communityInfo"),esfInfoCont=document.getElementById("esfInfo");
		if (BbsSortNew == "4") {
			if(communityInfoCont) communityInfoCont.style.display = "none";
			if(esfInfoCont) esfInfoCont.style.display = "none";
			DistrictShow(2, 4);
		} else if (BbsSortNew != "3") {
			if(communityInfoCont) communityInfoCont.style.display = "none";
			if (IsHasEsf == "true") {
				DistrictShow(1, 4);
			} else {
				if(esfInfoCont) esfInfoCont.style.display = "none";
				DistrictShow(2, 4);
			}
		} else {
			if (IsHasEsf != "true") {
				if(esfInfoCont) esfInfoCont.style.display = "none";
