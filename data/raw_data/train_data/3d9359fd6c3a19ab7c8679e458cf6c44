<script type="text/javascript">
	//获取cookie
	function getCookie(name)
	{
		var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
		if (arr = document.cookie.match(reg))
			return unescape(arr[2]);
		else
			return null;
	}
	function check_input() 
	{
		if (jQuery("#username").val() == "") {
			alert("请输入手机号/邮箱/用户名！");
			jQuery("#username").focus();
			return false;
		}
		if (jQuery("#password").val() == "") {
			alert("请输入密码！");
			jQuery("#password").focus();
			return false;
		}
		var pwdRtn = encryptedString(key_to_encode, jQuery("#strpassword").val());
		jQuery("#password").val(pwdRtn);
		return true;
	}
	//获取cookie
	function getCookie(name)
	{
		var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
		if (arr = document.cookie.match(reg))
			return unescape(arr[2]);
		else
			return null;
	}
	function errorBoxLogin() 
	{
		if (jQuery("#username").val() == "") {
			alert("请输入手机号/邮箱/用户名！");
			jQuery("#username").focus();
			return false;
		}
		if (jQuery("#password").val() == "") {
			alert("请输入密码！");
			jQuery("#password").focus();
			return false;
		}
		//var pwdRtn = encryptedString(key_to_encode, jQuery("#strpassword").val());
		var password = jQuery("#password").val();

		//基于https的通行证登录接入
		var uid = jQuery("#username").val();
		var backurl = jQuery("#backurl").val();
		var httpsToken = getCookie('token');
		var httpsService = "soufun-bbs-web";
		
		//alert("测试、检查遗漏，error_box.html，标注代码：第  56 行"+'----'+uid+'-------'+httpsToken+'----'+httpsService+'-----------'+backurl);
		jQuery.ajax({
			type: "get",
			url: "https://passport.fang.com/login.api",  //正式    
			//url: "https://passporttest.fang.com/login.api",//测试
			//url: '/NormalLoginInterface/Login',
			dataType: "jsonp",
			jsonp: "callback",
			data: { "Uid": escape(uid), "Pwd": escape(password), "Service": escape(httpsService), "Token": escape(httpsToken), "backurl":escape(backurl),"AutoLogin":"1" },
			xhrFields: {
				withCredentials: true
			},
			crossDomain: true,
			success: function (json) {
				if( json.Message=="Success" )
				{
					window.location.reload();
				}
				else
				{
					alert(json.Tip);
					jQuery("#username").val("");
					jQuery("#password").val("");
				}
			}
		});
	}
</script>
