			function checkFocusForum() {
				var forumgz = document.getElementById("forumgz");
				if(!forumgz) return;
				var new_loginid = GetPassportCookie("userid");
				if(!new_loginid) {
					forumgz.className = 'listjgz';
					forumgz.onclick = focusForum;
					return;
				}
				AJAXCALL(CurJsBbsHref+"ajax/focus_forum.php?city="+encodeURIComponent(City)+"&sign="+Sign+"&signName="+encodeURIComponent(forumName)+"&action=check", function(doc) {
					var data;
					try {
						data = eval("(" + doc + ")");
					} catch (e) { return; }

					if (data.succ > 1 || data.succ < 0) {
						//alert(data.error);
					} else {
						if(data.succ == 0) {
							forumgz.className = 'listjgz';
							if(window.addEventListener) {
								forumgz.addEventListener('click',focusForum);
							} else if(window.attachEvent) {
								forumgz.attachEvent('onclick',focusForum);
							}
						} else {
							forumgz.id = '';
							forumgz.className = 'listygz';
							if(window.removeEventListener) {
								forumgz.removeEventListener('click',focusForum);
							} else if(window.detachEvent) {
								forumgz.detachEvent('onclick',focusForum);
							}
						}
						forumgz.style.visibility='';
					}
				});
			}
			function focusForum() {
				var new_loginid = GetPassportCookie("userid");
				if(!new_loginid) {
					var tcdlk = document.getElementById('tcdlk');
					if(tcdlk) {
						tcdlk.style.display='block';
					} else {
						alert('请登录后再关注哦~');
					}
					return;
				}
				var forumgz = document.getElementById("forumgz");
				if(!forumgz) return;

				AJAXCALL(CurJsBbsHref+"ajax/focus_forum.php?city="+encodeURIComponent(City)+"&sign="+Sign+"&signName="+encodeURIComponent(forumName)+"&action=add", function(doc) {
					var data;
					try {
						data = eval("(" + doc + ")");
					} catch (e) { return; }

					if (data.succ > 1 || data.succ < 0) {
						alert(data.error);
					} else {
						forumgz.id = '';
						forumgz.className = 'listygz';
						if(window.removeEventListener) {
							forumgz.removeEventListener('click',focusForum);
						} else if(window.detachEvent) {
							forumgz.detachEvent('onclick',focusForum);
						}
					}
				});
			}
	</script>
		    
