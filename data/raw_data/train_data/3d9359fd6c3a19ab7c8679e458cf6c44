            <script>
        function show_recommand_menuc(id1,id2,index,count,classcurrent,classname){
                for(var i=0;i<count;i++){
                        var e2 = $(id2.toString()+i), e1 = $(id1.toString()+i);
                        if(e2) e2.style.display="none";
                        if(e1) e1.className=classname;
                }
                var e2n = $(id2.toString()+index), e1n = $(id1.toString()+index);
                if(e2n) e2n.style.display="block";
                if(e1n) e1n.className=classcurrent;  
        }
</script>
