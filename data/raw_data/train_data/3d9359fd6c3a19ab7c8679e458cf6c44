													<script type="text/javascript">
					function adloop(){
						var ado =['adao','adbo','adco','addo'];
						var adt = ['adat','adbt','adct','addt'];
						for(i in adt){
							if(document.getElementById(adt[i])){
							if(document.getElementById(ado[i]).style.display == 'block'){
								document.getElementById(ado[i]).style.display='none';
								document.getElementById(adt[i]).style.display='block';
							}else{
								document.getElementById(ado[i]).style.display='block';
								document.getElementById(adt[i]).style.display='none';
							}
							}
						}
						setTimeout(adloop,3000);
					}
					adloop();
					</script>
