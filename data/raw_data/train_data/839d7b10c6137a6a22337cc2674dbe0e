    <script type="text/javascript">
        //Extension object Array
        Array.prototype.exists = function(obj) {
            for (var i = 0; i < this.length; i++) {
                if (this[i] == obj) return true;
            }

            return false;
        };

        //Object QueryString
        var QueryString = {
            serialize: function(obj) {
                var list = [];

                if (obj.keyword) list.push('keyword=' + obj.keyword);
                if (obj.lon && obj.lat) list.push('coordinate=' + obj.lon + ',' + obj.lat);
                if (obj.minDistance && obj.maxDistance) list.push('distance=' + obj.minDistance + ',' + obj.maxDistance);
                if (obj.departurePlace) list.push('departurePlace=' + obj.departurePlace);
                if (obj.st) list.push('st=' + obj.st);
                if (obj.sc) list.push('sc=' + obj.sc);

                return list.length > 0 ? '?' + list.join('&') : '';
            },
            deserialize: function() {
                var dict = [], keyValuePairs;
                var queryString = window.location.search;

                if (queryString) {
                    queryString = queryString.substr(1, queryString.length); //remove   "?"
                    keyValuePairs = queryString.split('&');
                    if (keyValuePairs) {
                        for (var i = 0; i < keyValuePairs.length; i++) {
                            var keyValue = keyValuePairs[i].split('=');
                            if (keyValue.length == 2) {
                                dict.push({ name: keyValue[0], value: keyValue[1] });
                            }
                        }
                    }
                }

                return dict;
            },
            getValue: function (name) {            
                var dict = QueryString.deserialize();
                for (var i = 0; i < dict.length; i++) {
                    if (dict[i].name === name) return dict[i].value;
                }

                return undefined;
            }            
        };

        //Object Filters
        var Filters = {
            parse: function(str) {
                if (!str) return null;

                var dict = [];
                var matches = str.match(/[a-zA-Z]+[0-9]+/gi);
                if (matches) {
                    for (var i = 0; i < matches.length; i++) {
                        var match = matches[i];
                        var key = match.match(/[a-zA-Z]+/gi);
                        var value = match.match(/[0-9]+/gi);
                        dict.push({ key: key[0], value: value[0] });
                    }
                }

                return dict;
            },
            remove: function(filters, letters) {
                !letters ? letters = ['p'] : letters.push('p');

                for (var i = 0; i < letters.length; i++) {
                    var regExp = new RegExp(letters[i] + '\\d+', 'gi');
                    filters = filters.replace(regExp.valueOf(), '');
                }

                return filters;
            },
            join: function(originalFilters, dict) {
                var filters = '';
                if (originalFilters) {
                    filters += originalFilters;
                }

                if (dict) {
                    filters += dict.join('');
                }

                return filters;
            }
        };
    </script>
    
    <script type="text/javascript" src="http://webresource.c-ctrip.com/code/cquery/cQuery_110421.js"></script>
    <script type="text/javascript">
        window.$$ = {};
        window.$$.StartCity = 32;
        window.$$.Handler = {
            IndexHotStartCityNew: 'http://vacations.ctrip.com/Package-Booking-VacationsOnlineSiteUI/Handler/IndexHotStartCityNew.ashx',
            HotDestKeysNew: 'http://vacations.ctrip.com/Package-Booking-VacationsOnlineSiteUI/Handler/HotDestKeysNew.ashx?module=hotdest',
            HotDestFloatNew: "http://vacations.ctrip.com/Package-Booking-VacationsOnlineSiteUI/Handler/HotDestKeysNew.ashx",
            VacationSearch:"http://vacations.ctrip.com/handler/search/do"
        };

        function redirectTo(filters, queryString) {
            location.href = "/around/guangzhou/taocan/" + filters + queryString;
        }

        var WeekendAPI = {
            //自驾游，重新查询
            ListByGeographyAction: function(obj) {
                var filters = Filters.join(null, ['h4']), queryString = QueryString.serialize(obj);
                location.href = "/around/guangzhou-drive/taocan/" + filters + queryString;
            },
            //筛选区
            ListByFiltersAction: function(obj) {
                var values = obj.values;
                var keys = [];
                for (var i = 0; i < values.length; i++) {
                    var value = values[i];
                    if (value) {
                        var keyMatch = value.match(/[a-zA-Z]+/gi);
                        if (keyMatch) {
                            var key = keyMatch[0];
                            if (!keys.exists(key)) keys.push(key);
                        }
                    }
                }

                var filters = Filters.remove(document.getElementById("filters").value, keys);
                var queryString = location.search;

                filters = Filters.join(filters, values);
                redirectTo(filters, queryString);
            },
            //价格
            ListByPriceAction: function(obj) {
                var minPrice,maxPrice,priceRange,queryString;
                if (obj.minPrice && obj.minPrice > 0) {
                    //如果最大值没值，那么最大值=最小值
                    if(obj.maxPrice && obj.maxPrice > 0)
                    {
                        minPrice = obj.minPrice;
                        maxPrice = obj.maxPrice;
                    }
                    else
                    {
                        minPrice = 0;
                        maxPrice = obj.minPrice;
                    }
                    priceRange = minPrice + ',' + maxPrice;
                }
                else
                {
                    if(obj.maxPrice && obj.maxPrice > 0)
                    {
                        minPrice = 0;
                        maxPrice = obj.maxPrice;
                        priceRange = minPrice + ',' + maxPrice;
                    }
                }
                if(priceRange)
                {
                    queryString = (location.search ? location.search.replace(/&?price\=[^&]+/gi,'') + '&' : '?') + 'price=' + priceRange;
                }
                else
                {
                    queryString = "";
                }
                var filters = Filters.remove(document.getElementById("filters").value);
                redirectTo(filters, queryString);
            },
            //翻页
            ListByPageIndexAction: function(obj) {
                var filters = Filters.remove(document.getElementById("filters").value);
                var queryString = location.search;

                filters = Filters.join(filters, ['p' + obj.pageIndex]);
                redirectTo(filters, queryString);
            },
            ListByDateAction: function(obj) {
                var earliestDepartureDate = '';
                if (obj.maxDate) {
                    earliestDepartureDate = obj.maxDate;
                }
                var dateRange = earliestDepartureDate + ',';
                var queryString = (location.search ? location.search.replace(/&?date\=[^&]+/gi,'') + '&' : '?') + 'date=' + dateRange;
                var filters = Filters.remove(document.getElementById("filters").value);

                redirectTo(filters, queryString);
            },
            ListByLiveDateAction: function(obj) {
                //添加按入住日期查询逻辑
                var queryString = (location.search ? location.search.replace(/&?checkInDate\=[^&]+/gi,'') + '&' : '?') + 'checkInDate=' + obj.liveDate;
                var filters = Filters.remove(document.getElementById("filters").value);
                redirectTo(filters, queryString);
            },
            ListByClearDateAction: function(obj) {
                //添加清除日主日期查询逻辑
                var queryString = (location.search ? location.search.replace(/&?checkInDate\=[^&]+/gi,'') : '');
                var filters = Filters.remove(document.getElementById("filters").value);

                redirectTo(filters, queryString);
            }
        };
        
        // 处理热门目的地URL
        var INTERFACE = {
            hotDestAPI: 'http://m.ctrip.com/restapi/soa2/10149/json/GetNavigation?callback=?',
            DepartureCity: 32, //当前城市
            requestoptions: 3 //参数
        };
    </script>
