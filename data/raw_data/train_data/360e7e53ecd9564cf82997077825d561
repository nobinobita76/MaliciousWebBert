    <script type="text/javascript">
            //顶部分类菜单js
            $("#select li").click(function () {
                $(this).addClass("currt").siblings().removeClass("currt");
                $("#search_frm").attr("action", $(this).attr("data-search"));
                $("#key").focus();
                $("#key").val($("#key").val());
            })            
            // 点击搜索按键            
            $("#btnSo").click(function () {
                gotoso();
            })
            // 回车事件
            $('#key').bind('keydown', function (event) {
                if (event && event.keyCode == 13) { // enter 键
                    gotoso();
                    return false;
                }                
            })
            function gotoso()
            {
                if ($("#key").val().length > 0) {
                    for (var i = 0; i < $("#select li").length; i++) {
                        if ($('#select li').eq(i).attr("class") == "currt") {
                            var key_value = encodeURI($("#key").val());
                            if (typeof ($('#select li').eq(i).attr('data-search')) != 'undefined') {
                                location.href = $("#select li").eq(i).attr("data-search") + "?q=" + key_value;
                            }
                            else { // 搜索藏品
                                var url = window.location.href.toLowerCase();
                                // 判断当前哪个栏目为焦点
                                var sourl = "";
                                if (url.indexOf("http://trade.cang.com/") > -1 || url.indexOf("http://www.cang.com/trade/show-") > -1) {
                                    //搜索地摊【地摊站点、地摊详情】
                                    sourl = "http://so.cang.com/product/ditan?q=";
                                } else if (url.indexOf("http://jd.cang.com/") > -1) {
                                    sourl = "http://so.cang.com/product/estimation?q=";
                                }
                                else { // 古玩城为默认
                                    sourl = "http://so.cang.com/product/mall?q=";
                                }
                                location.href = sourl + key_value;
                            }
                        }
                    }
                }
            }
        </script> 
