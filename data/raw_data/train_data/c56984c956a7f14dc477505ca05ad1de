document.onkeydown=nextpage
function nextpage(event) {
	event = event ? event : (window.event ? window.event : null); 
if (event.keyCode==13) location="html/0//" 
 if (event.keyCode==37) location="book_j.php?bid=&cid=-1" 
if (event.keyCode==39) location="book_j.php?bid=&cid=1" 
 if (event.keyCode==45) window.open('book_sc.php?bid=&cid=',height=100, width=200); 
}
</script>
