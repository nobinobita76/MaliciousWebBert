//js翻页
var pageMax = $(".lib .sp-a i")
var pageBs = $(".lib .sp-b a")
pageMax.eq(1).text($(".page_max i").eq(1).text());
pageMax.eq(0).text($(".page_index strong").eq(0).text());
if($(".pager_sp2").length > 1){
    pageBs.eq(0).attr("href",$(".pager_sp2 a").eq(0).attr("href"))
	pageBs.eq(1).attr("href",$(".pager_sp2 a").eq(1).attr("href"))
	}else{
		if($(".pager_sp2 a").text() == "上一页"){
			pageBs.eq(0).attr("href",$(".pager_sp2 a").eq(0).attr("href"))
			}else{
				pageBs.eq(1).attr("href",$(".pager_sp2 a").eq(0).attr("href"))
				}
		}
pageBs.each(function(index, element) {
    if($(this).attr("href") == "javascript:void(0);" ){
		$(this).click(function(){
			if(index == 0){
				alert("这已经是第一页了!")
				}else{
					alert("这已经是最后一页了!")
					}
			})
		}
});
