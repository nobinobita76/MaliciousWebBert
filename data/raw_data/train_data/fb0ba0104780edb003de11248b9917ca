    <script type="text/javascript">
        //举报信息的submit点击事件
        function submitClick() {
            var txtWay = $("#txtWay").val(); //联系方式
            var txtNews = $("#txtNews").val(); //多文本框
            var txtResult = $("#reportResult").val();
            var SourceLink = document.URL;
            if (txtNews == "" || txtResult == "" || txtWay == "") {
                alert("举报原因、举报详情和联系方式不能为空！");
                return false;
            } else {
                $.ajax({
                    type: "post",
                    url: "/ReportData/insertReport.aspx",
                    data: "Url=" + "http://www.qihuiwang.com/Product/pro_mb.aspx?industryType=3&industryId=2960&id=189608317" + "&id=" + "189608317" + "&industryId=" + "4205" + "&Ip=" + "101.199.108.52" + "&txtNews=" + txtNews + "&rdoTxt=" + txtResult + "&txtWay=" + txtWay + "&SourceLink=" + SourceLink + "&type=0&Name=" + "【微商代理授权书范本】",
                    cache: false,
                    dataType: "text",
                    success: function (msg) {
                        if (msg == "1") {
                            reportdialog.close();
                        }
                        else {
                            reportdialog.close();
                        }
                    }
                });
            }
        }
        //短信询盘发送短信
        function submitInquiry() {
            var rePhone = /^13[0-9]{9}$|14[0-9]{9}|15[0-9]{9}$|18[0-9]{9}$|17[0-9]{9}$/;
            var contactName = $.trim($("#inqueryName").val());
            if (contactName == "") {
                alert("姓名不能为空！");
                return false;
            }
            var usertel = $.trim($("#inqueryTel").val());
            if (usertel == "" || !rePhone.test(usertel)) {
                alert("请正确填写联系电话！");
                return false;
            }
            if (usertel == "13101133999") {
                alert("不能向自己发送询盘信息！");
                return false;
            }
            if ($.trim($("#CheckCode").val()) == "") {
                alert("验证码不能为空！");
                return false;
            }
            $.ajax({
                type: "post",
                url: "/ajax/sendsmsmanager.aspx",
                data: $("#form2").serialize() + "&id=" + "1088081" + "&mobile=" + "13101133999" + "&contactName=" + contactName + "&Proname=" + "【微商代理授权书范本】" + "&url=" + "/product/q32960/189608317.html",
                cache: false,
                dataType: "text",
                success: function (msg) {
                    if (msg == "1") {
                        sendDialog.close();
                    } else {
                        alert(msg);
                    }
                }
            });
        }
    </script>
    <script type="text/javascript">
        $.fn.extend({
            reFormat: function () {
                //触发error
                $(this).on("error", function () {
                    if (typeof ($(this).attr("oldSrc")) == "undefined") {
                        $(this).attr("oldSrc", $(this).attr("src"));
                    }
                    var _jpg = /^jpg$/i;
                    var _this = $(this);
                    var _src = _this.attr("src");
                    var _defaultSrc = "http://img.qihuiwang.com/defaultImg/d-500.png";
                    if (_src.match(/\.(png|gif|jpeg|jpg)$/i)) {
                        //格式为jpg并error
                        if (_jpg.test(_src.substr(_src.lastIndexOf(".") + 1))) {
                            $(this).attr("src", _defaultSrc);
                            $(this).unbind("error");
                        }
                        else {
                            //替换jpg
                            $(this).attr("src", _src.replace(/\.(png|gif|jpeg)$/i, ".jpg"));
                        }
                    }
                    else {
                        $(this).attr("src", _defaultSrc);
                        $(this).unbind("error");
                    }
                });
            }
        });
        //调用
        $(".detsMain img").reFormat();
    </script>

    


