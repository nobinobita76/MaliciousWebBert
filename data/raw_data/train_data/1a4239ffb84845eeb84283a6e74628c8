            var target = event.currentTarget,
                width = $(target).width(),
                height = $(target).height();

            if (width > height) {
                var realWidth = w * width / height;
                $(target).height(h);
                $(target).width(realWidth);
                $(target).css('left', '50%');
                $(target).css('top', '0');
