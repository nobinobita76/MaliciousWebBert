var paginationEl=$(".pagination");
var commentListEl=$(".article-comments-list");
paginationEl.find("a").live("click",function(){
    var that=$(this);
     $.get(that.attr("href"),function(html){
        html=$(html);
        var pageHtml=html.find(".pagination").html();
        var commentListHtml=html.find(".article-comments-list").html();
        paginationEl.html(pageHtml);
        commentListEl.html(commentListHtml);
        window.location.href="https://lccs.kf5.com/hc/kb/article/204568/?from#article-comments";
     });
     return false;
});
//点击空白处，隐藏微信分享二维码
$(".share-list .wechat,.share-list .wechat *").attr("data-type","share-weixin");
$("body").click(function(event){
    if($(event.target).attr("data-type")!="share-weixin"){
        $(".wechat .qr-box").animate({opacity:0},400,function(){
            $(this).attr("style","display:none");
        });
    }
});
