					}]
			};
			if (typeof(hudongAdsTemplate) != "undefined" && typeof(hudongAdsTemplate.loadAds) == "function") {
				hudongAdsTemplate.loadAds(hudongAdDocOptions);
			}
		});
		$.ajaxSetup({cache: false});
		
	//百科倾听-优惠券增加逻辑
	$("#alllink_yh_0").click(function (){
		$("#alllink_yh_1").removeClass("on");
		$("#alllink_yh_2").removeClass("on");
		$("#alllink_yh_0").addClass("on");
		$("#type_1").hide();
		$("#type_2").hide();
		$("#type_0").show();
	})
	$("#alllink_yh_1").click(function (){
		$("#alllink_yh_0").removeClass("on");
		$("#alllink_yh_2").removeClass("on");
		$("#alllink_yh_1").addClass("on");
		$("#type_0").hide();
		$("#type_2").hide();
		$("#type_1").show();
	})
	$("#alllink_yh_2").click(function (){
		$("#alllink_yh_1").removeClass("on");
		$("#alllink_yh_0").removeClass("on");
		$("#alllink_yh_2").addClass("on");
		$("#type_1").hide();
		$("#type_0").hide();
		$("#type_2").show();
	})
	
});
