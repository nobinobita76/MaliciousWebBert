function initCreateDoc(){
	var isLogin = getHdCookie("hduser") != null;
	var loginUrl = "http://passport.baike.com/login.do?site_ource=zz_page";
	if($("#create_doc_btn").length > 0){
		$("#create_doc_btn").click(function(){
			var self = this;
			var redirectUrl = $(self).attr("action")+"&prd=so_chuangjian";
			if(!isLogin){
				redirectUrl = loginUrl+"&service="+encodeURIComponent(redirectUrl);
			}
			document.location = redirectUrl;
			return false;
		});
	}
}
