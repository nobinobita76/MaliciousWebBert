    function chsTw(p_page){
        $(".sherry_recommend_pages a").each(function(){
            id = parseInt($(this).html())-1;
            if(id==p_page){
                $(this).addClass("sherry_recommend_pages_now");
            }else{
                $(this).removeClass("sherry_recommend_pages_now");
            }
        });
        var id = 0;
        $(".sherry_recommendtions li").each(function(){
            if(id<(p_page+1)*4 && id>=(p_page)*4){
                $(this).show();
            }else{
                $(this).hide();
            }
            id++;
        });
    }
