  <script>
    var isLogin = 'False'
    $(".shang").click(function () {
        if (isLogin == "False") {
            location.href = "/login?reurl=" + location.href;
        }
    });
    $("#content").focus(function () {
        if ($(".tools a.face").hasClass("active")) {
            $(".tools a.face").removeClass("active");
            $(".rich .faces").hide()
        }
        if (isLogin == "False") {
            location.href = "/login?reurl=" + location.href;
        }
    });
    $(function () {
        var n = 0;
        var fid = "";
        //点击选择好友
        $("ul.contact_list").on("click", "li", function () {
            var num = $("ul.contact_list li span.choice.on").length;
            if ($(this).find("span").hasClass("on")) {
                n--;
                var oid = $(this).find(".customer_photo").attr("id");
                var name = $(this).find(".customer_photo").attr("name");
                var nick = $(this).find(".customer_photo").attr("nick");
                fid = fid.replace(oid + "," + name + "," + nick + "|", "")
                $("#atname").val(fid);
                $("span.wancheng").find("i").html(n);
                if (num <= 1) {
                    $("span.wancheng").removeClass("checked");
                }
                $(this).find("span.choice").removeClass("on");
                $(".friend").find("#" + oid + "").remove();
            } else {
                if (num > 4) {
                    tipFun("最多只能@5个好友~");
                } else {
                    n++;
                    var oid = $(this).find(".customer_photo").attr("id");
                    var name = $(this).find(".customer_photo").attr("name");
                    var nick = $(this).find(".customer_photo").attr("nick");
                    fid += oid + "," + name + "," + nick + "|";
                    $("#atname").val(fid)
                    $("span.wancheng").addClass("checked").find("i").html(n);
                    $(this).find("span.choice").addClass("on").end().
                        find("input").attr("checked", true);
                    $(this).find(".customer_photo").clone().prependTo(".friend");
                }
            }
        });

        $("#at").click(function () {
            if ($("ul.contact_list").html().indexOf("li") > 0) {
                $("div.friend_wrap").show();
                n = 0;
                fid = "";
                $("div.friend").append('<span class="circle"></span>');
                $("span.wancheng").find("i").html(n);
                $("#atname").val("");
            } else {
                tipFun("你还没有关注的好友~");
            }
        });
    })

    //点击完成按钮
    $("div.finishi").on("click", "span.wancheng.checked", function () {
        $("div.friend").html("");
        $("ul.contact_list li span.choice.on").removeClass("on");
        $("div.friend_wrap").hide();
        $(".review .tools .at").removeClass("active");
        if ($("#arrorbox").length > 0) {
            $("#arrorbox").remove();
        }
        var atvalue = "";
        if ($("#atname").val() != "") {
            var atname = $("#atname").val().split('|');

            for (var i = 0; i < atname.length - 1; i++) {
                var item = atname[i].split(',');
                if (item.length > 0) {
                    atvalue += "@" + item[2] + " ";
                }
            }
        }
        $("#content").append(atvalue);
        $("#atnameinfo").val(atvalue);
    });
	var nick='';
    $(".zan,.a_zan").click(function () {
        if ($(this).attr("isOwn") == "true") {
            tipFun("不能给自己点赞~");
            return false;
        }
        else {
            zan(this);
        }
    });

    function showReplyInfo(obj) {
        var $this = $(obj);
        var replyId = $this.parent("dd").attr("data-id3");
        var louNum = $this.children("em").attr("floor");
        location.href = "/bbs/replyInfo/?topicId=" + "10108122" + "&replyId=" + replyId + "&louNum=" + louNum;
    }

    function setAsk(obj) {
        var $this = $(obj);
        var replyId = $this.parent().attr("replyid");
        var floor = $this.parent().attr("floor");
        location.href = "/bbs/replyInfo/?topicId=" + "10108122" + "&replyId=" + replyId + "&louNum=" + floor;
    }

    function setBest(obj) {
        var $this = $(obj);
        var userId = $this.parent().attr("userid");
        var replyId = $this.parent().attr("replyid");
        var floor = $this.parent().attr("floor");
        var param = { topicID: '10108122', up: floor, userID: userId };
        $.post("/bbs/SetAnswer", param, function (data) {
            if (data.code == "1000") {
                tipFun(data.message, function () { location.reload() });
            }
            else {
                tipFun(data.message);
            }
        });
    }
    //打赏
    var money = '';
    $('.renyi_item').keypress(function (e) {
        if (!String.fromCharCode(e.keyCode).match(/[0-9\.]/)) {
            return false;
        } else {
            $(".renyi_item").css({ "font-size": "18px", "color": "#ff7d00" });
            $(".rit_content .ds_left").css("color", "#245dff");
            $(".jinbi_item").removeClass("jinbi_hover");
        }
    });
    $(".renyi_item").focus(function () {
        $(".jinbi_item").removeClass("jinbi_hover");
        $(this).css("border", "1px solid #999");
    }).blur(function () {
        $(this).css("border", "1px solid #ccc");
    })
    if ($(".renyi_item").text() == 0 || $(".renyi_item").text() == "") {
        $(".rit_content .ds_left").css("color", "#ccc");
    }
    //余额不足取消
    $(".bal_cancel").click(function () {
        dsClose();
    })
    $(".jinbi_item").not(".renyi_item").click(function () {
        $(this).addClass("jinbi_hover").siblings().removeClass("jinbi_hover");
        $(".rit_content .ds_left").css("color", "#245dff");
        $(".renyi_item").val("");
    })
    //点击打赏
    $("div.info_wrap a.shang").click(function () {
        if ($("#hidUserID").val() == '0') {
            tipFun("请先登录~");
            window.location.href = "/login/?reurl=" + window.location.href;
            return;
        }
        if (!$("body").hasClass("nosilde")) {
            $("body").addClass("noslide");
        }
        $(".rit_content").show();
        $(".rit-maskCon").show();
    });
    function checkInputNum(ele) {
        if (!/^([1-9]){1,2}$/g.test(ele.val().replace(".",""))) {
            var str = ele.val().substr(0, 2)
            ele.val(str);
            tipFun("最多打赏99哦~~")
        }
    }
    Dir('center', $(".rit_content"));
    Dir('center', $(".balance_not"));
    //点击打赏
    $(".rit_content .dialog-footer1 .ds_left").click(function () {
         var tid = $("#hidTid").val();
        if ($(".jinbi_item").hasClass("jinbi_hover")) {
            var mon = Number($(".jianbi_list").find(".jinbi_hover em").text());
            if (mon > money) {
                $('.balance_not').show();
                $(".rit_content").hide();
            } else {
                smAjax({
                    url: "/BBS/SetBBSReward",
                    type: "post",
                    data: { coinNum: mon, tid: tid },
                    success: function (data) {
                        if (data.flag == 1000) {
                            $("div.info_wrap a.shang").addClass("on");
                            $("div.info_wrap a.shang em").text($("div.info_wrap a.shang em").text() * 1 + 1); 
					        if (data.reMsg != "undefined" && data.reMsg != "") {
                                goMove('.msg', data.reMsg);
					        }
					        tipFun(data.message);
					        var hitR = parseInt($("nav.list-nav em:eq(0) i").html());
					        var hitD = parseInt($("nav.list-nav em:eq(2) i").html());
					        if (hitR == 0 && hitD == 0) {
                                window.location.reload();
					        } else {
					            $("nav.list-nav em:eq(2)").addClass("active").siblings().removeClass("active");
					            $("nav.list-nav em:eq(2) i").html(parseInt($("nav.list-nav em:eq(2) i").html()) + 1);
					            $("div.tie_tab>div.tie_cont:eq(2)").show().siblings().hide();
					            pageIndex3 = 1;
					            $(".tie_cont:eq(2)").html("");
					            GetListData("/BBS/GetBBSDaShangList", 2, 0, '', pageIndex3, 0);
					        }
                            dsClose();
                        } else if (data.flag == 99) {
                            window.location.href = "/login/?reurl=" + window.location.href;
                        } else {
                            tipFun(data.message);
                        }
                    }
                });
            }
        } else {
            if ($(".renyi_item").val() == "0" || $(".renyi_item").val() == "") {
                tipFun('请输入1-99之间的金额哦~~');
            } else {
                var mon = Number($(".renyi_item").val());
                if (mon > money) {
                    $('.balance_not').show();
                    $(".rit_content").hide();
                } else {
                    smAjax({
                       url: "/BBS/SetBBSReward",
                        type: "post",
                        data: { coinNum: mon, tid: tid },
                        success: function (data) {
                            if (data.flag == 1000) {
                                $("div.info_wrap a.shang").addClass("on");
                                $("div.info_wrap a.shang em").text($("div.info_wrap a.shang em").text() * 1 + 1); 
					            if (data.reMsg != "undefined" && data.reMsg != "") {
                                    goMove('.msg', data.reMsg);
					            }
					            tipFun(data.message);
					            var hitR = parseInt($("nav.list-nav em:eq(0) i").html());
					            var hitD = parseInt($("nav.list-nav em:eq(2) i").html());
					            if (hitR == 0 && hitD == 0) {
					                window.location.reload();
					            } else {
					                $("nav.list-nav em:eq(2)").addClass("active").siblings().removeClass("active");
					                $("nav.list-nav em:eq(2) i").html(parseInt($("nav.list-nav em:eq(2) i").html()) + 1);
					                $("div.tie_tab>div.tie_cont:eq(2)").show().siblings().hide();
					                pageIndex3 = 1;
					                $(".tie_cont:eq(2)").html("");
					                GetListData("/BBS/GetBBSDaShangList", 2, 0, '', pageIndex3, 0);
					            }
					            dsClose();
                            } else if (data.flag == 99) {
                                window.location.href = "/login/?reurl=" + window.location.href;
                            } else {
                                tipFun(data.message);
                            }
                        }
                    });
                }
            }
        }
    })
    function dsClose() {
        $(".rit_content").hide();
        $(".balance_not").hide();
        $(".rit-maskCon").hide();
        if ($("body").hasClass("noslide")) {
            $("body").removeClass("noslide");
        }
    }
    $(".rit-maskCon").click(function () {
        dsClose();
    })
</script> 
