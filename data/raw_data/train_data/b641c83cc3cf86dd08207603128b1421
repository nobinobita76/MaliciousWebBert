<script>
  (function() {
    KISSY.use('node', function(S,n) {
      var dl = n.one('.help-menu');
      if(!dl) return;
      dt = dl.all('dt');
      dt.on('click',function(){

        var p = n.one(this).parent();

        p[0].className = (p.hasClass('current')?"":"current");

      });
      S.one(window).on('scroll', function() {
        if (S.one(document).scrollTop() > 500) {
          return S.one('.float-tool .top').show();
        } else {
          return S.one('.float-tool .top').hide();
        }
      });
      return S.all('#J_gotoTop').on('click', function() {
        return S.all('body,html').animate({
          scrollTop: 0
        }, .2);
      });
    })

    KISSY.use('node, cookie, json', function(S, n, c, json){
      //清空
      n.one('.J_clear').on('click', function(e){
        e.preventDefault();
        c.remove("search_history");
        n.one('.J_search_history').remove();
      });
    })
  }).call(this);
</script>

<script src="//g.alicdn.com/etao/opensearch/0.15.5/scripts/jquery-1.9.1.min.js"></script>
<script src="//g.alicdn.com/opensearch/opensearch-console/0.16.0/scripts/jquery-ui-1.10.2.js"></script>
<script>
   $(function() {

        //给Input添加自己输入提示
        var hit = 10; // 显示的下拉提示列表的长度，可修改

        addSuggest("#top-search-box","#top-search-form");
        addSuggest("#foot-search-box","#foot-search-form");
        function addSuggest(inputid,formid) {
            var aInput=$(formid).find("input");
            setTimeout(function ()  {
                //伪静态化处理
                $(formid).submit(function(){
                    var _this=this;
                    this.action="/ss/"+Base64.encodeURI(aInput.eq(0).val());
                    setTimeout(function (){_this.submit()});
                    
                    return false;
                })
                $(inputid).autocomplete({
                    delay: 0,
                    source: function (request, response) {
                        var reqStr = $.trim($(inputid).val());
                        if (reqStr.length < 1) {
                            return;
                        }
                        $.ajax({
                            url: '/s/ajax', 
                            xhrFields: {
                                withCredentials: true
                            },
                            dataType: 'json',
                            data: {                             //定义请求
                                k: reqStr,
                            },
                            success: function (data) {
                                if (data.suggestions) {
                                    var arr = [],
                                            len = data.suggestions.length;
                                    for (var i = 0; i < len; ++i) {
                                        if (data.suggestions[i]['suggestion']) {
                                            arr.push(data.suggestions[i]['suggestion']);
                                        }
                                    }
                                    if (arr.length >= hit) {
                                        response(arr.slice(0, hit));
                                    } else {
                                        response(arr);
                                    }
                                }
                            }
                        });
                    },
                    select: function (e, ui) {
                        $(inputid).attr("vaule", ui.item.value);
                        setTimeout(function(){
                            $(formid).submit();
                        },0);
                    }
                }).bind("input.autocomplete", function () {
                    // 修复在Firefox中不支持中文的BUG
                    $(inputid).autocomplete("search", $(inputid).val());
                });


            }, 0);
        }


        //给其他搜索模块的显示和隐藏
        var otherBtn = $(".other-search .other-search-btn"),
            otherList = $(".other-search .other-content-list"),
            otherIcon=$(".other-search .other-search-btn i"),
            bListShowing=false;



        otherBtn.click(function(){
            if(bListShowing){
                otherList.animate({
                    "opacity":0
                },300,function(){
                    otherList.slideUp(300);
                });

            }else{
                otherList.slideDown(300,function(){
                    otherList.animate({
                        "opacity":1
                    },300);
                });
            }
            otherIcon.toggleClass('icon-down3');
            otherIcon.toggleClass('icon-up3');
            bListShowing=!bListShowing;
        })
    });

</script>
