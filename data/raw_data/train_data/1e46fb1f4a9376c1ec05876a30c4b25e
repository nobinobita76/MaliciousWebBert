<script type="text/javascript">function $Trans(show){
	var $box = null;
	var $len = 0;
	var $arr = new Array();
	var $delay = 4000;
	var $number = null;
	var $oThis = this;
	var $key = 0;
	var $timeout = null;
	var $showNumber = show && true;
	var $last = 0;
	
	this.loadFromElement = function(id)
	{
		var t = document.getElementById(id);
		$arr = t.getElementsByTagName('div');
		$len = $arr.length-1;
		return;
	}
	
	this.setBox = function(id)
	{
		$box =test(id);
	}
	
	this.setDelay = function(delay)
	{
		$delay = delay;
	}
	
	this.display = function()
	{
		if(!$box)return;
		$box.innerHTML = '';
		if($showNumber)
		{
			_showNumber();
		}
		_play(parseInt(Math.random()*$len+1));
	}
	
	function _showNumber(){
		var str = '';
		var id = $box.id;
		for(var i = 1; i <= $len; i++)
		{
			if (i == 1) 
			{
				str += '<li style="list-style:none;float:left;width:18px;height:13px;line-height:13px;text-align:center;margin-left:2px;background:#ffffff;" title="' + (i) + '"><a style="cursor:pointer;display:block;width:18px;height:13px;font-size:10px;font-family:Tahoma;color:#333333;">' + i + '</a></li>';
			}
			else
			{
				str += '<li style="list-style:none;float:left;width:18px;height:13px;line-height:13px;text-align:center;margin-left:2px;background:#ffffff;" title="' + (i) + '"><a style="cursor:pointer;display:block;width:18px;height:13px;font-size:10px;font-family:Tahoma;color:#333333;">' + i + '</a></li>';
			}
		}
		$box.innerHTML =  str;
		$number = (test(id)).getElementsByTagName('li');
		for(var i = 0; i < $len; i++)
		{
			$number[i].onclick = function(){_play(this.title);}
		}
		$last = 1;
	}
	
	function _change()
	{
		for (i=0;i<$len;i++) 
		{
			$arr[i].style.display = "none" ;
			if($showNumber)
			{
				$number[i].getElementsByTagName('a')[0].style.color = '#000000';
				$number[i].getElementsByTagName('a')[0].style.background = '#ffffff';
			}
		}
		if ($arr[$key])
		{
			$arr[$key].style.display = "block" ;
			if($showNumber)
			{
				$number[$key].getElementsByTagName('a')[0].style.color = '#fff';
				$number[$key].getElementsByTagName('a')[0].style.background = '#ffa900';
			}			
		}
	}
	
	function _play(key){
		window.clearTimeout($timeout);
		_newKey(key);
		_change();
		if($len > 1)$timeout = window.setTimeout(function(){_play()}, $delay);
	}
	
	function _stop(){
		window.clearTimeout($timeout);
	}
	
	function _newKey(key){
		if($showNumber)$number[$key].getElementsByTagName('a').className = '';
		$key = (key ? key-1 : $key + 1) % $len;
		if($showNumber)$number[$key].getElementsByTagName('a').className = 'sel';
	}

	function test(_id)
	{
		if(typeof _id != 'string')return _id
		var _e = document.getElementById(_id);
		if(_e)return _e;
		_e = document.getElementsByName(_id);
		if(_e.length == 1)return _e[0];
		return _e;
	}
}


var trs_A = new $Trans(true);
trs_A.loadFromElement('adm_show_1250');
trs_A.setBox('adm_cnt_1250');
trs_A.setDelay(4000);
trs_A.display();</script>
