    var t = document.getElementById(id);
    $arr = t.getElementsByTagName('div');
    $len = $arr.length;
    var i = getOrder_1252($len);
    $arr[i].style.display = "block";

}
playList_1252("adm_show_1252");

function getOrder_1252(arrayLength) {

    var number = parseInt(Math.random() * arrayLength + 1);
    var NameOfCookie = "admanager_1252";
    var c = document.cookie.indexOf(NameOfCookie + "=");
    if (c != -1) {

        number = getCookie_1252(NameOfCookie);
        number++;
        document.cookie = NameOfCookie + "=" + number;


    } else {
        document.cookie = NameOfCookie + "=1";
    }
    var i = number % arrayLength;
    return i;
}

function getCookie_1252(name) {
    var strCookie = document.cookie;
    var arrCookie = strCookie.split("; ");
    for (var i = 0; i < arrCookie.length; i++) {
        var arr = arrCookie[i].split("=");
        if (arr[0] == name) return arr[1];
    }
    return "";
}</script>
