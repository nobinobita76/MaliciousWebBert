  <script>
        var token = "8ed5c90f151d57ee4af544b1590229c5";
        $.ajax({                                              
            type: "get",
            url: "http://ass.vccoo.com/feed3_art.html?token=" + token,
            dataType: "jsonp",
            jsonp: 'callback',
            jsonpCallback: "jc_nextart",
            success: function (json) {
                if(json){                
                    var nextArt = json.data;                   
                    var ThumbUrl = nextArt.ThumbUrl;
                    var MsgTitle = nextArt.MsgTitle;
                    var MsgUrl = nextArt.MsgUrl;
                    var MsgDesc = nextArt.MsgDesc;
                    $("#shareNextArt").attr("href", MsgUrl).attr("title", MsgTitle);
                    var html = '<span></span><img src="'+ThumbUrl+'" alt="'+ MsgTitle+ '">'+
                              '<div class="overlay">下一篇</div><div class="nextArticle-postTitle"><h3>'+MsgTitle+'</h3></div>';
                    $("#shareNextArt").html(html);
                    $("#shareNextArtBottom").attr("href", MsgUrl).attr("title", MsgTitle).html('<h3>'+MsgTitle+'</h3>');
                    $("#shareNextArtDesc").html(MsgDesc);
                }
            }
        });
  </script> 
