        <div id="newContainer" class="newContainer"> 
         <div class="head">
          热点事件
         </div> 
         <div class="content"> 
          <link href="http://cdn.vccoo.com/css/timeline.css?v=17.02.17.01" rel="stylesheet" /> 
          <div id="news-first" class="news-first"> 
           <div style="    height: 100%;"> 
            <div class="animated-background"> 
             <div class="background-masker header-left"></div> 
             <div class="background-masker header-right"></div> 
             <div class="background-masker header-bottom"></div> 
             <div class="background-masker subheader-left"></div> 
             <div class="background-masker subheader-right"></div> 
             <div class="background-masker subheader-bottom"></div> 
             <div class="background-masker subheader1-left"></div> 
             <div class="background-masker subheader1-right"></div> 
             <div class="background-masker subheader1-bottom"></div> 
            </div> 
           </div> 
          </div> 
          <script>
    function setCityData() {
        $.ajax({
            type: "get",
            url: "http://ass.vccoo.com/feed3_index.html",
            dataType: "jsonp",
            jsonp: 'callback',
            async: true,
            jsonpCallback: "success_jsonpCallback",
            success: function (json) {
                if (json) {
                    $("#news-first").html("");
                    var newJson = eval(json);
                    var htmlStr = "";
                    var shortcity = newJson.shortcity;
                    var newList = newJson.data;
                    if (newList != null && newList.length > 0) {
                        var model = newList[0];
                        var nickName = model.NickName;
                        if (nickName != null && nickName.Length > 10) {
                            nickName = nickName.Substring(0, 10) + "…";
                        }
                        htmlStr2 = '<div class="news-first-img"><a href="' + model.MsgUrl + '" target="_blank" title="' + model.MsgTitle + '">' +
                         '<img class="news-pic" src="' + model.ThumbUrl + '" alt="' + model.MsgTitle + '" style="opacity: 1;"><span></span></a></div>';
                        $("#news-first").append(htmlStr2);
                        htmlStr1 = '<div class="news-first-content">' +
                         '<div class="news-first-title"><a href="' + model.MsgUrl + '" target="_blank" title="' + model.MsgTitle + '"><h3>' + model.MsgTitle + '</h3></a></div>' +
                         '<div class="news-first-name"><a href="' + model.PubUrl + '" target="_blank" title="' + model.NickName + '">' + nickName + '</a>' +
                         '<a id="local" class="local" title="' + shortcity + '"><i class="fa-local"></i>' + shortcity + '</a></div></div>';
                        $("#news-first").append(htmlStr1);

                    }
                }

            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
            }
        });
    }


</script> 
