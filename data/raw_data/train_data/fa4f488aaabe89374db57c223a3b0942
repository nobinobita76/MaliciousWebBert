<script type="text/javascript">
function upheader(){
	var keyword=$("#header_keyword").val();
	var form=document.getElementById("header_formId");
	if(keyword==""){
		alert('请输入关键词');
		return;
	}else{
		form.submit();
	}
}
function searchxzssss_ww(key){
	var form=document.getElementById("header_formId");
	document.getElementById('header_keyword').value = key;
	document.getElementById('new_header_indexnameId').value = "bn";
	form.submit();
}

</script>

