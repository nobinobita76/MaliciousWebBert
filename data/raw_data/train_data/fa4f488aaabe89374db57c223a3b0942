<script type="text/javascript">
   	var tsarea=document.getElementById("tsarea");
   	if(tsarea!=null){
   		tsarea=tsarea.value;
	$.post('/php/check_sign_in.php', {'tsarea':tsarea}, function(result) {
		var denglu = document.getElementById("tt20111227");
		if(denglu) denglu.innerHTML = result;
	});
	$.post('/php/islogin.php', {}, function(result) {		
		if(result) {
			$.post('/php/sylogin2.php',{},function(re){
				document.getElementById("hydl20111229").className = "hydl_20120504";
				document.getElementById("hydl20111229").innerHTML = re;
			});
		}		
	});
   	}
</script>
	
