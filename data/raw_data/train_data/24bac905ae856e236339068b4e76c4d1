<script type="text/javascript">
var js_bbs_url = "http://bbs.wdzj.com/";
var global_uid = "";

var timerNav = null;
$(".h-circle").on("mouseover", function() {
    $(".selectMenu").show();
});

$(".h-circle").on("mouseout", function(event) {
    var s = event.toElement || event.relatedTarget;
    var self = this;
    clearTimeout(timerNav);
    timerNav = setTimeout(function() {
        if (!self.contains(s)) {
            $(".selectMenu").hide();
        }
    }, 100);
});

$(".selectMenu").on("mouseover", function() {
    clearTimeout(timerNav);
});

$(".selectMenu").on("mouseout", function(event) {
    var s = event.toElement || event.relatedTarget;
    if (!this.contains(s)) {
        $(".selectMenu").hide();
    }
});
</script>
   
