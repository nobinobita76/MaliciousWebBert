            <script type="text/javascript">
                                 
        var timer = null;
        var bbs_index = 0;
            
                
                function hideView(){
                    $('#diy_reward').val('');
                    $(".mask").hide();
                    $(".bbs-reward").hide();
                }
                
                $(".bbs-close-btn").click(function(){
                    hideView();
                });
                
                
                $(".bbs-cancel").click(function(){
                    hideView();
                });
                


                $(".bbs-radio span").click(function(){
                    isSel();
                    $(this).find("em").addClass("bbs-rAct");
                    bbs_index = $(this).index();
                $('#diy_reward').val('');
                });

        
        $('.other-money').focus(function(){
            timer = setInterval(function(){
                if($(".other-money").val() != ""){
                    isSel();
                } else {
                    $(".bbs-radio span").eq(bbs_index).find("em").eq(0).addClass("bbs-rAct");
                }
            },30);
        });

        $(".other-money").blur(function(){
            clearInterval(timer);
        });


                $('.img-reward').click(function () {
                    var url = "http://bbs.wdzj.com/" + 'thread/is_login';
                    $.ajax({
                        type: 'get', url: url, data: {},
                        cache: 'false',
                        success: function (data) {
                            if(data.success == 1){
                                $('.bbs-zanzhu').show();
                            }else{
//                                $('.bbs-login').show();
                                window.location.href = 'https://passport.wdzj.com/user/login';
                                return
                            }
                            $('.mask').show();
                            $('.bbs-reward').show();
                        },
                        error: function () {
                        }
                    });
                });


        function isSel(){
            var $span = $(".bbs-radio span");
            for(var i=0; i<$span.size(); i++){
                if($span.eq(i).find("em")){
                    $span.eq(i).find("em").eq(0).removeClass("bbs-rAct");
                }
            }
        }
                var bTrue = false;
                $(".reward-more").click(function(){
                   $(this).hide();
                   $(".reward-head").css("height",'auto');
                })
                
                var submitTrue = true;
                $(".reward_submit").click(function(){
                  if(!submitTrue) return ;
                  submitTrue = false;
                  num = $('.bbs-rAct').attr('gold');
                  if (typeof(num) == "undefined"){
                      num = $('#diy_reward').val();
                  }
                  if(num==0){
                      alert('亲，多给一点')
                      submitTrue = true
                      return 
                  }
                  url = "http://bbs.wdzj.com/thread/reward";
                  $.ajax({
                        type: 'post', 
                        url: url, 
                        data: { tid:934156,gold:num },
                        cache: 'false',
                        success: function (data) {
                            var code = data.error_code;
                            if(code == 0){
                                $('.bbs-reward').hide();
                                endStatus(1,'打赏成功');
                                setTimeout(function(){
                                    window.location.reload();
                                },1000);
                            }else if(code == 2){
                                $('.bbs-yue span').html("余额"+data.data.gold+"金币，<a href='http://member.wdzj.com/member/accout?type=integral' target='_blank'>先去攒金币吧！</a>");
                                $('.bbs-yue span').show();
                            }else if(code == 1){
                                alert('请登录后操作');
                            }else if(code == -1){
                                alert('系统繁忙');
                            }else{
                                alert(data.msg)
                            }
                            submitTrue = true;
                        },
                        error: function () {
                        }
                    });
                })
               
            
                               
            var onclikComment = true;
            //回复提交表单
            $('#commentsubmit').click(function ()//提交表单 
            {           
                if(!onclikComment)return
                //判断表单内容
                var tid = 934156
                var fid = 42
                var parent_id = $('#pidinput').val();
                editor.sync();
                var message = editor.text(); // jQuery            
                message = message.replace(/\ +/g, "");//去掉空格
                message = message.replace(/[ ]/g, "");//去掉空格
                message = message.replace(/[\r\n]/g, "");//去掉回车换行  
                if (message == '') {
                    endStatus(0,'回复不能为空');
                    return false;
                }
                var message = $('#editor_id').val();
                
                var $end = $(".commenting").find(".bbs-chk").attr('data-val')
                onclikComment = false;
                $('#commentsubmit strong').html('评论中');
                $.ajax({

                    type: "post",
                    url: "http://bbs.wdzj.com/thread/reply",
                                        data: { fid:fid, tid:tid, parent_id:parent_id, message:message },
                                        dataType: "json",
                            success: function(data){
                            if (data.error_code == 0) {
                                commentTotal ++;
                               endStatus(1,'回复成功');
                               if($end == 'true'){                         
                                   getComment(1,1);
                               }else{
                                   getComment(1,1);
                               }
                               var parent_id = $('#pidinput').val('0');
                               window.editor.html('');
                            }else{
                                endStatus(0,data.msg,1);
                            }
                            yanzm();
                            onclikComment = true;
                        }
                })
            })
            
            function threadComment(type){
                if(type){
                    getCommentAuthor(1053805,1,1);
                    $('#threadComment').attr('onclick','threadComment(0)');
                    $('#threadComment').html('查看全部');
                }else{
                    getComment(1,1,1);
                    $('#threadComment').attr('onclick','threadComment(1)');
                    $('#threadComment').html('只看TA');
                }
            }
            
            
//            快速回复
            var onclikCommentfast = true;
            $('#smessage').click(function ()//提交表单 
            {    
                                window.location.href = "https://passport.wdzj.com/user/login";
                return false;
                
                //判断表单内容
                var tid = 934156
                var fid = 42
                var parent_id = 0;
                var fastmessage = $('#fastmessage').val();
                if (fastmessage == '') {
                    endStatus(0,'回复不能为空');
                    return false;
                }
                if(!onclikCommentfast)return
                onclikCommentfast = false;
                $.ajax({
                    type: "post",
                    url: "http://bbs.wdzj.com/thread/reply",
                    data: { fid:fid, tid:tid, parent_id:parent_id, message:fastmessage },
                    dataType: "json",
                            success: function(data){
                           if (data.error_code == 0) {
                                commentTotal ++ ;
                               endStatus(1,'回复成功');
                               getComment(1,1);
                               var parent_id = $('#pidinput').val('0');
                               $('#fastmessage').val('');
                            }else{
                                endStatus(0,data.msg,1);
                            }
                            
                            onclikCommentfast = true;
                            }

                        })
                })
                
                
                 //显示已投票的信息
                function  fwin_viewvote(polloptionid) {
                    $('.vote-list').show();
                     $('.mask').show();
                    $.ajax({
                        type: "get",
                        url: "http://bbs.wdzj.com/thread/ajaxViewvote?tid=934156&polloptionid=" + polloptionid,
                        dataType: "html",
                        success: function (data) {

                            json = eval('(' + data + ')');

                            if (json.error_code == 1) {
                                //显示投票选项
                                if (polloptionid == '') {
                                    $(".pst").html(json.data.options[0].polloption);
                                    $(".ps").empty();
                                    var html = "";
                                    $.each(json.data.options, function (name, value) {
                                        html += '<li onclick="fwin_viewvote(' + value.polloptionid + ')">' + value.polloption + '</li>';
                                    });
                                    $(".ps").append(html);
                                }

                                //显示投票信息
                                $(".person-list").empty();
                                if (json.data.voters.length <= 0) {
                                    $(".person-list").append('<li><p>无</p></li>');
                                    return false
                                }
                                var htm = '<li>';
                                $.each(json.data.voters, function (name, value) {
                                    htm += '<p><a  href="http://member.wdzj.com/space-'+value.uid+'.html">' + value.username + '</a></p>';
                                });
                                htm += '</li>';
                                $(".person-list").append(htm);

                            }
                        }
                    })
                }
                
                
