                $.ajax({
                        type: "get",
                        url: "http://bbs.wdzj.com/adv/hongbaoAdv",
                        data: { labelName:name },
                        success: function (data) {
                            $(".hongbao_adv").html(data);
                        }
                    });
                   
            })
                function yanzm(){
                        var $this = $('.yanzm');
                        $('#seccode').val('');
                        var url = "http://bbs.wdzj.com/" + 'seccode';
                        $.ajax({
                            type:'get', url: url, data: { type:'post_seccode' },
                            cache:'false',
                            success: function(data){
                                if (data) $this.html(data);
                            },
                            error: function(){
                            }
                        });
                    }
