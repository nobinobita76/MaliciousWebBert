<script type="text/javascript">











function tel_getAuthcodeRequest()
{
    var telnum = $.trim($("#tel_phone").val());
    if (telnum == null || telnum == "")
    {
        alert("手机号不能为空");
        return;
    }
    if (!/^[0-9]{11}$/.test(telnum))
    {
        alert("请输入正确的11位手机号码");
        return;
    }
    tel_getAuthcode(telnum);
}

 function tel_requestOrder()
 {
     var authcode = $.trim($("#tel_authcode").val());
     if (authcode == null || authcode == "")
     {
         alert("验证码不能为空");
         return;
     }
     
     tel_order(tel_phone_num, authcode, productcode);
 }

function tel_shareRequest()
{
    var sharetelnum = $.trim($("#tel_sharephone_input").val());
    if (sharetelnum == null || sharetelnum == "")
    {
        alert("分享手机号不能为空");
        return;
    }
    if (!/^[0-9]{11}$/.test(sharetelnum))
    {
        alert("请输入正确的11位手机号码");
        return;
    }
    tel_share(tel_phone_num, tel_authcode, productcode, sharetelnum);
}

</script>






