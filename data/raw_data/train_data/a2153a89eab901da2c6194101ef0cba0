<script>
	function setGo() {
		if(event.keyCode==13){
			goPageNum(2,'/product_0_0_0_{pageNow}.html');
		}
	}
	function go2page() {
		goPageNum(2,'/product_0_0_0_{pageNow}.html');
	}
   function goPageNum(count,listhref) {
	   var p = document.getElementById("txt_goto").value;
		if (p == "" || !/^[1-9]+[0-9]*]*$/.test(p)) {
		  alert('页码不正确');return;
		}
		if(p<=0 || p> count) {
			 alert('页数不正确');
			 return;
		}
		location.href = listhref.replace('{pageNow}',p);
		
	}
		
