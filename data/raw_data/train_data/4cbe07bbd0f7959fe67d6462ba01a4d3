</style><script type="text/javascript">    	$(document).ready(function(){

    	    /*滚动条动画*/

    		$(".smooth").click(function(){

    			var href = $(this).attr("href");

    			var pos = $(href).offset().top;

    			$("html,body").animate({scrollTop: pos}, 500);	

    			return false;

    		});

    		

    		/*头部切换效果*/

    		var page = 1; //假设第一个版面为1

    		var i = 4;    //假设第一个版面有4张图片

            var v_show = $(".nav-list"); 

            var _w = v_show.width(); //获取版面的宽度

            var len = v_show.find("li").length; //获取整个版面中有多少个li

            var page_count = Math.ceil(len/i);  //计算共有几个版面     Math.ceil只要为正数，就向上取整

            var ulWidth = _w * page_count;      

            $(".nav-list ul").css("width",ulWidth);

    		$("span.next").click(function(){

    		   if(!v_show.is(":animated")){  //判断是否处于动画 

				   if(page==page_count){

						v_show.animate({left:'0px'},'slow');

						page=1;

						

					}else{	

						v_show.stop().animate({left:'-='+_w},'slow');

						page++;

					}

                }

    		});

    		$("span.prev").click(function(){

                if(!v_show.is(":animated")){

                    if(page==1){

                         v_show.animate({left:'-='+_w*(page_count-1)},'slow');

                         page=page_count;

						

                    }else{

                        v_show.animate({left:'+='+_w},'slow');

                        page--;

                    }

                }

            });

    	});

