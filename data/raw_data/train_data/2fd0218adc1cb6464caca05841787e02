	<script type="text/javascript" src="http://special.zhaopin.com/test/dscnav/2016/szcode/js/zpspecialmod_xz.js"></script>
	<script type="text/javascript">
		JobsFilter.keywordtips = "输入职位关键字";
		JobsFilter.noresultTips = "<center><span class=\"unnamed1\"><b>抱歉，没有符合条件的职位！</b></span></center>";
		JobsFilter.dateSepa = "-";
		JobsFilter.getDataObjByArr = function(arrData) {
			return {
				division: arrData[6], //分公司ID
				zdept: arrData[9], //部门ID
				jobarea: arrData[3], //地点ID
				funtype: arrData[7], //职位类别ID
				issuedate: arrData[4], //发布日期
				tagname: arrData[8], //职位标签
				keyword: [arrData[0], arrData[2]] //职位名称中文 部门名称中文 地点中文
			};
		};
	</script>
	<script type="text/javascript">
		var cid;
		var strUrl;
		cid = "";
		strUrl = window.location.href;
		pos = strUrl.indexOf("?");
		if(pos > 0) {
			parastr = strUrl.substring(pos + 1);
			pos = parastr.indexOf("=");
			if(parastr.substring(0, pos) == 'comid') {
				cid = parastr.substring(pos + 1);
			}
		}
		$(".c_" + cid).show();
		$(document).ready(function() {
			/*if(cid == "")    
			{    
			    cid = "";默认显示ID    
			}    */
			$(".com_" + cid).show();
			/*if(cid == "1057282"){
				$(".it").show();
			}
			else{
				$(".it").hide();
			}*/
			if(cid == "1057283"){
				$(".notice").show();
			}
		});
		$(".apply").on("click", function() {
			$(this).parent().parent().next("tr").toggle();
		});
    $(".deptName").on("click",function(event) {
      var deptid = $(this).data("deptid");
      if($(".job_"+deptid).is(":hidden")){
        $(".dept_jobs").hide();
        $(".job_con").hide();
        $(".job_"+deptid).show();
      }else{
        $(".job_con").hide();
        $(".dept_jobs").hide();
      }
    })
	</script>
	<script>
		$(function() {
			$('.conTxt').each(function() {
				var t = $(this).html(),
					i = t.indexOf("1,");
				$(this).html(t.substring(i + 2));
			});
		})
	</script>
      <script>
  	$(function(){
		$("#division").change( function() {
			var tmp = $(this).val();
			window.location.href="career_result.html?division="+tmp;
			if(tmp=="1057292"){
				window.location.href="http://zhaopin.telecomjs.com";
			}
			else if(tmp=="1057283"){
				window.location.href="http://special.zhaopin.com/campus/2016/bj/zgdx092358/";
			}
			else if(tmp=="1057303"){
				window.location.href="http://job.gx.vnet.cn";
			}
			else if(tmp=="1057302"){
				window.location.href="http://hr.gdtel.com.cn";
			}
			else if(tmp=="1057295"){
				window.location.href="http://job.fjtelecom.com";
			}
			else if(tmp=="1000000"){
				window.location.href="career_result.html?division=1080647|1080648|1080649|1080650|1080651|1080652|1080653|1080654|1080655|1080656|1080657|1080658|1080659|1080660|1080661";
			}
		});
	})
  </script>
	
