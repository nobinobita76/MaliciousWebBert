	var areacode = '110101';
    var specialArea = '0';
    var sortcmsid = '5352';
    function recomBaidu(kw){
         ws ='http://www.baidu.com/s?wd='+encodeURIComponent(kw)+' site:findlaw.cn&tn=SE_hldp05850_hkve2jn0';
         window.open(ws);
    }
	$(document).ready(function(){
		var jDiscussBtn = $('.Qa_discuss>p>a');
		var jAnswerCommentBtn = $('div.Asked_content>div.Comment>p.Limited_to>a');

		jDiscussBtn.hoverTip();
		jDiscussBtn.filter('[popup=true]').click(function(){
			var jThis = $(this);
			var sPopupname = jThis.attr('popupname');
			if (sPopupname == 'report') {
				QUES.currReportData = jThis.attr('data');
			} else if (sPopupname == 'answerAdd' || sPopupname == 'adoption') {
				QUES.currAnswerAddUser = jThis.closest('div.Adoption_Content').find('div.Adoption_left>p.name>a');
				$('#answerAddUsername, #adoptionAnswerUsername').attr('href', QUES.currAnswerAddUser.attr('href')).text(QUES.currAnswerAddUser.text());
				if (sPopupname == 'answerAdd') {
											var sTitle = '回复 <a href="javascript:" class="popup_name">' + QUES.user + '</a> 追问';
                        var answerAddPopupOne2OneHref = '';					var aid = $(this).parent('p').attr('aid');
					HB.cache.set('cur_answerAdd_aid', aid);

                    $('#popup__answerAdd').remove();
                    $('#popup__report').remove();
                    HB.cache.set('ask.popup.' + sPopupname, null);

					HB.ask.popup(sPopupname, {}, function(){
                        $('#answerAddPopupTitle').html(sTitle);
                        if (answerAddPopupOne2OneHref!='') {
                            $('#answerAddPopupOne2OneTips').show().find('a').attr('href', answerAddPopupOne2OneHref);
                        }
                    });
				} else {
					$('#adoptionAnswerUsername').attr('href', QUES.currAnswerAddUser.attr('href')).text(QUES.currAnswerAddUser.text());

					jThis.attr('nocacheUrl', 'http://china.findlaw.cn/ask/question_35846777.html?nocache=1');
					HB.cache.set('adoptionClickBtn',jThis);
				}

				QUES.currPostData = jThis.attr('data');
			}
			HB.ask.popup(sPopupname);
		});
		jDiscussBtn.filter('.dis').click(function(){
			$(this).closest('.Qa_discuss').next('.Asked_content').toggle(200);
		});

		jAnswerCommentBtn.click(function(){
			var jThis = $(this);
			var iAid = jThis.attr('aid');
			if (iAid < 0) return false;
			var sContent = $('#answerCommentContent_' + iAid).val();
			var cflag = $("input[name='cflag_"+ iAid +"']:checked").val();
			if (typeof(cflag) == 'undefined') {
				HB.ask.alert('勾选或请填写评论内容！');
				return false;
			}
			if(sContent=='' || typeof(sContent) == 'undefined'){
				sContent = (cflag==1)?'这个回复对我很有帮助':'这个回复对我帮助不大';
			}
			if (HB.cache.get('answerCommentSubmit_' + iAid)) return false;
			HB.cache.set('answerCommentSubmit_' + iAid, true);
			$.post(
				'/ask_front/?m=ask&c=Question&a=answerComment',
				{aid: iAid, answerCommentContent: sContent,cflag:cflag,qid:QUES.qid,requestmode:'async'},
				function(sResponse) {
					var oResponse = HB.JSON.parse(sResponse);
					sResponse = null;
					HB.ask.alert(oResponse.error, '', oResponse.errno === 0 ? 'answerComment' : '',function(){
							if (oResponse.errno === 0) {
								location.reload();
							}
						});
					HB.cache.set('answerCommentSubmit_' + iAid, false);
					//if (oResponse.errno === 0) {location.reload();}
				}
			);
		});

		$('div.Adoption_right>div.Qa_discuss>i[type=supportcount]').click(function(){
			var jThis = $(this);
			var iAid = jThis.closest('div.Adoption_Content').attr('aid');
			var sCookieName = 'answerVoteList';
			var _sCookie = HB.cookie.get(sCookieName);
			if (_sCookie) {
				if (_sCookie.indexOf(',' + iAid + ',') !== -1) return false;
			} else {
				_sCookie = ',';
			}

			if (jThis.data('addSupportSending') || jThis.data('addSupportSended')) return false;
			jThis.data('addSupportSending', true);
			$.get('/ask_front/?m=ask&c=Question&a=addAnswerSupport&requestmode=async&qid='+ QUES.qid +'&aid=' + iAid, function(sResponse){
				var oResponse = HB.JSON.parse(sResponse);
				sResponse = null;
				if (oResponse.errno == 0) {
					_sCookie += iAid + ',';
					var oNow = new Date();
					var oExpire = new Date(oNow.getFullYear(), oNow.getMonth(), oNow.getDate(), 23, 59, 59);
					HB.cookie.set(sCookieName, _sCookie, (oExpire.getTime()-oNow.getTime())/1000);

					jThis.removeClass('cltip').next('.tipc.').fadeIn(300).delay(3000).fadeOut(300);
					var iCurrSpcount = parseInt(jThis.text(), 10);
					jThis.text(++iCurrSpcount);
					jThis.data('addSupportSended', true);
				}
				jThis.data('addSupportSending', false);
			});
		});

		HB.ask.IDCard($('.Adoption_left .p_img a img'));
		$.get('/ask_front/?m=ask&c=ajax&a=updateQuesClicks&requestmode=async&qid=35846777');
	});   
</script>
