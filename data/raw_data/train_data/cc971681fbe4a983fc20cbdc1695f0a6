<script type="text/javascript">
$(document).ready(function(){
	var jSiteMap = 	$('#Site_map');
	var jHelper = $('#helper');
	var jMessageCenter = $('#message_center');

	jSiteMap.add('.Site_Map_list').hover(
		function(){
			jSiteMap.addClass('hover');
			$('.Site_Map_list').css('display','');
		},function(){
			jSiteMap.removeClass('hover');
			$('.Site_Map_list').css('display','none');
		}
	);

	jHelper.hover(
		function(){
			jHelper.addClass('hover');
			$('#din').removeClass('din');
			$('#din').addClass("dib");
		},function(){
			jHelper.removeClass('hover');
			$('#din').addClass("din");					
		}
	);
	
	jMessageCenter.hover(
		function(){
			jMessageCenter.addClass('hover');
			$('#dinc').removeClass('din');
			$('#dinc').addClass("dib");
		},
		function(){
			jMessageCenter.removeClass('hover');
			$('#dinc').addClass('din');	
		}
	);
	
	$('#usercode_ipt, #password_ipt').inputTip();
});
</script>    

