<script type="text/javascript">
function asktjyz () {
    var havedsubmitask = $.cookie('havedsubmitask');
    if (havedsubmitask != null) {
        alert('您发布咨询速度太快了，请稍等片刻。');
        return false;
    }
    var mob = $('#askmob').val();
    var con = $('#questionContent').val();
    if (mob != '') {
        if(!(/^1[3|4|5|7|8][0-9]\d{4,8}$/.test(mob))){
            alert("手机号填写错误");
            $('#askmob').focus();
            return false;
        }
        $('#askmob1').attr('value', mob);
    }
    
    if (con == '' || con == '有法律问题，马上在线咨询专业律师！') {
        alert('问题内容不能为空');
        $('#questionContent').focus();
        return false;
    } else {
        if (con.length > 1999) {
            alert('问题内容不能大于2000个字符');
            $('#questionContent').focus();
            return false;
        } else if (con.length > 20) {
            $('#questionTitle').attr('value', con.substr(0, 20));
        } else {
            $('#questionTitle').attr('value', con);
        }
    }
    return true;
}
$('.ask-content').focus(function() {
    if (!$('.content-warning-box').is(':hidden')) {
        $('.content-warning-box').hide();
    }
   
    $('.content-main').addClass('to-green');
    $('.font-num2000,.content-tips-box').show();
}).blur(function() {
  
    $('.content-main').removeClass('to-green');
    $('.font-num2000,.content-tips-box').hide();

    var $contentLength = $('.ask-content').val().length;
    if ($contentLength < 10 || $contentLength > 2000) {
        $('.content-warning-box').show();
    }
});
</script>

