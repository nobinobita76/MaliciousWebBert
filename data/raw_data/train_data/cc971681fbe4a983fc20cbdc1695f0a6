<script type="text/javascript">
var needValidate = 0;
function showMaskBox()
{
    $('.mask1002').show();
    $('.tips-box').show().css('top', $(window).scrollTop()+100);
}

function hideMaskBox()
{
    $('.mask1002').hide();
    $('.tips-box').hide();
}
$(document).ready(function(){
        // 提问者禁止对自己问题进行回答
        if("" == "wl6615towi"){
            $('#questionReplyArea').css('display','none');
        }

        $('#questionHandle a').click(function(){
            HB.ask.popup($(this).attr('popupname'));
            });
        $('#answerContent').inputTip({sTip: '温馨提示：回答即可得2分，答案被当事人采纳则获得悬赏分以及奖励10分。'}).click(function(){
            $('#answerForm>.post_c>p>span').text('尊敬的用户登录回复会更便捷。').css('color', 'red');
            });
        if (HB.cookie.get('replycon_'+QUES.qid)) {
        $('#answerContent').css('color','#555').val(HB.cookie.get('replycon_'+QUES.qid));
        }

        //咨询回答
        var jOpenAnswerBtn = $('#openAnswerBtn');
        var jQuestionReplyArea = $('#questionReplyArea');
        jOpenAnswerBtn.toggle(
            function(){
            jOpenAnswerBtn.addClass('Reward_rgb');
            jQuestionReplyArea.slideDown(200);
            },
            function(){
            jOpenAnswerBtn.removeClass('Reward_rgb');
            jQuestionReplyArea.slideUp(200);
            }
            );
        jOpenAnswerBtn.click();
        $.post(
                '/ask_front/?m=ask&c=Question&a=getQuestionScore',
                {qid: QUES.qid,requestmode:'async'},
                function(sResponse) {
                var oResponse = HB.JSON.parse(sResponse);
                sResponse = null;
                if (oResponse.errno == 0) $('#questionScore').text(oResponse.score);
                }
              );

        var jAnswerForm = $('#answerForm');
        jAnswerForm.submit(function(){
                if (needValidate === 1) {
                showMaskBox();
                return false;
                }
                if ($('#answerContent').val() == '' || $('#answerContent').val() == '温馨提示：回答即可得2分，答案被当事人采纳则获得悬赏分以及奖励10分。') {
                HB.ask.alert('回复内容不能为空！');
                return false;
                }
                //判断是否有律师回答过相同回答
                if (hasReplyCon($('#answerContent').val())) {
                HB.ask.alert('真巧，已经有律师回复了相同的内容哦！你可尝试给公众更多有用的信息，谢谢');
                return false;
                }
                var content = $('#answerContent').val().replace(/(^\s)|(\s$)/g,'');
                var checkcontentkeyword = true;
                $.ajaxSetup({
async : false
});
                $.post('/ask_front/?m=ask&c=ajax&a=checkillegal&type=2',{keyword:content},function(ret){
                    if (ret.status==2){
                    HB.ask.alert('回复内容中'+ret.info);
                    checkcontentkeyword = false;
                    }
                    }, 'json');
if (!checkcontentkeyword) {
    return false;
}
var islogin = "<{islogin}>";
if (islogin) {
    $('#answerSubmit').css('cursor', 'wait').parents('span').next().css('display', 'block');
    return true;
} else {
    HB.cookie.set('replycon_'+QUES.qid, $('#answerContent').val());
    HB.ask.popup('login');
}
return false;
});
$('#opa_phone').inputTip();
});
</script>
