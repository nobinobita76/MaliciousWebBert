<script type="text/javascript"> 
function switchto(method){
	var loc = "/uniportal/forwardLogin.do?getloginMethod="
	loc += method;
	var redirect = encodeURIComponent("http://consumer-tkb.huawei.com/tkb/tskb_index_v2.html");
	loc += "&redirect=";
	loc += redirect;
	window.location = loc ;
}
</script>

