seajs.use(['JQuery'], function(Entry) {
    seajs.use('JQueryCookie', function () {
            var  $layout_toTouch= $('#layout_toTouch');
            // 手机电脑切换
            $layout_toTouch.on('click', function () {
                var now = new Date(),
                expirationDate = new Date(now.getFullYear(), now.getMonth(), now.getDate()+3, 0, 0, 0);
                $.cookie('modelType', 'touch',  { expires: expirationDate, path: '/',domain:".jiaoyimao.com" });
                window.location.href=$(this).attr('ext-attr');
            });
        });
    });
