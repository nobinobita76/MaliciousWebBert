<script type="text/javascript">
   var jsVersion = '2017033018';
   seajs.baseUrl = "http://www.jiaoyimao.com/";
   seajs.staticUrl = "http://image.jiaoyimao.com/";
   seajs.imageUrlPrefix = "https://static.jiaoyimao.com";
   seajs.jsMode = 'app/';

   seajs.globe = {
		   urlRegi: 'https://api.open.uc.cn/cas/register?redirect_uri=https%3A%2F%2Fwww.jiaoyimao.com%2Flogin%3FredirectUrl%3Dhttp%253A%252F%252Fwww.jiaoyimao.com%252Fgoods%252F1490862986596774.html&client_id=94&display=pc'
   };

   
        seajs.jsMode = 'dist/' + seajs.jsMode;
   

   seajs.loadModule = function(moduleName){
       var toString = Object.prototype.toString;
       moduleName = seajs.jsMode +  moduleName ;

       seajs.use(moduleName, function(Entry){
           if(toString.call(Entry) == '[object Function]'){
               var entry = new Entry();
               entry.init();
           }else if ( toString.call(Entry) == '[object Object]'  && Entry.init){
               Entry.init();
           }
       });
   };

   seajs.config({
        base : "http://image.jiaoyimao.com/public/pc/js/dev",
        
        crossorigin: true,
        
        alias : {
            
            "JQuery" : "lib/jquery-1.10.2.js",
            "JQueryCookie" : "lib/jquery.cookie-1.4.0.min.js",
            "Mustache" : "lib/mustache.js",
            "WdatePicker" : "lib/date/WdatePicker.js",
            "JSON" : "lib/json2.js",
            "_": "lib/lodash.min.js",
            "moment" : "lib/moment.js",
            "ErrorLogger" : "lib/ErrorLogger.js",
            "chosenJQuery" : "lib/chosen.jquery.js",
            "handlebars" : "lib/handlebars-v2.0.0.js",
            "WebUploader" : "lib/webuploader/webuploader.min.js"
        },
        map:[
             [ /^(.*\/lib\/.*\.(?:css|js))(.*)$/i, '$1?' + jsVersion],
             [ /^(.*\/app\/.*\.(?:css|js))(.*)$/i, '$1?' + jsVersion],
             [ /^(.*\/common\/.*\.(?:css|js))(.*)$/i, '$1?' + jsVersion]
        ]
    });

   seajs.use('JQuery', function(Entry){
       $(document).ready(function () {
    	   onPageReady();
       });
   });

   function onPageReady() {
	   /**加载页面js模块**/
   		$("div[seaModule]").each(function(){
       	    var moduleName = $(this).attr("seaModule") ;
       	    seajs.loadModule(moduleName);
    	});
   }
</script>
