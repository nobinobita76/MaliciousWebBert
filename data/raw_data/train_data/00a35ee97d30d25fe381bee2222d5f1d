function switchToOld()
{
	document.cookie = "aq_ver=redirect_to_aq_v1;PATH=/; DOMAIN=aq.qq.com";
	top.location.href="/cn/index";
}
function hide_ad_app()
{
	$("#ad_app_priv").hide();
}
