<script language="javascript">	

function changeDivStyle(divid,type)
{
	if("in"==type)
	{
		$("#"+divid).attr("class","hoverOn");
	}
	else if("out"==type)
	{
		$("#"+divid).attr("class","hover");
	}
}

$("#verifybtn").click(function () {
	$("#verifyimage").attr("src", getCaptchaImageURL());//更换验证码图片链接。
	});
	
$("#verifyimage").attr("src", getCaptchaImageURL());//更换验证码图片链接。

document.onkeydown=function(event)
{
	event = event?event:(window.event?window.event:null);
	if (event.keyCode==13)
	{
		$("#next").click();
	}
};

$("#next").click(function(){
	if(!checkvcajax($("#verifycode").val(),true))
	{
		return false;
	}
	else
	{
		if($("#oldCertNo").val()==cert_cue_word)
		{
			$("#oldCertNo").val("");
		}
	  
		$("#1bao_info").submit();
		return true;
	}
});

var cert_cue_word = "请填写您以前设置的证件号码";
function checkvcajax(vcode,issubmit)
{
	var ret = checkVerifyCode($("#verifycode").val());
	var reValue = false;
	if( ret==0)
	{
		if(issubmit)//提交时才进行ajax检查
		{
			$.ajax({
			type:"POST",
			url:"/cn2/ajax/check_verifycode",
			data:"verify_code="+vcode,
			dataType:"json",
			async:false,
			success:function(json){
				if(json.Err!="0") 
				{
					$("#verifytip")[0].style.display="";	
					$("#verifytip")[0].innerHTML="您输入的验证码有误";
					changeCaptchaImage('verifyimage', 'verifycode');
				}
				else
				{
					$("#verifytip")[0].style.display="none";
					reValue = true;
				}
			  }
			});
		}
		else
		{
			$("#verifytip")[0].style.display="none";
			reValue = true;
		}
	}
	else if(ret==1)//空
	{
		$("#verifytip")[0].style.display="";	
		$("#verifytip")[0].innerHTML="请输入验证码";
	}
	else
	{
		$("#verifytip")[0].style.display="";	
		$("#verifytip")[0].innerHTML="您输入的验证码有误";
	}
	
	return reValue;
};

$(document).ready(function()
{
	$("#oldCertNo").css({color:"#ccc"});
	$("#oldCertNo").blur(function()
	{
		 if($("#oldCertNo").val()=="")
		 {
			$("#oldCertNo").val(cert_cue_word);
			$("#oldCertNo").css({color:"#ccc"});
		 }
	});
	$("#oldCertNo").focus(function()
	{
		 if($("#oldCertNo").val()==cert_cue_word)
		 {
			$("#oldCertNo").val("");
			$("#oldCertNo").css({color:""});
		 }
	});	
	
	//设置密码明文显示
	$("#cotrlpaw").click(
		function()
		{
			var spanID;
			var passID;
			for(var i = 1; i < 4; i++)
			{
				var xType = "password";
				if($("#cotrlpaw").attr("checked"))
				{
					xType = "text";
				}
				
				spanID = "#pwd" + i;
				passID = "hispwd" + i;
				$(spanID).html("<input type=\""+xType+"\" value=\""
					+$("#" + passID).val()+"\"name=\""+passID+"\" id=\""+passID
					+"\" class=\"input_text gray\" maxlength=\"16\" onfocus=\"changeDivStyle('baseinfo','in')\" onblur=\"changeDivStyle('baseinfo','out')\"/>");
			}
		}
	);
	
	$("#verifycode").blur(
		function()
		{
			checkvcajax($(this).val(),false);
		}
	);
	
	$("#verifycode").focus(
		function()
		{
			$("#verifytip")[0].style.display="none";	
		}
	);
});
</script>
