<script type="text/javascript">
	  $(function(){
		  var hidden_part_H = $(".hidden_part").outerHeight(true);
		  var window_H = $(window).height();
		  if(hidden_part_H< window_H){
			$(".Continue_reading").hide();
		  }else{
			$(".Continue_reading").click(function(){
			  $(".article-con").animate({"max-height":hidden_part_H+20+'px'},"fast");
			  $(this).hide();
			});           
		  }


	  });
</script>


