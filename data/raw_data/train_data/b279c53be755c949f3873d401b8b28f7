$(function(){
	pic301();
	$(".detailcontent1 iframe").each(function(){ 
	 	var src=$(this).attr("src");
		if(src.indexOf("v.qq.com")==-1) return;
		var reg = /.*width\=(\d+).*/g;
		var reg0 = /.*width\=([^\&]+).*/g;
   		var w = src.replace(reg,"$1");
   		var w1 = src.replace(reg0,"$1");
		var reg1 = /.*height\=(\d+).*/g;
		var reg2 = /.*height\=([^\&]+).*/g;
   		var h = src.replace(reg1,"$1");
   		var h1 = src.replace(reg2,"$1");
		src=src.replace(w1,w);
		src=src.replace(h1,h);
		$(this).attr("src",src);
		if (h!=""){	$(this).attr("height", h);}
	 	if (w!=""){$(this).attr("width", w); }
		$(this).wrap("<div style='text-align:center; text-indent:0;'></div>");
	});
	$(".detailcontent1 img").each(function(){  
		$(this).wrap("<div style='text-align:center; text-indent:0;'></div>");
	});
	$('.sback').click(function(){
		document.body.scrollTop=0;
	});
	var RUrl='https://mp.weixin.qq.com/s?__biz=MzI4NTM3NDk5MQ==&tempkey=0dVMZW8fz9QW%2FLPjbRFMKhKThwUruamoyCDbPqQCk3lVGSXMhxZyc4t5WbBmgHURCJAxQ1Pd6QzIAkBgLFUU1hMVJ0LfQFRknfP0UH8Rzv1MZdx6aMp0E9mSELgQQV6yu%2BbSS6gdgBGod2vzjRZUSg%3D%3D&chksm=6bef9c125c981504af8ba38a796869b0ce73f96c043e408c09866ad606ab9e753c8704432c51#rd';
