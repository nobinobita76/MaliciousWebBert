                        $(function () {
                           
                            $(".top_select li").bind("click", function () {
                                $(".top_select li").removeClass("hover_on");
                                $(this).addClass("hover_on");
                                aIndex = $(".top_select li").index(this);
                                $(".infos").addClass("hide");
                                $(".infos:eq(" + aIndex + ")").removeClass("hide");
                                
                            });
                            Loade();
                        });
                        function Loade() {
                            var p = '54097';
                            var g = '817821';
                            Ajax("/Intro", { p: p, g: g }, function (data) {
                                tempdata = data;
                                if (data.list != null) {
                                    $('#J_smsHtml').empty();
                                    var div = $('<div class="p_intro"><ul class="attributes-list"></ul></div>');
                                    $('#J_smsHtml').append(div);
                                    $(".attributes-list").empty();
                                    $(data.list).each(function (i, item) {
                                        if (item.DefaultValue != "==请选择=="){
                                            if(item.AttrName=='经营类别' || item.AttrName=='剂型' || item.AttrName=='类型'){
                                                $(".attributes-list").append('<li>' + item.AttrName + ':' + item.DefaultValue + '</li>');
                                            }
                                        }
                                            
                                    });
                                    if($(".attributes-list").find('li').length == 0){
                                        $('#J_sms').remove();
                                        $('#J_smsHtml').remove();
                                    }
                                }
                                //var $div = $('<div class="efficacy"><b>产品功效：</b> </div>');
                                //$div.append(data.Efficacy == null ? "" : data.Efficacy);
                                //$(".infos:eq(1)").append($div);
                                //if (data.SalesPolicy.length > 0) {
                                //    $(".infos:eq(1)").append(data.SalesPolicy);
                                //} else {
                                //    $(".top_select li:eq(0)").click();
                                //    $("#solicy").parent().remove();
                                //    $(".infos:eq(1)").remove();
                                //}
                                
                                $("#solicy").parent().remove();
                                $("#J_solicyHtml").remove();
                                $(".top_select li:eq(0)").click();
                            });
                        }
                        function Ajax(url, data, cb) {
                            $.ajax({
                                url: url,
                                data: data,
                                type: 'post',
                                dataType: 'json',
                                success: function (data) {
                                    if (cb) {
                                        cb(data);
                                    }
                                }
                            });
                        }
                    </script>
