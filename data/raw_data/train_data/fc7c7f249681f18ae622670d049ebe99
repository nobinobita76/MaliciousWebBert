function show_local_export(cache_aid,tag){
	$('#permalinkDialog').hide();
	if($('#expert_cont').text()==''){
		$('#expertcontent').show();
		
		exp_html+='<div class="panel-footer">';
		exp_html+='<input type="submit" onclick="do_local_export(\''+cache_aid+'\',\''+tag+'\')" class="export_button export-panel-action" value="Save">';
		exp_html+='<input type="submit" onclick="$(\'#expertcontent\').hide()" class="export_button export-panel_cancel" value="Cancel">';
		exp_html+='</div>';
		exp_html+='</ul>';
		$('#expert_cont').html(exp_html);
	}else{
		$('#expertcontent').toggle();
	}
}


function do_local_export(cache_aid,tag){
	if(tag== undefined || tag== 'undefined'){tag='en';}
	var selval=getSex('exportSaveFormat');
	var urls='control.html?control_type=export_list&types='+selval+'&pt_tag='+tag+'&selTag='+cache_aid;
	window.open(urls,'_self')
}
/******导出功能******/	var articles_sc_titles		= 'urseew';
var articles_remove_titles	= 'urseew bpchange';
/**
* 用户收藏文件的总入口
*/
function user_articles_control(sid,tag)
{
	if($('#user_ou_a').css('display') != 'none')
	{
		var newan=sid;
		var tmp_v = $('#folder_'+newan).attr('class');
		if(tmp_v == articles_remove_titles)
		{
			del_user_articles(sid);
		}
		else if(tmp_v == articles_sc_titles)
		{
			dosave(sid,tag);
		}
	}
	else
	{
		$('#myModal_login').modal('show')
	}
}
//////////////////////////////////////////////////////////
/**
* 发送删除文献请求
*/
function del_user_articles(sid)
{
	$.ajax({
	  type: 'GET',
	  url: 'control.html?control_type=do_folder',
	  data: {'sid':sid,'type':3},
	  cache:false, 
	  timeout:4000,
	  success: del_user_articles_back,
	  dataType: 'json',
	  error:del_user_articles_error
	});
}
/**
* 删除成功返回
*/
function del_user_articles_back(data,textStatus)
{
	var newan=data.an_value;
	if(data.status == 'ok')
	{
		//$('#folder_'+newan).attr({"class":articles_sc_titles});
		attr_class(newan,articles_sc_titles)
	}
	else
	{
		//$('#folder_'+newan).attr({"class":articles_remove_titles});
		attr_class(newan,articles_remove_titles)
	}
	$('#user_sc_articles').html(data.save_articles_nums);
}

/**
* 删除错误返回
*/
function del_user_articles_error(XMLHttpRequest, textStatus, errorThrown)
{

}
/////////////////////////////////////////////////////////////////
/**
* 发送获取用户收藏文章列表请求
*/
function get_user_sc_articles_lists()
{
	$.ajax({
	  type: 'GET',
	  url: 'control.html?control_type=do_folder',
	  data: {'type':2},
	  cache:false, 
	  timeout:4000,
	  success: user_sc_articles_lists_return_back,
	  dataType: 'json',
	  error:save_articles_error
	});
	
}
/**
* 处理用户的收藏列表
*/
function user_sc_articles_lists_return_back(data,textStatus)
{
	if(data.save_articles_lists.length>0)
	{
		var tmp_v = data.save_articles_lists.split(',.');
		for(i=0;i<tmp_v.length;i++)
		{
			var newan=tmp_v[i];
			//$('#folder_'+newan).attr({"class":articles_remove_titles});
			attr_class(newan,articles_remove_titles)
		}
		$('#user_sc_articles').html(tmp_v.length);
	}
}
/////////////////////////////////////////////////////////////////
/**
* 发送文献收藏请求
*/
function dosave(sid,tag)
{
	var newan=sid;
	$.ajax({
	  type: 'GET',
	  url: 'control.html?control_type=do_folder',
	  data: {'sid':sid,'tag':tag,'type':1},
	  cache:false, 
	  timeout:4000,
	  success: save_articles_back,
	  dataType: 'json',
	  error:save_articles_error,
	  beforeSend:function(){},
	  complete:function(){}
	});
}

/**
* 保存文献成功返回
*/
function save_articles_back(data,textStatus)
{
	var newan=data.an_value;
	if(data.status == 'ok')
	{
		//$('#folder_'+newan).attr({"class":articles_remove_titles});
		attr_class(newan,articles_remove_titles)
	}
	else
	{
		attr_class(newan,articles_sc_titles)
		//$('#folder_'+newan).attr({"class":articles_sc_titles});
	}
	$('#user_sc_articles').html(data.save_articles_nums);
}

/**
* 保存文献系统出错
*/
function save_articles_error(XMLHttpRequest, textStatus, errorThrown)
{

}

//显示收藏夹内容
function attr_class(newan,class_name){
	$('#folder_'+newan).attr({"class":class_name});
	if(foldertag=='search_list'){
		//列表页显示样式
		if(class_name == 'urseew'){
			$('#folder_'+newan).attr({"title":'放入收藏夹'});
		}else{
			$('#folder_'+newan).attr({"title":'取消收藏'});
		}
	}else if(foldertag=='articles'){
		//详细页显示样式
		if(class_name == 'urseew'){
			$('#folder_'+newan).html('<img src="public/img/f_a.jpg">放入收藏夹');
		}else{
			$('#folder_'+newan).html('<img src="public/img/f_o.jpg">取消收藏');
		}
	}
}	
//获取馆藏信息的处理
function get_opac_holdings_information(marc_no,zb)
{
	if($('#gc_on'+zb).attr('class')!=undefined){
		$("#gc_"+zb).toggle();
	}else{
		$('#gc_on'+zb).addClass('gc_on');
		$('#gc_'+zb).html('馆藏信息载入中，请稍后');
		$.ajax({ 
			type:"get",
			cache:false,     
			dataType:'json',
			url:"widgets/findplus_opac/book_message.html?control=search_opac_detail&action=user_check&type=holdings&marc_no="+encodeURIComponent(marc_no)+"&zb="+zb, 
			success:get_opac_holdings_information_success,
			error:get_opac_holdings_information_error
		}); 
	}
}

/**
* 成功获取馆藏信息
*/
function get_opac_holdings_information_success(data)
{
	var disList = data.disList;//头部
	var res		= data.res;//信息
	var cell	= data.cell;//宽度
	var txtHtml = '';

	if(res == null)
	{
		txtHtml +='无法获取馆藏信息';
	}
	else
	{
		txtHtml +='<table width="100%" cellspacing="1" cellpadding="2" border="0" bgcolor="#d2d2d2">';
		//头部
		txtHtml +='<tr align="center">';
		for(var i=0;i<disList.length;i++)
		{
			txtHtml +='	<td bgcolor="#eeeeee"  class="greytext1">'+disList[i]+'</td>';
		}
		txtHtml +='</tr>';
		//头部
		
		
		for(var i=0;i<res.length;i++)
		{
			txtHtml +='<tr align="center">'; 
			for(var j=0;j<res[i].length;j++)
			{
				txtHtml +='<td width="'+cell[j]+'%" bgcolor="#FFFFFF"  class="whitetext">'+res[i][j]+' </td>';
			}
			txtHtml +='</tr>';
		}
		txtHtml+='</table>';
	}
	$('#gc_'+data.zb).html(txtHtml);		
}


/**
* 失败获取馆藏信息
*/
function get_opac_holdings_information_error()
{

}

//获取馆藏信息的处理



/***被引次数**/
function get_atr_cated(){
	for(var key in doi_arr){
		$.ajax({
			type	:	'get',
			url	:	'control.cx',
			data	:	{'control_type':'cated_wos_catch',doi:doi_arr[key],resultid:key,rand:Math.random()},
			success: function(msg){
				var data=eval('('+msg+')');
				if(data.wos_citedcount==0){
					//var innerhtml= 'WoS Cited: '+data.wos_citedcount+' &nbsp;&nbsp; Scopus Cited: '+data.citedcount
					var innerhtml= 'WoS Cited: '+data.wos_citedcount;
				}else{
					//var innerhtml= '<a href="'+data.wos_url+'" target="_blank">WoS Cited: '+data.wos_citedcount+'</a> &nbsp;&nbsp; Scopus Cited: '+data.citedcount
					var innerhtml= '<a href="'+data.wos_url+'" target="_blank">WoS Cited: '+data.wos_citedcount;
				}
				$('#cated_'+data.resultid).html(innerhtml)
				$('#cated_'+data.resultid).addClass('custom-link')
			}
		});
	}
}


/***被引次数**/


/***期刊检索**/
var wiki_url='http://primobe.lib.sjtu.edu.cn:8991/cgi-bin/wiki.cgi?q=creative+accounting+or';
function send_url_message(url)
{
	var a=document.createElement("script");
	a.setAttribute("type","text/javascript");
	a.setAttribute("src",url);
	document.body.appendChild(a);
}

function showiki(wikcon)
{
	if(strlen(wikcon)>160){					  
		var wikicon= '<fieldset id="wiki_con"><legend><b>creative accounting or</b></legend>';
		wikicon+='<div style="display:none;" class="wikicon" id="full-wikicon">'+wikcon+' <a href="javascript:;" onclick="hide_full()" id="full-wikicon-plug">[收起]</a></div>'; 
		var wikcon=getInterceptedStr(wikcon,160)+'...';
		wikicon+='<div style="display:block;" class="wikicon" id="wikicon">'+wikcon+' <a href="javascript:;" onclick="show_full()" id="wikicon-plug">[更多]</a></div>';  
		wikicon+='</fieldset>';
	}else{
		var wikicon='<fieldset id="wiki_con"><legend><b>creative accounting or</b></legend><div style="display:block;" class="wikicon" id="wikicon">'+wikcon+'</div></fieldset>';					  
	}             
	$('#divwiki_con').append(wikicon);
}


function get_qk_message(title,ids){
	$.ajax({
		type	:	'get',
		url	:	'?h=get_qk_message',
		data	:	{title:encodeURIComponent(title),id:ids,rand:Math.random()},
		success: function(msg){
			var data=eval('('+msg+')');
			if(data.contents != ''){	  
				var html_s= '<fieldset id="wiki_con"><legend><b>'+data.title+' &nbsp;期刊导览</b></legend><div class="wikicon" id="wikicon">'+data.contents+'</div></fieldset>';
				$('#divwiki_con').html(html_s);
			}else{
				send_url_message(wiki_url);
			}
		}
	});
}

function click_m(obj){
	if($(obj).html()=='更多&gt;&gt;'){
		$(obj).html('收起&gt;&gt;');
		$('.sh_div').toggleClass('hide');
	}else{
		$(obj).html('更多&gt;&gt;');
		$('.sh_div').toggleClass('hide');
	}
}
/***期刊检索**/	$(document).ready(function(){					send_url_message(wiki_url);				//get_atr_cated(); //加载被引次数	})/*** 处理输入年代参数*/function do_yearurl1(){	var fromtext=$('#fromtext').val()	var tromtext=$('#tromtext').val()	fromtext=fromtext.replace(/^\s+|\s+$/g,"");			var pattern = /\d{4}/;	if(pattern.test(fromtext)==false || pattern.test(tromtext)==false)	{		alert('年代输入有误！')	}	else	{		if(fromtext>tromtext)		{			open_url("?h=search_list&query=creative accounting or&action=,,addexpander(fulltext)&subjects=&limiter="+tromtext+"-"+fromtext);		}		else		{			open_url("?h=search_list&query=creative accounting or&action=,,addexpander(fulltext)&subjects=&limiter="+fromtext+"-"+tromtext);		}		}}	</script> 
