<script type="text/javascript">
$(function(){
	$('.inSear_time').each(function(){
		gettime(this.id);				//hotelComm.js
	});
	
	//表单1
	$(".f_hotel_index_search_submit1").live("click", function(){
		if($("#city1").parent("div").length>0){
			if($("#city1").val()=='入住城市' || $("#city1").val()==''){
				$("#city1").parent("div").addClass("input_box2");
				$("#city1").parent("div").removeClass("input_box");
				$("#city1").val("入住城市");
				return false;
			}else{
				if($("#city1").parent("div").attr("class")=="input_box2"){
					$("#city1").parent("div").addClass("input_box");
					$("#city1").parent("div").removeClass("input_box2");
				}
			} 
			if($("#checkindate1").val()=='入住日期' || $("#checkindate1").val()==''){
				$("#checkindate1").parent("div").addClass("input_box2");
				$("#checkindate1").parent("div").removeClass("input_box");
				$("#checkindate1").val("入住日期");
				return false;
			}else{
				if($("#checkindate1").parent("div").attr("class")=="input_box2"){
					$("#checkindate1").parent("div").addClass("input_box");
					$("#checkindate1").parent("div").removeClass("input_box2");
				}
			}
			if($("#checkoutdate1").val()=='离开日期' || $("#checkoutdate1").val()==''){
				$("#checkoutdate1").parent("div").addClass("input_box2")
				$("#checkoutdate1").parent("div").removeClass("input_box");
				$("#checkoutdate1").val("离开日期");
				return false;
			}else{
				if($("#checkoutdate1").parent("div").attr("class")=="input_box2"){
					$("#checkoutdate1").parent("div").addClass("input_box");
					$("#checkoutdate1").parent("div").removeClass("input_box2");
				}
			}
		}	
		if($("#city1").parent("li").length>0){
			if($("#city1").val()=='入住城市' || $("#city1").val()==''){
				$("#city1").addClass("sel3_text2");
				$("#city1").removeClass("sel3_text");
				$("#city1").val("入住城市");
				return false;
			}else{
				if($("#city1").attr("class")=="sel3_text2"){
					$("#city1").addClass("sel3_text");
					$("#city1").removeClass("sel3_text2");
				}
			}
		}
		if($("#checkindate1").parent("li").length>0){
			if($("#checkindate1").val()=='入住日期' || $("#checkindate1").val()==''){
				$("#checkindate1").addClass("sel3_text2");
				$("#checkindate1").removeClass("sel3_text");
				$("#checkindate1").val("入住日期");
				return false;
