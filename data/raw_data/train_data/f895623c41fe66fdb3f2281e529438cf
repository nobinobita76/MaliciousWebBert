	//时间递减
		function gettime(id){
			var dome  = document.getElementById(id);
			var time  = dome.title;
			var Senc  = (time%86400);
			var day   = parseInt(time/86400);
			var hours = parseInt(Senc/3600);
			var mines = parseInt(Senc%3600/60);
			var second= parseInt(Senc%3600)%60;
			
			second = second - 1;
			if(0 > second){
				second = 59;
				mines = mines - 1;
			}
			if(0 > mines){
				mines = 59;
				hours = hours - 1;
			}
			if(0 > hours){
				hours = 23;
				day = day - 1;
			}
			
			var timeVal = '';
			if(0 <= day){
				timeVal = day+'天';
			}
			if(0 <= hours){
				timeVal = timeVal+hours+'小时';
			}
			if(0 <= mines){
				timeVal = timeVal+mines+'分';
			}
			if(0 <= second){
				timeVal = timeVal+second+'秒';
			}
			dome.title = time-1;
			dome.innerHTML = timeVal;
			setTimeout(function(){gettime (id); }, 1000);
		}

</script>
