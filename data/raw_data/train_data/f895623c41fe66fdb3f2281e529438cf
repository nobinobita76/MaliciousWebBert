<script type="text/javascript">

function over($id){
	document.getElementById($id).className = 'top_menu_up'; 
}
function out($id){
	document.getElementById($id).className = 'top_menu_down'; 
}
 
</script>

<script type="text/javascript">
$(document).ready(function(){
	$("a").filter(".checiBuy").css('cursor','pointer');
	$(".checiBuy").css('text-align','center');
	$(".checiBuy").live("click", function(){
		var to = typeof $(this).attr("to") != 'undefined'? $(this).attr("to"):'';
		var action = $("#daigou_post").attr("action");
		var parrent = /-[a-zA-Z0-9]*\.html/;
		var replace = '-'+to+'.html';
		var action = action.replace(parrent,replace);
		$("#daigou_post").attr("action",action);
		$("#daigou_post").submit();
	});
	
	
});


</script>

<script>
$(document).ready(function(){
	$("#checivalue").live("keyup",function(){
		$reg = /[^a-zA-Z0-9]/g;
		$(this).val($(this).val().replace($reg,''));
	})
})
</script>

<script language="javascript">
$(function(){
	$(".btn2").click(function(){
		var sel3_text='';
		sel3_text = $(".sel3_text[name=checi]").val();
		if(sel3_text == '' || sel3_text == '请输入车次'){
			$(".sel3_text[name=checi]").attr('value','请输入车次');
			$(".sel3_text[name=checi]").parent().addClass("input_boxCC_red");
			return false;
			}

	});
})

</script>

<script type="text/javascript">

	$(function(){
	
		//释放下拉框
		$(".xiala p").live('click',function(){
			var a_val = $(this).children().attr("a_id");
			//alert(a_val);
			$(".input_out").val(a_val);
			$(".xiala").hide();
		});
	
		$("#sel_checi").focus().live('keyup',function(){
			$('#checiBtn').removeAttr('disabled');		
		    //获取keyup传输进来的值
		    var nkey=$.trim($(this).val());
		   
		    //若值为空则不显示;
		   if(nkey==''||nkey==null){
		        $(".xiala").hide();
		    }else{
				$.ajax({
		             url:'http://www.tieyou.com/ajax/checiAbout.html',
		             dataType:"jsonp",
		             data:{'checi':nkey},
		             jsonp:"jsonpcallback",
		             success:function(data){
		            	 //判断是否含有车次
		            	 if(data){
			            	 var strLi='';
			            	  $.each(data, function(key, value) {
			            		  	strLi += '<p><a href="http://checi.tieyou.com/'+value+'.html" target="_blank">'+value+'</a></p>';
			            		  });
			            	  $(".xiala").html(strLi);
			                  $(".xiala").show();
		            	 }else{
		            		 $(".xiala").html('<p class="tip_none">暂无您查询的车次</p>');
		            		 $(".xiala").show();
		            		 $('#checiBtn').attr('disabled','disabled');
		            	 }
		             }
	        	});
		    }
		});
		
	});

	function jumpTocheci(){
		if ($('#sel_checi').val() == '' || $('#sel_checi').val() == '请输入车次') {
			$('#sel_checi').focus();
			return false;
		}
		location.href = 'http://checi.tieyou.com/' + $('#sel_checi').val() + '.html';
	}
</script>

