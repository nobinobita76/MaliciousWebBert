         <script type="text/javascript">
function delbody_view(obj)
{
	$('anc_msgbody').innerHTML="<a href=\"javascript:;\" onclick=\"layer_view($('" + obj.id + "'),'','','anc_delbody','Processor.asp?AjaxFlag=1&amp;action=Del&amp;b=100&amp;ID='+p_getselected()+'','',1,'',1);\">批量删除，已选择 <b id=layer_selectnum>" + p_getnum() + "</b> 条记录</a><br><input class=\"fmchkbox\" type=\"checkbox\" name=\"selmsg\" id=\"selmsg\" value=\"1\" onclick=\"achoose();\" />选择全部";
	layer_view(obj,'','','anc_msgbody','','');
}

function layer_outmsg(obj,str,fresh)
{
	var menuobj_get,tmp;
	tmp = $(obj).childNodes;
	menuobj_get=obj;
	if(tmp.length==2)
	if(tmp[0].className=="ajaxitemtitle")
	menuobj_get=tmp[1];
	
	menuobj_get.innerHTML=str;
	if(fresh==1)
	this.location="a.asp?b=100&id=36440";
	else
	{
		setTimeout("$('" + obj + "').style.display='none';",3000);
	}
}

var layer_num=0,layer_olditem;
/*
obj 受点击对象
w 显示层宽
h 层高
menu 显示层对象ID
file 执行导入文件地址
js 执行导入的js文件
fresh 是否强制刷新
filepara 导入文件相应的post数据
center 是否居中 1.居中 0.显示在点击对象之下
*/
function layer_view(obj,w,h,menu,file,js,fresh,filepara,center)
{
	if(menu=="anc_msgbody")
	$("anc_delbody").style.display="none";
	else
	$("anc_msgbody").style.display="none";

	var menuobj=$(menu);
	
	if(w!="")menuobj.style.width = w;
	if(h!="")menuobj.style.height = h;
	
	var menuobj_get,tmp,menu_get;
	tmp = menuobj.childNodes;
	
	menuobj_get=menuobj;
	menu_get = menu;
	if(tmp.length==2)
	{
		if(tmp[0].className=="ajaxitemtitle")
		{
			menuobj_get=tmp[1];
			menu_get=menuobj_get.id;
			if(menuobj_get.id=="")
			{
				menuobj_get.id="ajaxitem_body"+layer_num;
				menu_get = "ajaxitem_body"+layer_num;
				layer_num++;
			}
		}
	}

	if(obj.id==""){obj.id="ajaxitem_view"+layer_num;layer_num++;}
	if(fresh==1 || (file && menuobj_get.innerHTML=="loading..."))
	{
		if(fresh==1)menuobj_get.innerHTML="loading..."
		if(!isUndef(filepara))
		getAJAX(file,filepara,menu_get);
		else
		getAJAX(file,"",menu_get);
		if(js)editor_inputjs(menu_get,js);
	}
	if(fresh==1 || file=="" && js)
	{$import(js,"js");}

	if(fresh!=1)
	{
		if(menuobj.style.display != 'none')
		{
			//menuobj.style.display = 'none';
			if(layer_olditem==obj.id)return;
		}
	}
	if(center==1)
	{
		setCenterDiv(menuobj);
	}
	else
	{
		var x = getX(obj);
		var y = getY(obj)-1;
		var distHeight = obj.clientHeight;
		if(!distHeight)distHeight+=obj.offsetHeight;
		y += distHeight;
		menuobj.style.top = y + 'px';
		menuobj.style.left = x + 'px';
	}
	menuobj.style.display = 'block';
	relocationxy(obj.id,menuobj.id,0,center);
	layer_olditem = obj.id;
}
</script> 
