         <script type="text/javascript">
var GBL_domain="cftn.cn";
var DEF_DownKey="sLvGA3pLXuii",HU="../";

function appandOpinion(obj,id,num)
{
	if($('opinion' + id))return;
	var tmp = document.createElement("div");
	tmp.setAttribute('id','opinion' + id);
	getAJAX('a.asp','ol=5&B=100&ID=' + id + '&num=' + num,'opinion' + id);
	insertAfter(tmp,obj.parentNode.parentNode);
}
function insertAfter(newElement,targetElement)
{
	var parent = targetElement.parentNode;    
	if(parent.lastChild == targetElement)
	{
		parent.appendChild(newElement);    
	}
	else
	{
		parent.insertBefore(newElement,targetElement.nextSibling);    
	}
}
</script> 
