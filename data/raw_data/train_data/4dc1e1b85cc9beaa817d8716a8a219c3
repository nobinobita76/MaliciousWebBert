<script type="text/javascript">
//ajax评论和点赞
$(document).ready(function(){
	// 读取日立信息
	var calid  = "0";
	if( calid>0 ){
	    var postUrl ="/htnews/2014/article_rl_new.php";
		var typeid ="1888";
		var flag   ="p,k";
		$.ajax({
            type: "POST",
            url: postUrl,
            dataType:'html',
            data: "calid="+calid+"&typeid="+typeid+"&flag="+flag,
            success: function(dataInfo){
           		if(dataInfo!=''){
           			$("#contentsOfArticles_words").prepend(dataInfo);
           		}
           }
   		});
	}
    //COOKIE 中读取收藏
	 var userid= getCookie('UserID');
     var aid ="183801" ;
     var c_name = "fav_"+aid+"_"+userid;
     var isdo = getCookie(c_name);//已经收藏
     if(isdo) {
		//$(".btn_collect02").html("收藏成功");
		//$(".btn_collect02").addClass("btn_collect02_cur");
        $(".btn_collect02").remove();
        $(".btn_pinglun02").after('<a target="_blank" href="/newhome/datum.php?action=lecture&type=z" class="fl btn_collect02 btn_collect02_cur">收藏成功</a>');
	 }
     //cookie 中读取默认的评价
     var commonName = "common_"+aid;
     var isCommon = getCookie(commonName);
     if(isCommon){

             if(isCommon=='hao'){
                $(".hao").addClass('hao_cur');
            }else{
                $(".nao").addClass('nao_cur');
            }
     }
     //读取评价的数据
    var url ="/htnews/2014/addCommon.php";
    $.ajax({
            type: "POST",
            url: url,
            dataType:'json',
            data: "aid="+aid+"&action=getCommon",
            success: function(msg){
	            if(parseInt(msg.status)>0){
	                $(".hao").text(msg.goods);
	                $(".nao").text(msg.poors);
	            }
           }
    });
	

	//点赞
	$(".comment").click(function(){
		var reg = $(this).attr('reg');
		var num = $(this).html();
		var aid ="183801" ;
		var c_name = "common_"+aid;
  		var isCommon = getCookie(c_name);
		if(isCommon){
			alert('您已经评价过了');
			return;  
		}
    	  var url ="/htnews/2014/addCommon.php";
       	  $.ajax({
    	   type: "POST",
    	   url: url,

    	   data: "aid="+aid+"&reg="+reg,
    	   success: function(msg){
    		 if(msg>0 ) {
    			setCookieFun(c_name,reg,1);
    			var noNUm = parseInt(num) + 1;
                if(reg=='hao'){
                    $(".hao").addClass('hao_cur');
                }else{
                    $(".nao").addClass('nao_cur');
                }
    			$("."+reg).html(noNUm);
    			return;
    		 }else{
    			 return;
    		 }
    	   }
    	});
	})
	
	//换一换功能
	var aid ="183801" ;
	var c_name = "change"+aid;
	setCookieFun(c_name,0,1);
	$(".btn_change").click(function(){
		var aid ="183801" ;
		var c_name = "change"+aid;
		var changeNum = getCookie(c_name);
		var hidenum = $("#hidenum").val();

		if(changeNum=='' || changeNum=='undefined' || changeNum==0){
			changeNum=2;
			setCookieFun(c_name,1,1);
		}
		if(changeNum >= hidenum){
			for(var i=1;i<=hidenum;i++){
				$("#show_"+i).hide();
			}
			$("#show_"+changeNum).show();
			setCookieFun(c_name,1,1);
		}else{
			for(var i=1;i<=hidenum;i++){
				$("#show_"+i).hide();
			}
			if(changeNum==0) changeNum =2;
			$("#show_"+changeNum).show();
			changeNum ++;
			setCookieFun(c_name,changeNum,1);
		}
		
	});
	
  //登录后才可以收藏
  $(".btn_collect02").click(function(){
  var userid= getCookie('UserID');
  var aid ="183801" ;
  var posturl ="/news/20161024/183801.html";
  var title ="2016年四川定向乡镇公务员考试《公共基础知识》试题及解析(5)";
  if(userid==''||userid=='undefiend'){
	alert("该文章登录后才可以收藏");
	location.href="/reg/login.php";
	return;
  }
   var c_name = "fav_"+aid+"_"+userid;
   var isdo = getCookie(c_name);
   if(isdo) {
    $(".btn_collect02").remove();
    $(".btn_pinglun02").after('<a target="_blank" href="/newhome/datum.php?action=lecture&type=z" class="fl btn_collect02 btn_collect02_cur">收藏成功</a>');
	return;  
   }
   var url ="/htnews/2014/myfavoritearticle_add.php";
   $.ajax({
	   type: "POST",
	   url: url,
	   data: "aid="+aid+"&userid="+userid+"&url="+posturl+"&title="+title+"&type=z",
	   success: function(msg){
		 if(msg>0 ) {
			//$(".btn_collect02").html("收藏成功");
			//$(".btn_collect02").addClass("btn_collect02_cur");
            $(".btn_collect02").remove();
            $(".btn_pinglun02").after('<a target="_blank" href="/newhome/datum.php?action=lecture&type=z" class="fl btn_collect02 btn_collect02_cur">收藏成功</a>');
			setCookieFun(c_name,1,30);
			return;
		 }else{
			 //alert("收藏失败");
		 }
	   }
	});
  })
});
 	//cookie设置
 	function setCookieFun(c_name,value,expiredays){
	    var exdate=new Date()
	    exdate.setDate(exdate.getDate()+expiredays)
	    cookieVal=c_name+ "=" +escape(value)+((expiredays==null) ? "" : ";expires="+exdate.toGMTString())+";path=/";
	//    alert(cookieVal);
	    document.cookie=cookieVal;
 	}
 	//获取cookie
 	function getCookie(c_name){
	    if (document.cookie.length>0)
	    {
	      c_start=document.cookie.indexOf(c_name + "=")
	      if (c_start!=-1){ 
	        c_start=c_start + c_name.length+1 
	        c_end=document.cookie.indexOf(";",c_start)
	        if (c_end==-1) c_end=document.cookie.length
	//        document.write(document.cookie.substring(c_start,c_end)+"<br>");
	        return unescape(document.cookie.substring(c_start,c_end))
	        } 
	     }
	    return ""
    }
</script> 
