$(document).ready(function(){	
$("#keyword").keydown(function(event){ 
   var key_code = event.which;
	if (key_code==13){
		$("#btnSearch").click();
	}
}); 
$("#btnSearch").click(function(){
	var selectCourse= $("#selectMenu").html();
	//var keyword = $("#keyword").val();
	if(selectCourse=='资讯'){
		$('#form').attr("action","/news/list/index.php");
	}else if(selectCourse=='资料'){
		$('#form').attr("action","/beikao/list/index.php");
	}else if(selectCourse=='视频'){
		$('#form').attr("action","/video/list.php");
	}else if(selectCourse=='专题'){
		$('#form').attr("action","/zhuanti/index.php");
	}else{
		$('#form').attr("action","/class/list.php");//课程
	}
	$('#form').submit();
});



})
</script>
