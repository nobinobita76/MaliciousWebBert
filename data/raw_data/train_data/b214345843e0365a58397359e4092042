			window.external.addFavorite('http://www.022go.cn','一起快乐购- 最靠谱的一元众筹夺宝平台！主打一元抢购 2015年最火爆一元购物网站');
		}
		else if(window.sidebar){
		   window.sidebar.addPanel('一起快乐购- 最靠谱的一元众筹夺宝平台！主打一元抢购 2015年最火爆一元购物网站','http://www.022go.cn', "");
