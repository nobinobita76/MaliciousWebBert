function duobaojilu( pg ){

         /* 云购记录 */

           $.post( HTTP + "json.php?romd="+(Date.parse(new Date())/1000)+Math.round(Math.random()*100000000),{ 'y' :'goodsjilu','d':'soso', 'cpid' :CPID , 'cpqs' : CPQS,'pg':pg  },function(result){

          DATA =  jieshou(result);

          if( DATA ){

              if( DATA .code == 1){

                  D = DATA.data;

                  html = '<div class="content">';
                  if( pg == 1) html +='<div class="m-detail-recordList-start"><i class="ico ico-clock"></i></div>';

                  html +='<div id="pro-view-60">';
                  html +='<div class="m-detail-recordList-timeSeperate">'+D.shuju['0']['rizhi']+'<i class="ico ico-recordDot ico-recordDot-solid"></i></div>';



                     
                  
            
          
                    if(D.shuju){

                     html +='<ul class="m-detail-recordList m-detail-recordList-'+D.shuju['0']['rizhi']+'" pro="list">';

                     linshi = Array(D.shuju['0']['rizhi']);

                        for(var i in D.shuju){

                            if( ! chazhao( linshi, D.shuju[i]['rizhi'] ) ){

                                linshi.push(D.shuju[i]['rizhi']);

                                html +='</ul></div><div id="pro-view-60"><div class="m-detail-recordList-timeSeperate">'+D.shuju[i]['rizhi']+'<i class="ico ico-recordDot ico-recordDot-solid"></i></div><ul class="m-detail-recordList m-detail-recordList-'+D.shuju[i]['rizhi']+'" pro="list">';
                            
                            
                            }
                        
                          html +='<li class="f-clear" data-rid="470" id="'+D.shuju[i]['did']+'">   <span class="time">'+D.shuju[i]['atime']+'</span>   <i class="ico ico-recordDot ico-recordDot-hollow"></i>   <div class="m-detail-recordList-userInfo">       <div class="inner">           <p>               <span class="avatar"><img width="20" height="20" onerror="this.src=\'/wangyi/p/yy/lib/img/avatar/40.jpeg\';this.onerror=null" src="'+D.shuju[i]['utouxiang']+'"></span>               <a href="'+HTTP+'user.php?y=userjilu&uid='+D.shuju[i]['uid']+'" target="_blank" title="'+D.shuju[i]['uname']+'(ID:'+D.shuju[i]['uid']+')">'+D.shuju[i]['uname']+'</a>               '+D.shuju[i]['ip']+' 参与了<b class="times txt-red">'+D.shuju[i]['shuliang']+'人次</b>                                             <a style="display: none;" class="w-button w-button-simple btn-checkCodes" data-func="code" date="'+D.shuju[i]['did']+'" href="javascript:void(0)">所有云购号码 <i class="ico ico-arrow-gray ico-arrow-gray-down"></i></a>           </p>           <a style="display:none" class="btn-close" href="javascript:void(0)">x</a><p class="codes"></p>       </div>   </div></li>';

                        }
                       
                      html += '</ul>';
                    
                    
                    }

                   

                     if(i < 49)html +='<div class="m-detail-recordList-end"><i class="ico ico-clock"></i></div>';

                   html += ' </div></div>';

                   if(D.page)
                  html+='<div class="pager"><div class="w-pager page"> '+D.page+'</div></div>';
                  $("#recordPanel").html( html );


                  $(".m-detail-recordList-userInfo").hover(function(){

                      if( $(this).find('.btn-close').css('display') =='none') $(this).find('.w-button ').show();

                    
                  
                  
                  },function(){
                      $(this).find('.w-button ').hide();
                  
                  
                  });


                  $(".m-detail-recordList-userInfo .w-button ").click(function(){

                     var tdeid =  $(this).attr('date');

                      $(this).hide();
                       var tmulu = $("#"+tdeid);

                       if($(tmulu).find('.codes').html() != '') { 

                           $(tmulu).find('.codes').show();

                       }else  {

                            $.post( HTTP + "json.php?romd="+(Date.parse(new Date())/1000)+Math.round(Math.random()*100000000),{ 'y' :'xymtongji','d':'soso', 'cpid' :tdeid  },function(result){

                                  DATA =  jieshou(result); 
                                  html = '';
                                  
                                  if(DATA.code == 1){

                                       D = DATA.data;
                                         for( var m in D.xymde){

                                       html += '<b>'+D.xymde[m]+'</b> ';

                                     }

                                            }
                                              $(tmulu).find('.codes').html(html);
                                  });
                           
                         

                        }

                      

                     

                          $(tmulu).find('.m-detail-recordList-userInfo').addClass('m-detail-recordList-userInfo-detail');
                          $(tmulu).find('.btn-close').show();

                          $(tmulu).find('.btn-close').click(function(){

                          $(tmulu).find('.m-detail-recordList-userInfo').removeClass('m-detail-recordList-userInfo-detail');
                          $(tmulu).find('.codes').hide();



                      $(this).hide();

                        });

                  });

              


              
              
              
              }else $("#recordPanel").html('<div class="empty"><p class="status-empty"><i class="littleU littleU-cry"></i>&nbsp;&nbsp;暂时还没有参与记录</p></div>');
                
                
          
          
          }else $("#recordPanel").html('<div class="empty"><p class="status-empty"><i class="littleU littleU-cry"></i>&nbsp;&nbsp;暂时还没有参与记录</p></div>');
          
          
          });




}

function huidiaosw( DATA ){


 if(DATA.code == 1){

        D = DATA.data;
        html ='';

        for( var m in D.xymde){

                  
           html += '<span class="iCodeItem">'+D.xymde[m]+'</span> ';

       }

          $("#cfangcode").html( html );

         

          $("[pro=myCodes]").html( '<a href="javascript:huancode();" id="goumaide" data="'+D.zongshu+'">你购买了( '+D.zongshu+' )人次, 点击查看幸运码</a>');
  
  }else   $("[pro=myCodes]").html( LANG.nodb );
 
}


function huancode(){


              html =  '<div class="w-duobaoCodeList" pro="codeWarper"> <div style="padding:30px;"> <div pro="list" class="w-duobaoCodeList-list"><dl class="iItem f-clear" id="pro-view-43"><dt>';

              html +=$("#cfangcode").html();
                  
              
             html +='</dt></dl>  </div></div></div>';

             zongshu = $("#goumaide").attr('data');

          

              layer.open({  title :'参与了 '+zongshu+' 人次',
                        type : 1,
                    closeBtn : 1,
                        area : '600px',
                      offset :'auto',
                     content : html
         }); 





}

function tiaozhengtishi( num ){

       if(num < 2) $("[pro=txtOneBuyTip]").html(' 多参与1人次，获奖机会翻倍！');
       else if(num < 5) $("[pro=txtOneBuyTip]").html(' 参与人次越多，获奖机会越大哦！');
       else if(num < 10) $("[pro=txtOneBuyTip]").html(' 参与10人次，极有机会获奖！！');
       else if(num >= 10) $("[pro=txtOneBuyTip]").html(' 加大参与人次，云购在望！');

}



function chakan( id  ){

     layer.load();

     var canyu = $("#did" + id +" .col-opt .txt-impt").html()*1;
     if(canyu > 10000){
     
      
     layer.closeAll('loading');
      layer.msg( '参与人数大于 1W 无法查看详情',{ offset :'auto' ,time : 3000});


     return false;
     }


       $.post( HTTP + "json.php?romd="+(Date.parse(new Date())/1000)+Math.round(Math.random()*100000000),{ 'y' :'xymtongji','d':'soso', 'cpid' :id  },function(result){

          DATA =  jieshou(result);

          
            layer.closeAll('loading');

          if(DATA.code == 1){
              D = DATA.data;

              html = '';
           

             var zhonghao =  $("#btnWinnerCodes").attr('data') * 1;

              html =  '<div class="w-duobaoCodeList" pro="codeWarper"> <div style="padding:30px;"> <div class="w-duobaoCodeList-hd" pro="codeTitle">这单参与了'+D.zongshu+'人次</div><div pro="list" class="w-duobaoCodeList-list"><dl class="iItem f-clear" id="pro-view-43"><dt>';

              for( var m in D.xymde){

                  if( zhonghao == D.xymde[m])

                      html += '<span class="iCodeItem" style="color:Red;">'+D.xymde[m]+'</span> ';

                 else html += '<span class="iCodeItem">'+D.xymde[m]+'</span> ';

             }
                  
              
             html +='</dt></dl>  </div></div></div>';

              html;

              layer.open({  title :'云购号码',
                        type : 1,
                    closeBtn : 1,
                        area : '600px',
                      offset :'auto',
                     content : html
         }); 

          
          
          }
          
          
      });


}



function huanhuis( tdid , $fanhui ){
          


         if( $fanhui.canyu > 0){

      


             html = '<div class="m-detail-tabHistory-period">期号 '+$fanhui['cpqs']+'</div>   <div class="m-detail-tabHistory-info">       <img onerror="this.src=\'/wangyi/p/one/web/lib/img/avatar/90.jpeg\';this.onerror=null;" src="'+$fanhui['utouxiang']+'">       <div class="m-detail-tabHistory-info-txt">           <span>恭喜 <a href="'+HTTP+'user.php?y=userjilu&uid='+$fanhui['uid']+'" title="'+$fanhui['uname']+'" target="_blank"><span class="m-detail-tabHistory-info-nickname">'+$fanhui['uname']+'</span></a>（<span class="m-detail-tabHistory-info-address" title="'+$fanhui['ip']+'">'+$fanhui['ip']+'</span>）获得了本期商品</span>           <span>用户ID：'+$fanhui['uid']+'（ID为用户唯一不变标识）</span>           <span>本期参与：<strong>'+$fanhui['canyu']+'人次</strong></span>       </div>   </div>   <div class="m-detail-tabHistory-result">       <span>幸运号码：<strong>'+$fanhui['zhonghao']+'</strong></span>       <span>揭晓时间：'+$fanhui['ktime']+'</span>       <span>云购时间：'+$fanhui['stime']+'</span>   </div>   <div class="m-detail-tabHistory-operation"><a href="'+$fanhui['url']+'" target="_blank">查看详情</a></div>';
              

                  $("#dj"+tdid).html (html) ;

                  $("#dj" + tdid).addClass('m-detail-tabHistory-published');

           }

}


function huidiaos2( result ){

       
                  DATA =  jieshou( result );

                  if ( DATA ){
                        
                      if(DATA.code == 1){

                           fanhui =  DATA.data;

                            if( fanhui .canyu > 0){

                               clearInterval(DAOJISHI);

                               window.location.href= location.href; 


                          }

                          
                      }


                  
                  }



}

function huidiaos( result ){

               

                  DATA =  jieshou( result );

                  if ( DATA ){
                        
                      if(DATA.code == 1){

                         fanhui =  DATA.data;
                  
                         huanhuis( fanhui.cpid+'_'+fanhui.cpqs , fanhui );
                         

                      }


                  
                  }

 }



function CountDown(){
     

        if($(".djtime").length < 1){
        
           clearInterval(DAOJISHI);
           return false;

        }

        $(".djtime").each(function(){


 
         var time = $(this).attr('djtime');
         var biaoshi = $(this).attr('data');
        

        if( time < 0){
              
               $(this).attr({'djtime':time -1 })
             
            
              if( biaoshi == '' ||  typeof biaoshi === 'undefined'){

                  
             
                         if( time % 100 == 0){

                         $fanhui = duqudange( CPID,CPQS,huidiaos2);

                   }     
                  

              }else{

                /*动态读取了*/
                

                  if( time % 100 == 0){

                       var CP = biaoshi.split('_'); 

                    
                    
                    $fanhui = duqudange( CP['0'] , CP['1'],huidiaos);


                 }
              
              
              }

             
            return false;

        }

         if(time < 1){

              $(this).attr({'djtime':-1 });

              $(this).html('正在计算中…');

              return false;

         }

         var strtime = shijian( time ,100);


          if(time > 360000){

                                hh = String(strtime['0']).split('');
                               ii = String(strtime['1']).split('');
                               ss = String(strtime['2']).split('');

        
                             }else{

                               hh = String(strtime['1']).split('');
                               ii = String(strtime['2']).split('');
                               ss = String(strtime['3']).split('');

                             }



         if( biaoshi == ''){
         
         thsss = ''+hh['0']+''+hh['1']+':'+ii['0']+''+ii['1']+':'+ss['0']+''+ss['1']+'';
        }else thsss = '<b>'+hh['0']+'</b><b>'+hh['1']+'</b>:<b>'+ii['0']+'</b><b>'+ii['1']+'</b>:<b>'+ss['0']+'</b><b>'+ss['1']+'</b>';

         $(this).html(  thsss);

         $(this).attr({'djtime':time -1 });

      
      

      
      
      });


}



$(function(){

    $("[pro=oneExplanationTip]").hover(function(){
    
      $(this).addClass( 'm-detail-main-one-explanation-hover' );
    },function(){

        
      $(this).removeClass( 'm-detail-main-one-explanation-hover' );
    
    
    });
   
    $("[pro=thumbnailList] li").hover(function(){
    
        $("[pro=thumbnailList] li").removeClass('w-gallery-thumbnail-item-selected');

        $(this).addClass('w-gallery-thumbnail-item-selected');

        var twzhi = $(this).attr('data');
        wzhi = Array('31','118','205','292','379','379');

        $(".w-gallery-picture img").attr( { src : $(this).find('img').attr('src')} );

        $(".ico-arrow-red-up").animate( {'left': wzhi[twzhi]+'px' },50);

    });

    tiaozhengtishi( $(".w-number-input").val() * 1 );

   $("[pro=cpjianshao]").click(function(){

    
      
      var woqu = $(this).attr('data');
      
      var shuliang = $(woqu + " .w-number-input").val() * 1 ;
       
      var zongliang =  $(woqu + " .w-number-input").attr('xiangou') * 1; 
      var newnum = shuliang-1;
       if( newnum > zongliang ) newnum = zongliang;
       else if(newnum  < 1)newnum = 1;

       tiaozhengtishi( newnum );

       $(woqu + " .w-number-input").val(newnum);
    
   
   });

   $("[pro=cpzengjia]").click(function(){

        
      var woqu = $(this).attr('data');
      var shuliang  =  $(woqu + " .w-number-input").val() * 1;
      var zongliang =  $(woqu + " .w-number-input").attr('xiangou') * 1; 

       var newnum = shuliang + 1;
       if( newnum > zongliang ) newnum = zongliang;

       tiaozhengtishi( newnum );

       $(woqu + " .w-number-input").val(newnum);

   });

   $(".w-number-input").hover(function(){
   
      $(this).select();
   
   })

    $(".w-number-input").change(function(){ 

        var shuliang =  $(this).val() * 1;
        var zongliang =  $( this ).attr('xiangou') * 1; 
        if( isNaN( shuliang ) ) shuliang = NUM;
      
        if(shuliang > zongliang) shuliang = zongliang;

        
        tiaozhengtishi( shuliang );

        $(this).val( shuliang );
    
    });


    $("[pro=formulaSum],[pro=formulaLottery]").hover(function(){

        $(this).find('.more').show();
    
    },function(){

        $(this).find('.more').hide();
    
    
    
    });


    $("#introTab").click(function (){

        /*商品详情*/
        $(".w-tabs-tab-item").removeClass('w-tabs-tab-item-selected');
        $(this).addClass('w-tabs-tab-item-selected');
        $(".w-tabs-panel-item").hide();
        $("#introPanel").show();
    
    
    });


     $("#resultTab").click(function (){

       /*计算结果*/
        $(".w-tabs-tab-item").removeClass('w-tabs-tab-item-selected');
        $(this).addClass('w-tabs-tab-item-selected');
        $(".w-tabs-panel-item").hide();
        $("#resultPanel").show();



    
    
    });

     $("#recordTab").click(function (){

       /*云购参与记录*/
        $(".w-tabs-tab-item").removeClass('w-tabs-tab-item-selected');
        $(this).addClass('w-tabs-tab-item-selected');
        $(".w-tabs-panel-item").hide();
        $("#recordPanel").show();

        duobaojilu(1);




    
    
    });

    $("#shareTab").click(function (){

       /*晒单*/
        $(".w-tabs-tab-item").removeClass('w-tabs-tab-item-selected');
        $(this).addClass('w-tabs-tab-item-selected');
        $(".w-tabs-panel-item").hide();
        $("#sharePanel").show();

        saidanjilu(1);

    });

    $("#historyTab").click(function (){

       /*往期云购*/
        $(".w-tabs-tab-item").removeClass('w-tabs-tab-item-selected');
        $(this).addClass('w-tabs-tab-item-selected');
        $(".w-tabs-panel-item").hide();
        $("#historyPanel").show();

        wangqijilu(1);
   
    
    
    });


    if( $(".djtime").length ){
       

         DAOJISHI = setInterval("CountDown()",10);
         
     }




});
</script>
 </body>
</html>