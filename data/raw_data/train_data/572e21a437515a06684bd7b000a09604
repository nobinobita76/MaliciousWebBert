  <script>
		var a = $("#divSortList");
		var b = false;
		a.children("dl").each(function() {
			$(this).hover(function(c) {
				b = true;
				bI(c);
				$(this).addClass("hover");
				if ($(this).next().length > 0) {
					$(this).append("<i></i>")
				}
			}, function(c) {
				b = false;
				$(this).removeClass("hover").children("i").remove()
			})
		})
		
		var bI = function(a) {
			if (a && a.stopPropagation) {
				a.stopPropagation()
			} else {
				window.event.cancelBubble = true
			}
		};

</script> 
  <script type="text/javascript" charset="utf-8">



$( function (){

    if(indexs == 1) $(".m-catlog-wrap").css({height:'512px'});



   
 
    if(ajaxgouwu == 1) gouwulist();

    $(".w-miniCart-btn").click( function(){
    
         gojiesuan();
    });

    $(".m-toolbar-myDuobao").hover( function(){
     
       $(this).addClass('m-toolbar-myDuobao-hover');
     
    },function(){

         $(this).removeClass('m-toolbar-myDuobao-hover');
    
    });


    $(".w-miniCart").hover( function(){
                           
                        if ($("#cartlist").css('display') != 'block') $("#cartlist").show();

    } , function(){     if ($("#cartlist").css('display') != 'none')
                                    $("#cartlist").hide();

    });


    $("img.sssss").lazyload();

    $(".m-catlog").hover(function(){

            $(".m-catlog .m-catlog-wrap").animate({height: 512}, 2);

             $(".m-catlog .m-catlog-wrap"). css({ visibility:'visible'});

            $(".m-catlog-fold .ico-arrow, .ico-arrow-white-down").css({"background-position":'-0px -25px'});

    },function(){

            if ( indexs != 1  || $(".ico-arrow-white-down").css('display') != 'none')  $(".m-catlog .m-catlog-wrap").css({'height': 0});
            

            
         
            $(".m-catlog-fold .ico-arrow, .ico-arrow-white-down").css({"background-position":'0px -25px'});

    });


 $(window).scroll(function () {
     
            
        var top = parseInt($(this).scrollTop());

        if(indexs == 1)
           var biho = 530;
        else var biho = 114;

        if( top > biho ){

                     $(".w-back-top").show();
                 if(ajaxgouwu == 1){
                   $(".g-header").css({"padding":'0 0 51px 0'});
                   if( $(".m-catlog .m-catlog-wrap").css('height')  == '512px')
                       $(".m-catlog .m-catlog-wrap").css({'height': 0});


                     
                       $(".g-header").addClass('g-header-fixed');

                      

                       $(".m-catlog").addClass('m-catlog-fold');
                       $(".m-catlog-fold .ico-arrow, .ico-arrow-white-down").css({"background-position":'0px -25px'});
                   

                       $(".ico-arrow").show();

                       }
          
               
           


        }else{

            $(".g-header").css({"padding":'0'});

            if(  $(".m-catlog .m-catlog-wrap").css('height')  == '0px' && indexs == 1)
             $(".m-catlog .m-catlog-wrap").animate({height: 512}, 2);
            else if( indexs == 2 ) $(".m-catlog .m-catlog-wrap").css({'height': 0});

            if( indexs == 1 ) $(".ico-arrow-white-down").hide();

            $(".w-back-top").hide();
            $(".g-header").removeClass('g-header-fixed');
            $(".m-catlog").addClass('m-catlog-normal');



        }

       
    });

     

} );
</script> 
