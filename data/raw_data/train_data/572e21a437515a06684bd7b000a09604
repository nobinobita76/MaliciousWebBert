function wangqijilu( pg ){




           $.post( HTTP + "json.php?romd="+(Date.parse(new Date())/1000)+Math.round(Math.random()*100000000),{ 'y' :'goodsjilu','d':'soso', 'cpid' :CPID , 'lx' : 1,'pg':pg  },function(result){

          DATA =  jieshou(result);

          if( DATA ){

               if( DATA .code == 1){

                   html = '<div class="content"><ul class="m-detail-tabHistory-list f-clear"><div id="pro-view-15">';

                   D = DATA.data;

                   
                   
                      

                    if( D.shuju ){

                         for(var i in D.shuju){

                             if( D.shuju[i]['daikai'] == 1 ){

                             html += '<li class="m-detail-tabHistory-item m-detail-tabHistory-countdown" id="dj'+D.shuju[i]['cpid']+'_'+D.shuju[i]['cpqs']+'">';
                             html += '<div class="m-detail-tabHistory-period">期号 '+D.shuju[i]['cpqs']+'</div>';   
                             html += '<div class="m-detail-tabHistory-info">  <div class="w-countdown"> ';        
                             html += '<span class="w-countdown-title">揭晓倒计时</span> ';  

                             var strtime = shijian( D.shuju[i]['djitime'] , 100 ); 
                            
                            if(D.shuju[i]['djitime']  > 360000){

                                hh = String(strtime['0']).split('');
                               ii = String(strtime['1']).split('');
                               ss = String(strtime['2']).split('');

        
                             }else{

                               hh = String(strtime['1']).split('');
                               ii = String(strtime['2']).split('');
                               ss = String(strtime['3']).split('');

                             }

                             html += '<span class="w-countdown-nums djtime" pro="countdownNum" djtime="'+D.shuju[i]['djitime']+'" data="'+D.shuju[i]['cpid']+'_'+D.shuju[i]['cpqs']+'"><b>'+hh['0']+'</b><b>'+hh['1']+'</b>:<b>'+ii['0']+'</b><b>'+ii['1']+'</b>:<b>'+ss['0']+'</b><b>'+ss['1']+'</b></span>';         
                             html += '</div>  </div>   <div class="m-detail-tabHistory-result"></div> <div class="m-detail-tabHistory-operation"><a href="'+D.shuju[i]['url']+'">查看详情</a></div> </li>';  
                             
                             
                             
                             }else{

                              html += '<li class="m-detail-tabHistory-item m-detail-tabHistory-published" id="pro-view-18">   <div class="m-detail-tabHistory-period">期号 '+D.shuju[i]['cpqs']+'</div>   <div class="m-detail-tabHistory-info">       <img onerror="this.src=\'/wangyi/p/one/web/lib/img/avatar/90.jpeg\';this.onerror=null;" src="'+D.shuju[i]['utouxiang']+'">       <div class="m-detail-tabHistory-info-txt">           <span>恭喜 <a href="'+HTTP+'user.php?y=userjilu&uid='+D.shuju[i]['uid']+'" title="'+D.shuju[i]['uname']+'" target="_blank"><span class="m-detail-tabHistory-info-nickname">'+D.shuju[i]['uname']+'</span></a>（<span class="m-detail-tabHistory-info-address" title="'+D.shuju[i]['ip']+'">'+D.shuju[i]['ip']+'</span>）获得了本期商品</span>           <span>用户ID：'+D.shuju[i]['uid']+'（ID为用户唯一不变标识）</span>           <span>本期参与：<strong>'+D.shuju[i]['canyu']+'人次</strong></span>       </div>   </div>   <div class="m-detail-tabHistory-result">       <span>幸运号码：<strong>'+D.shuju[i]['zhonghao']+'</strong></span>       <span>揭晓时间：'+D.shuju[i]['ktime']+'</span>       <span>云购时间：'+D.shuju[i]['stime']+'</span>   </div>   <div class="m-detail-tabHistory-operation"><a href="'+D.shuju[i]['url']+'" target="_blank">查看详情</a></div></li>';

                                    
    
                             
                             
                             
                             }
                         
                         
                         }


                    }


                   html += ' </div> </ul></div>';

                  if(D.page)  html+='<div class="pager"><div class="w-pager page"> '+D.page+'</div></div>';

                  $("#historyPanel").html( html );

                  if( $(".djtime").length ){

                         clearInterval( DAOJISHI );

                         DAOJISHI = setInterval( "CountDown()" ,10);
         
                   }

                       window.scrollTo(0,500);
                   




               } else $("#historyPanel").html('<div class="empty"><p class="status-empty"><i class="littleU littleU-cry"></i>&nbsp;&nbsp;暂时还没有往期记录</p></div>');
          
          
          } else $("#historyPanel").html('<div class="empty"><p class="status-empty"><i class="littleU littleU-cry"></i>&nbsp;&nbsp;暂时还没有往期记录</p></div>');


          });


