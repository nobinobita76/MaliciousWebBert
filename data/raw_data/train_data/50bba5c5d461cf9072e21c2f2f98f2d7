    function isonlinedoublered(type) {
        if (type==2) {
            return true;
        }
        if (type!=1) {
            return false;
        }
        //2015.02.18-2015.03.05
        var localtime = Math.round(new Date().getTime()/1000);
        if (parseInt(localtime)>=1424188800&&parseInt(localtime)<=1425571199) {
            return true;
        }
        return false;
    }
