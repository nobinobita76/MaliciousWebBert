<script>


var loadingTimer;
var site = {
	_init:function(){
	$('#loadingBox').fadeOut(600);
	$(".main").css({"display":"block"});
	loaded=true;
	ready();
	trackPageview("home");	
	
	}   
};



$('body').jpreLoader({
	}, function() {	//jPreLoader callback function
		clearInterval(loadingTimer);
		site._init(); 
});
$("#jpreOverlay").css({"display":"none","width":0,"height":0});
$("#jprePercentage").html("0%");

loadingTimer=setInterval(function(){
  var percentStr=String($("#jprePercentage").text());
  $("#loadingBox .percentNum").html(percentStr);
},100);	


$(function(){
	trackPageview("loading");
	})
	

</script>




