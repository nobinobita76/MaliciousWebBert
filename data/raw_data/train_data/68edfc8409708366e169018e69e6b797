  <script type="text/javascript">
function add_num_p(){
    var p_num=parseInt($('#p_nums').val());
    p_num++;
    $('#p_nums').attr('value',p_num);    
}
function plus_num_p(){
  var p_num=parseInt($('#p_nums').val());
  if(p_num>1) {
    p_num--;
    $('#p_nums').attr('value',p_num); 
  }   
}
function add_num_c(){
    var c_num=parseInt($('#c_nums').val());
    c_num++;
    $('#c_nums').attr('value',c_num);    
}
function plus_num_c(){
  var c_num=parseInt($('#c_nums').val());
  if(c_num>0) {
    c_num--;
    $('#c_nums').attr('value',c_num); 
   }
}
function buy_now() {
    var p_num=$("#p_nums").val();
    var c_num=$("#c_nums").val();	
    var date=$("#begintime").val().substr(0,10);
    if(p_num>0) {
        insert_buy('line','10','221',date,'',p_num,c_num,'16');
    } else {
        alert('成人数量不能为空！');return;
    }
}
function buy_now2() {
    var p_num=$("#p_nums2").val();
    var c_num=$("#c_nums2").val();	
    var date=$("#begintime2").val().substr(0,10);
    if(p_num>0) {
        insert_buy('line','10','221',date,'',p_num,c_num,'16');
    } else {
        alert('成人数量不能为空！');return;
    }
}
function insert_buy(app_prefix,cmd_num,id,start_date,end_date,number,child_number,routing_id) {
    $.ajax({
        url: 'http://www.516lyw.com/orders/order/insert_buy',
        type: 'POST',
        data:{
            app_prefix: app_prefix,
            cmd_num: cmd_num,
            id: id,
            start_date: start_date,
            end_date: end_date,
            number: number,
            child_number: child_number,
            routing_id: routing_id
        },
        dataType : 'json',
        error: function(result){alert('控制器错误！')},
        success: function(result){
            if(result.err > 0){
                alert("不能购买！");
                return;
            }
            buy('one',result.data);
        }
    });
}
function buy(type,data_id){
    $("#data_id").val(data_id);
    $("#type").val(type);
    if($("#type").val()=="all"){
        var i=0;
        $("input[id=id]").each(function(){
            if($(this).attr("checked")==true){
                i++;
            }
        });
        if(i==0){
            alert("请选择物品再提交！");
            return false;
        }
    }
    document.formbuy.submit();
}
</script> 
  <script type="text/javascript">
	var YUI = {	//前台YUI
		config : {
			page : "zhoubianlvyou",
		},
		public : {
			changeTab : function(thisTarget,toTarget,index){
				$(thisTarget).eq(index).addClass("first").siblings().removeClass("first");
				$(toTarget).eq(index).show().siblings(toTarget).hide();
			},
			getUrl : function(name){
				var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)"); //构造一个含有目标参数的正则表达式对象
				var r = window.location.search.substr(1).match(reg);  //匹配目标参数
				if (r != null) return unescape(r[2]); return null; //返回参数值
			},
			toFixed : function(dom,top,css){
				$(window).on("scroll",function(e){
					var marginTop = $(this).scrollTop();
					if(marginTop >= top){
						$(dom).css(css);
					}else{
						$(dom).removeAttr("style");
					}
				});
			}
		},
		headNav : {
			target : {
				firstLink : $(".all-nav"),//第一个hover对象
				navUl : $(".menu-list"),//Ul
				navLi : $(".menu-list-li"),
			},
			init : function (){
				var _self = this;
				if (YUI.config.page == "index"){ 
					_self.target.navLi.hover(function(){
						$(this).addClass("on").find(".submenu-list").show();
					},function(){
						$(this).removeClass("on").find(".submenu-list").hide();
					});
				}else{
					_self.target.firstLink.hover(function(){
						$(this).find(".menu-list").show();
					},function(){
						$(this).find(".menu-list").hide();
						$(".menu-list li").removeClass("on");
					});
					_self.target.navLi.on("mouseover",function(){
						$(this).addClass("on").siblings().removeClass("on");
					});
				};
			}
		},
		memberLoginInfo : {
			btn : $(".huiyuann"),
			wrap : $(".newMenu-popUb"),
			init :function(){
				var _self = this,t;
				_self.btn.hover(function(e){
					$(this).next(".newMenu-popUb").show();
				},function(){
					$(this).next(".newMenu-popUb").hide();
				});
			}
		},
		weixinHover : {
			btn : $(".topr-weixin"),
			wrap : $(".newMenu-weixinTip"),
			init : function(){
				var _self = this;
				_self.btn.hover(function(e){
					_self.wrap.show();
				},function(){
					_self.wrap.hide();
				});
			},
		},
		foot : {
			hover : {
				target : $("#footToTopUl"),
				init : function(){
					this.target.find("li").hover(function(){
						$(this).find(".content").show();
					},function(){
						$(this).find(".content").hide();
					});
				}
			},
			toTop : {
				init : function(){
					$(window).on("scroll",function(){
						var mt = $(this).scrollTop();
						if(mt>=600){
							$("#goTopWrap").fadeIn(500);
						}else{
							$("#goTopWrap").fadeOut(500);
						}
					});
					$("#goTop").click(function(){
						$("html,body").stop(true,true).animate({"scrollTop":0},500);
					});
				}
			},
			init : function(){
				this.hover.init();
				this.toTop.init();
			}
		},
		init : function(){//总的初始化
			this.headNav.init();
			this.memberLoginInfo.init();
			this.weixinHover.init();
			this.foot.init();
		},
	}
	YUI.init();
</script> 
  <script type="text/javascript">
    YUI.line = {
		//幻灯
        slide : {
            target : {
                small : $(".small_pic_01"),
                big : $(".big_pic_01"),
            },
            ani : function(){
                var _self = this;
                _self.target.small.on("mouseenter","li",function(){
                    var idx = $(this).index();
                    _self.target.big.find(".big_pic_list").hide().eq(idx).show();
                });
            },
            init : function(){
                this.ani();
            }
        },
		//日历
        calendar : {
            init : function(){
                showCalendar(0,221);
            },
            next_cal : function(date,line_id) {
                showCalendar(date,line_id)
            },
            past_cal : function(date,line_id) {
                showCalendar(date,line_id)
            }
        },
		//固定导航 立即预定
		yuding : {
			init : function(){
				$("#goPay").on("click",function(){
					$("html,body").animate({"scrollTop":"200px"},500);
				});
			}
		},
		//固定导航
        navFixed : {
            target : {
                wrap : $(".dtlNav"),
            },
            ani : function(){
                var _self = this;
                _self.target.wrap.on("click",".dtlNav__a",function(e){
                    var target = "#"+$(this).attr("tag");
                    var top = $(target).offset().top;
                    $(this).addClass("dtlNav__a_hover").siblings().removeClass("dtlNav__a_hover");
                    $("html,body").animate({"scrollTop":top},500);
                });
                YUI.public.toFixed(".dtlNav",800,{"position":"fixed","z-index":"1000","top":"0","width":"1190px","left":"50%","margin-left":"-595px"});
            },
            init : function(){
                this.ani();
            }
        },

		//预定儿童价格说明
		saleTips : {
			init : function(){
				$(".green_tips").hover(function(){
					$(this).find(".tro_san").show().siblings(".yinc").stop(true,true).slideDown(200);
				},function(){
					$(this).find(".tro_san").hide().siblings(".yinc").stop(true,true).slideUp(200);
				})
			}
		},
        init : function(){
            this.slide.init();
            this.navFixed.init();
            this.calendar.init();
            this.saleTips.init();
			this.yuding.init();
        },
    };
    YUI.line.init();
    function showCalendar(date,line_id) {
        $.ajax({
            url: '/line/line/get_calendar',
            type: 'POST',
            data: {
                date: date,
                line_id: line_id
            },
            dataType: 'html',
            success: function(data) {
                document.getElementById("cal_show").innerHTML = data
            }
        })
    }
</script> 
