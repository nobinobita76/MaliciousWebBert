(function(){
	$(document).scroll(function(){
		var topads = $(".ads_top"), ads_h = topads.height();
		if($(document).scrollTop() < ads_h && $(".ads_top").length){
			$(".ads_btm").hide();
		}else{
			$(".ads_btm").show();
		}
	});
	$(".ads-cancel").bind("click", function(){
		$(this).parents(".ads").remove();
	});
    /*
     * 将url参数解析为json对象
     */
    function parseQueryString(url){
        var obj={};
        var keyvalue=[];
        var key="",value=""; 
        var paraString=url.substring(url.indexOf("?")+1,url.length).split("&");
        for(var i in paraString){
            keyvalue=paraString[i].split("=");
            key=keyvalue[0];
            value=keyvalue[1];
            obj[key]=value; 
        } 
        return obj;
    }
    /*
     * 跳转到app原生页面
     */
    function gotoApp(href, type){
        var paramJson = parseQueryString(href);
        var map = {'full':0,'order':0,'add':1,'askdoc':2,'private':3};
        if(type == 'doc'){
            mJavaScriptObject.funGotoDocHomepageActivity(paramJson.unit_id, paramJson.dep_id, paramJson.doc_id, map[paramJson.type]);
        }else if(type == 'depart'){ //科室医院列表
            $.post("https://wap.91160.com/news/getpubcat.html", {'cat_id':paramJson.cat_id}, function(res){
                if(res){
                    mJavaScriptObject.funGotoDepartmentsList(res.cat_no, res.catname, paramJson.cat_id);
                }
            }, 'json');
        }else if(type == 'bounty'){ //悬赏咨询首页
            mJavaScriptObject.funGotoOfferRewardInTime('');
        }else if(type == 'asklist'){    //指定医生咨询列表
            mJavaScriptObject.funGotoConsultationDesignatedDoctorList('1');
        }else if(type == 'private'){    //app私人医生列表
            mJavaScriptObject.funGotoConsultationDesignatedDoctorList('2');
        }
        return false;
    }
    if((typeof(mJavaScriptObject)=='object')){
        $("a").on("click", function(){
            var href = $(this).attr("href");
            if(href.indexOf("doctor/detail.html") > -1){
                return gotoApp(href, 'doc');
            }else if(href.indexOf("depart/unit.html") > -1){
                return gotoApp(href, 'depart');
            }else if(href.indexOf("ask/bounty.html") > -1){
                return gotoApp(href, 'bounty');
            }else if(href.indexOf("ask/askdoc.html") > -1){
                return gotoApp(href, 'asklist');
            }else if(href.indexOf("test/appprivate.html") > -1){
                return gotoApp(href, 'private');
            }
        });
    }
})();
</script>

</body></html>