    $(function () {
        xue.updateEndTime('.settime');//执行倒计时函数
    });

    var rollStart = $('.scroll-box-wrap');
    var offset = rollStart.offset();
    var rollBox = rollStart.prev();
    $(window).scroll(function () {
        if ($(window).scrollTop() > offset.top) {
            rollStart.addClass("rollbox");
            rollStart.find('.scroll-btn').show();
            rollBox.stop().animate({top: 0, paddingTop: 10}, 0);
        } else {
            rollStart.removeClass("rollbox");
            rollStart.find('.scroll-btn').hide();
            rollBox.stop().animate({top: 0}, 0);
        }
    });
    //收藏课程
    function collect(id) {
        $.ajax({
            url: '/Courses/ajaxCollect/',
            type: 'post',
            dataType: 'json',
            data: {courseId: id, type: 1},
            success: function (data) {
                if (data.stat == 1) {
                    $('#collectId').addClass('collect_current').attr('onclick', '');
                    $('#collectId').parents('span').eq(0).addClass('collect_current');
                    $('#collectId').html('已收藏');
                } else if (data.stat == 2) {
                    window.location.href = 'https://login.xueersi.com/login/aHR0cDovL3d3dy54dWVlcnNpLmNvbS9rYy80MDEzOS0xNzgyLTE3ODQ4Lmh0bWw=';
                } else {
                    if (data.msg == '您已收藏过') {
                        $('#collectId').addClass('collect_current').attr('onclick', '');
                    }
                    alert(data.msg);
                }
            },
            error: function () {
                alert('数据读取错误..');
            }
        });
    }
    //截取中文字符
    function SetString(str, len) {
        var strlen = 0;
        var s = "";
        for (var i = 0; i < str.length; i++) {
            if (str.charCodeAt(i) > 128) {
                strlen += 2;
            } else {
                strlen++;
            }
            s += str.charAt(i);
            if (strlen >= len) {
                return s;
            }
        }
        return s;
    }
    //评论列表
    function createCommList(url, params) {
        $.ajax({
            url: url,
            type: 'POST',
            dataType: 'json',
            data: {courseId: params},
            success: function (result) {
                var box = $('#comm_page_list');
                if (result.sign == 1) {
                    var tpl = '';
                    $.each(result.msg.data, function (k, v) {
                        tpl += '<div class="comment-detail-list">';
                        tpl += '<div class="user-pic">';
                        tpl += '<p class="pic ui-userinfo" data-id="' + v.stu_id + '"  data-params="userId=' + v.stu_id + '" data-id="' + v.stu_id + '"><img width="40" height="40" src="' + v.head_img_url + '"></p>';
                        tpl += '<p class="name" title="' + v.username + '">' + v.username + '</p>';
                        var province = SetString(v.province, 6);
                        tpl += '<p class="site" title="' + v.province + '">' + province + '</p>';
                        tpl += '</div>';
                        tpl += "<div class='user-text-box'>";
                        tpl += '<p class="text"> ' + v.content + '</p>';
                        tpl += '<p class="time">' + v.create_time + '</p>';
                        //tpl += '<span class="corner_bg"></span>';
                        if (v.is_cream == 1) {
                            tpl += '<span class="added_icon"></span>';
                        }
                        tpl += '</div>';
                        tpl += '</div>';
                    });
                    tpl += '</div>';
                    box.html(tpl);
                    $('.ui-pages').pages({
                        total: result.msg.rows, // 总记录数
                        size: 10, // 每页显示记录数
                        index: 1, // 当前页
                        // 点击分页时的回调，返回被点击的页数
                        click: function (index) {
                            ajaxCommPages(url, params, index);
                        }
                    });
                }
            }
        });
    }


    function ajaxCommPages(url, course_id, page) {
        $.ajax({
            url: url,
            type: 'POST',
            data: {courseId: course_id, curpage: page},
            dataType: 'json',
            success: function (result) {
                var box = $('#comm_page_list');
                if (result.sign == 1) {
                    var tpl = '';
                    $.each(result.msg.data, function (k, v) {
                        tpl += '<div class="comment-detail-list">';
                        tpl += '<div class="user-pic">';
                        tpl += '<p class="pic ui-userinfo" data-id="' + v.stu_id + '"  data-params="userId=' + v.stu_id + '" data-id="' + v.stu_id + '"><img width="40" height="40" src="' + v.head_img_url + '"></p>';
                        tpl += '<p class="name" title="' + v.username + '">' + v.username + '</p>';
                        var province = SetString(v.province, 6);
                        tpl += '<p class="site" title="' + v.province + '">' + province + '</p>';
                        tpl += '</div>';
                        tpl += "<div class='user-text-box'>";
                        tpl += '<p class="text"> ' + v.content + '</p>';
                        tpl += '<p class="time">' + v.create_time + '</p>';
                        //tpl += '<span class="corner_bg"></span>';
                        if (v.is_cream == 1) {
                            tpl += '<span class="added_icon"></span>';
                        }
                        tpl += '</div>';
                        tpl += '</div>';
                    });
                    tpl += '</div>';
                    box.html(tpl);
                } else {
                    alert(result.sign);
                }
            }
        });
    }
    //错误提示
    function commentError(msg) {
        var _errorBox = $('#course_comm_tips');
        _errorBox.text(msg);
        _errorBox.css('color', '#cc0000');
        setTimeout(function () {
            $('#course_comm_tips').text('请填写评价内容，长度在10~140字符之间');
        }, 3000);
    }
    //课程评价
    function addCourseComm() {
        var content = $.trim($('#comm_content').val());
        var courseId = $.trim($('#course_comm_courseID').val());
        var length = content.length;
        if (length < 10) {
            commentError('请填写评价内容，长度在10~140字符之间');
            return false;
        }
        if (length > 140) {
            commentError('请填写评价内容，长度在10~140字符之间');
            return false;
        }
        if (courseId == '') {
            commentError('提交失败，请刷新页面后重试');
            return false;
        }
        var _params = "content=" + content + "&courseId=" + courseId + '&type=2';
        $.ajax({
            type: "post",
            url: '/courses/addCourseComm',
            dataType: "json",
            timeout: 7000,
            data: _params,
            success: function (data) {
                if (data.stat == 3) {
                    window.location.href = 'https://login.xueersi.com/login/aHR0cDovL3d3dy54dWVlcnNpLmNvbS9rYy80MDEzOS0xNzgyLTE3ODQ4Lmh0bWw=';
                } else if (data.stat == 1) {
                    $('#comm_content').val('');
                    //                    commentError(data.msg);
                    var comUrl = '/courses/ajaxCommList/';
                    var params = 40139;
                    createCommList(comUrl, params);
                    // 加金币
                    if (data.type == 2) {
                        // 加金币
                        var gold = '<div class="Extend_add_gold"><p><strong>1</strong></p></div>';
                        var wrap = $('.ui-comment-items');
                        wrap.append(gold);
                        wrap.find('.Extend_add_gold').stop().animate({
                            opacity: 1,
                            bottom: -7
                        }, '5000', function () {
                            setTimeout(function () {
                                wrap.find('.Extend_add_gold').fadeOut('normal');
                            }, 3000);
                        });
                        $('#comm_content').empty();
                        $('#course_comm_tips').empty();
                    }
                } else {
                    commentError(data.msg);
                }
            },
            error: function () {
                alert('数据读取错误..');
            }
        });
    }
    $(function () {
        var comUrl = '/courses/ajaxCommList/';
        var params = 40139;
        createCommList(comUrl, params);
        $('#course_comm_form').submit(function () {
            addCourseComm();
        });
    });
    function qqteacher() {
        var h = $(window).height();
        $('.qq-layer-item').css('height', h);
    }
    qqteacher();
</script>

