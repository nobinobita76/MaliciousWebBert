    // 点击搜索按钮执行搜索方法
    function search() {
        var url = 'http://www.xueersi.com/search';
        var keyword = $('#mq').val();
        if (keyword == '') {
            keyword = '直播课';
        } else if (keyword.length > 50) {
            //限制字符长度
            keyword = keyword.substr(0, 38);
        }
        url += '?keyword=' + keyword;
        window.location.href = url;
    }
