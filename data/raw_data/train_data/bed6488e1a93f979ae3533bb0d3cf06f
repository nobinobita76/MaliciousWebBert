    function look(obj) {
        $("div[class='index_zqwg_one']").hide();
        $("div[class='index_zqwg_two']").show();
        $(obj).parent('div').hide();
        $(obj).parent('div').prev('div').show();
    }

    function expertLook(obj, num) {
        $("div[id='exone" + num + "']").hide();
        $("div[id='extwo" + num + "']").show();
        $(obj).hide();
        $(obj).prev('div').show();
    }

    function change(obj, id) {
        $('#rebobang').find('li').removeClass('current2');
        $(obj).addClass('current2');
        $('#rebodiv').children('ul').hide();
        $('#' + id).show();
    }

    function toggleDuoshuoComments(container, a_id, url, user, num) {
        if ($(container).html() != null) {
            $(container).html('');
        }
        var el = document.createElement('div'); //该div不需要设置class="ds-thread"
        el.setAttribute('data-thread-key', a_id); //必选参数
        el.setAttribute('data-url', url); //必选参数
        el.setAttribute('data-author-key', user); //可选参数
        DUOSHUO.EmbedThread(el);
        jQuery(container).append(el);
        $("div[class='box']").hide();
        $("#comment-box" + num).show();
    }

    function toDisplay(id) {
        $('#gj').find('ul').hide();
        $(id).show();
    }

    function toDis(id) {
        $('#gzzz').find('ul').hide();
        $(id).show();
    }
    $(function(){
        $('.index_saicheng_in > li').click(function(){
            //$('.index_saicheng_in > li')
            $('.index_saicheng_in > li').removeClass('index_current');
            $(this).addClass('index_current');
            $('.index_saicheng_one').hide();
            var id = $(this).attr('data-id');
            $('.match_'+id).show();
        })
    })
    
    </script>
