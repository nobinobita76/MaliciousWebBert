    <script type="text/javascript">
    var bg2 = 'url(/Public/Home/images/sanjiaobg2.png) no-repeat 44px center';
    var bg4 = 'url(/Public/Home/images/sanjiaobg4.png) no-repeat 44px center';
    $(function() {

        $("#category").val($("#keyword").html());

        /*$("#land").toggle(
          function(){
            $('#username').val('');
            $('#passwd').val('');
            $("#lands").show();},
          function(){
          $("#lands").hide();}
        );*/
        $('#land').click(function() {
            $('#username').val('');
            $('#passwd').val('');
            $("#lands").show();
        });

        var jsbfdh = document.getElementById("keyword");
        if (jsbfdh != null) {
            document.getElementById("keyword").onmouseout = function(evt) {
                var div = document.getElementById("seul");
                var myEvent = evt ? evt : (window.event ? window.event : null);
                var x = myEvent.clientX;
                var y = myEvent.clientY;
                //var divx1 = div.offsetLeft; 
                var divx1 = 499;
                //var divy1 = div.offsetTop; 
                var divy1 = 69;
                var divx2 = divx1 + div.offsetWidth;
                var divy2 = divy1 + div.offsetHeight;

                if (x < divx1 || x > divx2 || y < divy1 || y > divy2) {
                    $("#seul").hide();
                }
            }
        }

    });

    function login() {
        if ($('#username').val() == '') {
            alert('请输入用户名');
            return false;
        }
        if ($('#passwd').val() == '') {
            alert('请输入登录密码');
            return false;
        }
        $.ajax({
            url: 'index.php-s=-Home-User-login.htm'/*tpa=#/index.php?s=/Home/User/login*/,
            type: 'post',
            dataType: 'json',
            async: false,
            data: {
                Name: $('#username').val(),
                Pwd: $('#passwd').val()
            },
            success: function(data) {
                if (data.Status == 1) {
                    alert('登录成功');
                    window.parent.location.href= '#zq.6383.com/';
                } else {
                    alert(data.Msg);
                }
            }
        });
    }

    function addc(obj) {
        $(obj).addClass('index_selected_in');
    }

    function removec(obj) {
        $(obj).removeClass('index_selected_in');
    }

    function sel(obj) {
        var title = $(obj).html();
        $('#keyword').html(title);
        $('.index_selected').hide();
        $('#keyword').css('background', bg2);
        $("#category").val($("#keyword").html());
    }

    function dis(obj) {
        var cs = $('.index_selected').css('display');
        if (cs == 'none') {
            $('.index_selected').show();
            $(obj).css('background', bg4);
        } else {
            $('.index_selected').hide();
            $(obj).css('background', bg2);
        }
    }

    function disc(obj) {
        $(obj).show();
    }

    function hid(obj) {
        $(obj).hide();
    }

    function searchkey(obj) {
        var key = $(obj).html();
        $('#content').val(key);
    }

    function hao(obj) {
        $(obj).show();
    }

    function gbhao(obj) {
        $(obj).hide();
    }

    $(document).click(function() {
        $("#land").click(function() {
            $("#lands").show();
            return false;
        });
        $("#lands").click(function() {
            $("#lands").show();
            return false;
        });
        $("#index_renmenbisai").click(function() {
            return false;
        });
        $("#lands").hide();
    });
    $(function(){
        var $div_li = $(".index_top_in_wxwb a");
        $div_li.mouseover(function(){
            var index =  $div_li.index(this);
            $(".index_dyh img").eq(index).show();
        })
        $div_li.mouseout(function(){
            var index =  $div_li.index(this);
            $(".index_dyh img").eq(index).hide();
        })

        $(".index_land_pleaseland").bind("click",function (){
            $(".index_landtk").toggle();
        })
       
        $('.index_new_nav_col1  li > a').each(function(){
           var reg =  new RegExp('/','g');
             var parentUrl = window.parent.location.href.replace(reg,''),aUrl = $(this).attr('href').replace(reg,'');
            var aa = parentUrl.indexOf(aUrl);
            //alert(parentUrl);
            //alert(aUrl);
            if(aa>0 && aUrl !=='index.html'/*tpa=#/index.html*/){
                $(this).addClass('action');
            }
        })
        var user_info = getCookie('MemberName');
        if(user_info){
            $('.memberName').html(user_info);
            $('.logoutHide').hide();
            $('.membershow').show();
        }
    })
    function getCookie(objName){
        //获取指定名称的cookie的值
        var arrStr = document.cookie.split("; ");
        for(var i = 0;i < arrStr.length;i ++){
             var temp = arrStr[i].split("=");
            if(temp[0] == objName){
                return unescape(temp[1]);
            }
        }
    }
