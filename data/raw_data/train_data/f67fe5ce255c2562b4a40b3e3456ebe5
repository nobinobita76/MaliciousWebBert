				<script type="text/javascript"> 
                        $(document).ready(function(){
                        	
							 //鼠标滑动 触发ajax事件
                              $(".navLe").mouseover(function(){
                            	  	 $("#newServer").empty();
                            	  	 $.post("/g/st/CheckEnterGameTime",{type:"newserver"},function(data, status){
                            	    	$("#newServer").append(data);
          							 });
                            	  
                                     $(".cmLista").show();
									 $(".cmListb").hide();
                              });
                              
                              $(".navRi").mouseover(function(){
                            	    $("#closePre").empty();
                            	    $.post("/g/st/CheckEnterGameTime",{type:"close"},function(data, status){
                            	    	$("#closePre").append(data);
          							});
                            	  
                                    $(".navRi").addClass("navLea");
									$(".navLe").addClass("navRia");
									$(".cmListb").show();
									$(".cmLista").hide();
                              });
                              
							  $(".navLe").mouseover(function(){
								     $(".navLe").removeClass("navRia");
									 $(".navRi").removeClass("navLea");
                              });
							  
                              
                        });
                        
                        function gameTip(gid,sid){
                        	window.open("/g/st/CheckEnterGameTime?gid="+gid+"&sid="+sid);
            			}
              </script>
				
