			// 第三方登录跳转地址
			var loginUrl = 'http://game.kuwo.cn:80/g/st/Entry_2014?src=NewerIndex_2014';
			
			jQuery(document).ready(function(){
				gameSearchText();
				// 网页游戏
				var detail = null;
				jQuery(".cAcm").mouseenter(function(){
					var my = jQuery(this); 
					detail = setTimeout(function(){
						my.find(".cAbg").slideDown("fast");
						my.find(".cAtex").slideDown("fast");
						my.find(".cAtYk").slideDown("fast");
					}, 200);
				}).mouseleave(function(){
					clearTimeout(detail);
					jQuery(this).find(".cAbg").css("display","none");
					jQuery(this).find(".cAtex").css("display","none");
					jQuery(this).find(".cAtYk").css("display","none");
				});
				
				jQuery("#closeweixin").click(function(){
					jQuery("#gamehall_weixin").hide();
				});
				
				// 文字滚动
				wordScroll("boxm", 34, 30, 3000);
				
				// 轮播图
				$("#slide").cxSlide({
					speed:800, //切换速度(ms)
					time:5000, //自动轮播间隔时间(ms)
					plus:true,
					minus:true
				});
				
				// 右下角广告
				var www_game_popup_cookie = getCookie('www_game_popup');
				if(www_game_popup_cookie==null || www_game_popup_cookie==''){
					document.getElementById('ad_div_id').style.display = 'block';
					var oHead = document.getElementsByTagName("head")[0];
					var oScript = document.createElement("script");
					oScript.src = "http://wa.kuwo.cn/lyrics/img/kwgg/kwgg_207.js";
					oScript.charset = "gbk"
					oHead.appendChild(oScript);
					addCookie('www_game_popup','show_www_game', 24*60*60, '/', 'kuwo.cn');
					window.onresize = window.onscroll = window.onload = getNavi;
				}
				
				// 检查cookie信息
				var username = getCookie('username');
				var userid = getCookie('userid');
				var uph = getCookie('uph');
				if(username!=null && username!='' && userid!=null && userid!='' && uph!=null && uph!=''){
					var date = new Date().getTime();
					$.get("/g/st/Entry_2014?logSrc=index&date="+date, function(data, status){
						var jsonData = eval('(' + data + ')');
						var result = jsonData.result;
						if(result == "succ"){
							showUserInfo(jsonData);
						}
					});
				}
				
				//进入页面  加载数据 避免varnish缓存
        	  	$.post("/g/st/CheckEnterGameTime",{type:"newserver"},function(data, status){
        	    	$("#newServer").append(data);
				});
			});
			
