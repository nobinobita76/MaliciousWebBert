<script type="text/javascript">
	var checkIn = null;
	/* var iconC = null;
	var iconD = null;
	var iconE = null; */
	var iconF = null;
	$(document).ready(function(){
		if($("#checkIn1").css("display") == "block"){checkIn = setval("checkIn");}
		
		if($("#overBtn")[0]){
			/* iconC = setval("iconC");
			iconD = setval("iconD");
			iconE = setval("iconE"); */
			iconF = setval("iconF");
		}
		
		// 图标点击事件
		$("img[id^=checkIn]").click(function(){clsval(checkIn, "checkIn");});
		/* $("img[id^=iconC]").click(function(){clsval(iconC, "iconC");});
		$("img[id^=iconD]").click(function(){clsval(iconD, "iconD");});
		$("img[id^=iconE]").click(function(){clsval(iconE, "iconE");}); */
		$("img[id^=iconF]").click(function(){clsval(iconF, "iconF");});
	});
	
	// 设置计时器
	function setval(str){
		var cookie = getCookie('www_game_' + str);
		var val = null;
		if(cookie==null || cookie==""){
			val = setInterval(function(){iBlink(str)}, 500);
		}
		
		return val;
	}
	
	// 图标闪动
	function iBlink(type){
		var t1 = $("#" + type + "1");
		var t2 = $("#" + type + "2");
		if(t1.css("display") == "none"){
			t2.hide();
			t1.show();
		}else{
			t1.hide();
			t2.show();
		}
	}
	
	// 清除计时器并添加cookie
	function clsval(val, str){
		if(val != null){
			clearInterval(val);
		}
		$("#"+str+"2").hide();
		$("#"+str+"1").show();
		addCookie('www_game_'+str, 'show_www_game', 24*60*60, '/', 'kuwo.cn');
	}
</script>

