            function findGoodsBySpec(){
    			var selectSpecArray = new Array(); //已选中的
            	$(".selectInfoWarp .selectWarp .selectBox").find("a").each(function(){
            		//遍历所有，取选中值
            		if ($(this).hasClass("on")) {
                        var specValue = $(this).attr("data-value");
                        selectSpecArray.push(specValue);
                    } 
            	});
            	
            	var spec = selectSpecArray.join(";"); 
            	var productId = '767'; 
            	var goodsId = null;
                $.ajax({
                    url: 'findGoodsBySpec',
                    type: 'post',
                    async: false,
                    data: {"specStr" : spec,"productId" : productId},
                    dataType: 'json',
                    success: function(data) {
                       goodsId = data;
                    }
                });
                
                return goodsId;
            }
            
			
			function queryGoodsStock(areaId){
				var goodsId = $("#goodsId").val();
				var isInstall = $("#isInstall").val() == 1 ? true : false;
				var stockInfo = null;
				$.ajax({
                     url : '' + "/dealerGoods/queryStockByAddress",
                     type : 'post',
                     data: {"goodsId": goodsId , "isInstall":isInstall ,"areaId":areaId},
                     async: false,
                     success: function(data){
                    	 stockInfo = data;
                         var stock = stockInfo.stock;
                      	 if(stock <= 0){
                      		//无货时改变"加入购物车"鼠标样式
                      	 	$("#addCart").css("cursor","not-allowed");
                      	 	$("#immediately").css("cursor","not-allowed");
                      	 }else{
                      		$("#addCart").css("cursor","pointer");
                      		$("#immediately").css("cursor","pointer");
                      	 }
                     }
                });
				return stockInfo;
			}
			
			//updated by qinzhonglaing 2016/1/3：获取当前区域下的库存
			function queryStockByArea(){
				var cookieAreaId = '310106';
				var goodsId = $("#goodsId").val();
				var isInstall = $("#isInstall").val() == 1 ? true : false;
				var stock = 0;
				$.ajax({
                     url : '' + "/dealerGoods/queryStockByAddress",
                     type : 'post',
                     data: {"goodsId": goodsId , "isInstall":isInstall ,"areaId":cookieAreaId},
                     async: false,
                     success: function(data){ stock = data.stock; }
                });
				return stock;
			}
			
			
			
			function queryIsInstall(areaId){
				var isQuery = 'false';
				if(isQuery){
					//仅商品支持安装服务时查询
					var goodsId = $("#goodsId").val();
					$.ajax({
	                     url : '' + "/dealerGoods/queryIsInstall",
	                     type : 'post',
	                     data: {"goodsId": goodsId ,"areaId":areaId},
	                     async: false,
	                     success: function(data){
	                     	var isInstall = data.isInstall;
	                        if(isInstall){
	                        	$("#installDiv").show();
	                        }else{
	                        	$("#installDiv").hide();
	                        }
	                     }
	                });
				}
			}
			
			function reflushGuessULike(){
				var bt=baidu.template;
                //设置左分隔符为 <!
                baidu.template.LEFT_DELIMITER='<!';
                //设置右分隔符为 <!
                baidu.template.RIGHT_DELIMITER='!>';
               
                    $.ajax({
                        url:"/goods/guessYouLove",
                        type: "POST",
                        data: {},
                        success: function(data){
                           // var data = page.rows;
                            if(data){
                            	$("#ul_guLike").html("");
                                $.each(data,function(i,e){//debugger;
                                    var a={
                                        id: e.id,
                                        title: e.title,
                                        image: e.image,
                                        price:e.price
                                    //    startDate:new Date(e.startDate).Format("yyyy.MM.dd"),
                                       // endDate: new Date(e.endDate).Format("yyyy.MM.dd"),
                                    }
                                    var html=bt('t_gULike', a);
                                    
                                    $("#ul_guLike").append(html);
                                    $('img.lazy').lazyload();
                                });
                               
                            }
                            /* if(page.lastPage){
                                $(".more_btn").hide();
                            }else{
                                page+=1;
                            } */
                        },
                         error:function(page){
                            alert("获取失败.请稍候再试");
                        } 
                    })
                    
			}
			
			function gobrandSearch(id, name){
				$("#brandId").val(id);	
				$("#searchBrandForm #brandName").val(name);
				$("#searchBrandForm").submit();
			}
			function gocategorySearch(id, name){
				$("#categoryId").val(id);	
				$("#categoryName").val(encodeURI(name));
				$("#searchCategoryForm").submit();
			}
        </script> 
  </div> 
  <form action="/goods/search" method="get" id="searchBrandForm"> 
   <input type="hidden" id="brandId" name="brand" /> 
   <input type="hidden" id="brandName" name="brandName" /> 
  </form> 
  <form action="/goods/search" method="get" id="searchCategoryForm"> 
   <input type="hidden" id="categoryId" name="cat" /> 
   <input type="hidden" id="categoryName" name="catName" /> 
  </form>  
 </body>
</html>