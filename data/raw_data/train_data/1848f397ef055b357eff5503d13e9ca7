	 	if(typeof($("#"+phone_4+"_"+str_year).val())!="undefined"){
	   	    $(":input[name='terminalinfo.phone_0']").val(phone_4+"_"+str_year);
       	}else if(typeof($("#"+phone_3+"_"+str_year).val())!="undefined"){
	       	$(":input[name='terminalinfo.phone_0']").val(phone_3+"_"+str_year);
       	}
       	
        $('ul.choiceType a.curr').filter(function(){return $(this).css('display') != 'none' && $(this).attr('notPhone') == undefined}).each(function(index){
           	$(":input[name='terminalinfo.phone_"+(index+1)+"']").val($(this).attr('val'));
        })
       
        //双11活动临时修改
        if('wt_store_cfg22193619070'=='wt_store_cfg42087858646' || 'wt_store_cfg22193619070'=='wt_store_cfg42087773860'){
        	$(":input[name='terminalinfo.phone_4']").val("HAVE_YHQ_200M120");
        }
        //双11活动临时修改 2016-11-03
        if('wt_store_cfg22193619070'=='wt_store_cfg42102983818'){
        	$(":input[name='terminalinfo.phone_4']").val("HAVE_YHQ_100M1109");
        }
         //双12活动临时修改 2016-12-05
        if('wt_store_cfg22193619070'=='wt_store_cfg42131793542'){
        	$(":input[name='terminalinfo.phone_4']").val("HAVE_YHQ_SSE100M");
        }

            $(":input[name^='terminalinfo.phone_']").filter(function(){return $.trim($(this).val())==''}).eq(0).val($('#priceLevel ol li.current').attr('rel'));
        
        
        
        
        //选号后，地区限制带下来
        var areaidValcuandi = $("#sch-add .curr").attr("data-val");
        $('#choosenumarea').val(areaidValcuandi);
        //原来有号码的则在haokaParamMap存在下,把地区信息置入
        
       //没有号码的情况
       var telPhoneNum = $('#telPhoneNum').text();
       if(telPhoneNum.substring(0,1)!='1'){
           $('#choosenumarea').val('defined');
       }
        

        if(window.tmpParam1 && window.tmpParam0){
            $('#hasNumber'+window.tmpParam0).val('0');
