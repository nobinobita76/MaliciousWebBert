    if(checkInfo()){
       $(":input[name^='terminalinfo.phone']:not([name='terminalinfo.phone_"+_not_to_clear_index+"'])").val('');
     	//自动订单价格配置修改--验证当前是宽带新装自动订单并且该地市价格不同则进行修改
