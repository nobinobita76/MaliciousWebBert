       $.ajax({
        type : 'POST',
        data : {'returnurl':returnurl},
        url : '/market/sale/getwxtaiaozhuanurl.whtml',
        success : function(data){
        	if(data.result_code=="success"){
			   window.location.href=data.result_url;
			}
        }
   	  })
    }
});
