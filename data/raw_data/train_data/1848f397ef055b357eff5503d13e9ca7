function setParam(event,_this){
    if(!window.tmpParam1 || !window.tmpParam0){
        window.tmpParam0 = event.data.param0;
        window.tmpParam1 = event.data.param1;
    }
}
function setParamtwo(event,_this){
    if(!window.tmpParam2 || !window.tmpParam3){
        window.tmpParam2 = event.data.param0;
        window.tmpParam3 = event.data.param1;
    }
}

function countDisplay(obj){
	var count = 0;
	$(obj).find("a").each(function(){
		if($(this).css("display")!="none") count++;
	});
	return count;
}
function checkInfo(){
    var result = true;
    var alertMsg = "";
    $('div[level]').filter(function(){
    return (($(this).attr("verify")=="Y" && countDisplay($(this))>0) || ($(this).css('display') != 'none'&& $(this).attr('id')!='优惠券Div'&& $(this).attr('id')!='预约号Div' && $(this).children('li').children('a').attr('dxkk')!='dxkk'))
    }).each(function(){
        if(!$(this).find('a').hasClass('curr')){
            alertMsg+=('\r\n请先选择'+$(this).attr('id').substring(0, $(this).attr('id').length - 3));
            result = false;
        }
    })
    $(':input[id^=hasNumberType]').filter(function(){return $.trim($(this).val())!='' && $(this).attr('id').replace(/hasNumberType/,'') < 2} ).each(function(){
        var $that = $(this);
        var _nbr = $('#'+$that.attr('id').replace('Type','')).val();
        if(!/^1(33|53|80|81|89|77|73)\d{8}$/.test(_nbr)){
            //var loc = $('div :input').filter(function(){return $(this).css('display')!='none' && $(this).val() == _nbr}).parent('li').parent().prev("p").text()
            var loc = $(".choiceType li a[func*='"+$that.val()+"']").filter(function(){return $(this).css('display')!='none'}).parent('li').parent().prev("label").text().replace(":","");
            alertMsg+=('\r\n请输入浙江电信手机号码！');
            result = false;
        }
    })
    if(alertMsg!=""){
    	alert(alertMsg);
    }
    return result;
}


