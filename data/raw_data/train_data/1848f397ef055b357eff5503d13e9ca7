<script>
function setCookie(name, value) {
			var days = 30;
			var exp = new Date();
			exp.setTime(exp.getTime() + days * 24 * 60 * 60 * 1000);
			document.cookie = name + "=" + value + ";expire=" + exp.toGMTString()+";path=/";
	}
function getCookie(name){
	var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
     if(arr != null) return unescape(arr[2]); return null;
}
//渠道头尾显示
$(function(){
      //关于手机网厅头尾隐藏控制-----start
	  var mallurl = window.location.href;
	  if(mallurl.indexOf("?")>-1){
		  var res = mallurl.split("?");
		  var urichannel = res[1];
		  if(urichannel.indexOf("hf=nhf")>-1){
		     $("#headershow").hide();
		     $("#footershow").hide();
		     $("#zgdxlogo").hide();
		     //setCookie("hf","nhf");
		     sessionStorage.setItem("hf","nhf");
		  }else if(urichannel.indexOf("hf=nh")>-1){
		     $("#headershow").hide();
		     //setCookie("hf","nh");
		     sessionStorage.setItem("hf","nh");
		  }else if(urichannel.indexOf("hf=nf")>-1){
		     $("#footershow").hide();
		      //setCookie("hf","nf");
		      sessionStorage.setItem("hf","nf");
		  }
		  if(urichannel.indexOf("dh=hide")>-1){
		     $("#showdnfk").hide();
		  } 
	  }
	  //cookie里面再判断头尾隐藏
	  var hf = sessionStorage.getItem("hf");
	  if(hf=="nhf"){
		$("#headershow").hide();
		$("#footershow").hide();
		$("#zgdxlogo").hide();
	  }else if(hf=="nh"){
		$("#headershow").hide();
	  }else if(hf=="nf"){
	  	$("#footershow").hide();
	  }
      //关于手机网厅头尾隐藏控制-----end
      var get_minihome = sessionStorage.getItem("minihome");
      if(get_minihome=="minihome"){
           $("#footershow").hide();
           $("#minihome").show();
      }
});


//链接参数拼装组合
function hreffun(hrefstr){
    var mallurl = window.location.href;
    var hiddenchannel = $("#hiddenchannel").val();

	if(hiddenchannel!=''){
	   if(hrefstr.indexOf("channel")>-1){//已有渠道信息
	      window.location=hrefstr; 
	   }else{
	     if(hrefstr.indexOf("?")>-1){
		       window.location=hrefstr+"&channel="+hiddenchannel;
		  }else{
		       window.location=hrefstr+"?channel="+hiddenchannel;
		  }
	   }
	}else{
	   window.location=hrefstr;      
	}
}



//反馈	
$(function(){	
	setFooter();
	 $("#J_feedbackSubmit").bind("click", function(){
		 var data = "tucao=" + encodeURIComponent(encodeURIComponent($("#J_iptIdea").val())) 
			+ "&url=" + window.location.href;
			$.ajax({
				url : "/market/fankui/fankui.whtml",
				data : data,
				type : "post",
				beforeSend : function(){//异步加载处理
					//mask.hide("feedback-main");
					$("#wrap-feedback").hide();
					$('.feedback-mask').remove();
				},
				complete:function(){					
				},
				success:function(data){//请求成功					
				},
				error:function(){					
				} 
			});	 
	 });
})

//终端设备以及浏览器版本判断
var browser={
    versions:function(){
        var u = navigator.userAgent, app = navigator.appVersion;
        return {//移动终端浏览器版本信息
            trident: u.indexOf('Trident') > -1, //IE内核
            presto: u.indexOf('Presto') > -1, //opera内核
            webKit: u.indexOf('AppleWebKit') > -1, //苹果、谷歌内核
            gecko: u.indexOf('Gecko') > -1 && u.indexOf('KHTML') == -1, //火狐内核
            mobile: !!u.match(/Mobile/), //是否为移动终端
            ios: !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/), //ios终端
            android: u.indexOf('Android') > -1 || u.indexOf('Linux') > -1, //android终端或者uc浏览器
            iPhone: u.indexOf('iPhone') > -1 || u.indexOf('Mac') > -1, //是否为iPhone或者QQHD浏览器
            iPad: u.indexOf('iPad') > -1, //是否iPad
            webApp: u.indexOf('Safari') == -1, //是否web应该程序，没有头部与底部
            weixin: u.indexOf('MicroMessenger') > -1 , //微信浏览器
            ad_pa:u.indexOf('534') > -1 && u.indexOf('Linux') > -1, //安卓自带浏览器
            uc:u.indexOf('UCBrowser') > -1  //uc浏览器
        };
    }(),
    language:(navigator.browserLanguage || navigator.language).toLowerCase()
}

//footer导航以及反馈弹窗
function setFooter(){
	//footer导航
	$('.footer .btn-nav').click(function(e){
		e.preventDefault();
		var _this = $(this);
		var sTop = $(window).scrollTop()+100+'px';
		_this.find('i').toggleClass('hide');
		_this.parent('.list-service').next('.short-nav').toggle();
		if (!_this.find('i').hasClass('hide')) {
			$('html,body').animate({'scrollTop':sTop});
		};
	});
	//反馈弹窗
	$('#J_feedBack').click(function(e){
		e.preventDefault();
		var _this = $(this);
		var wth_win = $(window).width();
		var ht_win = $(window).height();
		var ht_doc = $(document).height();
		$('body').append('<div class="feedback-mask"></div>');
		$('.feedback-mask').height(ht_doc).show();
		$('#wrap-feedback').width(wth_win).height(ht_win/2).show();

	});
	$('.feedback-close').click(function(){
		$('#wrap-feedback').hide();
		$('.feedback-mask').remove();
	});

	$('#J_clet').click(function(e){
		e.preventDefault();
		var wth_win = $(window).width();
		var ht_win = $(window).height();
		var ht_doc = $(document).height();
		var top = 0;
		$('body').append('<div class="feedback-mask"></div>');
		$('.feedback-mask').height(ht_doc).show();
		if (browser.versions.mobile) {
			if (browser.versions.ios) {
				if (browser.versions.iPad) {
					$('.icon-refer').addClass('refer-pad').show();
					$('.show-pad').show().siblings('.clet-show').hide();
				}else{
					$('.icon-refer').removeClass('refer-pad').show();
					$('.show-ph').show().siblings('.clet-show').hide();
				}
			}else{
				$('.show-ad1').show().siblings('.clet-show').hide();
			}
			top = (ht_win-$('.wrap-clet').height())/2+'px';
			$('.wrap-clet').css('top',top).show();
		}else{
			return false;
		}
	});

	$('.clet-close').click(function(e){
		e.preventDefault();
		$('.wrap-clet').hide();
		$('.feedback-mask').remove();
	});
}

</script>


