<script type="text/javascript">
//返回按钮自定义
$(function(){
    //是否为mini厅,是mini厅则隐藏尾部
    var subfromaction = $('#subForm').attr("action");
	if(subfromaction.indexOf('/market/mini_order/index.whtml')>=0){
	     $("#footershow").hide();
         $("#minihome").show();
         //setCookie("minihome","minihome");
         sessionStorage.setItem("minihome","minihome"); 
	}
	
	//将商品链接地址存入
	 sessionStorage.setItem("shangpinlianjie",window.location.href); 
	
	$("#backBrowdetail").bind("click",function(){
	    //判断是否是杭州银行返回
	    var sign = "";
	    if(sign!=""){
	        goBackPageFunction();
	    }else{
			history.go(-1);
		}
	})
	
	//杭州银行sign置入
	//hzbank_sign();
	//杭州银行加密串
	var sign_val = "";
    var name = "wt_store_cfg22193619070";
    if(sign_val!=""){
        if(name.indexOf("hzbank")>=0 && name.indexOf("hzbank1698595698")<0 ){
	        $('#hasNumber5').val(sign_val);
			$('#hasNumberType5').val("HAVE_0_杭州银行合约机_主");
			//隐藏导航
			 $("#footershow").hide();
	         $("#minihome").show();
	         sessionStorage.setItem("minihome","minihome"); 
			 sessionStorage.setItem("hzbank","yes");
			 sessionStorage.setItem("sign",sign_val);
		 }
		 else if(name.indexOf("hzbank1698595698")>=0){ 
	    	sessionStorage.setItem("sign",sign_val);
	    	$('#hasNumber5').val(sign_val);
			$('#hasNumberType5').val("HAVE_0_杭州银行合约机解冻_主");
			sessionStorage.setItem("hzbanktuifei","yes");
    	}	  		  
    }
    else{
        sessionStorage.setItem("hzbank","no");
        sessionStorage.setItem("hzbanktuifei","no");
    }
    
    
    
	
});

