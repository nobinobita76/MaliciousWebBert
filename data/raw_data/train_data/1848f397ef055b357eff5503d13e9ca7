var loading = {
	init:function(){
		var ht_body=$("body").height();
		$("#loading-mask").height(ht_body);
	},
    show:function(){
    	$('#loading-mask').remove();
    	var html = '<div id="loading"><div class="loading-container"><img src=\"http://zj.189.cn/img/ct10000v5/zjprWap/activities/201506_kdhd/images/loading.gif\" /></div></div><div id="loading-mask"></div>';
		$('body').append(html);
		loading.init();
		$('#loading-mask').show();
    },
    hide:function(id){
		$('#loading,#loading-mask').remove();
    }
}

//弹窗
var mask = {
  show:function(id){
		$("#mask").remove();
		$("body").append("<div id='mask'></div>");
		var maskHeight=$("body").height();
		if (maskHeight<document.documentElement.clientHeight) {
			maskHeight = document.documentElement.clientHeight;
		}
		$("#mask").height(maskHeight);
		setTimeout(function(){
			$("#mask").show();
			if(id!=null) {
				$("#"+id).show();
				moveit(id);
			}
		},100);
		
  },
  hide:function(id){
		$("#mask").remove();
		$("#"+id).hide();
  }
}

//调整弹框屏幕居中位置
function moveit(id) {
	var marginleft = -($("#"+id).width()/2);
	var marginheight = -($("#"+id).height()/2);
	var top = ($(window).height())/2 + $(window).scrollTop();
	var left = ($(window).width())/2 + $(window).scrollLeft();
	$("#"+id).css({
		'margin-left': marginleft,
		'margin-top': marginheight,
		'top': top,
		'left': left
	});
}

