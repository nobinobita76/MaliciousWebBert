<script>
    
function init(){
    var needAddFuncList = $('li a[func][func!=none]');
    for(var i = 0; i < needAddFuncList.size(); i++){
        var tr_a = needAddFuncList[i];
        var funcList = $(tr_a).attr('func').split(';');
        
        for(var j = 0; j < funcList.length; j++){
            try{
                /*对 "choose('1',2)" 这种类似的JS函数执行字符串进行解析*/
                var funcStr = funcList[j];
                if(typeof eval(funcStr.substring(0, funcStr.indexOf('('))) != 'function'){
                    continue;
                }
                (function(funcStr){
                    var _func = eval(funcStr.substring(0, funcStr.indexOf('(')));
                
                    if(_func == shut_down){
                        _func();
                    }
                    var paramsStr = funcStr.substring(funcStr.indexOf('(')+1, funcStr.indexOf(')'));
                    var paramsArr = paramsStr.split(',');
                    var params = "{}";
                    
                    if($.trim(paramsStr)!=""){
                        params = "{";
                        for(var k = 0; k < paramsArr.length; k++){
                            params += "param" + k + ":" + paramsArr[k] + ",";
                        }
                        params = params.substr(0, params.length-1) + "}";
                    }

                    $(tr_a).bind("click", eval("("+params+")"), function(event){_func(event,this)});
                })(funcStr)

            } catch (e){
//                console.log(e)
            }

        }
    }
}

var totalprice = [0,0,0,0,0,0,0,0];
phonetype = function(thisobj){
    $(thisobj).parents('ul').find('a').removeClass("curr");
    $(thisobj).addClass("curr");

    $('.commodityDesc div').filter(function(){
        return $(this).attr('level') > $(thisobj).parents('div').attr('level')
    }).find('#taocanPay').remove();
}
function phoneonklick(thisobj,nextViewTypes,field_code,i,price,levels,unit){
    if("dxkk"==unit)
    {
        if($(thisobj).hasClass('curr'))
        {
            totalprice[i] = totalprice[i] - price;
            $(thisobj).removeClass("curr");
            hideNextLevel(i,levels);
            showPrice(i,totalprice[i],field_code);
        }else{
            totalprice[i] = totalprice[i] + price;
            $(thisobj).addClass("curr");
            showNextLevel(nextViewTypes,"'"+ field_code +"'",i,levels,this);
            showPrice(i,totalprice[i],field_code);
        }
    }else{
        $('#_price_desc').empty();
        prize();
        if($(thisobj).hasClass('curr'))
        {return};
        phonetype(thisobj);
        showNextLevel(nextViewTypes,"'"+ field_code +"'",i,levels,this);
        if(field_code.indexOf("_oldk")>-1||field_code.indexOf("_newk")>-1){
           
        }else{
            showPrice(i,price,field_code);
        }
       
    }
	
	hideViewType();
}

function hideNextLevel(currLv,levels){
    for(var i = currLv + 1; i <= levels; i++){
        $('div[level='+i+']').hide();
        $('div[level='+i+'] a').removeClass('curr');
        totalprice[i]=0;
    }

    //
    if(currLv < levels){
        $(":input[name^='terminalinfo']:not([name^='terminalinfo.phone'])").val('');
    }
}

function showNextLevel(arr, code, currLv,levels,_this, not_to_show_selector){
    for(var i = currLv + 1; i <= levels; i++){
        $('div[level='+i+']').hide();
        $('div[level='+i+'] a').removeClass('curr');
        totalprice[i]=0;
    }

    //
    if(currLv < levels){ 
        //2016.2.26新增开始
        var hasNumber5_baoliu = $('#hasNumber5').val();
        var hasNumberType5_baoliu = $('#hasNumberType5').val();
        //2016.2.26新增开始
        $(":input[name^='terminalinfo']:not([name^='terminalinfo.phone'])").val('');
        //杭州银行存放sign值到hasNumber5和hasNumberType5，这边重新选择套餐后信息被重置，现添加一步判断操作，保留杭州银行sign的值
        //2016.2.26新增开始
        var name = "wt_store_cfg22193619070";
        if(name.indexOf("hzbank")>=0){
          //杭州银行将保留的值回填
          $('#hasNumber5').val(hasNumber5_baoliu);
          $('#hasNumberType5').val(hasNumberType5_baoliu);
        }
        //2016.2.26新增结束
    }

    if(typeof arr == 'string'){
        arr = [arr];
    }
   
    for(var i = 0; i < arr.length; i++){
        var id = arr[i] + 'Div';
        $('#'+id).show();
        $('#'+id+' li').find('a:not('+not_to_show_selector+')').hide().filter('[rel='+code+']').show();
        
        if($('#'+id+' li').find('a:not('+not_to_show_selector+')').filter('[rel='+code+']').length == 0){
            $('#'+id).hide();
        }
        
        //券类只显示输入框
        if(id.indexOf("券")!=-1||id.indexOf("编号")!=-1){
        	$('#'+id +' ul').hide();
        	$('#'+id +' li a').filter('[rel='+code+']').click();
        }
        else if($('#'+id).text().indexOf("号码随机")!=-1){
        	$('#'+id +' ul').hide();
        	$('#'+id +' li a').filter('[rel='+code+']').click();
        }
    }
    $('.choiceType li').show().filter(function(){return $(this).find('a').css('display')=='none'}).hide();

}


var price = [];
function showPrice(level, curr_level_price,field_code){
    price[level] = curr_level_price;
    var finally_price = 0;
    for(var i = 0; i <= level; i++){
        finally_price += price[i];
    }
        finally_price += parseFloat('0');
    $('#shopPrice').html(finally_price.toFixed(2)+'元');
    
    //4g新增1029
    $("#totalmoneylose").val(finally_price);
    var taocanzhuti = $("#taocanzhuti").val();
    if(finally_price==0){
		if(taocanzhuti=="wap59tc"){
		    $('#shopPrice').html(parseInt("50").toFixed(2)+'元');
		    $("#totalmoneylose").val("50");
		}else if(taocanzhuti=="wap79tc"||taocanzhuti=="wap99tc"){
		    $('#shopPrice').html(parseInt("100").toFixed(2)+'元');
		    $("#totalmoneylose").val("100");
		}
	}
	//4g新增1029
    
    
    
}


function getInventory(event,_this){
return;
    $.ajax({
        type : 'POST',
        data : {id: $(_this).attr('currId'), name:'wt_store_cfg22193619070'},
        url : '/market/sale/getInventory.whtml',
        success : function(rst){
        	var data = rst.data;
            if(!isNaN(parseInt(data))){
                if(parseInt(data) <=0){
                    shut_down();
                }else{
                    $('#orangeBuyLay').unbind('click').removeClass('grayBtn')
                }
                $(_this).siblings(':not(a[func])').remove();
            }
        }
    })
}

function showInfomation(event,_this){
    if(window.infoFlag){
        return;
    }
    $('.commodityDesc:eq(0) div:eq(0)')
    
    
    
    

    window.infoFlag = true;
}

function show_desc(event,_this){
    var desc = $(event.data.param0);
    
    if(event.data.param1){
        $('#_price_desc').empty().append(desc);
    }else{
        $(_this).siblings(':not(a[func])').remove();
        if($('#taocanPay').size() > 0){
            $('#taocanPay').remove();
        }
        $(_this).parents('ul').after(desc);
    }
	
}

function shut_down(){
    $('#orangeBuyLay').bind('click',function(e){
        e.preventDefault();
    }).addClass('orangeBuyBtn');
}
// have_lipin(4,'HAVE_LIPIN_LANYA')
var _not_to_clear_index = -1;
function have_lipin(event,_this){
    var index = event.data.param0;
    _not_to_clear_index = index;
    var value = event.data.param1;
    $(":input[name='terminalinfo.phone_"+index+"']").val(value);
}
function have_number_studentid(event,_this){
	var _this_txt = $(_this).text();
	var $insertObj = $(_this);
	$insertObj = $(_this).parent().parent();
    var inputTxt = $('#hasNumberInput'+event.data.param0);
    var studentid = "";
    if(document.URL.indexOf('studentid=')!=-1){
    	var studentid = document.URL.substring(document.URL.indexOf('studentid=')+('studentid=').length);
    }
    if(inputTxt.length > 0){
        inputTxt.remove();
        if(_this_txt.indexOf("随机赠送")==-1){
        	$("<input class=\"inputIpt perW90\" id=\"hasNumberInput"+event.data.param0+"\" onblur=\"if(''==$.trim($(this).val())){$('#hasNumber"+event.data.param0+"').val('');$('#hasNumberType"+event.data.param0+"').val(''); }else{$('#hasNumber"+event.data.param0+"').val($(this).val());$('#hasNumberType"+event.data.param0+"').val('"+event.data.param1+"');}\" style=\"display:inline-block;\" type=\"text\" placeholder='"+_this_txt+"' value=\""+studentid+"\" /> ").insertAfter($insertObj)
        }else{
        	$("<font style='color:red;font-size:14px;'>"+_this_txt+"</font><input style='display:none' class=\"inputIpt perW90\" id=\"hasNumberInput"+event.data.param0+"\" onblur=\"if(''==$.trim($(this).val())){$('#hasNumber"+event.data.param0+"').val('');$('#hasNumberType"+event.data.param0+"').val(''); }else{$('#hasNumber"+event.data.param0+"').val($(this).val());$('#hasNumberType"+event.data.param0+"').val('"+event.data.param1+"');}\" style=\"display:inline-block;\" type=\"text\" placeholder='"+_this_txt+"' value=\""+studentid+"\" /> ").insertAfter($insertObj)
        }
    }else{
    	if(_this_txt.indexOf("随机赠送")==-1){
        	$("<font style='color:red;font-size:14px;'>"+_this_txt+"</font><input style='display:inline-block;' class=\"inputIpt perW90\" id=\"hasNumberInput"+event.data.param0+"\" onblur=\"if(''==$.trim($(this).val())){$('#hasNumber"+event.data.param0+"').val('');$('#hasNumberType"+event.data.param0+"').val(''); }else{$('#hasNumber"+event.data.param0+"').val($(this).val());$('#hasNumberType"+event.data.param0+"').val('"+event.data.param1+"');}\" style=\"display:inline-block;\" type=\"text\" placeholder='"+_this_txt+"' value=\""+studentid+"\" /> ").insertAfter($insertObj)
        }else{
        	$("<input class=\"inputIpt perW90\" id=\"hasNumberInput"+event.data.param0+"\" onblur=\"if(''==$.trim($(this).val())){$('#hasNumber"+event.data.param0+"').val('');$('#hasNumberType"+event.data.param0+"').val(''); }else{$('#hasNumber"+event.data.param0+"').val($(this).val());$('#hasNumberType"+event.data.param0+"').val('"+event.data.param1+"');}\" style=\"display:inline-block;\" type=\"text\" placeholder='"+_this_txt+"' value=\""+studentid+"\" /> ").insertAfter($insertObj)
        }
    }
	if(studentid!=""){
		$('#hasNumber'+event.data.param0).val(studentid);
		$('#hasNumberType'+event.data.param0).val(event.data.param1);
	}
	$(inputTxt).click();
    $(_this).attr('notPhone','input');
}

//  have_number(0)  主副卡标记(0,1,2...)
function have_number(event,_this){
    var inputTxt = $('#hasNumberInput'+event.data.param0);
    if(inputTxt.length > 0){
        inputTxt.remove();
        $("<input class=\"inputIpt perW90\" id=\"hasNumberInput"+event.data.param0+"\" onblur=\"if(''==$.trim($(this).val())){$('#hasNumber"+event.data.param0+"').val('');$('#hasNumberType"+event.data.param0+"').val(''); }else{$('#hasNumber"+event.data.param0+"').val($(this).val());$('#hasNumberType"+event.data.param0+"').val('"+event.data.param1+"');}\"  style=\"margin-left:5px;display:inline-block;border:1px solid #cad5e3;\" type=\"text\" /> ").insertAfter($(_this))
    }else{
        $("<input class=\"inputIpt perW90\" id=\"hasNumberInput"+event.data.param0+"\" onblur=\"$('#hasNumber"+event.data.param0+"').val($(this).val());$('#hasNumberType"+event.data.param0+"').val('"+event.data.param1+"');\"  style=\"margin-left:5px;display:inline-block;border:1px solid #cad5e3;\" type=\"text\" /> ").insertAfter($(_this))
    }

    $(_this).attr('notPhone','input');
    $('#hasNumber'+event.data.param0).val(inputTxt.val() ? inputTxt.val() : '');
    $('#hasNumberType'+event.data.param0).val(event.data.param1)

    $('ul.choiceType a').filter(function(){return /^\d{11}$/.test($(this).html()) && !$(this).hasClass('curr')}).html('选择新号码');
}

//  have_number_tmp(0)  主副卡标记(0,1,2...)
function have_number_tmp(event,_this){
	var _this_txt = $(_this).text();
	var $insertObj = $(_this);
	$insertObj = $(_this).parent().parent();
    var inputTxt = $('#hasNumberInput'+event.data.param0);
    if(inputTxt.length > 0){
        inputTxt.remove();
        $("<input class=\"inputIpt perW90\" id=\"hasNumberInput"+event.data.param0+"\" onblur=\"if(''==$.trim($(this).val())){$('#hasNumber"+event.data.param0+"').val('');$('#hasNumberType"+event.data.param0+"').val(''); }else{$('#hasNumber"+event.data.param0+"').val($(this).val());$('#hasNumberType"+event.data.param0+"').val('"+event.data.param1+"');}\" type=\"text\" placeholder='"+_this_txt+"'/> ").insertAfter($insertObj)
    }else{
        $("<input class=\"inputIpt perW90\" id=\"hasNumberInput"+event.data.param0+"\" onblur=\"if(''==$.trim($(this).val())){$('#hasNumber"+event.data.param0+"').val('');$('#hasNumberType"+event.data.param0+"').val(''); }else{$('#hasNumber"+event.data.param0+"').val($(this).val());$('#hasNumberType"+event.data.param0+"').val('"+event.data.param1+"');}\" type=\"text\" placeholder='"+_this_txt+"'/> ").insertAfter($insertObj)
    }
	$(inputTxt).click();
}

// 地市选择
function have_number_city(event,_this){
	var _this_txt = $(_this).text();
	var $insertObj = $(_this);
	if($("#phone_3").val()!="")
	{return};
	//$insertObj.hide();
	$insertObj = $(_this).parent().parent();
    var inputTxt = $('#hasNumberInput'+event.data.param0);
    $insertObj = $("#city_show"); 
    if(inputTxt.length > 0){
        inputTxt.remove();
        
        $("<select id=\"area\" name=\"area\" onchange=\"getC3C4()\" style=\"width:80px;height:30px;\">").insertAfter($insertObj);
        $("<label>地区选择:</label><select id=\"SelectCity\"  onchange=\"goCheck(this.value)\" style=\"width:80px;height:30px;\"><option lvl=\"hz\" value=\"杭州市\">杭州市</option><option lvl=\"hu\" value=\"湖州市\">湖州市</option><option lvl=\"jx\" value=\"嘉兴市\">嘉兴市</option><option lvl=\"nb\" value=\"宁波市\">宁波市</option><option lvl=\"sx\" value=\"绍兴市\">绍兴市</option><option lvl=\"tz\" value=\"台州市\">台州市</option><option lvl=\"wz\" value=\"温州市\">温州市</option><option lvl=\"ls\" value=\"丽水市\">丽水市</option><option lvl=\"jh\" value=\"金华市\">金华市</option><option lvl=\"zs\" value=\"舟山市\">舟山市</option><option lvl=\"qz\" value=\"衢州市\">衢州市</option></select>").insertAfter($insertObj);
    }else{
    	$("<select id=\"area\" name=\"area\" onchange=\"getC3C4()\" style=\"width:80px;height:30px;\">").insertAfter($insertObj);
    	$("<label>地区选择:</label><select id=\"SelectCity\"  onchange=\"goCheck(this.value)\" style=\"width:80px;height:30px;\"><option lvl=\"hz\" value=\"杭州市\">杭州市</option><option lvl=\"hu\" value=\"湖州市\">湖州市</option><option lvl=\"jx\" value=\"嘉兴市\">嘉兴市</option><option lvl=\"nb\" value=\"宁波市\">宁波市</option><option lvl=\"sx\" value=\"绍兴市\">绍兴市</option><option lvl=\"tz\" value=\"台州市\">台州市</option><option lvl=\"wz\" value=\"温州市\">温州市</option><option lvl=\"ls\" value=\"丽水市\">丽水市</option><option lvl=\"jh\" value=\"金华市\">金华市</option><option lvl=\"zs\" value=\"舟山市\">舟山市</option><option lvl=\"qz\" value=\"衢州市\">衢州市</option></select>").insertAfter($insertObj);
    }
    $.ajax({
		url: '/market/sale/getC3C4Name.whtml',
		type: 'POST',
        success:function(data){
        	var result = eval("(" + data + ")");
            result_code = result.result_code;
            c3 = result.area_name;
            c4 = result.city_name;
            if(result_code=="1"){
            	$("#SelectCity").val(c3);
            	goCheck(c3,c4);
            	$("#area").val(c4);
            	$("#c3").val(c3);
            	$("#c4").val(c4);
            	var phone_3=$("#gift_goods_code").val()+"_city_"+$("#SelectCity").find("option:selected").attr("lvl");
            	var phone_4=$("#gift_goods_code").val()+"_city_"+$("#SelectCity").find("option:selected").attr("lvl")+"_"+$("#area").find("option:selected").attr("lvl");
            	$("#phone_3").val(phone_3);	
       			$("#phone_4").val(phone_4);
            }
        }
    });
	$(inputTxt).click();
    $(_this).attr('notPhone','input');
}
	//获取c3，c4
	function getC3C4(){
		$("#c3").val($("#SelectCity").val());
        $("#c4").val($("#area").val());
        var goods_code = $("#gift_goods_code").val();    
        var phone_3=goods_code+"_city_"+$("#SelectCity").find("option:selected").attr("lvl");
        var phone_4=goods_code+"_city_"+$("#SelectCity").find("option:selected").attr("lvl")+"_"+$("#area").find("option:selected").attr("lvl");
        $("#phone_3").val(phone_3);	
        $("#phone_4").val(phone_4);
       	//更新价格
        prize();
	}
	//自动订单价格单独跟新
	function prize(){
	 	var goods_code = $("#gift_goods_code").val();    
        var phone_3=goods_code+"_city_"+$("#SelectCity").find("option:selected").attr("lvl");
        var phone_4=goods_code+"_city_"+$("#SelectCity").find("option:selected").attr("lvl")+"_"+$("#area").find("option:selected").attr("lvl");
       	var year = $("#heyueText2").html();
       	var str_year = year.substring(year.length-3,year.length-1);
	 	if(typeof($("#"+phone_4+"_"+str_year).val())!="undefined"){
	   	    $("#shopPrice").html(parseInt($("#"+phone_4+"_"+str_year).val())+parseInt($("#totalmoneylose").val())+".00元");
	   	    return;
       	}else if(typeof($("#"+phone_3+"_"+str_year).val())!="undefined"){
       		$("#shopPrice").html(parseInt($("#"+phone_3+"_"+str_year).val())+parseInt($("#totalmoneylose").val())+".00元");
	       	return;
       	}else{
       	   	$("#shopPrice").html($("#totalmoneylose").val()+".00元");
	       	return;
       	}
	}
//获取地区方法
	function goCheck(e,c4){
		if(e == "none"){
			var selOpt = $(".country");
			selOpt.remove();
			return;
		}
		var selOpt_ = $(".country");
		selOpt_.remove();
		if(e=='杭州市'){
			var arr = ['杭州','萧山','余杭','富阳','临安','桐庐','建德','淳安'];
			var lvl = ['hz','xs','yh','fy','la','tl','jd','ca'];
			var selObj = $("#area");
			for(var i=0;i<arr.length;i++){
					selObj.append("<option class='country' lvl='"+lvl[i]+"' value='"+arr[i]+"'>"+arr[i]+"</option>");
					
			}
		}else if(e=='金华市'){
			var arr = ['金华','兰溪','义乌','浦江','永康','磐安','东阳','武义'];
			var lvl = ['jh','lx','yw','pj','yk','pa','dy','wy'];
			var selObj = $("#area");
			for(var i=0;i<arr.length;i++){
					selObj.append("<option class='country' lvl='"+lvl[i]+"' value='"+arr[i]+"'>"+arr[i]+"</option>");
			}
		}else if(e=='宁波市'){
			var arr = ['宁波','镇海','北仑','慈溪','余姚','奉化','宁海','象山','鄞州'];
			var lvl = ['nb','zh','bl','cx','yy','fh','lh','xs','qz'];
			var selObj = $("#area");
			for(var i=0;i<arr.length;i++){
					selObj.append("<option class='country' lvl='"+lvl[i]+"' value='"+arr[i]+"'>"+arr[i]+"</option>");
			}
		}else if(e=='台州市'){
			var arr = ['椒江','黄岩','路桥','温岭','临海','天台','玉环','仙居','三门'];
			var lvl = ['jj','hy','lq','wl','lh','tt','yh','xj','sm'];
			var selObj = $("#area");
			for(var i=0;i<arr.length;i++){
					selObj.append("<option class='country' lvl='"+lvl[i]+"' value='"+arr[i]+"'>"+arr[i]+"</option>");
			}
		}else if(e=='嘉兴市'){
			var arr = ['嘉兴','嘉善','平湖','海盐','海宁','桐乡'];
			var lvl = ['jx','js','ph','hy','hn','tx'];
			var selObj = $("#area");
			for(var i=0;i<arr.length;i++){
					selObj.append("<option class='country' lvl='"+lvl[i]+"' value='"+arr[i]+"'>"+arr[i]+"</option>");
			}
		}else if(e=='湖州市'){
			var arr = ['湖州','德清','长兴','安吉'];
			var lvl = ['hu','dq','cx','aj'];
			var selObj = $("#area");
			for(var i=0;i<arr.length;i++){
					selObj.append("<option class='country' lvl='"+lvl[i]+"' value='"+arr[i]+"'>"+arr[i]+"</option>");
			}
		}else if(e=='衢州市'){
			var arr = ['衢州','江山','常山','开化','龙游'];
			var lvl = ['qz','js','cs','kh','ly'];
			var selObj = $("#area");
			for(var i=0;i<arr.length;i++){
					selObj.append("<option class='country' lvl='"+lvl[i]+"' value='"+arr[i]+"'>"+arr[i]+"</option>");
					
			}
		}else if(e=='温州市'){
			var arr = ['瑞安','平阳','苍南','永嘉','乐清','洞头','文成','泰顺','温州'];
			var lvl = ['ra','py','cn','yj','lq','dt','wc','ts','wz'];
			var selObj = $("#area");
			for(var i=0;i<arr.length;i++){
					selObj.append("<option class='country' lvl='"+lvl[i]+"' value='"+arr[i]+"'>"+arr[i]+"</option>");
			}
		}else if(e=='绍兴市'){
			var arr = ['绍兴','诸暨','上虞','嵊州','新昌'];
			var lvl = ['sx','zj','sy','sz','xc'];
			var selObj = $("#area");
			for(var i=0;i<arr.length;i++){
					selObj.append("<option class='country' lvl='"+lvl[i]+"' value='"+arr[i]+"'>"+arr[i]+"</option>");
			}
		}else if(e=='舟山市'){
			var arr = ['舟山','普陀','岱山','嵊泗'];
			var lvl = ['zs','pt','ds','ss'];
			var selObj = $("#area");
			for(var i=0;i<arr.length;i++){
					selObj.append("<option class='country' lvl='"+lvl[i]+"' value='"+arr[i]+"'>"+arr[i]+"</option>");
			}
		}else if(e=='丽水市'){
			var arr = ['丽水','缙云','青田','龙泉','庆元','景宁','遂昌','云和','松阳'];
			var lvl = ['ls','jy','qt','lq','qy','jn','sc','yh','sy'];
			var selObj = $("#area");
			for(var i=0;i<arr.length;i++){
					selObj.append("<option class='country' lvl='"+lvl[i]+"' value='"+arr[i]+"'>"+arr[i]+"</option>");
			}
		}	
		 getC3C4();
	}

function limitArea(event){
	var value = event.data.param0;
    window.areaLimit = value;
}


//一旦触发这个函数，在参数里出现的地区显示，不出现的选号地区消失。
function limitXuanhaoArea(event){
	var areavalue = event.data.param0;
    if (typeof(areavalue) != "undefined" && areavalue != "") {
        for(var i=0;i<=10;i++){
          //570到580循环
		  var area = '';
		  if(i==10){
		    area = '580';
		  }else{
		    area = '57'+i;
		  }
		  if (areavalue.indexOf(area) <= '-1'){
           $("#"+area+"areacurr").parent('li').remove();
		  }
		}	
	}
}


function setNbrChooseDetail(MonthConsume,NbrRull){
    $('#MonthConsume').val(MonthConsume);
    $('#NbrRull').val(NbrRull);
}
//chooseNumber(189,'ALL',0,'select')    保底消费，选号方式，主副卡标记(0,1,2...),选号或填好标记
function chooseNumber(event,_this){
	setNbrChooseDetail(event.data.param0,event.data.param1);
    if(!/^\d{11}$/.test($(_this).html())){
        $(_this).removeClass('curr');
    }
    $('#hidCardXuanhaoflag').val(event.data.param2);
    
    var areaflag = $('#areaflag').val();
    if(areaflag == "defined"&&event.data.param2!='0'){
          
    }
   
    if(event.data.param2 =='0'&& areaflag !='defined'){
     
      var obj = document.getElementById("areaSelect");
      if(areaflag !='haoka'){
            obj.options.length=0;
            obj.options.add(new Option("杭州","571"));
            obj.options.add(new Option("湖州","572"));
            obj.options.add(new Option("嘉兴","573")); 
            obj.options.add(new Option("宁波","574"));
            obj.options.add(new Option("绍兴","575"));
            obj.options.add(new Option("台州","576"));
            obj.options.add(new Option("温州","577"));
            obj.options.add(new Option("丽水","578"));
            obj.options.add(new Option("金华","579"));
            obj.options.add(new Option("舟山","580"));
            obj.options.add(new Option("衢州","570"));
      }       
    }
    
    if(event.data.param2 !='0'){
       if(areaflag !='defined'){ 
        var obj = document.getElementById("areaSelect");
        if(areaflag=="571"){
            obj.options.length=0;  
            obj.options.add(new Option("杭州","571"));     
        }
        if(areaflag=="572"){
            obj.options.length=0;   
            obj.options.add(new Option("湖州","572"));     
        }
        if(areaflag=="573"){
            obj.options.length=0;   
            obj.options.add(new Option("嘉兴","573"));     
        }
		if(areaflag=="574"){ 
		    obj.options.length=0;  
            obj.options.add(new Option("宁波","574"));     
        }
        if(areaflag=="575"){ 
            obj.options.length=0;  
            obj.options.add(new Option("绍兴","575"));     
        }  
		if(areaflag=="576"){
		    obj.options.length=0;   
            obj.options.add(new Option("台州","576"));     
        }  
		if(areaflag=="577"){  
		    obj.options.length=0; 
            obj.options.add(new Option("温州","577"));     
        }  
		if(areaflag=="578"){
		    obj.options.length=0;   
            obj.options.add(new Option("丽水","578"));     
        }  
		if(areaflag=="579"){
		    obj.options.length=0;   
            obj.options.add(new Option("金华","579"));     
        }  
		if(areaflag=="580"){
		    obj.options.length=0;   
            obj.options.add(new Option("舟山","580"));     
        }   
		if(areaflag=="570"){
		    obj.options.length=0;   
            obj.options.add(new Option("衢州","570"));     
        }
      }else{
         if(areaflag!='haoka'){
           //alert('请先选择主号');
           }
        
        }  		 
    }
    
    $('#hasNumberInput'+event.data.param2).hide();

    $('#hasNumberType'+event.data.param2).val(event.data.param3);
}
function castArea(id){
		if("571"==id){return "杭州";}
		else if("572"==id){return "湖州";}
		else if("573"==id){return "嘉兴";}
		else if("574"==id){return "宁波";}
		else if("575"==id){return "绍兴";}
		else if("576"==id){return "台州";}
		else if("577"==id){return "温州";}
		else if("578"==id){return "丽水";}
		else if("579"==id){return "金华";}
		else if("580"==id){return "舟山";}
		else if("570"==id){return "衢州";}
		else{return "杭州";}
}
var chooseNumber_randomflag= false;
function chooseNumber_random(event,_this){
	if(chooseNumber_randomflag)return;
	chooseNumber_randomflag = true;
	setNbrChooseDetail(event.data.param0,event.data.param1);
    if(!/^\d{11}$/.test($(_this).html())){
        $(_this).removeClass('curr');
    }
    $('#hidCardXuanhaoflag').val(event.data.param2);
    
    var areaflag = $('#areaflag').val();
    if(areaflag == "defined"&&event.data.param2!='0'){
          
    }
   
    if(event.data.param2 =='0'&& areaflag !='defined'){
     
      var obj = document.getElementById("areaSelect");
      if(areaflag !='haoka'){
            obj.options.length=0;
            obj.options.add(new Option("杭州","571"));
            obj.options.add(new Option("湖州","572"));
            obj.options.add(new Option("嘉兴","573")); 
            obj.options.add(new Option("宁波","574"));
            obj.options.add(new Option("绍兴","575"));
            obj.options.add(new Option("台州","576"));
            obj.options.add(new Option("温州","577"));
            obj.options.add(new Option("丽水","578"));
            obj.options.add(new Option("金华","579"));
            obj.options.add(new Option("舟山","580"));
            obj.options.add(new Option("衢州","570"));
      }       
    }
    
    if(event.data.param2 !='0'){
       if(areaflag !='defined'){ 
        var obj = document.getElementById("areaSelect");
        if(areaflag=="571"){
            obj.options.length=0;  
            obj.options.add(new Option("杭州","571"));     
        }
        if(areaflag=="572"){
            obj.options.length=0;   
            obj.options.add(new Option("湖州","572"));     
        }
        if(areaflag=="573"){
            obj.options.length=0;   
            obj.options.add(new Option("嘉兴","573"));     
        }
		if(areaflag=="574"){ 
		    obj.options.length=0;  
            obj.options.add(new Option("宁波","574"));     
        }
        if(areaflag=="575"){ 
            obj.options.length=0;  
            obj.options.add(new Option("绍兴","575"));     
        }  
		if(areaflag=="576"){
		    obj.options.length=0;   
            obj.options.add(new Option("台州","576"));     
        }  
		if(areaflag=="577"){  
		    obj.options.length=0; 
            obj.options.add(new Option("温州","577"));     
        }  
		if(areaflag=="578"){
		    obj.options.length=0;   
            obj.options.add(new Option("丽水","578"));     
        }  
		if(areaflag=="579"){
		    obj.options.length=0;   
            obj.options.add(new Option("金华","579"));     
        }  
		if(areaflag=="580"){
		    obj.options.length=0;   
            obj.options.add(new Option("舟山","580"));     
        }   
		if(areaflag=="570"){
		    obj.options.length=0;   
            obj.options.add(new Option("衢州","570"));     
        }
      }else{
         if(areaflag!='haoka'){
           //alert('请先选择主号');
           }
        
        }  		 
    }
    
    $('#hasNumberInput'+event.data.param2).hide();

    $('#hasNumberType'+event.data.param2).val(event.data.param3);
    
    $.ajax({
		url: '/market/xuanhao/getareaid.whtml',
		type: 'POST',
		dataType: 'json',
		data: {},
        success:function(data){//请求成功
        	$("input[name='xuanhaoQuery.areaid']").val(data);
        	$("#hiddenarea").text("归属地："+castArea(data));
        	$("#hiddenarea").attr('areaValue',data);
            if(data!=""){
              $("#"+data+"areacurr").trigger("click");       
		    }else{
		      $("#"+"571"+"areacurr").trigger("click");
			}
            loadNumberRandom();
        },
        error:function(){
           
        }
    });   
    
    
    
}

