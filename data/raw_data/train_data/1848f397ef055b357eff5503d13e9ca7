function getparastr(strname){
           var pos,parastr,para,tempstr;
		   var htmlHref = window.location.href;
		   pos = htmlHref.indexOf("?");
		   parastr = htmlHref.substring(pos+1); 
           para = parastr.split("&"); 
           tempstr=""; 
           for(i=0;i<para.length;i++){  
           	 tempstr = para[i];  
           	 pos = tempstr.indexOf("=");  
           	 if(tempstr.substring(0,pos) == strname){
           	    return tempstr.substring(pos+1);  
           	 } 
           }
           return null;
        }	
		function loadCoupon(){
			coupon = getparastr("coupon");
			if(coupon==null){
			}else{
				$("#hasNumberInput3").val(coupon).focus(); 
				$("#hasNumberInput2").val(coupon).focus(); ;
			}
		}
		var $$ = function(func){  
            if (document.addEventListener) {  
                window.addEventListener("load", func, false);  
            }  
            else if (document.attachEvent) {  
                window.attachEvent("onload", func);  
            }  
        }
         $$(function(){  
            loadCoupon();  
        }) 
        
        
        //我要吐糟功能开始

        function savetucao()
		{
			   	var toCaoText = encodeURIComponent($("#toCaoText").val());
			   	if(toCaoText=="")
			   	{
			   		alert('请输入吐槽内容');
			   		return;
			   	}
			   	$(".nav-con").find("input[type='button']").attr("disabled",true);
		        var strTuCaoUrl = "http://wapzj.189.cn/mall/wt_store_cfg22193619070/";
		        strTuCaoUrl += "?name="+"wt_store_cfg22193619070";
		        strTuCaoUrl += "&channel="+"";
		        strTuCaoUrl = encodeURIComponent(strTuCaoUrl);
		        strTuCaoUrl = strTuCaoUrl.replace("http","");
		        $.ajax({
		            url : "/market/sale/saveTuCao.whtml",
		            type : "POST",
		            dataType: 'json',
		            data : {"tuCaoText":toCaoText,"tuCaoUrl":strTuCaoUrl},
		            success : function(data){
		            	var result = data.result;
		            	if(result=="success")
		            	{
		            	 	  alert('您的吐槽已收，感谢您对浙江电信欢go网的支持!');                          
		            	}else
		            	{
		            		alert('吐糟失败!');
		            	}
		            	$(".nav-con").find("input[type='button']").attr("disabled",false);
		          }
		        });
		}

