 // 匹配电信号码
    function isCT (v) {
        var r = /1[3-9]+\d{9}/,
            re = new RegExp(r);
        if (v.length != 0) {
            if (v.search(re) != -1) {
                var s = v.substring(0, 3);
                return s == "133" || s == "153" || s == "189" || s == "180" || s == "181" || s == "177" || s == "171" || s == "173" ? true : false;
            } else {
                var s = v.substring(0, 5);
                return s == "10649" && v.length == 13 ? true : false;
            }
        } else {
            return false;
        }
    }


$(function(){
	
	$("#getPassword").click(function(){
        var field_value = $("#confirmTel").val();
        loading.show(); 
    	sendsmsVali(field_value);
	});


	$("#confirm_2").click(function(){
			var phone_code = $("#coinfirmPwd").val();
			var field_value = $("#confirmTel").val();
			if(phone_code==null || phone_code==""){
	            alert("验证码不能为空!");
	            return;
        	}
			
			$.ajax({
				url: '/market/kxlyh/checkyzm.whtml',
				type: 'POST',
				data:{"field_value":field_value,"phone_code":phone_code}, 
		        success:function(data){
		            if(data=="success"){
		            	mask.hide('userConfirm');
		            	alert('恭喜您获得300元老用户购机优惠，请尽快完成下单支付！');
		            	$("#coinfirmPwd").val("");
		            	loading.show();
		            	$('#subForm').submit();
		            }else if(data == "nberror"){
						alert('请输入正确的手机验证码。');
						$("#coinfirmPwd").val("");
					}
		        }
		    });
		})
})

function kxlyh(){
	var field_value = $("#hasNumber0").val();
	var field_code = $("#hasNumberType0").val();
	if(field_code=="HAVE_300_小卡"||field_code=="HAVE_300_小卡_主"){
		loading.show();
		$.ajax({
			url: '/market/kxlyh/getlyh.whtml',
			type: 'POST',
			data:{"field_value":field_value,"field_code":field_code}, 
	        success:function(data){
	        	loading.hide();
	            if(data=="success"){
	            	
	            	$("#confirmTel").val(field_value);
	            	mask.show('userConfirm');
	            }else if(data == "buy"){
					alert('您已享受过老用户优惠购机，您可以把本次活动信息分享给您身边的好友.');
					loading.show();
					$('#subForm').submit();
				}else if(data == "error"){
					alert('非常抱歉，系统出错，请稍后重试！');
				}else if(data == "Notinlist"){
					alert('您的手机号码暂不符合老用户购机条件，您可以原价购买该手机，点击“确定”填写订单详情！');
					loading.show();
					$('#subForm').submit();
				}else if(data == "exception"){
					alert('非常抱歉，系统繁忙，请稍后重试！');
				}
	        }
	    });
	}
	else if('wt_store_cfg22193619070'=='wt_store_cfg22211362103'||'wt_store_cfg22193619070'=='wt_store_cfg22199650752'){
		var areaId= $('#choosenumarea').val();
		var zhuhaoma  =$('#zhuhaoma').val();
		var goodsCode = $('#gift_goods_code').val();
		if(!isCT(zhuhaoma)){
			alert("请输入电信号码！");
			return ;
		}
		loading.show();
		$.ajax({
			url: '/market/kxlyh/getFkyx.whtml',
			type: 'POST',
			dataType:'json',
			data:{"field_value":field_value,"field_code":field_code,"areaId":areaId,"zhuhaoma":zhuhaoma,"goodsCode":goodsCode}, 
	        success:function(data){
	        	$('#masterNumber').val(zhuhaoma);
	        	$('#llfxkNumber').val(field_value);
	        	var alertMessage='';
	            var dqError = data.DQERROR;
	            var qfError = data.QFERROR;
	            var bmdError = data.BMDERROR;
	            var error = data.ERROR;
	            if(error!=0){
	            	alert(error);
	            	return ;
	            }
	            else{
	            	if(dqError==0){
	            		alertMessage+=('\r\n您好，选择号码与主卡号码所属地区不一致，请重新选择副卡号码或重新填写主卡号码。');
	            	}if(qfError==1){
	            		alertMessage+=('\r\n您好，您当前的主卡号码已欠费，缴清欠费后即可参加本次活动，点此前往充值（超链接：http://www.189.cn/chongzhi/）');
	            	}
	            	if(bmdError==0){
	            		alertMessage+=('\r\n您好，您当前的主卡号码暂不能参加本次活动。');
	            	}
	            	
	            }
	            if(alertMessage==''){
	            	loading.hide();
	            	$('#subForm').submit();
	            }
	            else{
	            	loading.hide();
	            	alert(alertMessage);
	            }
	        }
	    });
		
	}
	else{
		$('#subForm').submit();
	}
}

function sendsmsVali(field_value){
	$("#getPassword").attr("disabled",true);
	$.ajax({
	       type: 'POST',
	       datatype:'json',
	       url: '/market/kxlyh/valiSendSms.whtml',
	       data:{'field_value':field_value},
	       error: function() {
				$("#getPassword").attr("disabled",false);
	           return;
	       },
	       success: function(data) {
	       		loading.hide(); 
				$("#getPassword").attr("disabled",false);
		    	var result = data.resultFlg;
	            if(result=='SUCCESS'){
	            	$("#getPassword").spendBtn({
	        			grayBtnClass:"grayBtn",
	        			limitTime:"30",
	        			text:"重新发送",
	        			callBack:inputBtn
	        		});
	            }else{
	            	alert('发送失败');
	            }
	       }
	});
}

function inputBtn(){
	
}

;(function($) {
	$.fn.spendBtn = function(settings) {
		var defaults = {
			grayBtnClass:"grayBtn",
			limitTime:"60",
			text:"",
			callBack:""
		};
		// 以用户的自定义选项覆盖默认选项
		var options = $.extend(defaults,settings);
		return this.each(function() {
			var _self = $(this),tempTime;
			_self.addClass(options.grayBtnClass).val("重新发送"+options.limitTime).attr("disabled",true);
			setBtnGray();
			if(typeof options.callBack === 'function'){
				options.callBack();
			}
			function setBtnGray(){
				if(options.limitTime > 0){
					options.limitTime--;
					_self.val("重新发送"+options.limitTime)
					tempTime = setTimeout(function(){setBtnGray()}, 1000);
				}else {
					_self.removeClass(options.grayBtnClass).val('重新获取').attr("disabled",false);
					clearTimeout(tempTime);
				}	
			}
		});
	};
})(jQuery);


function showCountDown(){
    var seconds = Math.floor(DateUtil.remaining_time/1000);
    var fillInfo = [Math.floor(seconds / 60 / 60 /24),
        Math.floor(seconds % (60 * 60 * 24) / 60 / 60),
        Math.floor(seconds % (60 * 60) / 60),
        Math.floor(seconds % 60)];
    $('#saleStage li.current em').each(function(index){
        $(this).html(('00'+fillInfo[index]).substr((fillInfo[index]+'').length));
    })
    DateUtil.remaining_time = DateUtil.remaining_time - DateUtil.interval;
}
function refresh(){
    window.location.href = window.location.href;
}
</script><script>
var GOODS_TYPE = "";
var GOODS_TYPE_L_NAME = "仅购买手机";//仅购买手机字符
var HEYUE_NAME = "HEYUE";//"合约类型";
var TAOCAN_NAME = "TAOCAN";//"合约套餐";
var PHONE_ZHU = "主号";
var PHONE_FU = "副号";
var NUMBER_NEW_TEXT = "选择新号码";
var NUMBER_OLD_TEXT = "已有手机号码";
var NUMBER_NEW_OBJECT = null;//选择新号码控件
var NUMBER_OLD_OBJECT = null;//已有号码控件
var PACKAGE_ID = "package_";
var HEYUE_ARRAY = [];

var hideViewType = function(){
   	$("div[type="+HEYUE_NAME+"]").hide();
    $("div[type="+TAOCAN_NAME+"]").hide();
	//裸机
	if(GOODS_TYPE=="L"){
		$(".choiceLine a[data-rel='taocanContent']").hide().parent().prev().hide();
		$(".choiceLine a[data-rel='telNumberContent']").hide().parent().prev().hide();
	}
}
var initArray = function(){
	$("div[type="+HEYUE_NAME+"] li a").each(function(){
		var that_hy = $(this);
		var obj_hy = {};
		obj_hy.name = that_hy.text();
		obj_hy.id = that_hy.attr("currid");
		obj_hy.val = that_hy.attr("val");
		obj_hy.rel = that_hy.attr("rel");
		obj_hy.dxkk = that_hy.attr("dxkk");
		obj_hy.def = that_hy.hasClass("curr")?true:false;
		//obj_hy.func = that_hy.attr("func");
		var taocan = [];
		$("div[type="+TAOCAN_NAME+"] li a[rel='"+obj_hy.val+"']").each(function(){
			var that_tc = $(this);
			var obj_tc = {};
			obj_tc.name = that_tc.text();
			obj_tc.id = that_tc.attr("currid");
			obj_tc.val = that_tc.attr("val");
			obj_tc.rel = that_tc.attr("rel");
			obj_tc.dxkk = that_tc.attr("dxkk");
			obj_tc.def = that_tc.hasClass("curr")?true:false;
			//处理文本
			var data_text = that_tc.attr("func");
			data_text = data_text.replace("show_desc('","");
			data_text = data_text.replace("')","");
			var data_text_arr = data_text.split(";");
			data_text = data_text_arr[0];
			data_text = data_text.replace(/<br\s*\/?>/ig, ";"); 
			data_text = data_text.replace(/<[^>]*>/ig, "");
			//end
			if(data_text=="none"){
			  obj_tc.func ="";
			}else{
			obj_tc.func = data_text;
			}
			taocan.push(obj_tc);
		});
		obj_hy.taocan = taocan;
		HEYUE_ARRAY.push(obj_hy);
		//有前标签，重置
		if(obj_hy.rel!=''){
			$(".choiceType li a[val='"+obj_hy.rel+"']").bind(
				"click", 
				eval("({rel:'"+obj_hy.rel+"'})"), 
				function(event){refreshViewType(event,this)}
			);
		}
	});
	
	if(HEYUE_ARRAY.length==0){
		$(".choiceLine a[data-rel='taocanContent']").parent().hide().prev().hide();
	}else{
		//默认选择的合约套餐
		for(var i=0;i<HEYUE_ARRAY.length;i++){
			var obj = HEYUE_ARRAY[i];
			if(obj.def){
				getViewType(obj.rel);
				showPackage();
				var tc = obj.taocan;
				for(var j=0;j<tc.length;j++){
					if(tc[j].def){
						$("#"+PACKAGE_ID+obj.val+" a[data-val='"+tc[j].val+"']").click();
						$("#contractPackageSubmit").click();
						break;
					}
				}
				break;
			}
		}
	}
};
var refreshViewType = function(args){
	var  rel = args.data.rel;
	getViewType(rel);
};
var getViewType = function(rel){
	//重置按钮
	$("#heyueNum").text("");
	$("#heyueText").text("选择合约套餐").parent().removeClass("curr");
	$("#contractPackageSubmit").unbind();
	$("#contractPackageSubmit").addClass("grayBtn");
	//处理合约类型和合约套餐
    var contractPackageWarpObject = $(".contractPackageWarp");
   	contractPackageWarpObject.html("");
   	for(var i=0;i<HEYUE_ARRAY.length;i++){
   		var obj = HEYUE_ARRAY[i];
   		if(obj.rel==rel){
   			//仅购买手机和合约处理
   			//if(obj.name!=GOODS_TYPE_L_NAME){
	   			contractPackageWarpObject.append('<h3>'+obj.name+'</h3>');
	   			contractPackageWarpObject.append('<div class="grayBline"></div>');
	   			contractPackageWarpObject.append('<ul class="contractPackageWarpUL" id="'+PACKAGE_ID+obj.val+'"></ul>');
	   		//}
	   		//else if(GOODS_TYPE=="L"){
	   		//	$(".choiceType a[val='"+obj.val+"']").click();//默认点击
	   		//}
	   		var tc = obj.taocan;
	   		for(var j=0;j<tc.length;j++){
		   		var packageHtml='<li>';
		        packageHtml+='<div class="choiceLine">';
		        packageHtml+='<a href="javascript://" data-val="'+tc[j].val+'" data-parent="'+tc[j].rel+'" data-rel="'+tc[j].name+'" data-title="'+tc[j].name+'" data-text="'+tc[j].func+'"><i class="heyueIcon"></i><span id="heyueText" class="">'+tc[j].name+'</span><i class="rightIcon"></i></a>';
		        packageHtml+='</div>';
		        packageHtml+='<div class="grayBline"></div>';
		        packageHtml+='</li>';
		   		$("#"+PACKAGE_ID+tc[j].rel).append(packageHtml);
	   		}
   		}
   	}
};

//显示合约和套餐
var showPackage = function(){
	bodyHeight("taocanContent");
	contractPackage();
	
	if($("#choiceTaoCanCurrent").length>0){
		//跳转
		scrollBack("choiceTaoCanCurrent");
		return false;
	}
};
//获取是否包含选号
var hasNeedNumber = function(tc_rel,tc_txt){
	var xh_a = $(".choiceType li a[rel='"+tc_rel+"'][title='"+tc_txt+"']");
	var obj_xh = [];
	if(xh_a.length>0){
		for(var i=0;i<xh_a.length;i++){
			var that_xh = xh_a.eq(i);
			obj_xh.push(that_xh);
		}
	}
	return obj_xh
};
//选择号码提交
var selectedNumber = function(code_number,type){
	var _xh_index = $("#telNumberContent").attr("data-val");
	//检查是否有同号码存在
	var number_exisit = false;
	$("input[id^='hasNumber']").each(function(){
		if($(this).attr("id")!="hasNumber"+_xh_index && $(this).val()==code_number){//不为本身
			number_exisit = true;
			return;
		}
	});
	if(number_exisit){
		alert("该号码已被你选住，请换个号码！");
		return false;
	}
	if(type=="old"){
		if(!$.isEmptyObject($(NUMBER_OLD_OBJECT))){//老号码
		    window.tmpParam1=true;
			window.tmpParam0=true;
			$NUMBER_OLD_OBJECT = NUMBER_OLD_OBJECT[0];
			$NUMBER_OLD_OBJECT.click();
			$NUMBER_OLD_OBJECT.next("input").val(code_number);
			$("#"+$NUMBER_OLD_OBJECT.next("input").attr("id").replace("Input","")).val(code_number);
			
		}
	}else{
		if(!$.isEmptyObject($(NUMBER_NEW_OBJECT))){//新号码
			$(NUMBER_NEW_OBJECT[_xh_index]).click();
			$(NUMBER_NEW_OBJECT[_xh_index]).text(code_number).addClass('curr').attr("notPhone","Y");
		    //回填选择的号码
			var hidCardXuanhaoflag=$('#hidCardXuanhaoflag').val();
		    $('#hasNumber'+hidCardXuanhaoflag).val(code_number);
		}
	}
	//选择回填
	//$(".choiceLine a[data-rel='telNumberContent'][data-val='"+_xh_index+"'] span[id^='telPhoneNum']").text(code_number);
	
	var $telPhoneNum = $(".choiceLine a[data-rel='telNumberContent'][data-val='"+_xh_index+"'] span[id^='telPhoneNum']");
	$telPhoneNum.text(code_number);
	if(!($telPhoneNum.closest("a").hasClass("curr"))) {
		$telPhoneNum.closest("a").addClass("curr")
	}
	
	return true;
}
//选择号码
var selectNumberByRel = function(tc_rel){
	NUMBER_NEW_OBJECT = hasNeedNumber(tc_rel,NUMBER_NEW_TEXT);
	NUMBER_OLD_OBJECT = hasNeedNumber(tc_rel,NUMBER_OLD_TEXT);
	//查看套餐下是否有号码
	if(!$.isEmptyObject(NUMBER_NEW_OBJECT)){
		//新选号和副号
		//隐藏副号
		if(NUMBER_NEW_OBJECT.length<2){
			$(".choiceLine a[data-rel='telNumberContent'][data-val='1']").hide().parent().hide().prev().hide();
		}else{
			$(".choiceLine a[data-rel='telNumberContent'][data-val='1']").show().parent().show().prev().show();
		}
		for(var i=0;i<NUMBER_NEW_OBJECT.length;i++){
			if(i==0){
				NUMBER_NEW_OBJECT[i].click();
			}
			var title = NUMBER_NEW_OBJECT[i].parent().parent().parent().hide().attr("verify","Y").attr("id");//隐藏，取名称，需校验
			title = title.substr(0,title.indexOf("Div"));
			$(".choiceLine a[data-rel='telNumberContent'][data-val='"+i+"']").removeClass("curr").children("span").html("选择"+title).parent().parent().show().prev().show();
			$("#changeNumberLink").click();
		}
		
		if(!$.isEmptyObject(NUMBER_OLD_OBJECT)){
			$("#oldTelphone").prev().show().prev().show();
		}else{
			$("#oldTelphone").prev().hide().prev().hide();
		}
		$(".choiceLine a[data-rel='telNumberContent']").attr("data-value","0").removeClass("curr");
	}else{//隐藏选号
		$(".choiceLine a[data-rel='telNumberContent']").parent().hide().prev().hide();
	}
}
//根据选择主副号变更相关内容
var changeXuanhaoContent = function(obj){
	var _xh_index = obj.attr("data-val");
	var title = obj.children("span[id^='telPhoneNum']").text();
	$("#telNumberContent h2").text(title);
	$("#telNumberContent").attr("data-val",_xh_index);//当前选择的号码
	if(NUMBER_NEW_OBJECT!=null){
		if(NUMBER_NEW_OBJECT.length>1){
			if(title.indexOf(PHONE_FU)!=-1){//副号不显示已有号码
				$("#oldTelphone").hide().prev().hide().prev().hide();
			}else if(title.indexOf(PHONE_ZHU)!=-1){//主号码根据情况是否有已有号码
				if(!$.isEmptyObject(NUMBER_OLD_OBJECT)){
					$("#oldTelphone").prev().show().prev().show();
				}else{
					$("#oldTelphone").prev().hide().prev().hide();
				}
			}
		}
	}
	
	
}

$(document).ready(function(){
	initArray();//初始化数组
});

</script>
