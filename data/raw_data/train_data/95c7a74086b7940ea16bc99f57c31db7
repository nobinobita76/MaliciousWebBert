$(".sina_login,.qq_login,.wx_login").click(function(){
    var cki=1;
    var ck=setInterval(checkcookie,500);    
    function checkcookie(){
        var url="/cc/checkcookie";
        $.getJSON(url,{type:"cookie",ctime:Math.random()},function(data){
            if(data.msg){
                is_login_state = true;
                window.clearInterval(ck);
                $(".close_btn").click();
                pn = $(".header_name").length;
                //console.log(data);
                if(pn == 0){
                    html='<div class="login_yes"><a href="'+data.uurl+'"><p style="background-image:url('+data.icon+')" class="header_perPic left"></p></a><a href="'+data.uurl+'"><sapn class="header_name">'+data.nick+'</sapn></a><a href="javascript:;" class="sign_out">登出</a></div>';
                    $(".personal").html(html);  
                  //  console.log(data);
                    if(data.isgroup >= 300){
                        banner_html = '<a href="http://www.miaopai.com/admin_media/video">管理中心</a>';
                        $(".headerNav").append(banner_html);
                    }
                }
            }
        })
    }
});
//每次登陆页面判断是否有cookie
$.getJSON("/cc/checkcookie",{type:"cookie",ctime:Math.random()},function(data){
    if(data.msg){
        is_login_state = true;
        html='<div class="login_yes"><a href="'+data.uurl+'"><p style="background-image:url('+data.icon+')" class="header_perPic left"></p></a><a href="'+data.uurl+'"><sapn class="header_name">'+data.nick+'</sapn></a><a href="javascript:;" class="sign_out">登出</a></div>';
        $(".personal").html(html);
            if(data.isgroup >=300){
                ban_html = '<a href="http://www.miaopai.com/admin_media/video">管理中心</a>';
                $(".headerNav").append(ban_html);
            }
    }else{
        is_login_state = false;
        $(".personal").html('<div class="login"><a href="javascript:;">登录</a></div>');
    }
});
<!--登出-->
$(document).delegate(".sign_out","click",function(){    
    var url="/ou/logout";
    window.location.href=url
});
