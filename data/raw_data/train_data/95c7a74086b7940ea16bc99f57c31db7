$(function(){
    $(".download").click(function(){
        window.location.href ="http://www.miaopai.com/download?position=web";
    }); 
    //视频
        MiaopaiPlayer({
            "parent":document.getElementsByClassName("MIAOPAI_player")[0],
            "width":"100%",
            "height":"480px",
            "autoplay":false,
            "title":"",
            "scid":"bozRRZqyfobw59OekcvkL9LtB1tbu4yr",
            "videoSrc":"http://gslb.miaopai.com/stream/bozRRZqyfobw59OekcvkL9LtB1tbu4yr.mp4?ssig=b3ea269cc0694b627564c724c740c140&time_stamp=1491087472831&cookie_id=&vend=1&os=3&partner=1&platform=2&cookie_id=&refer=miaopai&scid=bozRRZqyfobw59OekcvkL9LtB1tbu4yr",
            "poster":"http://bsyimg4.cdn.krcom.cn/stream/bozRRZqyfobw59OekcvkL9LtB1tbu4yr_m.jpg"
        });
    //下载分块儿滚动固定
    var top1=$('.cotentRight').offset().top;
    var left1=$('.cotentRight').offset().left;
    var footerHeg=$('.footer').offset().top;
    var ewmHeg=517;
    $(window).scroll(function () {
        var scroll_top=$(window).scrollTop();
        if(scroll_top>=top1&&scroll_top+ewmHeg<footerHeg){
            $('.cotentRight').css({'position':'fixed','top':0,'left':'50%','margin-left':'172px'});
        }else if(scroll_top+ewmHeg>footerHeg){
            $('.cotentRight').css({'position':'absolute','top':(footerHeg-ewmHeg),'left':'50%','margin-left':'172px'});
        }else{
            $('.cotentRight').css({'position':'static','margin-left':'0'})
        }
    })
})
</script>
