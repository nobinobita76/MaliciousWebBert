    var top1=$('.cotentRight').offset().top;
    var left1=$('.cotentRight').offset().left;
    var footerHeg=$('.footer').offset().top;
    var ewmHeg=517;
    $(window).scroll(function () {
        var scroll_top=$(window).scrollTop();
        if(scroll_top>=top1&&scroll_top+ewmHeg<footerHeg){
            $('.cotentRight').css({'position':'fixed','top':0,'left':'50%','margin-left':'172px'});
        }else if(scroll_top+ewmHeg>footerHeg){
            $('.cotentRight').css({'position':'absolute','top':(footerHeg-ewmHeg),'left':'50%','margin-left':'172px'});
        }else{
            $('.cotentRight').css({'position':'static','margin-left':'0'})
        }
    })
})
