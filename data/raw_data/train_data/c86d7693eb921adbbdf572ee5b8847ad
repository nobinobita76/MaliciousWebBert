<script type="text/javascript" id="ImScriptInterface" src="http://im.21cp.com/WebIM/js/core/focu_webim_2011_im.js"></script> <script type="text/javascript" src="http://im.21cp.com/WebIM/js/core/focu_webim_2011_im1.js?t=123456"></script>

<script type="text/javascript">
(function(){


                var accounts = [];
                $('.im-online-state-list').each(function(){
                    var self =$(this),
                    cssClassTxt = self.attr('class'),
                    cssClassList = cssClassTxt.split(' ');
                  
                    $.each(cssClassList,function(index,val){
                        if(val.indexOf('bind_') != -1){
                            for(var i = 0; i < accounts.length; i++){
                                if(accounts[i].cls == val){
                                    return;
                                }
                            }
                            accounts.push({name:val.replace('bind_',''),cls:val});
                        }
                    });
                });
                window.onload = function(){
                        focu_webim_chat_entrance_createIcon_new({
                            accounts:accounts,
                            style:{
                                type:3, // 系统样式类型，中塑杨杨默认的为类型3.
                                customize:true,  // 开启定制  
                                online:'http://static.21cp.cc/comm/img/im/onLineshop.gif',  // 在线状态图标
                                offline:'http://static.21cp.cc/comm/img/im/offLineshop.png', //离线状态图标
                                online_hover:'http://static.21cp.cc/comm/img/im/onLineshop.gif', // 在线状态鼠标悬停图标
                                offline_hover:'http://static.21cp.cc/comm/img/im/offLineshop.png' // 离线状态鼠标悬停图标
                            }
                        });
                    };                            
                 })();
                </script>

    
