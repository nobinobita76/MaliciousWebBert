<script>
/*增加判断，如果有广告图，请为#index-header重置固定在顶部的距离值。*/
$(document).ready(function() {
ecWap.cartService.setCartNum();
var uri = document.location.pathname;
if (null == uri || '' == uri || '/member/exchange' == uri) {
uri = '/index';
}
//走马灯
var noticeSwiper = new Swiper('#noticeSlider', {
loop: true,
direction: 'vertical',
autoplay: 4000,
onlyExternal:true,//禁用鼠标或触摸翻页
});
//校验浏览器是不是safari
var userAgent = navigator.userAgent.toLowerCase();
if(userAgent.indexOf("iphone") > 0 && userAgent.indexOf("safari") > 0) {
$('.header-ads').hide();
$('#index-header').css('top', 0);
if(ecWap.cookie.get("btmBUB") == null) {
$('.btmBUB').show();
}
$(window).bind('scroll', function() {
$('.btmBUB').hide();
});
}
Utils.imagelazy.lazyLoad('img');
});
//关闭浮层数据存cookie
function btmSetCookie() {
$('.btmBUB').hide();
if(ecWap.cookie.get("btmBUB") == null) {
ecWap.cookie.set("btmBUB",
"1", {
expires : 365,
path : "/",
domain : ".vmall.com"
});
}
};
//用户首次登录弹出继续购物对话框
window.onload = function() {
var boxMsg_agree = 1;
if (ecWap.account.isFirstLogin && ecWap.account.isFirstLogin == 1) {
var boxMsg = '<div class="ecWap-dialog-content-list clearfix">'
+ '<ul>'
+ '<li><p class="ecWap-icon"><img src="https://res.vmallres.com/nwap/20170227/images/echannelWap/sale/official_mall.png?20170209" /></p><p class="ecWap-title"><span>官方商城</span></p></li>'
+ '<li><p class="ecWap-icon"><img src="https://res.vmallres.com/nwap/20170227/images/echannelWap/sale/quality_goods.png?20170209" /></p><p class="ecWap-title"><span>正品行货</span></p></li>'
+ '<li><p class="ecWap-icon"><img src="https://res.vmallres.com/nwap/20170227/images/echannelWap/sale/safe_shop.png?20170209" /></p><p class="ecWap-title"><span>安全购物</span></p></li>'
+ '<li><p class="ecWap-icon"><img src="https://res.vmallres.com/nwap/20170227/images/echannelWap/sale/7days_return.png?20170209" /></p><p class="ecWap-title"><span>七天退货</span></p></li>'
+ '<li><p class="ecWap-icon"><img src="https://res.vmallres.com/nwap/20170227/images/echannelWap/sale/freight_discount.png?20170209" /></p><p class="ecWap-title"><span>运费优惠</span></p></li>'
+ '<li><p class="ecWap-icon"><img src="https://res.vmallres.com/nwap/20170227/images/echannelWap/sale/15days_change.png?20170209" /></p><p class="ecWap-title"><span>15天换货</span></p></li>'
+ '<li><p class="ecWap-icon"><img src="https://res.vmallres.com/nwap/20170227/images/echannelWap/sale/nationwide_warranty.png?20170209" /></p><p class="ecWap-title"><span>全国联保</span></p></li>'
+ '<li><p class="ecWap-icon"><img src="https://res.vmallres.com/nwap/20170227/images/echannelWap/sale/ease_sale.png?20170209" /></p><p class="ecWap-title"><span>安心售后</span></p></li>'
+ '</ul>'
+ '</div>'
+ '<p style="margin-left:-.3em"><label class="radiobox"><input type="checkbox" checked="checked" id="boxMsg_agree" name="group1"><i></i><span>同意接收营销信息 <a href="/saleinfo/saleMore"><strong>更多相关信息 &gt;</strong></a></span></label></p>';
ecWap.box(boxMsg, {
"onok": function(box) {
if ($('#boxMsg_agree').is(':checked')) {
boxMsg_agree = 1;
} else {
boxMsg_agree = 0;
}
$.ajax({
type: "GET",
url: "/saleinfo/setSaleInfoRcvCfg.json?boxMsg_agree=" + boxMsg_agree
});
ecWap.box.close();
},
"className": "ecWap-box ecWap-box-em",
"isconfirm": false,
"title": '欢迎来到华为商城',
"ok_txt": '开始购物',
