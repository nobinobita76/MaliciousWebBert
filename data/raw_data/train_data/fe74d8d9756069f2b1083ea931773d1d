    <div class="con_right question">
      <h3 class="con_right_title"> <span></span> <em><a href="http://kefu.xoyo.com/index.php?act=game.list&amp;game=jxsj&amp;cid=1119" target="_blank"><img src="http://pic.xoyo.com/cms/jxsj/v3/index/transparent.gif" width="37" height="18" title="更多"></a></em> </h3>
      <div class="question_wrap">
        <div class="question_box">
            <!-- 客服问问组件7877 start -->
            
<script language="JavaScript">
    function dosearch_kefu(){
        var keyword = $("#xykf_keyword").val();
        if(keyword.length == 0){
            alert("请填写关键词！");
            $("#xykf_keyword").focus();
            return false;
        }
        var game = $("#gamename").val();
        window.open('http://kefu.xoyo.com/index.php?act=game.list&keyword=' + encodeURI(keyword) + '&game=' + game);
        // http://kefu.xoyo.com/index.php?act=game.list&game=jx3&keyword=%E6%98%A5%E8%8A%82%E6%B4%BB%E5%8A%A8  encodeURI
    }
</script>

