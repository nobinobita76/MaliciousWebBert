    <script src="http://go.evolife.cn/js/jquery.js" type="text/javascript" language="javascript"></script> 
    <script language="javascript" type="text/javascript" src="http://www.evolife.cn/pages/2012/jquery_colorbox-496.js"></script> 
    <script src="http://go.evolife.cn/js/toolset-1.0.js" type="text/javascript" language="javascript"></script> 
    <script src="http://go.evolife.cn/js/comment-1.1.js" type="text/javascript" language="javascript"></script> 
    <script src="http://go.evolife.cn/js/article-1.0.js" type="text/javascript" language="javascript"></script> 
    <script type="text/javascript" language="javascript">
					CComments.init();
					CComments.load( articleID, "article", 1, 30, "json" );
					var userinfo = '';
					var tokenurl = '';
					freshWeiboStatus();
					function freshWeiboStatus(){
						$.getJSON("/go/user/api.php?method=weibo_getinfo", function(info_result){
							userinfo = info_result;
							if( userinfo == '' || userinfo.weibo_uid == null){
								$( "#comment_user" ).val('');
								$( "#comment_login_from" ).html("<a href='javascript:void(0);' onclick='doWeiboLogin();return false;'><img src='http://img.evolife.cn/u/weibo_login.png' alt='新浪微博登陆'></a>");
							}else{
								$( "#comment_user" ).val(userinfo.weibo_username);
								$( "#comment_login_from" ).html(" <a href='javascript:void(0);' onclick='doWeiboLogout();return false;'>退出</a>");		
							}		
						});	
						$( "#comments_login_body" ).html('');
					}
					function ckurl(content){ 
					 if (content.indexOf("http")>-1){
					  alert("不能含有网址");
					  return true;
					 }
					 if (content.indexOf(".")>-1){
					  alert("不能含有网址");
					  return true;
					 }
					 if (content.indexOf("发票")>-1){
					  alert("不能含有发票");
					  return true;
					 }
					 if (content.indexOf("发 票")>-1){
					  alert("不能含有发票");
					  return true;
					 }
					 return false;
					} 
					function postComment(){
						var commentContent = $( "textarea[id=comment_content]" ).val();
						var commentUser = $.trim( $( "input[id=comment_user]" ).val() );
						var commentPostWeibo = document.getElementById("postweibo").checked;
						var hasurl = ckurl(commentContent);
						if(hasurl) return false;
						if(commentPostWeibo) doWeiboPost(commentContent);
						CComment.post( articleID, commentContent, commentUser );
					}
					function doWeiboPost(weiboContent){
						if(weiboContent == '') return false;
						var content,articleURL,articleTitle;
						articleURL = document.URL;
						articleTitle = $.trim( document.title );	
						//获取文章正文的图片地址
						var articlePics = new Array();
						var articlePicObjects = $( "div[class=post_content]" ).find( "img" );
						for( var i = 0; i < articlePicObjects.length && i < 4; i++ ){
							articlePics.push( articlePicObjects[i].src );
						}
						articlePics.reverse();
						content = '“'+weiboContent+'”，我在爱活网看到文章：【'+articleTitle+'】 @爱活新鲜播  '+articleURL;						
						$.post("/go/user/api.php?method=weibo_post",{content:content,img:articlePics[0]},function(result){
							//
						});
					}
					function doWeiboLogin(){
						$.get("/go/user/api.php?method=weibo_token", function(result){
							$( "#comments_login_body" ).html("<iframe frameborder='no' scrolling='no' src='"+result+"' width='600' height='350'></iframe>");
						});
					}
					function doWeiboLogout(){
						$.get("/go/user/api.php?method=weibo_clear", function(result){
							if(result == 'done'){
								freshWeiboStatus();
							}
						});					
					}
					function doDianZan(){
						$.get("/go/comment/ajax.php?segment=dozan&isget=2&id="+articleID, function(result){
							$("#dianzan").html(result);
						});					
					}
$.get("/go/comment/ajax.php?segment=dozan&isget=1&id="+articleID, function(result){
							$("#dianzan").html(result);
						});
				</script> 
