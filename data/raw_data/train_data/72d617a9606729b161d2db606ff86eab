<script type="text/javascript">
    goPage = (function (page) {
        var s = window.location.href;
        if (/page=/.test(s))
            s = s.replace(/page=[0-9]*/, "page=" + page);
        else
            s = s + "&page=" + page;
        if (!/\?/.test(s))
            s = s.replace("&", "?");
        window.location.href = s;
    });
</script>

