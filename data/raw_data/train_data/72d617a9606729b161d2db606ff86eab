<script src="http://mazecmg.gxrc.com/Js/siteJs/webApp/DicSlidingLayer.min.js"></script>
<script src="http://mazecmg.gxrc.com/Js/siteJs/webApp/slidingLayer.min.js"></script>
<script src="http://mazecmg.gxrc.com/Js/siteJs/webApp/WelfareSelect.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $("#topSelect .topUL > li span").click(function () {
            var $selList = $(this).next().next(".sel_list");
            if ($.trim($selList.attr("style")) == "display: block;") {
                $(this).parent().removeClass("current");
                $selList.hide();
            } else {
                $("#topSelect .topUL > li").removeClass("current");
                $(this).parent().addClass("current");
                $(".sel_list").hide();
                $selList.show();
            }
        });
        $("#topSelect .sel_list li").click(function () {
            $(this).parent().prev().prev().text($(this).text());

            var li_typeid = $(this).attr("id");
            var li_type = li_typeid.replace(/[\d-]/g, "");
            var li_value=li_typeid.replace(li_type, "");
            switch(li_type){
                case "ct": o_url.positionList.District = li_value; break;//地区
                case "wk": o_url.positionList.WorkAge = li_value; break;//工龄
                case "pa": o_url.positionList.Salary = li_value; break;//薪资
                case "update": o_url.positionList.update = li_value; break;//最近更新
            }
            getUrlSearch();

            $(this).parent().hide();
            $(this).parent().parent().removeClass("current");
        });
        if (location.href.indexOf("schType=2") >= 0) {
            $("#topSelect").hide();
        }
        $("#txtPosition").click(function () {
            $(this).DicSlidingLayer("position", "hid_txtPosition");
        });
        $("#txtIndustry").click(function () {
            $(this).DicSlidingLayer("industry", "hid_txtIndustry");
        });
        $("#txtCity").click(function () {
            $(this).DicSlidingLayer("city", "hid_txtCity");
        });
        $("#screening").slidingLayer({
            triggerButton: "#screening",
            popupBox: "#screeningBox",
            popupBoxCon: ".screeningBoxCon",
            popupBoxLst: ".screeningBoxLst",
            closeBtn: ".closeScreen",
            originalPage: "#ResultPage",
            direction: "right"
        });
        $("#txtWelfare").click(function () {
            $(this).WelfareSelect({
                txtReplace: "#txtWelfare",
                valueReplace: "#hid_txtWelfare"
            });
        });
        //完成
        $("#screeningBox .hd span").click(function () {            
            o_url.positionList.Career = $("#hid_txtPosition").val();
            o_url.positionList.Industry = $("#hid_txtIndustry").val();
            o_url.positionList.District = $("#hid_txtCity").val();
            o_url.positionList.WorkAge = $("#workSel").val();
            o_url.positionList.Salary = $("#paySel").val();
            o_url.positionList.Edu = $("#eduSel").val();
            o_url.positionList.posType = $("#posSel").val();
            o_url.positionList.welfare=$("#hid_txtWelfare").val();
            if($("#emerCbx").attr("checked")=="checked"){
                o_url.positionList.IsEmer = true;
            }
            else
                o_url.positionList.IsEmer = false;
            getUrlSearch();

            $("#screeningBox").animate({ 'width': '0' }, { queue: false, duration: 100 });
            $("#screeningBox .closeScreen").animate({ 'width': 0 }, { queue: false, duration: 100 });
            $("#screeningBox .closeScreen").css({ 'position': 'absolute' });
            $("#ResultPage").css({ "position": "absolute", "width": "100%", "right": "0%", "z-index": 99 });
        });
        $("#screeningBox .closeScreen").click(function () {
            $("#screeningBox .bd li a").text("请选择");
            $("#screeningBox .bd li input").val("");
            $("#screeningBox .bd li select").each(function () {
                $(this).val($(this).children().first());
            });
        });
        $(".vl").each(function () {
            var $this = $(this);
            if ($.trim($this.next().text()) == "") {
                $this.text("");
            }
        });
        
        getUrlParam = (function (name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
            var r = window.location.search.substr(1).match(reg);
            if (r != null) return r[2]; return null;
        });
        //获取搜索条件
        getSearchUrl = (function () {
            var url = "";
        });
        //搜索条件类
        o_url = {"positionList":
            {"keyword":"asp",
                "schType":1,
                "page":1,
                "pageSize":10,
                "District":0,
                "posType": 0,
                "Career":0,
                "Industry":0,
                "Salary":0,
                "WorkAge":0,
                "Edu":0,
                "IsEmer":false,
                "welfare":"",
                "update":""
            },
            "count":10
        };
        var opl = o_url.positionList;

        //根据url初始化搜索类
        var urlParam="";
        opl.keyword = getUrlParam("keyword");        
        if (!opl.keyword) opl.keyword = "";
        opl.schType = getUrlParam("schType");
        if (!opl.schType) opl.schType = 1;
        opl.page = getUrlParam("page");
        if (!opl.page) opl.page = 1;
        opl.pageSize = getUrlParam("pageSize");
        if (!opl.pageSize) opl.pageSize = 10;
        opl.District = getUrlParam("District");
        opl.posType = getUrlParam("posType");
        opl.Career = getUrlParam("Career");
        opl.Industry = getUrlParam("Industry");
        opl.Salary = getUrlParam("Salary");
        opl.WorkAge = getUrlParam("WorkAge");
        opl.Edu = getUrlParam("Edu");
        opl.IsEmer = getUrlParam("IsEmer");
        opl.welfare=getUrlParam("welfare");
        opl.update=getUrlParam("update");
        $("#city_li span").html($("#ct" + opl.District).html());
        $("#work_li span").html($("#wk" + opl.WorkAge).html());
        $("#pay_li span").html($("#pa" + opl.Salary).html());
        $("#update_li span").html($("#update" + opl.update).html());

        $("#hid_txtPosition").val(opl.Career);
        $("#hid_txtIndustry").val(opl.Industry);
        $("#hid_txtCity").val(opl.District);
        $("#workSel").val(opl.WorkAge);
        $("#paySel").val(opl.Salary);
        $("#eduSel").val(opl.Edu);
        $("#posSel").val(opl.posType);
        if (opl.IsEmer == "true")
            $("#emerCbx").attr("checked", "checked");


        //获取搜索url
        getUrlSearch = (function () {
            urlParam = "";
            if (opl.schType != null)
                urlParam += "schType=" + opl.schType + "&";
            if (opl.District != null && opl.District != 0)
                urlParam += "District=" + opl.District + "&";
            if (opl.posType != null && opl.posType != 0)
                urlParam += "posType=" + opl.posType + "&";
            if (opl.Career != null && opl.Career != 0)
                urlParam += "Career=" + opl.Career + "&";
            if (opl.Industry != null && opl.Industry != 0)
                urlParam += "Industry=" + opl.Industry + "&";
            if (opl.Salary != null && opl.Salary!=0)
                urlParam += "Salary=" + opl.Salary + "&";
            if (opl.WorkAge != null && opl.WorkAge!=0)
                urlParam += "WorkAge=" + opl.WorkAge + "&";
            if (opl.Edu != null && opl.Edu != 0)
                urlParam += "Edu=" + opl.Edu + "&";
            if (opl.IsEmer != null && opl.IsEmer==true)
                urlParam += "IsEmer=" + opl.IsEmer + "&";
            
            if (opl.welfare != null && opl.welfare!="")
                urlParam += "welfare=" + opl.welfare + "&";

            if (opl.update != null && opl.update!=0)
                urlParam += "update=" + opl.update + "&";
            

            if (opl.keyword != null && opl.keyword != "")
                urlParam += "keyword=" + opl.keyword + "&";
            if (opl.pageSize != null)
                urlParam += "pageSize=" + opl.pageSize + "&";            
            urlParam += "page=1&";
            if (urlParam != "")
                window.location.href = ("result?" + urlParam.substring(0, urlParam.length - 1));
            else
                window.location.href = "result";
        });

        //记录搜索关键字
        if (opl.keyword.length > 0 && opl.page == 1 && parseInt(1000) > 0) {
            var arrParam = window.location.search.substr(1).split("&");

            var onlyKey = "";

            var kp = 1;
            for (var p in arrParam) {
                if (kp == 0) break;
                var sp = arrParam[p].toLowerCase();
                if (!(sp.indexOf("schType") > -1 || sp.indexOf("keyword") > -1)) {
                    kp = 0;
                }
            }
            if (kp > 0) {
                onlyKey = "&onlyKey=1";
            }
            var schType = "0";
            if (opl.schType!=null && opl.schType == 2)
                schType = "-1";

            var postUrl="http://search.tgxrc.com/positionSearch/searchKeyLog.ashx?";
            if (/\.gxrc\./i.test(window.location))
                postUrl = "http://search.gxrc.com/positionSearch/searchKeyLog.ashx?";
            var c_url = postUrl + window.location.search.substr(1) + "&maxCount="
                + 1000 +onlyKey + "&uid=" + 0;
            if(/schType=/i.test(c_url))
                c_url=c_url.replace(/schType=\d*/i,"schType="+schType);            
            else
                c_url+= ("&schType=" + schType);            
            $.get(c_url, function (data, status) {
                 ;
            });
        }
        ////记录搜索历史到cookies
        //获取人才网二级域名
        var v_domain = document.domain;
        if (/localhost/i.test(v_domain)) {//判断是否为本地
            v_domain = "";
        }
        else if(/gxrc/i.test(v_domain)){//广西人才网的域名
            v_domain = "domain=" + v_domain.replace(/[^\.]*.?\./, ".") + ";"; //去除前缀，得到结果如：".gxrc.com"
        }
        //取cookies函数
        getCookie = (function (name){        
            var arr = document.cookie.match(new RegExp("(^| )" + name + "=([^;]*)(;|$)"));
            if (arr != null) return arr[2]; return null;

        });
        //删除cookie
        delCookie = (function (name) {            
            var exp = new Date();
            exp.setTime(exp.getTime() - 1);
            var cval = getCookie(name);
            if (cval != null) document.cookie = name + "=" + cval + ";" + v_domain + "path=/;expires=" + exp.toGMTString();            
        });
        //将搜索记录写入cookies
        saveSchKey = (function (name, value)//cookie名，值，value是已经编码的关键字
        {            
            if (/\S/.test(value)) {
                //查找是否有重复关键字
                var obj_searchLog = $parseJson(getCookie(name));
                if (obj_searchLog != null) {
                    for (var i = 0; i < obj_searchLog.length; i++) {
                        if (unescape(obj_searchLog[i].k) == value && obj_searchLog[i].t == opl.schType)
                            return false;
                    }
                }
                else
                    obj_searchLog = [];
                //在前面增加一条记录
                obj_searchLog.unshift({ k: escape(value), t: opl.schType });
                //限制50条记录
                if (obj_searchLog.length > 50) {
                    obj_searchLog.pop(); //删除最后一项
                }                
                var exp = new Date();
                var Days = 30;
                exp.setTime(exp.getTime() + Days * 24 * 60 * 60 * 1000);
                document.cookie = name + "=" + $toJsonString(obj_searchLog) + ";" + v_domain + "path=/;expires=" + exp.toGMTString();
            }
        });
        saveSchKey("searchLog","");
        //字符串转json
        function $parseJson(str) {
            try {
                eval('var obj=' + str);
                return obj;
            } catch (e) {
                return null;
            }
        }
        //json转换为字符串
        function $toJsonString(obj) {
            var isArray = obj instanceof Array;
            var r = [];
            for (var i in obj) {
                var value = obj[i];
                if (typeof value == 'string') {
                    value = '"' + value + '"';
                } else if (value != null && typeof value == 'object') {
                    value = $toJsonString(value);
                }
                r.push((isArray ? '' : i + ':') + value);
            }
            if (isArray) {
                return '[' + r.join(',') + ']';
            } else {
                return '{' + r.join(',') + '}';
            }
        }

    });
    if($("#page").height()<$(document).height()){
        $("#page").css("height",$(document).height());
    }
</script>


