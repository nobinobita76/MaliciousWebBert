function topsearch()
{
    var t=document.getElementById("stypes").value;
    var v=document.getElementById("allkwd").value;
    if(v==""&&v.length<2)
    {
        showErrorResult("关键字必须大于2个字符。");return;
    }
    search(t,"",v);
}
function joinvalue(chr)
{
    document.getElementById("stypes").value=chr;
}
function navi()
{
    var content="<div style=\"text-align:center;\">要浏览的网址：<input id=\"urls\" name=\"urls\" class=\"f-text\" type=\"text\" style=\"width:250px;\"></input>";
        var  W = WinTip("导航", content);
        W.ConfirmButton="浏览网页";
        W.Width=400;
        W.Confirm=function()
        {
            var urls=jQuery("#urls").val();
            if(urls=="")
            {
                alert("请填写网址");return;
            }
            window.location.href="/url.html?urls="+urls;                   
        }
        W.Create();     
}
