function AddFavorite(sURL, sTitle){
    try{window.external.addFavorite(sURL, sTitle);}
    catch (e){try{ window.sidebar.addPanel(sTitle, sURL, "");}
        catch (e){alert("加入收藏失败，请使用Ctrl+D进行添加");}
    }
}
$(document).ready(function(e){			
	t = $(".tieba_position").offset().top;
	q = $(".method").offset().top;
	$(window).scroll(function(e){
		s = $(document).scrollTop();	
		if(s > t - 10){	$(".tieba_position").addClass("tieba_fixed");}else{$(".tieba_position").removeClass("tieba_fixed");}
		if(s > q - 10){	$(".method").addClass("method_fixed");}else{$(".method").removeClass("method_fixed");}
	});
	$(".gz_tieba").load('/wap/site_ajax.asp?id=3',function(Data){
			if(ewmdata.isOpen==1){			
				var ewmsj=(ewmdata.mobiUrl1!="")?'<div class="gz_1"><img src="'+ewmdata.mobiUrl1+'" title="'+ewmdata.mobiTit1+'" />扫描访问手机版</div>':'';
				var ewmwx=(ewmdata.mobiUrl2!="")?'<div class="gz_1"><img src="'+ewmdata.mobiUrl2+'" title="'+ewmdata.mobiTit2+'" />本站微信二维码</div>':'';		
					$(".gz_tieba").html(ewmsj+ewmwx);	
			}				
	})
});
$(".topicshow").live("mouseenter", function() {
  $(this).addClass("topday_bg");
});
$(".topicshow").live("mouseleave", function() {
  $(this).removeClass("topday_bg");
});
$('.time_menu li').bind('click', function() {
  $(this).addClass("time_menu_bg").siblings().removeClass("time_menu_bg");
});
</script> 
