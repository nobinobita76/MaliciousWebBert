<script>
var MainSlides = $("#J_MainSlides"),
    slidesnav = MainSlides.find(".slidesjs-navigation").hide();
MainSlides.slidesjs({
    width:190,
    height:338,
    play:{
        active: false,
        auto:true,
        effect: "slide",
        pauseOnHover: true
    },
    pagination: false,
    navigation:false,
    callback:{
        complete:function(i){
            MainSlides.data("plugin_slidesjs").play();
        }
    }
});
MainSlides.hover(function(){
    slidesnav.fadeIn(100);
},function(){
    slidesnav.fadeOut(100);
});

var friendlinks = $("#ftlinks ul");
var more = $("<div class='more'>更多</div>");
friendlinks.css({
    height:22,
    overflow:"hidden"
}).after(more);
more.click(function(){
    friendlinks.css("height","auto");
    if(/IE 6/i.test(navigator.userAgent)){
        $(".footer").css("zoom","1");
    }
    more.remove();
});
</script>
