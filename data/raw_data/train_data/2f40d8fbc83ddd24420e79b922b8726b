<script type="text/javascript" src="https://res.cngoldres.com/mobile/cngold/js/m_comment_beta30.js"></script>
<script type="text/javascript">
    $(function(){
      //初始化全局变量数据
      MCommentDetailGlob.commentConstant.articleId = "2242271";
      MCommentDetailGlob.commentConstant.articleTitle = '万足金_万足金最新价格_万足金多少钱一克_有万足金吗_万足金是什么意思_什么是万足金';
      MCommentDetailGlob.commentConstant.articleUrl = "http://m.cngold.org/gold/zt2242271.html";
      MCommentDetailGlob.commentConstant.commentDomain = "http://comment2.cngold.org";

      //发起js调用，获取手机端数据
      $.ajax({
          type: 'get',
          url: "http://comment2.cngold.org/show_comment_detail.htm",
          data: {
              commentKey: MCommentDetailGlob.commentConstant.articleId,
              sourceType: 1,
              url:MCommentDetailGlob.commentConstant.articleUrl,
              userId:getCookieByName("IDENTITY")
          },
          dataType: 'jsonp',
          jsonp: 'callback',
          jsonpCallback: "commentCallBack",
          success: function (data) {

              //数据加载成功
              if(data.flag){
                  showCommentData($.parseJSON(data.data));
              }
          }
      });

      //初始化一些方法
      intCommentMethod();

      dealInputClickEvet();
     //百度分享插件
    window._bd_share_config = {
        common : {
            "bdText" : MCommentDetailGlob.commentConstant.articleTitle,
            "bdDesc":MCommentDetailGlob.commentConstant.articleTitle,
            "bdUrl": MCommentDetailGlob.commentConstant.articleUrl,
            "bdMini" : "2",
            "bdMiniList" : false,
            "bdPic" : "",
            "bdStyle" : "1",
            "bdSize" : "16"
        },
        share : {}
    };
      with(document)0[(getElementsByTagName('head')[0]||body).appendChild(createElement('script')).src='http://bdimg.share.baidu.com/static/api/js/share.js?cdnversion='+~(-new Date()/36e5)];

    });

    //加载数据
    function showCommentData(data){
        var commentList = data.comments;
        var commentDigit = data.commentThumbUp;
        var commentNum = data.commentNum;
        var commentStr ="";

        MCommentDetailGlob.commentConstant.commentKey = data.commentKey;

        //评论条数
        if(commentNum != 0) {
          $(".comment_num").text(commentNum);
          // $(".comment_detail_link").attr("href",encodeURI("http://comment2.cngold.org/m_comment_detail.htm?commentKey="+data.commentKey+"&commentUrl="+window.location.href+"&commentTitle="+MCommentDetailGlob.commentConstant.articleTitle));
        }
        

        //评论数据
        commentStr = showCommentList(commentList);
        if(commentList.length == 0){
          commentStr += '<div class="comment_emptyComment"><img src="https://res.cngoldres.com/comment/cngold/image/empty.png" alt="无评论"/></div>';
        }

        $(".comment_custom_comment_detail").empty().append(commentStr);

        //点赞数据展现
        showDigit(commentDigit);
        //点赞 事件绑定
        dealDigitClickEvent();
        //回复 点击事件绑定
        dealReplyClickEvent();
    }

    //将数据展现到界面上去
    function showCommentList(commentList) {
      var commentStr ="";

      if(commentList.length > 0) {
          for(var i = 0 ; i < commentList.length; i++){

              var first = commentList[i];
              first.createdAt = dealTime(first.createdAt);

              if(first.photo == ""){
                first.photo = MCommentDetailGlob.commentConstant.userPhoto;
              }
              if(first.nickName == "") {
                first.nickName = first.commentAddress;
              }
             commentStr += commentDiv(first);
              
          }
          var url = encodeURI("http://comment2.cngold.org/m_comment_detail.htm?commentKey="+MCommentDetailGlob.commentConstant.commentKey+"&commentUrl="+window.location.href);
          var readMoreStr = '<a class="comment_mores" href="'+url+'"><p>查看更多精彩评论<img src="https://res.cngoldres.com/comment/cngold/image/m_comment2.0_more-jt.png" alt=""/></p></a>';
          commentStr += readMoreStr;     
      }

      return commentStr;
    }

    //插入成功执行函数
    function createSuccess(commentContent){
        packUpCommentBlock();
        var commentStr = "";

        var commentData = assembleCommentData(commentContent);
        commentStr += commentDiv(commentData);

        //隐藏沙发
        if($(".comment_emptyComment").length != 0 && $(".comment_emptyComment").css("display") != "none"){
          $(".comment_emptyComment").hide();
          //展示
          $(".comment_custom_comment_detail").prepend(commentStr);
          firstPublishComment();
        }else{
          //展示
          $(".comment_custom_comment_detail").prepend(commentStr);
          defindAlert("发表成功！");
        }

        //绑定回复 点击事件
        dealReplyClickEvent();
        //点赞 事件绑定
        dealDigitClickEvent();
    }


    //回复按钮 绑定事件,评论页面和评论详情页面的回复按钮是跳转到评论回复页面中去，
    function dealReplyClickEvent(){
      $(".comment_mCommentBody, .comment-reply").off("click").on("click",function(){
        //回复触发函数
        MCommentDetailGlob.replyData.id = $(this).siblings(".comment_mCommentHeader").find(".comment_id").val();
        MCommentDetailGlob.replyData.nickName = $(this).parents(".comment_mCommentModel").find(".comment_name span:first").text();
        MCommentDetailGlob.replyData.commentContent = $(this).parents(".comment_mCommentModel").find(".comment_mCommentBody:first").text();

        if(MCommentDetailGlob.replyData.id == -1) {
          defindAlert("请刷新后再回复");
        }else{

          var url = "http://comment2.cngold.org/m_comment_reply.htm?topCommentId="+MCommentDetailGlob.replyData.id+"&commentKey="+MCommentDetailGlob.commentConstant.commentKey;
          document.location.href = url;
        }

      });
    }

    //点击输入的时候 评论和评论详情页面需要清空 全局变量 中的数据，但是回复页面中需要保留
    function dealInputClickEvet(){
      //点击输入
      $(".comment_repely_input input").focus(function(){
          showCommentBlock();
          clearGlobCommentData();
          clearGlobReplyData();

          //设置placeholder
          $("#comment_comment_detail").attr("placeholder","请输入评论内容...");
      });
    }


    </script>
