    		if ($(this).html().length > 5) {
	    		if ($(this).find('iframe').length == 0) {
	    			pline++;
	    		}
    		}
    		if (pline == pos) {
    			$(wjstr).appendTo($(this));
    			pline = 1000;
    		}
    	});
    }
    
