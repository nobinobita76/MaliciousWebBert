                +           '<span class="attent-other"></span>'
                +       '</div>'
                +   '</div>'
                +'</div>'
                +'</div><div class="j-attent-tip-wrap attent-tip-wrap"><i></i></div>';

        var jAttWrap = $(".j-attent-dialog-wrap");

        if(jAttWrap.length === 0){
            jAttWrap = $(attentHtml).appendTo("body");
        }
    }
