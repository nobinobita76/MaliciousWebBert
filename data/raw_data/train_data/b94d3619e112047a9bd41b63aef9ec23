  <script src="/Public/Home/js/common.js" type="text/javascript"></script> 
  <script src="/Public/Home/js/extend.func.js" type="text/javascript"></script>   
  <script type="text/javascript">
    $(function(){
        var slideHeight = 85; // px
        var defHeight = $('#wrapsaa').height();
        if(defHeight >= slideHeight){
            $('#wrapsaa').css('height' , slideHeight + 'px');
            $('#read-more').append('<a href="#">[展开]</a>');
            $('#read-more a').click(function(){
                var curHeight = $('#wrapsaa').height();
                if(curHeight == slideHeight){
                    $('#wrapsaa').animate({
                        height: defHeight
                    }, "normal");
                    $('#read-more a').html('[收起]');
                    $('#gradients').fadeOut();
                }else{
                    $('#wrapsaa').animate({
                        height: slideHeight
                    }, "normal");
                    $('#read-more a').html('[展开]');
                    $('#gradients').fadeIn();
                }
                return false;
            });
        }
    });
</script> 
  <script type="text/javascript">
    $(function(){
        var slideHeight = 65; // px
        var defHeight = $('#wrapsn').height();
        if(defHeight >= slideHeight){
            $('#wrapsn').css('height' , slideHeight + 'px');
            $('#read-moren').append('<a href="#">[展开]</a>');
            $('#read-moren a').click(function(){
                var curHeight = $('#wrapsn').height();
                if(curHeight == slideHeight){
                    $('#wrapsn').animate({
                        height: defHeight
                    }, "normal");
                    $('#read-moren a').html('[收起]');
                    $('#gradientsn').fadeOut();
                }else{
                    $('#wrapsn').animate({
                        height: slideHeight
                    }, "normal");
                    $('#read-moren a').html('[展开]');
                    $('#gradientsn').fadeIn();
                }
                return false;
            });
        }
    });
</script>
