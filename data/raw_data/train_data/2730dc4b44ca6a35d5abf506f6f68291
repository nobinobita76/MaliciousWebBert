  <script>

        $(function () {
            var xkw = {
                init: function () {
                    xkw.leftBar();
                    xkw.initEvents();

                },
                initEvents: function () {
                    $("#SearchKey").keypress(function (event) {

                        if (event.keyCode == 13) {
                            searchchk();
                        }
                    });
                },

                //左侧导航显示与隐藏
                leftBar: function () {
                    var showBox = $('.submenu li .nav_class');
                    var sanj = $('.submenu li .trigBox');
                    $('.submenu  li').hover(function () {
                        showBox.hide();
                        sanj.hide();
                        $(this).children('.nav_class, .trigBox').show();
                        //计算高度
                        var index = $(this).index();
                        var $nav = $(this).children('.nav_class');
                        if ($nav.height() > (15 - index) * 37) {
                            $nav.css({
                                'top': '-' + ($nav.height() - (15 - index) * 37 + 1) + 'px'
                            });
                        } else {
                            $nav.css({
                                'top': '0px'
                            });
                        }

                    }, function () {
                        showBox.hide();
                        sanj.hide();
                    });

                },

                scrollTop: function (n, t, o) {
                    function e() {
                        a = setInterval(function () {
                            s.animate({
                                top: -t
                            }, function () {
                                //var i = s.find("li:eq(0)");
                                var i = s.find("li").slice(0, 9);
                                s.append(i),
                                    s.css("top", 0)
                            })
                        }, o)
                    }
                    var a = null,
                        o = o || 4e3,
                        s = $(n);
                    e(),
                        s.on("mouseenter", function () {
                            clearInterval(a)
                        }),
                        s.on("mouseleave", function () {
                            e()
                        })
                }

            }

            xkw.init();

        });

        function searchchk() {
            var href = window.location.href;
            if (href.indexOf('zj') > -1) {
                if ($("#SearchKey").val()) {

                    window.location.href = "http://www.zxxk.com/zj?kw=" + encodeURIComponent($("#SearchKey").val());

                }
                else {
                    window.location.href = "http://www.zxxk.com/zj";
                    //  layer.msg('搜索条件不能为空');

                }
            }
            else
                window.open("http://search.zxxk.com/search1.aspx?keyword=" + encodeURIComponent($("#SearchKey").val()));
        }
    </script> 
  <script>
        $(function () {
            var Request = GetRequest();
            if (Request["kw"]) {
                $("#SearchKey").val(Request["kw"]);

                // console.log(Request["kw"]);
            }
        });
        function GetRequest() {
            var url = decodeURIComponent(location.search); //获取url中"?"符后的字串
            var theRequest = new Object();
            if (url.indexOf("?") != -1) {
                var str = url.substr(1);
                strs = str.split("&");
                for (var i = 0; i < strs.length; i++) {
                    theRequest[strs[i].split("=")[0]] = unescape(strs[i].split("=")[1]);
                }
            }
            return theRequest;
        }
    </script> 
