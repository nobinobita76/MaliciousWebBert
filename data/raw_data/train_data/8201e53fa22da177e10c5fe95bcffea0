    window.location.href = 'https://sns-m.liepin.com/appconnection/applytofriends/';
  }else if(type == '4'){
	  //猜你认识->app下载页
	  window.location.href = 'https://sns.liepin.com/tongdao-app/index.html?mscid=c_a1_050';
  }else if(type == '5'){
	  //动态->app下载页
	  window.location.href = 'https://sns.liepin.com/tongdao-app/index.html?mscid=c_a1_050';
  }else if(type == '6'){
	 //人脉职位->H5职位详情
	 window.location.href = 'https://sns-m.liepin.com/appconnection/showcjobdetail/?job_id='+job_id;
  }else if(type == '7'){
	 //企业职位->H5职位详情
	 window.location.href = 'https://m.liepin.com/job/showejobdetail/?job_id='+job_id;
  }else if(type == '8'){
 	 //谁浏览我的主页、查看更多->app下载页
	 window.location.href = 'https://sns.liepin.com/tongdao-app/index.html?mscid=c_a1_050';
  }else if(type == '9'){
	 //私信立即回复 来自猎头私信-H5点对点私信页
	 window.location.href = 'https://m.liepin.com/message/showhmessagelist/?to_user_id='+to_user_id+'&user_kind=2';
  }else if(type == '10'){
	 //私信立即回复 来自企业私信-H5点对点私信页
	 window.location.href = 'https://m.liepin.com/message/showhmessagelist/?to_user_id='+to_user_id+'&user_kind=1';
  }else if(type == '11'){
	 //私信查看更多 来自猎头私信->H5私信页
	 window.location.href = 'https://m.liepin.com/message/home/';
  }else if(type == '12'){
		 //认证技能标签->app下载页
		 window.location.href = 'https://sns.liepin.com/tongdao-app/index.html?mscid=c_a1_050';
  }else if(type == '13'){
		 //点头像->app下载页
		 window.location.href = 'https://sns.liepin.com/tongdao-app/index.html?mscid=c_a1_050';
