<script type="text/javascript">

var type = 'null';
var job_id="null";
var to_user_id = "null"
var user_id = "null"

//平台、设备和操作系统 
var system = {
  win: false,
  mac: false,
  xll: false,
  ipad: false
};
//检测平台 
var p = navigator.platform;
system.win = p.indexOf("Win") == 0;
system.mac = p.indexOf("Mac") == 0;
system.x11 = (p == "X11") || (p.indexOf("Linux") == 0);
system.ipad = (navigator.userAgent.match(/iPad/i) != null) ? true : false;
//跳转语句，如果是手机访问就自动跳转到wap.baidu.com页面 
if (system.win || system.mac || system.xll || system.ipad) {//PC
  if(type == '1'){
	  //经理人接受->peopleyoumayknow
    window.location.href = 'https://sns.liepin.com/sns/peopleyoumayknow/';
  }else if(type == '2'){
	 //猎头接受->peopleyoumayknow
	 window.location.href = 'https://sns.liepin.com/sns/peopleyoumayknow/';
  }else if(type == '3'){
	 //人脉 查看更多按钮->peopleyoumayknow
	 window.location.href = 'https://sns.liepin.com/sns/peopleyoumayknow/';
  }else if(type == '4'){
	 //猜你认识 加好友->可能认识的人snscontacts
	 window.location.href = 'https://sns.liepin.com/sns/snscontacts/';
  }else if(type == '5'){
	 //动态->app下载页
	 window.location.href = 'https://sns.liepin.com/tongdao-app/index.html?mscid=c_a1_050';
  }else if(type == '6'){
	 //人脉职位->H5职位详情
	 window.location.href = 'https://sns-m.liepin.com/appconnection/showcjobdetail/?job_id='+job_id;
  }else if(type == '7'){
	 //企业职位->pc职位详情
	 window.location.href = 'https://job.liepin.com/'+parseInt(job_id/10000)+"_"+job_id;
  }else if(type == '8'){
	 //谁浏览我的主页、查看更多->我的个人主页
	 window.location.href = 'https://sns.liepin.com/main/';
  }else if(type == '9'){
	 //私信立即回复 来自猎头私信->pc点对点私信页
	 window.location.href = 'https://c.liepin.com/message/#h:2:/message/getmsglist4givenuserh/?uid='+to_user_id;
  }else if(type == '10'){
	 //私信立即回复 来自企业私信
	 window.location.href = 'https://c.liepin.com/message/#e:2:/message/getmsglist4givenusere/?uid='+to_user_id;
  }else if(type == '11'){
	 //私信查看更多 来自猎头私信->pc私信页
	 window.location.href = 'https://c.liepin.com/message/#h:1:/message/gethmsg/';
  }else if(type == '12'){
	 //认证技能标签->我的个人主页
	 window.location.href = 'https://sns.liepin.com/main/';
  }else if(type == '13'){
		 //点头像->他人的个人主页
		 window.location.href = 'https://sns.liepin.com/main/'+user_id;
  }
} else {//移动
  if(type == '1'){
	  //经理人接受->新朋友
    window.location.href = 'https://sns-m.liepin.com/appconnection/applytofriends/';
  }else if(type == '2'){
