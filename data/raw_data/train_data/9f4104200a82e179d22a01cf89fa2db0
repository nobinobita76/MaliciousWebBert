<script type="text/javascript">
  var lang = "cn";
  var _pstIdx = 0;
  var _pallIdx = 0;
  function chgpst(t) {
    var n1 = parseInt(_pstIdx/2);
    var n2 = _pstIdx%2;
    if(t==1 || t==2)
      n1 = t-1;
    else
      n2 = 1-n2;
    $('#tab_person_'+_pstIdx).hide();
    _pstIdx = n1*2+n2;
    $('#tab_person_'+_pstIdx).show();
    if(t==3)
      $('.teamMainBut').html((n2?'主':'客')+'<br />队<br />>>');
  }
  function chgpall(t) {
    var n1 = parseInt(_pallIdx/2);
    var n2 = _pallIdx%2;
    if(t==1 || t==2)
      n1 = t-1;
    else
      n2 = t-3;
    $('#tab_personall_'+_pallIdx).hide();
    $('#tab_personallhead_'+_pallIdx).hide();
    _pallIdx = n1*2+n2;
    $('#tab_personall_'+_pallIdx).show();
    $('#tab_personallhead_'+_pallIdx).show();
  }

  function chgMatch(mid) {
    toMatch(mid, "cn");
  }
  function chgLang(lang) {
    toMatch("1292947", lang);
  }
  function toMatch(mid, lang) {
    if(mid) {
      if(window.location.href.indexOf(".html")>0)
        window.location = 'http://www.gooooal.com/analysis/event/'+parseInt(mid/100)+'/event_'+lang+'_'+mid+'.html';
      else
        window.location = "?matchId="+mid+"&lang="+lang;
    }
  }
</script>
