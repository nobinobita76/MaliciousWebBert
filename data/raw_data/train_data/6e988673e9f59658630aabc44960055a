    $(function () {
        $("#js_ask_question").on('click',function (e) {
            requirejs(['common/component/openAsk/openAsk'], function(oa){
                oa.qyerOpenAsk({
//                    fid:12,		//所在频道id
//                    from:2,		//来源标识
                    onSuccess:function(){		//创建成功回调
                        //console.log(1);
                        return false;		//return false为不自动跳转，默认为跳转问题详情页。
                    },
                    onError:function(){		//失败回调
                        //console.log(2);
                    }
                });
            })
        });
    });

</script>
<script>
    require(["common/component/uploadPopup/uploadPopup"],function(uploadPopup){
        var up = new uploadPopup({
            max:40,
            items:[
                {
                    model:"local",
                    opt:{
                        headOpt:{
                            models:["list","create"]
                        }
                    }
                },{
                    model:"myphoto",            //从我的相册选择这个模块，必须要有list子模块，否则无法选择图片
                    opt:{
                        headOpt:{
                            models:["list"]
                        }
                    }
                }
            ]
        });
        $(up).on("fireuploadimage",function(evt,data){
            //[{"url":"http://test1362383214.qiniudn.com/album/user/711/79/RkhURRMPYw/index"}]
            //console.log(data);
            if (typeof data == "string") {
                data = JSON.parse(data);
            }

            var photoIds = [];
            if (typeof data.photoids != "undefined") {
                photoIds.push(data.photoids);
            } else {
                for(i=0;i<data.length;i++){
                    photoIds.push(data[i].id);
                }
            }

            var data = {photos:photoIds, poiid:$("#poiid").val()};
            $.ajax({
                url:'/api.php?action=uploadPhotoToPoi',
                data: data,
                type: "post",
                dataType: 'json',
                success: function(data) {
                    //toast
                    requirejs(['common/ui/tip/tip'],  function   (tip) {
                        tip.tip({
                            text:data.data.msg
                        });
                    });
                    up.hide();
                    window.location.reload();
                }
            });

        })
        $(".pl_poiphoto_native").on("click",function(){
            var poiid = $("#poiid").val();
            up.show();
        })
    })
</script>

