    $(function () {
        $(".yl_flash_list li").mouseover(function () {
            $(this).addClass("move");
            $(this).addClass("hover");
        });
        $(".yl_flash_list li").mouseleave(function () {
            $(this).removeClass("move");
            $(this).removeClass("hover");
        });

        $(".yl_coupons_ul li").mouseover(function () {
            $(this).addClass("yl_hover_change");
        });
        $(".yl_coupons_ul li").mouseleave(function () {
            $(this).removeClass("yl_hover_change");
        });

        $(".yl_leftcoupons").mouseover(function () {
            $(this).addClass("yl_hover_bg");
        });
        $(".yl_leftcoupons").mouseleave(function () {
            $(this).removeClass("yl_hover_bg");
        });

        $(".yl_exchange_ul li").children('.bigm_box3').mouseover(function () {
            $(this).addClass("yl_hover_bg2");
        });
        $(".yl_exchange_ul li").children('.bigm_box3').mouseleave(function () {
            $(this).removeClass("yl_hover_bg2");
        });

        $(".yl_leftcoupons").mouseover(function () {
            $(this).addClass("yl_hover_color");
        });
        $(".yl_leftcoupons").mouseleave(function () {
            $(this).removeClass("yl_hover_color");
        });

        $(".yl_left_list li").mouseover(function () {
            var src_1=$(this).find('img').attr('src1');
            $(this).find('img').attr('src',src_1);
        });
        $(".yl_left_list li").mouseleave(function () {
            var src_2=$(this).find('img').attr('src2');
            $(this).find('img').attr('src',src_2);
        });

    });

    function home_page_str(obj){
        var id = $(obj).attr('id');
        $(obj).parent().removeClass('active');
        $(obj).parent().siblings().addClass('active');
        $('#'+id+'_ul').show();
        $('#'+id+'_ul').siblings().hide();
    }
</script>

<div id="imgbox-loading"><div style="opacity: 0.4; "></div></div><div id="imgbox-overlay"></div></body></html>