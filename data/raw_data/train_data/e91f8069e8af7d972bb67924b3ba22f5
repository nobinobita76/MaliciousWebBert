<script type="text/javascript">
        var nva_menu = document.getElementById('nva_menu');
		if("undefined"==typeof addHandler)var addHandler=function(e,t,h){var v=['addEventListener','attachEvent'];if(e[v[0]]){e[v[0]](t,h,false);}else if(e[v[1]]){e[v[1]]('on'+t,h);}else{e['on'+t] = h;}};
		function whichElement(e){
			var targ;
			if (!e) var e = window.event;
			if (e.target) targ = e.target;
			else if (e.srcElement) targ = e.srcElement;
			if (targ.nodeType == 3) // defeat Safari bug
			   targ = targ.parentNode;
			var tname;
			tname = targ.tagName;
			if(targ != nva_menu && !nva_menu.contains(targ)){
				if(document.getElementById('nva_menu').className == 'mbbs fr hover'){
					if(tname)document.getElementById('listmenu_menu').click()
				}
			}
		}
		
		function whichElement_myxcar(e){
			var box_daohang = document.getElementById('daohang') || document.getElementById('myxcar_box_daohang');
			if(!box_daohang)return
			var vote_add = document.getElementById('vote_add');
			var targ;
			if (!e) var e = window.event;
			if (e.target) targ = e.target;
			else if (e.srcElement) targ = e.srcElement;
			if (targ.nodeType == 3) // defeat Safari bug
			   targ = targ.parentNode;
			var tname;
			tname = targ.tagName;
			if(targ != box_daohang && targ != vote_add && !box_daohang.contains(targ)){
				box_close();
			}
		}

		if(window.Node && Node.prototype && !Node.prototype.contains){
			Node.prototype.contains = function(arg){
				return !!(this.compareDocumentPosition(arg) & 16)
			}
		}
		addHandler(document,'click',function(event){
			whichElement(event);
			whichElement_myxcar(event);
		})
        function getScript(src,callback){
			var head = document.getElementsByTagName('head')[0];
			var js = document.createElement('script');
			js.setAttribute('src',src);
			js.onload = js.onreadystatechange = function (){
				if (!this.readyState || this.readyState == "loaded" || this.readyState == "complete"){
					head.removeChild(js);
					if (callback) callback();
				}
			}
			head.appendChild(js);
		}
		</script>
<script type="text/javascript">
(function($){
	$(function(){
		if(document.getElementById('listmenu_menu'))document.getElementById('listmenu_menu').innerHTML = '<iframe id="daohang" name="daohang" style="width:822px;" border="0" frameborder="0" marginwidth="0" marginheight="0" scrolling="no"  allowtransparency="true" target="_blank"></iframe>';
		/* 顶部登录经过效果 */
		$(".menubox").hover(function(){
			if(!$(this).data("w"))$(this).data("w",$(this).width());
			$(this).css("width",$(this).data("w"));
			$(this).addClass("hover");
		},
		function(){
			$(this).removeClass("hover");
		});
		$(".mess").hover(function(){
			if(timer){
			clearTimeout(timer)
			if($('#count_msg').html() != '&nbsp;'){
			$('#header_msg').attr('class','menutb')
			}
			}
			if($('#count_msg').html() != '&nbsp;'){
			$(this).css("width",$(this).width());
			$(this).addClass("active");
			}
			},function(){
			if($('#count_msg').html() != '&nbsp;'){
			$(this).removeClass("active");
			if(timer)clearTimeout(timer)
			timer = setTimeout("msgFlicker()",500)
			}
		});
		$(".mbbs").toggle(
			function () {
				if(!$('#daohang').attr('src')){
					var _myDate = new Date();
					var _version = ''; 
					_version += _myDate.getFullYear();
					_version += _myDate.getMonth()+1;
					_version += _myDate.getDate();
					_version += _myDate.getHours();
					_version += _myDate.getMinutes();
					$('#daohang').attr('src','http://www.xcar.com.cn/bbs/header/bbsnav.htm?action=bbsnav&domain='+document.domain+'&v='+_version);
					//console.log(123);
				}
				$(this).css("width",$(this).width());
				$(this).addClass("hover");
				clickP('131','1','');
				//console.log(123);
			},
			function () {
				$(this).removeClass("hover");
		});
	});
	$(".m_page_select .option li").live('mouseover',function(){
		$(this).addClass("active").siblings().removeClass("active");
	});
})(jQuery)

jQuery(function ($) {
	$('.artZoom').artZoom({
		path: '/images',	// 设置artZoom图片文件夹路径
		preload: true,		// 设置是否提前缓存视野内的大图片
		blur: true,			// 设置加载大图是否有模糊变清晰的效果
		
		// 语言设置
		left: '左旋转',		// 左旋转按钮文字
		right: '右旋转',		// 右旋转按钮文字
		source: '查看大图',   // 查看原图按钮文字
		hide: '收起'
	});
});


</script>
