<!--站短举报uid-->
<input type="hidden" id="report_uid" value="">
	<script src="http://reg.xcar.com.cn/js/xlogin_box_new.js" type="text/javascript"></script>
<script>
	//2014-7-2  wang.peijun 举报弹窗 激活验证
    $(".reportinfo").bind('click',function(){
        $(".reportinfo").unbind('click');
        $('#report_uid').val($(this).attr('reportid'));
        $(this).xLoginBox({show:true,step:2,func:report_alert});
        return false;
    });
    //站短弹窗举报调用
    function report_win(obj){
        $("#report_someone").unbind('click');
        $(obj).xLoginBox({show:true,step:2,func:report_alert});
        return false;
    }
    function report_alert(){
        //防止重复点击
        $(".reportinfo").unbind('click');
		$("#report_someone").unbind('click');
        var his_uid = Number($('#report_uid').val());

		if(his_uid == 0){
			his_uid = Number($('#report_someone').attr('reportid'));

			if(his_uid == 0 || isNaN(his_uid)){
				his_uid = Number($('#report_someone').attr('report_uid'));
			}
		}



		if(his_uid){
			$.ajax({
				type: 'post',
				data:'uid='+his_uid,
				url: '/ajax/ajax_report.php?action=report',
				success: function(data) {
					if(data){
						var report = $().wBox({html:data,top:180});
						if($('#lucency_shade').length>0){
							//判断页面遮罩层是否已经存在
							$('#lucency_shade').remove();
						}
						report.showBox();
						var tmp=0;
						$(".selectbox").hover(function(){
									$(this).addClass("selectbox_hover");
								},function(){
									$(this).removeClass("selectbox_hover");
								}
						);
						$(".option li").hover(function(){
									tmp && clearTimeout(tmp);
									$(this).addClass("active");
								},function(){
									$(this).removeClass("active");
								}
						);
						$(".option li,.selecttxt").bind('mouseover',function(){
							tmp && clearTimeout(tmp);
						});
						$(".option li,.selecttxt").bind('mouseout',function(){
							tmp && clearTimeout(tmp);
							tmp = setTimeout(function(){
								$('.option').hide();
								$(".selectbox").removeClass("selectbox_on");
							},300);
						});
						$(".selecttxt").click(function(event){
							event.stopPropagation();
							$(".selectbox").removeClass("selectbox_on");
							$(this).parent().addClass("selectbox_on");
							$(".option").hide();
							$(this).parent().find(".option").toggle();
						});
						$('.option').bind('mouseover',function(event){
							tmp && clearTimeout(tmp);
						});
						$('.option').bind('mouseout',function(event){
							tmp && clearTimeout(tmp);
							tmp = setTimeout(function(){
								$('.option').hide();
								$(".selectbox").removeClass("selectbox_on");
							},300);
						});
						$(document).click(function(event){
							var eo = $(event.target);
							if($(".selectbox").is(":visible") && eo.attr("class")!="option" && !eo.parent(".option").length)
								$('.option').hide();
							$(".selectbox").removeClass("selectbox_on");
						});
						$("#header_select_pb .option li").click(function(){
							var value=$(this).children("a").text();

							if(value=='选择原因'){
								value = '请选择或输入举报原因';
							}
							$("#reason").val(value);
							$("#header_select_pb .selectem").html(value);
							$('#header_select_pb').children('.option').hide();
							$('#header_select_pb').children('.selecttxt').removeClass('selectbox_on');
							return false;
						});
						$('#reason').focus(function(){
							if($(this).val()=='请选择或输入举报原因'){
								$(this).val('');
							}
						});
						$('#reason').blur(function(){
							if($(this).val()==''){
								$(this).val('请选择或输入举报原因');
							}
						});
						//提交操作
						$('.submit_li').children('.submit_but').click(function(){
							report_submit();
						});

						$(".reportinfo,#report_someone").bind('click',function(){
							$(".reportinfo,#report_someone").unbind('click');
							$('.reportinfo,#report_someone').val($(this).attr('reportid'));
							$(this).xLoginBox({show:true,step:2,func:report_alert});
							return false;
						});

					}
				}
			});
		}
        
    }
    //提交举报信息
    function report_submit(){
        if($('#reason').val()!='请选择或输入举报原因'){
            //提交操作
            $('.submit_li').children('.submit_but').unbind('click');
            var reason = $('#reason').val();
            var his_uid = Number($('#report_uid').val());

            $.ajax({
                type: 'post',
                data:'uid='+his_uid+'&reason='+encodeURIComponent(reason),
                url: '/ajax/ajax_report.php?action=report_submit',
                success: function(data){
                    if(data){
                        $('#wBox').remove();
						$('#lucency_shade').remove();
                        var sub = $().wBox({html:data,top:180});
                        sub.showBox_ncover();
                    }
                }
            });
        }else{
            $('#report_error').html('亲，选一个举报原因吧，手动输入也可以。');
        }
    }

//信息反馈提交后 提示 2013-10-25 wang.peijun 
	var type = window.location.search;//获取?后参数
	var parameter = type.split("=");//分割数组
	var url = window.location.href;//获取整个url
	var pd_type='';//是否刷新
	if(url.indexOf('feedtype=')){//判断是否有反馈页面跳转参数
		for (key in parameter) {
			val = parameter[key];
			if(val=='ok'){
				pd_type=1;
				url = url.indexOf('feedtype=ok') != -1 ? url.replace(/[\?|\&]feedtype=ok$/i,'') : url;
				tishi('ok','您的问题已提交');
			}else if(val=='illegal'){
				pd_type=1;
				url = url.indexOf('feedtype=illegal') != -1 ? url.replace(/[\?|\&]feedtype=illegal$/i,'') : url 
				tishi('error','非法操作');
			}else if(val=='img_big'){
				pd_type=1;
				url = url.indexOf('feedtype=img_big') != -1 ? url.replace(/[\?|\&]feedtype=img_big$/i,'') : url 
				tishi('error','图片过大');
			}else if(val=='yzm_no'){
				pd_type=1;
				url = url.indexOf('feedtype=yzm_no') != -1 ? url.replace(/[\?|\&]feedtype=yzm_no$/i,'') : url 
				tishi('error','验证码错误');
			}else if(val=='img_type'){
				pd_type=1;
				url = url.indexOf('feedtype=img_type') != -1 ? url.replace(/[\?|\&]feedtype=img_type/i,'') : url 
				tishi('error','图片格式错误');
			}

			if(pd_type==1){
				setTimeout(function(){
					location.href=url;
				},300);
			}
		}
	}
	$("#close_reporter").bind('click', function(){
		$("#reporter").empty().hide();
	});
