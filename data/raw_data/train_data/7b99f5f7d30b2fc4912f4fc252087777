}

//手机版搜索
function toWapSearch(selTab){
	var key = jQuery("#keyword").val() || jQuery("#articleForkeyword").val();
	if(key=="" || key==undefined){
		//key = "套餐";
	}
	if(jQuery("#keyword").size()==0){
		key = "";
	}
	if(selTab!="" && (jQuery("#keyword").size()>0 || jQuery("#articleForkeyword").size()>0)){
		if(key=="" || key==undefined){
			alert("请输入关键字");
			return false;
		}
	}
	var keyEncrypted = encryptedString(key);
	var url = "/ckb/wap/search_result/"+getWebCityForLowerCase()+".html";
	if(selTab=="onlineKf"){
		url = "/ckb/wap/search_result/"+getWebCityForLowerCase()+"/"+keyEncrypted+"/"+selTab+".html";
	}else{
		if(key!=""){
			url = "/ckb/wap/search_result/"+getWebCityForLowerCase()+"/"+keyEncrypted;
			if(selTab!="" && selTab!=undefined && selTab!="searchKeyword"){
				url = "/ckb/wap/search_result/"+getWebCityForLowerCase()+"/"+keyEncrypted+"/"+selTab+".html";
			}else if(selTab!="searchKeyword"){
				url = "/ckb/wap/search_result/"+getWebCityForLowerCase()+".html";
			}
		}
	}
	location.href = url;
}

//根据搜索类型跳转到新闻或知识点或爱问
function toWapNewsOrKnowledgeOrAiwenUrlForSearch(id_,newsUrl,type,zqType,targetUtl){
	var url = "";
	var selTab = jQuery("#selTab").val();
	if(selTab=="all"){
		if(type=="news"){
			url = newsUrl;
		}else if(type=="knowledge"){
			url = "/ckb/wap/knowledge_detail/"+getWebCityForLowerCase()+"/"+getChannelKnlgForCreateTime(id_)+"/"+id_;
			if(targetUtl!="" && targetUtl!=undefined){
				url = targetUtl;
			}
		}else if(type=="aiwen"){
			//专区类型(区分02:互助或01:互动)
			if(zqType=="01"){
				url = "/ckb/wap/forum/hd/"+id_+"/answer_ques.html";
			}else if(zqType=="02"){
				url = "/ckb/wap/forum/hz/"+id_+"/answer_ques.html";
			}
		}
	}else if(selTab=="knowledge"){
		url = "/ckb/wap/knowledge_detail/"+getWebCityForLowerCase()+"/"+getChannelKnlgForCreateTime(id_)+"/"+id_;
		if(targetUtl!="" && targetUtl!=undefined){
			url = targetUtl;
		}
	}else if(selTab=="news"){
		url = newsUrl;
	}else if(selTab=="aiwen"){
		//专区类型(区分02:互助或01:互动)
		if(zqType=="01"){
			url = "/ckb/wap/forum/hd/"+id_+"/answer_ques.html";
		}else if(zqType=="02"){
			url = "/ckb/wap/forum/hz/"+id_+"/answer_ques.html";
		}
	}
	location.href = url;
}

//话费类情况JS
function huaFeiMonth(id_){
	$("#"+id_).addClass("on").siblings().removeClass("on");
	var m = $("#"+id_).index();
	$("#yw_all").children().eq(m).show().siblings().hide();
	
	var monthVal = $("#"+id_).attr("monthVal");
	//我的业务-话费类情况数据查询
	mobileMyBusinessHuaFeiByMonth("hua_fei_month",monthVal);
}

//公共提示语操作
function operationCommonDiv(){
	$("#wap_common_msg_a").click(function(){
		var hrefUrl = window.location;
		var isReload = jQuery("#isReload").val();
		if(isReload=="0"){
			var weatherCityName = $("#common_head_weatherCity option:selected").html();
			if(weatherCityName!=""&&weatherCityName!=undefined){
				var hrefRep = replaceStr(hrefUrl,"?c=","")
				location.href = hrefRep+"?c="+converCityShortNameByCityName(weatherCityName).toLowerCase();
			}else{
				//刷新当前页面
				location.reload(true);
			}
		}else{
			jQuery("#wap_common_div").hide();
		}
	});
}

//地市名称转换地市缩写
function converCityShortNameByCityName(cityName){
	var shortName = "GZ";
	if(cityName==""){
	}else if(cityName=="深圳"){
		shortName = "SZ";
	}else if(cityName=="东莞"){
		shortName = "DG";
	}else if(cityName=="佛山"){
		shortName = "FS";
	}else if(cityName=="汕头"){
		shortName = "ST";
	}else if(cityName=="中山"){
		shortName = "ZS";
	}else if(cityName=="惠州"){
		shortName = "HZ";
	}else if(cityName=="江门"){
		shortName = "JM";
	}else if(cityName=="揭阳"){
		shortName = "JY";
	}else if(cityName=="湛江"){
		shortName = "ZJ";
	}else if(cityName=="珠海"){
		shortName = "ZH";
	}else if(cityName=="茂名"){
		shortName = "MM";
	}else if(cityName=="肇庆"){
		shortName = "ZQ";
	}else if(cityName=="梅州"){
		shortName = "MZ";
	}else if(cityName=="潮州"){
		shortName = "CZ";
	}else if(cityName=="韶关"){
		shortName = "SG";
	}else if(cityName=="清远"){
		shortName = "QY";
	}else if(cityName=="汕尾"){
		shortName = "SW";
	}else if(cityName=="阳江"){
		shortName = "YJ";
	}else if(cityName=="河源"){
		shortName = "HY";
	}else if(cityName=="云浮"){
		shortName = "YF";
	}
	return shortName;
}

