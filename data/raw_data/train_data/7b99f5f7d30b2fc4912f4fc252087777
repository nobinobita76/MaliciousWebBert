function toBackPage(){
	history.go(-1);
}

//获取家庭组网筛选条件所选的值
function getMobileJtzwTagConditionSelectVal(){
	jQuery("#jtzwParamVal").val("");
	var jtzwParamVal0 = jQuery("#jtzwParamVal0").val();
	var jtzwParamVal1 = jQuery("#jtzwParamVal1").val();
	var jtzwParamVal2 = jQuery("#jtzwParamVal2").val();
	
	jQuery("#jtzwParamVal").val(jtzwParamVal0+","+jtzwParamVal1+","+jtzwParamVal2);
	var filterSelectVal = jQuery("#jtzwParamVal").val();
	return filterSelectVal;
}

//跳转至文章内容页面
function searchMobileJtzwTagCriteriaResult(){
	var jtzwTagId = jQuery("#hiddenJtzwTagId").val();
	//获取筛选条件所选的值
	var filterSelectVal = getMobileJtzwTagConditionSelectVal();
	if(filterSelectVal!=""){
		var url = "/ckb/wap/article/"+filterSelectVal+"/"+jtzwTagId+".html";
		location.href = url;
	}else{
		showWapMsgDiv("请选择网络应用下筛选条件！","关闭");
	}
}

