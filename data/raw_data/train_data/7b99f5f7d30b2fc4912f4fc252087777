<script>
/*****************************热门话题*************************************/
//热门话题换一批ajax处理
function changeHotTopic(){
	var nextPage = jQuery("#nextPageTopic").val();
	var eachPage = jQuery("#eachPageTopic").val();
	var rowCount = jQuery("#rowCountTopic").val();
	var lastPage = jQuery("#lastPageTopic").val();
	var sceneType = jQuery("#sceneType").val();
	var currentPage = jQuery("#currentPageTopic").val();
		//alert("rowCount="+rowCount+"---lastPage="+lastPage+"--currentPage="+currentPage);
	if(currentPage != "" && currentPage==lastPage){
		jQuery("#currentPageTopic").val("0");
	}else{
		jQuery.ajax({
			type: "POST",
			url: "/ckb/mobileAjax.do?action=newsMediaMobileTopic",
			data: {"propertyId":""+jQuery("#newsInfoId_").val()+"",
				"currentPage":""+nextPage+"","eachPage":""+eachPage+"",
				"rowCount":""+rowCount+"","sceneType":""+sceneType+""},
			success: function(e){
				$("#topic_list").html(e);
					var lastPage = jQuery("#lastPageTopic").val();
					var currentPage = jQuery("#currentPageTopic").val();
					if(currentPage != "" && currentPage==lastPage){
						$("#topicTrade").hide();
					}
			}
		});
	}
}
/*****************************10000知道手机版首页*************************************/
//栏目内容下一页批ajax处理
function changeHotNews(sceneType){
	//记录初始化分页参数
	var currentPage = eval(jQuery("#currentPage_"+sceneType).val())+eval(1); //获取下一页页号
	var pageCount = jQuery("#pageCount_"+sceneType).val(); //总页数
	var eachPage = 4;
	var cityId = jQuery("#cityId").val();
	//alert("nextPage="+nextPage+"rowCount="+rowCount+"---lastPage="+lastPage+"--currentPage="+currentPage);
	if(currentPage<=pageCount){
		jQuery.ajax({
			type: "POST",
			url: "/ckb/mobileAjax.do?action=newsMediaMobileList",
			data: {"propertyId":""+jQuery("#newsInfoId_").val()+"",
				"currentPage":""+currentPage+"","eachPage":""+eachPage+"","cityId":""+cityId+"","sceneType":""+sceneType+""},
			success: function(e){
				$("#news_"+sceneType).append(e);
				$("#currentPageInfo").val("N");
				jQuery("#currentPage_"+sceneType).val(currentPage);
				homePageHeightAdapt(sceneType);
				if(currentPage ==  pageCount){
					$("#newsTrade_"+sceneType).hide();
				}

				jQuery(function(){
					var autoWindowWidth = $(window).width();
					var autoWidth = autoWindowWidth - 130;
					$(".index_box_text").animate({width:autoWidth});
				});
			}
		});

	}
}

//用户签到提交
function signOpertion(){
	if($("#sign_user").html()=="已签到"){
		//setTimeoutMsgDivHide("submit_msg_p","您今天已经签到!",2500);
	}else{
		var queryParam = {"signType":"forumFans","custId":""+$("#custId").val()+""};
		var jsonResult = getAjaxSubmit(false,"/ckb/fanscenter/signDay.do",queryParam,"json");
		if(jsonResult.result =="Y") {
			var day = $("#serialSignDays").html();
			day = parseInt(day) + 1;
			var fansInt = parseInt(jsonResult.fansInt);

			$("#ln5").hide();
			$("#ln6").show();
			$("#serialSignDays").html(day);
		}else{
			alert("签到失败!");
		}
	}
	/*
	jQuery.ajax({
		type: "POST",
		url: "/ckb/ajax.do?action=mobileSignOpertionSubmit",
		data: {"custId":""+jQuery("#custId").val()+""},
		success: function(result){
			if(result!= null){
				location.reload();
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
			//alert(XMLHttpRequest.status);
		}
	});
	*/
}

/**************************************推荐(赞)、分享、点击数(浏览量)操作 开始*****************************************************/
//推荐(赞)、分享、点击数(浏览量)操作
function operatingArtice(operation,paragraphId,articleTitle,id_){
	var contentType = "";
	var isCommend = jQuery("#"+id_+paragraphId).attr("isCommend");
	if(isCommend=="F" || operation=="dj"){
		try{setCookieName("djCookie","T");}catch(e){}
		if(id_=="recommendCount_a" || id_=="newsContrecommendCount_a"){
			//contentType = "paragraphContent";
		}
		jQuery.ajax({
			type: "POST",
			contentType: "application/x-www-form-urlencoded; charset=utf-8",
			url: "/ckb/keNewV3.do?action=operatingArtice",
			data: {"operation":""+operation+"","paragraphId":""+paragraphId+"",
				"contentType":""+contentType+"","articleTitle":""+articleTitle+""},
			dataType: 'json',
			success: function(result){
				var jsonObj = result;
				if(jsonObj !=null) {
					if(operation=="tj"){
						if(jsonObj.resultStr>0){
							if(id_=="recommendCount_a"){
								jQuery("#"+id_+paragraphId).attr("isCommend","T");
								var htmlStr = "推荐<span>"+jsonObj.recommendCount+"</span>";
								htmlStr = "（"+jsonObj.recommendCount+"）";
								jQuery("#"+id_+paragraphId).html(htmlStr);
							}else if(id_=="recommendCountSpan"){
								jQuery("#"+id_+paragraphId).attr("isCommend","T");
								jQuery("#"+id_+paragraphId).html(jsonObj.recommendCount);
								jQuery("#"+id_+paragraphId).removeClass("index_like");
								jQuery("#"+id_+paragraphId).addClass("index_like_after");
							}else if(id_=="knowledgeRecommend"){
								jQuery("#"+id_+paragraphId).attr("isCommend","T");
								jQuery("#knowledgeRecommendCount_"+paragraphId).html(jsonObj.recommendCount);
							}else if(id_=="newsContrecommendCount_a"){
								jQuery("#"+id_+paragraphId).attr("isCommend","T");
								jQuery("#newsContrecommendCount_a"+paragraphId).html(jsonObj.recommendCount);
								jQuery("#"+id_+paragraphId).addClass("on");
							}else if(id_=="newsComment_a"){
								jQuery("#"+id_+paragraphId).attr("isCommend","T");
								jQuery("#newsComment_a"+paragraphId).html(jsonObj.recommendCount);
							}else if(id_=="recommendArticle_a"){
								jQuery("#"+id_+paragraphId).attr("isCommend","T");
								jQuery("#"+id_+paragraphId).html("（"+jsonObj.recommendCount+"）");
								jQuery("#"+id_+paragraphId).addClass("on");
							}
						}
					}
				}
			}
		});
	}
}

/**************************************新闻列表页面 开始*****************************************************/
//获取下10条的手机版新闻列表
function mobileAppendNextNewsInfo(me){
	try{
		// throw new Error("新闻异常");
		var nextPage = jQuery("#nextPage").val() == undefined ? 1:jQuery("#nextPage").val();
		var eachPage = jQuery("#eachPage").val() == undefined ? 4:jQuery("#eachPage").val();
		var rowCount = jQuery("#rowCount").val();

		var lastPage = jQuery("#lastPage").val();
		var currentPage = jQuery("#currentPage").val();
		jQuery("#pageNewsTjDiv").remove();
		jQuery(".showmore a").html("加载中,请稍候...<img src=\"/ckb/knlgfile/kmweb/km/10000zhidao/newsMedia/wap/images/index/showmore_icon.png\" class=\"showmore_img\">");
		jQuery.ajax({
			type: "POST",
			url: "/ckb/mobileAjax.do?action=getAppendNextMobileNewsInfo",
			data: {"tagId":""+jQuery("#newsInfoId_").val()+"",
				"currentPage":""+nextPage+"","eachPage":""+eachPage+"",
				"rowCount":""+rowCount+"","city":""+jQuery("#web_city").val()+"",
				"cityId":""+jQuery("#web_city_id").val()+"",
				"sceneShortName":""+jQuery("#sceneShortName").val()+""},
			success: function(result){
				try{
					if(result!=""){
						jQuery(".showmore a").html("显示下一页<img src=\"/ckb/knlgfile/kmweb/km/10000zhidao/newsMedia/wap/images/index/showmore_icon.png\" class=\"showmore_img\">");
						if(nextPage == 1){
							jQuery(".index_news").html(result).css("height","auto");
							if(parseInt(jQuery("#currentPage").val()) == jQuery("#lastPage").val()){
								jQuery(".showmore").css("display","none");
							}else{
								jQuery(".showmore").css("display","block");
							}
						}else{
							jQuery(".index_news .index_box:last").after(result);
						}
						currentPage = jQuery("#currentPage").val();
						//图片延迟加载
						$(".index_box .index_box_img_"+currentPage+" img").lazyload({
							effect:"fadeIn"
						});//.removeClass("loadingimg").removeAttr("src").addClass("index_box_img_css");
						
						if((parseInt(currentPage))==lastPage){
							jQuery(".showmore").css("display","none");
							jQuery("#isEnd").val("true");
						}
						if(me != undefined && me!=""){
							me.resetload();
						}
						$(".news_box .news_comment .commentbox .area").each(function(i){
							var id_ = $(this).attr("id");
							if(id_!="" && id_!=undefined){
								$("#login_a_"+id_).click(function(){
									//隐藏登录或注册层
									hideLoginOrRegisterDiv();
									//根据登录状态，显示登录或注册层
									showLoginOrRegisterDiv();
								});
							}
						});
						/*重新加载newspage分享JS*/
						jQuery(function(){
							jQuery(".news_share").hover(function(e) {
								jQuery(this).children().show();
							},function(){
								jQuery(this).children().hide();
							});

							var autoWindowWidth = $(window).width();
							var autoWidth = autoWindowWidth - 130;
							$(".index_box_text").animate({width:autoWidth});
						});
					}
				}catch(e){
					console.log(e.message);
					jQuery(".index_news").html("");
				}
			},error: function(XMLHttpRequest, textStatus, errorThrown) {
				//alert(XMLHttpRequest.status);
			}
		});
	}catch(e){
		console.log(e.message);
	}
}
//手机版新闻详细页面广告
function mobileLmAdverInfo(){
	try{
		// throw new Error("广告异常");
		jQuery.ajax({
			type: "GET",
			url: "/ckb/mobileAjax.do?action=getLmAdverInfo",
			data: {"sceneType":""+jQuery("#news_cont_scene_type").val()+""},
			success: function(result){
				try{
					if(result.Trim() != ""){
						jQuery("#owl-example").html(result).css("display","block").owlCarousel();
					}
				}catch(e){
					console.log(e.message);
				}
			},error: function(XMLHttpRequest, textStatus, errorThrown) {
				//alert(XMLHttpRequest.status);
			}
		});
	}catch(e){
		console.log(e.message);
	}
}
/**************************************注册页面 开始*****************************************************/
//发送短信验证码到指定的手机号码
function sendMessageCaptchaCode(shortMsgSendType){
	jQuery.ajax({
		type: "POST",
		async: false,
		url: "/ckb/ajax.do?action=sendMessageCaptchaCode",
		data: {"shortMsgSendType":""+shortMsgSendType+"","accessNum":""+jQuery("#phoneNum").val()+""},
		success: function(result){
			if(result!="") {
				jQuery("#isOver").val("F");
				jQuery("#isSend").val("T");
				jQuery("#sendCaptchaCode").val(result);

				//短信验证码已成功发送到您填写的手机内
				jQuery("#captchaMsg").html("短信验证码已成功发送，请查收！");
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
			//alert(XMLHttpRequest.status);
		}
	});
}
//注册10000知道用户
function accountRegister(){
	if(checkRegister()){
		var url = "/ckb/wap/news.html";
		var registerType = jQuery("#registerType").val();
		if(registerType=="fans"){
			url = "/ckb/wap/fanscenter/main.do";
		}
		jQuery.ajax({
			type: "POST",
			async: false,
			url: "/ckb/ajax.do?action=accountRegister",
			data: {"cityId":""+jQuery("#web_city_id").val(),"mobileNumber":""+encryptAES(jQuery("#phoneNum").val())+"",
				"userName":""+jQuery("#userName").val()+"","userPwd":""+encryptAES(jQuery("#userPwd").val())+"",
				"msgCaptchaCode":""+jQuery("#msgCaptchaCode").val()+"",
				"captchaCode":""+jQuery("#captchaCode").val()+"","regCostCaptchaCode":""+jQuery("#regCostCaptchaCode").val()+""},
			success: function(result){
				if(result>0) {
					jQuery("#timeMsg").html("");
					jQuery("#getCaptchaCode").html("发送验证码");
					//刷新当前页面

					var activtyGuessHfFlag = jQuery("#activtyGuessHfFlag").val();
					if(activtyGuessHfFlag=="1"){
						url = "/ckb/wap/personalInfo/user_center.html";
					}else{
						var cookieUrl = getCookieName("recordBrowseTrajectory");
						if(cookieUrl!="" && cookieUrl!=undefined){
							url = cookieUrl;
						}
					}
					location.href = url;
				}else{
					if(result==-99){
						jQuery("#captchaMsg").html("验证码不正确！");
						return;
					}else if(result==-98){
						jQuery("#captchaMsg").html("用户名只能由汉字、数字、字母组成！");
						return;
					}else if(result==-97){
						jQuery("#captchaMsg").html("短信验证码失效！");
						return;
					}else if(result==-96){
						jQuery("#captchaMsg").html("输入短信验证码有误！");
						return;
					}
				}
			},error: function(XMLHttpRequest, textStatus, errorThrown) {
				//alert(XMLHttpRequest.status);
			}
		});
	}
}
//验证用户输入的账号是否存在
function checkExistCust(account,accountPwd,divMsgId,checkType){
	var isExist = true;
	var queryParam = {"dataType":"checkExistCust","checkType":""+checkType+"",
		"account":""+account+"","accountPwd":""+encryptAES(accountPwd)+"",
		"isCaptchaCode":""+jQuery.trim(jQuery("#isCaptchaCode").val())+"",
		"captchaCode":""+jQuery.trim(jQuery("#costCaptchaCode").val())+""};
	var lk = encryptAES(JSON.stringify(queryParam));
	jQuery.ajax({
		type: "POST",
		async: false,
		url: "/ckb/service/dataView.do",
		data: {"lk":""+lk+""},
		dataType: 'json',
		success: function(jsonObj){
			if(jsonObj.isExistCust=="-1"){
				jQuery("#"+divMsgId).html(jsonObj.errorMsg);
				changeCaptchaCode('captchaCodeImg','loginimgcode');
				isExist = false;
			}else if(jsonObj.isExistCust=="-2"){
				jQuery("#"+divMsgId).html(jsonObj.errorMsg);
				changeCaptchaCode('captchaCodeImg','loginimgcode');
				isExist = false;
			}else if(jsonObj.isExistCust=="-3"){
				jQuery("#"+divMsgId).html(jsonObj.errorMsg);
				changeCaptchaCode('captchaCodeImg','loginimgcode');
				isExist = false;
			}else if(jsonObj.isExistCust=="-99"){
				jQuery("#"+divMsgId).html(jsonObj.errorMsg);
				$("#mobileLogin_a").hide();
				$("#zhidaoUpdatePwd_a").show();
				$("#zhidaoUpdatePwd_a").click(function(){
					$("#lnpp4").hide();
					$("#lnpp5").hide();
					$("#lnpp6").hide();
					$("#zhidaoUpdatePwd_a").remove();
					$("#confim_div").show();
					$("#confim_update").click(function(){
						jQuery("#login_popup1").remove();
						checkAccountUserData();
					});
				});
				isExist = false;
			}else if(jsonObj.isExistCust=="0"){
				isExist = true;
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
			//alert(XMLHttpRequest.status);
		}
	});
	return isExist;
}

//用户登录
function checkAccountUserData(){
	var accountUser = jQuery.trim(jQuery("#accountUser").val());
	var accountPhone = jQuery.trim(jQuery("#accountPhone").val());
	var accountCostCaptchaCode = jQuery.trim(jQuery("#accountCostCaptchaCode").val());
	if(accountUser=="" || accountUser==undefined){
		jQuery("#confim_msg").html("请输入用户名！");
		return false;
	}
	if(accountPhone=="" || accountPhone==undefined){
		jQuery("#confim_msg").html("请输入手机号码！");
		return false;
	}else{
		if(accountPhone.length<11){
			jQuery("#confim_msg").html("请输入11位手机号码！");
		return false;
		}
	}
	if((accountCostCaptchaCode=="" || accountCostCaptchaCode==undefined)){
		jQuery("#confim_msg").html("请输入验证码！");
		return false;
	}
	
	var queryParam = {"dataType":"checkZhidaoAccountPhone","accountUser":""+encryptAES(accountUser)+"",
		"accountPhone":""+encryptAES(accountPhone)+"","accountCostCaptchaCode":""+accountCostCaptchaCode+""};
	var lk = encryptAES(JSON.stringify(queryParam));
	jQuery.ajax({
		type: "POST",
		url: "/ckb/service/dataView.do",
		data: {"lk":""+lk+""},
		success: function(resultData){
			if(resultData.resultCode=="0"){
				jQuery("#confim_div").hide();
				jQuery("#confim_submit_update_div").show();
				jQuery("#confim_submit_update").click(function(){
					checkAccountUpdatePwd();
				});
			}else{
				jQuery("#confim_msg").html(resultData.resultReason);
				changeCaptchaCode('accountCostCaptchaCodeImg','uppwdimgcode');
				return false;
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
		}
	});
}

//用户修改密码
function checkAccountUpdatePwd(){
	var accountUser = jQuery.trim(jQuery("#accountUser").val());
	var accountNewPassword = jQuery.trim(jQuery("#accountNewPassword").val());
	var accountNewPassword2 = jQuery.trim(jQuery("#accountNewPassword2").val());
	if(accountNewPassword=="" || accountNewPassword==undefined){
		jQuery("#confim_update_pwd_msg").html("请输入新密码！");
		return false;
	}
	if(accountNewPassword2=="" || accountNewPassword2==undefined){
		jQuery("#confim_update_pwd_msg").html("请再次输入确认密码！");
		return false;
	}
	if(accountNewPassword!=accountNewPassword2){
		jQuery("#confim_update_pwd_msg").html("新密码与确认密码不一致,请重新输入");
		return false;
	}
	
	var queryParam = {"dataType":"checkZhidaoUpdatePwd","accountUser":""+encryptAES(accountUser)+"",
		"accountNewPassword":""+encryptAES(accountNewPassword)+"",
		"accountNewPassword2":""+encryptAES(accountNewPassword2)+""};
	var lk = encryptAES(JSON.stringify(queryParam));
	jQuery.ajax({
		type: "POST",
		url: "/ckb/service/dataView.do",
		data: {"lk":""+lk+""},
		success: function(resultData){
			if(resultData.resultCode=="0"){
				jQuery("#confim_div").hide();
				jQuery("#confim_submit_update_btn_div").hide();
				jQuery("#confim_submit_update_div").show();
				jQuery("#confim_update_pwd_msg").html(resultData.resultReason);
				setTimeout("toLoginRedirectUrl()",2000);
			}else{
				jQuery("#confim_update_pwd_msg").html(resultData.resultReason);
				return false;
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
		}
	});
}

function toLoginRedirectUrl(){
	var redirectUrl = "/ckb/wap/zhidao/login.do";
	var linkUrl = $.trim($("#link").val());
	if(linkUrl!="" && linkUrl!=undefined){
		redirectUrl+="?link="+linkUrl;
	}
	location.href = redirectUrl;
}
/**************************************10000知道新闻关联FAQ****************************************************/
var isLoad = false;//记录是否加载过
//查询关联产品二级目录
function showFaqLvTwo(productId,faqbId){
	var faqbhtml = $("#"+faqbId).html().trim();
	var sceneShortName = $("#sceneShortName").val();
	var webCity = $("#web_city").val();
	var mobileLvOneId = $("#mobileLvOneId").val();
	if(mobileLvOneId != "" && isLoad != true){
		var faqbId = "faqb"+ mobileLvOneId;
		isLoad = true;
	}
	if(faqbhtml == null || faqbhtml == ""){
		if(productId!=null && productId!=""){
			jQuery.ajax({
				type: "POST",
				url: "/ckb/mobileAjax.do?action=newsMediaMobileFaqLvTwo",
				data: {"productId":""+productId+"","sceneShortName":""+sceneShortName+"","webCity":""+webCity},
				success: function(e){
					$("#"+faqbId).html(e);
					var mobileLvTwoId = $("#mobileLvTwoId").val();
					if(mobileLvTwoId != "" && mobileLvTwoId != 0){
						var faqcId = "faqc"+ mobileLvTwoId;
						$("#"+faqcId).siblings("span").click();
					}

				}
			});
		}
	}
	var sceneShortCssType = $("#sceneShortCssType").val();
	if($("#"+faqbId).css("display") == "none"){
		$("#"+faqbId).show();
		$("#"+faqbId).parent(".faqa").siblings().children(".faqb").hide();
		if($("#"+faqbId).hasClass("faqa_icon"+sceneShortCssType)){
			$("#"+faqbId).removeClass("faqa_icon"+sceneShortCssType).addClass("faqa_icon"+sceneShortCssType+"up");
			$("#"+faqbId).parent(".faqa").siblings().children(".faqa1").removeClass("faqa_icon"+sceneShortCssType+"up").addClass("faqa_icon"+sceneShortCssType);
		}else{
			$("#"+faqbId).removeClass("faqa_icon"+sceneShortCssType+"up").addClass("faqa_icon"+sceneShortCssType);
			$("#"+faqbId).parent(".faqa").siblings().children(".faqa1").removeClass("faqa_icon"+sceneShortCssType).addClass("faqa_icon"+sceneShortCssType+"up");
		}
	}
}

//查询IPTVTab关联数据
function showItvTabLvOne(tabId,sceneShortCssType){
	jQuery.ajax({
		type: "POST",
		url: "/ckb/mobileAjax.do?action=mediaMobileItvLvOne",
		data: {"sceneShortCssType":""+sceneShortCssType+"","tabId":""+tabId+""},
		success: function(e){
			$(".faq_list").html(e);
			//给返回的数据添加事件
			$(".faqb").on("click","span",function(e) {
				var productId = $(this).attr("productId");
				var faqchtml = $(this).siblings(".faqc").html().trim();
				var sceneShortName = $("#sceneShortName").val();
				var webCity = $("#web_city").val();
				if(faqchtml == null || faqchtml == ""){
					if(productId!=null && productId!=""){
						jQuery.ajax({
							type: "POST",
							url: "/ckb/mobileAjax.do?action=newsMediaMobileFaq",
							data: {"productId":""+productId+"","sceneShortName":""+sceneShortName+"","webCity":""+webCity},
							success: function(data){
								var data2 = data.trim();
								$("#faqc"+productId).html(data2);
							}
						});
					}
				}
				if($(this).siblings().css("display") == "none")
				{
					$(this).siblings(".faqc").show();
					$(this).parent(".faqb1").siblings().children(".faqc").hide();
					if($(this).hasClass("faqb_icon1"))
					{
						$(this).removeClass("faqb_icon1");
						$(this).addClass("faqb_icon1up");
						$(this).parent(".faqb1").siblings().children("span").removeClass("faqb_icon1up").addClass("faqb_icon1");
					}
					else
					{
						$(this).removeClass("faqb_icon1up").addClass("faqb_icon1");
						$(this).parent(".faqb1").siblings().children("span").removeClass("faqb_icon1").addClass("faqb_icon1up");
					}
				}
			});
		}
	});
}

//常见问题反馈
function submitStarRating(){
	var isSolve = jQuery("#isSolve").val();
	//问题描述
	var quesDesc = "";
	var isSubmit = "F";
	var contentStr = "";
	if(isSolve=="N"){
		var textCont = jQuery("#textCont").val();
		var cont = document.getElementsByName("radContent");

		for(var i=0;i<cont.length;i++){
			if(cont[i].checked){
				isSubmit = "T";
				quesDesc+=cont[i].value;
				break;
			}
		}
		if(quesDesc!="" && textCont!=""){
			isSubmit = "T";
			contentStr = quesDesc+","+textCont;
			quesDesc+=","+textCont;
		}else if(quesDesc!="" && textCont==""){
			isSubmit = "T";
			contentStr = quesDesc;
		}else if(quesDesc=="" && textCont!=""){
			isSubmit = "T";
			quesDesc=textCont;
			contentStr = textCont;
		}
		jQuery("#quesDesc").val(quesDesc);
		jQuery("#content").val(contentStr);
		if(isSubmit=="F"){
			alert("请对常见问题评价！");
		}
	}else{
		if($(".judge1").html()!="以上内容是否解决了您的问题呢？"){
			isSubmit = "F";
		}else{
			isSubmit = "T";
		}
	}

	var superSceneId = "";
	if(jQuery("#mobileLvTwoId").size()>0){
		superSceneId = jQuery("#mobileLvTwoId").val();
	}
	if(isSubmit=="T"){
		var isProdScene = "F";
		var isPresent = jQuery("#isPresent").val();
		var actionName = jQuery("#actionName").val();

		if(actionName=="mobileFaqSceneInfo" || actionName=="mobileFaqDetail"){
			isProdScene = "T";
		}
				
		var pjFormData = resetQuery("pjForm",true);//jQuery('#pjForm').serializeObject();
		if(isSolve!="Y"){
			if(isPresent!="T"){
				showWapLoginDiv();
			}
			var faqSceneUrlData = location.href+"~"+isSolve;
			jQuery("#login_solve_p").show();
			jQuery.ajax({
				type:"post",
				contentType: "application/x-www-form-urlencoded; charset=utf-8",
				url: '/ckb/keNewV3.do?action=doStarRating',
				data:{"is_New10000":"M2","type":"quesFeedback",
					"touristId":""+jQuery("#touristId").val()+"",
					"superSceneId":""+superSceneId+"",
					"isFlag":""+isSolve+"","isProdScene":""+isProdScene+"",
					"pjFormData":""+encryptAES(JSON.stringify(pjFormData))+""},
				success: function (data) {
					if(data>0){
						if(isPresent!="T"){
							setCookieName("recordFaqSolveUrl",encryptAES(faqSceneUrlData));
						}
						$(".judge1").text("您反馈的信息已经成功提交，感谢您的反馈。");
						$("#solveN").css("display","none");
						$(".yesno").css("display","none");
						$("#btnSubmitSolve").css("display","none");
					}else{
						$(".judge1").text("您反馈的信息提交失败。");
						$("#solveN").css("display","none");
						$(".yesno").css("display","none");
						$("#btnSubmitSolve").css("display","none");
					}
				}
			});
		}else{
			jQuery.ajax({
				type:"post",
				contentType: "application/x-www-form-urlencoded; charset=utf-8",
				url: '/ckb/keNewV3.do?action=doStarRating',
				data:{"is_New10000":"M2","type":"quesFeedback",
					"touristId":""+jQuery("#touristId").val()+"",
					"superSceneId":""+superSceneId+"",
					"isFlag":""+isSolve+"","isProdScene":""+isProdScene+"",
					"pjFormData":""+encryptAES(JSON.stringify(pjFormData))+""},
				success: function (data) {
					if(data>0){
						$(".judge1").text("您反馈的信息已经成功提交，感谢您的反馈。");
						$("#solveN").css("display","none");
						$(".yesno").css("display","none");
						$("#btnSubmitSolve").css("display","none");
					}else{
						$(".judge1").text("您反馈的信息提交失败。");
						$("#solveN").css("display","none");
						$(".yesno").css("display","none");
						$("#btnSubmitSolve").css("display","none");
					}
				}
			});
		}
	}
}

/********************************************10000知道手机版新闻评论*********************************************************/

//提交发表评论
function submitPublishComment(newsId,typeId,replayValId,plateType){
	var replyContent;
	var isPresent = jQuery("#isPresent").val();
	var publishCustId = jQuery("#custId").val();
	var publishCustName = jQuery("#custName").val();
	if(isPresent!="T"){
		showWapLoginDiv();
	}else{
		if(replayValId=="mArticleContent"){
			replyContent = jQuery("#"+replayValId).val();
		}else if(replayValId != null && replayValId != ''){
			replyContent = jQuery("#"+replayValId).val();
		}else{
			replyContent = jQuery("#writetext").val();
		}

		//var commentDiv = jQuery(".comment").attr("style");
		//alert(publishCustId+"---->"+publishCustName+"------>"+replyContent);
		//async: false,
		/**/
		if(checkMaliciousCode(replyContent)){
			if(replyContent!=""){
				//敏感词查询
				if(filterSenstive(replyContent)){
					return;
				}
				jQuery.ajax({
					type: "POST",
					url: "/ckb/mobileAjax.do?action=submitComment",
					data: {"type":"publish","plateType":""+plateType+"",
						"newsId":""+newsId+"","typeId":""+typeId+"","cityId":""+jQuery("#web_city_id").val()+"",
						"publishCustId":""+publishCustId+"","publishCustName":""+publishCustName+"",
						"content":""+replyContent+"","friendName":"wap"},
					success: function(result){
						if(result!=""){
							if(replayValId=="mArticleContent"){
								alert("操作成功！");
								jQuery('.Publish').hide();
								$("#"+replayValId).val("");
								location.href = decodeBase64(jQuery("#redirectUrl").val());
							}else{
								$(".reply-li").after(result);
								$("#writetext").val("");
								$("#writeframe2").html("");
								$(".publishpopup").hide();
							}
						}
					},error: function(XMLHttpRequest, textStatus, errorThrown) {
						//alert(XMLHttpRequest.status);
					}
				});
			}else{
				alert("发表您的评论！");
			}
		}else{
			alert("不能提交,内容存在恶意代码");
		}
	}
}

//回复评论
function submitComment(newsId,replayValId,typeId,plateType){
	var isPresent = jQuery("#isPresent").val();
	var replyCustId = jQuery("#custId").val();  //当前评论用户id
	var replyCustName = jQuery("#custName").val(); //当前评论用户名称
	var superCommentId = jQuery("#commentId").val();//当前评论id
	var publishCustId = jQuery("#publishCustId").val();//评论用户id
	var publishCustName = jQuery("#publishCustName").val();//评论用户名称
	var replayContent = jQuery("#"+replayValId).val();//回复内容
	//alert(replyCustId+"---->"+replyCustName+"------>"+superCommentId+"--->"+publishCustId+"--->"+publishCustName+"--->"+replayContent);
	//if(replayContent!=""){
	if(isPresent!="T"){
		showWapLoginDiv();
	}
	
	if(replayContent!=""){
		if(checkMaliciousCode(replayContent)){
			//敏感词查询
			if(filterSenstive(replayContent)){
				return;
			}
			jQuery.ajax({
				type: "POST",
				async: false,
				url: "/ckb/mobileAjax.do?action=submitComment",
				data: {"plateType":""+plateType+"","newsId":""+newsId+"",
					"superCommentId":""+superCommentId+"","publishCustId":""+publishCustId+"",
					"publishCustName":""+publishCustName+"","replyCustId":""+replyCustId+"",
					"typeId":""+typeId+"","cityId":""+jQuery("#web_city_id").val()+"","firstCommentId":""+jQuery("#firstCommentId").val()+"",
					"replyCustName":""+replyCustName+"","content":""+replayContent+"","friendName":"wap"},
				success: function(result){
					if(result!=""){
						alert("操作成功！");
						//首评论id
						var firstCommentId = $("#firstCommentId").val();
						$("#comment_"+firstCommentId).after(result);
						$(".answerpopup").hide();
						$("#"+replayValId).val("");
					}
				},error: function(XMLHttpRequest, textStatus, errorThrown) {
					//alert(XMLHttpRequest.status);
				}
			});
		}else{
		alert("不能提交,内容存在恶意代码");
		}
	}
}

/******************************************10000知道手机版新闻个人信息************************************************/
/***********************个人资料***********************/
//保存个人信息数据
function doWabSavePersonalInfo(){
	if(validationUserDataSubmit()){
		jQuery.ajax({
			type: "POST",
			async: false,
			url: "/ckb/cunLogin.do?action=savePersonalInfo&custId="+jQuery("#custId").val(),
			data : jQuery('#signInForm').serialize(),
			dataType:"json",
			success: function(result){
				if(result!=null){
					if(result.resultcode>0){
						if(result.guessDayFlag=="1"){
							if(result.isJoin=="0"){
								$(".infopopup1 p").attr("style","line-height:20px;font-size:15px;padding:10px;text-align:left;text-indent:2em;");
								$(".infopopup1 p").html(result.msg);

								$("#zl_submit .myDatum_ok_popup").attr("href","/ckb/wap/personalInfo/ques_reply.html");
								$("#zl_submit").show();
							}else if(result.isJoin=="1"){
								$(".infopopup1 p").attr("style","line-height:20px;font-size:15px;padding:10px;text-align:left;text-indent:2em;");
								$(".infopopup1 p").html("您已参加过抽奖了！");

								$("#zl_submit .myDatum_ok_popup").attr("href","/ckb/wap/personalInfo/ques_reply.html");
								$("#zl_submit").show();
							}else{
								$("#zl_submit").show();
							}
						}else{
							$("#zl_submit").show();
						}
					}
				}
				/*
				if(result>0){
					$("#zl_submit").show();
				}
				*/
			},error: function(XMLHttpRequest, textStatus, errorThrown) {
				//alert(XMLHttpRequest.status);
			}
		});
	}
}

	/*****************个人中心修改密码*********************/
//提交用户修改密码
function mobileNewsUpPassword(){
var custId = jQuery("#custId").val();
if(validationUserPwd("",custId)){
	jQuery.ajax({
		type: "POST",
		url: "/ckb/ajax.do?action=updateCustPwdSubmit",
		data: {"type":"updatePwd","custId":""+custId+"","isJson":"T",
			"time":""+encryptAES(jQuery("#referCode").val())+"",
			"rawPwd":""+encryptAES(jQuery("#rawPwd").val())+"",
			"custPwd":""+encryptAES(jQuery("#confirmPwd").val())+""},
		success: function(data){
			if(data.result>0){
				$("#mm_submit").show();
			}else{
				$("#newPwd_Message").html(msg);
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
			//alert(XMLHttpRequest.status);
		}
	});
	}
}
	//验证用户输入的原密码
function checkCustRawPwd(custId){
	var isMathch = false;
	jQuery.ajax({
		type: "POST",
		async: false,
		url: "/ckb/ajax.do?action=updateCustPwdSubmit",
		data: {"type":"checkPwd","custId":""+custId+"",
			"rawPwd":""+encryptAES(jQuery("#rawPwd").val())+""},
		success: function(result){
			if(result>0){
				isMathch = true;
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
			//alert(XMLHttpRequest.status);
		}
	});
	return isMathch;
}

