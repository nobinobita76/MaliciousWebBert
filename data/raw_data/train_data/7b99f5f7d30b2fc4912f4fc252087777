}

//跳转到集团阅小知
function toMobileSSWW(){
	var webCityName = jQuery.trim(jQuery("#web_city_name").val());
	if(webCityName=="" || webCityName==undefined){
		webCityName = "广州";
	}
	url = 'http://xiaozhi.189.cn:8082/APPOnlineCustomer/servlet/tmain?ReqParam={"province":"广东","city":"'+webCityName+'","applycode":"gddxkf10000zd"}';
	location.href="http://xiaozhi.189.cn:8082/APPOnlineCustomer/servlet/tmain";
}

//跳转至我要提问
function toMyAsk(){
	var isPresent = jQuery("#isPresent").val();
	var quesTypeId = jQuery("#quesTypeId").val();
	var forumIndexIsNew = jQuery("#forumIndexIsNew").val();
	var urlHref = "/ckb/wap/forum/my_ques.html?qtId="+quesTypeId;
	if(forumIndexIsNew!="T"){
		urlHref = "/ckb/wap/personalInfo/my_ques.html";
	}
	if(isPresent!="T"){
		showWapLoginDivByUrl(urlHref);
	}else{
		location.href = urlHref;
	}
}

//展开或收起菜单(知识点内容页)
function openMenuDiv(elsId,elsSrc){
	var elsIdObj = document.getElementById(elsId);
	var elsSrcObj = document.getElementById(elsSrc);
	
	elsSrcObj.style.display=elsSrcObj.style.display=='none'?'block':'none';
	elsIdObj.innerText=elsIdObj.innerText =='- 收起'?'+ 展开':'- 收起';
}

//跳转到手机版常见问题详细内容页面
function toWapFaqDetailByChannelKnlg(ki,si,faqId){
	var urlStr = "/ckb/wap/faq_detail/"+getWebCityForLowerCase()+"/"+ki;
	if(si!=""){
		urlStr+="/"+si;
	}
	urlStr+="/"+faqId;
	location.href = urlStr;
}

