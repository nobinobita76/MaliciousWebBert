	$("#kspc").click(function(e) {
        $(".kdpcewm").show();
    });
	$(".kdpcewm span").click(function(e) {
        $(".kdpcewm").hide();
    });

	jQuery("#kdpcewm_close_span").click(function(){
		$(".kdpcewm").hide();
	});
}

