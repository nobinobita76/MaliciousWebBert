//问卷调查关闭
function closeZhidaoWjdc(){
	if(jQuery("#wjdc_skip").size()>0){
		var wjdc_skipCookie = jQuery.cookie("wjdc_skipCookie");
		if(wjdc_skipCookie!="" && wjdc_skipCookie!=undefined){
			jQuery(".popup1").hide();
		}else{
			jQuery(".popup1").show();
		}
		jQuery(".skip").click(function(e) {
			jQuery(".popup").hide();
			jQuery.cookie('wjdc_skipCookie', 'wjdc_skipCookie1', { expires: 365, path: '/' });
		});
		jQuery(".popup-pic-bottom").click(function(e) {
			jQuery(".popup1").hide();
			jQuery.cookie('wjdc_skipCookie', 'wjdc_skipCookie1', { expires: 365, path: '/' });
		});
	}
}
	
