</script>

<script>
/**************************************跳转页面 开始*****************************************************/
function getBodyWith(){
	var ww = document.body.clientWidth;
	return ww;
}

//根据页面主窗体宽度而变化(搜索栏和联想层)
function getResizeBodyWith(searchWith,suggestWidth){
	var ww = getBodyWith();
	jQuery(".sTxt").width(ww-searchWith);
	jQuery("#suggest_container").width(ww-suggestWidth);
	jQuery(window).resize(function() {
		var w = jQuery(this).width();
		st = jQuery(".sTxt");
		st.width(w - searchWith);
		jQuery("#suggest_container").width(w-suggestWidth);
	});
}

//跳转到新闻内容页
function toWapNewsCont(toUrl){
	location.href=toUrl;
}

//跳转到指定页面链接
function toWapTargetUrlPage(toUrl){
	location.href=toUrl;
}

//FAQ详细页跳转到指定页面链接
function openWeb(toUrl){
	toWapTargetUrlPage(toUrl);
}

//主页内容高度自适应
function homePageHeightAdapt(sceneType){
	var height = $("#news_"+sceneType).parent(".content-slide").height();
	//获取内容高度
	$(".swiper-container").css("height",eval(height)+eval(21));
}

