		function createQrcodeUrlById(logoImgId,qrcodeCanvas_div,isWxLoginUrl,linkUrl,imgWidth,imgHeight,oper_type){
			var qrcodeUrl = linkUrl;
			if(isWxLoginUrl==true){
				if(linkUrl==""){
					qrcodeUrl = getLoginWeixinByUrlAndIsEncode('wap','false','');
				}else{
					qrcodeUrl = getLoginWeixinByUrlAndIsEncode('wap','true',linkUrl);
				}
			}
			document.getElementById(logoImgId).src="/ckb/qrcode.do?link="+qrcodeUrl+"&height="+imgHeight+"&width="+imgWidth+"&oper_type="+oper_type;
			jQuery(qrcodeCanvas_div).show();
		}

