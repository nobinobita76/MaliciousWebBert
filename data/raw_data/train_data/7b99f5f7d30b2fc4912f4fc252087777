		jQuery.ajax({
			type: "POST",
			url: "/ckb/ajax.do?action=doPersonalCenterSubmit",
			data: {"type":"productBind","phoneNum":""+jQuery("#telephone").val()+""},
			success: function(result){
				if(result>0){
					$("#cp_popup").show();
				}
			},error: function(XMLHttpRequest, textStatus, errorThrown) {
				//alert(XMLHttpRequest.status);
			}
		});
	}
}


