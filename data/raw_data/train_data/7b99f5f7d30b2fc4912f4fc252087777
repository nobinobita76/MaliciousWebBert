function openImg(imgScr){//打开图片
   var pathName=window.location.pathname;
   if(pathName.indexOf('SmtCCS_manage')!=-1||pathName.indexOf('SmtCCS_tkms')!=-1){
	imgScr=imgScr.replace('ckb','SmtCCS_tkms');
   }
	window.location.href=imgScr;
}
function openContentNouns(id){
	var obj = document.getElementById(id);
	if (obj.style.display=="none"){
		obj.style.display="";
	}else{
		obj.style.display="none";  
	}
}
window.onload=function(){
	var now = new Date();
	var showAll = false;
	for(var i=1;i < 100;i++){
		var id = "xgyh"+i;
		var show = false;
		var table = document.getElementById(id);
		if(table!=null){
			var timeStr = document.getElementById(id+"Time");//当前优惠时间
			if(timeStr!=null){
				var date = timeStr.value;
				if(date!=''){
					date = date + " 23:59:59";
					var time = new Date(date.replace(/-/g,"/"));
					if(now > time){
						table.style.display="none";
						for(var j=1;j < 20;j++){
							var obj= document.getElementById(id+"C"+j);
							if(obj!=null){ obj.style.display="none"; }
						}
					}
				}
			}
		}
	}
	for(var i=1;i < 100;i++){
		var obj1 =  document.getElementById("xgyh"+i);
		if(obj1!=null){
			if(obj1.style.display!="none"){
				showAll = true;
				break;
			}
		}
	}
	if(!showAll){
		var obj1 =  document.getElementById("xgyh");
		if(obj1!=null){ obj1.style.display="none"; }
	}
}
function toPlace2(ss){
	for(var i=1;i < 10;i++){
		var obj1 = document.getElementById("els"+i);
		var obj2 = document.getElementById("els"+i+"src");
		if(obj1 !=null){
			obj1.innerText = "- 收起";
			
		}
		if(obj2 !=null){
			obj2.style.display="";
		}
	}
	var objP = jQuery("#"+ss).position();
	if(objP!=null){
		var pTop = objP.top-15;
		jQuery("html,body").animate({scrollTop:pTop},200);
	}
}
