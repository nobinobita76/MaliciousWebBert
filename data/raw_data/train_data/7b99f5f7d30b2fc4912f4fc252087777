//我的资料初始化
function myDatumInfo(){
	var w = $(window).width();
	var x = w - 127;
	$(".infotext").animate({width:x});
	var y = ( w - 110 ) / 2;
	$(".infosn1").animate({width:y});
	var z = w - 110;
	$(".infosn2").animate({width:z});
}

//兴趣爱好选择加载
function interestsInfo(){
	$(".hobbypopup p").children("span").each(function(){
		if(!$(this).hasClass("zl_ok")){
			$(this).addClass("zl_no");
		}
	});
}

//验证个人中心问题反馈-个人资料
function validationUserDataSubmit(){
	var isFileOk = true;
	//验证电话号码
	var patternMobile = /^1[3|4|5|7|8][0-9]\d{8}$/;
	var patternEmail = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(.[a-zA-Z0-9_-])+/ ;
	var realName = $("#realName").val();
	var telephone = $("#mobileNumber").val();
		//用户名验证
		if(realName==""){
			alert("请输入用户名");
			return false;
		}else{
			var isErrorMsg = "";
			//if(tempUserCustName!="" && tempUserCustName!=userCustName){
			//	isErrorMsg = checkExistCustForErrorMsg(userCustName,"","register");
			//}
			if(isErrorMsg!="") {
				showTipMsg(true,"msgTip_userCustName",isErrorMsg);
				return false;
			}else{
				showTipMsg(false,"msgTip_userCustName","");
			}
		}
		
		//手机号码验证
		if(telephone!=""){
			if(!patternMobile.test(telephone)) {
				alert("手机号码有误");
				return false;
			}else{
				if(telephone.length>11){
					alert("请输入11位手机号码");
					return false;
				}else if(telephone.length>0 && telephone.length<5){
					alert("请输入11位手机号码");
					return false;
				}else{
					showTipMsg(false,"msgTip_mobileNumber","");
				}
			}
		}
	return true;
}
