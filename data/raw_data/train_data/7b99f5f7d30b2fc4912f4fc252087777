/*****************个人中心修改密码*********************/
//验证用户修改密码
function validationUserPwd(id_,custId){
	var rawPwd = jQuery("#rawPwd").val();
	var newPwd = jQuery("#newPwd").val();
	var newPwdMinlength = jQuery("#newPwd").attr("minlength");
	var newPwdMaxlength = jQuery("#newPwd").attr("maxlength");
	var confirmPwd = jQuery("#confirmPwd").val();
	
	if(id_==""){
		if(rawPwd==""){
			showTipMsg(true,"rawPwd_Message","请输入原密码");
			return false;
		}else{
			if(!checkCustRawPwd(custId)){
				showTipMsg(true,"rawPwd_Message","您密码输入有误，请重新输入");
				return false;
			}else{
				showTipMsg(false,"rawPwd_Message","");
			}
		}
		
		if(newPwd==""){
			showTipMsg(true,"newPwd_Message","请输入新密码");
			return false;
		}else{
			if(newPwd!=""){
				if(jQuery("#newPwd").val().length<newPwdMinlength){
					showTipMsg(true,"newPwd_Message","您输入的新密码不能小于"+newPwdMinlength+"数，请重新输入");
					return false;
				}else{
					var msg = validNumOrEnOrStrMsgJs("newPwd");
					if(msg!=""){
						showTipMsg(true,"newPwd_Message",msg);
						return false;
					}else{
						showTipMsg(false,"newPwd_Message","");
					}
				}
			}else{
				showTipMsg(false,"newPwd_Message","");
			}
		}
		
		if(confirmPwd==""){
			showTipMsg(true,"confirm_PwdMessage","请输入确认密码");
			return false;
		}else{
			if(newPwd!="" && confirmPwd!=""){
				if(confirmPwd!=newPwd){
					showTipMsg(true,"confirm_PwdMessage","您输入的密码与上次不符合，请重新输入");
					return false;
				}else{
					showTipMsg(false,"confirm_PwdMessage","");
				}
			}
		}
	}else{
		if(id_=="rawPwd"){
			if(rawPwd==""){
				showTipMsg(true,"rawPwd_Message","请输入原密码");
				return false;
			}else{
				if(!checkCustRawPwd(custId)){
					showTipMsg(true,"rawPwd_Message","您密码输入有误，请重新输入");
					return false;
				}else{
					showTipMsg(false,"rawPwd_Message","");
				}
			}
		}
		
		if(id_=="newPwd"){
			if(newPwd==""){
				showTipMsg(true,"newPwd_Message","请输入新密码");
				return false;
			}else{
				if(newPwd!=""){
					if(jQuery("#newPwd").val().length<newPwdMinlength){
						showTipMsg(true,"newPwd_Message","您输入的新密码不能小于"+newPwdMinlength+"数，请重新输入");
						return false;
					}else{
						var msg = validNumOrEnOrStrMsgJs("newPwd");
						if(msg!=""){
							showTipMsg(true,"newPwd_Message",msg);
							return false;
						}else{
							showTipMsg(false,"newPwd_Message","");
						}
					}
				}else{
					showTipMsg(false,"newPwd_Message","");
				}
			}
		}
		
		if(id_=="confirmPwd"){
			if(confirmPwd==""){
				showTipMsg(true,"confirm_PwdMessage","请输入确认密码");
				return false;
			}else{
				if(newPwd!="" && confirmPwd!=""){
					if(confirmPwd!=newPwd){
						showTipMsg(true,"confirm_PwdMessage","您输入的密码与上次不符合，请重新输入");
						return false;
					}else{
						showTipMsg(false,"confirm_PwdMessage","");
					}
				}
			}
		}
	}
	return true;
}

