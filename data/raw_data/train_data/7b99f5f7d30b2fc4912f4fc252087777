	$("#superCommentId").val(superCommentId);
	$("#publishCustId").val(publishCustId);
	$("#publishCustName").val(publishCustName);
	$("#firstCommentId").val(firstCommentId);
	if(isPresent!="T"){
		showWapLoginDiv();
	}else{
		$(".answerpopup").show();
	}
}

