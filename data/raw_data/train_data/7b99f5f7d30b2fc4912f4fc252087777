		url: "/ckb/mobileAjax.do?action=mobileNewsMyForumData",
		data: {"urlType":"my_forum","tabType":""+tabType+"",
			"custId":""+jQuery("#custId").val()+""},
		success: function(result){
			if(result!=""){
				$("#"+idName).html(result);
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
			//alert(XMLHttpRequest.status);
		}
	});
}

//保存用户提交个人中心数据-订阅功能
function saveWapMyDyOpertion(){
	var dyData = "";
	$(".dy_right span").each(function(i){
		var dyId = jQuery(this).attr("id");
		var dyType = jQuery(this).attr("dyType");
		var dySceneId = jQuery(this).attr("dySceneId");
		var dyStatus = jQuery(this).attr("class");
		dyData+=dyId+"_"+dyType+"_"+dyStatus+"_"+dySceneId+","
		//alert("id="+dyId+"<---->dyStatus="+dyStatus);
	});
	//alert(dyData);
	if(dyData.lastIndexOf(",")!=-1){
		dyData = dyData.substring(0,dyData.lastIndexOf(","));
	}

	jQuery.ajax({
		type: "POST",
		url: "/ckb/ajax.do?action=doPersonalCenterSubmit",
		data: {"type":"dy","dyData":""+dyData+""},
		success: function(result){
			if(result>0) {
				$("#dy_submit").show();
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
			//alert(XMLHttpRequest.status);
		}
	});
}


//问题反馈-站内回复更新已解决问题状态值
function solveWapQuestion(problemId){
	jQuery.ajax({
		type: "POST",
		url: "/ckb/ajax.do?action=doPersonalCenterSubmit",
		data: {"type":"solveQues","problemId":""+problemId+""},
		success: function(result){
			if(result>0) {
				$("#"+problemId).parents(".answer_show").prev("li").children("i").show();
				$("#"+problemId).parents(".answer_show").hide();
				$(".answer_btn").hide();
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
			//alert(XMLHttpRequest.status);
		}
	});
}


