//我要提问初始化
function myIssueFeedbackInfo(){
	var w = $(window).width();
	var w = w - 53;
	$(".fk_left").animate({width:w});
	$(".ok_popup").click(function(e) {
        $(this).parents(".infopopup").hide();
        window.location.reload();
    });
    
	var h = $(window).width();
	var h = h - 30;
	$(".tj_input").animate({width:h});
	$(".tj_ta").animate({width:h});
}

//验证个人中心问题反馈-我的提交问题的数据
function validationQuesSubmit(){
	var isFileOk = true;
	var patternMobile = /^1[3|4|5|8][0-9]\d{8}$/; //验证电话号码
	var patternEmail = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(.[a-zA-Z0-9_-])+/ ;
	var mobilePhone = $("#mobilePhone").val();
	//var email = $("#email").val();
	var quesTitle = $("#quesTitle").val();
	var quesDesc = $("#quesDesc").val();
	if(quesTitle==""){
	 	$("#tip_quesTitle").html('*请输入问题标题');
	 	return false;
	}else{
	 	$("#tip_quesTitle").html('');
	} 
	if(quesDesc==""){
	 	$("#tip_quesDesc").html('*请输入问题描述');
	 	return false;
	}else{
	 	$("#tip_quesDesc").html('');
	}
	
	if($("#textfield").val()!=""){
		var msg = checkImgSuffixFormatForHtmlStr($("#textfield").val());
		if(msg!=""){
			$("#tip_uploadAttach").html(msg);
			return false;
		}else{
			$("#tip_uploadAttach").html('');
		}
	}
	return true;
}

//站内回复-继续提问
function operationContinueAsk(operation,problemId,id_){
	if(operation=="show"){
		//显示当前提问层
		jQuery("#goon_box_"+problemId).toggle();
	}else if(operation=="cancel"){
		//隐藏当前继续提问层
		jQuery("#goon_box_"+problemId).hide();
	}else if(operation=="submitGoOnAsk"){
		//提交继续提问
		//alert(jQuery("#answer_show_"+problemId+" p:last").attr("id"));
		var afterProblemId = jQuery("#answer_show_"+problemId+" p:last").attr("id");
		if(jQuery("#goon_textarea_"+problemId).val().Trim()!=""){
			jQuery.ajax({
				type: "POST",
				url: "/ckb/ajax.do?action=doPersonalCenterSubmit",
				data: {"type":"quesFeedback","isPage":"T",
					"isUploadAttach":"F","superProblemId":""+problemId+"",
					"custId":""+jQuery("#custId").val()+"",
					"quesDesc":""+jQuery("#goon_textarea_"+problemId).val()+""},
				success: function(result){
					if(result!=""){
						jQuery("#"+afterProblemId).after(result);
						//隐藏当前继续提问层
						jQuery("#goon_box_"+problemId).hide();
						jQuery("#gx_title_i_"+problemId).html("140");
						jQuery("#goon_textarea_"+problemId).val("");
						jQuery("#goon_ask_"+problemId).addClass("btn_disable");
					}
				},error: function(XMLHttpRequest, textStatus, errorThrown) {
					//alert(XMLHttpRequest.status);
				}
			});
		}else{
			alert("请输入继续提问内容！");
		}
	}
}

//******************************修改密码*******************************************/
//修改密码初始化
function updatePasswordInfo(){
	var w = $(window).width();
	var w = w - 30;
	$(".tj_input").animate({width:w});
}

/*************************************************公共方法*********************************************************/
//显示错误信息
function showTipMsg(isboolean,idName,message){
	if(isboolean){
		$("#"+idName).html(message);
	}
}

//faq详细处理图片显示
function getImgHref(){
	try{
		$(".imgshow").imgbox({
			'speedIn'		: 0,
			'speedOut'		: 0,
			'alignment'		: 'center',
			'overlayShow'	: true,
			'allowMultiple'	: false
		});
	}catch(e){}
}
//FAQ详细显示图片
function showImg(imgPath){
	if(imgPath.length==20){
		window.location.href=document.getElementById(imgPath).href;
	}else{
		window.location.href="/ckb/baike.do?action=showWapImg&imgPath="+imgPath;
	}
}

//站内回复-更新阅读状态为已阅读
function updateStaticReplyIsRead(problemId,menuName){
	jQuery.ajax({
		type: "POST",
		url: "/ckb/ajax.do?action=doPersonalCenterSubmit",
		data: {"type":"updateIsRead","isPage":"F","readVal":"1",
			"updateType":"read","isUploadAttach":"F",
			"isJson":"T",
			"custId":""+jQuery("#custId").val()+"",
			"problemId":""+problemId+""},
		dataType: "json",
		success: function(result){
			var jsonObj = result;
			if(jsonObj!=""){
				if(jsonObj.resultcode>0){
					if(menuName=="staticReply"){
						//站内回复页面
						jQuery("#hfNews_"+problemId).removeClass("hf_news");
					}else if(menuName=="personalCenter"){
						//个人中心主页面
						if(jsonObj.stationReplyCountNews==0){
							jQuery("#hfNews_"+problemId).removeClass("hf_news");
						}
					}
				}
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
			//alert(XMLHttpRequest.status);
		}
	});
}


//获取天气信息
//isGps : 'T' or 'F'
var cityLowerCase;
function getWeather(isGps){
	//天气预报
	$.ajax({
		type: "GET",
		data: {
			"praiseCustId":""+$('#praiseCustId').val()+"",
			"isGps":""+isGps+""
		},
		url: "/ckb/ajax.do?action=getWeatherInfo",
		success: function(result){
			var jsonData = eval("("+result+")");
			//当天预报
			$("#common_head_weather_page").artTemplate("weatherTmpl",jsonData);
			//未来三天预报
			$("#weatherThreeDays").html($.fn.artTemplateHtml('weatherListTmpl',jsonData));
			cityLowerCase = jsonData.cityLowerCase;
			$("#cityId").value=jsonData.cityId;
			var a_href = $("#search_a").attr("href");
			if(a_href.indexOf("/*/") > 0){
				a_href = a_href.replace("*",jsonData.cityLowerCase);
				$("#search_a").attr("href", a_href);
			}
			initMobileWether();
		}
	});
}

//更新10000知道短信访问日志信息表系统类型
function updateSmsCallerForSystemType(){
	if(jQuery("#webOrWap").val()=="web" && jQuery("#smsLogId").val()!=""){
		var queryParam = {"isFy":"F","dataType":"updateSmsCallerForSystemType",
				"systemType":""+detectOS()+"","smsLogId":""+jQuery("#smsLogId").val()+""};
		jQuery.ajax({
			type: "POST",
			url: "/ckb/ajax/ajaxOperDataSubmit.do",
			data: queryParam,
			success: function(resultData){
				if(resultData.resultCode=="0"){
					
				}
			},error: function(XMLHttpRequest, textStatus, errorThrown) {
				//alert(XMLHttpRequest.status);
			}
		});
	}
}

//地市切换链接
function switchCityForActionUrl(){
	jQuery("#switchCity").change(function(){
		location.href = jQuery(this).find("option:selected").attr("actionUrl");
	});
}
