	var isPresent = jQuery("#isPresent").val();
	if(isPresent!="T"){
		var url = "/ckb/wap/personalInfo/my_ques.html";;
		recordBrowseTrajectoryByUrl(url);
		showWapLoginDiv();
	}else{
		location.href="/ckb/wap/personalInfo/my_ques.html";
	}
}

//查看原图
function showImg(imgPath){
	if(imgPath.length==20){
		window.location.href=document.getElementById(imgPath).href;
	}else{
		window.location.href="/ckb/baike.do?action=showWapImg&imgPath="+imgPath;
	}
}
/**************************************登录页面 开始*****************************************************/
//验证用户输入的账号是否存在,返回提示信息
function checkExistCustForErrorMsg(account,accountPwd,checkType){
	var errorMsg = "";
	//
	jQuery.ajax({
		type: "POST",
		async: false,
		url: "/ckb/ajax.do?action=checkExistCust",
		data: {"checkType":""+checkType+"","account":""+account+""},
		dataType: 'json',
		success: function(result){
			var jsonObj = result;
			if(jsonObj !=null) {
				if(jsonObj.isExistCust=='0'){
					errorMsg = "";
				}else{
					if(checkType=="login"){
						if(jsonObj.isExistCust=='-1'){
							errorMsg = jsonObj.errorMsg;
							isExist = false;
						}else if(jsonObj.isExistCust=='-2'){
							errorMsg = jsonObj.errorMsg;
							isExist = false;
						}
					}else if(checkType=="register"){
						if(jsonObj.isExistCust=='1'){
							errorMsg = jsonObj.errorMsg;
							isExist = false;
						}
					}
				}
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
			//alert(XMLHttpRequest.status);
		}
	});
	return errorMsg;
}

//手机版本站用户登录
function mobileAccountLogin(){
	var divMsgId = "login_error";
	var account = jQuery.trim(jQuery("#account").val());
	var accountPwd = jQuery.trim(jQuery("#accountPwd").val());
	var costCaptchaCode = jQuery.trim(jQuery("#costCaptchaCode").val());
	
	if(account=="" || account==undefined){
		jQuery("#"+divMsgId).html("请输入账号！");
		return false;
	}else if(accountPwd=="" || accountPwd==undefined){
		jQuery("#"+divMsgId).html("请输入密码！");
		return false;
	}else if(costCaptchaCode=="" || costCaptchaCode==undefined){
		jQuery("#"+divMsgId).html("请输入验证码！");
		return false;
	}
	
	var isExist = checkExistCust(account,accountPwd,divMsgId,"login");
	if(isExist){
		var activtyGuessHfFlag = jQuery("#activtyGuessHfFlag").val();
		var url = getCookieName("recordBrowseTrajectory");
		if(activtyGuessHfFlag =="1"){
			url = "/ckb/wap/personalInfo/user_center.html";
		}
		if(url!="" && url!=undefined){
			location.href = url;
		}else{
			//刷新当前页面
			location.reload(true);
		}
	}
}
/**************************************登录页面 结束*****************************************************/

/**************************************注册页面 开始*****************************************************/
var msg = "";
//倒计时
function toCountdownTime(){
	var sendTime = 122;
	for(var i=sendTime;i>=0;i--){
		setTimeout("loadCountdownTime("+i+")",(sendTime-i) * 1000);
	}
}

//显示倒计时
function loadCountdownTime(num){
	msg = num+"秒";
	$("#timeMsg").html(msg);
	if(num<=0){ 
		jQuery("#isOver").val("T");
		jQuery("#isSend").val("F");
		jQuery("#timeMsg").html("");
		jQuery("#captchaMsg").html("");
		jQuery("#sendCaptchaCode").val('');
	}
}

//验证手机号码是否非空或正确输入
function checkPhoneNum(){
	var phoneNum = jQuery("#phoneNum");
	//手机号码验证
	if(phoneNum.val()==""){
		jQuery("#captchaMsg").html("提示：请输入手机号码");
		return false;
	}else{
		var checkStr = /^\d+$/;
		var check = new RegExp(checkStr);
		if(!check.test(phoneNum.val())){
			jQuery("#captchaMsg").html("提示：请输入纯数字的手机号码");
			return false;
		}else{
			if(phoneNum.val().length<11){
				jQuery("#captchaMsg").html("提示：请输入11位手机号码");
				return false;
			}else{
				jQuery("#captchaMsg").html("");
			}
		}
	}
	return true;
}

//注册信息验证
function checkRegister(){
	var phoneNum = jQuery("#phoneNum");
	var userName = jQuery("#userName").val();
	var userPwd = jQuery("#userPwd").val();
	var userPwdMinlength = jQuery("#userPwd").attr("minlength");
	var userPwdMaxlength = jQuery("#userPwd").attr("maxlength");
	var captchaCode = jQuery("#msgCaptchaCode").val();
	
	//手机号码验证
	if(phoneNum.val()==""){
		jQuery("#captchaMsg").html("提示：请输入手机号码");
		return false;
	}else{
		var checkStr = /^[1][358][0-9]{9}$/;
		var check = new RegExp(checkStr);
		if(!check.test(phoneNum.val())){
			jQuery("#captchaMsg").html("提示：请输入正确的手机号码");
			return false;
		}else{
			if(phoneNum.val().length<11){
				jQuery("#captchaMsg").html("提示：请输入11位手机号码");
				return false;
			}else{
				jQuery("#captchaMsg").html("");
			}
		}	
	}
	
	//用户名验证
	if(userName==""){
		jQuery("#captchaMsg").html("提示：请输入用户名");
		return false;
	}else{
		//验证用户是否存在
		var isExistCust = checkExistCust(userName,userPwd,"captchaMsg","register");
		if(!isExistCust){
			return false;
		}else{
			jQuery("#captchaMsg").html("");
		}
	}
	
	//密码验证
	if(userPwd==""){
		jQuery("#captchaMsg").html("提示：请输入密码");
		return false;
	}else{
		if(userPwd!=""){
			if(jQuery("#userPwd").val().length<userPwdMinlength){
				jQuery("#captchaMsg").html("您输入的新密码不能小于"+userPwdMinlength+"数，请重新输入");
				return false;
			}else{
				var msg = validNumOrEnOrStrMsgJs("userPwd");
				if(msg!=""){
					jQuery("#captchaMsg").html(msg);
					return false;
				}
			}
		}else{
			jQuery("#captchaMsg").html("");
		}
	}
	//短信验证码验证
	/**/
	
	if(captchaCode==""){
		jQuery("#captchaMsg").html("提示：请输入短信验证码");
		return false;
	}else{
		var isOver = jQuery("#isOver").val();
		var isSend = jQuery("#isSend").val();
		if(isOver=="T"){
			jQuery("#captchaMsg").html("提示：验证码无效，请重新获取验证码");
			return false;
		}else{
			var checkStr = /^\d+$/;
			var check = new RegExp(checkStr);
			if(!check.test(captchaCode)){
				jQuery("#captchaMsg").html("提示：请输入纯数字的验证码");
				return false;
			}else{
				return true;
				/*
				var sendCaptchaCode = jQuery("#sendCaptchaCode").val();
				
				//alert(captchaCode+"<---->"+sendCaptchaCode);
				if(captchaCode!=sendCaptchaCode){
					jQuery("#captchaMsg").html("提示：验证码有误，请重新输入");
					return false;
				}else{
					jQuery("#captchaMsg").html("");
				}
				*/
			}
		}
	}
	return true;
}
