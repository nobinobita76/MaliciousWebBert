//FAQ一级目录跳转
function skipLvOne(){
	var mobileLvOneId = $("#mobileLvOneId").val();
	var mobileLvTwoId = "";
	var sceneShortName = $("#sceneShortName").val();
	var sceneId = $("#sceneId").val();
	var webCity = $("#web_city").val().toLowerCase();
	if(mobileLvTwoId == ""){
		mobileLvTwoId = 0;
	}
	/*
	window.location.href="/ckb/mobileNewsMedia.do?action=mobileFaqSceneLvTwo&mobileLvOneId="+mobileLvOneId+"&sceneShortName="+sceneShortName;
	*/
	window.location.href="/ckb/wap/faq_scene/"+webCity+"/"+sceneShortName+"/"+ sceneId+"/"+mobileLvOneId+"_"+mobileLvTwoId+".html?isMl=T";
}

//FAQ二级目录跳转
function skipLvTwo(){
	var mobileLvOneId = $("#mobileLvOneId").val();
	var mobileLvTwoId = $("#mobileLvTwoId").val();
	var sceneShortName = $("#sceneShortName").val();
	var webCity = $("#web_city").val().toLowerCase();
	var sceneId = $("#sceneId").val();
	window.location.href="/ckb/wap/faq_scene/"+webCity+"/"+sceneShortName+"/"+sceneId+"/"+mobileLvOneId+"_"+mobileLvTwoId+".html?isMl=T";
	/*window.location.href="/ckb/mobileNewsMedia.do?action=mobileFaqSceneLvTwo&mobileLvOneId="+mobileLvOneId+"&mobileLvTwoId="+mobileLvTwoId+"&sceneShortName="+sceneShortName;*/
}

