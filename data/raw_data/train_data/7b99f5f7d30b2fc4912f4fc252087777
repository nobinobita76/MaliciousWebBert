	/*微信登录*/
	jQuery(".lnpp6 #wxLogin").click(function(){
		if(jQuery("#browserName").val()!="Weixin"){
			jQuery(".login_popup1").hide();
			createQrcodeUrl(true,'',0,0);
		}else{
			loginWeixinByUrlAndIsEncode('wap',jQuery(this).attr("isEncode"),'');
		}
	});
	
	/*关闭动态生成二维码层*/
	jQuery(".qrcodeCanvas_btnpp").click(function(){
		//关闭动态生成二维码层
		jQuery(".qrcodeCanvas_div").hide();
		//显示登录层
		jQuery(".login_popup1").show();
	});

	/*本站用户登录*/
	$("#mobileLogin_a").click(function(){
		mobileAccountLogin();
	});

	/*签到JS*/
	$("#sign_user").click(function(e) {
        signOpertion();
    });

	//热门话题换一批ajax处理
	$("#topicTrade").click(function(){
		changeHotTopic();
	});

	//注册页面关闭事件
	$(".register_close").click(function(e) {
		toBackPage();
    });

	//登录页面关闭事件
	$(".ln_close").click(function(e) {
		toBackPage();
    });

	/*FAQ、知识点是否已解决*/
	jQuery(".yesno .faq_ro").click(function(){
		var valueType = jQuery(this).attr("valueType");
		if(valueType=="N"){
			jQuery("#solveN").show();
			jQuery("#btnSolveN").show();
		}else{
			jQuery("#solveN").hide();
			jQuery("#btnSolveN").show();
		}
		jQuery("#isSolve").val(valueType)
	});

	/*搜索js*/
	$(".searchtitle span").click(function(e) {
		$(this).addClass("on").siblings().removeClass("on");
		var m = $(this).index();
		$(".searchdiv").children("ul").eq(m).show().siblings().hide();
	});

	/*********************************************10000知道手机版分享、返回顶部**********************************************************/
	/*返回顶部JS*/
	$(".backtop").click(function(e) {
        $("html,body").animate({scrollTop:0},100);
    });
	$(window).scroll(function(){
		if ( $(window).scrollTop()>10){
			$(".backtop").fadeIn(500);
		}else{
			$(".backtop").fadeOut(500);
		}
	});

	/*向上滑显示、向下滑隐藏JS*/
	var initTop = 0;
	$(window).scroll(function(){
		var scrollTop = $(document).scrollTop();
		//if($(".floatright").css("display") == "block"){
			if(scrollTop > initTop){
				$(".return").hide();
				/*
				$(".floatright").hide();
				$("#zdicon").hide();
				$(".zdicon_search").hide();
				*/
			}else{
				$(".return").show();
				$(".floatright").show();
				/*
				$("#zdicon").show();
				$(".zdicon_search").show();
				*/
			}
			initTop = scrollTop;
			/*
		}else{
			if(scrollTop > initTop){
				$(".return").hide();
				$("#zdicon").hide();
				$(".zdicon_search").hide();
			}else{
				$(".return").show();
				$("#zdicon").show();
				$(".zdicon_search").show();
			}
			initTop = scrollTop;
		}
		*/
	});

	$("#zdicon").click(function(e) {
		$(this).siblings().toggle();
    });

	/*分享JS*/
	$("#share").click(function(e) {
		$(".sharepupop").show();
	});
	$(".sharepp_close").click(function(e) {
		$(".sharepupop").hide();
	});

	/****************************************************10000知道FAQ*************************************************************/
	//faq明细跳转IPTVTab目录
	$("#sceneItvTab").click(function(){
		skipLvOne();
	});

	//faq明细跳转一级目录
	$("#sceneLvOne").click(function(){
		skipLvOne();
	});
	//faq明细跳转二级目录
	$("#sceneLvTwo").click(function(){
		skipLvTwo();
	});

	/*****************个人中心首页*********************/
	$(".previous_page").click(function(){
		var guessHfFlag = jQuery("#guessHfFlag").val();
		if(guessHfFlag=="1"){
			location.href = "/ckb/wap/news.html";
		}else{
			toBackPage();
		}
	});

});

/**************************************事件加载 开始*****************************************************/
//根据不同请求调用相应的功能
function initFunctionByAction(){
	var actionName = jQuery("#actionName").val();
	if(actionName=="newsMediaMobileIndex" || actionName=="mobileNewsMediaIndexForMultiChannel"){
		//首页方法加载
		initMobileIndex();
	}else if(actionName=="mobileSearch"){
		//搜索事件加载
		initMobileSearch();
	}else if(actionName=="mobileNewsInfo"){
		//手机版新闻列表事件加载
		initMobileNewsInfo();
	}else if(actionName=="mobileNewsContent" || actionName=="articleConentByIdView"
		|| actionName=="mobileKnowledgeDetail" || actionName=="mobileKnowledgeDetailForMultiChannel"){
		//新闻内容或知识点内容页事件加载
		initNewsOrKnowledgeForImgWidth();

		if(actionName=="mobileNewsContent" || actionName=="articleConentByIdView"){
			//新闻内容页事件加载
			initMobileNewsCont();
		}else if(actionName=="mobileKnowledgeDetail"){
			//ajax获取知识点内容
			//getKnowledgeDetailContent();
		}else if(actionName=="mobileKnowledgeDetailForMultiChannel"){
			//更新10000知道短信访问日志信息表系统类型
			updateSmsCallerForSystemType()
		}
	}else if(actionName == "articleConentPlByIdView"){
		var isPresent = jQuery("#isPresent").val();
		if(isPresent!="T"){
			showWapLoginDiv();
		}
	}else if(actionName == "mobileNewsComment"){
			//手机版新闻内容评论页事件加载
			initMobileNewsComment();
	}else if(actionName=="mobileSceneSearchFunction"){
		//手机版各栏目下查询功能
		initMobileSceneSearchFunction();
	}else if(actionName=="mobileFaqSceneLvTwo"){
		//场景FAQ事件加载
		initFaqScene();
	}else if(actionName=="mobileFaqSceneInfo"){
		//faq详细处理图片显示
		getImgHref();
		
		//调用faq搜索接口相关问题加载
		xgQuestion("faq",4);
	}else if(actionName=="mobileFaqDetail"){
		//调用faq搜索接口相关问题加载
		xgQuestion("faq",4);
	}else if(actionName=="mobileNewsMediaLogin" || actionName=="toMobileZhidaoLogin"){
		//登录页面加载事件
		initMobileNewsMediaLogin();
	}else if(actionName=="toNewsMediaMobileUserRegister"){
		//手机版注册事件加载
		initMobileNewsMediaUserRegister();
	}else if(actionName=="toMobileNewsMediaUserPersonalCenter" || actionName=="getWeixinZhidaoUserInfoCallback"){
		//个人中心事件加载
		initMobileNewsMediaUserPersonalCenter();
	}else if(actionName=="toStaticSquaHtml"){
		//故障排查静态页事件加载
		initMobileStaticSquaHtml();
	}else if(actionName=="mobileMyComment"){
		//我的评论事件加载
		initMaobileMyComment();
	}
}

//手机版注册事件加载
function initMobileNewsMediaUserRegister(){
	//注册提交
	$("#register").click(function(){
		accountRegister();
	});
	//发送短信验证码到指定的手机号码
	$("#getCaptchaCode").click(function(){
		var timeMsg = jQuery("#getCaptchaCode").html();
		if(timeMsg == "发送验证码" && checkMobileYdLtDx("checkMobileForYdOrLtOrDx",jQuery("#phoneNum").val().Trim())){
			jQuery("#isOver").val("F");
			jQuery("#isSend").val("T");
			sendMessageCaptchaCode('register_dxyzm');
			//toCountdownTime();
			toCountdownYzmTime("getCaptchaCode","发送验证码",122);
		}
	});
}

//搜索事件加载
function initMobileSearch(){
	var ww = document.body.clientWidth;
	$(".shtext").width(ww-120);
	jQuery(window).resize(function() {
		var w = $(this).width(),
		st = jQuery(".shtext");
		st.width(w - 120);
	});

	jQuery(".shtext").focus(function(e) {
        jQuery(".shbtn2").css("display","inline");
		jQuery(".shbtn1").css("display","none");
    });
	/*
	jQuery(".shtext").blur(function(e) {
        jQuery(".shbtn2").css("display","none");
		jQuery(".shbtn1").css("display","inline");
    });
	*/
	if(jQuery(".adpupop").size()>0){
		var holidayCookie = getCookieName("holidayCookie");
		if(holidayCookie!="" && holidayCookie!=undefined){
			jQuery(".adpupop").hide();
		}else{
			jQuery(".adpupop").show();
		}
	}

	if(jQuery("#mobileSearchAdverDiv").size()>0){
		var wapHolidayCookie = getCookieName("wapHolidayCookie");
		if(wapHolidayCookie!="" && wapHolidayCookie!=undefined){
			$("#mobileSearchAdverDiv").hide();
		}else{
			$("#mobileSearchAdverDiv").show();
		}

		/*节假日关闭js*/
		$("#mobile_search_adver_close").click(function(e) {
			$("#mobileSearchAdverDiv").hide();
			setCookieName("wapHolidayCookie","1")
		});
	}

	$(".shtext").keyup(function(e) {
		if ($(this).val().length > 0)
		{
			$(".shbtn2").css("display","inline");
			$(".shbtn1").css("display","none");
		}
		else
		{
			$(".shbtn2").css("display","none");
			$(".shbtn1").css("display","inline");
		}
	});
	$(".shx").click(function(e) {
		$(".shtext").val("");
		$(".shbtn2").css("display","none");
		$(".shbtn1").css("display","inline");
	});

	/*手机版搜索结果下一页 js*/
	$("#mobileSearchNextPage").click(function(){
		mobileSearchChangeNextPage(undefined);
	});

	/*搜索评价*/
	$("#sspl").click(function(e) {
		$(".ssplbox").toggle();
	});
	$("#ssplyes").click(function(e) {
	   $(".ssplbox").hide();

	   if($(this).hasClass("on")==false && $("#ssplno").hasClass("on")==false){
		   //搜索评价提交
		   searchEvalutionSubmit("ssplyes");
		   $("#result_p").css("color","red");
		   $("#result_p").css("font-size","18px");
		   $("#result_p").html("</br>提交成功，谢谢您的建议！");
		   $(".ssplbtn1").remove();

		   /*
		   $("#ssplyes").addClass("on");
		   $("#ssplno").addClass("on");
		   $("#result_p").html("您对本次的搜索结果为“满意”");
		   */
	   }
	});
	$("#ssplno").click(function(e) {
		if($("#ssplyes").hasClass("on")==false && $("#ssplno").hasClass("on")==false){
			$(".ssplbox").hide();
			$(".ssplbox2").show();
		}else{
			$(".ssplbox").hide();
		}
	});
	$("#pl_textarea").blur(function(){
		if($(this).val()=="" || $(this).val()=="搜索不到？体验不好？告诉我们您的意见吧"){
			$(this).val('搜索不到？体验不好？告诉我们您的意见吧');
			$(this).css("color","#ccc");
		}
	});
	$("#pl_textarea").focus(function(){
		if($(this).val()=="搜索不到？体验不好？告诉我们您的意见吧"){
			$(this).val('');
		}
		$(this).css("color","black");
	});
	$("#pl_submit").click(function(e) {
		if($("#ssplyes").hasClass("on")==false && $("#ssplno").hasClass("on")==false){
			//搜索评价提交
			searchEvalutionSubmit("pl_submit");
			$("#result_p").css("color","red");
		   	$("#result_p").css("font-size","18px");
		   	$("#result_p").html("</br>提交成功，谢谢您的建议！");
		   	$(".ssplbtn1").remove();
		   	/*
			$("#ssplyes").addClass("on");
	   		$("#ssplno").addClass("on");
			$("#result_p").html("您对本次的搜索结果为“不满意”");
			*/
		}
	});
	$(".sspla3").click(function(e) {
		$(".ssplbox2").hide();
		$(".ssplbox").show();
	});
	$(".sspla2").click(function(e) {
		$(".ssplbox2").hide();
	});

	//搜索结果列表滚动分页
	initDroploadByFunction("#mobileSearchNextPage",'','','50',mobileSearchChangeNextPage);
}

//新闻内容页事件加载
function initNewsOrKnowledgeForImgWidth(){
	$(".imgshow img").each(function(){
		$(this).parent().parent().css("text-indent","0em");
		$(this).css("width","100%");
		$(this).css("height","auto");
	});
}

//首页方法加载
function initMobileIndex(){
	if($("#mobileIndexAdverDiv").size()>0){
		var please_qrj_cookie = getCookieName("please_qrj");
		if(please_qrj_cookie!="" && please_qrj_cookie!=undefined){
		}else{
			$("#mobileIndexAdverDiv").show();
		}
		//$(".iptvad1 img").attr("onclick","toWapTargetUrlPage('http://gd.zhidao.189.cn/ckb/wap/ywsd.html?ut=2')");
		$("#mobile_index_adver_close").click(function(e) {
			$("#mobileIndexAdverDiv").hide();
			setCookieName("please_qrj","please_qrj");
		});
	}
	if($("#currentPageInfo").val() != "T"){
		window.location.reload();
	}
	var tabsSwiper = new Swiper('.swiper-container',{
		speed:500,
		onSlideChangeStart: function(){
			$(".nav .on").removeClass('on');
			$(".nav .nav_cell").eq(tabsSwiper.activeIndex).addClass('on');

			if($(".nav .nav_cell").eq(3).hasClass("on"))
			{
				$(".pn1").animate({left:"-74px"});
				$(".pn2").animate({left:"0px"});
				$(".pn3").animate({left:"92px"});
				$(".pn4").animate({left:"184px"});
				$(".pn5").animate({left:"276px"});
				$(".pn6").animate({left:"368px"});
			}
			else if($(".nav .nav_cell").eq(4).hasClass("on"))
			{
				$(".pn1").animate({left:"-148px"});
				$(".pn2").animate({left:"-74px"});
				$(".pn3").animate({left:"18px"});
				$(".pn4").animate({left:"110px"});
				$(".pn5").animate({left:"202px"});
				$(".pn6").animate({left:"294px"});
			}
			else if($(".nav .nav_cell").eq(5).hasClass("on"))
			{
				$(".pn1").animate({left:"-222px"});
				$(".pn2").animate({left:"-148px"});
				$(".pn3").animate({left:"-74px"});
				$(".pn4").animate({left:"18px"});
				$(".pn5").animate({left:"110px"});
				$(".pn6").animate({left:"202px"});
			}
			else
			{
				$(".pn1").animate({left:"0px"});
				$(".pn2").animate({left:"74px"});
				$(".pn3").animate({left:"166px"});
				$(".pn4").animate({left:"258px"});
				$(".pn5").animate({left:"350px"});
				$(".pn6").animate({left:"442px"});
			}
			//自适应高度
			var sceneType = $(".on").attr("sceneType");
			homePageHeightAdapt(sceneType);
		}
	});

	$(".nav .nav_cell").on('touchstart mousedown',function(e){
		e.preventDefault()
		$(".nav .on").removeClass('on');
		$(this).addClass('on');

		tabsSwiper.swipeTo($(this).index());

	});

	$(".nav .nav_cell").click(function(e){
		e.preventDefault();
	});
}

//我的业务数据查询事件加载
function initMobileSearchMyBusiness(){
	var urlType = jQuery("#urlType").val();
	if(urlType=="package_details"){
		jQuery(".yw_ul li").click(function(){
			var id_ = jQuery(this).attr("id");
			var divId = jQuery("#"+id_).attr("divId");
			jQuery("#"+divId).show();
			var type = jQuery(this).attr("type_");

			if(type!="hua_fei_month"){
				//我的业务数据查询
				mobileMyBusiness(divId,type);
			}else{
				//我的业务数据查询
				mobileMyBusinessHuaFeiByMonth("hua_fei_month","0");
			}
		});

		/*地市选择JS*/
		$(".yw_city").click(function(e) {
			$(".infods").show();
		});
		$(".infods ul li a").click(function(e) {
			$(this).parents(".infods").hide();
			$(".yw_city em").text($(this).text());
		});
		var x = $(".yw_data i.on").width();
		/*定位已用JS*/
		var y = x - 65;
		if ( y < 65)
		{
			$(".yw_took").animate({left:0});
		}
		else
		{
			$(".yw_took").animate({left:y});
		}
	}
}

//手机版新闻列表事件加载
function initMobileNewsInfo(){
	// $("#owl-example").owlCarousel();
	//手机版显示下10条新闻列表数据
	$(".showmore a").click(function(e) {
		mobileAppendNextNewsInfo(undefined);
	});
	//4条新闻列表数据
	mobileAppendNextNewsInfo(undefined);
	//广告
	mobileLmAdverInfo();
	
	/*classify*JS*/
	$(".classify_icon").click(function(e) {
		if($(this).hasClass("cy_up"))
		{
			$(this).siblings("ul").children("span").show();
			$(this).removeClass("cy_up").addClass("cy_down");
		}
		else
		{
			$(this).siblings("ul").children("span").hide();
			$(this).removeClass("cy_down").addClass("cy_up");
		}
	});
	
	//异步获取热门搜索词
	getHotSearch();
	
	//文章列表滚动分页
	initDroploadByFunction(".index_news",'','','50',mobileAppendNextNewsInfo);
	
	// var currentPage = jQuery("#currentPage").val();
	//图片延迟加载
	// $(".index_box .index_box_img_"+currentPage+" img").lazyload({
		// effect:"fadeIn",
		// threshold : 1000
	// });//.removeClass("loadingimg").removeAttr("src").addClass("index_box_img_css");
}

function initMobileNewsComment(){
	$(".arpp_colse").click(function(e) {
		 $(".answerpopup").hide();
		 $("#writeframe").html("");
	});
	$(".write .write1 input.writetext").click(function (e){
		if($("#isPresent").val() =="T"){
		}else{
			showWapLoginDiv();
		}
	});
}
//手机版新闻内容页事件加载
function initMobileNewsCont(){
	$(".arpp_colse").click(function(e) {
         $(".answerpopup").hide();
		 $("#writeframe").html("");
    });
	/*发表评论JS*/
    $("#publish").click(function(e) {
		var isPresent = jQuery("#isPresent").val();
		if(isPresent!="T"){
			showWapLoginDiv();
		}else{
        	$(".publishpopup").show();
		}
    });
	$(".phpp_colse").click(function(e) {
         $(".publishpopup").hide();
		 $("#writeframe2").html("");
    });

	$("#writeframe").keyup(function(){
		$("#writeSurplus").html("剩余"+(eval(500)-eval(strlen($("#writeframe").html())))+"个字");
	});

	$("#writeframe2").keyup(function(){
		$("#writeSurplus2").html("剩余"+(eval(500)-eval(strlen($("#writeframe2").html())))+"个字");
	});
	
	/*文章图片延迟加载*/
	jQuery(".artcleLazyloadImg").lazyload({
		effect: "fadeIn"
	});

	//分享
    jQuery('.pin3').click(function(e) {
    	jQuery('.share').show();
    });
    jQuery('.share-close').click(function(e) {
    	jQuery('.share').hide();
    });
	//发表
	/*
    jQuery('.pin1').click(function(e) {
    	jQuery('.Publish').show()
    });
    */
    jQuery('.cancel').click(function(e) {
		jQuery('.Publish').hide();
    });
	
    jQuery('.footer-left').click(function(e) {
    	if($("#isPresent").val() =="T"){
    		jQuery('.Publish').show();
		}else{
			showWapLoginDiv();
		}
    });
    jQuery('.cancel').click(function(e) {
		jQuery('.Publish').hide();
    });
    
    jQuery('.reply').click(function(e) {
    	jQuery('footer').hide()
        jQuery('#footer2').show()
    });
    
    jQuery("#write-comments").attr("href",jQuery("#write-comments").attr("href")+"&redirectUrl="+encodeBase64(location.href));
	
	//文章内容加载
	//getArticleContent();
}

//公共头部天气预报事件加载
function initMobileWether(){
	/*天气弹出JS*/
    $(".weather").click(function(e) {
        $(".weather_popup1").toggle();
    });
	$("#common_head_wether_close").click(function(e) {
        $(".weather_popup1").hide();
    });
	$("#common_head_wether_fds").click(function(e) {
        $(this).parents(".fweather1").hide().siblings(".fcity1").show();
    });
	$("#common_head_setcity_no").click(function(e) {
        $(this).parents(".fcity1").hide().siblings(".fweather1").show();
    });
	//公共头部获取对应的城市数据
	$("#common_head_wholeCity").change(function(){
		getMobileWholeCountryCity("common_head_wholeCity","common_head_weatherCity");
	});

	$("#common_head_setcity_ok").click(function(e) {
       saveMobileWeatherCityForOperatorUser("common_head_wholeCity","common_head_weatherCity","common_head_weather_wap","设置成功","关闭");
	   $("#common_head_setcity_no").parents(".fcity1").hide().siblings(".fweather1").show();
    });

	/*导航栏JS*/
	$(".nav_cell").click(function(e) {
        $(this).addClass("on").siblings().removeClass("on");
		var m = $(this).index();
		$("#banner").children().eq(m).show().siblings().hide();
    });

	/*登录层的天气预报事件js*/
	$("#login_wether_fds").click(function(e) {
		$(".weather_popup2").show();
		$(this).removeClass("fr1").addClass("fr1_on");
    });
	$("#login_wether_close").click(function(e) {
        $(".weather_popup2").hide();
		$(".fr1_on").removeClass("fr1_on").addClass("fr1");
    });
	$(".fds").click(function(e) {
        $(this).parents(".fweather2").hide().siblings(".fcity2").show();
    });
	$("#login_setcity_no").click(function(e) {
        $(this).parents(".fcity2").hide().siblings(".fweather2").show();
    });

	//登录层获取对应的城市数据
	$("#login_wholeCity").change(function(){
		getMobileWholeCountryCity("login_wholeCity","login_weatherCity");
	});

	$("#login_setcity_ok").click(function(e) {
         saveMobileWeatherCityForOperatorUser("login_wholeCity","login_weatherCity","weather_info_wap","设置成功","关闭");
		 $("#login_setcity_no").parents(".fcity2").hide().siblings(".fweather2").show();
		 $(".fr1_on").removeClass("fr1_on").addClass("fr1");
    });

	var isPresent = $("#isPresent").val();
	if(isPresent=="T"){
		$(".ln2 a").attr("href","/ckb/wap/personalInfo/user_center.html");
	}

	/*公共提示信息操作 js*/
	operationCommonDiv();
}

//手机版各栏目下查询功能
function initMobileSceneSearchFunction(){
	var urlType = jQuery("#urlType").val();
	if(urlType=="jtzw"){
		//家庭组网
		var w = $(window).width();
		var x = w - 140;
		$(".skill_st").animate({width:x});

		/*家庭组网查询条件获取*/
		jQuery("#jtzw_li .skill_st").each(function(i){
			var id_ = "jtzwUl_"+i;
			var jtzwVal = jQuery("#"+id_+" option:selected").val();

			if(i==0){
				jQuery("#jtzwParamVal0").val(jtzwVal);
			}
			if(i==1){
				jQuery("#jtzwParamVal1").val(jtzwVal);
			}
			if(i==2){
				jQuery("#jtzwParamVal2").val(jtzwVal);
			}

			jQuery("#jtzwParamVal").val(jQuery("#jtzwParamVal").val()+","+jtzwVal);
		});

		jQuery("#jtzwUl_0").change(function(e) {
			var jtzwVal = $(this).val();
			//alert("change="+jtzwVal)
			jQuery("#jtzwParamVal0").val("");
			jQuery("#jtzwParamVal0").val(jtzwVal);
		});
		jQuery("#jtzwUl_1").change(function(e) {
			var jtzwVal = $(this).val();
			jQuery("#jtzwParamVal1").val("");
			jQuery("#jtzwParamVal1").val(jtzwVal);
		});
		jQuery("#jtzwUl_2").change(function(e) {
			var jtzwVal = $(this).val();
			jQuery("#jtzwParamVal2").val("");
			jQuery("#jtzwParamVal2").val(jtzwVal);

		});

		/**家庭组网查询**/
		jQuery("#jtzwSearch").click(function(){
			searchMobileJtzwTagCriteriaResult();
		});
	}else if(urlType=="discount_activities"){
		//优惠活动
		var w = $(window).width();
		var x = w - 140;
		$(".skill_st").animate({width:x});
	}else if(urlType=="pick_search"){
		//随手查
		var w = $(window).width();
		var x = w - 140;
		$(".skill_st").animate({width:x});
		var y = w - 140;
		$(".skill_tt1").animate({width:y});
		var z = w - 200;
		$(".skill_tt2").animate({width:z});
		var a = w -145;
		$(".detail").animate({width:a});
		/*
		$("#sscbtn1").click(function(e) {
			$("#ssc").animate({left:0},500);
		});
		$("#sscbtn2").click(function(e) {
			$("#ssc").animate({left:w},500);
		});
		*/
	}else if(urlType=="search_phone"){
		//查手机
		var w = $(window).width();
		var x = w - 140;
		$(".skill_st").animate({width:x});
	}else if(urlType=="roaming_guide"){
		//漫游锦囊
		var w = $(window).width();
		var x = w - 140;
		$(".skill_st").animate({width:x});
	}else if(urlType=="topic"){
		//话题
	}
}

//场景FAQ事件加载
function initFaqScene(){
	var mobileLvOneId = $("#mobileLvOneId").val();
	if(mobileLvOneId != ""){
		var faqbId = "faqb"+ mobileLvOneId;
		$("#"+faqbId).siblings(".faqa1").click();
	}
	/*ITV*/
	$(".fad_itv li").click(function(e) {
		$(this).addClass("on").siblings().removeClass("on");
		var m = $(this).index();
		$("#itvlist").children().eq(m).show().siblings().hide();
		var tabId = $(this).attr("tabid");
		var sceneShortCssType = $("#sceneShortCssType").val();
		//ajax加载IPTVTab数据
		showItvTabLvOne(tabId,sceneShortCssType);
	});

	/*下拉场景切换JS*/
	$(".faq_title1 span").click(function(e) {
		if($(this).hasClass("tipdwon"))
		{
			$(".faq_pupop").show();
			$(this).removeClass("tipdwon").addClass("tipup");
		}
		else
		{
			$(".faq_pupop").hide();
			$(this).removeClass("tipup").addClass("tipdwon");
		}
	});


	$(".faqb").on("click","span",function(e) {
		var productId = $(this).attr("productId");
		var faqchtml = $(this).siblings(".faqc").html().trim();
		var sceneShortName = $("#sceneShortName").val();
		var webCity = $("#web_city").val();
		if(faqchtml == null || faqchtml == ""){
			if(productId!=null && productId!=""){
				jQuery.ajax({
					type: "POST",
					url: "/ckb/mobileAjax.do?action=newsMediaMobileFaq",
					data: {"productId":""+productId+"","sceneShortName":""+sceneShortName+"","webCity":""+webCity},
					success: function(data){
						var data2 = data.trim();
						$("#faqc"+productId).html(data2);
					}
				});
			}
		}
		if($(this).siblings().css("display") == "none")
		{
			$(this).siblings(".faqc").show();
			$(this).parent(".faqb1").siblings().children(".faqc").hide();
			if($(this).hasClass("faqb_icon1"))
			{
				$(this).removeClass("faqb_icon1");
				$(this).addClass("faqb_icon1up");
				$(this).parent(".faqb1").siblings().children("span").removeClass("faqb_icon1up").addClass("faqb_icon1");
			}
			else
			{
				$(this).removeClass("faqb_icon1up").addClass("faqb_icon1");
				$(this).parent(".faqb1").siblings().children("span").removeClass("faqb_icon1").addClass("faqb_icon1up");
			}
		}
	});

	$(".no").click(function(){
		if($(".judge1").html()=="以上内容是否解决了您的问题呢？"){
			$(".no").unbind("hover");
			$(this).css("color","#fff");
			$(this).css("background","#8bb900");
			$(".judge3").css("display","");
		}
	});
}

//登录页面加载事件
function initMobileNewsMediaLogin(){
	changeHotTopic();
}

//个人中心事件加载
function initMobileNewsMediaUserPersonalCenter(){
	var urlType = jQuery("#urlType").val();
	if(urlType=="user_center"){
		//我的资料
		myDatumInfo();

		//地址加载
		$("#placeResidenceSheng").change(function(){
			var id_ = $("#placeResidenceSheng option:selected").attr("id");
			changeCity(id_,"PLACE_RESIDENCE","placeResidenceCity");
		});
		//提交个人资料
		$(".zl_submit").click(function(){
			doWabSavePersonalInfo();
		});
		$(".myDatum_ok_popup").click(function(){
			$(this).parents(".infopopup").hide();
			window.location.reload();
		});

		/*infotab切换*/
		$(".zl_tab span").click(function(e) {
			$(this).addClass("on").siblings().removeClass("on");
			var m = $(this).index();
			$("#zl_list").children().eq(m).show().siblings().hide();
		});

		$(".infotext").focus(function(){
			$(this).siblings(".info_xiugai").css("display","none");
			$(this).siblings(".info_ok").css("display","block");
		})
		$(".infotext").blur(function(){
			$(this).siblings(".info_ok").css("display","none");
			$(this).siblings(".info_xiugai").css("display","block");
		})
		/*性别JS*/
		$("#sex").click(function(e) {
			$("#sexpopup").show();
		});
		$(".sexpopup p").click(function(e) {
			$("#gender").val($(this).attr("id"));
			$(this).children("span").removeClass("zl_no").addClass("zl_ok");
			$(this).siblings("p").children("span").removeClass().addClass("zl_no");
			$("#sexpopup").hide();
			var t = $(this).text();
			$("#sex input").val(t);
		});
		/*兴趣爱好JS*/
		$("#hobby").click(function(e) {
			$("#hobbypopup").show();
		});
		$(".hobbypopup p").click(function(e) {
			var M =$(this).children("span");
			if(M.hasClass("zl_no"))
			{
				M.removeClass("zl_no").addClass("zl_ok");
			}
			else
			{
				M.removeClass("zl_ok").addClass("zl_no");
			}
		});
		var interestsText ="";
		$(".hobbybtn").click(function(e) {
			var interestsText = "";
			$(".hobbypopup .zl_ok").each(function(i){
				if(i !=0){
					interestsText += (","+ $(this).parent('p').text());
				}else{
					interestsText = $(this).parent('p').text();
				}
			});
			$("#interests").val(interestsText);
			$("#hobbypopup").hide();
		});
	}else if(urlType=="update_pwd"){
		//修改密码
		updatePasswordInfo();

		$(".mm_submit").click(function(){
			mobileNewsUpPassword();
		});
	}else if(urlType=="package_details"){
		//我的业务数据查询事件加载
		initMobileSearchMyBusiness();
	}else if(urlType=="my_forum"){
		//我的论坛

		//获取最新消息
		$("#newMsg").click(function(){
			getMyForumByTabType("newMsg","newMsgData");
		});
		//获取我的提问
		$("#myAsk").click(function(){
			getMyForumByTabType("myAsk","myAskData");
		});
		//我的回答
		$("#myAnswer").click(function(){
			getMyForumByTabType("myAnswer","myAnswerData");
		});
		$(".lt_tab span").click(function(e) {
			$(this).addClass("on").siblings().removeClass("on");
			var m = $(this).index();
			$("#lt_list").children().eq(m).show().siblings().hide();
		});
	}else if(urlType=="my_dy"){
		//我的订阅

		$(".dy_submit").click(function(){
			saveWapMyDyOpertion();
		});

		$(".dy_right span").click(function(e) {
			if($(this).hasClass("ydy"))
			{
				$(this).text("未订阅");
				$(this).removeClass("ydy").addClass("wdy");
			}
			else
			{
				$(this).text("已订阅");
				$(this).removeClass("wdy").addClass("ydy");
			}
		});
		$(".mySubscription_ok_popup").click(function(e) {
			$(this).parents(".infopopup").hide();
			window.location.reload();
		});
	}else if(urlType=="ques_reply"){
		//站内回复

		$(".answer_ok").click(function(e) {
			var	problemId = $(this).attr("id");
			//问题反馈-站内回复更新已解决问题状态值
			solveWapQuestion(problemId);
		});

		$(".hf_downup").click(function(e) {
			if($(this).hasClass("hf_down"))
			{
				var problemId = jQuery(this).attr("id");
				//站内回复-更新阅读状态为已阅读
				updateStaticReplyIsRead(problemId,"staticReply");

				$(this).parent().next().show();
				$(this).removeClass("hf_down").addClass("hf_up");
			}
			else
			{
				$(this).parent().next().hide();
				$(this).removeClass("hf_up").addClass("hf_down");
			}
		});

		$(".answer_ok").click(function(e) {
			$(this).parents(".hf_smallbox").hide()
			$(this).parents(".hf_smallbox").prev().children(".hf_downup").removeClass("hf_up").addClass("hf_down");
			$(this).parents(".hf_smallbox").siblings("a").children().show();
			$(this).parent(".hf_btn").siblings().children(".hf_unresolved").hide();
			$(this).siblings().hide();
			$(this).css("background","#ccc");
		});
	}else if(urlType=="product_binding"){
		/*产品绑定*/

		var isPhoneBind = $("#isPhoneBind").val();
		if(isPhoneBind!='T'){
			$(".cp_submit").click(function(e) {
				saveBindProductOpertionForCust();
			});
		}

		//获取验证码
		//发送短信验证码到指定的手机号码
		$(".cp_click").click(function(){
			var timeMsg = jQuery("#timeMsgForProductBind").html();
			if(timeMsg == "" && checkMobilePhoneForSendMessageCaptchaCode("telephone","telephoneMeg")){
				var phoneNumVal = jQuery("#telephone").val();
				sendMessageCaptchaCodeForPhone("register_dxyzm","sendCaptchaCode","telephoneMeg",phoneNumVal);
				toCountdownTimeForSend(122,"timeMsgForProductBind","telephoneMeg","sendCaptchaCode");
			}
		});
		//关闭提示框
		$(".bangding_ok_popup").click(function(e) {
			$(this).parents(".infopopup").hide();
			$(".cp_p1").hide();
			$("#tel").hide();
			$(".cp_submit").parent().hide();
			$(".cp_tel").text($("#cp_telnum").val());
			window.location.reload();
		});

		$(".cp_submit").click(function(e) {
			$("#cp_popup").show();
		});
	}else if(urlType=="my_ques"){
		/*我要提问*/

		myIssueFeedbackInfo();
		$("#info_ask .fk_left a").click(function(e) {
			var selectSceneShortName = $(this).attr("id");
			$("#selectQuesType").val(selectSceneShortName);
			$("#info_ask").hide();
			$("#info_submit").slideDown();
		});
		$("#filebtn").change(function(){
			jQuery("#textfield").val(jQuery(this).val());
		});

		//提交
		$("#submit_right_a").click(function(e) {
			doQuestionFeedback();
		});
			//上传附件操作
		$("#uploadAttach").click(function(){
			var uploadFile = $("#textfield").val();
			if(uploadFile!="" && uploadFile!=undefined){
				if(uploadFile.lastIndexOf("\\")!=-1){
					var uploadFileSub = uploadFile.substring(uploadFile.lastIndexOf('\\')+1);
					$(".att_del").show();
					//$("#filebtn").attr("disabled","disabled");
					$("#uploadAttach").attr("disabled","disabled");
					$("#att_del").html("删除");
					$("#uploadAttachName").html(uploadFileSub);
				}
			}
		});

		//删除附件
		$("#att_del").click(function(){
			$(this).hide();
			$("#filebtn").val('');
			$("#textfield").val('');
			$("#uploadAttachName").html("");

			$("#filebtn").removeAttr("disabled");
			$("#uploadAttach").removeAttr("disabled");
		});

		/*文本域JS*/
		$(".tj_ta").focus(function(e) {
			$(this).siblings("label").hide();
		});
		$(".tj_ta").blur(function(e) {
			if ($(this).val().length > 0)
			{
				$(this).siblings("label").hide();
			}else
			{
				$(this).siblings("label").show();
			}
		});
	}else if(urlType=="my_read"){
		/*我的阅读与我的订阅切换js*/
		$(".bothtitle span").click(function(e) {
			$(this).addClass("yddy").siblings().removeClass("yddy");
			var n = $(this).index();
			$(".both1").children().eq(n).show().siblings().hide();

			//查询我的阅读或我的订阅数据
			getMySubscription($(this).attr("id"));
		});
	}else if(urlType=="my_comment"){
		//我的评论
		$(".mypl_show").click(function(e) {
			$(this).hide().next().show();;
		});
		$(".mypl_hide").click(function(e) {
			$(this).parent("div").hide().prev().show();
		});
	}
}

