		function showLoginDivByLink(isOpen,isEncode,url,redirectUrl){toTargetUrlForLink(isOpen,isEncode,url,redirectUrl)}
		function showRegisterDivByLink(isOpen,isEncode,url,redirectUrl){toTargetUrlForLink(isOpen,isEncode,url,redirectUrl)}
		function initDroploadByFunction(id_,domRefreshStr,domLoadStr,thresholdStr,callback){
			if(domLoadStr=="1"){
				domLoadStr = '<div class="dropload-load"><span class="loading_drop"></span>加载中...</div>'
			}
			var currentPage = jQuery("#currentPage").val();
			var lastPage = jQuery("#lastPage").val();
			if(currentPage>=lastPage){
				jQuery("#isEnd").val("true");
			}
			jQuery(id_).dropload({
		        scrollArea : window,
		        threshold:thresholdStr,
		           domDown : {                                                          // 下方DOM
		               domClass   : 'dropload-down',
		               domRefresh : domRefreshStr,
		               domLoad    : domLoadStr,
		               domNoData  : '<div class="dropload-noData"></div>'
		           },
		        loadDownFn : function(me){
		        	//console.log(jQuery("#isEnd").val());
					if(jQuery("#isEnd").val()!="true"){
		        		callback(me);
		      		}else{
						jQuery(".dropload-down").attr("style","height:0px");
		      			// 锁定
						me.lock();
					   	// 无数据
					   	me.noData();
					   	me.resetload();
		      		}
		        }
		    });
		}
		function jsonStringify(data,space){
		    var seen=[];
		    return JSON.stringify(data,function(key,val){
		        if(!val||typeof val !=='object'){
		            return val;
		        }
		        if(seen.indexOf(val)!==-1){
		            return '[Circular]';
		        }
		        seen.push(val);
		        return val;
		    },space);
		}

