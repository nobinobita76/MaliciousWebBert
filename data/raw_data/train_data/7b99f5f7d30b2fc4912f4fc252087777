		}
	});
	return cKnlgCreateTime
}

//验证图片格式
function checkImgSuffixFormat(imgFile){
	var isReturn = "0";
	var imgArray = new Array("jpg","bmp","png","JPEG","BMP","PNG");
	var img = new Image();
	img.src =  imgFile;
	if(imgFile==null || imgFile==""){
		isReturn = "1";
		return;
	}

	if(imgFile.lastIndexOf('.')!=-1){
		var imgSuffix = imgFile.lastIndexOf('.');
		var imgSuffixSub = imgFile.substring(imgSuffix+1);
		//alert(imgSuffixSub);

		for(var i=0;i<imgArray.length;i++){
			if(imgSuffixSub!=imgArray[i]){
				isReturn = "2";
			}else{
				imgSuffix = "0"
				break;
			}
		}

		if(imgSuffix=="1"){
			alert("请选择要上传的图片文件进行操作");
			return;
		}else if(imgSuffix=="2"){
			alert("请选择以“.jpg”、“.bmp”、“.png”为后缀的图片文件");
			return;
		}else{
			isReturn = "0";
		}
	}
	if(img.fileSize > 300*1024){
		alert("请选择300k以下的图片");
		isReturn = "1";
	}
	if(isReturn=="0"){
		return true;
	}else{
		return false;
	}
}

//更换头像
function uploadUserImg(fileBrowser) {
	jQuery("#upgradeImg").attr("src","/ckb/images/mobileV2_10000/ia/loading.gif");
	jQuery("#tempImg").val(fileBrowser.value);
	if(checkImgSuffixFormat(jQuery("#tempImg").val())){
		$('#custForm').ajaxSubmit({
			type:"post",
			url: '/ckb/mobileIa.do?action=mobileIaUpLoadFile&filePath'+jQuery("#tempImg").val(),
			success: function (data) {
				var path = data;
				var htmlReg = new RegExp("\<.*?\>","i");
				var m=htmlReg.exec(path);
				path=path.replace(m,"");
				pathRep = path.replace("</pre>","");
				jQuery("#upgradeImg").attr("src",pathRep);
			}
		});
	}
}

/***************************************10000知道手机版论坛****************************************************/
//提交论坛问题
function submitProblem(){
	if(verifyProblemData()){
		jQuery("#submitPopupDiv").show();
		
		var type = $("#forumZqType option:selected").val();
		type = type==""?"hd":type;
		jQuery("#type").val(type.toUpperCase());
		jQuery.ajax({
			type: "POST",
			async: false,
			url: "/ckb/mobileAjax.do?action=mobileForumSubmitQuestion",
			data : jQuery('#raiseQuestionFrom').serialize(),
			success: function(result){
				if(result!= null && result!= ""){
					//alert("提交成功！");
					
					console.log("type="+type);
					
					var url = "/ckb/wap/forum/"+ type.toLowerCase() +"/"+ result +"/answer_ques.html";
					updateHtmlMsgById("submit_msg_p","您的帖子提交成功！");
					jQuery("#submitPopupDiv").show();
					toTargetUrlPageById("stpp_btn_close",url,2,"submitPopupDiv");

					//window.location.href="/ckb/wap/forum/"+ type.toLowerCase() +"/"+ result +"/answer_ques.html";
					//window.location.reload();
				}
			},error: function(XMLHttpRequest, textStatus, errorThrown) {
				//alert(XMLHttpRequest.status);
			}
		});
	}
}

