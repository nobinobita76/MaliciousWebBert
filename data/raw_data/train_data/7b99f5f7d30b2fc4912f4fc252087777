			if(result != null){
				$("#"+type+"_result").html(result);
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
			//alert(XMLHttpRequest.status);
		}
	});
}

/******************************************我的提问********************************************************/
//问题反馈
function doQuestionFeedback(){
	if(validationQuesSubmit()){
		/**/
		var param = "";
		var isUploadAttach = "F";
		var uploadAttach = jQuery("#textfield").val();
		if(uploadAttach!="" && uploadAttach!=undefined){
			isUploadAttach = "T";
			param+="&filePath="+uploadAttach;
		}
		param+="&isUploadAttach="+isUploadAttach;
		jQuery("#tj_submit p").html("问题成功提交");
		$('#feedbackQuesForm').ajaxSubmit({
			type:"post",
			url: '/ckb/ajax.do?action=doPersonalCenterSubmit&type=quesFeedback'+param,
			success: function (data) {
				var path = data;
				var htmlReg = new RegExp("\<.*?\>","i");
				var m=htmlReg.exec(path);
				var pathRep=path.replace(m,"");
				var result = pathRep.replace("</pre>","");
				if(result>0){
					$("#tj_submit").show();
				}else if(result==-99){
					jQuery("#tj_submit p").html("存在恶意操作数据！");
					$("#tj_submit").show();
				}else{
					jQuery("#tj_submit p").html("操作失败！");
					$("#tj_submit").show();
				}
			},error: function(XMLHttpRequest, textStatus, errorThrown) {
				//alert(XMLHttpRequest.status);
			}
		});
	}
}

/******************************************手机版搜索下一页********************************************************/
//手机版搜索结果下一页
function mobileSearchChangeNextPage(me){
	var nextPage = jQuery("#nextPage").val();
	var eachPage = jQuery("#eachPage").val();
	var rowCount = jQuery("#rowCount").val();

	var lastPage = jQuery("#lastPage").val();
	var currentPage = jQuery("#currentPage").val();
	var key = jQuery("#keyword").val();
	var keyEncrypted = encryptedString(key);
//async: false,
	jQuery("#mobileSearchNextPage_i").html("加载中,请稍候...");
	jQuery.ajax({
		type: "POST",
		url: "/ckb/mobileAjax.do?action=mobileSearchNextPage",
		data: {"selTab":""+jQuery("#selTab").val()+"","keyword":""+keyEncrypted+"",
			"cityId":""+jQuery("#web_city_id").val()+"",
			"currentPage":""+nextPage+"","eachPage":""+eachPage+"",
			"forumCount":""+jQuery("#forumCount").val()+"",
			"rowCount":""+rowCount+"","currentCount":""+jQuery("#rownum").val()+""
		},
		success: function(result){
			if(result!=""){
				jQuery("#mobileSearchNextPage_i").html("下一页");
				jQuery("#searchul li:last").after(result);
				if((parseInt(currentPage)+1)==lastPage){
					jQuery(".searchbtn").css("display","none");
					jQuery("#isEnd").val("true");
				}

				if(me != undefined){
					me.resetload();
				}
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
			//alert(XMLHttpRequest.status);
		}
	});
}

//我的业务数据查询
function mobileMyBusiness(id_,type){
	var monthPrev = jQuery("#monthPrev").val();
	jQuery.ajax({
		type: "POST",
		url: "/ckb/mobileAjax.do?action=mobileSearchMyBusinessForCust",
		data: {"type":""+type+"","accessNum":""+jQuery("#accessNum").val()+"",
			"monthPrev":""+monthPrev+""},
		success: function(result){
			if(result!=""){
				if(type=="hf_contrast"){
					jQuery("#"+id_).html(result);
				}else{
					jQuery("#"+id_).html(result);
				}

				jQuery(function(){
					//流量充值短信
					jQuery("#ll_cz_a").click(function(){
						jQuery("#isReload").val("1");
						sendMessagMobileCzForPhone("ll_cz",jQuery("#accessNum").val());
					});

					//短信充值短信
					jQuery("#dx_cz_a").click(function(){
						jQuery("#isReload").val("1");
						sendMessagCzForPhone("dx_cz",jQuery("#accessNum").val());
					});
				});
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
			//alert(XMLHttpRequest.status);
		}
	});
}

//我的业务-话费类情况数据查询
function mobileMyBusinessHuaFeiByMonth(type,monthVal){
	var monthPrev = jQuery("#monthPrev").val();
	if(monthVal!="0"){
		monthPrev = monthVal;
	}
	jQuery.ajax({
		type: "POST",
		url: "/ckb/mobileAjax.do?action=mobileSearchMyBusinessForCust",
		data: {"type":""+type+"","isPage":"F",
			"accessNum":""+jQuery("#accessNum").val()+"","monthPrev":""+monthPrev+""},
		dataType: "json",
		success: function(result){
			var jsonObj = result;
			if(jsonObj !=null) {
				if(jsonObj.resultCode=="0"){
					jQuery("#allTotal").html(jsonObj.allTotal);
					jQuery("#newPoint").html(jsonObj.newPoint);
					if(monthVal=="0"){
						jQuery("#yw_btn").show()
						if(parseInt( jsonObj.totalMoney)>0){
							jQuery("#totalMoney").html(jsonObj.totalMoney);
						}else{
							jQuery("#totalMoney").html("本期费用已结清，无需付费");
						}
					}else{
						jQuery("#yw_btn").hide();
					}
				}
			}
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
			//alert(XMLHttpRequest.status);
		}
	});
}

//替换字符串
function replaceStr(str,targetStr,replaceStr){
	var str = "";
	jQuery.ajax({
		type: "POST",
		async: false,
		url: "/ckb/ajax.do?action=replaceString",
		data: {"str":""+str+"","targetStr":""+targetStr+"","replaceStr":""+replaceStr+""},
		success: function(result){
			str = result;
		},error: function(XMLHttpRequest, textStatus, errorThrown) {
			//alert(XMLHttpRequest.status);
		}
	});
	return str;
}

/************************************随手查***************************************************/
//验证业务号码
function checkPhone(){
	var reason = true;
	var errorCount = 0;
	if($("#searchCostPhoneNum").val()==""){
		showWapMsgDiv("请输入手机号码！","关闭");
		return false;
	}else{
		var phoneType = $("#searchCostPhoneType").val();
		//手机号码验证
		if(phoneType=="SJ"){
			if($("#searchCostPhoneNum").val().length<11){
				showWapMsgDiv("请输入11位数手机号码！","关闭");
				reason = false;
			}else if(!/^[0-9]*$/.test($("#searchCostPhoneNum").val())){
				showWapMsgDiv("格式有误(必须为数字)，请输入11位数手机号码！","关闭");
				return false;
			}else{
				var cHead = new Array("133", "153", "189", "180", "183", "181", "185");
				var phoneNumSub = jQuery("#searchCostPhoneNum").val().substring(0,3);
				for(var i=0;i<cHead.length;i++){
					if(cHead[i]!=phoneNumSub){
						errorCount = 1;
					}else{
						errorCount = 0;
						break;
					}
				}
			}
		}

		if(errorCount>0){
			showWapMsgDiv('请输入"133、153、180、181、189"开头的11位手机号码！',"关闭");
			return false;
		}else{
			return true;
		}
		//固定电话验证
		if(phoneType=="GH"){
			if($("#searchCostPhoneNum").val().length<8){
				if(!/^[0-9]*$/.test($("#searchCostPhoneNum").val())){
					showWapMsgDiv("请输入8位数固话号码！","关闭");
					return false;
				}else{
					showWapMsgDiv("请输入8位数固话号码！","关闭");
					return false;
				}
			}
		}
	}
	return true;
}

//验证客户密码
function checkCustPwd(){
	var reason = true;
	if($("#custPwd").val()==""){
		showWapMsgDiv("请输入客户密码！","关闭");
		reason = false;
	}else{
		jQuery.ajax({
			type: "POST",
			async: false,
			url: "/ckb/ajax.do?action=checkSubmit",
			data: {"type":"checkPwd","areaCode":""+jQuery("#searchCostAreaCode").val()+"",
				"accessNumType":""+jQuery("#searchCostPhoneType").val()+"",
				"accessNum":""+jQuery("#searchCostPhoneNum").val()+"",
				"txtPwd":""+jQuery("#custPwd").val()+""},
			dataType: 'json',
			success: function(result){
				var jsonObj = result;
				if(jsonObj!=""){
					if(jsonObj.resultcode=="0"){

					}else{
						showWapMsgDiv(jsonObj.reason,"关闭");
						reason = false;
					}
				}
			},error: function(XMLHttpRequest, textStatus, errorThrown) {
				//alert(XMLHttpRequest.status);
			}
		});
	}
	return reason;
}

//验证码校验
function checkCaptchaCode(){
	var reason = true;
	if(jQuery("#captchaCode").val()==""){
		showWapMsgDiv("请输入验证码！","关闭");
		reason = false;
	}else{
		jQuery.ajax({
			type: "POST",
			async : false,
			url: "/ckb/ajax.do?action=checkCaptchaCode",
			data: {"captchaCode":""+$("#captchaCode").val()+""},
			success: function(data){
				if(data=="false"){
					$("#captchaCodeImg").attr("src","/ckb/ajax.do?action=changeCaptchaCode&s="+changeRandomCode());
					showWapMsgDiv("验证码输入有误！","关闭");
					reason = false;
					//setTimeout("closeDiv(messageDiv,'')",3000);
				}
			}
		});
	}
	return reason;
}

//验证费用查询数据提交是否正确
function checkSearchCostSubmit(){
	jQuery("#isReload").val("1")
	var reason = true;
	if(!checkPhone()){
		reason = false;
		return false;
	}
	if(reason){
		if(!checkCustPwd()){
			reason = false;
			return false;
		}
	}
	if(reason){
		if(!checkCaptchaCode()){
			reason = false;
			return false;
		}
	}
	return true;
}

//费用查询
function searchCost(){
	if(checkSearchCostSubmit()){
		var htmlStr = "<img style=\"vertical-align: middle;\" src=\"/ckb/knlgfile/kmweb/km/10000zhidao/newsMedia/wap/images/public/loading2.gif\" width=\"30\" height=\"30\">正在查询中。。。";
		showWapMsgForLogining(htmlStr),
		jQuery.ajax({
			type: "POST",
			url: "/ckb/ajax.do?action=ajaxSearchCost",
			data: {"isWap":"T","isPage":"F",
				"custId":""+jQuery("#custId").val()+"",
				"areaCode":""+jQuery("#searchCostAreaCode").val()+"",
				"accessNumType":""+jQuery("#searchCostPhoneType").val()+"",
				"accessNum":""+jQuery("#searchCostPhoneNum").val()+""},
			dataType: "json",
			success: function(result){
				var jsonObj = result;
				if(jsonObj !=null) {
					jQuery("#wap_loading_common_div").hide();

					//用户绑定的手机号码
					jQuery("#isBindPhone").val(jsonObj.isBindPhone);
					//用户手机号码
					jQuery("#accnbrNum").html(jsonObj.accnbr);
					//余额
					jQuery("#ye_em").html(jsonObj.balance+"元");
					//积分
					jQuery("#jf_em").html(jsonObj.availableIntegral+"分");
					//通话
					jQuery("#th_p").html(jsonObj.th+"分钟");
					//通话百分比
					jQuery("#ferRate").html(jsonObj.ferRate);
					//通话百分比图定位
					jQuery("#ferRateCss").attr("style","width:"+jsonObj.ferRate);
					//流量
					jQuery("#ll_p").html(jsonObj.ll+"MB");
					//流量百分比
					jQuery("#mRate").html(jsonObj.mRate);
					//流量百分比图定位
					jQuery("#mRateCss").attr("style","width:"+jsonObj.mRate);
					//短信
					jQuery("#dx_p").html(jsonObj.dx+"条");
					//短信百分比
					jQuery("#msgRate").html(jsonObj.msgRate);
					//短信百分比图定位
					jQuery("#msgRateCss").attr("style","width:"+jsonObj.msgRate);
					//是否为预付费标识
					jQuery("#isYFF").val(jsonObj.isYFF);
					$("#ssc").animate({left:0},500);
					//jQuery("#searchCostDiv").html(result);
				}

			},error: function(XMLHttpRequest, textStatus, errorThrown) {
				//alert(XMLHttpRequest.status);
			}
		});

		jQuery(function(){
			var isYFF = jQuery("#isYFF").val();
			//流量充值短信
			jQuery("#ll_cz_a").click(function(){
				sendMessagMobileCzForPhone("ll_cz",jQuery("#accessNum").val());
			});

			//短信充值短信
			jQuery("#dx_cz_a").click(function(){
				sendMessagMobileCzForPhone("dx_cz",jQuery("#accessNum").val());
			});
			jQuery("#layer_sbt1").click(function(e) {
				var custId = jQuery("#custId").val();
				var isBindPhone = jQuery("#isBindPhone").val();

				if(isYFF=="Y"){
					var w = $(window).width();
					jQuery("#ssc").animate({left:w},500);
				}else{
					//用户已登录
					if(custId!="-1" && custId!=""){
						//判断用户是否绑定手机
						alert(isBindPhone);
						if(isBindPhone!=""){
							//我的业务—套餐详情
							location.href = "/ckb/wap/personalInfo/package_details.html";
						}else{
							//提示用户绑定手机
							showWapMsgDiv("查询更多信息，请绑定手机！","关闭");
						}
					}else{
						showWapLoginDiv();
					}
				}
			});

			/*返回上一步*/
			jQuery("#layer_back").click(function(){
				/*清空随手查相应值*/
				//清空手机号码
				jQuery("#searchCostPhoneNum").val("");
				//清空客户密码
				jQuery("#custPwd").val("");
				//情况验证码
				jQuery("#captchaCode").val("");
				//重新获取验证码
				changeCaptchaCode('captchaCodeImg')
				jQuery(".firstlayer").animate({left:319},600);
			});
		});
	}
}

