    $(function () {
        var a = location.href;
        var b = 0;
        var c = $(".m_menu  ul li").length;
        $(".m_menu  ul li").each(function (e) {
            var d = $(this).find("a").attr("href");
            if (d.length > 5 && a.substring(a.length - d.length).toUpperCase() == d.toUpperCase()) {
                $(this).addClass("cur");
                $(this).siblings("li").removeClass("cur");
                return;
            }
            b++;
        });
        if (b == c) {
            $(".m_menu  ul li:eq(0)").addClass("cur");
            $(".m_menu  ul li:eq(0)").siblings("li").removeClass("cur");
        }
    });
