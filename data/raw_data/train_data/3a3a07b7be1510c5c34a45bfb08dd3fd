<script id="__vb" src="http://js.ub.fang.com/_vb.js?c=bbspostpage" type="text/javascript"></script>
<script type="text/javascript" src="http://js.ub.fang.com/_ub.js?v=201407181100"></script>
<script>
        _ub.city = '衡水'; //所在城市（中文）（这个地方的城市取得是页面左上角那个地方的城市）
        _ub.biz = 'b'; //论坛‘b’
        _ub.location = 0; //方位 ，网通为0，电信为1，如果无法获取方位，记录0
		//浏览时采用此种方式
		var forumId = '14874';
		var forumNameUB = encodeURIComponent('水静界');
		var forumLevel = encodeURIComponent('黄金论坛');
		var postId = '206991701';
		if(forumId!=''){
			if(postId==''){
				_ub.collect(1,{'vwg.page':'bbspostpage','vwb.forumid':forumId,'vwb.forumname':forumNameUB,'vwb.forumlevel':forumLevel});	
			}
		}else{
			 _ub.collect(1,{'vwg.page':'bbspostpage'});
		}
