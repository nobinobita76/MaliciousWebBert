                <script language="javascript" type="text/javascript">
               	//设置看房路线下拉框
                var kftLines = document.getElementsByName('bmkft_line');
                for(var i=0;i<kftLines.length;i++){
                	kftLine = kftLines[i];
                    kftLine.onclick = function() {
                    	showBoxDouble('bmkft_ul','');
                        document.getElementById('bmkft_line_text').value = this.title;
                    }
                }
                //看房路线下拉框关闭事件
                document.onclick = function (event){     
                    var e = event || window.event;  
                    var elem = e.srcElement||e.target;  
                           
                    while(elem)  
                    {   
                        if(elem.id == "bmkft_kanfangluxian")
                        {  
                                return;  
                        }  
                        elem = elem.parentNode;       
                    }  
                    //隐藏看房路线  
                    showBoxDouble('bmkft_ul','none');
                }
                
                function showBoxDouble(boxId,disp){
                    var theBox = document.getElementById(boxId);
                    if(disp == ''){
                        if (theBox.style.display == "none") {
                            theBox.style.display = "block";
                        } else {
                            theBox.style.display = "none";
                        }
                    }else{
                        theBox.style.display = disp;
                    }
                }

                //获取验证码
                function getCheckCode(btnId,mobileId){
                    mobile = document.getElementById(mobileId).value;
                    sendsmsapi(btnId, mobile);
                }
                
                //获取验证码
                function sendsmsapi(btnId, newPhone) {
                    newPhone = jQuery.trim(newPhone);
                    var mobileRegex = /^1\d{10}$/;

                    if (newPhone == "") {
                        showMessageBox("手机号不能为空", 0,'block');
                        return;
                    }
                    if (!mobileRegex.test(newPhone)) {
                        showMessageBox("手机号格式错误", 0,'block');
                        return;
                    }

                    jQuery.ajax({
                        type: "get",
                        url: "https://passport.fang.com/loginsendmsm.api?tm"+new Date().getTime(),
                        dataType: "jsonp",
                        jsonp: "callback",
                        data: {"service":"soufun-bbs-web", "mobilephone":escape(newPhone)},
                        xhrFields: {
                            withCredentials: true
                        },
                        crossDomain: true,
                        success: function (json) {
                            if( json.Message=="Success" )
                            {
                                //验证码发送成功
                                showMessageBox('验证码发送成功', 1,'block');
                                updateTimeButton(60, btnId);
                            }
                            else
                            {
                                //验证失败
                                if(json.Tip!=''){
                                    showMessageBox(json.Tip, 0,'block');
                                }else{
                                    showMessageBox('验证码发送失败，请重新获取', 0,'block');
                                }
                                updateTimeButton(60, btnId);
                                return false;
                            }
                        },
                        error: function(XMLHttpRequest, textStatus, errorThrown){
                            alert(XMLHttpRequest.status);
                            alert(XMLHttpRequest.readyState);
                            alert(textStatus);
                        }
                    });
                }
                //按钮60S禁用倒计时
                function updateTimeButton(time, btnId){
                    var btn = document.getElementById(btnId);
                    var handler = setInterval(function () {
                        if (time < 1) {
                            clearInterval(handler);
                            handler = null;
                            btn.innerHTML = "获取验证码";
                            btn.onclick = function (){
                            	getCheckCode('bmkft_yzm','bmkft_phone');
                            };
                        } else {
                            btn.onclick = function (){
                            	
                            };
                            time--;
                            btn.innerHTML = "重新发送("+ time +")";
                        }
                    }, 1000)
                }
                //检查看房团字段
                function checkKftForm(){
                    //1.验证姓名
                    var kftname = document.getElementById('bmkft_name').value;
                    if(kftname==''){
                    	showMessageBox("请输入真实姓名", 0,'block');
                        return false;
                    }
                    if(kftname.length>8){
                        showMessageBox("姓名最多输入8个字符", 0,'block');
                        return false;
                    }
                    //2.验证手机号
                    var kftphone = document.getElementById('bmkft_phone').value;
                    var rexp = /^1[3,4,5,6,7,8]{1}[0-9]{9}$/;
                    if(!rexp.test(kftphone)){
                    	showMessageBox("请输入正确的手机号", 0,'block');
                        return false;
                    }
                    //3.检查验证码
                    var kftcheckcode = document.getElementById('bmkft_checkcode').value;
                    if(kftcheckcode==''){
                        showMessageBox("请填写验证码", 0,'block');
                        return false;
                    }
                    //4.验证人数
                    var kftNum = document.getElementById('bmkft_num').value;
                    //if(isNaN(kftNum) || kftNum == "" || parseInt(kftNum)<=0){
                    if(!(/(^[1-9]\d*$)/.test(kftNum))){
                        showMessageBox("请输入大于0的人数", 0,'block');
                        return false;
                    }
                    if(kftNum>20){
                    	showMessageBox("报名人数最多20人", 0,'block');
                        return false;
                    }
                    //5.检查看房路线 
                    var bmkftlines = document.getElementsByName('bmkft_line');
                    var linecheck = 0;
                    for(var i=0;i<bmkftlines.length;i++){
                    	if(bmkftlines[i].checked){
                    		linecheck = 1;
                    		break;
                    	}
                    }
                    if(linecheck == 0){
                        showMessageBox("请选择看房团路线", 0,'block');
                        return false;
                    }
                    //6.看房团协议 同意《看房团活动声明》
                    var kftagree = (document.getElementById('bmkft_agree').checked == true ? 1 : 0);
                    if(!kftagree){
                        showMessageBox("请阅读《看房团活动声明》", 0,'block');
                        return false;
                    }
                    //设置提交按钮3秒后显示
                    var bmkft_wybm = document.getElementById('bmkft_wybm');
                    btn_wait(bmkft_wybm);
                    
                    //校验验证码，通过时回调提交表单
                    kfVerifySms(kftcheckcode,kftphone,'kftForm');
                	
                }

                //验证码的回调判断函数
                function doforumRoute(flag,msg,formId){
                    if(flag){
                        doSubmitForm(formId);
                    }else{
                    	showMessageBox(msg, 0,'block');
                    }
                }
                
                //验证手机号验证码
                function kfVerifySms(vCode,mobilePhone,formId) {
                    var httpsService = "soufun-bbs-web";
                    jQuery.ajax({
                        type: "get",
                        async:false,
                        url: "https://passport.fang.com/loginverifysms.api?tm" + new Date().getTime(),
                        dataType: "jsonp",
                        jsonp: "callback",
                        data: {
                            "Service": escape(httpsService),
                            "mobilephone": escape(mobilePhone),
                            "mobilecode": escape(vCode)
                            //"vcode": escape(vCode),
                            //"backurl": ""
                        },
                        xhrFields: {
                            withCredentials: true
                        },
                        crossDomain: true,
                        success: function (json) {
                            //最简单的一种方式
                            var flag = false;
                            if( json.Message =="Success" )
                            {
                                flag = true;
                            }
                            if(json.Tip == ''){
                                var msg = '验证失败，请重新获取验证码';
                            }else{
                                var msg = json.Tip;
                            }
                            doforumRoute(flag,msg,formId);
                        },
                        error: function (XMLHttpRequest, textStatus, errorThrown) {
                            alert(XMLHttpRequest.status);
                            alert(XMLHttpRequest.readyState);
                            alert(textStatus);
                        }
                    })
                }
                
                function btn_wait(me){
                    me.removeAttribute('href');
                    me.removeAttribute('onclick');
                    me.setAttribute("disabled",true);
                    window.setTimeout(function(){
                        me.setAttribute("onclick","checkKftForm();");
                        me.setAttribute("href","javascript:void(0)");
                        me.removeAttribute('disabled');
                    },3000);
                }
                //看房团表单提交
                function doSubmitForm(formId){
                    var kftform = document.getElementById(formId);
                    var kftname = kftform.bmkft_name.value;
                    var kftphone = kftform.bmkft_phone.value;
                    var kftNum = kftform.bmkft_num.value;
                    var bmkftline = kftform.bmkft_line.value;
                    var kftagree = (kftform.bmkft_agree.checked == true ? 1 : 0);
                    var pageurl = kftform.pageurl.value;
                    var url = "http://shuijingjie.fang.com/bbs/ajax/lookHouse.php";
                    jQuery.ajax({
                        type: "get",
                        url: url,
                        data: { name:kftname, phone:kftphone, num:kftNum , line:bmkftline , agreement:kftagree , pageurl:pageurl },
                        success: function (res) {
                            res = eval('(' + res + ')'); //转为json对象
                            if( res.succ == 1 ) {
                                //console.log(res);
                                showMessageBox('恭喜您看房团报名成功', 1,'block');
                                //window.location.replace( location.href+'?v='+Math.random() );
                            } else {
                                console.log(res);
                                showMessageBox(res.error, 0,'block');
                                //window.location.replace( location.href+'?v='+Math.random() );
                            }
                        },
                        error: function () {
                            showMessageBox("请求失败，请稍后重新尝试", 0,'block');
                            window.location.replace( location.href+'?v='+Math.random() );
                        }
                    });
                }    
                
                //弹出提示框
                function showMessageBox(msg, type, disp){
                    if(type == "1"){
                        //var kftform = document.getElementById('kftForm');
                        //kftform.reset();
                        if(msg==""){
                            msg = "恭喜您看房团报名成功";
                        }
                        document.getElementById("bmkft_pop_suc_msg").innerHTML = msg;
                        showBoxDouble('bmkft_pop_suc',disp);
                    }else{
                        if(msg==""){
                            msg = "报名失败，请重新提交";
                        }
                        document.getElementById("bmkft_pop_err_msg").innerHTML = msg;
                        showBoxDouble('bmkft_pop_err',disp);
                    }
                }
                </script>
