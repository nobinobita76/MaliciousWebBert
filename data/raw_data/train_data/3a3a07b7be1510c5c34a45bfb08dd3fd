<script language="javascript">
function show_logbox(msg){
    document.getElementById('tcdlk').style.display='block';
    jQuery("#strusername").focus();
}
function close_logbox(){
    document.getElementById('tcdlk').style.display='none';
}
function showPassword(password_text){
	document.getElementById('strpassword').style.display='block';
    jQuery("#strpassword").focus();
	password_text.style.display='none';
}
function hidePassword(password){
	if(password.value==''){
		document.getElementById('strpassword_text').style.display='block';
		password.style.display='none';
	}
}
function topnavnewtjLogin() 
{
    var uid = jQuery("#strusername").val();
    if (uid == "" || uid == "手机号/邮箱/用户名") {
        alert("请输入手机号/邮箱/用户名！");
        jQuery("#strusername").focus();
        return false;
    }

    var password = jQuery("#strpassword").val();
    if (password == "" || password == "请输入密码") {
        alert("请输入密码！");
        jQuery("#strpassword").focus();
        return false;
    }

    //基于https的通行证登录接入
    var backurl = jQuery("#backurl").val();
    var httpsToken = getCookie('token');
    var httpsService = "soufun-bbs-web";
    
    //alert("正式接口测试、检查遗漏，top_nav_new_tj.html，标注代码：P第  74 行"+uid+'---'+password+'------'+httpsToken+'----'+httpsService+'-----------'+backurl);
    jQuery.ajax({
        type: "get",
        url: "https://passport.fang.com/login.api",  //正式    
        //url: "https://passporttest.fang.com/login.api",//测试
        //url: '/NormalLoginInterface/Login',
        dataType: "jsonp",
        jsonp: "callback",
        data: { "Uid": escape(uid), "Pwd": escape(password), "Service": escape(httpsService), "Token": escape(httpsToken), "backurl":escape(backurl),"AutoLogin":"1" },
        xhrFields: {
            withCredentials: true
        },
        crossDomain: true,
        success: function (json) {
            if( json.Message=="Success" ) {
                window.location.reload();
            } else {
                alert(json.Tip);
                jQuery("#strpassword").focus();
            }
        }
    });
}
//获取cookie
function getCookie(name)
{
    var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
    if (arr = document.cookie.match(reg))
        return unescape(arr[2]);
    else
        return null;
}
</script>

