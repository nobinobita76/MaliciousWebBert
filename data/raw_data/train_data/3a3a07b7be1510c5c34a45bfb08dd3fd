<script type="text/javascript">
	var inptext = document.getElementById('inptext');
	var butsearch = document.getElementById('butsearch');
	var buttiwen = document.getElementById('buttiwen');

	inptext.onfocus = function(){
		if(inptext.value == "请输入您的问题"){
			inptext.value = "";
			inptext.style.color = "#000";
		}
	}

	inptext.onblur = function(){
		if(inptext.value == ""){
			inptext.style.color = "#aaa";
			inptext.value = "请输入您的问题";
		}
	}
	
	//点击搜索所有按钮
	butsearch.onclick = function(){
		if(inptext.value.length==""){
			window.location.href = "http://www.fang.com/ask/search.aspx";
		}else{
			if(inptext.value == "请输入您的问题"){
				inptext.value = "";
			}
			window.location.href = "http://www.fang.com/ask/search.aspx?asktitle="+inptext.value;
		}
	}
	//点击提问按钮
	buttiwen.onclick = function(){
		if(inptext.value.length==""){
			window.location.href = "http://www.fang.com/ask/Ask_StepTwo.aspx";
		}else{
			if(inptext.value == "请输入您的问题"){
				inptext.value = "";
			}
			window.location.href = "http://www.fang.com/ask/Ask_StepTwo.aspx?asktitle="+inptext.value;
		}
	}
	
</script>
