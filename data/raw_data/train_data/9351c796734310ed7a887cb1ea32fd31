        var _resize = function() {
            var html = document.documentElement,
                    w = html.clientWidth / 1125,
                    w = 100 * w;
            html.setAttribute('style', 'font-size:' + w + 'px');
        }
        _resize();
        window.addEventListener('resize', _resize);
