<script>
$(function () {
	$('.btn_close').bind('click', function () {
		$('.kcjs_open1').animate({'left':'-141'},500);
		$('.kcjs_close1').show();
	})
	$('.kcjs_close1').bind('mouseenter', function () {
		$('.kcjs_open1').animate({'left':'1'},500);
		$(this).hide('slow');
	})
})
</script>	



