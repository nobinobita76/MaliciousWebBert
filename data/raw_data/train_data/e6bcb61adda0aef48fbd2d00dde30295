<script type="text/javascript">
    $(function(){
        var channel = window['_staticHeader'] && _staticHeader['currentChannel'];
        if(!channel) return;
        if (channel == 'JOB') {
            $('.dj-sub-title h2').html('职位<em>CAREER</em>');
        }
    });
    // 导航下拉菜单交互 + 搜索交互
    $(function(){
        var matchbox = {
            init : function(args){
                this.$body = $('body');
                this.$el = args.$el;
                this.$tipWrap = args.$tipWrap;
                this.$tip = this.$tipWrap.find('ul');
                this.clsTipHover = 'hover-search';
                this.cached = {};
                this.bind();
                this.render = template.compile('<%' +
                        'for(var i=0,len=list.length; i<len; i++){' +
                        'var item = list[i]' +
                        '%>' +
                        '<li data-value="<%= item.keyName%>"><%= item.keyName%></li>' +
                        '<% } %>');
                this.timer = null;
            },
            bind : function(){
                var _this = this;
                $(document).bind('click.djHeaderSearchBlur', function(){
                    _this.$tipWrap.hide();
                });
                this.$el.bind('click', function(e){
                    var bol_fetch = _this.$tipWrap.filter(':visible').length == 0;
                    if(bol_fetch){
                        _this.fetch();
                    }
                });
                this.$el.bind('blur', function(e){
                    _this.$tipWrap.hide();
                });
                this.$el.bind('keydown', function(e){
                    return _this.handlerKeydown(e);
                });
                this.$el.bind('keyup', function(e){
                    return _this.handlerKeyup(e);
                });
                this.$tip.delegate('li', 'mousedown', function(){
                    var $this = $(this);
                    var value = $this.attr('data-value');
                    _this.$el.val(value);
                    _this.$el.closest('form').submit();
                });
                this.$tip.delegate('li', 'mouseenter', function(){
                    $(this).addClass(_this.clsTipHover)
                });
                this.$tip.delegate('li', 'mouseleave', function(){
                    $(this).removeClass(_this.clsTipHover)
                });
            },
            build : function(data){
                var html = this.render(data);
                this.$tip.html(html);
                this.$tipWrap[data.list.length ? 'show' : 'hide']();
            },
            fetch : function(){
                var matchType = this.$el.attr('data-match');
                var _this = this;
                if(!matchType) return;
                clearTimeout(this.timer);
                this.timer = setTimeout(function(){
                    var val = $.trim(_this.$el.val());
                    var key = encodeURI(val);
                    var result = _this.cached[key];
                    if(result){
                        _this.build(result);
                        return;
                    }
                    $.ajax({
                        url : /\.ui$/.test(location.host) ? '/up/autoheader/json/search-suggest.php' : '//so.dajie.com/job/ajax/nav/keyword',
                        type : 'get',
                        dataType : 'jsonp',
                        data : {
                            keyword : val,
                            match : matchType
                        },
                        success : function(r){
                            if(r.result == 0){
                                _this.build(r.data);
                                _this.cached[key] = r.data;
                            }
                        }

                    });
                }, 100);
            },
            handlerKeydown : function(e){
                var code = e.keyCode;
                switch(code){
                    case  38:
                        // 上一条
                        e.preventDefault();
                        this.handlerSelectItem('up');
                        break;
                    case 40 :
                        // 下一条
                        e.preventDefault();
                        this.handlerSelectItem('down');
                        break;
                }
            },
            handlerKeyup : function(e){
                var code = e.keyCode;
                switch(code){
                    case 37:
                    case 38:
                    case 39:
                    case 40 :
                        return;
                    default :
                        this.fetch();
                }
            },
            handlerSelectItem : function(type){
                var items = this.$tip.find('li');
                var length = items.length-1;
                var clsTipHover = this.clsTipHover;
                var index = items.index(items.filter('.'+clsTipHover));
                var newIndex = type == 'up' ? index -1 : index+1 > length ? 0 : index+1;
                if(index == -1){
                    items[type == 'up' ? 'last' : 'first']().addClass(clsTipHover);
                }else{
                    items.removeClass(clsTipHover);
                    items.eq(newIndex).addClass(clsTipHover);
                }
                var value = items.filter('.'+clsTipHover).attr('data-value');
                this.$el.val(value);
            }
        };
        (function () {
            var $headerWrap = $('#dj-header-wrap');
            var $section = $headerWrap.find('.dj-top-section');
            var $doc = $(document);
            var iframe = '<iframe class="iframe-mask" frameborder="0" style="" src="javascript:void(0);"></iframe>';
            var query_subListWrap = '.dj-header-nav-sub-item';
            $headerWrap.delegate('.has-more, .uinfo-inner', 'mouseover', handler_show_menu);
            $headerWrap.delegate('.search-type', 'click', handler_show_menu);
            $headerWrap.delegate('.has-more, .uinfo-inner, .search-type', 'mouseleave', handler_hide_menu);
            $headerWrap.delegate('.dj-nav-g, .dj-uinfo-action', 'mouseout', handler_overflow_hide);
            // ie-placeholder
            if($.browser.msie){
                $headerWrap.delegate('.dj-nav-box .search-input', 'focus.iefix', handler_fix_placeholder);
                $headerWrap.delegate('.dj-nav-box .search-input', 'blur.iefix', handler_fix_placeholder);
//          $headerWrap.find('.dj-nav-box .search-input').triggerHandler('blur.iefix');
                handler_fix_placeholder.call($headerWrap.find('.dj-nav-box .search-input')[0], {type:'blur'})
            }
//        if(navVersion !== 'normal'){
            // 搜索框展开
            $headerWrap.delegate('.dj-nav-box', 'click', handler_focus_search);
            $headerWrap.delegate('.dj-nav-box input', 'focus', handler_focus_search);
            matchbox.init({
                $el : $headerWrap.find('.dj-nav-box input'),
                $tipWrap : $headerWrap.find('.suggest')
            });
            // 搜索框关闭
            $doc.bind('click.djHeaderSearchBlur', handler_blur_search);
            // 搜索下拉交互
            $headerWrap.delegate('.dj-nav-box li a', 'click', handler_change_mode);
//        }else{
//          $headerWrap.find('.dj-nav-box input').attr('readonly', 'readonly');
//        }
            // 切换导航
            $headerWrap.delegate('.dj-header-changeNav', 'click', function(e){
                e.preventDefault();
                _staticHeader['changeNav']();
            });
            function handler_show_menu(e){
                var $this = $(this);
                var bol_oper_sub_menu = $(e.target).closest(query_subListWrap).length;
                // 不对下拉菜单做相应
                if(bol_oper_sub_menu) return;
                var $navSelect = $this.find(query_subListWrap);
                var num_width = $navSelect.outerWidth();
                var num_height = $navSelect.outerHeight();
                $this.addClass('on');
                // 划过的时候隐藏所有的消息弹出层
                $('.dj-tips-base').hide();
                // 修改完毕
                if (!$navSelect.parents('.on').find('iframe').length) {
                    $navSelect.parents('.on').append(iframe);
                }
                $this.find('.iframe-mask').css({"width":num_width, "height":num_height}).show();
            }
            function handler_hide_menu(){
                var $this = $(this);
                $this.removeClass('on');
                $this.find('.iframe-mask').hide();
            }
            function handler_overflow_hide(e){
                if (e.clientY < 0) {
                    setTimeout(function () {
                        $headerWrap.find('#dj-nav-list > li, .uinfo-inner, .search-type').removeClass('on');
                        $headerWrap.find('.iframe-mask');
                    }, 0);
                }
            }

            var cls_search_focus = 'dj-nav-box-focus';
            function handler_focus_search(e){
                searchTip.hide();
                // 火狐focus事件下stopPropagation时会没有输入提示符。。。
                if(e.type.indexOf('focus') == -1){
                    e.stopPropagation();
                }
                if(!$section.hasClass(cls_search_focus)){
                    // 划过的时候隐藏所有的消息弹出层
                    $('.dj-tips-base').hide();
                    $section.addClass(cls_search_focus);
                }
                // 防止死循环
                if(e.type.indexOf('focus') == -1){
                    $section.find('input').focus();
                }
            }
            function handler_blur_search(e){
                $section.removeClass(cls_search_focus);
            }
            function handler_change_mode(e){
                e.preventDefault();
                var $this = $(this);
                var href = $this.attr('data-href');
                var type = $this.attr('data-type');
                var name = $this.attr('data-name');
                var holder = $this.attr('data-holder');
                var match = $this.attr('data-match');
                var $form = $this.closest('form');
                var $input = $form.find('.search-input');
                var $menuCtrl = $this.closest('.search-type');
                var $ico = $menuCtrl.find('> .ico');
                $form.attr('action', href);
                $input.attr('name', name);
                $input.attr('placeholder', holder);
                $input.attr('data-match', match);
                handler_hide_menu.call($menuCtrl);
                $ico.attr('class', type);
            }

            function handler_fix_placeholder(e){
                var _this = this;
                var type = e.type;
                var holder = _this.getAttribute('placeholder');
                var value = _this.value;
                if(e.type == 'focus' && value == holder){
                    _this.value = '';
                    return;
                }
                setTimeout(function(){
                    var isFocus = $(_this).closest('.dj-nav-box-focus').length;
                    var bolBlur = type == 'focusout' || type == 'blur';
                    if(bolBlur && value == '' && !isFocus){
                        _this.value = holder;
                    }
                }, 80);
            }

            var searchTip = {
                init : function(){
                    var _this = this;
                    this.hash = 'dj-header-search-tip';
                    this.max = 2;
                    this.$tip = $('#dj-header-search-tip');
                    this.show();
                    this.$tip.find('.cls').bind('click', function(e){
                        e.stopPropagation();
                        _this.hide();
                    });
                },
                bolHome : function(){
                    if(/\.ui$/.test(location.host)){
                        var urlHome = 'https://assets.dajieimg.com.ui';
                    }else{
                        var urlHome = '//www.dajie.com/home';
                    }
                    var bol = location.href.indexOf(urlHome) == 0;
                    return bol;
                },
                bolData : function(){
                    var endDate = new Date(2014,8,18) - 0;
                    var nowDate = new Date() - 0;
                    return nowDate < endDate;
                },
                bolShow : function(){
                    var bolHash = localStorage.getItem(this.hash) - 0;
                    var result = bolHash < this.max;
                    if(result){
                        localStorage.setItem(this.hash, bolHash+1);
                    }
                    return result;
                },
                show : function(){
                    if(!this.bolHome()) return; // 不是主页
                    if(!this.bolData()) return; // 过期
                    if(!this.bolShow()) return; // 超过显示次数
                    this.$tip.show();
                },
                hide : function(){
                    // 用户主动关闭后不再显示
                    localStorage.setItem(this.hash, this.max);
                    this.$tip.hide();
                }
            };
            searchTip.init();

        })();
    });

</script>        </div>
    </div>
    <script type="text/javascript">
        var is_login = $.cookie('dj_auth') != null || $.cookie('dj_auth_v3') != null; //必须是全局变量，别的项目有用
        window['is_login'] = is_login;
        _staticHeader = {
            isMini : false,
            isC : false,
            isRecruit : false,
            currentChannel: 'NONE',
            isBusinessCorp: false
        };
    </script>
