    <div class="smbAlertBox" id="rightFloatingBox" style="display: none;">
    	<div class="smbAlertBoxBg"></div>
        <div class="smbAlertBoxCon_200" style="margin-top:-200px;">
        	<div class="smbAlertTit"><h3>提示信息</h3><a></a></div>
        	
            <div class="smbAlertPrompt_200">
            	<h4>请留下您的公司名称与电话，速卖宝会在1小时内联系您！</h4>
            	<dl class="clearfix">
            		<dt>公司名称：</dt>
            		<dd>
            			<input type="text" name="" id="companyName" maxlength="30" value="请输入您的公司名称">
            			<p class="smbWarning" id="companyNameMsg"></p>
            		</dd>
            	</dl>
            	<dl class="clearfix">
            		<dt>电&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;话：</dt>
            		<dd>
            			<input type="text" name="" id="telephone" maxlength="15" value="请输入您的电话号码">
            			<p class="smbWarning" id="telephoneMsg"></p></dd>
            	</dl>
            	<a id="rigFloBoxSub" class="banSub">立即提交</a>
            	
            </div>
        </div>
    </div>
    
<script language="javascript"> 
	$(function(){
		$(".smbAlertBoxCon_200 .smbAlertTit a").click(function(){
			$("#rightFloatingBox").hide(); 
			resetRightFloatingBox();
		})
		$(".rFixedLy").click(function(){
			$("#rightFloatingBox").show(); 
		})
		
		$("#companyName").on('focus', function (){
			var companName = $(this).val();
			if (companName == "请输入您的公司名称"){
				 $(this).val("");
			}
		});
		$("#companyName").on('blur', function (){
			var companName = $(this).val();
			if (companName == ""){
				 $(this).val("请输入您的公司名称");
			}
		});
		
		$("#telephone").on('focus', function (){
			var telephone = $(this).val();
			if (telephone == "请输入您的电话号码"){
				 $(this).val("");
			}
		});
		$("#telephone").on('blur', function (){
			var telephone = $(this).val();
			if (telephone == ""){
				 $(this).val("请输入您的电话号码");
			}
		});
		// 在线留言-立即提交
		$("#rigFloBoxSub").click(function (){
			var companyName = $("#companyName").val();
			var telephone = $("#telephone").val();
			var flag = true;
			if (companyName == "" || companyName== "请输入您的公司名称"){
				$("#companyNameMsg").html("<i></i>请输入您的公司名称！");
				flag = false;
			} else {
				$("#companyNameMsg").html("");
			}
			var isPhone = /^([0-9]{3,4}-?)?[0-9]{7,8}$/;  
		    var isMob = /^1(3|4|5|7|8)\d{9}$/; 
			if (telephone == "" || telephone== "请输入您的电话号码"){
				$("#telephoneMsg").html("<i></i>请输入您的电话号码！");
				flag = false;
			} else if(!isPhone.test(telephone) && !isMob.test(telephone)){
				$("#telephoneMsg").html("<i></i>请输入正确的电话号码！");
				flag = false;
			} else {
				$("#telephoneMsg").html("");
			}
			
			if (flag){
				$(this).html("提交中...");
				$.ajax({
				   url: "http://sumy.inc.hc360.com/sumy/common/save/online/msg.shtml",
				   type: "POST",
				   data: "sname=" +companyName +"&smobile=" + telephone,
				   success: function(data){
					 $("#rightFloatingBox").hide();
					 resetRightFloatingBox();
				   },
				   error: function(){
					   alert("提交失败!");
				   }
				});
			}
		});
		
	})
	//重置
	function resetRightFloatingBox(){
		 $("#rigFloBoxSub").html("立即提交");
		 $("#companyName").val("请输入您的公司名称");
		 $("#telephone").val("请输入您的电话号码");
		 $("#companyNameMsg").html("");
		 $("#telephoneMsg").html("");
	}
</script>    

