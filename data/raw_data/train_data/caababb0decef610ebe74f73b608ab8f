<script type="text/javascript" src="http://style.org.hc360.com/js/build/source/core/jquery.min.js"></script>
<script src="http://sumy.inc.hc360.com:80/sumy/js/jquery.paginate.js" type="text/javascript"></script>
<script language="javascript">
	var pageSize = 30;
	var totalRecord = 0;
	var pageCount = 0;
	var optionText = null;//查询字段名称
	var optionValue = null;//查询字段值
	var data = {};//查询参数

	$(function(){//一进页面默认执行的查询
		
		$(".smbAlertBoxCon .smbAlertTit a").click(function(){
			$("#smbAlertBox1").hide(); 
		})
		$(".smbAlertBoxCon_2 .smbAlertTit a").click(function(){
			$("#smbAlertBox2").hide(); 
		})
		
		searchByOption(1, null, null);
	
		$("#pubdate").click(function(){//发布时间升序排序
			delete data["enddateorder"];
			if($("#enddate").attr("class") == "seleUpCur"){
				$("#enddate").attr("class", "seleUp");
			}
			if($("#enddate").attr("class") == "seleDownCur"){
				$("#enddate").attr("class", "seleDown");
			}
			if($("#pubdate").attr("class") == "seleUp"){
				$("#pubdate").attr("class", "seleUpCur");
				searchByOption(1, "pubdateorder", "desc")
				return;
			}
			if($("#pubdate").attr("class") == "seleUpCur"){
				$("#pubdate").attr("class", "seleDownCur");
				searchByOption(1, "pubdateorder", "asc")
				return;
			}
			if($("#pubdate").attr("class") == "seleDown"){
				$("#pubdate").attr("class", "seleDownCur");
				searchByOption(1, "pubdateorder", "asc")
				return;
			}
			if($("#pubdate").attr("class") == "seleDownCur"){
				$("#pubdate").attr("class", "seleUpCur");
				searchByOption(1, "pubdateorder", "desc")
				return;
			}
		})
		$("#enddate").click(function(){//截止时间升序排序
			delete data["pubdateorder"];
			if($("#pubdate").attr("class") == "seleUpCur"){
				$("#pubdate").attr("class", "seleUp");
			}
			if($("#pubdate").attr("class") == "seleDownCur"){
				$("#pubdate").attr("class", "seleDown");
			}
			if($("#enddate").attr("class") == "seleUp"){
				$("#enddate").attr("class", "seleUpCur");
				searchByOption(1, "enddateorder", "desc")
				return;
			}
			if($("#enddate").attr("class") == "seleUpCur"){
				$("#enddate").attr("class", "seleDownCur");
				searchByOption(1, "enddateorder", "asc")
				return;
			}
			if($("#enddate").attr("class") == "seleDown"){
				$("#enddate").attr("class", "seleDownCur");
				searchByOption(1, "enddateorder", "asc")
				return;
			}
			if($("#enddate").attr("class") == "seleDownCur"){
				$("#enddate").attr("class", "seleUpCur");
				searchByOption(1, "enddateorder", "desc")
				return;
			}
		})
		
		var defval = $("#searchkeyword").val();
		$("#searchKey").click(function(){
			var keyword = $("#searchkeyword").val()==defval?"":$("#searchkeyword").val();
			searchByOption(1, "keyword", keyword);
		})
		
		$("#searchkeyword").focus(function(){
			var keyword = $("#searchkeyword").val();
			if(keyword == defval){
				$("#searchkeyword").val("");
			}
		})
		
		$("#searchkeyword").blur(function(){ 
			var thisval = $(this).val(); 
			if(thisval==""){ 
				$(this).val(defval); 
			} 
		});
		
		$(".upArrow").click(function(){
			$(".listLayoutBox").css({"display":"none"});
			$(".upArrow").css({"display":"none"});
			$(".downArrow").css({"display":"block"});
		})
		
		$(".downArrow").click(function(){
			$(".listLayoutBox").css({"display":"block"});
			$(".downArrow").css({"display":"none"});
			$(".upArrow").css({"display":"block"});
		})
		
		$("#moreIndustry").click(function(){
			$("#industryul").addClass("hAuto");
			$("#moreIndustry").css({"display":"none"});
			$("#lessIndustry").css({"display":"block"});
		})
		
		$("#lessIndustry").click(function(){
			$("#industryul").removeClass("hAuto");
			$("#lessIndustry").css({"display":"none"});
			$("#moreIndustry").css({"display":"block"});
		})
		
		$('#searchkeyword').keydown(function(e){
			if(e.keyCode==13){
				var keyword = $("#searchkeyword").val()==defval?"":$("#searchkeyword").val();
				searchByOption(1, "keyword", keyword);
			}
		})
	})
	function searchByOption(pageNum, optionText, optionValue){
		var from=(pageNum-1)*pageSize;
		data["fromStr"] = from;
		data["sizeStr"] = pageSize;
		if(optionText!=null&&optionValue!=null){
			if(optionValue == ""){
				delete data[optionText];
			}else {
				data[optionText] = optionValue;
			}
		}
		query(data, pageNum);
	}
	function query(data, pageNum){
		var listStr = "";
		var aurl="http://sumy.inc.hc360.com:80/sumy/public/smbcontacts/selectSmbContacts.shtml";
		$.ajax({
	        url:aurl,
	        type:"POST",
	        dataType:"json",
	        data:data,
	        contentType:"application/x-www-form-urlencoded; charset=UTF-8",
	        timeout:5000,
	        async: false,
	        success:function(data){
	        	if(data!=null && data!=""){
	        		if(data.total == 0){
	        			$(".smbBoxConLeft").html('<div><p>抱歉，没有找到和您查询条件相符的信息。</p><p>您可以试着调整一下您搜索的产品词，或者去<a href="http://sumy.inc.hc360.com:80/sumy/public/smbcontacts/showSmbContacts.shtml" class="sCur">精选买家采购信息库</a>试试</p></div>');
	        			return;
	        		}else {
	        			for(var i=0;i<data.objects.length;i++){
	        				var obj = data.objects[i];
	        				listStr += '<div class="smbListCon">'+
	        						   '<div class="listTit">'+
	        						   '<h3>采购'+obj.keyword+'</h3>';
	        				if(obj.highbuyer == "1"){
	        					listStr += '<em class="yIco">优</em>';
	        				}
	        				if(obj.bigbuyer == "1"){
	        					listStr += '<em class="mIco">买</em>';
	        				}
	        				if(obj.longbuyer == "1"){
	        					listStr += '<em class="cIco">长</em>';
	        				}
	        				if(obj.custombuyer == "1"){
	        					listStr += '<em class="dIco">定</em>';
	        				}
	        				listStr += '</div><div class="smbListCenter">'+
	        						   '<div class="parameter"><ul>';
	        				if(obj.longbuyer == "1" && obj.highbuyer != "1" && obj.bigbuyer != "1" && obj.custombuyer != "1"){
	        					if(obj.num==0){
	        						listStr += '<li>采购数量：<span>详谈</span></li>';
	        					}else{
			        				listStr += '<li>采购数量：<span>'+obj.num+'&nbsp;'+obj.unit+'</span></li>';
		       					}
	        				}else{
	        					if(obj.num==0){
			        				listStr += '<li>采购数量：<span>详谈</span></li>';
	        					}else{
			        				listStr += '<li>采购数量：<span>'+obj.num+'&nbsp;'+obj.unit+'</span></li>';
		       					}
	        				}
	        				listStr += '<li>信息发布时间：<span>'+getMyDate(obj.pubdate)+'</span></li>';
	        				
	        				if(obj.pubdate == obj.enddate){
	        					listStr += '<li>采购截止时间：<span>长期</span></li>';
	        				}else{
	        					listStr += '<li>采购截止时间：<span>'+getMyDate(obj.enddate)+'</span></li>';;
	        				}	
	        				
	        				listStr += '<li>供货商首选区域：<span>'+obj.perferarea+'</span></li>';
	        				if(false == true){
	        					if(obj.linkmantype == "0"){
		        					listStr += '<li>询盘买家类型：<span>个人</span></li>';
	        					}else if(obj.linkmantype == "3"){
	        						listStr += '<li>询盘买家类型：<span>个体</span></li>';
	        					}else{
	        						listStr += '<li>询盘买家类型：<span>公司</span></li>';
	        					}
	        				}else {
	        					listStr += '<li>询盘买家类型：<span><a href="http://sumy.inc.hc360.com/sumy/auth/gopay/lxb.shtml">乐享/尊享专权信息</a></span></li>';
	        				}
	        				listStr += '</ul></div><div class="parameterRig"><dl><dt><a href="javascript:void(0)" onclick="lookContact('+obj.oid+',this)" class="contactBtn" onmousedown="return hcclick(\'?hcsumy_smqz=hcsumy_smqz_xxs_txt01\')">查看联系方式</a>'+
	        				
	        							'<div id="'+obj.oid+'contectAlert1" class="contectAlert" style="display:none;"><div class="contectAlert_close" onclick="contectAlertClose()"></div><div class="cAlertLeft"><div class="cTel">'+
	        							'</div><p class="cName"></p><div class="leftBot"><p>联系时，请说明是在慧聪网看到的</p>'+
	        							'<p>友情提示：此电话已被查看<b class="bei2">'+obj.lookover_num+'</b>次。</p></div></div>'+
	        							'<div class="cAlertRig"><span><img src="http://style.org.hc360.com/images/my/images/smb/newImg/codeImg.jpg" /></span><p>微信扫一扫，<br />立享微信推送特权！</p></div></div>'+
	        							
	        							'<div id="'+obj.oid+'contectAlert2" class="contectAlert" style="display:none;"><div class="contectAlert_close" onclick="contectAlertClose()"></div>'+
	        							'<div class="cAlertLeft2"><div class="cPrompt">对不起，您来晚了！</div>'+
	        							'<div class="leftBot"><p>已经被<b>6</b>家卖家查看了，</p><p>为了保护双方权益,此条信息已无法查看，</p><p>下次请来早些吧！</p></div></div>'+
	        							'<div class="cAlertRig"><span><img src="http://style.org.hc360.com/images/my/images/smb/newImg/codeImg.jpg" /></span><p>微信扫一扫，<br />立享优先2小时查看特权！</p></div></div>'+
	        							
	        							'<div id="'+obj.oid+'contectAlert3" class="contectAlert" style="display:none;"><div class="contectAlert_close" onclick="contectAlertClose()"></div><div class="cAlertCon"><div class="cPrompt">对不起，您的查看条数已用完，无法查看更多电话</div>'+
	        							'<div class="cAlertConBot"><p>您可以<a href="http://sumy.inc.hc360.com/sumy/auth/gopay/lxb.shtml" target="_blank">升级乐享版</a>，可查看365条联系电话。</p></div></div></div>'+
	        							
	        							'<div id="'+obj.oid+'contectAlert4" class="contectAlert" style="display:none;"><div class="cAlertCon"><div class="cPrompt">对不起，您的会员已经到期，无法查看更多电话</div>'+
	        							'<div class="cAlertConBot"><p></p></div></div></div></dt>';
	        				/* if(obj.pubdate == obj.enddate){
	        					listStr += '<dd>采购截止时间：长期</dd>';
	        				}else{
	        					listStr += '<dd>采购截止时间：'+getMyDate(obj.enddate)+'</dd>';
	        				} */
							//被<b>'+obj.lookover_num+'</b>家卖家查看 /	        							
	        				listStr += '<dd> 被<b class="bei">'+obj.lookover_num+'</b>家卖家查看 /	共<b>6</b>家卖家可查看</dd></dl></div></div>';//+
	        							//'<p class="smbListBot">'+obj.application+'</p></div>';
	        				if(obj.application=='更多描述:'||obj.application=='更多描述：'){
	        					listStr += '<p class="smbListBot">更多描述：本条信息已经过电话核实。</p>';
	        				}else{
	        					listStr += '<p class="smbListBot">'+obj.application+'</p>';
	        				}
	        				listStr += '</div>';
	        				
	        				$(".smbBoxConLeft").html(listStr);
	        			}
	        			listStr += '<div class="demo"><div id="pagination0"></div></div>';
	        			$(".smbBoxConLeft").html(listStr);
	        			totalRecord=data.total;
						pageCount=totalRecord%pageSize==0?totalRecord/pageSize:Math.floor(totalRecord/pageSize)+1;
						$("#pagination0").paginate({
							count 		: pageCount,	//此处要写清本次分页的总页数,不支持动态改变,不可写非int型数.个人建议在进入页面的时候就把页数传过来
							start 		: pageNum,			//开始页码,从1开始,一般设置成1
							display     : 10,			//在分页条上显示的个数,分页数等于或超过本设置会显示设置的个数,不够的话会显示真实个数,不过就不会出现滚动效果了.
							border					: true,	//是否显示外框
							text_color  			: '#888',	//文字颜色
							background_color    	: '#EEE',	//背景颜色
							text_hover_color  		: 'black',	//鼠标放上去时文字的颜色
							background_hover_color	: '#CFCFCF',//鼠标放上去时背景的颜色
							rotate      			: true,		//是否滚动
							images					: false,	//
							mouse					: 'press',	//可选值为'press'和'slide',具体差别请自己体验.
							onChange     			: function(page){//本插件唯一可触发的事件,在点击页数的时候触发,只传过来当前被选中的页数,我想这其实足够了.														
														searchByOption(page,null,null);
													  }
						});
						
	        		}
	        	}
	        }
	    });
	}
	
	function contectAlertClose(){
		$(".contectAlert").hide();
	}
	
	//采购商级别查询
	function searchByLevel(level, obj){
		$(obj).addClass("sCur");
		$(obj).parent().siblings().children().removeClass("sCur");
		optionText = "purLevel";
		if(level == 0){//采购商级别选择不限
			optionValue = "";
			searchByOption(1, optionText, optionValue);
		}else if(level == 1){//采购商级别选择优质
			optionValue = "highbuyer";
			searchByOption(1, optionText, optionValue);
		}else if(level == 2){//采购商级别选择大买家
			optionValue = "bigbuyer";
			searchByOption(1, optionText, optionValue);
		}else if(level == 3){//采购商级别选择长期采购
			optionValue = "longbuyer";
			searchByOption(1, optionText, optionValue);
		} else if(level == 4){//采购商级别选择定制需求
			optionValue = "custombuyer";
			searchByOption(1, optionText, optionValue);
		}
	}
	//采购商行业查询
	function searchByIndustry(aid, obj){
		$(obj).addClass("sCur");
		$(obj).parent().siblings().children().removeClass("sCur");
		optionText = "purIndustry";
		if(aid == 0){
			optionValue = "";
		}else {
			optionValue = aid;
		}
		searchByOption(1, optionText, optionValue);
	}
	//信息发布时间查询
	function searchByRange(range, obj){
		$(obj).addClass("sCur");
		$(obj).parent().siblings().children().removeClass("sCur");
		optionText = "pubdateRange";
		if(range == 0){//信息发布时间选择不限
			optionValue = "";
			searchByOption(1, optionText, optionValue);
		}else if(range == 1){//信息发布时间选择 最近一周
			optionValue = "oneweek";
			searchByOption(1, optionText, optionValue);
		}else if(range == 2){//信息发布时间选择 最近三天
			optionValue = "threeday";
			searchByOption(1, optionText, optionValue);
		}else if(range == 3){//信息发布时间选择 最近一天
			optionValue = "oneday";
			searchByOption(1, optionText, optionValue);
		}
	}
	
	//根据毫米转换为带格式的日期
	function getMyDate(str){
	    //var oDate = new Date(str);//ie不兼容
	    var oDate = new Date(Date.parse(str.replace(/-/g, "/")));
	   	oYear = oDate.getFullYear();
	  	oMonth = oDate.getMonth()+1;
	   	oDay = oDate.getDate();
	   	oHour = oDate.getHours();
	   	oMin = oDate.getMinutes();
	   	oSen = oDate.getSeconds();
	   	//oTime = oYear +'-'+ getzf(oMonth) +'-'+ getzf(oDay) +' '+ getzf(oHour) +':'+ getzf(oMin) +':'+getzf(oSen);//最后拼接时间
	   	oTime = oYear +'-'+ getzf(oMonth) +'-'+ getzf(oDay);//最后拼接时间
	   	return oTime;
  	};
  	//补0操作
  	function getzf(num){
   		if(parseInt(num) < 10){
    		num = '0'+num;
   		}
   		return num;
  	}
  	
  	//查看联系方式按钮
  	function lookContact(oid,obj){
  		//先判断用户是否登录
  		if(false == false){//用户未登录，点击按钮跳转到购买页
  			window.location.href = "http://sso.hc360.com/ssologin?logincode=3&loginparam=supplyend_ssotop&ReturnURL=http://sumy.inc.hc360.com/sumy/public/smbcontacts/showSmbContacts.shtml";
  		}else {//用户已登录
  			//判断用户是否已经购买速卖宝会员
  			var buyFlag = undefined;
  			if(buyFlag == false){//用户没有购买会员
  				$("#smbAlertBox2").css({"display":"block"});
  				//window.location.href = "http://sumy.inc.hc360.com/sumy/auth/gopay/lxb.shtml";
  			}else {//用户购买了会员
  				//先判断用户之前是否已经查看过该联系方式，如果查看过则直接显示并且不需要计算次数
  				$.ajax({
  					url:"http://sumy.inc.hc360.com:80/sumy/public/smbusermobile/islooked.shtml",
  			        type:"POST",
  			        dataType:"json",
  			        data:{"oid":oid},
  			        contentType:"application/x-www-form-urlencoded; charset=UTF-8",
  			        timeout:5000,
  			        async: false,
  			        success:function(result){
  			        	var contectAlertId = "";
  			        	
  			        	if(result.display == "5"){
  			        		//jQuery(obj).parent().parent().find('.bei').text(1);
  			        		jQuery(obj).closest('dl').find('.bei').text(parseInt(jQuery(obj).closest('dl').find('.bei').text())+1);
  			        		jQuery(obj).closest('dl').find('.bei2').text(parseInt(jQuery(obj).closest('dl').find('.bei2').text())+1);
  			        	}
  			        	
  			        	if(result.display == "1" || result.display == "5" ){
  			        		$("#"+oid+"contectAlert1").children(".cAlertLeft").children(".cTel").html(result.mobile);
  			        		$("#"+oid+"contectAlert1").children(".cAlertLeft").children(".cName").html("联系人："+(result.username==null?"":result.username));
  			        		$("#"+oid+"contectAlert1").css({"display":"block"});
  			        		contectAlertId = oid + "contectAlert1";
  			        	}else if(result.display == "2"){
  			        		$("#"+oid+"contectAlert2").css({"display":"block"});
  			        		contectAlertId = oid + "contectAlert2";
  			        	}else if(result.display == "3"){
  			        		if(result.prodtype == "2"){
  			        			$("#"+oid+"contectAlert3").children(".cAlertCon").children(".cAlertConBot").html('<p>您可以<a href="http://sumy.inc.hc360.com/sumy/auth/gopay/lxb.shtml" target="_blank">升级尊享版</a>，可查看500条联系电话。</p>');
  			        		}if(result.prodtype == "3"){
  			        			$("#"+oid+"contectAlert3").children(".cAlertCon").children(".cAlertConBot").html('<p></p>');
  			        		}
  			        		$("#"+oid+"contectAlert3").css({"display":"block"});
  			        		contectAlertId = oid + "contectAlert3";
  			        	}else if(result.display == "4"){
  			        		$("#"+oid+"contectAlert4").css({"display":"block"});
  			        		contectAlertId = oid + "contectAlert4";
  			        	}
  			        	var ca = $(".contectAlert");
  			        	for(var i=0;i<ca.length;i++){
  			        		if(ca[i].id != contectAlertId){
  			        			$(ca[i]).css({"display":"none"});
  			        		}
  			        	}
  			        }
  				})
  			}
  		}
  	}
  	
  	function subWeChat(){
  		$("#smbAlertBox1").css({"display":"block"});
  		/* 微信订阅 
  		location.href = "http://sumy.inc.hc360.com/sumy/auth/subscribe/wechatsub.shtml";*/
  	}
</script>     
