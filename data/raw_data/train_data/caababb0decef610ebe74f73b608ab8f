	  	function autoScroll(obj){  
			$(".scrollBox2").find("dl").animate({  
				marginTop : "-34px"  
			},500,function(){  
				$(this).css({marginTop : "0px"}).find("dd:first").appendTo(this);  
			})  
		}  
		$(function(){  
			$.ajax({
  					url:"http://sumy.inc.hc360.com:80/sumy/public/smbcontacts/getRedis.shtml",
  			        type:"POST",
  			        dataType:"json",
  			        contentType:"application/x-www-form-urlencoded; charset=UTF-8",
  			        timeout:5000,
  			        async: true,
  			        success:function(result){
  			        	$("#yearspan").text(result.year);
  			        	$("#monthspan").text(result.month);
  			        	$("#dayspan").text(result.day);
  			        	$("#incount").text(result.incount);
  			        	$("#usecount").text(result.usecount);
  			        	$("#coverarea").text(result.coverarea);
  			        	$("#coverproduct").text(result.coverproduct);
  			        	$("#topArea").html(result.topArea+"（"+result.topAreaCount+"条）");
  			        	$("#topProduct").html(result.topProduct+"（"+result.topProductCount+"条）");
  			        	var str = '';
// 			        	var str = '<dd><span class="leftName" title="北京宏伟一方经贸有限公司">北京宏伟一方经贸有限公司</span><span class="rigDeta">2016-11-13</span></dd>';
// 			        	str += '<dd><span class="leftName" title="泰州坚盾安防公司">泰州坚盾安防公司</span><span class="rigDeta">2016-11-12</span></dd>';
// 			        	str += '<dd><span class="leftName" title="郑州卡特化工有限公司">郑州卡特化工有限公司</span><span class="rigDeta">2016-11-14</span></dd>';
// 			        	str += '<dd><span class="leftName" title="钟海强(个体商户)">钟海强(个体商户)</span><span class="rigDeta">2016-11-10</span></dd>';
//  			        	str += '<dd><span class="leftName" title="北京力度五金商贸有限公司">北京力度五金商贸有限公司</span><span class="rigDeta">2016-11-08</span></dd>';
//  			        	str += '<dd><span class="leftName" title="徐州力强机械有限公司">徐州力强机械有限公司</span><span class="rigDeta">2016-11-11</span></dd>';
//  			        	str += '<dd><span class="leftName" title="福州艾克家居纺织公司">福州艾克家居纺织公司</span><span class="rigDeta">2016-11-08</span></dd>';
  			        	for(var i=0;i<result.finishOrders.length;i++){
  			        		var obj = result.finishOrders[i];
  			        		str += '<dd><span class="leftName" title="'+obj.corpname+'">'+obj.corpname+'</span><span class="rigDeta">'+obj.updatetimeStr+'</span></dd>';
  			        	}
  			        	$("#finshOrders").html(str);
  			        }
  			});
			setInterval('autoScroll(".scrollBox2")',3000);
		}) 
