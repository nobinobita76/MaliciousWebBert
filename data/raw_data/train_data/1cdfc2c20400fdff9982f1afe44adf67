	function openQQ(){

		var appurl =  data.appurl || window.location.href;

		if(!appurl){
			return;
		}

		if(appurl.indexOf('_proxy=') === -1){
			appurl += (appurl.indexOf('?') === -1 ? '?' : '&') + '_proxy=1';
		}

		if(!window.mqq){
			return;
		}

		window.mqq.invoke('ui','openUrl',{
			url: appurl,
			target: 1,
			style: 0
		});

		setTimeout(function(){
			window.mqq.invoke('ui','popBack');
		},1000);
	}

	function addVia(url,via){

		if(url.indexOf('&via=') > -1){
			return url.replace(/&via=[^&]*/,'&via=' + via);
		}else if(url.indexOf('?via=') > -1){
			return url.replace(/\?via=[^&]*/,'?via=' + via);
		}else if(url.indexOf('?') > -1){
			return url += '&via=' + via;
		}else{
			return url += '?via=' + via;
		}

	}

	function getJumpUrl(via){
		var shareurl = data.appurl || window.location.href;

		if(via){
			shareurl = addVia(shareurl,via);
		}
		
		var jumpurl  = window.location.protocol + '//h5.qzone.qq.com/app/open/?_proxy=1&_proxyByURL=1&_wv=145191&appurl=' + encodeURIComponent(shareurl);

		return jumpurl;
	}

	function gotoQQ(){
		if(window.location.hostname === 'h5.qzone.qq.com'){
			if(data.qua && (data.qua.app === 'SQ')){
				openQQ();
			}else{
				window.location.href = ('mqqapi://forward/url?url_prefix=' + btoa(getJumpUrl()) + '&version=1&src_type=web');
			}
		}else{
			if(data.qua && (data.qua.app === 'QZ')){
				window.location.href  = getJumpUrl('H5.QZONE.SHARE');
				return;
			}else{
				if(String(navigator.userAgent).indexOf('MicroMessenger/') > -1){
					window.location.href  = 'https://qzs.qzone.qq.com/qzone/hybrid/page/jumpQQ.html?url=' + encodeURIComponent(getJumpUrl('H5.WECHAT.SHARE'));
				}else{
					window.location.href  = 'https://qzs.qzone.qq.com/qzone/hybrid/page/jumpQQ.html?url=' + encodeURIComponent(getJumpUrl());
				}

			}
		}

		if(data.qua && (data.qua.app === 'SQ' || data.qua.app === 'QZ')){
			setTimeout(function(){
				window.mqq.invoke('ui','popBack');
			},1000);
		}
	};

	function retry(){
		window.mqq.invoke('ui','openUrl',{
			url: window.location.href,
			target: 1,
			style: 0
		});

		setTimeout(function(){
			window.mqq.invoke('ui','popBack');
		},1000);
	}
</script>
