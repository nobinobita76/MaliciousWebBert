	<script src="/shikebiao/js/cities.js"></script>
	<script src="/shikebiao/js/searchhistory.js"></script>
    <script>
    var list = '[{"carrierName":"南方航空","carrierCode":"CZ","flightNo":"CZ3511","departureTime":"13:25","arrivalTime":"15:35","stopCity":"杭州","flyTime":"2h10m","departureAirportName":"新白云机场","departureAirportCode":"CAN","departureTermainalBuilding":" ","arrivalAirportName":"萧山机场","arrivalAirportCode":"HGH","arrivalTerminalBuilding":" ","mealType":"无","airplaneModel":{"minSeats":0,"maxSeats":0,"carrierName":"南方航空","code":"321"},"highLightFlag":false,"departureTimeSegment":"NOON","arrivalTimeSegment":"NOON","arrivalTimeFull":"2017-04-01 15:35:00","departureTimeFull":"2017-04-01 13:25:00","departureTimeDate":1491024300000,"arrivalTimeDate":1491032100000}]';
    $(function() {
    	//去掉footer部的部分内容
		$(".wrap").hide();
		$(".lv-footer").show();
		
		var $document = $(document);
        // 航线搜索
        $(".search-btn").on("click", function(e) {
            e.stopPropagation();
            var $this = $(this);
            if (validateFtInput($this)) {
            	searchSubmit();
        	}
        });

        // 航班搜索
        $(".search-flight-btn").on("click", function(e) {
            e.stopPropagation();
            var $this = $(this);
            if (validateInput($this)) {
            	var flightNo = $("#flightNo").val();
            	var reg = /^[0-9A-Za-z]{1,6}$/;
            	var re = new RegExp(reg);
            	if(re.test(flightNo)) {
            		window.location.href="/shikebiao/" + flightNo + ".html";
            	} else {
            		alert("请输入正确的航班号");
            	}
            }
        });

		// 排序
		$document.on("click",".ticket-sort",function () {
			var $this = $(this);
			$this.siblings().removeClass("ticket-sort-active");
			if(!$this.hasClass("ticket-sort-active")){
				$this.addClass("ticket-sort-active");
			} else {
				var $thisI = $this.find("i");
				$thisI.toggleClass("down");
				sortRepaint();
			}
		});
		showSearchCityHistory();
    });
    
    function searchSubmit(){
    	$(".ft-overlay, .ft-dialog-loading").show();
    	var deptCity=$("#deptCity_").val();
		var arrvCity=$("#arrvCity_").val();
        for(var i=0;i<allCities.length;i++){
            if(allCities[i].indexOf(deptCity)!=-1){
                deptCity=allCities[i].split("|")[3];
                $("input[name='departureAirportCode']").val(deptCity);
                break;
            }
        }
        
        for(var i=0;i<allCities.length;i++){
            if(allCities[i].indexOf(arrvCity)!=-1){
                arrvCity=allCities[i].split("|")[3];
                $("input[name='arrivalAirportCode']").val(arrvCity);
                break;
            }
        }
        var departureAirportCode=$('#departureAirportCode').val();
        var arrivalAirportCode =$('#arrivalAirportCode').val();
        writeSearchCityHistory();
        window.location.href="/shikebiao/" + departureAirportCode + "-" +arrivalAirportCode + ".html";
    }
    
    function lookSubmit(object){
    	$(".ft-overlay, .ft-dialog-loading").show();
    	var deptCity=$("#deptCity_").val();
		var arrvCity=$("#arrvCity_").val();
        for(var i=0;i<allCities.length;i++){
            if(allCities[i].indexOf(deptCity)!=-1){
                deptCity=allCities[i].split("|")[3];
                $("input[name='departureAirportCode']").val(deptCity);
                break;
            }
        }
        
        for(var i=0;i<allCities.length;i++){
            if(allCities[i].indexOf(arrvCity)!=-1){
                arrvCity=allCities[i].split("|")[3];
                $("input[name='arrivalAirportCode']").val(arrvCity);
                break;
            }
        }
        var departureAirportCode=$('#departureAirportCode').val();
        var arrivalAirportCode =$('#arrivalAirportCode').val();
        var date1=$(object).parent().prev().find('#go_Date').val();
        if(date1 == "") {
        	date1 = '2017-04-02';
        }
        var flightNo = $("#flightNo").val();
        //单程
    	var url = "";
        if(flightNo != null && flightNo != "") {
        	url = "http://flight.lvmama.com/booking/"+'CAN'+"-"+'HGH'+".html?date1="+date1+"&date2="+date1+"&routeType=OW&departureCityCode="+'CAN'+"&arrivalCityCode="+'HGH'+ "&flightNo=" + flightNo;
        } else {
        	flightNo = $(object).parent().parent().find(".tc-number").html();
        	url = "http://flight.lvmama.com/booking/"+departureAirportCode+"-"+arrivalAirportCode+".html?date1="+date1+"&date2="+date1+"&routeType=OW&departureCityCode="+departureAirportCode+"&arrivalCityCode="+arrivalAirportCode+ "&flightNo=" + flightNo;
        }
    	window.open(url);
    }
    
	 //排序算法
	 var sorting = {
			 	//出发时间升序
			    SortByDepUp: function(arr){
			        var len=arr.length, tmp;
			        for(var i=0;i<len-1;i++){
			            for(var j=0;j<len-1-i;j++){
			                if(arr[j].departureTimeFull>arr[j+1].departureTimeFull){
			                    tmp = arr[j];
			                    arr[j] = arr[j+1];
			                    arr[j+1] = tmp;
			                }
			            }
			        }
			        return arr;
			    },
			    //出发时间降序
			    SortByDepDown: function(arr){
			        var len=arr.length, tmp;
			        for(var i=0;i<len-1;i++){
			            for(var j=0;j<len-1-i;j++){
			                if(arr[j].departureTimeFull<arr[j+1].departureTimeFull){
			                    tmp = arr[j];
			                    arr[j] = arr[j+1];
			                    arr[j+1] = tmp;
			                }
			            }
			        }
			        return arr;
			    }
			}
	 function getSortedResult(){
		//根据筛选条件获取的结果
		var sortedResult = JSON.parse(list);
		//根据排序条件获取的结果
		var sortType =$('.ticket-sort-active').find("i").attr("class");
		if(sortType == "down"){//降序
			sortedResult = sorting.SortByDepDown(sortedResult);
		} else {//升序
			sortedResult = sorting.SortByDepUp(sortedResult);
		}
		return sortedResult;
	}
	 
	//重画排序后的界面
	function sortRepaint(){
		var sortedResult = getSortedResult(list);
		repaint(sortedResult);
	}
	var type = '2';
	function repaint(list){ 
		if(list != null && list.length > 0) {
			var html = "";
			for(var i=0;i<list.length;i++) {
				var flightInfo = list[i];
				if(flightInfo.airplaneModel != null && flightInfo.airplaneModel != ""
						&& flightInfo.airplaneModel.code != null && flightInfo.airplaneModel.code != "") { //有机型信息
					html += '<div class="ticket-item clearfix">';
					html += '<div class="ticket-company">';
					html += '<div class="tc-info"><i class="fh-icon-ac fh-icon-ac-' + flightInfo.carrierCode + '"></i>' + flightInfo.carrierName + '</div>';
					if(type == "1") {
						html += '<div class="tc-number"><a href="http://flight.lvmama.com/shikebiao/' + flightInfo.flightNo + '.html" target="_blank"><font color="#666">' + flightInfo.flightNo + '</a></div>';
					} else {
						html += '<div class="tc-number">' + flightInfo.flightNo + '</div>';
					}
					html += '</div>';
					html += '<div class="ticket-time">';
					html += '<div class="depa-time">' + flightInfo.departureTime + '</div>';
					html += '<div class="arrival-time">' + flightInfo.arrivalTime;
					if(flightInfo.highLightFlag) {
						html += "<em class=\"add-one-day\" tip-content=\"航班到达日期为起飞日期<span class='tip-add-one'>+1天</span>。\">+1</em>";
					}
					html += '</div>';
					html += '</div>';
					html += '<div class="ticket-airport">' + flightInfo.departureAirportName + '<br>' + flightInfo.arrivalAirportName + '</div>';
					html += '<div class="ticket-plane"><span data-plan="' + flightInfo.airplaneModel.code + '" data-name="' + flightInfo.airplaneModel.name + '" data-type="' + flightInfo.airplaneModel.airplaneType + '" data-min="' + flightInfo.airplaneModel.minSeats + '" data-max="' + flightInfo.airplaneModel.maxSeats + '">' + flightInfo.airplaneModel.code + '</span></div>';
					html += '<div class="ticket-schedule"><span class="active">一</span><span class="active">二</span><span class="active">三</span><span class="active">四</span><span class="active">五</span><span class="active">六</span><span class="active">日</span></div>';
					html += '<div class="ticket-meal">' + flightInfo.mealType + '餐食</div>';
					html += '<div class="ticket-date"><input type="text" class="ticket-date-input" readonly="readonly" placeholder="选择日期" id="go_Date"/><i></i></div>';
					html += '<div class="ticket-operation"><a href="javascript:;" class="btn btn-sm btn-orange" onclick="lookSubmit(this);">查看</a></div>';
					html += '</div>';
				} else { //无机型信息
					html += '<div class="ticket-item clearfix" style="position: relative;">';
					html += '<div class="ticket-company">';
					html += '<div class="tc-info"><i class="fh-icon-ac fh-icon-ac-' + flightInfo.carrierCode + '"></i>' + flightInfo.carrierName + '</div>';
					if(type == "1") {
						html += '<div class="tc-number"><a href="http://flight.lvmama.com/shikebiao/' + flightInfo.flightNo + '.html" target="_blank"><font color="#666">' + flightInfo.flightNo + '</a></div>';
					} else {
						html += '<div class="tc-number">' + flightInfo.flightNo + '</div>';
					}
					html += '</div>';
					html += '<div class="ticket-time">';
					html += '<div class="depa-time">' + flightInfo.departureTime + '</div>';
					html += '<div class="arrival-time">' + flightInfo.arrivalTime;
					if(flightInfo.highLightFlag) {
						html += "<em class=\"add-one-day\" tip-content=\"航班到达日期为起飞日期<span class='tip-add-one'>+1天</span>。\">+1</em>";
					}
					html += '</div>';
					html += '</div>';
					html += '<div class="ticket-airport">' + flightInfo.departureAirportName + '<br>' + flightInfo.arrivalAirportName + '</div>';
					html += '<div class="ticket-plane"><span data-plan="" data-name="" data-type="" data-min="" data-max=""></span></div>';
					html += '<div class="ticket-schedule" style="position: relative;left:7.45%;"><span class="active">一</span><span class="active">二</span><span class="active">三</span><span class="active">四</span><span class="active">五</span><span class="active">六</span><span class="active">日</span></div>';
					html += '<div class="ticket-meal" style="position: relative;left:7.45%;">' + flightInfo.mealType + '餐食</div>';
					html += '<div class="ticket-date" style="position: relative;left:7.45%;"><input type="text" class="ticket-date-input" readonly="readonly" placeholder="选择日期" id="go_Date"/><i></i></div>';
					html += '<div class="ticket-operation" style="position: relative;left:7.45%;"><a href="javascript:;" class="btn btn-sm btn-orange" onclick="lookSubmit(this);">查看</a></div>';
					html += '</div>';
				}
			}
			$("#ticketContent").html(html);
			initcalender();
		}
	}
	
	function initcalender() {
		var ticketDateCalendar = lv.calendar({
			trigger: ".ticket-date-input",
			triggerEvent: "click",
			bimonthly: true,
			template: "small"
		});
		$(document).on("click",".ticket-date i",function (e){
			e.stopPropagation();
			var $this = $(this);
			$this.siblings(".ticket-date-input").click();
		});
	}
    </script>
