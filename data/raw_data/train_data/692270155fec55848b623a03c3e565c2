<script>
    $(window).load(function(){
        // swiper1 js
        var mySwiper=new Swiper('.swiper1',{
            slidesPerView: 'auto',
        })
        $('.swiper1 .swiper-slide').on('click', function(event) {
            swiper.setWrapperTranslate(0);
            event.preventDefault();
            mySwiper.slideTo(mySwiper.clickedIndex-1,500,false);
            commodityShow(mySwiper.clickedIndex);
            $('.swiper1 .swiper-slide a').removeClass('active');
            $(this).find('a').addClass('active');
        });

        // swiper2 js
        var swiper = new Swiper('.swiper2', {
            scrollbar: '.swiper-scrollbar',
            direction: 'vertical',
            slidesPerView: 'auto',
            mousewheelControl: true,
            autoHeight: true,
            freeMode: true,
        });

        function commodityShow(num){
            switch(num){
                case 0:
                $('.commodity').show();
                swiper.update();
                break;
                case 1:
                $('.commodity.game__style_xxyx').show();
                $('.commodity.game__style_yljc').hide();
                swiper.update();
                break;
                case 2:
                $('.commodity.game__style_xxyx').hide();
                $('.commodity.game__style_yljc').show();
                swiper.update();
                break;
            }
        }


        var type="2";

        if (type == "1")
        {

            $('.swiper1 .swiper-slide a').removeClass('active');
            $('.swiper1 .swiper-slide a').eq(1).addClass('active');
            commodityShow(1);

        }
        else if (type == "2")
        {
            $('.swiper1 .swiper-slide a').removeClass('active');
            $('.swiper1 .swiper-slide a').eq(2).addClass('active');
            commodityShow(2);
        }
    })
</script>
