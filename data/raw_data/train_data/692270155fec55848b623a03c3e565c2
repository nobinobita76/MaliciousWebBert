</div><script>
    var helpId = "";
$('.getHelpBean').on('click', function(){
    if (!helpId)
    {
        alert('请先登录');
        location.href="http://h5.7u.com/Member/loginJs";
        return false;
    }

    $.ajax({
        url:'http://h5.7u.com/Vip/reliefInfo',
        type:'get',
        dataType:'json',
        async:false,
        success:function(res){
            if(res.CODE == 'ok')
            {
                $('.jjj-info .cl-orange').text(res.DATA.Award);
                $('.jjj-info .last_cnt').text(res.DATA.Cnt);
                $('.jjj-info').show();
            }
            else if (res.CODE == 'ObjectNotExist:VipUserModel')
            {
                $('.foot_open_vip').show();
            }
            else
            {
                $('.no-jjj .showmsg-content').text(res.MESSAGE);
                $('.no-jjj').show();
            }
        }
    });

});

$('.jjj-i-konw').click(function(){
    $('.no-jjj').hide();
});

$('.now-get-jjj').click(function(){
    $.ajax({
        url:'http://h5.7u.com/Vip/getReliefAward',
        type:'get',
        dataType:'json',
        async:false,
        success:function(res){
            if(res.CODE == 'ok')
            {
                alert('成功领取了'+res.DATA.Result+'救济金！');
                location.reload();
            }
            else
            {
                //showMsg('1', );
                alert('您当前未达到领取条件！');
            }
        }
    });
});
</script>
