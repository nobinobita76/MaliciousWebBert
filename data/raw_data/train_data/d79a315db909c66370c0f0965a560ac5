  <script>
		$(function(){
			var li=$(".newList li")
			for (var i = 0; i < li.length; i++) {
				var children=li[i].children;
				for (var j = 0; j < children.length; j++) {
					if(children[j].nodeName.toLowerCase()=="p"){
						$(".new_right").eq(i).css("width","640px")
					}
				}
			}
		})
</script> 
  <script>
$(function(){
if($(".amaina p")){
var p=$('amaina p').find('span')
	for (var i = 0; i < p.length; i++) {
		var $s=p.eq(i).closest('p').attr('style')
		var $p=p.eq(i).attr('style')
		p.eq(i).attr('style',$s+$p)
	}

var em=$('main p').find('span').find('em')
	for (var i = 0; i < em.length; i++) {
		var $emp=em.eq(i).closest('p').attr('style')
		var $ems=em.eq(i).closest('span').attr('style')
		em.eq(i).attr('style',$emp+$ems)
	}
console.log(11111111111)
}else{}
})

</script> 
