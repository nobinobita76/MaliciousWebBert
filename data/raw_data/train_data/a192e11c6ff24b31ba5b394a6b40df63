                    }());
                </script>
                <script type="text/javascript">
                    (function () {
                        syncFavor();
                        //收藏数据的同步处理
                        function syncFavor() {
                            var favoriteUnits = getFavoriteUnits();
                            if (favoriteUnits.length > 0)
                            {
                                var ids = {};
                                $(favoriteUnits).each(function () {
                                    ids[this.UnitID] = this.AddTime;
                                });
                                $.ajax.post({
                                    url: 'https://vip.tujia.com/MFavorite/SyncLocalFavorite/',
                                    parameter: { unitIdJson: JSON.stringify(ids) },
                                    returnType: 'json',
                                    onSuccess: function (returnObj) {
                                        if (returnObj.result === 0)
                                        {
                                            $.storage.remove('favoriteUnits');
                                        }
                                        //触发同步本地收藏至服务端操作完成的动作
                                        $.trigger('afterSyncFavor');
                                    },
                                    onFail: function () {
                                        //触发同步本地收藏至服务端操作完成的动作
                                        $.trigger('afterSyncFavor');
                                    }
                                });
                            }
                            else
                            {
                                //触发同步本地收藏至服务端操作完成的动作
                                $.trigger('afterSyncFavor');
                            }
                        }
                        function getFavoriteUnits() {
                            var favoriteUnits = $.storage.containsKey('favoriteUnits') ? $.storage.getJson('favoriteUnits') : null;
                            if ($.checkType(favoriteUnits) !== type.eArray)
                            {
                                favoriteUnits = [];
                            }
                            return favoriteUnits;
                        }
                    }());
                </script>


            



            
