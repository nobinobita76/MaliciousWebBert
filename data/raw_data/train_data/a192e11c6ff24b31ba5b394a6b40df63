    <script type="text/javascript">
        (function () {
            var enableValidateImage = true;
            var dialogContext = $.dialog;
            var loadingHtmlId = 'loadingImg';
            var tabSwitchedHandler = function() {
                var mobile="";
                if($('.current')[0].getAttribute('tabindex')==0){
                    mobile = $('#username').getVal();
                    if(!$.isStringEmpty(mobile)){
                        $('#mobile').setVal(mobile);
                    }
                }
                else{
                    mobile = $('#mobile').getVal();
                    if(!$.isStringEmpty(mobile)){
                        $('#username').setVal(mobile);
                    }
                }
            };
            $('#tabLoginContent').createTab({onTabSwitched: tabSwitchedHandler});
            $('#sendVercode').click(function () {
                if (!$.validate.valid($('#tabLoginContent .mtab-content *[tabindex="0"] li')[0]))
                {
                    return;
                }
                var mobile = $('#mobile').getVal();
                if (enableValidateImage)
                {
                    tjGlobal.validateImageDialog.show({
                        dialogUrl: '/H5Site/VerifyImageDialog',
                        dialogContext: $.dialog,
                        loadingHtmlId: 'loadingImg',
                        onCommit: function (arg) {
                            realLoginSendCode(arg.verifyCode);
                        }
                    });
                }
                else
                {
                    realLoginSendCode();
                }

                function realLoginSendCode(vercode) {
                    var p = { mobile: mobile };
                    if (!$.isStringEmpty(vercode))
                    {
                        p.imageCode = vercode;
                    }
                    dialogContext.showLoading(loadingHtmlId);
                    $.ajax.get({
                        url: '/H5Site/SendSMSLoginCode',
                        // json
                        parameter: p,
                        // 'text'(default), 'json', 'html'
                        returnType: 'json',
                        onSuccess: function (returnObj) {
                            dialogContext.closeCurrentDialog();
                            if (returnObj.isSuccess)
                            {
                                resetSendWaiting();
                            }
                            else
                            {
                                if (returnObj.errorCode === 10001)
                                {
                                    //需要输入图片验证码
                                    tjGlobal.validateImageDialog.show({
                                        dialogUrl: '/H5Site/VerifyImageDialog',
                                        dialogContext: $.dialog,
                                        loadingHtmlId: 'loadingImg',
                                        onCommit: function (arg) {
                                            realLoginSendCode(arg.verifyCode);
                                        }
                                    });
                                }
                                else
                                {
                                    dialogContext.alert(returnObj.message);
                                }
                            }
                        },
                        onFail: function () {
                            dialogContext.closeCurrentDialog();
                            dialogContext.alert('发送手机验证码失败!');
                        }
                    });
                }

                function resetSendWaiting() {
                    var btnSend = $('#sendVercode');
                    btnSend.hide();
                    var btn = $('#sendedVercode');
                    btn.text('60秒后重发');
                    btn.show();
                    var timer = 60, timerout;
                    timerout = setInterval(function () {
                        timer--;
                        btn.text(timer + '秒后重发');
                        if (timer < 0)
                        {
                            clearInterval(timerout);
                            btn.hide();
                            btnSend.show();
                        }
                    }, 1000);
                }
            });
            $('#submit0').click(function () {
                if (!$.validate.valid($('#tabLoginContent .mtab-content *[tabindex="0"]')[0]))
                {
                    return;
                }
                var mobile = $('#mobile').getVal();
                var smsCode = $('#vercode').getVal();
                var loginData = {
                    mobile: mobile,
                    smsCode: smsCode,
                    userExpiresHours: true
                };
                dialogContext.showLoading(loadingHtmlId);
                $.ajax.post({
                                        url: '/H5Site/LoginNormal',
                    // json
                    parameter: loginData,
                    // 'text'(default), 'json'
                    returnType: 'json',
                    onSuccess: function (returnObj) {
                        dialogContext.closeCurrentDialog();
                        if (returnObj.isSuccess)
                        {
                            if (returnObj.redirectUrl) {
                                dialogContext.showLoading(loadingHtmlId, function () {
                                    $.loadPage(returnObj.redirectUrl);
                                });
                            }
                            else {
                                dialogContext.showLoading(loadingHtmlId, function () {
                                    $.loadPage('http://vip.tujia.com/nosearch/m_orders?page');
                                });
                            }
                        }
                        else
                        {
                            dialogContext.alert(returnObj.message);
                        }

                    },
                    onFail: function (e) {
                        dialogContext.closeCurrentDialog();
                        if (e.type === 'timeout')
                        {
                            dialogContext.alert('登录超时!');
                        }
                        else
                        {
                            dialogContext.alert('登录失败!');
                        }
                    }
                });
            });
            $('#submit1').click(function () {
                if (!$.validate.valid($('#tabLoginContent .mtab-content *[tabindex="1"]')[0]))
                {
                    return;
                }
                var username = $('#username').getVal();
                var password = $('#password').getVal();
                var loginData = {
                    username: username,
                    pwd: password,
                    userExpiresHours: true
                };
                $.ajax.post({
                    url: '/H5Site/LoginIsNeedVerifyCode',
                    // 'text'(default), 'json'
                    returnType: 'json',
                    onSuccess: function (returnObj) {
                        var isNeedVerifyCode = returnObj.IsNeed;
                        if (isNeedVerifyCode) {
                            tjGlobal.validateImageDialog.show({
                                dialogUrl: '/H5Site/VerifyImageDialog',
                                dialogContext: $.dialog,
                                loadingHtmlId: 'loadingImg',
                                onCommit: function (arg) {
                                    realLogin(arg.verifyCode);
                                }
                            });
                        }
                        else {
                            realLogin();
                        }
                    },
                    onFail: function (returnObj) { }
                });

                function realLogin(imageCode) {
                    if (!$.isStringEmpty(imageCode))
                    {
                        loginData.verifycode = imageCode;
                    }

                    dialogContext.showLoading(loadingHtmlId);
                    $.ajax.post({
                        url: '/H5Site/Login',
                        // json
                        parameter: loginData,
                        // 'text'(default), 'json'
                        returnType: 'json',
                        onSuccess: function (returnObj) {
                            dialogContext.closeCurrentDialog();
                            if (returnObj.isSuccess)
                            {
                                if (returnObj.redirectUrl) {
                                    dialogContext.showLoading(loadingHtmlId, function () {
                                        $.loadPage(returnObj.redirectUrl);
                                    });
                                }
                                else {
                                    dialogContext.showLoading(loadingHtmlId, function () {
                                        $.loadPage('http://vip.tujia.com/nosearch/m_orders?page');
                                    });
                                }
                            }
                            else
                            {
                                dialogContext.alert(returnObj.message);
                            }

                        },
                        onFail: function (e) {
                            dialogContext.closeCurrentDialog();
                            if (e.type === 'timeout')
                            {
                                dialogContext.alert('登录超时!');
                            }
                            else
                            {
                                dialogContext.alert('登录失败!');
                            }
                        }
                    });
                }

            });
            $('.go2Register').click(function () {
                dialogContext.showLoading(loadingHtmlId, function () {
                    $.loadPage('/H5Site/RegisterPage?srcUrl=http%3A%2F%2Fvip.tujia.com%2Fnosearch%2Fm_orders%3Fpage');
                });
            });
            $('#go2ForgetPwd').click(function () {
                dialogContext.showLoading(loadingHtmlId, function () {
                    $.loadPage('/H5Site/ResetPassword');
                });
            });

        }());
    </script>


