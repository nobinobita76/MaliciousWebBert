                        tjGlobal.rightTopMenu.enableLocateOfRightTopMenu = true;
                        tjGlobal.rightTopMenu.enableLoginOfRightTopMenu = true;
                        tjGlobal.rightTopMenu.reload({ isLogin: false });
                    });
                </script>
                        

