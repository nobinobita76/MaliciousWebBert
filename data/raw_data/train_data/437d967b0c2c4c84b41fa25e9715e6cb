<script type="text/javascript">
	var reqLoginMode = 0;//0 正常登录 1 异步登录
	var hasSubmit = false;
	var module = -1;
	//getElByid
	function $(id){ 
		return document.getElementById(id);
	}
	
	var safeEdit = '';
	function $(id){
		return document.getElementById(id);
	}
	function AsynLoadFrame(id) {
		var d = $(id);

	    if ( -1 == navigator.userAgent.indexOf("MSIE") ) {
    	    d.setAttribute("src", d.getAttribute("alt"));
	    }
    	else {
			//alert(d.src + d.getAttribute("alt"));
			d.setAttribute("src", d.getAttribute("alt"));
        	//d.location = d.getAttribute("alt");
		}
	}
	//AsynLoadFrame("frame_logo");
	//AsynLoadFrame("frame_lftcol");
	//AsynLoadFrame("iframe2");
	//AsynLoadFrame("iframe3");
	
	var Util = {};
	Util.Cookie = {
		get:function(v_name){
			var lowerCookieName = v_name.toLowerCase();
			var cookieStr = document.cookie;
			
			if (cookieStr == ""){
				return "";
			}
			
			var cookieArr = cookieStr.split("; ");
			var pos = -1;
			
			for (var i=0; i<cookieArr.length; i++){
				pos = cookieArr[i].indexOf("=");
				if ((cookieArr[i].substring(0, pos).toLowerCase()) == lowerCookieName){
					return cookieArr[i].substring(pos+1, cookieArr[i].length);
				}
			}
			return "";
		},
		getVal:function(v_offset){
			var endstr = document.cookie.indexOf (";", v_offset);
			if (endstr == -1){
				endstr = document.cookie.length;
			}
			return unescape(document.cookie.substring(v_offset, endstr));		
		},
		set:function(v_name, v_value){
			var expdate = new Date((new Date()).getTime() + 604800000);
			var domain = "sdo.com";
			document.cookie = v_name + "=" + escape (v_value) +"; expires="+ expdate.toGMTString()+((domain == null) ? "" : ("; domain=" + domain));
		},
		setStatSession:function(v_name,v_value,domain,path){
			document.cookie = v_name + "=" + escape(v_value) +(domain == null?"":("; domain="+domain))+((path == null)? "" : ("; path=" + path));
		},
		del:function(v_name){
			var exp = new Date();
			exp.setTime (exp.getTime() - 1);
			var cval = Util.Cookie.get(v_name);
		
			document.cookie = v_name + "=" + cval + "; expires="+ exp.toGMTString();
		}
	};

	function RedirectURL(v_url, v_blank){
		if (v_url == ""){
			return;
		}
		if (!v_blank) {
			location.href = v_url;
		}else{
			window.open(v_url);
		}
	}
	function isSDID(username){
		if(username.length == 0 || !isNaN(username)){
			return false;
		}
		return true;
	}
	// 清掉两边空格
	function trim(str) {
		return str.replace(/^\s+/, '').replace(/\s+$/, '');
	}
	
	function check_form(){
		if(hasSubmit) {
			return;
		}
		hasSubmit = true;
		
        var ptname = $('ptname'),
        ptpwd    = $('ptpwd'),
		username = ptname.value,
		password =  ptpwd.value;
		
		if (ValidateInput(username) == false){
			hasSubmit = false;
			return false;
		}
		if (username == "" || username == ptname.getAttribute('placeholder')){
		    alert ("请您输入盛大游戏通行证");
			$("ptname").focus();
			showOutline($("ptname"), 'inputRed');
			hasSubmit = false;
			return false;
		}
		
		if (password == "" || password == "请输入密码" ){
			alert ("请您输入密码");
			if(module > 0){
				$('safe_edit').focus();
			}else{
				ptpwd.focus();
				showOutline(ptpwd, 'inputRed');
			}
			hasSubmit = false;
			return false;
		}
		$('username').value = username;
		$('password').value = password;

		reqLoginMode == 0 ? loginByPage():loginByAsync();
	}
   
	//通过页面提交方式进行提交
	function loginByPage(){
		var ptname = $('ptname'),username = ptname.value;
		setCookieByServer("login_input_name",username,"sdo.com");
		try{
			$('loginCustomerUrl').value = window.top.location;
			//alert($('loginCustomerUrl').value);
		}catch(e){}
		
		try{
			document.charset = "GB2312";
		}catch(ex){}

		window.setTimeout('formsubmit()',500);
		return true;
	}
	
	//通过异步的方式进行提交
	var accessTimer;
	function loginByAsync(){
		reqLoginData.username = trim($('ptname').value);
		reqLoginData.password = trim($('ptpwd').value);
//		accessTimer = window.setTimeout(blockLoginPage,500);
		//blockLoginPage();
		var LoginAuth = new LoginNS.Authen(reqLoginUrl,reqLoginData,accessSucc,accessFail,10,accessExp);
		window.setTimeout(function(){LoginAuth.pwdLogin();},0);
	}


	//访问成功后的回调接口
	function accessSucc(data){
		window.clearTimeout(accessTimer);
		hasSubmit = false;//释放登录按钮
		if(data.nextUrl.length>0){
//			$('#loging').html("正在进入应用，请稍候...");		
			if(data.nextPageType == 0){
				top.location = data.nextUrl;
			}else{
				location = data.nextUrl;		
			}
		}else if(data.msg){
//			unbBlockLoginPage();
//			$("#pass_tips").text(data.msg).show();
		}
	}
	
	function accessFail(){
		window.clearTimeout(accessTimer);
		hasSubmit = false;
//		unbBlockLoginPage();
//		$("#pass_tips").text("登录服务正在维护中，请稍候再试...").show();
	}
	
	function accessExp(){
		window.clearTimeout(accessTimer);
		hasSubmit = false;
//		unbBlockLoginPage();
//		$("#pass_tips").text("登录服务正在维护中，请稍候再试...").show();
	}

	//锁住当前页面，禁止用户做其他行为
