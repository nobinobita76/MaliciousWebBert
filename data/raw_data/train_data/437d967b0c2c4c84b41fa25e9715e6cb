	// 用户行为统计代码
	function RecordIpicStaticSdo(authId, type, msg) {
		if (window.sdoLogger) {
			sdoLogger.authId = authId;
			sdoLogger.clickLog("type=" + type + "&clickTarget=" + msg);
		}
	}
	
	clog('LoginSDOLoad');
