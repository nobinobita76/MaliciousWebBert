	function formsubmit(){
//		try{document.charset = "GB2312";}catch(e){}
		$("form").submit();
	}

	function submit_form(evt){
		if (evt.keyCode == 13){
			check_form();
		}
	}

	function ValidateInput(sInputVal){
		sInputVal = "a" + sInputVal;//"    
		var iu, iuu, regArray = new Array("\"","`","<",">","'",";");
		iuu = regArray.length;
		for(iu = 0;iu < iuu ; iu ++){ 		
			if (sInputVal.indexOf(regArray[iu]) > 0){
				alert("注册名不可以包含：" + regArray[iu] );
				return false;
			}
		}
		return true;
	}
	function showOutline(e, color){
		color = color ? color : 'inputGreen';
		e = e.parentNode.parentNode;
		if(e.className.indexOf(color) < 0) e.className += ' '+ color + ' ';
	}
	function hideOutline(e){
		e = e.parentNode.parentNode;
		if(e.className.length > 0){
			var classNames = ' ' + e.className + ' ';
			classNames = classNames.replace(' inputGreen ', ' ').replace(' inputRed ', ' ');
			e.className = classNames.replace(/^(\s|\u00A0)+|(\s|\u00A0)+$/g, "");;
		}
	}

    //服务端写cookies函数
    function setCookieByServer(name,value,domain)
    { 
        var url =  location.protocol + '//' + location.host + '/sdo/Login/Tool.php?act=setSessionCookie&name=' + encodeURIComponent(name) + '&value=' + encodeURIComponent(value);         
        new Image().src = url;
    }

	//给url增加参数
	function addPara( url,key,value )
	{ 
		return parseUrl( url ).addPara( key,value ).toString();
	}  
	//给url删除参数    
	function delPara( url,key )
	{
		return parseUrl( url ).delPara( key ).toString();
	}    
	//url分析器,对上面两个函数进行支持
	function parseUrl( url )
	{
		var lnk = document.createElement('a'); 
		lnk.href = url;
		var path = lnk.protocol + '//' + lnk.host + '/' + lnk.pathname.replace(/^\//,''),
		params = lnk.search.replace(/^\?/,''),
		hash   = lnk.hash,
		paraArr = params ? params.split('&') : [],
		paraObj = {},
		curr,result;
		for(var i=0,l=paraArr.length; i<l; i++)
		{
			curr = paraArr[i].split('=');
			paraObj[curr[0]] = curr[1];            
		};
		result = {
			
			path     : path,
			params   : paraObj,
			hash     : hash,
			addPara  : function( key,value ){ this.params[key] = value;return this; },
			delPara  : function( key ){ delete this.params[key]; return this; },
			toString : function()
			{
				var paraArr = [],paraObj = this.params;
				for( var p in paraObj )paraArr.push( p + '=' + paraObj[p] );  
				return this.path + '?' + paraArr.join('&') + this.hash;
			}
		};
		lnk = path = params = hash = paraArr = paraObj = curr = null;
		return result;
	}
	
	//写cookies函数
	function setCookie(name,value)//两个参数，一个是cookie的名子，一个是值
	{
		var Days = 30; //此 cookie 将被保存 30 天
		var exp  = new Date(); 
		exp.setTime(exp.getTime() + Days*24*60*60*1000);
		document.cookie = name + "="+ escape (value) + ";expires=" + exp.toGMTString() +";path=/";//path是cookie的访问路径
	}
	//取cookies函数  
	function getCookie(name)      
	{
		var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
		if(arr != null)
			return unescape(arr[2]);
		else
			return null; 
	}
	//删除cookie
	function delCookie(name)
	{
		var exp = new Date();
		exp.setTime(exp.getTime() - 1);
		var cval=getCookie(name);
		if(cval!=null)
		document.cookie= name + "="+cval+";expires="+exp.toGMTString();
	} 
	
	function addEvent( el,type,fn )
	{
		if(el.addEventListener)
		{   
			el.addEventListener(type,fn,false);
		}
		else
		{
			el.attachEvent('on'+type,fn);
		}
	}

	var timerHandle = '';
	/*try{
		$('ptname').focus();
	}catch(e){}*/
    function reportPagePref( url )
    {
		var p = PagePerformance, cp = {};
		cp.bgStart = p.bgStart;
		cp.bgStart_bgEnd = p.bgEnd - p.bgStart;
		cp.contentLoaded = p.contentLoaded;
		cp.contentLoaded_headStart = p.headStart - p.contentLoaded;
		cp.headStart_bodyStart = p.bodyStart - p.headStart;
		cp.bodyStart_bodyEnd   = p.bodyEnd - p.bodyStart;
		cp.bodyEnd_onloadStart = p.onloadStart - p.bodyEnd;
		cp.onloadStart_onloadEnd   = p.onloadEnd - p.onloadStart;
		cp.BeaconJSStart_BeaconJSEnd = p.BeaconJSEnd - p.BeaconJSStart; 
		cp.type = 'php';
		var params    = [];
		for( var i in cp) 
			params.push( i + '=' + cp[i] );
		new Image().src =  url + '?' + params.join('&') + '&rand=' + Math.random();    
    };
    PagePerformance.bodyEnd = +new Date;
    PagePerformance.bgEnd     = "1491059906141"; 
    PagePerformance.BeaconJSStart = +new Date;
	
