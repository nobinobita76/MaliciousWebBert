			}
		});
	}

	// 对方是否有本站链接
	function getbacklink(id,domain,link_list,spider,re){
		if(!re){
			var url = link_list[id];
			if(id==link_list.length){
				return false;
			}
		}else{
			var url = link_list;
			$('#link'+(id+1)).html('<img src="http://static.aizhan.com/images/loading.gif" />：正在访问对方网页');
		}

		var rfunc = 'getbacklink('+id+',\''+domain+'\',\''+url+'\',\''+spider+'\',1);',
				rhtml = '<a href="javascript:void(0);" onclick="'+rfunc+'" id="link'+id+'">重新查询</a>',
				elem = $('#link'+(id+1));

		$.ajax({
			url:"http://linkche.aizhan.com/backlink",
			type:"GET",
			data:{'mapUrl':domain,'url':encodeURIComponent(url),'agent':spider,'cc':'e326699a1b2fffbc27b0edb092102df5','rn':1491093504},
			dataType:"json",
			timeout:8000,
			cache:false,
			error:function(XMLHttpRequest,textStatus,errorThrown){
				if(!re){
					getbacklink((id+1),domain,link_list,spider);
				}
				if(textStatus=="timeout"){
					elem.html('<font color="red" style="padding-right:8px;">请求超时!</font>'+rhtml);
				}else{
					elem.html('<font color="red" style="padding-right:8px;">请求出错!</font>'+rhtml);
				}
			},
			success:function(json){
				if(!re){
					getbacklink((id+1),domain,link_list,spider);
				}
				
				var html = "",
						phtml = "";

				// 错误信息
				if(Object.prototype.toString.call(json) !== '[object Object]'){
					elem.html('<font color="red" style="padding-right:8px;">返回错误!</font>'+rhtml);
					return;
				}
				if(!!json['error']){
					elem.html('<font color="red" style="padding-right:8px;">'+json['error']['message']+'</font>'+rhtml);
					return;
				}
				if(!!json['robots']){
					elem.html('<font color="red">'+json['robots']+'</font>');
					return;
				}
				// 链接
				if(!!json['sum']){
					if(!!json['mapLink']){
						// nofollow
						if(!!json['mapNofollow']){
							html += "<div><font color='red' style='padding-right:8px;'>发现NOFOLLOW!</font> 首页链接：<a rel='nofollow' href='"+json['mapLink']+"' title='"+json['mapLink']+"' style='padding-right:8px;' target='_blank'>"+json['mapVal']+"</a></div>";
						}else{
							html += "<div>首页链接：<a rel='nofollow' href='"+json['mapLink']+"' title='"+json['mapLink']+"' style='padding-right:8px;' target='_blank'>"+json['mapVal']+"</a></div>";
						}
						if(!!json['position']){
							phtml = json['position']+"/";
						}
					}
					if(!!json['linkOther']){
						html += '<div class="other-link">非首页链接：';
						if(json['linkOther'].length>2){
							var tiphtml = '<div class="other-link-more"><a rel="nofollow" href="javascript:void(0);">等...</a></div><div class="other-link-tip">其他非首页链接：';
						}
						for (var i = 0; i < json['linkOther'].length; i++) {
							if(i<2){
								html += "<a rel='nofollow' href='"+json['linkOther'][i]+"' title='"+json['linkOther'][i]+"' style='padding-right:4px;' target='_blank'>"+json['linkOtherVal'][i]+"</a>";
							}else{
								tiphtml += "<a rel='nofollow' href='"+json['linkOther'][i]+"' title='"+json['linkOther'][i]+"' style='padding-right:8px;' target='_blank'>"+json['linkOtherVal'][i]+"</a>";
							}
						};
						if(json['linkOther'].length>2){
							tiphtml += '</div>';
							html += tiphtml;
						}
						html += '</div>';
					}
					if(!!!json['mapLink'] && !!!json['linkOther']){
						html += '<font color="red" style="padding-right:8px;">首页无本站链接!</font>';
					}
					if(!!json['linkSubdomain']){
						html += '<div class="subdomains-link">子域名链接：';
						if(json['linkSubdomain'].length>2){
							var tiphtml = '<div class="subdomains-link-more"><a href="javascript:void(0);">等...</a></div><div class="subdomains-link-tip">其他子域名链接：';
						}
						for (var i = 0; i < json['linkSubdomain'].length; i++) {
							if(i<2){
								html += "<a rel='nofollow' href='"+json['linkSubdomain'][i]+"' title='"+json['linkSubdomain'][i]+"' style='padding-right:4px;' target='_blank'>"+json['linkSubdomainVal'][i]+"</a>";
							}else{
								tiphtml += "<a rel='nofollow' href='"+json['linkSubdomain'][i]+"' title='"+json['linkSubdomain'][i]+"' style='padding-right:8px;' target='_blank'>"+json['linkSubdomainVal'][i]+"</a>";
							}
						};
						if(json['linkSubdomain'].length>2){
							tiphtml += '</div>';
							html += tiphtml;
						}
						html += '</div>';
					}
					html += "外链数："+phtml+json['sum'];
				}else{
					html += '<font color="red">首页无本站链接!</font>';
				}
				elem.html(html);
				if(!!json['linkOther']){
					if(json['linkOther'].length>2){
						OtherlinkTip(id);
					}
				}
				if(!!json['linkSubdomain']){
					if(json['linkSubdomain'].length>2){
						subDomainsTip(id);
					}
				}
			}
		});
	}
	function OtherlinkTip(i){
		var obj = $('.link_list').eq(i),
				obj_link = obj.children('.other-link'),
				obj_tip = obj_link.children('.other-link-tip'),
				obj_more = obj_link.children('.other-link-more'),
				top = (obj_more.offset().top-obj.offset().top)-obj_tip.height()-19;

		obj_tip.css('top',top+'px');
		obj_more.bind('mouseover',function(){
			obj_tip.show();
		});
		obj_tip.bind('mouseover',function(){
			$(this).show();
		});
		obj_link.bind('mouseout',function(){
			obj_tip.hide();
		});
	}
	function subDomainsTip(i){
		var obj = $('.link_list').eq(i),
				obj_link = obj.children('.subdomains-link'),
				obj_tip = obj_link.children('.subdomains-link-tip'),
				obj_more = obj_link.children('.subdomains-link-more'),
				top = (obj_more.offset().top-obj.offset().top)-obj_tip.height()-19;

		obj_tip.css('top',top+'px');
		obj_more.bind('mouseover',function(){
			obj_tip.show();
		});
		obj_tip.bind('mouseover',function(){
			$(this).show();
		});
		obj_link.bind('mouseout',function(){
			obj_tip.hide();
		});
	}
	if($(".search_domain b").width()>200){
		$(".search_domain b").css({"float":"left","width":"200px","white-space":"nowrap","text-overflow":"ellipsis","overflow":"hidden"});
		$(".search_domain").hover(function(){
			$(this).css("position","relative");
			$(this).find("p").css({"position":"absolute","z-index":"9","left":"0","top":"0","white-space":"nowrap","background":"#fff"});
			$(this).find("b").removeAttr("style");
		},function(){
			$(this).removeAttr("style");
			$(this).find("p").removeAttr("style");
			$(this).find("b").css({"float":"left","width":"200px","white-space":"nowrap","text-overflow":"ellipsis","overflow":"hidden"});
		});
	}

	function openShutManager(oSourceObj,oTargetObj,shutAble,oOpenTip,oShutTip){
		var sourceObj = typeof oSourceObj == "string" ? document.getElementById(oSourceObj) : oSourceObj;
		var targetObj = typeof oTargetObj == "string" ? document.getElementById(oTargetObj) : oTargetObj;
		var openTip = oOpenTip || "";
		var shutTip = oShutTip || "";
		if(targetObj.style.display!="none"){
			if(shutAble) return;
			targetObj.style.display="none";
			sourceObj.parentNode.parentNode.removeAttribute('style');
			document.getElementById('multUrl').value="";
			if(openTip && shutTip){
				sourceObj.value = shutTip;
			}
		} else {
			targetObj.style.display="block";
			sourceObj.parentNode.parentNode.style.paddingRight=0;
			if(openTip && shutTip){
				sourceObj.value = openTip;
			}
		}
	}
	function check_form(){
		var obj = document.getElementById('site'),
				msg = $('.total'),
				query_domain = url2domain(obj.value);

		if(query_domain){
			if(isDomain(query_domain)){
				$('#query_form').attr('action','/'+query_domain+'/');
				return true;
			}else{
				alert('域名格式错误！');
				obj.focus();
			}
		}else{
			msg.html('<font color="red">请输入要查询的域名！</font>');
			obj.focus();
		}
		return false;
	}
	
	var obj = document.getElementById('site'),
			query_domain = url2domain(obj.value);

	if(query_domain){
		if(isDomain(query_domain)){
			addAllSites(query_domain);
		}
	}
