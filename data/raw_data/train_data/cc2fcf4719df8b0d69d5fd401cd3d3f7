<script type="text/javascript">	var quequeKey = [''];
	$(function(){
		var domain = url2domain($('.search_domain b').html());
		// 获取查询网址收录情况
		$.ajax({
			url:"http://www.aizhan.com/ajaxAction/shoulu4.php",
			data:{'datatype':'jsonp','domain':domain,'cc':'e326699a1b2fffbc27b0edb092102df5','rn':1491093504},
			dataType:"jsonp",
			success:function(json){
				$('#baidu_shoulu').html(json['baidu_all']);
				$('#baidu_1day').html(json['baidu_1days']);
				$('#baidu_weizhi').html(json['baidu_position']);
			},
			error:function(){
				$('#baidu_shoulu').html('--');
				$('#baidu_1day').html('--');
				$('#baidu_weizhi').html('--');
			}
		});
		// 获取查询网址pr
		$.ajax({
			url:"http://www.aizhan.com/ajaxAction/pr.php",
			data:{'datatype':'jsonp','domain':domain,'cc':'e326699a1b2fffbc27b0edb092102df5','rn':1491093504,'linknum':0},
			dataType:"jsonp",
			success:function(json){
				if(Object.prototype.toString.call(json) === '[object Object]' && json['error']===undefined){
					$('#getpr').html("<img src='http://static.aizhan.com/images/pr/pr"+json['pr']+".gif' align='absmiddle' />&nbsp;&nbsp;PR输出值："+json['out']);
				}else{
					$('#getpr').html('-');
				}
			},
			error:function(){
				$('#getpr').html('-');
			}
		});

		// 请求队列参数获取
		var pr = $('#pr').attr('checked'),
				baiduinfo = $('#checkbaiduinfo').attr('checked'),
				backlink = $('#checkbacklink').attr('checked'),
				spider = $('#spider').val(),
				link = $('.link a'),
				link_list = [];

		for (var i=0; i < link.length; i++) {
			link_list.push(link.eq(i).attr('href'));
		};
		// 请求队列
		window.onload = function(){
			queque(0,domain,link_list,pr,baiduinfo,backlink,spider);
		}

		function queque(start,domain,link_list,pr,baiduinfo,backlink,spider){
			if(backlink){
				getbacklink(start,domain,link_list,spider);
			}
			if(pr){
				getpr(start,link_list);
			}
			if(baiduinfo){
				getbaiduinfo1(start,link_list);
				getbaiduinfo2(start,link_list);
			}
		}
	});
	// 请求PR
	function getpr(id,link_list,re) {
		if(!re){
			var url = link_list[id];
			if(id>=link_list.length){
				return false;
			}
		}else{
			var url = link_list;
			$('#pr'+(id+1)).html('<img src="http://static.aizhan.com/images/loading.gif" />');
		}
		var rfunc = 'getpr('+id+',\''+url+'\',1);',
				rhtml = '<a href="javascript:void(0);" onclick="'+rfunc+'" id="gpr'+id+'">重查</a>',
				elem = $('#pr'+(id+1));

		$.ajax({
			url:"http://www.aizhan.com/ajaxAction/pr.php",
			data:{'domain':url2domain(url),'cc':quequeKey[id],'rn':1491093504},
			dataType:"jsonp",
			timeout:5000,
			cache:false,
			error:function(){
				if(!re){
					getpr((id+1),link_list);
				}
				$('#pr'+id).html(rhtml);
			},
			success:function(json){
				if(!re){
					getpr((id+1),link_list);
				}
				if(Object.prototype.toString.call(json) !== '[object Object]' || json['error']!==undefined){
					elem.html(rhtml);
					return;
				}
				elem.html(json['pr'] < 0?0:json['pr']);
			}
		});
	}
	// 请求百度总收录/位置
	function getbaiduinfo1(id,link_list,re) {
		if(!re){
			var url = link_list[id];
			if(id>=link_list.length){
				return false;
			}
		}else{
			var url = link_list;
			$('#sl'+(id+1)).html('<img src="http://static.aizhan.com/images/loading.gif" />');
		}

		var rfunc = 'getbaiduinfo1('+id+',\''+url+'\',1);',
				rhtml = '<a href="javascript:void(0);" onclick="'+rfunc+'" id="sl'+id+'">重查</a>',
				elem = $('#sl'+(id+1));

		$.ajax({
			url:"http://www.aizhan.com/ajaxAction/shoulu4.php",
			data:{'domain':url2domain(url),'cc':quequeKey[id],'rn':1491093504},
			dataType:"jsonp",
			timeout:5000,
			cache:false,
			error:function(){
				if(!re){
					getbaiduinfo1((id+1),link_list);
				}
				$('#sl'+id).html(rhtml);
			},
			success:function(json){
				if(!re){
					getbaiduinfo1((id+1),link_list);
				}
				if(Object.prototype.toString.call(json) !== '[object Object]' || json['error']!==undefined){
					elem.html(rhtml);
					return;
				}
				if(json['sum_record']<=20){
					elem.html('<a href="http://www.baidu.com/s?wd=site:'+url2domain(url)+'" target="_blank"><font color="red">'+json['baidu_all']+'</font>/'+json['baidu_position']+'</a>');
				}else{
					elem.html('<a href="http://www.baidu.com/s?wd=site:'+url2domain(url)+'" target="_blank">'+json['baidu_all']+'/'+json['baidu_position']+'</a>');
				}
			}
		});
	}
	// 请求百度权重/流量
	function getbaiduinfo2(id,link_list,re) {
		if(!re){
			var url = link_list[id];
			if(id>=link_list.length){
				return false;
			}
		}else{
			var url = link_list;
			$('#gpr'+(id+1)).html('<img src="http://static.aizhan.com/images/loading.gif" />');
		}

		var rfunc = 'getbaiduinfo2('+id+',\''+url+'\',1);',
				rhtml = '<a href="javascript:void(0);" onclick="'+rfunc+'" id="link'+id+'">重查</a>',
				elem = $('#gpr'+(id+1));

