function showDiv(size,obj){
	if(size>32){
		$(obj).parents(".guan-wrap").siblings().show();
	}
}
function hideDiv(size,obj){
	if(size>32){
		$(obj).parents(".guan-wrap").siblings().hide();
	}
}


