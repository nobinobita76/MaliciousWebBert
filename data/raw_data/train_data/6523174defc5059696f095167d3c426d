var proid = 100523674134;
var cook_contact = getCookie(proid+"_product_detail_contact");//console.log('cook_contact='+cook_contact);
function getCookie(name)
{
var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");
if(arr=document.cookie.match(reg))
return unescape(arr[2]);
else
return null;
}
if(cook_contact)
{
/*$("#contact_info").show();
$("#contact_img").hide();*/
}
//发送联系方式至手机
$("#pd_sent_click").click(function(){
$(".pd_popup").fadeIn(200);
getPhone();
});
//关闭联系方式至手机
$(".pd_popup_nav_close").click(function(){
$(".pd_popup").fadeOut(200);
});
});
/**
* ajax获取到手机号码
* 创建者：hdd
* 创建 时间：2013.1.28
*/
function getPhone() {
var t = new Date().getTime();
var html = '';
$.ajax({
type: 'post',
url : '/yellow/product_medium_page/get_phone.php',
data: {'time' : t},
dataType: 'json',
success: function(data) {
if (data['errno'] > 0) {
html += '<input type="text" class="pd_sent_tel" id="send_contact_mobile" name="send_contact_mobile" value="请输入您的手机号码" onblur="javascript:sendContactMobile_1();" onclick="javascript:sendContactMobile_2();"/>';
} else {
html += '<input type="text" class="pd_sent_tel" id="send_contact_mobile" name="send_contact_mobile" value="'+data['msg']+'"/>';
}
html += '<input type="button" class="pd_sent_btn" value="发送" onclick="submitSend();"/>';
$('#pd_sent_input_id').html(html);
}
});
}
