<script type="text/javascript">
//失去焦点
function sendContactMobile_1() {
var sendContactMobile = $("#send_contact_mobile").val();
if(sendContactMobile != '' && sendContactMobile != "请输入您的手机号码"){
} else {
$("#send_contact_mobile").val("请输入您的手机号码");
}
}
//获取到焦点
function sendContactMobile_2() {
var sendContactMobile = $("#send_contact_mobile").val();
if(sendContactMobile != '' && sendContactMobile != "请输入您的手机号码"){
} else {
$("#send_contact_mobile").val("");
}
}
/**
* ajax发送短信
* 创建者：hdd
* 创建时间：2013.1.28
*/
function submitSend() {
var time = new Date().getTime();
var send_contact_mobile = $('#send_contact_mobile').val();
var send_content = $('#send_content').html();
//验证手机号码
var reg = /^(13[0-9]|15[0-9]|18[0-9])\d{8}$/;
if(reg.test(send_contact_mobile) == false) {
alert("请正确填写手机号码!");
return false;
} else {
$.ajax({
type: 'post',
url : '/yellow/product_medium_page/phone_msg.php',
data: {'send_contact_mobile' : send_contact_mobile, 'send_content' : send_content, 't' : time},
dataType: 'json',
success: function(data) {
if (data['errno'] > 0) {
alert(data['msg']);
return false;
} else {
$('.pd_popup').hide();
alert(data['msg']);
return false;
}
}
});
}
}
</script>
