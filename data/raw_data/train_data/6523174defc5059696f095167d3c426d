<script type="text/javascript">
$(function() {
//需要滚动的内容
$li1 = $(".rp_slide_tab_pic li");
//滚动容器
$window1 = $(".rp_slide_box");
//左滚动对象
$left1 = $(".rp_slide_tab .rp_slide_all_l");
//右滚动对象
$right1 = $(".rp_slide_tab .rp_slide_all_r");
//容器宽
$window1.css("width", $li1.length*90);
var c = 1;
//总滚动次数
var len = Math.ceil($li1.length/7);
//缩略图左滚动
$left1.click(function() {
if(c <= 1)
{
return false;
}
$window1.animate({left:'+=540px'}, 1000);
c--;
});
//缩略图右滚动
$right1.click(function() {
if(c >= len){
return false;
}
$window1.animate({left:'-=540px'}, 1000);
c++;
});
var obj = $(".rp_slide_tab_pic li");
//返回目前选中li的索引值
function get_index(){
var $i = 0;
var index_=0;
obj.each(function(){
if($(this).find(".rp_slide_tab_bor").is(":visible")){
index_ = $i;
};
$i++;
});
return index_;
};
//清除所有选中样式
function ClearStyle(){
obj.each(function(){ $(this).find(".rp_slide_tab_bor").hide();});
}
//默认选中
var proid="100523674134_1";
$("#"+proid).show();
var page = Math.ceil((get_index() + 1)/6);
var right_px = 540*(page-1);
$window1.animate({left:'-='+right_px+'px'}, 10);
c = c+(page-1);
})
</script>


