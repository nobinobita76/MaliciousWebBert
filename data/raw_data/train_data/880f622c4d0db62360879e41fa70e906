  <script>
    $('.btn-login').click(function(){
var path = "/e/member/doaction.php";
$("#sign_form").attr("action",path).attr("target","_self").attr("method","post").submit();
});

$('.show-reg').click(function(){
 $('.umail').css("display","block");
$('.captcha').css("display","none");
$('.captcha-input').css("display","none");
});
$('.btn-back').click(function(){
 $('.umail').css("display","none");
$('.captcha').css("display","block");
$('.captcha-input').css("display","block");
});
$('.btn-reg').click(function(){
$("#enews").val("register");
$("#repassword").val($(".user-passwd").val());
var path = "/e/member/doaction.php";
$("#sign_form").attr("action",path).attr("target","_self").attr("method","post").submit();
});
</script> 
  <script>
  var mcid= 10;
    $(document).ready(function () {
           var html = window.location.pathname.split('/');
            var urlid = html[html.length-1].split('-');
           if(urlid[1] == mcid){
               $('#menu > li').removeClass();
               $('#menu_btn_'+mcid).addClass('active');
           }else{
               $('#home').addClass('active');
           }
    });
</script>  
