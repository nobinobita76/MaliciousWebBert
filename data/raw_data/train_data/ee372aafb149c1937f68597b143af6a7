<script>
var cpro_pwid = 980;
cpro_resize();
function cpro_resize(){
	var cw = document.documentElement.clientWidth;
	if(cw==0){
		cw = document.body.clientWidth;
	}
	if( cw >= (cpro_pwid+250) ){
		if(document.getElementById("googlefloat1")){document.getElementById("googlefloat1").style.display = "block";}
		if(document.getElementById("googlefloat2")){document.getElementById("googlefloat2").style.display = "block";}
	}else{
		if(document.getElementById("googlefloat1")){document.getElementById("googlefloat1").style.display = "none";}
		if(document.getElementById("googlefloat2")){document.getElementById("googlefloat2").style.display = "none";}
	}
}
window.onresize=function(){
	cpro_resize();
}
</script>
