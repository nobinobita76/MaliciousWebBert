        <h3 class="btn-group-cell"><a href="javascript:" class="btn-blank active" data-rel="catelogX">目录</a></h3>
    </nav>
</div>
<div class="module module-merge">
    <div id="catelogX" class="chapter-tab-x active">
        <div class="module-header">
            <div class="module-header-l">
                <h4 class="chapter-sub-title">共<output><%= data.vs.reduce(function (sum, d) { return sum + (d.cs ? d.cs.length : 0); }, 0) %></output>章</h4>
            </div>
            <div class="module-header-r">
                <a id="reverse" href="javascript:" class="module-header-btn dark">倒序</a>
            </div>
        </div>
        <ol id="volumes" class="chapter-ol chapter-ol-catalog">
<%
    var CATALOG_HEADER = data._headerHeight || 44; // 目录标题高度
    var VOLUME_TITLE_HEIGHT = data._volumeHeight || 36; // 卷标题高度
    var CHAPTER_TITLE_HEIGHT = data._headerHeight || 44; // 章节标题高度
    var LAST_CHAPTER_TITLE_HEIGHT = data._headerHeight || 64; // 最后章节高度
    var from = data._form || 0; // 开始内容位置，直出时为0
    var to = data._to || (CHAPTER_TITLE_HEIGHT * 100); // 结束内容位置，直出时为100章的高度
    var curStart = 0; // 当前元素开始位置
    var curEnd = 0; // 当前元素结束位置
    var topHeight = 0; // 顶部占位元素高度
    var bottomHeight = 0; // 底部占位元素高度
%>
<% for (var i = 0; i < data.vs.length; i++) { var volume = data.vs[i]; %>
    <%# 第一次初始化的时候标记最后卷，用于添加更新时间 %>
    <% if (i === data.vs.length - 1) { volume.isLast = true; } %>
    <%# 记录当前元素(卷标题)开始位置和结束位置 %>
    <% curStart = curEnd; curEnd += VOLUME_TITLE_HEIGHT; %>
    <%# 开始内容位置正好处于当前元素(卷标题)中时，添加顶部占位元素 %>
    <% if (curStart < from && curEnd >= from) { %>
    <div class="placeholder" style="height: <%= topHeight %>px;"></div>
    <% } %>
    <%# 当前元素(卷标题)的结束位置在开始内容位置和结束内容位置之间时，添加该元素 %>
    <% if (curEnd >= from && curEnd <= to) { %>
    <li class="chapter-bar"><%= volume.vN || '正文卷' %></li>
    <%# 当前元素(卷标题)的结束位置小于开始内容位置时，累计顶部占位元素高度 %>
    <%# 当前元素(卷标题)的结束位置大于结束内容位置时，累计底部占位元素高度 %>
    <% } else if (curEnd < from) { topHeight += VOLUME_TITLE_HEIGHT; } else if (curEnd > to) { bottomHeight += VOLUME_TITLE_HEIGHT; } %>
    <% for (var j = 0, n = (volume.cs ? volume.cs.length : 0); j < n; j++) { var chapter = volume.cs[j]; %>
        <%# 第一次初始化的时候标记最后章节，用于添加更新时间 %>
        <% if (j === volume.cs.length - 1) { chapter.isLast = true; } %>
        <%# 记录当前元素(章标题)开始位置和结束位置 %>
        <% curStart = curEnd; curEnd += CHAPTER_TITLE_HEIGHT; chapter._y = curStart - CHAPTER_TITLE_HEIGHT * 3 + CATALOG_HEADER; %>
        <%# 开始内容位置正好处于当前元素(章标题)中时，添加顶部占位元素 %>
        <% if (curStart < from && curEnd >= from) { %>
        <div class="placeholder" style="height: <%= topHeight %>px;"></div>
        <% } %>
        <%# 当前元素(章标题)的结束位置在开始内容位置和结束内容位置之间时，添加该元素 %>
        <% if (curEnd >= from && curEnd <= to) { %>
        <li class="chapter-li jsChapter">
            <a href="/book/<%= data.bookId %>/<%= chapter.id %>" class="chapter-li-a <%= chapter.fS && !chapter.sS ? ' unpay' : '' %>" data-chapter-id="<%= chapter.id %>">
                <span class="chapter-index <%= chapter.id === data._curId ? 'red' : '' %>"><%= chapter.cN %></span><!-- <span class="chapter-title"><%= chapter.cN %></span> -->
                <svg class="icon icon-lock"><use xlink:href="#icon-lock"></use></svg>
                <% if (volume.isLast && chapter.isLast) { %><%# 最后一个章节，需要添加更新时间 %>
                <p class="chapter-time"><time><%= chapter.uT %></time>更新</p>
                <% } %>
            </a>
        </li>
        <%# 当前元素(章标题)的结束位置小于开始内容位置时，累计顶部占位元素高度 %>
        <%# 当前元素(章标题)的结束位置大于结束内容位置时，累计底部占位元素高度(最后一个章节有更新时间) %>
        <% } else if (curEnd < from) { topHeight += CHAPTER_TITLE_HEIGHT; } else if (curEnd > to) { bottomHeight += volume.isLast && chapter.isLast ? LAST_CHAPTER_TITLE_HEIGHT : CHAPTER_TITLE_HEIGHT; } %>
    <% } %>
<% } %>
<%# 添加底部占位元素 %>
<div class="placeholder" style="height: <%= bottomHeight %>px;"></div>
        </ol>
    </div>
    <div id="bookmarkX" class="chapter-tab-x"></div>
</div>
</script>
