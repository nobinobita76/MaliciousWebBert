<script type="text/javascript">
  // tab内容长度字符限制内显示应届生租房专区
GJ.use('jquery', function(){
  var tabtxt = $("ul.seltab li");
  var rentUrl = $("#rentUrl");
  var bluecollar = $("#bluecollar");
  var jobfair_tab_id = $("#jobfair_tab_id");
  if( (tabtxt.length==1 && rentUrl.length>0) ){
    $("#rentUrl").show();
  }else if( (tabtxt.length>1 && rentUrl.length>0) && tabtxt.find("a:first").html().length<20 ){
    $("#rentUrl").show();
  }
  afirst = tabtxt.find("a:first").html().length;

  if (afirst<=17 && rentUrl.css("display") == 'block') {
        bluecollar.show();
  } else if (afirst<=23 && (rentUrl.css("display") == 'none' || rentUrl.css("display")==undefined)){
        bluecollar.show();
  }
  if( jobfair_tab_id.length > 0 && rentUrl.length == 0){
        if (afirst<=17 && bluecollar.css("display") == 'block') {
               jobfair_tab_id.show();
        } else if (afirst<=23 && (bluecollar.css("display") == 'none' || bluecollar.css("display")== undefined)){
            jobfair_tab_id.show();
        }
  }
})
</script>
<script type="text/javascript">
GJ.use('jquery', function(){
    // title 截取
    GJ.each($("[data-title]"), function(item) {
        var titleVal;
        if (!GJ.isUndefined(item) && GJ.isObject(item)) {
            titleVal = $(item).attr('data-title');
            if (!GJ.isUndefined(titleVal) && GJ.isString(titleVal)) {
                if (titleVal.length > 22) {
                    $(item).html(titleVal.substr(0, 22) + '...');
                }
            }
        }
    });
});
</script>
