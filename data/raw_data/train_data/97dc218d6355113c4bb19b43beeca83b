  <script type="text/javascript">
  $(document).ready(function(){
  //支持
    $(".list-ques-agree-btn").click(function(){
      var url = "/ajax/vote";
      var id = $(this).attr("data-opt");
      var obj = $(this);
      $.ajax({
        url: url,
        dataType:"json",
        type:"POST",
        data:{id:id, opt:1}, //opt表示操作类型 1为问题 2为答案
        success: function(data){
          layer.closeAll('loading');
          if(data!=''){
            //alert(data);
            if(data.err==0){
              //obj.html('支持('+data.num+')');
              obj.children(".zanshu").html(data.num);
            }
            layer.msg(data.msg, {
                icon: 1,
                time: 2000 //2秒关闭（如果不配置，默认是3秒）
            }, function(){
                //do something
            });
          }
        }
      });
    });
  });
  </script>
