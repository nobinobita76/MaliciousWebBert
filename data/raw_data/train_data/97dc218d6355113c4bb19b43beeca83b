        <script type="text/javascript">
            $(".but_page").click(function(){
              var page = $("#input_page_redirect-index-list").val();
               var link = "/index/list/limit";//window.location.href;
                var url = "";
               if (link.indexOf("page=")>0) {
                    url = link.replace(/page=\d+/, "page="+page);
                } else {
                    url = link+(link.indexOf("?")>0? "&page="+page : "/page/"+page);
                }
               window.location = url;
            });
        </script>
