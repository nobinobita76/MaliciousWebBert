var getCityBlock = function(elid) {
    var elpos = ElementPosition(cheshi.$(elid));
    var html = cheshi.$("city_block_2").innerHTML;

    if(html == "") {
        cheshi.getJSON("http://product.cheshi.com/ajax/index_201203.php?act=city_block_news", function(data) {
            cheshi.$("city_block_2").innerHTML = data;
            cheshi.show(cheshi.$("city_block_2"));
            InitCityBlockTab();
        });
    } else {
        cheshi.show(cheshi.$("city_block_2"));
    }
}
var InitCityBlockTab = function() {
    if(cheshi.$("city_block_2").innerHTML != "") {
        var sd = new SlideDoor("tab_city_block", "li");
        sd.ClickSlide(sd, "", "action");
    }
}
//滑动门类
var SlideDoor = function(container, itemTag){
    this._container = cheshi.$(container);  //容器对象
    this.Items = new Array();
    var els = this._container.getElementsByTagName(itemTag);
    
    if(els.length <= 0) {
        return false;
    }
    for(var i = 0; i < els.length; i++){
        if(els[i].getAttribute("ref") == undefined || els[i].getAttribute("ref") == null) continue;
        this.Items.push(els[i]);
    }
    this.ClearStyle = function(classname) {
        classname == undefined && (classname = "");
        for(var i = 0; i < this.Items.length; i++){ this.Items[i].className = classname; }
    }
    this.HideAll = function() {
        for(var i = 0; i < this.Items.length; i++) {
            cheshi.hide(cheshi.$(this.Items[i].getAttribute("ref")));
        }
    }
    this.NormalSlide = function(sdObj, normalStyle, focusStyle) {
        for(var i = 0; i < sdObj.Items.length; i++) {
            sdObj.Items[i].onmouseover = function() {
                sdObj.ClearStyle(normalStyle);
                this.className = focusStyle;
                var ref = this.getAttribute("ref");
                var textarea = cheshi.$(ref).getElementsByTagName("textarea");
                sdObj.HideAll();
                if(textarea[0] == undefined) {
                    cheshi.show(cheshi.$(ref));
                }else{
                    cheshi.$(ref).innerHTML = textarea[0].value;
                    cheshi.show(cheshi.$(ref));
                }
            }
        }
    }
    this.ClickSlide = function(sdObj, normalStyle, focusStyle) {
        for(var i = 0; i < sdObj.Items.length; i++) {
            sdObj.Items[i].onclick = function() {
                sdObj.ClearStyle(normalStyle);
                this.className = focusStyle;
                var ref = this.getAttribute("ref");
                var textarea = cheshi.$(ref).getElementsByTagName("textarea");
                sdObj.HideAll();
                if(textarea[0] == undefined) {
                    cheshi.show(cheshi.$(ref));
                }else{
                    cheshi.$(ref).innerHTML = textarea[0].value;
                    cheshi.show(cheshi.$(ref));
                }
            }
        }
    }
    this.NoRefSlide = function (sdObj, cvObjs, normalStyle, focusStyle) {

        for(var i = 0; i < sdObj.Items.length; i++) {
        
            sdObj.Items[i].onmouseover = function() {
                //alert(sdObj.Items.length);
                sdObj.ClearStyle(normalStyle);
                this.className = focusStyle;
                var ref = this.getAttribute("ref");
                var textarea = cvObjs[ref].getElementsByTagName("textarea");
                for (var j = 0; j < cvObjs.length; j++) {
                    cheshi.hide(cvObjs[j]);
                };
                if(textarea[0] == undefined) {
                    cheshi.show(cvObjs[ref]);
                }else{
                    cvObjs[ref].innerHTML = textarea[0].value;
                    cheshi.show(cvObjs[ref]);
                }
            }
        }
    }
}
