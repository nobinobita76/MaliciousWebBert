//选择城市
var setCity = function(pid, cid) {
    cheshi.common.setCity(pid, cid);
    cheshi.getJSON("http://seller.cheshi.com/ajax/seller_data_2014.php?act=news_product&pid="+pid+"&cid="+cid+"&seriesid="+seriesid+"&productid="+prdid,
    function(data) {
        cheshi.$("priceinfo").innerHTML = data;
        closeCityBlock()
    });
    if (pid > 0) {
        var b = "http://service.cheshi.com/services/ajax_index_login_201102.php?act=loc";
        b+="&pid="+pid+"&cid="+cid;
        cheshi.getJSON(b,
        function(d) {;
        })
    }
}

if(seriesid >0){
    setCity(0,0);
    
}
//关闭城市选择框
var closeCityBlock = function() {
    cheshi.common.hidenCityPanel();
    cheshi.hide(cheshi.$("city_block_2"));
}
// 图片报名
var picform = function(){
    var fdiv = cheshi.$("article"); 
    var pList = fdiv.getElementsByTagName("p");
    for(var i=0;i<pList.length;i++){
        var storyformData = cheshi.$("storyform_data").value;
        if(pList[i].getAttribute('class') == "pic_ption pic_600_1403"){
            var storyformData = storyformData.replace('story_form.php','story_form_2014.php');
        }
        if(pList[i].getAttribute('class')=="pic_ption" || pList[i].getAttribute('class')=="pic_ption pic_600_1403" ||  pList[i].getAttribute('class')=="pic_ption frame_2014"){
            var mySpan = document.createElement("span");
            mySpan.style.display="none";
            mySpan.innerHTML = storyformData;
            pList[i].appendChild(mySpan);
            var aList = pList[i].getElementsByTagName("a");
            //alert(aList[1].innerHTML);
            pList[i].getElementsByTagName("a")[1].onmouseover = function(){
                this.parentNode.getElementsByTagName("span")[0].style.display="";
                                this.parentNode.getElementsByTagName("em")[0].style.display="";
            }
                        pList[i].getElementsByTagName("a")[1].onclick = function(){
                       this.parentNode.getElementsByTagName("span")[0].style.display="none";
                       this.parentNode.getElementsByTagName("em")[0].style.display="none";
                        }
                        pList[i].getElementsByTagName("em")[0].onclick = function(){
                                 this.parentNode.getElementsByTagName("span")[0].style.display="none";
                                this.style.display="none";
                         } 
                      // pList[i].getElementsByTagName("span")[0].onmouseout = function(){
               //    this.style.display="none";
              // }
        }
    }
}

picform();
function sellerChange(obj,id) {
 var arrayli = obj.parentNode.getElementsByTagName("li"); //获取li数组
 var arrayul = document.getElementById(id).getElementsByTagName("div"); //获取ul数组
 for (i=0;i<arrayul.length;i++) {
  if (obj==arrayli[i]) {
   arrayli[i].className = "active";
   arrayul[i].style.display = "block";
  } else {
   arrayli[i].className = "";
   arrayul[i].style.display = "none";
  }
 }
}
