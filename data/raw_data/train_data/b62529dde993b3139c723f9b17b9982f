<script type="text/javascript">
//车市宝推广资源新闻最终页模块
if(seriesid != 0){
    var spreadUrl = 'http://seller.cheshi.com/newseller/appdir/spread_ajax.php?act=getSpreadResource&positionid=4&catesubclass=' + seriesid + '&cheshicallback=setSpreadResource';
    var c,_=Function;
    cheshi.getJSON(spreadUrl, function(data){
        if(data == '-1'){
            return false;
        }else{
    document.getElementById('sellerSpreadContainer').innerHTML = data;
    with(o=document.getElementById("sellerSpreadLiContainer")){if(o.getElementsByTagName('li').length <= 8){_("c=1");}else{innerHTML+=innerHTML;}onmouseover=_("c=1"); onmouseout=_("c=0");}(F=_("if(#%18||!c)#++,#%=o.scrollHeight>>1;setTimeout(F,#%176?10:3000);".replace(/#/g,"o.scrollTop")))();
        document.getElementById("sellerSpreadContainer").style.display = 'block';
        }
    });
}
function getScrollTop(){
    var scrollTop=0;
    if(document.documentElement&&document.documentElement.scrollTop){
        scrollTop=document.documentElement.scrollTop;
    }
    else if(document.body){
        scrollTop=document.body.scrollTop;
    }
    return scrollTop;
}

function openWX(url,obj) {
    var isIE6 = !-[1,] && !window.XMLHttpRequest;
    var pos=ElementPosition(obj);
    var scroll = getScrollTop();
    var wxDiv = document.getElementById('wxDiv');
    if(wxDiv){
        if(isIE6){
            wxDiv.style.position = 'absolute';
            wxDiv.style.left = (pos[0]-20)+'px';
            wxDiv.style.top= (pos[1]+30)+'px';
        }else{
            wxDiv.style.position = 'fixed';
            wxDiv.style.left = (pos[0]-20)+'px';
            wxDiv.style.top= (pos[1]+20-scroll)+'px';
        }
        wxDiv.style.display = '';
        wxDiv.style.zIndex = '100';
        wxDiv.innerHTML = '<b>扫描关注商家</b><br/><img align="center" width="80px" height="80px" src='+url+' />';
    }
}

function closeWX(){
    var wxDiv = document.getElementById('wxDiv');
    if(wxDiv){
        wxDiv.style.display="none";
    }
}


//cms后台插入经销商联系方式
function insert_openWX(url,obj) {
    var isIE6 = !-[1,] && !window.XMLHttpRequest;
    var pos=ElementPosition(obj);
    var scroll = getScrollTop();
    var insert_wxDiv = document.getElementById('insert_wxDiv');
    if(insert_wxDiv){
        if(isIE6){
            insert_wxDiv.style.position = 'absolute';
            insert_wxDiv.style.left = (pos[0]-20)+'px';
            insert_wxDiv.style.top= (pos[1]+30)+'px';
        }else{
            insert_wxDiv.style.position = 'fixed';
            insert_wxDiv.style.left = (pos[0]-20)+'px';
            insert_wxDiv.style.top= (pos[1]+20-scroll)+'px';
        }
        insert_wxDiv.style.display = '';
        insert_wxDiv.style.zIndex = '100';
        insert_wxDiv.innerHTML = '<b>扫描关注商家</b><br/><img align="center" width="80px" height="80px" src='+url+' />';
    }
}
//cms后台插入经销商联系方式
function insert_closeWX(){
    var insert_wxDiv = document.getElementById('insert_wxDiv');
    if(insert_wxDiv){
        insert_wxDiv.style.display="none";
    }
}

$(function($) {
    // 替换图片关联的车型信息
    var img_prd_ids = [];
    $('.car_tab .rows').each(function(i) {
        img_prd_ids.push($(this).attr('data'));
    });

    if (img_prd_ids.length) {
        var provinceid = '';
        var cityid = '';
        var loc = cheshi.utility.getCookie('cheshi_pro_city');
        if(loc != ""){
            var arr = cheshi.utility.base64Decode(loc).split('_');
            provinceid = arr[0];
            cityid = arr[2];
        }
        // 从solr中取车型数据
        var url = 'http://product.cheshi.com/ajax/0203/news_product.php?id=' + img_prd_ids.join() + '&provinceid='+provinceid+'&cityid='+cityid+'&jsoncallback=?';
        $.getJSON(url, function(r) {
            if (r.status) {
                img_prd_replace(r.data);
            }
        });

        function img_prd_replace(prds) {
            var prd_html = '\
                        <div class="l tab_l">\
                            <h3><a href="http://product.cheshi.com/model_{prd_id}/?kid=n0050" title="{prd_title}">{prd_title}</a></h3>\
                            <div class="chose"><a href="http://product.cheshi.com/model_{prd_id}/?kid=n0050">车型首页</a><span>|</span><a href="http://product.cheshi.com/model_{prd_id}/param.html?kid=n0050">参数配置</a><span>|</span><a href="{pic_link}?kid=n0050">图片</a><span>|</span><a href="http://product.cheshi.com/model_{prd_id}/price.html?kid=n0050">车型报价</a><span>|</span><a href="http://product.cheshi.com/model_{prd_id}/jiangjia.html?kid=n0050">降价</a></div>\
                        </div>\
                        <div class="city_y1 l mt_num">{left_days}</div>\
                        <div class="r tab_r">\
                            <a class="blcok_a" href="{price_link}?kid=n0050">\
                                <span><div>{price_title}</div></span><strong>{price_off}{price_content}</strong>\
                            </a>\
                        </div>';
            for (var i = 0; i < prds.length; i++) {
                html = prd_html;
                html = html.replace(/{prd_id}/g, prds[i].pid);
                html = html.replace(/{prd_title}/g, prds[i].prd_title);
                if (prds[i].left_days && prds[i].left_days > 0) {
                    html = html.replace('{left_days}', '<span>剩余<i class="cred">'+prds[i].left_days+'</i>天</span>');
                } else {
                    html = html.replace('{left_days}', '');
                }
                html = html.replace('{price_title}', prds[i].price_title);
                html = html.replace('{price_content}', prds[i].price_content);
                if (prds[i].price_off) {
                    html = html.replace('{price_off}', '<em class="down_ps">降'+prds[i].price_off+'</em>');
                } else {
                    html = html.replace('{price_off}', '');
                }
                html = html.replace('{price_link}', prds[i].price_link);
                html = html.replace('{pic_link}', prds[i].pic_link);

                $('.car_tab .rows').each(function(j) {
                    if ($(this).attr('data') == prds[i].pid) {
                        $(this).html(html);
                    }
                });
            }
        }
    }

    // 团购报名
    $('#baoming_btn, #baoming_close_btn').click(function() {
        $('#baoming_container1').toggle();
        return false;
    });

    // 限制团购报名按钮的位置 
    if ($('#baoming_btn').length > 0) {
        setInterval(function() {
            var a_top = $('#article_new').offset().top - $(document).scrollTop();
            var b_top = $('#baoming_btn').offset().top;
            if (b_top <= a_top || a_top > ($(window).height()/2)) {
                $('#baoming_btn').css('top', (a_top + 27) + 'px');
                $('#baoming_container1').css('top', (a_top + 27) + 'px');
            } else {
                $('#baoming_btn').css('top', '50%');
                $('#baoming_container1').css('top', '50%');
            }
        }, 10);
    }
});
</script> 
