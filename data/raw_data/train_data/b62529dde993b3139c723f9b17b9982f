                	cheshi.getJSON('http://my.cheshi.com/user/ajax/ajaxCollect.php?act=select&id='+id+'&type='+type,function(data){
						if(data > 0){
							cheshi.$('favid_collect').value = data;
							cheshi.show(cheshi.$('yes_collect'));
							cheshi.show(cheshi.$('yes_collect_1'));
							cheshi.hide(cheshi.$('no_collect'));
							cheshi.hide(cheshi.$('no_collect_1'));
						}else{
    						cheshi.show(cheshi.$('no_collect'));
    						cheshi.show(cheshi.$('no_collect_1'));
							cheshi.hide(cheshi.$('yes_collect'));	
							cheshi.hide(cheshi.$('yes_collect_1'));	
    					}
						return false;
    				}); 
					
