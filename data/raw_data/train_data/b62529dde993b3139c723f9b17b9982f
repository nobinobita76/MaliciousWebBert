//?c=news/View&id=1866528&page=2
    var event = eventTag||window.event; 
    var keynum = event.charCode||event.keyCode;
    if(keynum==39){
        if(1!=1 && 1 != 0 ){
          location.href='http://news.cheshi.com/20160622/1866528_2.shtml';
        }
    }else if(keynum==37){
        if(1==2){
          location.href='http://news.cheshi.com/20160622/1866528.shtml';
        }else
        if(1!=1){
          location.href='http://news.cheshi.com/20160622/1866528_0.shtml';
        }
    }
}
