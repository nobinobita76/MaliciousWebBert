<script>
function tabChange(obj,show_id){
    var ul = document.getElementById("tabcontent").getElementsByTagName("ul");
    var tab_ul = document.getElementById("fin_tab").getElementsByTagName("li");
    for(var i = 0; i < ul.length; i++){
        ul[i].style.display = "none";
    }
    for(var i = 0; i < ul.length; i++){
        tab_ul[i].className = "";
    }
    obj.className = "fin_tab_on";
    document.getElementById(show_id).style.display = "block";
}
</script>

