$(function(){
    $("a[@rel=chain]").hover(
	    function(e){
		    var pos=$(this).position();
			$("#chain_menu").css({"left":pos.left+6+"px","top":pos.top+14+"px"}).slideDown(100);
		},
		function(e){
		    $("#chain_menu").hide();
		}
	);
	$("#chain_menu").hover(
	    function(e){
		    $(this).show();
		},
		function(e){
		    $(this).hide();
		}
	);
});
</script> 
