function Moveduilian3() {
var x = 0;                                                 //浮动广告层固定于浏览器的x方向位置
var y = 200;                                                //浮动广告层固定于浏览器的y方向位置
var diff = (document.documentElement.scrollTop + y - document.all.duilian3.style.posTop)*.40;
var y = document.documentElement.scrollTop + y - diff;
eval('document.all.duilian3.style.posTop = y');
eval('document.all.duilian3.style.posRight = x');         //移动广告层
setTimeout("Moveduilian3();", 80);                   //设置20毫秒后再调用函数MoveLayer()
}
function winload() 
{ 
	document.all.duilian3.style.top=85; 
	document.all.duilian3.style.right=0; 
	var nm=readCookie("service");
	if(nm!="")
	{
		setduilian3(nm);
	}
} 
function setduilian3(nm)
{
	var whichEle;
	whichEle = document.getElementById("table01");
	var djEle;
	djEle=document.getElementById("dj");
	var djimg;
	djimg=document.getElementById("imgs");	
	
	if(nm=="off")	
	{
		whichEle.style.display="block";
		  djEle.title="[点击隐藏]";
		  djimg.src="qq/kefu1.gif";
	}
	if(nm=="on")	
	{
		 whichEle.style.display="none";	
		  djEle.title="[点击显示]";
		  djimg.src="qq/kefu1.gif";
	}
}
function changelayer()
{
		var whichEle;
		whichEle = document.getElementById("table01");
		var djEle;
		djEle=document.getElementById("dj");
		var djimg;
		djimg=document.getElementById("imgs");
		
		if(whichEle.style.display=="block")
		{
		  whichEle.style.display="none";	
		  djEle.title="[点击显示]";
		  djimg.src="qq/kefu1.gif";
		  SetCookie("on")  
		 
		  
		}
		else
		if(whichEle.style.display=="none"){
		  whichEle.style.display="block";
		  djEle.title="[点击隐藏]";
		  djimg.src="qq/kefu1.gif";
		  SetCookie("off")	  
		}
}

function  SetCookie(value)
//设定Cookie值
{	
	var Days = 30; //此 cookie 将被保存 30 天
    var exp  = new Date();    //new Date("December 31, 9998");
    exp.setTime(exp.getTime() + Days*24*60*60*1000);
	document.cookie  =  "service"  +  "="  +  escape  (value)  + ";  expires="+  exp.toGMTString() +";  domain=.www.hangoo.com" ;
	
}
function readCookie(name)
{ 　　 
	var cookieValue = "";
	var search = name + "=";
	if(document.cookie.length > 0)
	{
		offset = document.cookie.indexOf(search);
		if (offset != -1)
		{ 　　 
			offset += search.length;
			end = document.cookie.indexOf(";", offset); 　　 
			if (end == -1) end = document.cookie.length; 　　 
			cookieValue = unescape(document.cookie.substring(offset, end)) 　　 
		} 　　 
	} 　　 
	return cookieValue; 　　
} 

