    <script>
        //init css
        (function () {

            function getStyle(oElm, strCssRule){
                var strValue = "";
                if(document.defaultView && document.defaultView.getComputedStyle){
                    strValue = document.defaultView.getComputedStyle(oElm, "").getPropertyValue(strCssRule);
                }
                else if(oElm.currentStyle){
                    strCssRule = strCssRule.replace(/-(w)/g, function (strMatch, p1){
                        return p1.toUpperCase();
                    });
                    strValue = oElm.currentStyle[strCssRule];
                }
                return strValue;
            }

            function loadCss(file) {
                var head = document.getElementsByTagName('head').item(0);
                var css = document.createElement('link');
                css.href = "http://static.mikecrm.com/form/"+file + ".css?v=201606292";
                css.rel = 'stylesheet';
                css.type = 'text/css';
                head.appendChild(css);
            }
            function createElement(string) {
                var elem = document.createElement("div");
                elem.innerHTML = string;
                return elem.childNodes[0];
            }

            var UA = window.navigator.userAgent,
                detectStyle = '<style type="text/css">@media (max-width: 640px) {#MobileDetect {color: rgb(12, 34, 56)}}</style>',
                detectDiv = '<div id="MobileDetect"></div>',
                tag = false,
                style = null,
                div = null;

            window.isMobile = false;
            if(preview == 'phone'){
                UA = "Mobile";
                tag = true;
                window.isMobile = true;
                loadCss("css/form.mobile");
            } else if (/Mobile|iP(hone|od|ad)|Android|BlackBerry|IEMobile|WP/.test(UA)) {
                var style = createElement(detectStyle),
                    div = createElement(detectDiv);
                document.head.appendChild(style);
                document.head.appendChild(div);
                if (getStyle(div, "color") === 'rgb(12, 34, 56)' || /WP|Opera/.test(UA)) {
                    tag = true;
                    window.isMobile = true;
                    loadCss("css/form.mobile");
                }
            }
            if (!tag) {
                loadCss("css/form");
            }
            if (style) {
                style.remove();
            }
            if (div) {
                div.remove();
            }

            (function(){
                var cp = document.getElementById('fCopyright');
                if(cp){
                    var timer = setInterval(function(){
                        if(parseInt(getStyle(cp, "height"), 10) > 0){
                            cp.removeAttribute('style');
                            clearInterval(timer);
                        }
                    }, 50);
                }
            })();
            return true;
        })();
    </script>

