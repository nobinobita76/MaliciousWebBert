    $(".message-ul").each(function () {
        var _this = this;
        var t2 = 0;
        t2 = setInterval(function () {
            var b1 = $(_this).find('li:first');
            var b2 = $(_this).find('li:last');
            $(b1).insertAfter($(b2));
        }, 3000);
    });
    $(".newyear-body .login_div .inputa, .newyear-body .login_div .inputb").mouseover(function () {
        $(this).css({ "border-color": "#ca0000" });
    }).mouseout(function () {
        $(this).css({ "border-color": "#ccc" });
