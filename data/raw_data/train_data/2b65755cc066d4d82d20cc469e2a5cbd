<script type="text/javascript">
	jQuery(document).ready(function($){
		if(true){
		$('#s1_m1').appendTo('body');
		$('#s1_m1').css({'position':'absolute','right':'0','bottom':'0'});
		$('#s1_m1').eol_scrollAd({fix:true});
		if($('#s1_m1').find('object').length ||$('#s1_m1').find('embed').length){
		window.setTimeout(function(){$('#s1_m1').find('object').width('100px').height('100px');$('#s1_m1').find('embed').width('100px').height('100px')},5000);
		$('#s1_m1').hover(function(){$('#s1_m1').find('object').width('300px').height('300px');$('#s1_m1').find('embed').width('300px').height('300px')},function(){$('#s1_m1').find('object').width('100px').height('100px');$('#s1_m1').find('embed').width('100px').height('100px')});}else{
			var oldsrc=$('#s1_m1').find('img').attr('src');
			var hoversrc=$('#s1_m1').find('img').attr('hover-img');
			window.setTimeout(function(){$('#s1_m1').find('img').attr('src',hoversrc).width('100px').height('100px');},5000);
		$('#s1_m1').hover(function(){$('#s1_m1').find('img').attr('src',oldsrc).width('300px').height('300px');},function(){$('#s1_m1').find('img').attr('src',hoversrc).width('100px').height('100px');});
		}
		$('.closeBtn').css({'display':'block','text-align':'center'});
		}
		else{$('#buoy_l').hide();$('#buoy_r').hide();$('#s1_m1').hide();$('#float_l').hide();$('#float_r').hide();}
	})

</script>
