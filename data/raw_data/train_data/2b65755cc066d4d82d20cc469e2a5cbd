    <script type="text/javascript">
		  $(".div_dfz").click(function(event){
		    $(".df_t").removeClass('hide');
		    event.stopPropagation();
		  });
		  $(".div_dfz_close").click(function(){
			    $(".df_t").addClass('hide');
			  });
		  $(".df_t a").click(function(){
			    $(".df_t").addClass('hide');
			  });
		  $(".tabs_01").click(function(){
			  var sign = $(this).index();
			   $('.tabs_01').removeClass('tag_01');
			   $('.tabs_01:eq('+$(this).index()+')').addClass('tag_01');
			    $(".contenttabs_01").hide();
			    $('.contenttabs_01:eq('+$(this).index()+')').show();
			  });
		  $('.div_dfz_one').html(provinceGkcxName+'站');
		  $('.div_dfz_one').attr('href',filename1);
		  $(".div_floot").click(function(event){
			event.stopPropagation();
		});
</script>
