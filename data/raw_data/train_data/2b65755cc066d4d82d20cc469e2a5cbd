<div class="hejiaoyuIframe  test"><iframe src="http://edu.10086.cn/cloud/pages/eduapps/agent.jsp?ran=0.9045433988794684#height=1326" id="snsIframe3" width="0" height="0" style="display: none"></iframe><iframe src="http://112.35.7.169:9010/educloud/pages/eduapps/agent.jsp?ran=0.4011861844919622#height=1326" id="snsIframe3_test" width="0" height="0" style="display: none"></iframe><iframe src="http://edu.10086.cn/educloud/study/zq/agent.jsp?ran=0.8605449190363288#1326" id="snsIframe" width="0" height="0" style="display: none"></iframe></div>
<script type="text/javascript">
var address = window.location.href;
if(address.indexOf('http://10086.gkcx.eol.cn')>-1){
	var isChrome = window.navigator.userAgent.indexOf("Chrome") !== -1 ;
	$(".hejiaoyuIframe").html('<iframe src="http://edu.10086.cn/cloud/pages/eduapps/agent.jsp"  id="snsIframe3" width="0" height="0" style="display: none"></iframe><iframe src="http://112.35.7.169:9010/educloud/pages/eduapps/agent.jsp"  id="snsIframe3_test" width="0" height="0" style="display: none"></iframe><iframe src="http://edu.10086.cn/educloud/study/zq/agent.jsp"  id="snsIframe" width="0" height="0" style="display: none"></iframe>');
	var urlC = "http://edu.10086.cn/educloud/study/zq/agent.jsp";
	var urlC3 = "http://edu.10086.cn/cloud/pages/eduapps/agent.jsp";
	var urlC3_test = "http://112.35.7.169:9010/educloud/pages/eduapps/agent.jsp";
	document.getElementById("snsIframe").src=urlC;
	document.getElementById("snsIframe3").src=urlC3;
	document.getElementById("snsIframe3_test").src=urlC3_test;
    var oldHight=document.body.scrollHeight;
    var i=1;
    setInterval(function(){
        var Hight = document.body.scrollHeight;
		
        if(!isChrome){//非Chrome 浏览器
        	Hight = Hight +100;
        }
            if(Hight<540){
            	Hight = 540;
            }
        if(i==1){
	         document.getElementById("snsIframe").src=urlC+"?ran="+Math.random()+"#"+Hight;
			 document.getElementById("snsIframe3").src=urlC3+"?ran="+Math.random()+"#height="+Hight;
			 document.getElementById("snsIframe3_test").src=urlC3_test+"?ran="+Math.random()+"#height="+Hight;
	         oldHight=Hight;
        	i++;
        }else{
	        if(oldHight!==Hight){
	            document.getElementById("snsIframe").src=urlC+"?ran="+Math.random()+"#"+Hight;
				document.getElementById("snsIframe3").src=urlC3+"?ran="+Math.random()+"#height="+Hight;
				document.getElementById("snsIframe3_test").src=urlC3_test+"?ran="+Math.random()+"#height="+Hight;
	            oldHight=Hight;
	        }  
        }
             
    },200);
}
jQuery(document).ready(function($){
	if(address.indexOf('http://10086.gkcx.eol.cn')>-1){
		if(!isChrome){//非Chrome 浏览器
			$("a").live("focus",function(){
				 $(this).attr("target","_self"); 
				});	
        }else{
        	$("a").live("click",function(){
				 $(this).attr("target","_self"); 
				});	
        }
	 }
});
</script>
