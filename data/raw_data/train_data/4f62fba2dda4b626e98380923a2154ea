    function shoucang(ids){
        var title = document.getElementById("post_title").value;
        var n = 1;
        var num = Number(document.getElementById("num").value) + Number(n);
        $.ajax({
            url: '/user/Favorite/do_favorite',
            data: {object_id: ids, title: title},
            type: 'post',
            dataType: 'json',
            success: function(data){
                if(data === 1){
                    alert("Ç×£¬ÄãÒÑÊÕ²Ø¹ýÁËÅ¶");
                    return false;
                }
                if(data === 2){
                    document.getElementById("nums").innerHTML = num;
                    alert("ÊÕ²Ø³É¹¦");
                    return false;
                }
                if(data === 3){
                    alert("ÊÕ²ØÊ§°Ü");
                    return false;
                }
                else{
                    alert(data.info);
                }
            }});
    }
</script>
