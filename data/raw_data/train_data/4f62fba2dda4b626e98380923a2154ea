  <script>
    $(function(){
        fixed_right(2);
    })
        
    function fixed_right(n) {
        if ($("#footer").length > 0 && $(".rightPanel").length > 0) {
            var num = n, s_top = 0, header_height = $("header").height(), footer_height = $("#footer").height(), w_height = $(window).height(), left_height = $(".left-list").height(), right_height = $("aside").height(), rightPanel_last = $(".rightPanel:last"), g_g = $(".g_g:last"), g_g_length = $(".g_g").length, g_g_height, o_top, o_height, last_top;
            if (num == 1) {
                o_top = rightPanel_last.offset().top;
                o_height = rightPanel_last.outerHeight(true)
            }
            if (num == 2) {
                o_top = rightPanel_last.prev().offset().top;
                o_height = rightPanel_last.outerHeight(true) + rightPanel_last.prev().outerHeight(true)
            }
            $(window).scroll(function () {
                s_top = $(window).scrollTop();
                g_g_height = g_g_length > 0 ? (g_g.outerHeight(true) + 40) : 0;
                var footer_top = $("#footer").offset().top;
                if (left_height > right_height) {
                    if (s_top >= o_top) {
                        if (num == 2) {
                            rightPanel_last.prev().css({"position": "fixed", "top": "40px"});
                            last_top = 40 + rightPanel_last.prev().outerHeight(true);
                            rightPanel_last.css({"position": "fixed", "top": last_top})
                        } else {
                            if (num == 1) {
                                rightPanel_last.css({"position": "fixed", "top": "40px"})
                            }
                        }
                    } else {
                        if (s_top < o_top) {
                            rightPanel_last.prev().css({"position": "static", "top": "0"});
                            rightPanel_last.css({"position": "static", "top": "0"})
                        }
                    }
                    if (s_top > footer_top - g_g_height - o_height) {
                        var last_top2 = footer_top - g_g_height - s_top - o_height;
                        var last_prev_top = last_top2 + rightPanel_last.prev().outerHeight(true);
                        if (num == 2) {
                            rightPanel_last.prev().css({"position": "fixed", "top": last_top2});
                            rightPanel_last.css({"position": "fixed", "top": last_prev_top})
                        }
                        if (num == 1) {
                            rightPanel_last.css({"position": "fixed", "top": last_top2})
                        }
                    }
                }
            })
        }
    }
</script> 
