<script type="text/javascript">
$(document).ready(function () {
        $("#edt").width("100px");
	$("#e_controls").click(function(){
		$("#frienddiv").hide();
	});

	//@好友
	/**
	 * 内容输入@时
	 */
	$("#f_content").contents().find(".postcontent").keyup(function(e){
		
		if (e.keyCode == 50)
		{
			/*
			$("#e_friend").click();
			
			$.get(pathroot+"ajaxdiv.php?div=getFriends",function(date){
				$("#frienddiv").html(date);
				$("#frienddiv").show();
			});
			*/
		}
		else
		{
			$("#frienddiv").hide();
		}
	});
	
	
	$("#e_friend").click(function(){
		$.get(pathroot+"ajaxdiv.php?div=getFriends",function(date){
			
			$("#f_content").contents().find(".postcontent").click();
			
			$("#frienddiv").html(date);
			//调整显示位置
			//alert(" " + $.browser.safari + $.browser.opera + $.browser.msie + $.browser.mozilla);
			var sub_height = 493;
			if ($.browser.safari)
			{
				sub_height = 493+21;
			}
			else if ($.browser.opera)
			{
				sub_height = 493+21;
			}
			else if ($.browser.msie)
			{
				sub_height = 493+23;
				$("#frienddiv").css("left","333px");
			}
			else
			{
				
			}
			
			$("#frienddiv").css("top",($(document).height() - sub_height)+"px");
			
			//隐藏其余弹出框
			/*
			$("#e_image_menu").hide();
			$("#e_tbl_menu").hide();
			$("#e_url_menu").hide();
			*/
			hideMenu('');
			
			$("#frienddiv").show();
		});
	});
	
	//点击输入框时隐藏好友列表
	$("#f_content").contents().find(".postcontent").keyup(function(e){
		$("#frienddiv").hide();
	});
});
</script>

 
