        function scall() {
            if (!showad) { return; }
            if (window.screen.width < MinScreenW) {
                alert("临时提示：\n\n显示器分辨率宽度小于" + MinScreenW + ",不显示广告");
                showad = false;
                document.getElementById("Javascript.TopLeftDiv").style.display = "none";
                document.getElementById("Javascript.TopRightDiv").style.display = "none";
                document.getElementById("Javascript.CenterLeftDiv").style.display = "none";
                document.getElementById("Javascript.BottomLeftDiv").style.display = "none";
                document.getElementById("Javascript.BottomRightDiv").style.display = "none";
                return;
            }  
    }

    function hidead(n) {
        showad = false;
        if (n == 1)      { document.getElementById("Javascript.TopLeftDiv").style.display = "none"; }
        else if (n == 2) { document.getElementById("Javascript.TopRightDiv").style.display = "none"; }
        else if (n == 3) { document.getElementById("Javascript.CenterLeftDiv").style.display = "none"; }
        else if (n == 4) { document.getElementById("Javascript.BottomLeftDiv").style.display = "none"; }
        else if (n == 5) { document.getElementById("Javascript.BottomRightDiv").style.display = "none"; }
    }


    window.onscroll = scall;
    window.onresize = scall;
    window.onload = scall;

    function ADfun() {
        //顶部左边广告的显示
        var status = 'False';
       
          if (status == "True") {
              document.getElementById("Javascript.TopLeftDiv").style.display = "";
              document.getElementById("Javascript.TopLeftDiv").style.top = Toppx;
              document.getElementById("Javascript.TopLeftDiv").style.left = "0px";
          }
          //顶部右边广告的显示
          var status = 'False';
           
                if (status == "True") {
                    document.getElementById("Javascript.TopRightDiv").style.display = "";
                    document.getElementById("Javascript.TopRightDiv").style.top = Toppx;
                    document.getElementById("Javascript.TopRightDiv").style.right = "0px";
                }

          //中间左边广告的显示
                var status = 'False';
             
                if (status == "True") {
                    document.getElementById("Javascript.CenterLeftDiv").style.display = "";
                    document.getElementById("Javascript.CenterLeftDiv").style.top = "850px";
                    document.getElementById("Javascript.CenterLeftDiv").style.left = "0px";
                }

          //底部左边广告的显示
                var status = 'False';
          
                if (status == "True") {
                    document.getElementById("Javascript.BottomLeftDiv").style.display = "";
                    document.getElementById("Javascript.BottomLeftDiv").style.top = "0px";
                    document.getElementById("Javascript.BottomLeftDiv").style.left = "0px";
                }
          //底部右边广告的显示
                var status = 'False';
        
                if (status == "True") {
              document.getElementById("Javascript.BottomRightDiv").style.display = "";
              document.getElementById("Javascript.BottomRightDiv").style.top = "0px";
              document.getElementById("Javascript.BottomRightDiv").style.right = "0px";
          }
    }
    $(document).ready(function () { ADfun();})
