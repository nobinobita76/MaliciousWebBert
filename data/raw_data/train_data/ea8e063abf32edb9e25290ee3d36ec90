<script type="text/javascript">

    function qiehuan(num) {
       
        if(num==20)
        {
            //document.getElementById("qh_con20").style.display="block";
            //document.getElementById("mynav20").className="nav_on";

            document.getElementById("qh_con21").style.display = "none";
            document.getElementById("mynav21").className = "";

            for (var id = 0; id <13; id++)
            {
                if (id == num) {      
                    document.getElementById("qh_con"+id).style.display="block";
                    document.getElementById("mynav"+id).className="nav_on";
                }
                else {
                    document.getElementById("qh_con" + id).style.display = "none";
                    document.getElementById("mynav" + id).className = "";
                }
            }

        }else if(num==21)
        {
            document.getElementById("qh_con21").style.display="block";
            document.getElementById("mynav21").className="nav_on";

            //document.getElementById("qh_con20").style.display = "none";
            //document.getElementById("mynav20").className = "";

            for (var id = 0; id <13; id++)
            {
                if (id == num) {      
                    document.getElementById("qh_con"+id).style.display="block";
                    document.getElementById("mynav"+id).className="nav_on";
                }
                else {
                    document.getElementById("qh_con" + id).style.display = "none";
                    document.getElementById("mynav" + id).className = "";
                }
            }

        }else{
            for (var id = 0; id <13; id++)
            {
                if (id == num) {      
                    document.getElementById("qh_con"+id).style.display="block";
                    document.getElementById("mynav"+id).className="nav_on";
                }
                else {
                    document.getElementById("qh_con" + id).style.display = "none";
                    document.getElementById("mynav" + id).className = "";
                }
            }
            //document.getElementById("qh_con20").style.display = "none";
            //document.getElementById("mynav20").className = "";
            document.getElementById("qh_con21").style.display = "none";
            document.getElementById("mynav21").className = "";
        }
}
       
 
</script>


