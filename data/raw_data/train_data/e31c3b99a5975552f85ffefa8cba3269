            <div class="cbox-prompt-w" node-type="error-tips">
                <span node-type="prompt-empty" class="prompt-empty-w">评论内容为空！</span>
            </div>
        </div>
    </div>
</div>
                <!-- 放置cbox发布状态 -->
                <!-- 提示条 -->
                <!-- 零评论提示条 -->
                <div class="list-comment-empty-w">
                    
                </div>
                <!-- 提示连接到快站社区 -->
