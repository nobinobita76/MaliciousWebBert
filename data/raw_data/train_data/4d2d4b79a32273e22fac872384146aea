    <script>
        window.page_prev = 1;
        function ViewMoreCallback(data) {
            if (data.errorCode == 0) {
                var list = data.data;
                window.page_prev = data.page;
                var frag = document.createDocumentFragment();
                for (var i = 0; i < list.length; i++) {
                    var ent = list[i];
                    var warp = document.createElement("a");
                    warp.href = '/mnews/' + ent.id + '.html';
                    warp.className = 'lxw';
                    var html = [];
                    html.push('<div class="l_text">');
                    html.push('<h3>' + ent.title + '</h3>');
                    html.push('<p>');
                    html.push(ent.descript);
                    html.push('</p>');
                    html.push('</div>');
                    html.push('<span class="label1">' + ent.catename + '</span>');
                    html.push('<em>' + ent.adddate + '</em>');
                    warp.innerHTML = html.join('');
                    frag.appendChild(warp);
                }
                $("#more").html("上拉或点击查看更多...")
                document.getElementById('infocon').appendChild(frag);
            }
        }

        window.page_temp = 0;
        function ViewMore() {
            $("#more").html("内容正在加载中...")
            var page_prev = window.page_prev;
            if (window.page_temp != page_prev) {
                window.page_temp = page_prev;
                var s = document.createElement("script");
                s.src = '/news/list_more.json?key=news&encoding=utf-8&callback=ViewMoreCallback($data)&page_prev=' + page_prev;
                document.body.appendChild(s);
            }
        }
    </script>
