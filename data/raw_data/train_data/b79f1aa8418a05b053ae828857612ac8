<script>
    function showPopupLogin(){
        $('.codeDialog').css({"visibility":'visible'});
        $('.codeShade,.dialogCtn').addClass('active');
    }
    function changeVerify2() {
        $('.codeDialog .verify-img').attr('src', '/Member/verify/ImageId/2/'+Math.random(100000000,999999999));
    }
$(function(){
    
    $('.closeBtn-y').click(function(){
        $('.codeShade,.dialogCtn').removeClass('active');
        $('.codeDialog').css({"visibility":'hidden'});
       
    })




    $('.tourists-login').click(function(){
        $.ajax({
            type : 'POST',
            url : '/Member/visitorRegister',
            dataType : 'json',
            success : function(data){
                if(data.CODE !== 'ok'){
                    layer.msg(data.MESSAGE);
                } else{
                    $('.codeShade,.dialogCtn').removeClass('active');
                    $('.codeDialog').css({"visibility":'hidden'});
                    if (location.href.search('/Member/') != -1){
                        parent.location.href="/";
                    } else{
                        parent.location.reload();
                    }
                }
                
                if (data.CODE === 'logined'){
                    $('.codeShade,.dialogCtn').removeClass('active');
                    $('.codeDialog').css({"visibility":'hidden'});
                    if (location.href.search('/Member/') != -1){
                        parent.location.href="/";
                    } else{
                        parent.location.reload();
                    }
                }
                    
            },
        });
    });

    //登录点击
    $('.codeDialog .loginBox .login-btn').on('click', function(){
        if(!$('.codeDialog input[name="Username"]').val()) {
            layer.msg('请输入手机号',{time:2000});
            return false;
        }
        if(!$('.codeDialog input[name="Password"]').val()) {
            layer.msg('请输入密码',{time:2000});
            return false;
        }
        if (!/^([a-zA-Z0-9]{6,20})$/.exec($('.codeDialog input[name="Password"]').val())){
            layer.msg('密码格式错误',{time:2000});
            return false;
        }
        if(!$('.codeDialog input[name="ImageVerify"]').val()) {
            layer.msg('请输入验证码',{time:2000});
            return false;
        }
        if($('.codeDialog .voice-code').css('display')!='none' && $('.codeDialog input[name=VerifyCode]').val()=='') {
            layer.msg('请输入手机验证码',{time:2000});
            return false;
        }
        $.ajax({
            type : 'POST',
            url : '/Member/login',
            data : $('.codeDialog form').serialize(),
            dataType : 'json',
            success : function(data){
                if(data.CODE !== 'ok'){
                    changeVerify2();
                    layer.msg(data.MESSAGE);
                } else{
                    layer.msg('登录成功',{time:2000});
                    if (location.href.search('/Member/') != -1){
                        location.href="/";
                    } else{
                        location.reload();
                    }
                }
                if(data.CODE == 'ImageCodeError') {
                    changeVerify2();
                } else if (data.CODE == 'NeedCode' || data.CODE == 'VerifyCodeError'){
                    $('.codeDialog .voice-code').css('display', 'block');
                }
            },
        });
    });

    $('.codeDialog .loginBox').keypress(function(e){
        if(e.which == 13) {  
            jQuery(".codeDialog .loginBox .login-btn").click();  
        }  
    })

    var validCode=true;
    $(".codeDialog .msgs").click (function  () {
        var time=60;
        var code=$(this);
        var flag = true;
        if (validCode) {
            validCode=false;
            $.ajax({
                type : 'POST',
                url : '/Member/sendLoginCode',
                async : false,
                data : {
                    'ApiCode':'Member.popupLogin',
                    'ImageVerify':$('.codeDialog input[name=ImageVerify]').val(),
                },
                dataType : 'json',
                success : function(data){
                    if(data.CODE != 'ok'){
                       layer.msg(data.MESSAGE,{time:2000});
                       changeVerify2();
                       flag = false;
                       validCode = true;
                    } else{
                        code.addClass("disabled");
                        var t=setInterval(function  () {
                            time--;
                            code.html(time+"秒");
                            if (time==0) {
                                clearInterval(t);
                                code.html("重新获取");
                                validCode=true;
                                code.removeClass("disabled");
                            }
                        },1000)
                    }
                },
            });
            
        }
    });
})
</script>







<!--拜师弹出框-->
<div class="dingdang" style="display: none">
    <div class="dingdang-box">
        <h2>您确认向 <em>傻t逼宋奕菲 拜师吗？</em></h2>
        <p>最近登录时间：2016-09-22 10:23:00</p>
        <div class="br clearfix btn-box">
            <button class="sure iwbs" data-pid="">确认拜师</button>
            <button class="cancel">取消</button>
        </div>
    </div>
</div>

<script>
	$(function(){
		var id = "779847";

	    //拜师弹出框
        $('.i_wbs').on("click",function () {
            $('.dingdang h2 em').text($(this).data().name+' 拜师吗？');
            $('.dingdang p').text('最近登录时间：'+$(this).data().date);
            $('.iwbs').data().pid = $(this).data().pid;
            layer.closeAll();
            layer.open({
                type: 1,
                title: false,
                content: $('.dingdang'),
                area: ['591px', '347px'],
                cancel: function (index) {
                    layer.close(index);
                }
            });
        });

        $('.iwbs').live('click', function(){
	        var $this = $(this);
	        $.ajax({
	            type: "get",
	            url: "http://www.7u.com/Friend/applyFd",
	            data: {
	       	    	'Pid' : $this.data().pid
	            },
	            dataType: "json",
	            success: function(data){
	                if (data.CODE != 'ok'){
                        layer.closeAll();
	                    layer.msg(data.MESSAGE);
	                } else{
                        layer.closeAll();
	                    layer.msg('申请成功');

	                }
	            }
	        });
	    });

        //点赞
        var dzbtnClick = true;
	    $('.dzbtn').click(function(){
	    	if (!dzbtnClick)
	    	{
	    		return false;
	    	}
	    	dzbtnClick = false;
	    	$.ajax({
	            type: "get",
	            url: "http://www.7u.com/Home/assess",
	            data: {
	       	    	'Id' : id,
	       	    	'Type' : 'love',
	            },
	            dataType: "json",
	            success: function(data){
	                if (data.CODE != 'ok'){
	                    layer.msg(data.MESSAGE);
	                    dzbtnClick = true;
	                } else{
                        layer.closeAll();
	                    layer.msg('评价成功');
	                    $('.dzbtn').remove();
	                    $('.djdbtn').remove();
	                    $('.s-info').css('height', '100px');
	                    $('.like-btn').html(data.DATA.LikeCnt+'<em></em>');
	                }
	            }
	        });
	    });

	    //丢鸡蛋
	    var djdbtnClick = true;
	    $('.djdbtn').click(function(){
	    	if (!djdbtnClick)
	    	{
	    		return false;
	    	}
	    	djdbtnClick = false;
	    	$.ajax({
	            type: "get",
	            url: "http://www.7u.com/Home/assess",
	            data: {
	       	    	'Id' : id,
	       	    	'Type' : 'hate',
	            },
	            dataType: "json",
	            success: function(data){
	                if (data.CODE != 'ok'){
	                    layer.msg(data.MESSAGE);
	                    djdbtnClick = true;
	                } else{
                        layer.closeAll();
	                    layer.msg('评价成功');
	                    $('.dzbtn').remove();
	                    $('.djdbtn').remove();
	                    $('.s-info').css('height', '100px');
	                    $('.hate-btn').html(data.DATA.HateCnt+'<em></em>');
	                }
	            }
	        });
	    });

        $('.cancel').click(function(){
            layer.closeAll();
        })

        //发表评价
        var subComment = true;
        $('.up-comment').on('submit', function(){
        	if (!subComment)
        	{
        		return false;
        	}
        	subComment = false;
        	$.ajax({
	            type: "get",
	            url: "http://www.7u.com/Home/comment",
	            data: $('.up-comment').serialize(),
	            dataType: "json",
	            success: function(data){
	                if (data.CODE != 'ok'){
	                    layer.msg(data.MESSAGE);
	                    subComment = true;
	                } else{
                        layer.closeAll();
	                    layer.msg('提交评论成功，需等待客服审核。');
	                    $('.up-comment').remove();
	                }
	            }
	        });

        	return false;
        });

        //评论翻页
        $('.edit-nav .btn').live('click', function(){
        	var page = parseInt($(this).data('page'));
        	var count = parseInt($(this).data('count'));
        	if (page <= 0 || page>count)
        	{
        		return false;
        	}
        	$.ajax({
	            type: "get",
	            url: "http://www.7u.com/Home/getCommentList",
	            data: {
	            	'PageNumber' : page,
	            	'Pid' : id,
	            },
	            dataType: "json",
	            success: function(data){
	                if (data.CODE == 'ok'){
	                	$('.comments-list').html(data.HTML);	  
	                }
	            }
	        });
        });

        //动态分页
        var dtType = 1;
        $('.nav-page span').live('click', function(){
        	var page = $(this).data('page');
        	if (!page)
        	{
        		return false;
        	}
        	$.ajax({
	            type: "get",
	            url: "http://www.7u.com/Home/dtList",
	            data: {
	            	'PageNumber' : page,
	            	'Id' : id,
	            	'DtType' : dtType
	            },
	            dataType: "json",
	            success: function(data){
	                if (data.CODE == 'ok'){
	                	$('.dt_con').html(data.HTML);	  
	                }
	            }
	        });
        })

        //切换动态
        $('.con-tab a').click(function(){
        	$('.con-tab a').removeClass('current');
        	$(this).addClass('current');
        	dtType = $(this).data('type');
        	$.ajax({
	            type: "get",
	            url: "http://www.7u.com/Home/dtList",
	            data: {
	            	'Id' : id,
	            	'DtType' : dtType
	            },
	            dataType: "json",
	            success: function(data){
	                if (data.CODE == 'ok'){
	                	$('.dt_con').html(data.HTML);	  
	                }
	            }
	        });
	        return false;
        });

        $('.get_child').click(function(){
            location.href="http://www.7u.com/Friend/myChild";
        });
        
    });

</script></body></html>