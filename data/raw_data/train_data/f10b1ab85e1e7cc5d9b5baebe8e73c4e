            var InputRecord = getCookie("RecordLoginInput_" + EplatformID) != null ? getCookie("RecordLoginInput_" + EplatformID).replace(/%7C/g, "|") : "";

            $(document).bind("click", function (e) {
                try {
                    var target = $(e.target);
                    if (target.closest(".login_div").length == 0 && target.context.className != "liclosebtn") {
                        $(".user_nameul").hide();
                    }
                } catch (e) { }
            })
            $(".user_nameul li a[name='sel']").live("click", function () {
                var userItem = $(this).text();
                $("#UName").val(userItem);
                $(".user_nameul").hide();
                $("#PassWord").focus();
            })
            $(".user_nameul li .liclosebtn").live("click", function () {
                var thisVal = $(this).prev().text();
                var userAry = InputRecord.split('|');
                var aryIndex = -1;
                for (var i = 0, j = userAry.length; i < j; i++) {
                    if (userAry[i] == thisVal)
                        aryIndex = i;
                }
                if (aryIndex > -1) {
                    userAry.splice(aryIndex, 1);
                    InputRecord = userAry.join('|');
                    setCookie('RecordLoginInput_' + EplatformID, InputRecord, 365);

                    if ($(".user_nameul li").length > 1) {
                        $(this).parent().remove();
                    }
                    else {
                        $(".user_nameul").remove();
                    }
                }
            })
            $("#UName").click(function () {
                if ($(".user_nameul").is(":hidden")) {
                    if (InputRecord != "") {
                        var userHtml = '';
                        var userAry = InputRecord.split('|');
                        if (userAry != null && userAry.length > 0) {
                            for (var i = 0, item; item = userAry[i++]; ) {
                                userHtml += '<li><a name="sel" href="javascript:void(0)">' + item + '</a><a href="javascript:void(0)" class="liclosebtn">&times;</a></li>';
                            }
                            $(".user_nameul").html(userHtml);
                            $(".user_nameul").show();
                        }
                    }
                }
                else {
                    $(".user_nameul").hide();
                }
            })

            $("#UName").keydown(function (e) {
                if (e.keyCode == 9) {
                    $(".user_nameul").hide();
                }
            });
            //登录框历史登录用户


            //绑定第三方登录的事件
            $("#QQLogin").click(function () {
