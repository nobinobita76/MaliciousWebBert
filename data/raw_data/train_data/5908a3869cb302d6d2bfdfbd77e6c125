<script type="text/javascript">
$(document).ready(function(){
	var channel = channel || '';
	share.url = encodeURIComponent(share.url);
	share.pic = encodeURIComponent(share.pic);
	//if((typeof channel!=undefined) && (channel=='movie'||channel=='openclass'||channel=='tv')) $('#yyets_share').prepend('<a style="background:url('+GLOBAL.CONST.RES_URL+'images/rss.png) no-repeat;" href="'+GLOBAL.CONST.WWW_URL+'rss/'+rid+'" rel="rss" target="_blank"></a>');
	$('span#share_link a').click(function(){
		var url = '';
		var title = encodeURIComponent(share.title+' 分享地址:'+share.url+(channel=='tv'||channel=='movie'?' #'+share.cnname+'#':'')+','+GLOBAL.CONST.WWW_URL);
		switch($(this).attr('rel')){
			case 'weibo':
				url = 'http://service.weibo.com/share/share.php?title='+title+'&pic='+share.pic;
				break;
			case 'qzone':
				//url = 'http://share.v.t.qq.com/index.php?c=share&a=index&title='+title+'&url=&appkey=a75e1c5904c842b7b3aed10213379cca&site='+GLOBAL.CONST.WWW_URL+'&pic='+share.pic;
				url = 'http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?url='+share.url+'&title='+share.title+'&pics='+share.pic+'&summary=';
				break;
			case 'renren':
				url = 'http://share.renren.com/share/buttonshare.do?link='+share.url+'&title='+title;
				break;
			case 'qq':
				url = 'http://connect.qq.com/widget/shareqq/index.html?url='+share.url+'&showcount=0&desc=&title='+share.title+'&site='+GLOBAL.CONST.WWW_URL+'&pics='+share.pic;
				break;
		}
		if(url!='') share_window(url);
	})
	GLOBAL.JQUERY.Delay({obj:$('span#share_link a[rel=weixin]'),over:function(){
		var pos = $(this).position();
		var html = '<div class="qrcode" style="left:'+(pos.left-50)+'px;top:'+(pos.top+20)+'px;"><img src="/public/qrcode?url='+share.url+'"><br/>打开微信，点击底部的"发现"，使用＂扫一扫＂即可将网页分享到朋友圈</div>';
		$('#share_link').append(html);
	},out:function(){
		$('.qrcode').remove();
	}});
});
var share_window=function(share_url){
	window.open(share_url,'_blank','scrollbars=no,width=600,height=450,left=75,top=20,status=no,resizable=yes');
}
