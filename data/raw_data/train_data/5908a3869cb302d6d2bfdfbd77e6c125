<script src="http://js.zmzjstu.com/js/list.js" type="text/javascript"></script>
<script type="text/javascript">
var id = "51615";
var channel = 'subtitle';
var share = {
  title:'【美剧字幕】《《天蝎》第三季第十八集》(Scorpion S03E18)',
  url:GLOBAL.CONST.WWW_URL+'subtitle/'+id,
  pic:''
};

function commentPage(page,init){
  GLOBAL.CommentList({
    target:$('#comments_container'),
    reply:$('#reply_comment'),
    pagination:$('#page_container'),
    itemid:id,
    channel:channel,
    pagesize:10,
    page:page,
    pageFunction:'commentPage',
    init:init
  });
}

$(document).ready(function() {
  //短评列表生成
  GLOBAL.JQUERY.LazyLoad($('#comments_container'),function(){commentPage(0,1);});
  //加载计数器
  GLOBAL.Util.counter(channel,id);

  $('.subtitle-links h3 a').click(function(){
    GLOBAL.Util.counter('subtitle_download',id);
  });
});

if (typeof GLOBAL.CONST.USER.uid != "undefined"){
  $("#del_subtitle").click(function(e){
    if(window.confirm('确定删除字幕?')){
      $.post(GLOBAL.CONST.WWW_URL+'subtitle/index/del',{id:id},function(d){
        if (d.success == 1){
          window.location.href = GLOBAL.CONST.WWW_URL+"subtitle";
        }
      },"json");
    }
  });
}
</script>
