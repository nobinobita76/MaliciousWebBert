/*查看更多*/
var total = 300;
$(function() {
    if ($('#jbli li').length < 4) {
        $('#jbbtn').remove();
    }
    $("#jbbtn").bind("click",
    function() {
        var len = $('#jbli li').length;
        for (var i = 3; i < total; i += 3) {
            var num = i + 3;
            var listSelect = "#jbli li.dis-" + num;
            if (len > i) {
                if ($(listSelect).css('display') == 'none') {
                    $(listSelect).css('display', "block");
                    if (len < num + 1) {
                        $('#jbbtn').remove();
                    }
                    return;
                }
            }
        }
    });
})
</script>
