 	for (N in DCS){ 
 		if (DCS[N]) { 
 			P+=A(N,DCS[N]); 
 		} 
 	} 
 	P+=A('STS.SSOUser_SDC',dcsGetCookie('STS.SSOUser_SDC')); 
 	P+=A('SSUID',dcsGetCookie('SSUID')); 
 for (N in WT){ 
 		if (WT[N]) { 
 			P+=A("WT."+N,WT[N]); 
 		} 
 	} 
 	for (N in DCSext){ 
 		if (DCSext[N]) { 
 			P+=A(N,DCSext[N]); 
 		} 
 	} 
 	if (P.length>2048&&navigator.userAgent.indexOf('MSIE')>=0){ 
 		P=P.substring(0,2040)+"&WT.tu=1"; 
 	} 
 	dcsCreateImage(P); 
 } 
