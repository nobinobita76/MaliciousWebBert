    $.ajax({
        type:'get',
        url:"http://wuhan.liebiao.com/ajax/postclicks/?postid=272845366",
        cache:false,
        dataType:'json',
        success: function(json) {
            if (!json.error) {
                $('#post_visitors').html(json.msg.postclicks);
