  <script src="//ohfnc0uzy.qnssl.com/dist/mobile2-js-bundle-f3eb7ac0ac73a8c2edb3.js"></script>

  
  
  <script type="text/javascript">
  $(function () {
    $("a[data-expose-tracking-url]").each(function () {
      sendAdLog($(this).attr("data-expose-tracking-url"));
    });
    // 绑定点击事件
    $(document).on("click", "a[data-click-tracking-url]", function(){
      sendJumpAdLog($(this).attr("data-click-tracking-url"), $(this).prop('href'));
    });
    function sendAdLog(url){
      if (!url) return;
      var sender = new Image(1, 1);
      sender.onload = sender.onerror = function () {};
      sender.src = url;
    }
    function sendJumpAdLog(url, jump2Url){
      if (!url) return;
      var sender = new Image(1, 1);
      sender.onload = sender.onerror = function () {
        location.href = jump2Url;
      };
      sender.src = url;
    }
  })
  </script>





























