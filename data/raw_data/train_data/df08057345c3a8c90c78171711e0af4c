<script language="javascript">
	var compareCookie = document.cookie;
	var start;
	var end;
	var sel_id_str = "";//选择的proj_id串
	var sel_name_str = "";//选择的proj_name串
	var sel = new Array();
	var sel_num = 0;
	var Link = "http://newhouse.xian.fang.com/house/";
	//读cookie
	if(compareCookie != null && compareCookie != ""){
		sel_id_str = getCookieCompare("sel_id_str");
		sel_name_str = getCookieCompare("sel_name_str");
	}
	
	//如果用户点评了就显示红心，没点评就显示灰心
	$(function(){
		var redsrc = $('.redhand').attr("src");
		if(redsrc){
			$('.redhand+img').remove();
		}
	
	})
	
	//初始化悬浮框
	if(sel_id_str != "" && sel_id_str != null){
		var sel_id_arr = new Array();
		var sel_name_arr = new Array();
		sel_id_arr = sel_id_str.split("|");
		sel_name_arr = sel_name_str.split("|");
		for(i = 0; i < sel_id_arr.length - 1;i ++){
			sel[sel_id_arr[i]] = sel_name_arr[i];
		}
		sel_num = sel_id_arr.length - 1;
		for(i = 0; i < sel_id_arr.length - 1;i ++){
			add(sel_id_arr[i], sel_name_arr[i]);
			draw();
		}
	}
	function createCompareView(result){
		if('succ' !== result.status) return false;
		var viewUl = '<ul>';
		$.each(result.data, function(i, field){
			var fieldData = field.value.split("ζ");
			viewUl += '<li id="view_li_'+field.keyname+'" style="cursor:pointer;"><span><input class="lel" type="checkbox" name="toCompare" checked="checked" value="'+field.keyname+'"> <label class="lel">'+fieldData[0]+'</label></span><em onclick="del_li('+field.keyname+');"></em></li>';
		});
		viewUl  +="</ul>";
		$('#view_item').html(viewUl);
	}
	for(var k=0; k < essence_slideid.length; k++) {
		$('#slidebox_'+essence_slideid[k]).slide({mainCell:".bd ul",autoPlay:false});
		$('#slidebox_'+essence_slideid[k]+' .hd li').click(function(){
			$(this).parents('div.hd').siblings('.bd').fadeIn(1500);
		});
	}
	function show_video(obj)
  {
	var videourl = obj.getAttribute('videourl');
	var videoid = obj.getAttribute('videoid');

	if(videourl == '')
	{
		return false;
	}
		
	layer.open({
	    type: 1, //page层
	    area: ['632px', '425px'],
		title:'视频',
	    skin: 'layui-layer-molv', //墨绿风格
	    offset: ['10%' , '20%'], //坐标
	    shade: 0.6, //遮罩透明度
	    shift: 1, //0-6的动画形式，-1不开启
 		content: '<span style="padding:0px;" id="videoflv"></span>'
	}); 	
	document.getElementById('videoflv').innerHTML='<embed src="http://img2s.soufun.com/v/player11/loader.swf" FlashVars="Vinfoid='+videourl+'" quality="high" bgcolor="#ffffff" width="100%" height="100%" name="flvplayer2011_610_480" align="middle" allowScriptAccess="always" allowFullScreen="true" type="application/x-shockwave-flash" pluginspage="http://www.macromedia.com/go/getflashplayer" />';
  }
	
</script>


