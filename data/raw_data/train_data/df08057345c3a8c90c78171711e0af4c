<!-- shadow number -->

<script>
			$(function()
			{
				var shadow_config = {'hao123':'400-890-8503','hao360':'400-890-8504'};
				var cookieObj = {};
				var cookieArr = document.cookie.split(';');
				for(var index in cookieArr)
				{
					var item = cookieArr[index].replace(' ','');
					var key = item.split('=')[0];
					var value = item.split('=')[1];
					cookieObj[key] = value;
				}
				
				var sf = cookieObj.sf_source;
				if(sf == undefined)
				{
					return false;
				}
				else if(shadow_config[sf] == undefined)
				{
					return false;
				}				
								// 单独处理搜索框的电话
				$("#floatNaviBox span.fixnav_num")[0].innerHTML = shadow_config[sf];
				
				shadow_tel_global = shadow_config[sf].replace(/-/g,'');
				
				return shadow_config[sf];
			});

	//用户头像找不到 触发onerror事件

function nofind()
{


	var img=event.srcElement;

	img.src="http://js.soufunimg.com/space/images/space_img_01.jpg";

	img.onerror=null;// 控制不要一直跳动
	
   
}	

//周边楼盘和热门楼盘 鼠标放上显示楼盘详细信息
$(function(){
	$("#zblp_brief_0").hide();
	$("#zblp_detail_0").show();
	$("#lprp_brief_0").hide();
	$("#lprp_detail_0").show();
})

function Toshow(type,point){
	var count = $("a[id^="+type+"_brief_]").size();
	var i = 0;
	$('#'+type+'_detail_'+point).show();	
	$('#'+type+'_brief_'+point).hide();
	for(i;i<count;i++){
		if(i != point){
			$('#'+type+'_detail_'+i).hide();	
			$('#'+type+'_brief_'+i).show();
		}
	}
}

</script>
