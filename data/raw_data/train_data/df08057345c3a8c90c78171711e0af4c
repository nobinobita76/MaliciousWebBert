<script>
<!--js动态获取用户登录信息-->
var userid = '';
var username  = '';
$.ajax({
	type:'get',
	dataType:'json',
	url:'/house/ajaxrequest/getLoginData.php',
	async: false,
	data:{},
	complete:function(){},
	error:function(){},
	success:function(msg){
		if(msg){
			userid = msg.loginUid;
			username = msg.userName;
		}
	}
});
