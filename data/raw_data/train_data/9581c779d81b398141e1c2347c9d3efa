  <script type="text/javascript">
(function(){
	var _h2 = jQuery('#listWrap h2');
	_h2.bind({
		click : function(){
			jQuery(this).toggleClass('up').next().toggle();
		},
		mouseover : function(){
			jQuery(this).css('backgroundColor','#f0f0f0');
			jQuery(this).find('.more').css('color','#8D0200');
		},
		mouseout : function(){
			jQuery(this).css('backgroundColor','');
			jQuery(this).find('.more').css('color','#999')
		}
	});
	var _dropDown = jQuery('#bmdh, #btdh');
	_dropDown.each(function(){
		jQuery(this).hover(function(){
			jQuery(this).css('visibility','visible');
		}, function(){
			jQuery(this).css('visibility','hidden');
		});
	});
	
	var _subNav = jQuery('ul.subNav');
	_subNav_li = _subNav.find('li:not(:first)');
	_subNav_li.each(function(i){
		var n = i;
		jQuery(this).hover(function(){
			_dropDown.eq(n).css('visibility','visible');
		}, function(){
			_dropDown.eq(n).css('visibility','hidden');
		});
	});	
})(jQuery)
</script> 
