  <script type="text/javascript">
    var Pages = {};
    Pages = {
        rfType:{"v.6.cn":0,"www.laifeng.com":1,"fanxing.kugou.com":2,"v.17173.com":3},
        init:function(){
            Pages.wrap = $("#conListIndex");
            var url = "/index/getanchors";
            $.ajax({
                url: url,
                type: 'GET',
                dataType: 'json'
            })
            .done(function(data) {
                try {
                    data = JSON.parse(data)[0];
                } catch (e) {
                    data = eval(data)[0];
                }
                Pages.createList(data);
            }).fail(function() {}).always(function() {});
            
        },
        createList:function(dt){
            var str = new helper.strBuilder();
            if (dt && dt.length > 0) {
                for (var i = 0; i < dt.length; i++) {
                    var ds = dt[i];
                    str.append('<li class="rf'+Pages.rfType[ds.webtype]+'"><a target="_blank" href="/index/room?bgurl='+escape(ds.openurl)+'"><img class="lz_img" src="'+ds.anchorpic+'" height="130px" class="anchor"><div class="des"><i class="ap_bg"></i><em class="lines">在线人数:'+ds.onlinenum+'</em></div><span class="name">'+ds.anchorname+'</span><i class="rf">'+ds.webname+'</i></a></li>');
                };
            }else{
                str.append('<li class="noresi">暂无数据</li>')
            }
            Pages.wrap.html(str.toString());
        }
    }

    $(document).ready(function() {
        Pages.init();
    });
</script>   
