$(function() {
	var outcontent = "";
	$(".dzcloumclass").each(function(index) {
		outcontent += $(this).html();
		$(this).remove();
	});
	if(outcontent != null && typeof(outcontent) != "undefined") {
		$(".RightCon #tc_content").html(outcontent);
	}
	
	$(".Center .LeftCon").append($(".dzcloumdatas").html());
	$(".dzcloumdatas").remove();
})
