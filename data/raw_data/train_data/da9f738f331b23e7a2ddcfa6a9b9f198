     <script type="text/javascript">
function w_replay(){
	var data = {};
	data.content = $("#w-replay-content").val();
	data.code = $(".w-replay-code").val();
	data.nid = $("input[name=nid]").val();
	data.comment_floor = $("input[name=comment_floor]").val();
	data.username = $("input[name=username]").val();
	
	if(!data.nid){
		art.tip('参数丢失，请刷新重试');
		window.location.reload();
		return false;
	}
	if(!data.code){
		art.tip('请输入验证码');
		return false;
	}
	
	if(!data.content){
		art.tip('请输入评论内容');
		return false;
	}
	
	$.ajax({
		url:'/Comment/comment_post',
		type:'POST',
		data:data,
		dataType:'json',
		success:function(response){
			art.tip(response.info,response.status,function(){
				if(response.status =='success'){
					$("input[name=comment_floor]").val(0);
					window.location.reload();	
				}	
			})
		},error:function(){
			art.tip('网络故障，请重试！','error');	
		}
		
	})
		
	return false;
}

function replay_btn(comment_floor,username){
	$("input[name=comment_floor]").val(comment_floor);
	$("#w-replay-content").val('回复'+username+'：');
	window.location.hash = 'W_COMMENT_REPLAY';
	
}

$('#captcha_img').click(function(){ var src = $(this).attr('src').replace(/\/refresh\/\d+/i,'');$(this).attr('src',src+'/refresh/'+(Math.floor(Math.random()*10000))); });
//placeholder 兼容性处理
$(function(){
	var s = $('#login_form');
	if( 'placeholder' in document.createElement('input') ){
		$('.placeholder',s).remove();
	}else{
	//模拟不支持placeholder的浏览器
		$('.text',s).each(function() {
			var val = $(this).val();
			if(val.length>0){
				$(this).siblings('.placeholder').hide();
			}else{
				$(this).siblings('.placeholder').show();	
			}            
		});
		$('.text',s).on('keyup blur mousedown mouseup',function(){
			var val = $(this).val();
			if(val.length>0){
				$(this).siblings('.placeholder').hide();
			}else{
				$(this).siblings('.placeholder').show();	
			}
		});		
			
	}
})
</script> 
