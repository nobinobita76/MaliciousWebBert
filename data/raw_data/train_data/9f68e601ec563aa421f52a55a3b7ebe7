        <script>
$('#searchform').submit(function(){
	var word = $(this).find('input[name="word"]').val();
	var method = $(this).find('select').val();
	if(word == ''){
		alert('请输入关键字！');return false;
	}else{
		if(method == 1){
			window.open('/index.php?g=Web&m=Index&a=search&word='+word);
		}else if(method == 2){
			window.open('http://www.baidu.com/baidu?wd='+word);
		}else{
			window.open('http://www.so.com/s?q='+word);
		}
	}
	return false;
});
</script> 
