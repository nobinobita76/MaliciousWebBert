<script type="text/javascript">
    window._DATA = window._DATA || {};
    window._DATA.user = null;
    window.getStringWidth = function(str) {
        var width = len = str.length;
        for(var i=0; i < len; i++) {
            if(str.charCodeAt(i) >= 255) {
                width++;
            }
        }
        return width;
    }
    $(function () {
        $(".double.hd-nav .item").each(function () {
            var $a = $(this).find("a");
            var a_text = $a.html();
            var width = 16*a_text.length+36;
            $(this).width(width);
        });
        $(".single.hd-nav .item").each(function () {
            var $a = $(this).find("a");
            var a_text = $a.html();
            var width = 14*a_text.length+28;
            $(this).width(width);
        });
    });


