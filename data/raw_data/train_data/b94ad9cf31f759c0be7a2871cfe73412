  <script src="http://public1.zgkjsws.net/assets/global/jPages/jPages.min.js"></script> 
  <script>
    $(function(){
        $('body .content .col-xs-8 .inreply .num').find('br').remove();
        //我回复当前帖子输入框出现
        $('div.content .col-xs-8 .topic .response').click(function(){
            var html = $('#Myrep').html(),name = $(this).data('name'),editDiv = $(this).parent().siblings('div.mineinput'),
                    len = editDiv.children().length;
            console.log(len);
            $('div.mineinput').empty();
            if(!len){
                editDiv.html(html);
//                $('span.host').html(name+'<i>请输入您的评论...</i>');
                $('#reply-avatar').attr('src',$(this).data('avatar'));
                //表情加载
                twemoji.parse(document.getElementById('emoji-list'),{size:36,base:'http://s.public.yingxiong.com/herohtml_test/community-app/twemoji/'});
                $('#emoji-list').on('click','li',function(){
                    $('#editBox').append($(this).html());
                });
                lReply();
            }
        });

        $('div.content div.col-xs-8').on('click','i.clickrep',function(){
            var html = $('#MyrepAn').html(),name = $(this).data('name'),editDiv = $(this).parent().siblings('div.mineinput'),
                    len = editDiv.children().length;
            console.log(len);
            $('div.mineinput').empty();
            if(!len){
                editDiv.html(html);
//                $('span.host').html(name+'<i>请输入您的评论...</i>');
                //表情加载
                twemoji.parse(document.getElementById('emoji-list1'),{size:36,base:'http://s.public.yingxiong.com/herohtml_test/community-app/twemoji/'});
                $('#emoji-list1').on('click','li',function(){
                    $('#editBox1').append($(this).html());
                });
                lReply();
            }
        });

        $('div.y-reply-list').each(function(){
            var id = $(this).data('id'),container = 'lzl'+id,doc = '#lzlp'+id,len = $(this).children('div.inreply').length;
            if(len > 5){
                $(doc).jPages({
                    containerID : container,
                    previous : "上一页",
                    next : "下一页",
                    perPage : 5,
                    callback:function(pages,items){}
                });
            }
        });
        $('i.y-role-home').click(function(){
            var id = $(this).data('id');
            location.href = '/Home/User/index?fid=1&id='+id;
        });

        function lReply(){
            $('span.face').click(function(){
                $('div.facebox').toggleClass('hidden');
                if($(this).closest("div").next(".facebox").hasClass("hidden")){
                    $(this).closest("div").removeClass("line");
                }else{
                    $(this).closest("div").addClass("line");
                }
                var mySwiper2 = new Swiper('.swiper-face',{
                    pagination :'.swiper-pagination',
                    direction :'horizontal',
                    paginationClickable :true,
                });
            });
            $("#editBox,#editBox1").focus(function(){
                $(this).prev(".show").addClass("hidden");
            });

            $("#editBox,#editBox1").blur(function(){
                if($(this).text()==""){
                    $(this).prev(".show").removeClass("hidden");
                }
            });
            $('span.anonymity').click(function(){
                $(this).siblings('span.quizcheck').toggleClass('checkbg');
            });
            $('span.P-detail').click(function(){
                var content = $.trim($('#editBox').html()) || $.trim($('#editBox1').html()),
                        pid = $(this).parent('div.mineinput').data('pid'),
                        parent_id = $(this).parent('div.mineinput').data('rid'),
                        anonymity = $(this).siblings('span.quizcheck').hasClass('checkbg')?1:0,
                        boxId = $(this).parent('div.mineinput').data('prid') || $(this).parent('div.mineinput').data('rid');

                if(!content){
                    layer.msg('请输入内容');return;
                }
                var load = layer.load(1);
                $.ajax({
                    url:'/Home/Ajax/addReply/fid/1',
                    type:'post',
                    data:{content:content,pid:pid,anonymity:anonymity,parent_id:parent_id,pc:1},
                    dataType:'JSON'
                }).done(function(res){
                    layer.close(load);
                    layer.msg(res.msg);
                    if(res.status == 'success'){
                        if(res.value.im!=''){
                            iframeUrl(res.value.im);
                        }
                        $('div.mineinput').empty();
                        $('#lzl'+boxId).html(res.value.m_html);
                        $('#lzlp'+boxId).jPages({
                            containerID : 'lzl'+boxId,
                            previous : "上一页",
                            next : "下一页",
                            perPage : 5,
                            callback:function(pages,items){
                                if(pages.count < 2){
                                    $('#lzlp'+boxId).empty();
                                }else{
                                    $('#lzlp'+boxId).jPages(pages.count);
                                }
                            }
                        });
                    }
                });
            });
        }
    });
$('.filecheck_p').click(function(event) {
    var span=$(this).find('.filecheck');
    if(span.hasClass('checkbg')){
        span.removeClass('checkbg');
    }else{
        span.addClass('checkbg');
    }
});
$('.fileradio_p').click(function(event) {
    $('.vote_value').removeClass('checkbg');
    var span=$(this).find('.fileradio');
    span.addClass('checkbg');
});
function doVote(){
   var pid="182928";
   var role_id="";
   if(role_id>0){
       var vote= new Array();
       $('.vote_value').each(function(){
            if($(this).hasClass('checkbg')){
                var vote_id=$(this).data('value');
                if(vote_id>0){
                    vote.push(vote_id); 
                }
            }
       });
       if(vote.length>0){
            var url="/index.php?m=Home&c=Ajax&a=vote&fid=1";
              $.ajax({
                type: "POST", 
                url: url, 
                data:{pid:pid,vote:vote}, 
                dataType: 'json',
                beforeSend:function(){
                    blockUI();
                },
                complete:function(){
                    unblockUI();
                },
                success: function callBack(data){
                if(data){
                    if(data.msg){
                        if(data.msg_list){
                            var rowCount = data.msg_list.length;
                            if(rowCount>0){
                                var title=data.msg;
                                var msg_list='';
                                for (var i = 0; i < rowCount; i++) {
                                  msg_list+=data.msg_list[i].msg+"<br />";
                                }
                            }else{
                            var title=js_lang_tipmsg_tit;
                            var msg_list=data.msg;  
                            }
                        }else{
                            var title=js_lang_tipmsg_tit;
                            var msg_list=data.msg;
                        }
                    }
                    if(data.status=='success'){
                         layerMsg(msg_list);
                             setTimeout(function(){
                                 location.reload();
                             },1000);
                    }else{
                        showMsg(msg_list,title);
                    }
                }else{
                        showMsg(js_lang_no_back,js_lang_error);
                }   
            },
                error: function () {  
                    showMsg(js_lang_ajax_error,js_lang_error);  
                }    
            });
       }else{
          showMsg('请选择');
       }
   }else{
       loginCheck();
   }
}
</script> 
