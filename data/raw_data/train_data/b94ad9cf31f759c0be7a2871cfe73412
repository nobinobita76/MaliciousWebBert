  <script language="javascript">
    var reply_submit=0;
    function refreshAdd1(){
        $.getJSON('/home/ajax/getrole/fid/1.html', function(data){
            if(data.status=='success'){
                $('#role_id_b').attr('data-role',data.value.role_id);
                $('#roleInfo').html('<img src="'+data.value.avatar_url+'" alt="" style="width:45px;height: 45px;">');
                $('#roleInfoName').html(data.value.user_name);
            }
        });
        if(reply_submit==1){
            addReply(1);
        }
    }
    function unneedRole1(){
        $('#callModal1').modal('hide');
        addReply(1);
    }
    function needRole1(){
        var content=UE.getEditor('addReplyContent').getContent();
        $.ajax({
            url:'/home/ajax/savereply/fid/1.html',
            data:{content:content},
            dataType:'json',
            method:'post'
        })

        $('#callModal1').modal('hide');
        reply_submit=1;
        showBand1();
    }
    function showBand1(){
        $('#myModal').modal('toggle').load("Array?reply=1");
    }
    function delPosts(pid){
        if(loginCheckOnly()==true){
            layer.confirm('确认删除该帖子吗？', {
                btn: ['是的','再想想'] //按钮
            }, function(index){
                var url=basepath+"index.php?m=Home&c=Ajax&a=delPosts&fid=1&pid="+pid;
                ajaxUrlHtml(url,'',3);
                layer.close(index);
            }, function(index){
                layer.close(index);
            });
        }
    }
    function delReply(rid){
        if(loginCheckOnly()==true){
            layer.confirm('确认删除该回复吗？', {
                btn: ['是的','再想想'] //按钮
            }, function(index){
                var url=basepath+"index.php?m=Home&c=Ajax&a=delReply&fid=1&rid="+rid;
                ajaxUrlHtml(url,'',3);
                layer.close(index);
            }, function(index){
                layer.close(index);
            });
        }
    }
    function addReply(type){
        if(loginCheckOnly()==true){
            var role_id=$('#role_id_b').attr('data-role');
            if(role_id>0){
            }else if(type!=1){
                $('#callModal1').modal('show');
                return false;
            }
            var content=UE.getEditor('addReplyContent').getContent();
            if(content==''||content==null||content==undefined){
                showMsg('请填写内容','提示');
                return false;
            }
            if($('#anonymity').hasClass('checkbg')){
                var anonymity=1;
            }else{
                var anonymity=0;
            }
            var url=basepath+"index.php?m=Home&c=Ajax&a=addReply&fid=1";
            $.ajax({
                type: "POST",
                url: url,
                data: {content:content,pid:pid,anonymity:anonymity},
                dataType: 'json',
                beforeSend:function(){
                    blockUI();
                },
                complete:function(){
                    unblockUI();
                },
                success: function callBack(data){
                    if(data){
                        if(data.msg){
                            if(data.msg_list){
                                var rowCount = data.msg_list.length;
                                if(rowCount>0){
                                    var title=data.msg;
                                    var msg_list='';
                                    for (var i = 0; i < rowCount; i++) {
                                        msg_list+=data.msg_list[i].msg+"<br />";
                                    }
                                }else{
                                    var title=js_lang_tipmsg_tit;
                                    var msg_list=data.msg;
                                }
                            }else{
                                var title=js_lang_tipmsg_tit;
                                var msg_list=data.msg;
                            }
                        }
                        if(data.status=='success'){
                            blockUI();
                            if(data.value.im!=''){
                                iframeUrl(data.value.im);
                            }
                            setTimeout(function(){
                                location.reload();
                            },1000);
                        }else{
                            showMsg(msg_list,title);
                        }
                    }else{
                        showMsg(js_lang_no_back,js_lang_error);
                    }
                },
                error: function () {
                    showMsg(js_lang_ajax_error,js_lang_error);
                }
            });
        }
    }
    if(user_id>0){
        UE.getEditor('addReplyContent',{
            toolbars:[['link','simpleupload','drafts','emotion']],
            maximumWords:5000,
            //关闭字数统计
            wordCount:false,
            //关闭elementPath
            elementPathEnabled:false,
            //默认的编辑区域高度
            initialFrameHeight:100,
            zIndex:18999,
            serverUrl:'/home/ajax/ueditor.html'
        })
        $('#edui25_iframeholder>iframe').contents().find('body').css({"overflowY":"auto"});

    }
    function showMenuTop(cid){
        if(cid>0){
            $('.menu_top').each(function(){
                var mycid=$(this).attr('data-cid');
                if(cid==mycid){
                    if($(this).hasClass('information')){
                        $(this).addClass('information-1');
                    }
                    if($(this).hasClass('Competition')){
                        $(this).addClass('Competition-1');
                    }
                    if($(this).hasClass('data')){
                        $(this).addClass('data-1');
                    }
                    if($(this).hasClass('strategy')){
                        $(this).addClass('strategy-1');
                    }
                    $(this).addClass('menu-bg');
                }else{
                    if($(this).hasClass('information')){
                        $(this).removeClass('information-1');
                    }
                    if($(this).hasClass('Competition')){
                        $(this).removeClass('Competition-1');
                    }
                    if($(this).hasClass('data')){
                        $(this).removeClass('data-1');
                    }
                    if($(this).hasClass('strategy')){
                        $(this).removeClass('strategy-1');
                    }
                    $(this).removeClass('menu-bg');
                }
            })
        }
    }
    function showArtClass(cid){
        //alert(cid);
        if(cid>0){
            $('.art_class_group').each(function(){
                var mycid=$(this).attr('data-cid');
                if(mycid==cid){
                    if($('#menuTop'+cid).length>0){
                        showMenuTop(cid);
                    }
                    $(this).removeClass('hidden');
                    var parent=$(this).attr('data-parent');
                    if(parent>0){
                        showArtClass(parent);
                    }
                }
            })
        }
    }
    function activeArtClass(cid){
        //alert(cid);
        if(cid>0){
            if($('#menuTop'+cid).length>0){
                showArtClass(cid);
            }else{
                $('.cid-link').each(function(){
                    var mycid=$(this).attr('data-cid');
                    if(mycid==cid){
                        $(this).addClass('active');
                        var parent=$(this).attr('data-parent');
                        if(parent>0){
                            activeArtClass(parent);
                        }
                    }
                })
                $('.art_class_group').each(function(){
                    var mycid=$(this).attr('data-cid');
                    if(mycid==cid){
                        if($('#menuTop'+cid).length>0){
                            showMenuTop(cid);
                        }
                        $(this).removeClass('hidden');
                    }
                })
            }
        }
    }
    activeArtClass("66");
</script> 
