    function toDB(iNub)
    {
        if(iNub<10)
        {
            return "0"+iNub;
        }
        return iNub+"";
    }
    // 普通下载
    $(".downlist a").click(function(){
         _hmt.push(['_trackEvent', 'soft', 'down', $softid]);
     });
    var oDate=new Date(); //获取的是电脑的系统时间
    var sMon=toDB(oDate.getMonth()+1);//获取月份0-11
    var sDate=toDB(oDate.getDate());//获取日期 1-31
    var atime=sMon+sDate;
