    // scroll to top
	function scroll2Top(){var s2t=$('#gotopbtn');$(window).scroll(function(){if($(this).scrollTop()>100){s2t.fadeIn()}else{s2t.fadeOut()}});s2t.find('a').click(function(){$('body,html').animate({scrollTop:0},'easeInSine');return false})}

