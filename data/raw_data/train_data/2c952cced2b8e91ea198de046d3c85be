<script type="text/javascript">
    AM.ready("md5",function() {
        $(".add_zan .zanBox ,.ticket_more").live("click",function() {
            var str = "http://suzhou.19lou.com/forum-922-thread-121201324606458652-1-1.html";
            var m5 = $.md5(str);
            var p1 = m5.substr(0,2);
            var p2 = m5.substr(2,2);
            var actionUrl = '//att3.citysbs.com/qrcode/'+p1+'/'+p2+'/'+m5+'.png?data='+encodeURIComponent(str);
            $(".rwm img").attr('src',actionUrl);
            $(".loadAppBox").show();

        });
        $(".closed").live("click",function(event) {
            $('.loadAppBox').hide();
        });

    })
</script>
