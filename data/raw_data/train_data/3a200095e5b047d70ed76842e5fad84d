    function initWidthHeight(){
        var WIDTH = $(window).width();
        var HEIGHT = $(window).height();
        WIDTH = HEIGHT * 2 / 3;
        $('.content-app-desktop').css('width', WIDTH + 'px');
        $('.content-app-desktop').css('height', HEIGHT + 'px');
        $('.desktop-iframe').css('width', WIDTH + 'px');
        $('.desktop-iframe').css('height', HEIGHT + 'px');
    }
    initWidthHeight();
    $(window).resize(function(){
        window.location.reload();
        initWidthHeight();
    });
