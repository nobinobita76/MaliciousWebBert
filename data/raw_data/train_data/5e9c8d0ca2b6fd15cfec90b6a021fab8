        if(lytime=="" || lytime==null){
            alert('请选择日期');

           // $("#lytime").focus();
            return false;
        }

        else if(cyfs=="" || cyfs==null){
            alert('请选择出游方式');

            //$("#account").focus();
            return false;
        }

        else {
            document.bdprice.action = 'http://www.didiyou.com/lyxl/dingdantj.php';
            document.bdprice.submit();
        }

    }
