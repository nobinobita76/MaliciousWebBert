<script>
	//收藏js效果
	var ctlTimer = null;
	document.getElementById('J-clt-top').style.display = 'none';
	document.getElementById('J-clt-btm').onmouseover = function(){
		document.getElementById('J-clt-top').style.display = '';
	}
	document.getElementById('J-clt-top').onmouseover = function(){
		if(ctlTimer){
		clearTimeout(ctlTimer);
		ctlTimer = null;
		}
	}
	document.getElementById('J-clt-top').onmouseout = function(){
		ctlTimer = setTimeout(mouseout,300);
	}
	function mouseout(){
		document.getElementById('J-clt-top').style.display = 'none';
	}
</script>


	
		




<script type="text/javascript">
	document.domain='pconline.com.cn';

	window.$ajustGroup = function(img){
		if(img.width>100 && img.height>75){
			if(img.width/img.height > 100/75)
				img.height = 75;
			else
				img.width = 100;
		}
	}
</script>
