<script>
function initClipboard(text) {
	ZeroClipboard.setMoviePath("http://dp.pconline.com.cn/js/ZeroClipboard.swf");
	var clip = new ZeroClipboard.Client();
	clip.setHandCursor(true); 
	clip.setText(text);
	clip.addEventListener( "complete", function(){  
	    alert("博客地址已被复制到剪切板！");   
	}); 
	clip.glue("btnCopy"); 
	
}

window.onload = function() {	// fix for ie6
	//初始化
	var text = 'qzjs';
	if(text == "") {
		text = 'http://dp.pconline.com.cn/14210360/';
	}else {
		text = 'qzjs.photo.pconline.com.cn';
	}
	initClipboard(text);
}
</script>

	
	
