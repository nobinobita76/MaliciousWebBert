<script type="text/javascript">
(function() {
    var isIE = !!window.attachEvent;

    //菜单
    var navs = chldren($('navBox'));
    for (var i = 0; i < navs.length; i++) {
        if (navs[i].tagName.toLowerCase() == 'a') {
            (function(i) {
                if (navs[i].className == 'select') {
                    if (navs[i - 1] && navs[i - 1].tagName.toLowerCase() == 'span') {
                        if (isIE) {
                            navs[i - 1].style.filter = 'alpha(opacity=0)';
                        } else {
                            navs[i - 1].style.opacity = '0';
                        }
                    }
                    if (navs[i + 1] && navs[i + 1].tagName.toLowerCase() == 'span') {
                        if (isIE) {
                            navs[i + 1].style.filter = 'alpha(opacity=0)';
                        } else {
                            navs[i + 1].style.opacity = '0';
                        }
                    }
                    return;
                }
                ;
                navs[i].onmouseover = function() {
                    if (navs[i - 1] && navs[i - 1].tagName.toLowerCase() == 'span') {
                        if (isIE) {
                            navs[i - 1].style.filter = 'alpha(opacity=0)';
                        } else {
                            navs[i - 1].style.opacity = '0';
                        }
                    }
                    if (navs[i + 1] && navs[i + 1].tagName.toLowerCase() == 'span') {
                        if (isIE) {
                            navs[i + 1].style.filter = 'alpha(opacity=0)';
                        } else {
                            navs[i + 1].style.opacity = '0';
                        }
                    }
                }
                navs[i].onmouseout = function() {
                    if (navs[i - 1] && navs[i - 1].tagName.toLowerCase() == 'span') {
                        if (isIE) {
                            navs[i - 1].style.filter = 'alpha(opacity=100)';
                        } else {
                            navs[i - 1].style.opacity = '1';
                        }
                    }
                    if (navs[i + 1] && navs[i + 1].tagName.toLowerCase() == 'span') {
                        if (isIE) {
                            navs[i + 1].style.filter = 'alpha(opacity=100)';
                        } else {
                            navs[i + 1].style.opacity = '1';
                        }
                    }
                }
            })(i);
        }
    }
    //utils
    function $(id) {
        return document.getElementById(id);
    }

    function chldren(dom) {
        var children = dom.children || dom.childNodes,
                res = [];
        for (var i = 0,j = children.length; i < j; i++)
            if (children[i].nodeType == 1)res.push(children[i]);
        return res;
    }
})();

</script>
	
