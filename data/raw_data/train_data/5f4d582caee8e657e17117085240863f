<script type="text/javascript">
	function fHover(obj){
		if(!obj)return false;
		var objParent = obj.parentNode;
		if(objParent.className == "seclect"){
			return false;
		}
		if(objParent.className == ""){
			objParent.className = "hover"
		}
	}
	function fOut(obj){
		if(!obj)return false;
		var objParent = obj.parentNode;
		if(objParent.className == "seclect"){
			return false;
		}
		if(objParent.className == "hover"){
			objParent.className = ""
		}
	}
</script>

