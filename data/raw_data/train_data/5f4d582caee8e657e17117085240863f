	<script>
        window.bigPicLoaded = function(pic){
			if(pic.height>720)pic.height=720;
			if(pic.width>800)pic.width=800;
            if(pic.offsetWidth>750){
                pic.style.width = '750px';
            }	
			document.getElementById('J-loading').style.display = 'none';
		};

		window.$ajustGroup = function(img){
			if(img.width>100 && img.height>75){
				if(img.width/img.height > 100/75)
					img.height = 75;
				else
					img.width = 100;
			}
		}
	</script>
	
