<script language="JavaScript">
function set_enabled(box) 
{
	frm = box.form;
	ele_cnt = frm.elements.length;
	for(i=0; i<ele_cnt; i++) {
		ele = frm.elements[i];
		if (ele.name == 'opuscom_points[]') {
			//ele.disabled = (box.value=='1')?false:true;
			if(box.value=='1'){
				ele.disabled = false;
			}else{
				ele.disabled = true;
				document.getElementById("gradeContent").innerHTML="";
			}
		}
	}
}

function deleteReply(id, pid){
	if(confirm('确定删除这条评论吗?')){
		document.location = "http://dp.pconline.com.cn/public/action/do_reply.jsp?method=delete&returnUrl="+window.parent.location+"&id="+id+"&pid="+pid;
	}
}

String.prototype.trim = function() {
	return this.replace( /^\s+|\s+$/g, "" );
};

function submitForm(frm){
	var replyContent = document.form1.ubbContent.value;
	if(replyContent == ""){
		alert("评论内容不能为空");
		return false;
	}

	
		if(document.form1.username.value == "" || document.form1.password.value == "") {
			alert("用户名/密码不能为空。");
			return false;
		}
	

	replyContent = (replyContent + "").trim();
	if(replyContent == ""){
		alert("评论内容不能为空");
		document.form1.ubbContent.value = "";
		return false;
	}
	var count = 0;
    if(count >= 5) {
    	var captcha = document.form1.captcha.value;
    	if(captcha == null || "" == captcha) {
    		alert("请输入验证码");
    		return false;
    	}
    }
	
	var gradedContent = document.getElementById("gradeContent").innerHTML;
	document.form1.ubbContent.value = replyContent;
	
	/*
	if(checkForm(frm)){
		return true;
	}else{
		//restore the reply content
		document.form1.ubbContent.value = replyContent;
		return false;
	}
	*/
}

var total = 12;

function setCookie(arg_CookieName,arg_CookieValue,arg_CookieExpireMins){
    var todayDate=new Date();
    todayDate.setMinutes(todayDate.getMinutes() + arg_CookieExpireMins); 
    document.cookie=arg_CookieName+"="+escape(arg_CookieValue)+";path=/;expires="+todayDate.toGMTString()+";";
}

function saveParameter() {
    setCookie('blogPhotoTitle', '小男孩_盛盛_太平洋电脑网摄影博客','1');
}
</script>
