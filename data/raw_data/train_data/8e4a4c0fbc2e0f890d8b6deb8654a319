<script>
jindo.$Fn(function(event) {
	var gnbElement = jindo.$Element(jindo.$("gnb"));

	if (gnbElement == null) {
		return;
	}

	var clickElement = jindo.$Element(event.element);

	if (clickElement == null) {
		return;
	}

	if (clickElement.isChildOf(gnbElement)) {
		return;
	}

	if (gnbAllLayerClose != 'undefined') {
		gnbAllLayerClose();
	}
}).attach(document.body, "click");
</script>


