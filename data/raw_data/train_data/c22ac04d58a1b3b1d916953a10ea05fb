function parseSubData(infos){
    var item = {};
    var info = infos[0];
    item.submovieid = info.subId;
    item.multilang = [];
    item.vCut = 0;
    item.length = info.length;
    item.subStart = 0.0;
    item.subEnd = 0.0;

    var tmp, type = [0,0,0,0,0,0], index;
    item.sids = [0,0,0,0,0,0];
    item.surls = ['','','','','',''];
    item.length_r = [0,0,0,0,0,0];
    item.size = [0,0,0,0,0,0];
    for(var i = 0, l = infos.length; i < l; i++){
        tmp = infos[i];
        if(tmp.byteType >= 6) {
            index = tmp.byteType-2;
        } else{
            index = tmp.byteType-1;
        }
        
        type[index] = 1;
        item.sids[index] = tmp.id;
        item.surls[index] = tmp.previewVodUrl;
        item.length_r[index] = tmp.previewLength;
        item.size[index] = tmp.previewFilesize;
    }
    item.type = type.join('');
    G_SUBMOVIE_DATA.push(item);   
}

var subTypes = {};
for(var i = 0, l = sub_movies.length, item; i < l; i++){
    item = sub_movies[i];
    if(!subTypes[item.chapterTitle]){
        subTypes[item.chapterTitle] = [];
    }
    subTypes[item.chapterTitle].push(item);
};

for(var i in subTypes){
    item = subTypes[i][0];
    G_MOVIE_DATA.subids.push(item.subId);
    G_MOVIE_DATA.subnames.push(item.chapterTitle);
    G_MOVIE_DATA.subtype.push(item.chapterType);
    G_MOVIE_DATA.totalTime.push(item.length);

    parseSubData(subTypes[i]);
}
window.focus();
</script>
