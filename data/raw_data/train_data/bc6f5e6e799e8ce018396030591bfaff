<script type="text/javascript">

//type 1为个人房源viewyz
//经纪人的是0,view

function createLi(a){
    var li=document.createElement("li")
    var img=document.createElement("img");
    img.src= a['pic_url'];
    img.title= a['pic_desc'];
    li.appendChild(img);
    return li;
}
function id(obj){
    return document.getElementById(obj);
}
var flags=false;
</script>
<script>
    /* 自动填充搜索框 && 自动点击第一条结果 && 触发板块 */
J.ready(function() {
    var firstName = "",
        input = J.g("keyword"),
        url = location.search,
        has_prop_id = /prop_id/;
    if( has_prop_id.test(url) ) { // 传入prop_id
        var fc = setInterval(firstClick, 1000 * 0.5);
    } else if ( initParam.area && initParam.block ) { 
        var bc = setInterval(blockClick, 1000 * 0.5);
    } else if ( initParam.area && !initParam.block ) {
        var ac = setInterval(areaClick, 1000* 0.5);
    }
    function firstClick() {
        var liLen = J.g("test_div")[0].children.length;
        if ( liLen > 0 ) {
            firstName = J.g("test_div").first().first().last().first().html()
            input.val(firstName);
            J.g("test_div").first()[0].click();
            clearInterval(fc);
        }
    }
    function areaClick() {
        var areaList = J.g("area_link_list")[0].childNodes,
            areaLen = areaList.length,
            nowArea = J.g("area_link")[0].innerHTML,
            i = 1;
        for ( i =1; i < areaLen; ++i ) {
            if ( areaList[i].innerHTML === nowArea ) {
                areaList[i].click();
                clearInterval(ac);
                break;
            }
        }
    }
    function blockClick() {
        var blockList = J.g("bk_link_list")[0].childNodes,
            blockLen = blockList.length,
            nowBlock = J.g("bk_link")[0].innerHTML,
            i = 1;
        for ( i =1; i < blockLen; ++i ) {
            if ( blockList[i].innerHTML === nowBlock ) {
                blockList[i].click();
                clearInterval(bc);
                break;
            }
        }
    }
});
</script>

















































