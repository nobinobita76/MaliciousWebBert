    (function(w){
        try{
            var fixReferer = '';
            var curReferer = document.referrer;
            var site = 'anjuke';
            var st = new SiteTracker();
            var CustomParams = {};
            st.setSite(site);
            st.setPage("Maptemp_IndexPage");
            st.setPageName("Maptemp_IndexPage");
            st.setReferer(curReferer ? curReferer : fixReferer);
            st.setNGuid("aQQ_ajkguid");
            st.setNUid("ajk_member_id");
            if (typeof SojClientData != 'undefined' && typeof JSON != 'undefined') {
                for (var i in SojClientData) {
                    CustomParams[i] = SojClientData[i];
                }
            }
            st.setCustomParam(JSON.stringify(CustomParams));
            st.track();
            SojClientData = null;
        } catch(err){
            if (typeof SojClientData != 'undefined') {
                for (var j in SojClientData) {
                    CustomParams[i] = SojClientData[i];
                }
            }
            (function(data){
                var ret = [];
                for(var i in data){
                    ret.push('"' + i + '":"' + data[i] + '"');
                }
                ret = '{'+ret.join(",")+"}";
                st.setCustomParam(ret);
            })(CustomParams);
            st.track();
            SojClientData = null;
        }
    })(window);
