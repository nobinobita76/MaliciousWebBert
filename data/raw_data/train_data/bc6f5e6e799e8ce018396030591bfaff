            st.setNUid("ajk_member_id");
            st.buildParams();
            if(!/npv/.test(site)){
                var _trackUrl = st.getParams();
                delete _trackUrl.cp;
                delete _trackUrl.sc;
                window._trackURL = JSON.stringify(_trackUrl);
                // if(console && console.log){
                //     console.log(window._trackURL);
                // }
            }
        }catch(e){

        }
    })();
</script>
