J.ready(function(){
     Event.observe(window, 'resize', function() {
        this.resize();
     });
})
function resize(){
    setTimeout(function() {
        var header_h = $('header').offsetHeight || 42;//header高度

        var tip_h = $('listPager').offsetHeight == 0 ? 40 : $('listPager').offsetHeight; //分页高度
        var list_ot = J.g("listSort").visible()? 120 : 88;//距离header高度

        var h_height = (document.viewport.getHeight() - header_h),
            width = (document.viewport.getWidth() - 300),
            w_width = (width > 468 ? width : 468) + 'px';
        $('overlay').setStyle({
            height: h_height + 'px'
        });
        $('mapMain').setStyle({
            height: h_height + 'px'
        });
        $('jmap_fill').setStyle({
            height: h_height + 'px'
        });

        $('mapLeft').setStyle({
            left: 0,
            height: h_height + 'px'
        });
        $('houseImfor').setStyle({
            height: h_height + 'px'
        });
        J.g('prop_info_list').up(".mapList").setStyle({
            height: h_height-88+"px"
        });
        if(J.g('prop_info_list').length){
            var fix_h = $("listSort").offsetHeight ? 0 : 32;
            var tmp = (document.viewport.getHeight() - header_h - list_ot - tip_h - fix_h);
            J.g('prop_info_list').setStyle({
                height: tmp+"px"
            });
            J.g('prop_info_list').up(".listing").setStyle({
                height: tmp+"px"
            });
        };

    }, 200);
}


