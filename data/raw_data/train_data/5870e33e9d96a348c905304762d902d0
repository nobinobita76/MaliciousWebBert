	<script type="text/javascript" src="/templets/angang/js/jwplayer/jwplayer.js"></script>
<script type="text/javascript">
        // 播放视频
        function playvideo(videoUrl, imageUrl) {
			if(videoUrl){
				jwplayer("video").setup({
					skin: "/templets/angang/js/jwplayer/skins/glow.zip",
					stretching: "fill",
					flashplayer: "/templets/angang/js/jwplayer/player.swf",
					image: imageUrl,
					levels: [
						{file: videoUrl},
						{file: ""} 
					],
					control: false,
					width: 640,
					height: 360,
					autoplay: true
				});
			}
			else{
				alert('很抱歉！该条数据缺失，如需帮助请您联系网站管理员！');	
				return false;
			}
        }
        
        // 设置视频
        function setvideo(videoUrl, imageUrl) { 
            jwplayer("video").setup({
        		//skin: "/templets/angang/js/jwplayer/skins/glow.zip",
        		//stretching: "fill",
        		flashplayer: "/templets/angang/js/jwplayer/player.swf",
        		image: imageUrl,
        		levels: [
        			{file: videoUrl},
        			{file: ""} 
        		],
        		control: false,
        		width: 640,
        		height: 360
        	});   
        }
        // 初始化视频
        setvideo('/uploads/media/130727/xuanchuanpian.flv', '/templets/angang/js/video/chushihua.jpg');
    </script>

