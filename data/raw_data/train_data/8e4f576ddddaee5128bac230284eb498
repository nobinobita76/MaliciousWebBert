<script type="text/javascript" src="http://static1.ciurl.cn/js/jquery.1.3.2.js"></script>
<script language="javascript" type="text/javascript" src="/bbs/js/CJL.0.1.min.js"></script>
<script language="javascript" type="text/javascript" src="/bbs/js/LazyLoad.js"></script>
<script language="javascript" type="text/javascript" src="/bbs/js/lazy.js"></script>
<script language="javascript" type="text/javascript" src="/bbs/js/lazy_func.js"></script>
<script type="text/javascript">
var replyarr=new Array();
var back_url = 'http://m.ci123.com/bbs/post/84410608';
var islogin = 0;
var postid=84410608;
var lastpostid = 84413295;
var order = 0;
var avatar = '';
var gpost_state= '1';
var gpost_reply = 0;
var nickname='';
var lastk = 2;
var hashid = 0;
var faceReply = 0;
var g_platform = 0;
var urlpath = '/bbs/';
var specialid = 0;
var imgload = false;
function loadimg(){
		if(imgload){return false;}
		$("._avatar").each(function(i){
				url=$(this).attr("origin");
				this.src =url;
		});
		imgload=true;
}
$(document).ready(function(){
	loadimg();
	Lazy();
	total.onchange = Lazy;	
	if(hashid){
		var hashstr  = document.getElementById('replyContent');
		hashstr.scrollIntoView();
	}
})
function showReply(id,targetid){
	if(!islogin){
		loginBack(back_url);	
		return false;
	}
	if(gpost_state==0){
		alert('帖子被锁定');
		return false;
	}
	if(replyarr[id]){
		replyarr[id]=0;
		$("#replybox"+id).remove();
		$("#reply"+id+" .rbt").text('回复');
	}else{
		//alert(nick);
		replyarr[id]=1;
		str='<div class=rebox id=replybox'+id+'><form name="form3" method="post" onsubmit="return pubForm2(this);"><textarea name="content"></textarea><input type="hidden" name="rid" value="'+id+'" /><input type="hidden" name="targetid" value="'+targetid+'" /><input type="hidden" name="typeid" value="2"><div class="sub"><input type="submit" name="Submit" value=" 提交 " /></div></form><div>';
		$("#reply"+id).append(str);
		$("#reply"+id+" .rbt").text('回复中');
	}
}
function pubForm2(f){
	if(!islogin){
		loginBack(back_url);
		}
	if(gpost_state==0){
		return false;
	}
	gpost_state = 0;
	var c=f.content.value;
	var rid=f.rid.value;
	var tid = f.targetid.value;
	var type  = f.typeid.value;
	c=c.replace(/[,]/ig,"，");
	if(c.length>2){
		var url="/bbs/sub/pubreply.php";
		f.Submit.value = '提交中...';
		$.post(url,{postid:postid,content:c,targetid:tid,typeid:type},function(data){
			f.content.value='';
			if(data.error){
				if(data.error == '请先登陆'){
					loginBack(back_url);
				}else{
					$("#replybox"+rid).css("color","#ff0000");	
					$("#replybox"+rid).html('*'+data.error);
					window.setTimeout(function(){
						showReply(rid,tid,'');
					},5000);
				}
			}else{
				$("#replybox"+rid).html('回复：'+c);
				$("#reply"+rid+" .rbt").hide();
				$("#replynum").html(data.replynum);
			}
		},'json');		
	}else{
		$("#replybox"+rid).css("color","#ff0000");
		$("#replybox"+rid).html('*内容太少了，亲！');
				window.setTimeout(function(){
			$("#replybox"+rid).css("color","#333");
					$("#replybox"+rid).html('<form name="form3" method="post" onsubmit="return pubForm2(this);"><textarea name="content">'+c+'</textarea><input type="hidden" name="rid" value="'+rid+'" /><input type="hidden" name="targetid" value="'+tid+'" /><input type="hidden" name="typeid" value="2"><div class="sub"><input type="submit" name="Submit" value=" 提交 " /></div></form>');
				},3000);
	}
	gpost_state = 1;
	return false;
}
function replymore(t){
	if(gpost_reply){
		return false;
	}
	gpost_reply=1;
	t.innerHTML='<img src="/bbs/images/loading_16.gif" width="16" height="16" align="absmiddle" /> 正在穿越中...';
	var url='/bbs/sub/show_reply.php';
	$.post(url,{postid:postid,lastpostid:lastpostid,order:order},function(data){
		$('#replylist').append(data.str);
		lastpostid = data.lastpostid;
		if(data.total<1){
			$("#more2").hide();
		}else{
			t.innerHTML='显示更多回帖';
		}
		gpost_reply=0;
		$('#ext').hide();
	}, 'json');
}
function repub(){
	rform = document.getElementById('rform');
	rform.content.scrollIntoView();
	rform.content.focus();
}
function pubForm(f){
	if(!islogin){
		loginBack(back_url);
		return false;
	}
	if(gpost_state==1){
		gpost_state=0;
		var post_id=f.post_id.value;
		var content=f.content.value;
		content=content.replace(/[,]/ig,"，");
		if(content.length<=2){
			f.content.focus();
			gpost_state=1;
			$("#welcome").html("*内容太少了，亲！");
						$("#welcome").css("color","#ff0000");
						window.setTimeout(function(){
				$("#welcome").html("欢迎您，"+nickname+'！');
								$("#welcome").css("color","#333");
						},3000);
		}else{
			var url="/bbs/sub/pubreply.php";
			f.rsub.value = '提交中...';
			$.post(url,{postid:postid,content:content},function(data){
				if(data.error){
					if(data.error=='请先登陆'){
						loginBack(back_url);
					}else{
						$("#welcome").html("*"+data.error);
						$("#welcome").css("color","#ff0000");
						window.setTimeout(function(){
							$("#welcome").html("欢迎您，"+nickname+'！');
								$("#welcome").css("color","#333");
						},5000);
					}
				}else{
					f.content.value='';
					floor = data.replynum+1;
					str='<a name="'+data.postid+'"></a><li id="reply'+floor+'"><dl><dt><table width="100%" border="0" cellspacing="0" cellpadding="0"><tr><td width="46" align="center" rowspan="2"><img class="rhead" src="'+avatar+'" align="absmiddle" width="36"/></td><td height="18"><span>'+data.author+'</span></td></tr> <tr><td style="font-size:14px;padding-top:5px;">【'+data.bstate+'】</td></tr></table></dt><dd>刚刚</dd></dl><div class="rc"><p>'+data.content+'</p></div></li>';
					$("#replylist").append(str);
					$("#replynum").html(data.replynum);
				}
				f.rsub.value = '提交';
				gpost_state = 1;
				specialid = data.postid;
//				window.location.hash = data.postid;
			},'json');
		}
	}
	return false;
}
function moveEnd(obj){
	obj.focus();
	var len = obj.value.length;
	if (document.selection) {
		var sel = obj.createTextRange();
		sel.moveStart('character',len);
		sel.collapse();
		sel.select();
	} else if (typeof obj.selectionStart == 'number' && typeof obj.selectionEnd == 'number') {
		obj.selectionStart = obj.selectionEnd = len;
	}
}
function setFace(v){
	document.rform.content.value+=' /'+v+' ';
	moveEnd(document.rform.content);
}
function loginBack(back){
	document.rform.content.blur();
	window.location.href="http://m.ci123.com/user/login?backurl="+back;
}
function setFaceReply(face,num){
		if(!faceReply){
				faceReply = 1;
				var rform = document.getElementById('rform');
				rform.content.value = '/南南西.'+face;
				pubForm(rform);
				$("#quickReply").html('<tr><td class="leftTit">已回复</td><td class="replyOk"><p>'+face+'</p><img src="'+urlpath+'/images/'+num+'.gif" width="50" height="50"  alt="'+face+'" /></td></tr>');
		}
		return false;
}
function showBig(img){
	var bw=$(document.body).width();
	var bh=$(document.body).height();
	var wh=$(window).height();
	var st=$(document).scrollTop()+44;
	
	$("#showpic").show();
	$("#showpic").css({"height":bh});
	img = img.src;
	var reg = "\/([^_\/]+)_([^\.\"]+)";
	var match = img.match(reg);
	if(!match){
		return false;
	}
	picid = 'pic' + match[1];
	if (img.indexOf('qubaobei') > 0) {
		img = img.replace('_'+match[2],'_mw');
	} else {
		img = img.replace('_'+match[2],'');
	}
	imgW = bw>600 ? 600 : bw;
	if(!$("#"+picid).length>0){
		$("#bigpic").append('<div id="'+picid+'" style="display:none;"><img width="'+imgW+'" src="'+img+'" onclick="hideBig();" /></div>');
	}
	$("#"+picid).css({"position":'absolute',"top":st+'px',"z-index":333,"display":'block'});
}
function hideBig(){
		$("#showpic").hide();
		$("#"+picid).hide();
}

function appdlBlink() {
	var c = new Array('rgb(255, 20, 147)','rgb(0, 100, 0)','rgb(255, 165, 0)','rgb(0, 0, 128)');
	var o = document.getElementById('appdl');
	var n = 0;
	for (var i in c) {
		n++;
		if (o.style.color == c[i]) {
			break;
		}
	}
	if (n > c.length - 1) {
		n = 0;
	}
	o.style.color = c[n];
	setTimeout(function(){appdlBlink();},300);
}

function setOrder(n) {
	if (n == 0) {
		$('#order0').addClass('tloff');
		$('#order0').unbind('click');
		$('#order1').removeClass('tloff');
		$('#order1').unbind('click').click(function(){
			changeOrder(1);
		});
	} else {
		$('#order0').removeClass('tloff');
		$('#order0').unbind('click').click(function(){
			changeOrder(0);
		});;
		$('#order1').addClass('tloff');
		$('#order1').unbind('click');
	}
	$('#ordermenu').show();
}

var ordercount = 0;
function changeOrder(n) {
	if (n == order) {
		return ;
	}
	ordercount++;
	setOrder(n);
	order = n;
	$('#ext').show();
	if (n == 0) {
		$('#replylist').html('');
		lastpostid = postid;
		$('#more2').show();
		replymore(document.getElementById('more2'));
	} else {
		$('#replylist').html('');
		lastpostid = 999999999;
		$('#more2').show();
		replymore(document.getElementById('more2'));
	}
//	$.post('/tmp/orderlog.php', {postid:postid,order:order,platform:0,count:ordercount},function(data){});
}

$(document).ready(function(){
	setOrder(order);
	appdlBlink();
});
</script>
