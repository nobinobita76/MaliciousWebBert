<script> 

function collection(id){
	if (checkLogin()){
		var jssrc="http://www.17k.com/main/reader/mbs.do?method=save&jsonp=jsonpCallback&bookId="+id;
	    if (!document.body){
			 document.write("<script type=\"text/javascript\"  src=\'"+jssrc+"\' ><\/script>");
		 }else{
		  var s = document.createElement("script");
		  	  s.charset = "utf-8";
		  	  s.language = "javascript"; 
			  s.type = "text/javascript";
		      s.src = jssrc;
		   	  document.body.appendChild(s);
		 }
	}else{
		var url=document.location.href; 		
		location.href="http://passport.17k.com/login.jsp?url="+url;
	}
}
function jsonpCallback(data) {
	if (data.status==0) alert("收藏成功！");
	if (data.status==1) alert("收藏失败。");
	if (data.status==2) alert("书签加入成功！");
	if (data.status==3) alert("本书已经收藏过了！");
}

function checkLogin(){
	var strCookie=document.cookie;
	var arrCookie=strCookie.split("; ");
	for(var i=0;i<arrCookie.length;i++){
	    var arr=arrCookie[i].split("=");
	    if("sso.ssoId" == arr[0]){
	    	return true;
	    }
	}
	return false;
}

