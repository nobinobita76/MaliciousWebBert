	this.get_slidenum=function(flag,url) {
		if(flag==0)
		{
			if(url.indexOf('\?')>=0)
			   url=url.substring(0,url.indexOf('\?'));
			if(url==window.location.pathname)
			{
			   return true;
			}
		}
		else if(flag==1)
		{
			url_para=window.location.search;
			//alert(url_para.substr(url_para.indexOf('t2')).substring(0,url_para.substr(url_para.indexOf('t2')).indexOf('&')));
			//console.log(url.substr(url.indexOf('t2')).substring(0,url.substr(url.indexOf('t2')).indexOf('&')));
			
			var urltop2=swiper_slide_content_split[2];
			//console.log(urltop2.split("&").length-1);
			//console.log(urltop2);
			if(urltop2.split("&").length-1==0){
				if(urltop2==url_para.substr(url_para.indexOf(urltop2)).substring(0,url_para.substr(url_para.indexOf(urltop2)).indexOf('&'))){
					return true;
				}
			}		
			if(urltop2.split("&").length-1==1){
				urltop2arr=urltop2.split("&");
				if(urltop2arr[0]==url_para.substr(url_para.indexOf(urltop2arr[0])).substring(0,url_para.substr(url_para.indexOf(urltop2arr[0])).indexOf('&')) && urltop2arr[1]==url_para.substr(url_para.indexOf(urltop2arr[1])).substring(0,url_para.substr(url_para.indexOf(urltop2arr[1])).indexOf('&'))){
					return true;
				}
			}
		}
		else if(flag==2)
		{
			var url=window.location.href;
			//console.log(url.substring(url.indexOf('mini'),url.lastIndexOf('&')));
			//console.log(swiper_slide_content_split[1].lastIndexOf('&'));
			if(swiper_slide_content_split[1].indexOf('&')>0){
				if(swiper_slide_content_split[1].substring(1,swiper_slide_content_split[1].indexOf('&'))==url.substring(url.indexOf('mini'),url.indexOf('&'))){
					return true;
				}
			}
			if(swiper_slide_content_split[1].indexOf('&')=="-1"){
				if(swiper_slide_content_split[1].substring(0,swiper_slide_content_split[1].indexOf('?'))==window.location.pathname){
					return true;
				}
			}
			
		}
		else if(flag==3)
		{
			var url=window.location.pathname;
			var url_search=window.location.search;
			//console.log(url_search.substring(0,url_search.indexOf('&')));
			
			var pathstr=url.substr(1)+url_search.substring(0,url_search.indexOf('&'));
			if(swiper_slide_content_split[2]){
				var document_arr=swiper_slide_content_split[2].split('&');
				//console.log(document_arr);
				if(swiper_slide_content_split[2].indexOf('=')>0){
					//console.log(swiper_slide_content_split[2].indexOf('='));
					for(var i=0;i<=document_arr.length;i++){
						console.log(document_arr[i]);
						if(document_arr[i]==pathstr){
							return true;
						}
					}
				}else{
					for(var i=0;i<=document_arr.length;i++){
						//console.log(url.substr(1));
						if(document_arr[i]==url.substr(1)){
							return true;
						}
					}
				}
				
				
			}
			if(!swiper_slide_content_split[2]){
				var url_href=window.location.href;
				if(swiper_slide_content_split[1].indexOf('&')>0){
					if(swiper_slide_content_split[1].substring(1,swiper_slide_content_split[1].indexOf('&'))==url_href.substring(url_href.indexOf('mini'),url_href.indexOf('&'))){
						return true;
					}
				}
				if(swiper_slide_content_split[1].indexOf('&')=="-1"){
					//console.log(swiper_slide_content_split[1].substring(0,swiper_slide_content_split[1].indexOf('?')));
					//console.log(window.location.pathname);
					if(swiper_slide_content_split[1].substring(0,swiper_slide_content_split[1].indexOf('?'))==window.location.pathname){
						return true;
					}
				}
			}
			
			//console.log(url.substr(url.lastIndexOf("\/")+1));
		}
		else if(flag==4){
			//console.log(window.location.href);
			//console.log(swiper_slide_content_split[2]);
			if(swiper_slide_content_split[2]){
				var document_arr=swiper_slide_content_split[2].split('&&');
				for(var i=0;i<=document_arr.length;i++){
					//console.log(document_arr[i]);
					if(window.location.href.indexOf(document_arr[i])>0){
						return true;
					}
				}
			}
		}
		return false;
	}
	$.swiper_slide.default={
		swipe_class : "",
		is_swiper_more:false,
		initialSlide:0,
		flag:0,
	}

})(jQuery);
</script>

	
