<script>
$(function(){
	$(".swiper-more_Top").on("click",function(e){
        var val = $(this).attr('class');
        if(val.indexOf('on') == -1){			
		    $("body").bind("touchmove",function(event){
				event.preventDefault();
			});
            $(this).addClass('on');
            $(".menu").show(0).stop().animate({
                height:'100%'
            },0);

			//document.addEventListener('touchmove', bodyScroll(e), false);
        }else{
			$("body").unbind("touchmove");
            $(this).removeClass('on');
            $(".menu").stop().animate({
                height:'0'
            },0);
			
        }
		
    })
	
	$(".menu a").on("click",function(){
        $(this).parents(".menu").hide();
        $("#header span.right").removeClass('on');
		$("body").unbind("touchmove");
    })
});
</script>
