<script language="javascript">

function shoppercheck(){
 var str
   	login_cookie_sendurl("/common/set_cookie.php");
	 var form=document.form1;
 
 form.username.value=form.username.value.replace(/\s/ig,"");
 form.pwd_.value=form.pwd_.value.replace(/\s/ig,"");
 form.next_pwd_.value=form.next_pwd_.value.replace(/\s/ig,"");
 
 
 if (form.username.value=='')
  {
     alert('店主登录名未填')
	 form.username.focus()
	 return false
   }
   for(i=0;i<form.username.value.length;i++){
     str=form.username.value.charAt(i)
      if (str=='(' || str==')' || str=='=' || str=='/' || str=='~'|| str=='%'|| str=='^' || str=='!'|| str=='+'|| str=='?'|| str=='"'|| str==':'|| str==';'|| str==','|| str=='['|| str==']'|| str=='{'|| str=='}'|| str=='&'|| str=='#'|| str=='*'|| str=='<'|| str=='>'){
     alert ('请不要输入非法字符！不合法的店主登录名！');
	 form.username.focus()
     return false
     }
   }
   
   if (form.pwd_.value=='')
  {
     alert('店主第一层密码未填')
	 form.pwd_.focus()
	 return false
   }
      for(i=0;i<form.pwd_.value.length;i++){
     str=form.pwd_.value.charAt(i)
     if (str=='(' || str==')' || str=='=' || str=='/' || str=='~'|| str=='%'|| str=='^' || str=='!'|| str=='+'|| str=='?'|| str=='"'|| str==':'|| str==';'|| str==','|| str=='['|| str==']'|| str=='{'|| str=='}'|| str=='&'|| str=='#'|| str=='*'|| str=='<'|| str=='>'){
     alert ('请不要输入非法字符！不合法的第一层密码！');
	  form.pwd_.focus()
     return false
     }
   }
   if (form.next_pwd_.value=='')
  {
     alert('店主第二层密码未填')
	 form.next_pwd_.focus()
	 return false
   }
      for(i=0;i<form.next_pwd_.value.length;i++){
     str=form.next_pwd_.value.charAt(i)
    if (str=='(' || str==')' || str=='=' || str=='/' || str=='~'|| str=='%'|| str=='^' || str=='!'|| str=='+'|| str=='?'|| str=='"'|| str==':'|| str==';'|| str==','|| str=='['|| str==']'|| str=='{'|| str=='}'|| str=='&'|| str=='#'|| str=='*'|| str=='<'|| str=='>'){
     alert ('请不要输入非法字符！不合法的第二层密码');
	 form.next_pwd_.focus()
     return false
     }
   }

     
   	form.pwd.value=calcMD5(form.pwd_.value.toLowerCase());
	form.next_pwd.value=calcMD5(form.next_pwd_.value.toLowerCase());
	   set_pwd5(form,form.username.value,1);
        
} 


