<script type="text/javascript">
    $(function () {
        $(".tit_down").click(function () {
            if ($(this).hasClass("tit_up")) {
                $(this).removeClass('tit_up');
                $(this).parent().next('.zksq_box').hide();
                var nextHtml = $(this).parent().next('.zksq_box').next();
                if (nextHtml.hasClass("more")) {
                    nextHtml.hide();
                }
            } else {
                $(this).addClass('tit_up');
                $(this).parent().next('.zksq_box').show();
                var nextHtml = $(this).parent().next('.zksq_box').next();
                if (nextHtml.hasClass("more")) {
                    nextHtml.show();
                }
            }
        });

        var newProW = $('.newprolistbox li').width();
        $('.newprolistbox li .intro').find('span').find('a').css("height", newProW);
        $('.newprolistbox').next('.more').addClass('.newMore');
    });
</script>

