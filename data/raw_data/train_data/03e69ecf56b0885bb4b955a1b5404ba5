            function PhoneClickRecord(sourceType, sourceID, PhoneType, mobile, aID) {
                //sourceType 页面来源 1 商铺，2 pd 3 cd
                var ClickedUrl = window.location;
                var ReferUrl = "";
                if (document.referrer != null) { ReferUrl = document.referrer; }
                $.ajax({ url: "/reg/DoPhoneClickRecord/", async: false, data: { sourceType: sourceType, sourceID: sourceID, PhoneType: PhoneType, ClickedUrl: encodeURIComponent(ClickedUrl), mobile: encodeURIComponent(mobile), aID: aID, ReferUrl: encodeURIComponent(ReferUrl) } });
