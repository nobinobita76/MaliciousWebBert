    <script>
        var isLogin = 0; //是否登录
        var region = 0; //用户权限  0:学生 1：教师   2：学校    3:学校以上权限
        var gradepart = 0; //用户年级段    0：幼儿段   1：小学段   2：初中段   3：高中段     -1:无段
        var isshiyanqu = 2; //是否是实验区 1:实验区，2：非实验区
        var edition = -1; //版本 1：家庭版  2：学校版
        var AreaID = 0;
        var AreaType = 1;
        var comefrom = 0;
        var comefromcss = "http://file.safetree.com.cn/special/90/push.css"; //微信单独样式
        $(function () {
            WebServes.checkUserLoginNoLayer(function (json) {
                if (json != null && json.userid > -1) {//已登录
                    if (json.prvid == 12) { //福建跳回自己的专题
                        location.href = "http://xiaoche2016.safetree.com.cn";
                    }
                    isLogin = 1;
                    comefrom = json.comefrom; //站点编号
                    var RegisterUrl = getCookie("ServerSide");
                    if (RegisterUrl == "-1" || RegisterUrl == null) { //非实验区
                        isshiyanqu = 2;
                        region = json.region;
                        if (region == 10) {
                            $("#work1").html("<img src='http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_nwc.png'>");
                        } else {
                            $("#work2").html("<img src='http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_nwc.png'>");
                        }
                    } else {  //实验区
                        isshiyanqu = 1;
                        if (json.region > 0) {
                            location.href = "2016FirstClass_Execution_school.html";
                        } else {
                            location.href = "2016FirstClass_Execution_family.html";
                        }
                    }
                    GetWorkStatus(); //完成状态 
                }
            });
            weixin(comefrom, function (a, b, c) {
                comefrom = a, cityname = b, ipArea = c;
                $("#weixin_cityname").append("教育百师通" + cityname);
                if (comefrom >= 0) {
                    $("#wxbox-img1").attr("src", 'http://file.safetree.com.cn/angel/push/pushweixin.html?location=100007&siteid=' + comefrom + '&test=1&css=' + comefromcss).attr("onload", "javascript:getTitle();");
                    $("#wxbox-img2").attr("src", 'http://file.safetree.com.cn/angel/push/pushweixin.html?location=100007&siteid=' + comefrom + '&test=1&css=' + comefromcss).attr("onload", "javascript:getTitle();");
                    $("#wxbox-img4").attr("src", 'http://file.safetree.com.cn/angel/push/pushweixin.html?location=100007&siteid=' + comefrom + '&test=1&css=' + comefromcss).attr("onload", "javascript:getTitle();");
                }
            });
            PageData(1, 16, AreaID, AreaType);
        });

        //表格拼接数据
        function PageData(nowPage, pageSize, areaId, areaType) {
            var key = $.trim($("#searchkey").val());
            $("#AreaId").val(areaId);
            $("#AreaType").val(areaType);
            $.getJSON(Asmx + "/PageData?jsoncallback=?", {
                r: Math.random(),
                nowPage: nowPage,
                pageSize: pageSize,
                areaId: areaId,
                areaType: areaType,
                viewType: 1,
                SearchTxt: key
            }, function (json) {
                if (json) {
                    //拼接地区导航及表头--开始
                    var Html = '<div class="topa"><p>';
                    var TabTitle = json.AreaInfos;

                    for (var i = 0; i < TabTitle.length; i++) {
                        var strFunc = 'PageData(1, 16, ' + TabTitle[i].AreaID + ', ' + TabTitle[i].AreaType + ')';
                        if (i != TabTitle.length - 1) {
                            Html += TabTitle[i].AreaName + '&gt;';
                        } else {
                            Html += TabTitle[i].AreaName + '</p><span>';
                            Html += '<a href="javascript:void(0)" id="Down">下载此表</a></span>';
                        }
                    }
                    var msg = key == "" ? "请输入省份关键字" : key;
                    if (areaType == 2) {
                        msg = key == "" ? "请输入城市关键字" : key;
                    } else if (areaType == 3) {
                        msg = key == "" ? "请输入区县关键字" : key;
                    } else if (areaType == 4) {
                        msg = key == "" ? "请输入学校关键字" : key;
                    } else if (areaType == 5 || areaType == 6) {
                        msg = key == "" ? "请输入班级关键字" : key;
                    }
                    Html += '<div class="search-bd"><input type="text" placeholder="' + msg + '" id="searchkey" value="' + key + '"><a href="javascript:' + strFunc + ';">搜索</a></div>';
                    Html += '</div>';
                    //拼接地区导航及表头--结束
                    //拼接表格内容--开始
                    var TabContent = json.PageData;
                    Html += '<table  cellspacing="0" cellpadding="0">';
                    Html += '<tr> <th>地区</th> <th width="22%"> 已完成学校数 </th> <th width="22%">已完成班级数</th><th width="22%">已完成家庭数</th></tr>';
                    if (TabContent.length > 0) {
                        for (var j = 0; j < TabContent.length; j++) {
                            var trclass = "";
                            if (TabContent[j].ObjName == "累计") {
                                trclass = ' class="last-td"';
                            }
                            var Func = "javascript:void(0)";
                            if (areaType < 2 && TabContent[j].ObjID != 0) {
                                Func = 'javascript:PageData(1, 16, ' + TabContent[j].ObjID + ', ' + TabContent[j].AreaType + ',' + edition + ')';
                            }
                            Html += "<tr>";
                            if (areaType < 2) {
                                Html += '<tr ' + trclass + '><td width="22%"><a href="' + Func + '">' + TabContent[j].ObjName + '</a></td><td width="22%">' + TabContent[j].FinishSchoolCnt + '</td><td width="22%">' + TabContent[j].FinishClassRoomCnt + '</td><td width="22%">' + TabContent[j].FinishFamilyCnt + '</td></tr>';
                            } else {
                                Html += '<tr ' + trclass + '><td width="22%">' + TabContent[j].ObjName + '</td><td width="22%">' + TabContent[j].FinishSchoolCnt + '</td><td width="22%">' + TabContent[j].FinishClassRoomCnt + '</td><td width="22%">' + TabContent[j].FinishFamilyCnt + '</td></tr>';
                                Html += '</tr>';
                            }
                        }
                        if (json.sumCount >= 0) {
                            Html += "<tr>";
                        }
                        Html += '</table>';
                        //拼接表格内容--结束

                        //拼接分页
                        var PageStr = json.PageHtml;
                        Html += PageStr;
                    } else {
                        Html += "<tr><td colspan='4' align='center'>未搜索到您要查询的内容...</td></tr>";
                    }
                    $("#TabBox").html(Html);

                    //设置下载
                    $("#Down").attr("onclick", "DownShow(" + areaId.toString() + "," + areaType.toString() + ")");
                }
            });
        }
        //输入关键字 回车
        $("body").keydown(function (e) {
            if (e.keyCode == 13) {
                if ($.trim($("#searchkey").val()) == "") {
                    $("#searchkey").focus();
                    PageData(1, 16, $("#AreaId").val(), $("#AreaType").val());
                } else {
                    PageData(1, 16, $("#AreaId").val(), $("#AreaType").val());
                }
            }
        })

        //获取工作完成状态
        function GetWorkStatus() {
            if (region == 0) {    //学生
                if (getCookie(getCookie("UserID") + "_2016SafeCar") == null || getCookie(getCookie("UserID") + "_2016SafeCar") == "") {
                    $.getJSON(Asmx + "/GetFinishStatus?jsoncallback=?", { r: Math.random() }, function (res) {  //调用完成状态接口
                        if (res != null) {
                            if (res.StepOne == "True") {
                                $("#work2").html("<img src='http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_wc.png'>");
                                var zkcookName1 = getCookie("UserID") + "_2016SafeCar";
                                setCookie(zkcookName1, 1, 365);
                            } else {
                                if (res.StepOne == "True") {
                                    var zkcookName1 = getCookie("UserID") + "_2016SafeCar";
                                    setCookie(zkcookName1, 1, 365);
                                }
                                $("#work2").html("<img src='http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_nwc.png'>");
                            }
                        }
                    });
                } else {
                    $("#work2").html("<img src='http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_wc.png'>");
                }
            } else {
                if (region == 10) {
                    if (getCookie(getCookie("UserID") + "_2016SafeCar") != null && getCookie(getCookie("UserID") + "_2016SafeCar") != "") {
                        $("#work1").html("<img src='http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_wc.png'>");
                    } else {
                        $.getJSON(Asmx + "/GetFinishStatus?jsoncallback=?", { r: Math.random() }, function (res) {
                            if (res != null) {
                                if (res.StepOne == "True") {
                                    var zkcookName1 = getCookie("UserID") + "_2016SafeCar";
                                    setCookie(zkcookName1, 1, 365);
                                    $("#work1").html("<img src='http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_wc.png'>");
                                } else {
                                    $("#work1").html("<img src='http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_nwc.png'>");
                                }
                            }
                            else {
                                $("#work1").html("<img src='http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_nwc.png'>");
                            }
                        });
                    }
                }
            }

        }

        //下载提示
        function DownShow(areaId, areaType) {
            $("#AreaId").val(areaId);
            $("#AreaType").val(areaType);
            $("#down-tc").show();
        }

        //下载表格
        function DownExcel(areaId, areaType, edition) {
            $("#down-tc").hide();
            var areaId = $("#AreaId").val();
            var areaType = $("#AreaType").val();
            var key = $.trim($("#searchkey").val());
            $.getJSON(Asmx + "/DownData?jsoncallback=?", {
                r: Math.random(),
                areaId: areaId,
                areaType: areaType,
                viewType: 1,
                SearchTxt: key,
                Eplat: 0
            }, function (json) {
                if (json) {
                    if (json.Status == 'True') {
                        location.href = json.Msg;
                    } else {
                        alert("无数据无法进行下载");
                    }
                } else {
                    alert("无数据无法进行下载");
                }
            });
        }
    </script>

   


</body></html>