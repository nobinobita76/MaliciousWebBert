<script type="text/javascript">
    $z.getServerCookie('http://passport.zcool.com.cn',
            function(data) {
                if (data.passportServerEc != null && data.passportServerEc.length > 0) {
                    $.ajax({
                        type: "GET",
                        url: "http://m.hellorf.com/user/ssopassport/",
                        data: {
                            TOKEN: data.passportServerEc,
                            _mt: Math.random()
                        },
                        //async: false,
                        success: function (data) {
                            if(data.status=='1')
                            {
                                window.location.reload();
                            }

                        }
                    });
                }
            }
    );
</script><script type="text/javascript">
