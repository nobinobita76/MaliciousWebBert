<script>
    var baseDetailUrl  = "http://s.zol.com.cn";    
    var merchantId     = '191827';    
    var businessname     = '191827';
    //是否自动弹出生意宝
    var autoAlertDove   = 0;
    $(function($){
        // 友情链接
        $.extend({
           getMerchantLink : function(options){
                var defaults = {    
                   merchantId  : 0                                  
                };                         
                var options        = $.extend(defaults, options);            
                var merchantId     = defaults.merchantId;                       
                var url            = baseDetailUrl+"/index.php?c=Ajax_DetailLeft&a=GetMerchantLink&callback=?&t="+Math.random();            
                if (merchantId){
                    $.getJSON(
                            url,
                            {merchantId:merchantId},
                            function(data){
                                var str = '';
                                if (data.flag){
                                    for (key in data.info){
                                        str += '<li><a href="'+data.info[key].url+'" title="'+data.info[key].title+'" target="_blank">'+data.info[key].title+'</a></li>';
                                    }
                                }
                                if(!data.flag || str == ''){
                                    str = '<div class="no-prompt">暂无友情链接</div>';
                                }
                                $(".links").html(str);
                            }
                    );
                }
           } 
        });    
        
        // 获取店铺统计信息        
        $.extend({
           getMerchantStat : function(options){
                var defaults = {    
                   merchantId  : 0                                  
                };                         
                var options        = $.extend(defaults, options);            
                var merchantId     = defaults.merchantId;                       
                var url            = baseDetailUrl+"/index.php?c=Ajax_MerchantStore_MerchantStat&a=MerchantStat&callback=?&t="+Math.random();            
                if (merchantId){
                    $.getJSON(
                            url,
                            {merchantId:merchantId},
                            function(data){
                                var str = '';                                                 
                                if (data.flag){
                                    str += '<li>商品总数：<em>' + data.goodsCount + '</em></li>';
                                    str += '<li>留言总数：<em>' + data.goodsMessageCount + '</em></li>';
                                    str += '<li>店铺在线成交量：<em>' + data.orderCount + '</em></li>';
                                 
                                }
                                if (str == ''){
                                    str = '<div class="no-prompt">暂无店铺统计</div>';
                                }
                                $(".store-stat").html(str);
                            }
                    );
                }
           } 
        });  
        
        // 分页
        $.extend({
            zpPaging : function(options){
                var defaults = {    
                    page        : 1,                   
                    size        : 5,
                    totalNumber : 0,    
                    merchantId     : 0,
                    pageFun     : '',
                    type        :10
                };                       
                var options      = $.extend(defaults, options);   
                var merchantId     = defaults.merchantId;            
                var page        = defaults.page;            
                var size        = defaults.size;
                var totalNumber = defaults.totalNumber;
                var pageFun     = defaults.pageFun;
                var totalPage   = Math.ceil(totalNumber/size);
                var type        = defaults.type;
                if (page < 0){
                    page = 1;
                }
                if (page > totalPage){
                    page = totalPage;
                }

                var str         = '<div class="page">';
                    if (1 == totalPage){
                        str     = '<div class="page"><span class="cur">1</span></div>';
                    }else {
                        if (page > 1){
                            var prevPage = (page - 1);                                                                                                                                               
                            str += '<a href="javascript:void(0)" class="prev" onclick="$.'+pageFun+'({merchantId:'+merchantId+', page:'+prevPage+', type:'+type+'})">上一页</a>';
                        }else{
                            str += '<span class="no-prev">上一页</span>';
                        }

                        if (totalPage <= 5){
                            for (var i = 1; i<= totalPage; i++){
                                if (page == i){
                                    str += '<span class="cur">'+i+'</span>';
                                }else{                                
                                    str += '<a href="javascript:void(0)" onclick="$.'+pageFun+'({merchantId:' + merchantId + ', page:'+ i +', type:'+type+'})">'+i+'</a>';
                                }                            
                            }
                        }else{
                            if (page <= 5){
                                for (var i = 1; i<= 5; i++){
                                    if (page == i){
                                        str += '<span class="cur">'+i+'</span>';
                                    }else{                                    
                                        str += '<a href="javascript:void(0)" onclick="$.'+pageFun+'({merchantId:' + merchantId + ', page:'+ i +', type:'+type+'})">'+i+'</a>';
                                    }                            
                                }                            
                                str += '...';                            
                            }else{
                                str += '...';
                                var start = page - 2;
                                var end   = page + 3;
                                if (end >= totalPage){
                                    end = totalPage;
                                }
                                for (var i = start; i <= end; i++){
                                    if (page == i){
                                        str += '<span class="cur">'+i+'</span>';
                                    }else{                                    
                                        str += '<a href="javascript:void(0)" onclick="$.'+pageFun+'({merchantId:' + merchantId + ', page:'+ i +', type:'+type+'})">'+i+'</a>';
                                    }                              
                                }
                                if(page != totalPage){
                                    str += '...';
                                }
                            }
                        }

                        if (page == totalPage){
                            str += '<span class="no-next">下一页</span>';
                        }else{
                            var nextPage = (page + 1);                              
                            str += '<a href="javascript:void(0)" class="next" onclick="$.'+pageFun+'({merchantId:'+merchantId+', page:'+nextPage+', type:'+type+'})">下一页</a>';
                        }                    
                    }
                    str += '</div>';                            
                return str;
            }
        });  
    });    
    
    $(function(){
        // 隐藏显示顶部经销商信息
        $(".shop-infor").mouseover(function(){
           $(this).addClass("shop-infor-hover");  
        }).mouseleave(function(){
           $(this).removeClass("shop-infor-hover"); 
        });        
        
        // 搜索切换
        $(".findmod").mousemove(function(){
            $(this).addClass("findmod-cur");    
            $("#zp-search-info").show();
        }).mouseleave(function(){
            $("#zp-search-info").hide();
            $(this).removeClass("findmod-cur");
        });    

        $("#zp-search-info > li").click(function(){
            var rel = $(this).attr("rel");
            if (1 == rel){
                $("#search-type-content").html("本店");
            }else{
                $("#search-type-content").html("全站");
                $("#searchAction").val("List");
            }       
            $("#zp-search-info").hide();
            $("input[name='search-type']").val(rel);                
        });    
        
        //提交搜索
        $("input[name=do_search]").click(function(){
            var rel     = $("input[name='search-type']").val();
            if(rel == 1){
                var keyWord         = $("input[name='keyword']").val();
                var merchantId      = $("input[name='merchantId']").val();
                var locationUrl     = "http://s.zol.com.cn/shop_"+merchantId+"/hot.html?keyword="+keyWord;
//                var locationUrl     = encodeURI(locationUrl);
                window.location    = locationUrl;
                return false;
            }
        });
        
        // 自定义页面展开
        $("#zs-merchant-self-page").mouseover(function(){
           $(this).addClass("nav-more-hover"); 
        }).mouseleave(function(){
           $(this).removeClass("nav-more-hover"); 
        });
        
        
        // 热卖热销排行切换
        $(".rankingtab > li").mouseover(function(){
            $(this).addClass("current").siblings().removeClass("current");
            var rel = $(this).attr("rel");                        
            if (1 == rel){
                $("#zs-merchant-goods-sell").show();
                $("#zs-merchant-goods-top").hide();
            }else{
                $("#zs-merchant-goods-sell").hide();
                $("#zs-merchant-goods-top").show();                
            }
        })
        // 友情链接
        $.getMerchantLink({merchantId:merchantId});        
        
        // 获取店铺统计信息
        $.getMerchantStat({merchantId:merchantId});           
    })
</script>
