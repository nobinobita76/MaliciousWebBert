    <script>
        // 导航中的下拉
        if($(".sub-nav").length){
            $(".sub-nav").mouseover(function(){
                $(this).addClass("sub-nav-hover");
            }).mouseout(function(){
                $(this).removeClass("sub-nav-hover");
            });
        } 
        //普通版发布店铺
        $("#publish-btn-node-id").click(function(){
            if(confirm("确定使用普通版店铺?")){
                var url     = "/index.php?c=Ajax_MerchantStore_ZplusBaseSet&a=SetTemplateMoudle";
                $.getJSON(url,{'tid':1},function(backdata){
                    if(backdata.flag){
                        window.location    = "/index.php?c=MerchantStore_Default";
                    }else{
                        alert(backdata.msg);
                    }
                });
            }
        });
    </script>
