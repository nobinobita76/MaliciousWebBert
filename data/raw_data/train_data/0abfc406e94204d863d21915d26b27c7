<script>    
var merchantId     = '191827';
$(function($){
      //  ajax 获取评论信息      
       $.extend({
            getCommentInfo : function(options){
                var defaults = {    
                   merchantId  : 0,
                   page : 1
                };                                  
                var options        = $.extend(defaults, options);            
                var merchantId  = defaults.merchantId;           
                var page             =  defaults.page;           
                var commentType    = $(":input[name=wordsKey][checked]").val();         
                baseDetailUrl = "";
                var url            = baseDetailUrl+"/index.php?c=Ajax_MerchantStore_MerchantComment&a=GetMerchantComment&callback=?&t="+Math.random();                            
                if (merchantId){                    
                    $.getJSON(
                        url,
                        {merchantId:merchantId, commentType:commentType, page:page},
                        function(data){
                            var str             = '<div class="no-prompt">暂无咨询记录，有问题可以在下面留言给商家</div>';
                            if (parseInt(data.commentNumber)){                                
                                str = "<ul>";
                                var commentInfo = data.commentInfo;
                                for ( key in commentInfo){
                                    str += '<li>';
                                    str += '<div class="question">'+commentInfo[key].username+'：'+commentInfo[key].content+'<span class="time">'+commentInfo[key].inputDate+'</span></div>';
                                    if (commentInfo[key].isReply){
                                        str += '<div class="answer">商家回复：'+commentInfo[key].replayContent+'<span class="time">（'+commentInfo[key].replayDate+'）</span></div>';
                                    }                                    
                                    str += '</li>';
                                }
                                str += "</ul>";
                                var params = {page:page, size:10, totalNumber:data.commentNumber, merchantId:merchantId, pageFun:'getCommentInfo'};                        
                                str += $.zpPaging(params);   
                            }
                            $(".message-list").html(str);
                            $("#zs-merchant-comment-number").html(data.commentNumber);
                        }
                    );
                }else{
                    var str             = '<div class="no-prompt">暂无咨询记录，有问题可以在下面留言给商家</div>';
                    $(".message-list").html(str);
                }
                
            }
       });    
       
       
        // 提交评论
        $.extend({
           saveComment : function(options){
                var defaults = {    
                   merchantId  : 0                                  
                };                         
                var options        = $.extend(defaults, options);            
                var merchantId     = defaults.merchantId;                       
                var commentType    = $(":input[name=saveWordsKey][checked]").val();
                var content        = $("#merchant-message-content").val();
                var msg            = '';
                var str            = '';
                if ('' == content){
                    str = '<p>请填写留言内容！</p>';
                    $("#z-merchant-content-tip").html(str);
                    $("#z-merchant-content-box").show();
                    return false;
                }
                                
                var url            = baseDetailUrl+"/index.php?c=Ajax_MerchantStore_MerchantComment&a=SaveMerchantComment&callback=?&t="+Math.random();            
                if (merchantId){
                    $.getJSON(
                            url,
                            {merchantId:merchantId, commentType:commentType, content:content},
                            function(data){  
                                str = '<p>'+data.msg+'</p>';
                                $("#z-merchant-content-tip").html(str);
                                $("#z-merchant-content-box").show();   
                                $("#merchant-message-content").val("");
                            }
                    );
                }
           } 
        });          
       
}) ;       

$(function(){    
    $.getCommentInfo({merchantId:merchantId});
    $(":input[name=wordsKey]").click(function(){
        $.getCommentInfo({merchantId:merchantId});
    });
       $("#z-merchant-content-close").click(function(){
            $("#z-merchant-content-tip").html("");
            $("#z-merchant-content-box").hide();        
       });    
});
</script>
                    
