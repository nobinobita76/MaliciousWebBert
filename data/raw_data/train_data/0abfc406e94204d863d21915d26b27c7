<!--QQ点击统计-->
     <script>
        // 获取cookie
        function getCookie(name) {
            var strCookie = document.cookie;
            var arrCookie = strCookie.split("; ");
            for (var i = 0; i < arrCookie.length; i++) {
                var arr = arrCookie[i].split("=");
                if (arr[0] == name)
                    return arr[1];
            }
            return "";
        } 
        // 统计代码
        function countQQClick(event,other) {
            var statsImg = new Image();
            var rand = new Date().getTime();
            var ipck = getCookie('ip_ck');
            var src  = 'http://pvtest.zol.com.cn/images/pvevents.gif?t='
                    + rand + '&event=' + event
                    + '&ip_ck=' + ipck                    
                    + '&url=' + document.URL;
            if(typeof other != 'undefined'){
                src = src + other;
            }
            statsImg.src = src;
        };
            $(function(){
                $(".qq_talk").live('click',function(e){
                    var merchantId  = '191827',
                        qqNumber    = $(e.currentTarget).data("qqNumber");
                    countQQClick('default_kefu_qq_talk', '&merchant_id='+merchantId+'&qqNumber='+qqNumber); 
                });
            })
            
    </script>
