        <script>
        $(function() {
            //判断浏览器版本调用反馈建议插件
            if (!(($.browser.msie) && ($.browser.version == "6.0"))){
                $.survey({merchantId:191827});
            }else{
                $("#zp-welcome-store").click(function(){
                    $(this).hide();
                });        
            }
            
            //底部生意宝咨询按钮关闭
            $("#dove-foot-close").click(function(){
                var noProm      = $("#dove-no-prom").is(":checked");
                if(noProm){ //选择了不再展开，Ajax更新值
                    var url     = "/index.php?c=Ajax_DetailLeft&a=setCookie";
                    $.getJSON(url,{},function(){});
                }
                //没有选择不再展开
                $(".service-foot-pup").hide();
                $(".column-1").show();
            });
            
            //自定义区块
            $("[fn='delDiyPage']").click(function() {
                var diyId = parseInt($(this).attr('rel'));
                if (!diyId) {
                    return false;
                }
                if (!confirm('确定删除当前自定义区块吗？')) {
                    return false;
                }
                var url = '/index.php?c=MerchantStore_Diy&a=DelDiyPage&t=' + (new Date).valueOf();
                var data = {'diyId': diyId};
                $.getJSON(url, data, function(backdata) {
                    alert(backdata.msg);
                    if (backdata.flag) {
                        location.reload();
                    }
                });
            });

            //主推商品橱窗排序筛选JS
            $("#mainSort>li").click(function() {
                var divNum = $(this).attr("fn");
                $(this).addClass("current").siblings().removeClass("current");
                $(".main-box").hide();
                $("#mainData_" + divNum).show();
            });

            //限时抢购商品显示时间效果
            $(".ad-icon").mouseover(function() {
                $(this).addClass("ad-icon-hover");
            }).mouseout(function() {
                $(this).removeClass("ad-icon-hover");
            });

            $('.setHiddenBtn').click(function() {
                var _this = $(this);

                var elementId = _this.attr('data-id');
                var type = _this.attr('data-type');
                var showType = _this.attr('data-set');

                var isHidden = 1;
                if (showType == 'setshow') {
                    isHidden = -1
                }

                var url = "/index.php?c=Ajax_MerchantStore_ModuleLayer&a=SetHidden&elementId=" + elementId + "&type=" + type + '&isHidden=' + isHidden;
                $.get(url, function(data) {
                    checkLogin(data);
                    location.href = '/index.php?c=MerchantStore_Default';
                });

            });
        });
    </script>
    <script>
        $(function(){
            var reg = /spm=([0-9\.]+)/i;
            var str  = window.location.search.match(reg);
            if(str != null && str !=="undefined" ){
                var tipsStr = str[0];
                $(".spmTips").each(function(){
                    var url    = $(this).attr("href");
                    var newUrl = url+"?"+tipsStr;
                    $(this).removeAttr("href");
                    $(this).attr("href",newUrl); 
                 });
            }
            
            
        });
    </script>
    <!--QQ点击统计-->
     <script>
        // 获取cookie
        function getCookie(name) {
            var strCookie = document.cookie;
            var arrCookie = strCookie.split("; ");
            for (var i = 0; i < arrCookie.length; i++) {
                var arr = arrCookie[i].split("=");
                if (arr[0] == name)
                    return arr[1];
            }
            return "";
        } 
        // 统计代码
        function countQQClick(event,other) {
            var statsImg = new Image();
            var rand = new Date().getTime();
            var ipck = getCookie('ip_ck');
            var src  = 'http://pvtest.zol.com.cn/images/pvevents.gif?t='
                    + rand + '&event=' + event
                    + '&ip_ck=' + ipck                    
                    + '&url=' + document.URL;
            if(typeof other != 'undefined'){
                src = src + other;
            }
            statsImg.src = src;
        };
            $(function(){
                $(".qq_right_talk").live('click',function(e){
                    var merchantId  = '191827',
                        qqNumber    = $(e.currentTarget).data("qqNumber");
                    countQQClick('default_qq_talk', '&merchant_id='+merchantId+'&qqNumber='+qqNumber); 
                });
            })
            
    </script>

