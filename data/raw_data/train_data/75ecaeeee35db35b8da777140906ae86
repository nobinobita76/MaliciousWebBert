      <script type="text/javascript">
		function checkSelect(){
			if(document.getElementById("form_folder").value == 0){
				document.getElementById("msg").innerHTML = " 请选择卷宗、年份、科目";
			}else if(document.getElementById("form_year").value ==0){
				document.getElementById("msg").innerHTML = " 请选择年份、科目";
			}else if(document.getElementById("form_subject").value==0){
				document.getElementById("msg").innerHTML = " 请选择科目";
			}else{
				document.getElementById("msg").innerHTML = "";
				window.location.href="http://ti.51edu.com/list_"+document.getElementById("form_folder").value+"_"+document.getElementById("form_year").value+"_"+document.getElementById("form_subject").value+"_1/";
			}
		}
	</script> 
