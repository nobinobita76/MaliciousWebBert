<script>
jQuery(function(){
        $("#thisAdd li:first").addClass("thistitle");
	var $curChannel = '渠道介绍';
	var $list = $(".classmainleft");
	//console.log($curChannel);
	if ($curChannel != "首页"){
		$list.css("display","none");
		$("#mailListPoiner img").hover(function(){
			$(this).css({
				height:"45px",
				width:"190px",
				display:"block",
				cursor:"pointer"
			});
			$list.show();
		},function(){
			$list.hide();
		});
		$(".classmainleft ul li div").css({visibility:"hidden"});
		$(".classmainleft").hover(function(){
			$(".classmainleft ul li div").removeAttr("style");
			$(this).show();
		},function(){
			$(this).hide();
			$(".classmainleft ul li div").css({visibility:"hidden"});
		})
	}
})
</script>

