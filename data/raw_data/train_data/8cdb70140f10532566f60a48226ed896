var scroolL = 0;
var totalL = 1*220;
function scroolA(lr){
if(lr==1){
	scroolL += 220;
	jQuery("#slider_name").animate({ scrollLeft: scroolL },500);
}else if(lr==0){
	scroolL -= 220;
	jQuery("#slider_name").animate({ scrollLeft: scroolL },500);
}
}
