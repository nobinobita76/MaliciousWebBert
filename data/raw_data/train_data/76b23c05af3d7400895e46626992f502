var boo = true ;
function showSpace(comid)
{
	 
	 
	if(boo)
	{	
		boo = false ;
    	jQuery.ajax({
    		url:"businessSpaceAjax.view",
    		type:"post",
    		data:{commId:comid},
    		success:function(data) {
				boo = true ;
				var st = data.split("-")[1] ;
				var login = data.split("-")[0] ;
			    if(st ==1)
    			{
					
					var url = 'http://'+login+'.'+'space.ebnew.com' ;
					var obj = document.getElementById("spaceID");   
  					obj.target="_blank" ;
					obj.href = url ;
					obj.click() ; 
					 
				}else
    			{
    				alert('没有开通企业空间!') ;
					return false ;
    			}	 
    		}
    	});
	}else
	{
		alert('正在查询中!') ;
		return ;
	}
 
}
