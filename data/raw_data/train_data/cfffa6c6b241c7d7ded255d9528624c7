                    <td>-</td>
                    <td>-</td>
                    <td>-</td>
                    <td>-</td>
                {{else}}
                    <td>{{:result['hstotal']}}</td>
                    <td>{{:(result['hswin'] / (result['hstotal'] || 1) * 100).toFixed(0)}}%</td>
                    <td>{{:result['hxtotal']}}</td>
                    <td>{{:(result['hxwin'] / (result['hxtotal'] || 1) * 100).toFixed(0)}}%</td>
                {{/if}}
                <td>{{:((result['hwin'] || 0) / (result['hatotal'] || 1) * 100).toFixed(0)}}%</td>
            </tr>
            <tr class="{{:tid === 205 ? 'highlight' : ''}}">
                <td>客</td>
                <td>{{:result['aatotal']}}</td>
                <td>{{:result['awin'] ? (result['awin'] || 1) : '0'}}</td>
                <td>{{:result['adraw'] ? (result['adraw'] || 1) : '0'}}</td>
                <td>{{:result['alost'] ? (result['aloat'] || 1) : '0'}}</td>
                {{if result['stotal'] === undefined}}
                    <td>-</td>
                    <td>-</td>
                    <td>-</td>
                    <td>-</td>
                {{else}}
                    <td>{{:result['astotal']}}</td>
                    <td>{{:(result['aswin'] / (result['astotal'] || 1) * 100).toFixed(0)}}%</td>
                    <td>{{:result['axtotal']}}</td>
                    <td>{{:(result['axwin'] / (result['axtotal'] || 1) * 100).toFixed(0)}}%</td>
                {{/if}}
                <td>{{:((result['awin'] || 0) / (result['aatotal'] || 1) * 100).toFixed(0)}}%</td>
            </tr>
        {{else}}
            <tr><td class="no-data" colspan="15">暂无数据...</td></tr>
        {{/if}}
    </tbody>
</table>
