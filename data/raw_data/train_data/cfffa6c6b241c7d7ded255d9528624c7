                <td>{{:result['draw'] ? result['draw'] : '0'}}</td>
                <td>{{:result['lost'] ? result['lost'] : '0'}}</td>
                {{if result['stotal'] === undefined}}
                    <td>-</td>
                    <td>-</td>
                    <td>-</td>
                    <td>-</td>
                {{else}}
                    <td>{{:result['stotal']}}</td>
