<script id="tmpl-standing-team" type="text/x-jsrender" data-jsv-tmpl="_7">
<table class="clearfix {{:css}}">
    <thead>
        <tr>
            <th colspan="15">
                <a href="javascript:void(0);" data-link="team" data-tid="{{:tid}}">{{:team}}</a>
                {{if result && result['lname']}}
                    ({{:result['lname']}})
                {{/if}}
            </th>
        </tr>
        <tr>
            <th width="26">&nbsp;</th>
            <th>场次</th>
            <th>胜场</th>
            <th>平场</th>
            <th>负场</th>
            <th>进球</th>
            <th>失球</th>
            <th width="50">净胜球</th>
            <th>积分</th>
            <th>排名</th>
            <th>胜率</th>
        </tr>
    </thead>
    <tbody>
        {{if result && result['hnum'] !== undefined}}
            <tr>
                <td>总</td>
                <td>{{:result['hnum'] + result['anum']}}</td>
                <td>{{:result['hw'] + result['aw']}}</td>
                <td>{{:result['hd'] + result['ad']}}</td>
                <td>{{:result['hl'] + result['al']}}</td>
                <td>{{:result['hjq'] + result['ajq']}}</td>
                <td>{{:result['hsq'] + result['asq']}}</td>
                <td>{{:(result['hjq'] - result['hsq']) + (result['ajq'] - result['asq'])}}</td>
                <td class="red">{{:result['score']}}</td>
                <td>{{:result['pm']['all']}}</td>
                <td>{{:((result['hw']  + result['aw']) / (result['hnum'] + result['anum']) * 100).toFixed(0)}}%</td>
            </tr>
            <tr class="{{:tid === 336 ? 'highlight' : ''}}">
                <td>主</td>
                <td>{{:result['hnum']}}</td>
                <td>{{:result['hw']}}</td>
                <td>{{:result['hd']}}</td>
                <td>{{:result['hl']}}</td>
                <td>{{:result['hjq']}}</td>
                <td>{{:result['hsq']}}</td>
                <td>{{:result['hjq'] - result['hsq']}}</td>
                <td class="red">{{:result['hscore']}}</td>
                <td>{{:result['pm']['hpm']}}</td>
                <td>{{:(result['hw'] / (result['hnum'] || 1) * 100).toFixed(0)}}%</td>
            </tr>
            <tr class="{{:tid === 205 ? 'highlight' : ''}}">
                <td>客</td>
                <td>{{:result['anum']}}</td>
                <td>{{:result['aw']}}</td>
                <td>{{:result['ad']}}</td>
                <td>{{:result['al']}}</td>
                <td>{{:result['ajq']}}</td>
                <td>{{:result['asq']}}</td>
                <td>{{:result['ajq'] - result['asq']}}</td>
                <td class="red">{{:result['ascore']}}</td>
                <td>{{:result['pm']['apm']}}</td>
                <td>{{:(result['aw'] / (result['anum'] || 1 )* 100).toFixed(0)}}%</td>
            </tr>
        {{else}}
            <tr><td class="no-data" colspan="15">暂无数据...</td></tr>
        {{/if}}
    </tbody>
</table>
</script>

