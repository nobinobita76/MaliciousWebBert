   showMsg(s.message);
}

function photo_delete(){
	$.ajax({
		type : "GET",
		url:"/user/ajax.php?mod=album&act=photo_manager&type=delete&album_id=1395256&image_id=1540249",
		data:{ },
		async:false,
		dataType:"json",
        success:function(o){
        	$("#1540249").remove();
			showMsg(o.message,'妖气提示',{'确定':function(){window.location.href="http://i.u17.com/2088889/album/photo_show/a1395256_i1540252.html";$(this).dialog('close');}});
    	},
		error:function(){showMsg('删除失败,发生通信错误');}
	});
}

function send_edit(type){
var d_text=$('#'+type).val();
if(d_text==''){
   showMsg('描述不能为空');
}else{
	$.ajax({
		type : "GET",
		url:"/user/ajax.php?mod=album&act=photo_manager&type="+type+"&album_id=1395256&image_id=1540249",
		data:{ edit_text : d_text },
		async:false,
		dataType:"json",
        success:function(o){
			e_text = d_text+'<img src="http://static.u17i.com/v2/common/images/icon/icon_edit.gif" />';
        	btn_reset(type);
        	$('#on_'+type).html(e_text);
			if(type=='title'){
				$('#nav_title').html(d_text);var title_temp=d_text;
			}else{ discrip_temp=d_text; }
        	showMsg(o.message);
    	},
		error:function(){showMsg('修改失败,发生通信错误');}
	});
}
}
function cal(type){
	if(type=="title"){
		var len_num = 30;
		var class_name = '.letter_pic';
	}else if(type=='discrip'){
		var len_num = 60;
		var class_name = '.letter';
	}
		txt = $('#'+type).val();
		var len = txt.length;
		if (len > len_num) {
			len = len_num;
			$('#'+type).val(txt.substring(0, len_num));
		}
		$(class_name).html(len_num-len);
}


function btn_reset(type){
	if(type=="title"){
		get_title_discrip('title');
		$('#title_area').hide('fast');
		$('#on_title').show('fast');
	}else if(type=='discrip'){
		get_title_discrip('discrip')
		$('#discrip_area').hide('fast');
		$('#on_discrip').show('fast');
	}
}

var discrip_temp = '';
var title_temp = '时崎狂三 (7).png';
function get_title_discrip(t){
		if(t=='title'){
			message=title_temp;
		}else if(t=='discrip'){
			message=discrip_temp;
			}
        	$('#'+t).html(message);
			$('#'+t).attr('value',message);
}

