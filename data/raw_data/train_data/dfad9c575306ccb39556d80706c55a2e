		type : "GET",
		url:"/user/ajax.php?mod=album&act=photo_manager&type=delete&album_id=1395256&type=set_cover&image_id=1540249",
		data:{ },
		async:false,
		dataType:"json",
        success:function(o){
        	showMsg(o.message);
    	},
		error:function(){showMsg('设置失败,发生通信错误');}
	});
});

$('#discrip').keyup(function() {
		var len_num = 60;
		txt = $(this).val();
		var len = txt.length;
		if (len > len_num) {
			len = len_num;
			$(this).val(txt.substring(0, len_num));
		}
		$('.letter').html(len_num-len);
	});
$('#title').keyup(function() {
		var len_num = 30;
		txt = $(this).val();
		var len = txt.length;
		if (len > len_num) {
			len = len_num;
			$(this).val(txt.substring(0, len_num));
		}
		$('.letter_pic').html(len_num-len);
	});

