<script type="text/javascript">
var ul_offset = parseInt('-150');
$(document).ready(function(){
    $('#photo_frame').tucao({
	image_id:"1540249",
	total_tucao:"0",
	tucao_id:getUrlParam('tucao_id')==""?0:parseInt(getUrlParam('tucao_id'))
    });
	if(ul_offset!=0){
		$("#image_list_ul").css("left",ul_offset); 
		//$("#image_list_ul").animate({"left": parseInt("-150")}, "fast"); 
	}
	ready_doc();
})

$('.nav_list').mouseenter(function(){
		$(this).attr('class','current');
	}).mouseleave(function(){
		$(this).attr('class','nav_list');
	}
);

function ready_doc(){
	var ul_offset = parseInt('-150');
	var li_offset = $('#image_list_ul .current').position();
	var c_off = li_offset.left+ul_offset;
	var width = -parseInt($("#image_list_ul").css('width'));
	if(c_off<70){
		if(li_offset.left>225){
			$("#image_list_ul").animate({"left": "+=225px"}, "fast");
		}else{
			$("#image_list_ul").animate({"left": "0px"}, "fast"); 
		}
	}else if(c_off>550){
		if(-(li_offset.left+width)<705){
			$("#image_list_ul").animate({"left": width+705}, "fast"); 
		}else{
			$("#image_list_ul").animate({"left": "-=225px"}, "fast");
		}
	}
}
$('.nav_left').click(function(){
	var left = parseInt($("#image_list_ul").css('left'));
	if(isNaN(left) || left+225>=0){
		$("#image_list_ul").animate({"left": "0px"}, "fast"); 
	}else{
		$("#image_list_ul").animate({"left": "+=225px"}, "fast");
	}
	$('li img.pre').last().removeClass('pre').attr('src',function(){return $(this).attr('name')});
	$('li img.pre').last().removeClass('pre').attr('src',function(){return $(this).attr('name')});
	$('li img.pre').last().removeClass('pre').attr('src',function(){return $(this).attr('name')});
});

$('.nav_right').click(function(){
	var width = parseInt($("#image_list_ul").css('width'));
	var left = -parseInt($("#image_list_ul").css('left'));
	if(isNaN(left)){
		left = 0;
	}
	var end_left = width-705;//最后一组图片的距离
	if(width<=705){
		$("#image_list_ul").animate({"left": "0px"}, "fast");
	}else if(left>=end_left){
		$("#image_list_ul").animate({"left": -end_left}, "fast"); 
	}else{
		$("#image_list_ul").animate({"left": "-=225px"}, "fast");
	}
	$('li img.next').first().removeClass('next').attr('src',function(){return $(this).attr('name')});
	$('li img.next').first().removeClass('next').attr('src',function(){return $(this).attr('name')});
	$('li img.next').first().removeClass('next').attr('src',function(){return $(this).attr('name')});
});


$('.pic_a').click(function(){
	$('#image_url').attr('src',this.href);
	$("#image_url").attr('name',this.name);
})

function change_album(aid){
if(aid>0){
    $.ajax({
        type : "GET",
        url:"/user/ajax.php?mod=album&act=photo_manager&type=move&image_id=1540249",
        data:{ album_id : aid},
		async:false,
                dataType:"json",
        	success:function(o){
			showMsg(o.message,'妖气提示',{'确定':function(){window.location.href="http://i.u17.com/2088889/album/photo_show/a1395256_i1540252.html";$(this).dialog('close');}});
        },
		error:function(){showMsg('移动失败,服务器通信错误');}
    });
}
}

$('#on_discrip').click(function(){
	$('#discrip_area').removeClass("non");
})


$('#slidePre').click(function(){
	var left = parseInt($("#image_list_ul").css('left'));
	window.location.href="http://i.u17.com/2088889/album/photo_show/a1395256_i1540247_l"+left+".html#photo_show";
})

$('#slideNext').click(function(){
	var left = parseInt($("#image_list_ul").css('left'));
	window.location.href="http://i.u17.com/2088889/album/photo_show/a1395256_i1540252_l"+left+".html#photo_show";
})


$('a[name=pic_a]').click(function(){
	var left = parseInt($("#image_list_ul").css('left'));
	$('a[name=pic_a]').attr('href',function(){
		return this.href.replace('.html','_l'+left+'.html');								
	});
})
	
