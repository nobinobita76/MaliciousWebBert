    <script type="text/javascript">
        Taoche.SideBar.init({
            hideModules: ['shop'],
            compareChangeCallback: function (ids) {
                $('#carlist .carpic-layer-contrast').each(function () {
                    var a=$(this),id=a.attr('data-id');
                    if(Taoche.indexOfArray(ids,id)>-1) {//已加对比,绑定取消对比事件
                        a.unbind('click').addClass('carpic-layer-disabled').html('已加对比');
                    } else {//未加对比，绑定事件
                        a.unbind('click').removeClass("carpic-layer-disabled").html("加入对比").click(function (ev) {
                            var _id=$(this).unbind('click').addClass("carpic-layer-disabled").html("已加对比").attr('data-id');
                            Taoche.SideBar.addCompareCar(_id,ev);
                        });
                    }
                });
            },
            attenChangeCallback: function (ids) {
                var sType=$('#hidSourceTypeId').val();
                $('#carlist .carpic-layer-follow').each(function () {
                    var a=$(this),id=a.attr('data-id');
                    if(Taoche.indexOfArray(ids,id)>-1) {//已加关注,绑定取消关注事件
                        a.unbind('click').addClass('carpic-layer-disabled-help').html('取消关注').click(function () {
                            var _id=$(this).unbind('click').removeClass("carpic-layer-disabled-help").html("加入关注").attr('data-id');
                            Taoche.SideBar.removeAttenCar(_id);
                        });
                    } else {//未加关注，如果当前是"我的关注"页签直接删除此元素，否则绑定加关注事件
                        if(sType=='6') { $('#li'+id).remove(); return true; }
                        a.unbind('click').removeClass("carpic-layer-disabled-help").html("加入关注").click(function (ev) {
                            var _id=$(this).unbind('click').addClass('carpic-layer-disabled-help').html('取消关注').attr('data-id');
                            Taoche.SideBar.addAttenCar(_id,ev);
                        });
                    }
                });
                if(sType=='6') {//我的关注页签，需要对 已售 车源做同步删除操作
                    $('#carlist .carpic-similar').each(function () {
                        var li=$(this).parent().parent(),id=li.attr('id').replace('li','');
                        if(Taoche.indexOfArray(ids,id)<0) {//已取消关注，删除此元素
                            li.remove();
                        }
                    });
                }
            }
        });
