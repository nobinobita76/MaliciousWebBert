    <script type="text/javascript">
        //地图加载
        (function () {
            var map=new BMap.Map("allmap");
            map.centerAndZoom(Taoche.currentCityName,10);
            map.addEventListener("click",function (e) {
                var nw=window.open('_blank');
                nw.location='http://'+Taoche.currentCitySpell+'.taoche.com/map/';
            });
            var url='http://www.taoche.com/map/?areaname='+Taoche.currentCitySpell;
            $.get('/ajax/buycar/getdealerinmap.ashx',{ url: encodeURIComponent(url),count: 15,v: Math.random() },function (data) {
                var ds=eval('('+data+')');
                for(var i=0; i<ds.length; i++) {
                    var d=ds[i];
                    if(d.lng==''||d.lat==''||isNaN(d.lng)||isNaN(d.lat)) continue;
                    var pt=new BMap.Point(d.lng,d.lat);
                    var marker=new BMap.Marker(pt);
                    map.addOverlay(marker);
                }
            });
        })();
    </script>

