        var __zp_serialIds_str='';
        var __zp_lelvels_str='';
        var __zp_ucarids='';
        //动态传参代码必须部署在标准代码之前
        //pt:页面标签
        //city:城市ID，省站，全国为空
        //brand:品牌名称， 字符串数组
        //serialID:车系ID，字符串数组
        //category:级别名称，字符串数组
        //ucarId:推荐车源ID
        var __zp_tag_params={
            pt: "listPage",
            city: '北京',
            brand: '',
            serialID: __zp_serialIds_str!=''?__zp_serialIds_str.split(','):[],
            category: __zp_lelvels_str!=''?__zp_lelvels_str.split(','):[],
            ucarId: __zp_ucarids!=''?__zp_ucarids.split(','):[]
        };
