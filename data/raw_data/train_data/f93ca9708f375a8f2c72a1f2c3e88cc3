var hidefocus = false;
$(window).bind('scroll orientationchange', function(e) {
    if($(this).scrollTop() > 500) {
        if(!hidefocus && $(".footer-channels").is(':hidden')) {
            $(".footer-channels").removeClass('none');
        }
    } else {
        $(".footer-channels").addClass('none');
    }
});

