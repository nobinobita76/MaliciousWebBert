  <script>/**
 * device_detect
 * This is used to do agent based redirects (m. vs www. or store.) when the page first loads.
 */
//Create global reference for this function
var nike = nike || {};
nike.exp = nike.exp || {};
nike.exp.script = nike.exp.script || {};
nike.exp.script.device_detect = nike.exp.script.device_detect || {};


!function(){

  /**
   * check the browser's userAgent and redirect it to the appropriate presentation
   */
  nike.exp.script.device_detect.redirectOnAgentMismatch = function(isPresentationMobile, alternateUrl){
    var newUrl,
    ref,
    loc = window.location,
    selectedHost,
    queryString,
    presentationMatchesDevice;

    if(isPresentationMobile === undefined || !alternateUrl){
      log("Presentation vars are not defined, so we are skipping the agent redirect check");

    } else {
      //Compare values as strings so we don't have to worry about object type
      presentationMatchesDevice = ("" + isPresentationMobile).toLowerCase() === ("" + nike.exp.script.device_detect.isMobile()).toLowerCase();
      log("isPresentationMobile=" + isPresentationMobile + " isMobileAgent=" + nike.exp.script.device_detect.isMobile() + " presentationMatchesDevice=" + presentationMatchesDevice);

      // If current presentation is a mismatch for the device and it was not manually selected as "dev", redirect user to the correct presentation
      // If the presentation is manually selected as "dev", do nothing here, unless presentation is set to "mobile" and the page is desktop, then switch.
      if ((!checkSPCookie("dev") && !presentationMatchesDevice && !checkSPCookie("mobile")) || (checkSPCookie("mobile") && isPresentationMobile == "false")){
        var newUrl = nike.exp.script.device_detect.buildNewUrl(alternateUrl);
        if (window.location != newUrl) {
          window.location = newUrl;
        }
      }
    }
  };

  /**
   * Combine query string params, making sure that the alternate url takes precedence.
   */
  nike.exp.script.device_detect.buildNewUrl = function(alternateUrl) {
    var self = nike.exp.script.device_detect;
    var newUrl = '';
    var oldUrl = alternateUrl;
    var oldParams = {};
    var newParams = {};
    var combinedParams = {};

    if(alternateUrl.indexOf('?') != -1) {
      var pieces = alternateUrl.split('?');
      oldUrl = pieces[0];
      var query = pieces[1];
      oldParams = self.buildParamMap(query);
    }

    var newQueryString = buildQueryString();
    // remove the question mark
    newQueryString = newQueryString.replace('?', '');
    newParams = self.buildParamMap(newQueryString);

    // combine the params
    var combinedParams = newParams;
    for(var key in oldParams) {
      combinedParams[key] = oldParams[key];
    }

    // build the query string
    var queryString = '';
    var tokens = [];
    for(var key in combinedParams) {
      var token = key + '=' + combinedParams[key];
      tokens.push(token);
    }

    if(tokens.length > 0) {
      queryString = '?' + tokens.join('&');
    } else {
      queryString = '';
    }
    newUrl = oldUrl + queryString;

    if(window.location.hash) {
      newUrl += window.location.hash;
    }

    return newUrl;
  };

  /**
   * builds an object based on query string
   */
  nike.exp.script.device_detect.buildParamMap = function(queryString) {
    var params = {};
    var tokens = [];
    if(queryString.length == 0) {
      return {};
    }
    if(queryString.indexOf('&') == -1) {
      tokens = [queryString];
    } else {
      tokens = queryString.split('&');
    }
    for(var i in tokens) {
      var token = tokens[i];
      var parts = token.split('=');
      var key = parts[0];
      var val = '';
      if(parts.length == 2) {
        val = parts[1];
      }
      params[key] = val;
    }

    return params;
  };

  /**
   * check the browser's userAgent and redirect it to the appropriate presentation
   */
  nike.exp.script.device_detect.doAgentRedirect = function(isPresentationMobile, alternateHost, alternateSecureHost){
    var newUrl,
        ref,
        loc = window.location,
        selectedHost,
        queryString,
        presentationMatchesDevice;


    if(isPresentationMobile === undefined || alternateHost === undefined || alternateSecureHost === undefined){
      log("Presentation vars are not defined, so we are skipping the agent redirect check");

    } else {
      //Compare values as strings so we don't have to worry about object type
      presentationMatchesDevice = ("" + isPresentationMobile).toLowerCase() === ("" + nike.exp.script.device_detect.isMobile()).toLowerCase();
      log("isPresentationMobile=" + isPresentationMobile + " isMobileAgent=" + nike.exp.script.device_detect.isMobile() + " presentationMatchesDevice=" + presentationMatchesDevice);

      // If current presentation is a mismatch for the device and it was not manually selected as "dev", redirect user to the correct presentation
      // If the presentation is manually selected as "dev", do nothing here, unless presentation is set to "mobile" and the page is desktop, then switch.
      if ((!checkSPCookie("dev") && !presentationMatchesDevice && !checkSPCookie("mobile")) || (checkSPCookie("mobile") && isPresentationMobile == "false")){

        selectedHost = loc.protocol.toLowerCase() ==="http:" ? alternateHost : alternateSecureHost;

        if(selectedHost && selectedHost.length > 0){
          //Redirect to alternate host
          newUrl = selectedHost + loc.pathname;

          queryString = buildQueryString();

          if(queryString && queryString.length > 0){
            newUrl += queryString;
          }

          //Set hash
          if(loc.hash){
            newUrl += loc.hash;
          }

          window.location = newUrl;
        } else {
          //No alternate presentation
          log("No alternate url defined.");
        }
      }
    }
  };

  /**
   * Check agent for iphone, ipod, android mobile, windows phone, and iemobile.  If any are found, flag as mobile
   */
  nike.exp.script.device_detect._isMobile = undefined;
  nike.exp.script.device_detect.isMobile = function() {
    log("UserAgent: " + navigator.userAgent);
    nike.exp.script.device_detect._isMobile = /iPhone|iPod|Windows Phone|IEMobile/i.test(navigator.userAgent)
                                              || /android.*mobile/i.test(navigator.userAgent)
                                              || /mozilla.*mobile.*firefox/i.test(navigator.userAgent);

    return nike.exp.script.device_detect._isMobile;
  };


  /**
   * Test if user has manually selected a presentation already (sp cookie).
   *
   * Normally I would use nike.SitePresentationUtil to inquire about the sp cookie, but I'm not doing it here for performance reasons (SitePresentationUrl depends on several other scripts).
   * We need this script to run in the head right away to try and avoid loading up the entire page before doing the redirect.  It should
   * maybe event be inline so the script request doesn't have to compete with other requests, but for now it seems better to use an include
   * so it gets cached.
   *
  */
  function hasPresentationSelected(){
    return /(?:$|; ?)sp=[^;]*/i.test(document.cookie);
  }


  /**
   * Gets the value of a cookie by cookie name
   */
  function getCookie(name) {
    var allCookies = document.cookie.split( ';' );
    var tempCookie = '';
    var cookieName = '';
    var cookieValue = '';
    var cookieFound = false; // set boolean t/f default f
    var i;

    for ( i = 0; i < allCookies.length; i++ ){
      tempCookie = allCookies[i].split( '=' );
      cookieName = tempCookie[0].replace(/^\s+|\s+$/g, '');
      if ( cookieName == name ){
        cookieFound = true;
        // we need to handle case where cookie has no value but exists (no = sign, that is):
        if ( tempCookie.length > 1 ){
          cookieValue = unescape( tempCookie[1].replace(/^\s+|\s+$/g, '') );
        }
        return cookieValue;
        break;
      }
      tempCookie = null;
      cookieName = '';
    }

    if ( !cookieFound ){
      return null;
    }
  }

  /**
   * Checks if the SP cookie is set to the cookieValue
   * Returns true or false
   */
  function checkSPCookie(cookieValue) {
    return getCookie("sp") == cookieValue;
  }


  /**
   * Get the queryString for the current url and wrap in the referrer as the 'ref' param if it hasn't been added already.
   */
  function buildQueryString(){
    var retQS = window.location.search,
        ref;

    //Setup ref param to capture referrer if the query does not already have a ref param
    if(!/(^|&)ref=/.test(window.location.search)){
      ref = encodeURIComponent(document.referrer);
      // removes referrer if root domain is present in domain name
      var rootDomain = window.location.hostname;
      var hostNameParts = rootDomain.split(".");
      ref = ref.indexOf('/', 8) > -1 ? ref.slice(0, ref.indexOf('/', 8)) : ref;
      if(hostNameParts.length > 1) {
        var rootDomainPart = hostNameParts.slice(hostNameParts.length - 2);
        rootDomain = rootDomainPart ? "." + rootDomainPart.join(".") : rootDomain;
      }
      if(ref && ref.indexOf(rootDomain) > -1) {
        ref = "";
      }
    }

    if(ref && ref.length  > 0){
      if(retQS && retQS.length > 0){
        retQS += "&ref=" + ref;
      } else{
        retQS = "?ref=" + ref;
      }
    }
    return retQS;
  }

  //Wrap console log so it doesn't blow up if console is not available
  function log(objectToLog){
    var enableLogging = false;
    if(enableLogging && console && console.log){
      console.log(objectToLog);
    }
  }


}();
</script>
