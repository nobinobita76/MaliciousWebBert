function select_pid()
{
	var logoid = document.getElementById('logo').value;
	var series = document.getElementById('series').value;
	if(logoid == '-1')
	{
		alert('请选择车系');
		return false;
	}
	if(series == '-1')
	{
		alert('请选择车系');
		return false;
	}
	window.open( '/0200/skip_sell_list.php?logoid='+logoid+'&seriesid='+series,"_blank")
}
</script>

