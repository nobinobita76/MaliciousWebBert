  <script>
  function urlreform( i_group,i_index )
  {
  	var att_url = "";
  	var group_values = "-10,";
  	var group_arys = group_values.split("-");
  	
  	if ( group_arys.length > 1)
  		group_values = group_arys[1]; 

  	group_arys = group_values.split(",");
  	for ( var i = 0; i < group_arys.length;i++)
  	{
  		var att_value = group_arys[i].split("_");
  		
  		for ( var j = 0; j < att_value.length; j++ )
  		{		
  			if (  i == i_group && j != i_index+1 )
  			{
  				att_url+=att_value[j];
  				if ( j+1 < att_value.length )
  					att_url += "_";

  			}
  			else if (  i != i_group  )
  			{
  				att_url+=att_value[j];
  				if ( j+1 < att_value.length )
  					att_url += "_";
  			}
  			
  		}
  		if ( i+1 < group_arys.length )
  			att_url+=",";	
  	}
  	//alert(att_url);
  	
  	if ( att_url.length > 0 )
  		att_url = "-"+att_url;
  	
  	window.location.href = "http://shaoyang.xwstore.cn/live-4-0-1-0,0-0,0-0,0"+att_url+"-m--.html";
  }
  function addatturl( i_group,i_index )
  {
  	
  	var att_url = "-10,";
  	var temp_url = "";
  	if ( att_url.length <= 0 )
  	{
  		temp_url = i_group+"_"+i_index+",";
  	}
  	else
  	{
  		att_url = att_url.split("-")[1];
  		
  		var temp_ary_one = att_url.split(",");
  		var group_ary = new Array();
  		var group_values_ary = new Array();
  		var b_group = false;
  		var b_index = false;
  		for ( var i = 0; i < temp_ary_one.length; i++ )
  		{
  			
  			var temp_ary_two = temp_ary_one[i].split("_");
  			group_ary[i] = temp_ary_two[0];
  			
  			if ( i_group == temp_ary_two[0])
  			{
  				b_group = true;
  				for ( var j = 1; j < temp_ary_two.length; j++ )
  				{
  					group_values_ary[j-1] = temp_ary_two[j];
  					if ( i_index == temp_ary_two[j])
  					{
  						b_index = true;
  						break;
  					}
  				}
  				break;	
  			}
  		}
  		
  		for ( var i = 0; i < temp_ary_one.length; i++ )
  		{
  			if ( b_group && b_index )
  			{
  				temp_url = att_url;
  			}
  			else if ( b_group && !b_index )
  			{
  				var temp_ary_two = temp_ary_one[i].split("_");
  				group_ary[i] = temp_ary_two[0];
  				if ( i_group != temp_ary_two[0])
  				{
  					temp_url += temp_ary_one[i]+",";
  				}
  				else
  				{
  					temp_url +=temp_ary_one[i]+"_"+i_index+",";
  				}
  			}
  			else if ( !b_group && !b_index )
  			{
  				temp_url = att_url+i_group+"_"+i_index+",";
  			}
  		}
  	}
  	if ( temp_url.length > 1 )
  		att_url = "-"+temp_url;

  	window.location.href = "http://shaoyang.xwstore.cn/live-4-0-1-0,0-0,0-0,0"+att_url+"-m--.html";
  }
  function getareasmall(group_id_js,group_value_js)
  {
	  
	  var obj = document.getElementById("area_group_div").getElementsByTagName("a");
	  for( var i = 0; i < obj.length; i++ )
	  {
	     obj[i].className='';
	  }
  	document.getElementById("area_small_div").style.display="";
  	//document.getElementById("area_small_a").innerHTML = group_value_js;
  	document.getElementById("area_small_group_id").innerHTML = group_id_js;
  	immediately();

  }
  function immediately()
  {
  	
      //var element = document.getElementById("ajax_area_small_ser_value");
      /*if("\v"=="v") 
      {
          element.onpropertychange = webChange;
      }
      else
      {
          element.addEventListener("input",webChange,false);
      }*/
      //function webChange()
      {
          //if(element.value)
          {
              //element.value;
              var group_id = document.getElementById("area_small_group_id").innerHTML;
          	var ser = "";//document.getElementById("ajax_area_small_ser_value").value;
          	ser = ser.replace(/\s+/g, ""); 
          	
          	/*if (ser == "" )
          	{
          		document.getElementById("area_samll_values_infos").innerHTML="请输入地名的首字母或地名的中文名称！";
          		return;
          	}*/
  			var hz_count = 0;

  			if (/[^\x00-\xff]/g.test(ser))
  				hz_count = 1;
  			
          	var Verify_PassWord_Ajax = "";
          	var ajaxObjects = new sack();
          	ajaxObjects.requestFile = "/do/area_smallAction?dispatch=Ajax_area_small_View&group_id="+group_id+"&ser="+ser+"&hz_count="+hz_count;
          	
          	ajaxObjects.onCompletion = function()
          	{
          	    Verify_PassWord_Ajax = ajaxObjects.response;
              	document.getElementById("area_samll_values_infos").innerHTML=Verify_PassWord_Ajax;
          	}
          	ajaxObjects.runAJAX();
          	
          }
      }
  }

