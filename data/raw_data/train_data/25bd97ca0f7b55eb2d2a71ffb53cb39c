  function div_att_opt( div_att_id ,s_h )
  {
  	
  	var a_id_0 = "a_att_opt0_"+div_att_id;
  	var a_id_1 = "a_att_opt1_"+div_att_id;
  	if (s_h=="0")
  	{	
  		
  		document.getElementById(div_att_id).style.display="none";
  		document.getElementById(a_id_1).style.display = "none";
  		document.getElementById(a_id_0).style.display = "";
  	}
  	else
  	{
  		
  		document.getElementById(div_att_id).style.display="";
  		document.getElementById(a_id_1).style.display = "";
  		document.getElementById(a_id_0).style.display = "none";
  	}
  }
function prcandacr()
{
	var ser_rate = "";
	var ser_acreage = "";
	var ser_hire = "";
	if ( document.getElementById("rate1").value == "" )
		ser_rate += "0,";
	else
		ser_rate += document.getElementById("rate1").value+",";

	if ( document.getElementById("rate2").value == "" )
		ser_rate += "0";
	else
		ser_rate += document.getElementById("rate2").value;

	if ( document.getElementById("hire1").value == "" )
		ser_hire += "0,";
	else
		ser_hire += document.getElementById("hire1").value+",";

	if ( document.getElementById("hire2").value == "" )
		ser_hire += "0";
	else
		ser_hire += document.getElementById("hire2").value;
	

	if ( document.getElementById("acr1").value == "" )
		ser_acreage += "0,";
	else
		ser_acreage += document.getElementById("acr1").value+",";

	if ( document.getElementById("acr2").value == "" )
		ser_acreage += "0";
	else
		ser_acreage += document.getElementById("acr2").value;
	
	window.location.href = "http://shaoyang.xwstore.cn/live-4-0-1-"+ser_rate+"-"+ser_hire+"-"+ser_acreage+"-10,-m-.html";	
}
function remove_acr()
{
	window.location.href = "http://shaoyang.xwstore.cn/live-4-0-1-0,0-0,0-0,0-10,-m-.html";
}
function remove_rate()
{
	window.location.href = "http://shaoyang.xwstore.cn/live-4-0-1-0,0-0,0-0,0-10,-m-.html";
}
function remove_hire()
{
	window.location.href = "http://shaoyang.xwstore.cn/live-4-0-1-0,0-0,0-0,0-10,-m--.html";
}
  </script> 
  <script>
 function search()
 {
 	var ser = document.getElementById('text_ser').value;
 	if ( ser == "" )
 		return;
 	var url_js="";
 	url_js = "/liveser/"+transition(ser)+"-m.html";
 	window.open(url_js);
 }
 </script> 
