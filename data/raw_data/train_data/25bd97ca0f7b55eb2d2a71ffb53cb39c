function IsPcVersion()
{
    //平台、设备和操作系统
    var system =
    {
        win : false,
        mac : false,
        xll : false
    };
    //检测平台
    var p = navigator.platform;
    //alert(p);
    system.win = p.indexOf("Win") == 0;
    system.mac = p.indexOf("Mac") == 0;
    system.x11 = (p == "X11") || (p.indexOf("Linux") == 0);
    //跳转语句
    if(system.win||system.mac||system.xll)
    {//转向后台登陆页面
        //window.location.href="/xwstore.cn_index.jsp";
        return true;
    }
    else
    {
    	//手机版
        //window.location.href="wap/sj_index.jsp";
        return false;
    }
}
if ( IsPcVersion() )
{
	document.write('<a href="/do/addliveinfos?dispatch=addliveinfos&style=1" target="_blank" ><img src="/images/biebiao_cr01.gif" style="border:0px;" /></a>');	
}
else
{
	document.write('<a href="/do/mwapaddliveinfos?dispatch=mwapaddliveinfos&style=1" ><img src="/images/biebiao_cr01.gif" style="border:0px;" /></a>');
}
</script> 
