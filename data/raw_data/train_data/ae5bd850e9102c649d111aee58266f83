var shid=1;
function show_ty(id)
{
for(var i=0; i<7;i++)
{
document.getElementById("l_"+id).className="fl-n_aaa";
document.getElementById("r_"+id).style.display="none";
}
if(shid==1)
{
document.getElementById("r_"+id).style.display="";
document.getElementById("l_"+id).className="fl-n_bbb";
shid=2;
}
else 
{
document.getElementById("r_"+id).style.display="none";
shid=1;
}

}
</script> 
