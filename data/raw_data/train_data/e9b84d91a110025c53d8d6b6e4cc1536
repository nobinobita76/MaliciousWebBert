$("#gg").html(ggad);
$("a").each(function() {
if(this.href.indexOf(location.hostname) == -1 && this.href.indexOf(host1) == -1 && this.href.indexOf(host2) == -1  && this.href.indexOf(host3) == -1) {
var outurl = $(this).attr("href");
