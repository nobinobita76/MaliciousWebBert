function checkkw()
{
var kw = document.getElementById("kw").value;
if(kw == "" || kw == '在50万帖子，100万资料，8万问答中搜索'){
alert("请输入关键词，再进行搜索！\n\n关键词是你要搜寻的内容，多关键词用空格隔开");
document.getElementById("kw").focus();
return false;
}
return true;
}
