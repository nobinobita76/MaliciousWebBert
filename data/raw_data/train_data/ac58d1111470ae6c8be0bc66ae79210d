<script>
function addcollect(id){
   var cookie_pos = document.cookie.indexOf('esfuserid');
   var title;
   telno= $("#telno_01").val();
   if(cookie_pos < 0) {
   openWin('JS_noteHouse7_7');
   }else{     
	$.ajax({
	  type: "POST",
	  url: "/module/action/ajax_action.php",
	  data: "act=collectblock_api&blockid="+id+"&telno="+telno,
	  success: function(msg){
		if(msg==1){
			$("#JS_noteHouse7_7").hide();
			openWin('JS_noteHouse101');
            //用户行为记录统计(add by xz 2014/3/19)
			_ua.push(11,{"blockid":"752","district":4,"streetid":"180","keyword":"\u79d1\u5927\u82b1\u56ed","averprice":"17085.00"});
			setTimeout("location.reload()",3000);
		  }else if(msg == 3){
			$('#JS_noteHouse7_7').hide();
			$('#sendmsg_tips').html('已关注过此小区');
			openWin('JS_noteHouse111');
			setTimeout("location.reload()",3000);
		  }else if( msg == 2 )
		  {
		    $('#sendmessage_telno_01').hide();
		    $('#sendmessage_code_01').html('验证码不正确!').show();
		    return false;
		  }else{
			$("#JS_noteHouse7_7").hide();
			openWin('JS_noteHouse111');
			setTimeout("location.reload()",3000);
		  }
		}
		
    });
   }
}
</script>

