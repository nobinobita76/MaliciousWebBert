<script type="text/javascript" src="http://hf.sell.house365.com/block/script/js/indexXQ.js"></script>

<script>
//js需要添加
function website_tab(tab_index_id,tab_index_class,tab_index_add,tab_eq_id,tab_eq_class){
	//tab_index_id 为tab上部切换元素的父级盒子id 
	//tab_index_class 为tab上部可以切换的元素class
	//tab_index_add 为tab上部可以切换的元素当前选中加上的class
	//tab_eq_id 为tab下部切换元素的父级盒子id
	//tab_eq_class  为tab下部可以切换的元素class
	$("#"+tab_index_id).find("."+tab_index_class).each(function(index, element) {
		$(this).mouseover(function(){
			$(this).addClass(tab_index_add).siblings("."+tab_index_class).removeClass(tab_index_add);
			$("#"+tab_eq_id).find("."+tab_eq_class).hide().eq(index).show();
		})
	});

}
$(function(){
	website_tab("js_website_tab_hd","js_website_tab_item","item_on","js_website_tab_mod","js_website_tab_mod_info");
});
</script>
