<script type="text/javascript">
$(function(){
$('#fasong').click(function(){
    var telno = $("#telno_01").val();
	var validcode = $("#validcode_01").val();
	var partten = /^1\d{10}$/;
		if(!partten.test(telno)){
			$('#sendmessage_code_01').hide();
			$('#sendmessage_telno_01').html('请输入正确的手机号码').show();
			return false;
		}
	if( validcode =='' || validcode.length != 6){
		$('#sendmessage_telno_01').hide();
		$('#sendmessage_code_01').html('验证码不正确!').show();
		return false;
	}

	$.ajax({
            type: 'post',
            url : 'http://hf.sell.house365.com/module/action/ajax_action.php',
            data:"act=collectblock_api&telno="+$("#telno_01").val()+"&validcode="+$("#validcode_01").val()+"&kind=6"+"&blockid="+$("#blockid").val(),
			dataType:'html',
            success: function(msg){
				if( msg == 1 ){

					$("#JS_noteHouse7_7").hide();
					openWin('JS_noteHouse101');
                    //用户行为记录统计(add by xz 2014/3/19)
					_ua.push(11,{"blockid":"752","district":4,"streetid":"180","keyword":"\u79d1\u5927\u82b1\u56ed","averprice":"17085.00"});
					setTimeout("location.reload()",3000);
				 }
				 else if( msg == 3 )
				 {
					$('#JS_noteHouse7_7').hide();
					$('#sendmsg_tips').html('已关注过此小区');
					openWin('JS_noteHouse111');
					setTimeout("location.reload()",3000);
				 }else if( msg == 2 )
				  {
					$('#sendmessage_telno_01').hide();
					$('#sendmessage_code_01').html('验证码不正确!').show();
					return false;
				  }
				 else
				 {
					$("#JS_noteHouse7_7").hide();
					openWin('JS_noteHouse111');
					setTimeout("location.reload()",3000);
				 }
				}
	     })

})
})

function send_validcode(){
    var self = $('#send_code');
	var kind = 6;
	var telno = $("#telno_01").val();
	var time = 0;
	if(time == 0)
	{
		var partten = /^1\d{10}$/;
			if(!partten.test(telno)){
				$('#sendmessage_code_01').hide();
				$('#sendmessage_telno_01').html('请输入正确的手机号码').show();
				return false;
			}else{
				$('#sendmessage_telno_01').hide();
				$.post('http://hf.sell.house365.com/module/action/ajax_action.php',{act:'getVnum_api',telno:telno,kind:kind},
					function(data){
						if(data == '1'){
							time = 60;
							var oTime = null;
							self.val('重新获取('+time+')');
							self.css({"color":"#999"});
							oTime = setInterval(function(){if(time>0){time--;self.val('重新获取('+time+')');}else{clearInterval(oTime);self.val('获取验证码');self.css({"color":"#000"});$('#sendingnote').hide();}} ,1000);
						}
					}
				);
			}
	}
}
</script>
