<script type="text/javascript">
    var projectOptions =
            {
                selectFirst: false,
                width: 358,
                minChars: 1,
                max: 100,
                dataType: 'json',
                extraParams: {
                    format: 'json',
                    act: 'hint1'
                },
                parse: function (data) {
                    var parsed = [];
                    for (var i = 0; i < data.length; i++) {
                        parsed[parsed.length] = {
                            data: data[i],
                            value: data[i].Key,
                            result: data[i].Value
                        };
                    }
                    return parsed;
                },
                formatItem: function (item) {
                    return item.Key;
                }
            };
    var doctorOptions =
            {
                selectFirst: false,
                width: 358,
                minChars: 1,
                max: 100,
                dataType: 'json',
                extraParams: {
                    format: 'json',
                    act: 'hint2'
                },
                parse: function (data) {
                    var parsed = [];
                    for (var i = 0; i < data.length; i++) {
                        parsed[parsed.length] = {
                            data: data[i],
                            value: data[i].Key,
                            result: data[i].Value
                        };
                    }
                    return parsed;
                },
                formatItem: function (item) {
                    return item.Key;
                }
            };
    var hospitalOptions =
            {
                selectFirst: false,
                width: 358,
                minChars: 1,
                max: 100,
                dataType: 'json',
                extraParams: {
                    format: 'json',
                    act: 'hint3'
                },
                parse: function (data) {
                    var parsed = [];
                    for (var i = 0; i < data.length; i++) {
                        parsed[parsed.length] = {
                            data: data[i],
