        LoginCallback: LoginCallback,
        //PopSkip: true
    });

    // 定义回调方法
    function LoginCallback(cbtype, data) {
        //if (cbtype == "check") {
        //    // 用户名密码的检查结果
        //    if(data.Success)
        //        window.location.reload();
        //    else
        //        alert("检查结果:" +data.Info);
        //} else 
        if (cbtype == "login") {
            if (data.Success) {
                // 登录成功
                window.location.reload();
            } 
        } else if (cbtype == "logout") {
             //登出
            if (data.Success) {
                window.location.reload();
            } 
        } else if (cbtype == "error") {
            // 服务器错误
            alert("服务器错误:" + data.Info);
        } else if (cbtype == "qq") {
            window.location.reload();
        } else if (cbtype == "weibo") {
            window.location.reload();
        } else if (cbtype == "skip") {
            alert("暂不注册成功");
        }
    }
</script>

