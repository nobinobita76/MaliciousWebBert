        var WEBURL  = 'http://www.999d.com/',
            APIURL  = 'http://user.999d.com/';

        var xhr = new XMLHttpRequest();
        var wxApi = null;
        var url = encodeURIComponent(location.href.split('#')[0]);

        xhr.open('get',WEBURL+'api/weixin/jssdk.php?url='+url,false);
        xhr.onreadystatechange = function() {
            if (xhr.readyState == 4 && /^2\d{2}$/.test(xhr.status)) {

                wxApi = JSON in window ? JSON.parse(xhr.responseText) : eval("(" + xhr.responseText + ")");
            }
        }
        xhr.send(null);

        wx.config({
            debug: false,
            appId: wxApi.appId,
            timestamp: wxApi.timestamp,
            nonceStr: wxApi.nonceStr,
            signature: wxApi.signature,
            jsApiList: ['onMenuShareTimeline',
                'onMenuShareAppMessage',
                'onMenuShareQQ',
                'onMenuShareWeibo']
        });
    </script>
