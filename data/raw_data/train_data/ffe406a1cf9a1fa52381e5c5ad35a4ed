  <script>
       function seeE() {document.getElementById('captchaImage').src='/template/image.jsp?t='+new Date().getTime();}
     </script> 
  <script type="text/javascript">
	$(function(){
		$("#booking_now").click(function(){
			$("#intention_date").val($("#intention_date2").val());
			$("#adult_num").val($("#adult_num2").val());
			$("#children_num").val($("#children_num2").val());
			$("#order").show();

		});
			$("#close").click(function(){
		$("#order").hide();
	})
	})

	function checkform() {
		var fd_leavedates1 = $("#fd_leavedates1").val();//门市
          var intention_date=$("#intention_date").val();//团期
		  var captcha=$("#captcha").val();//验证码
				if (intention_date==""||intention_date==0){
			alert("请选择团期！");
			return false;
		}
		if(fd_leavedates1==""||fd_leavedates1==0){
		    alert("请选择离你最近门市");
			return false;
		}
                if(document.validateForm.fd_name1.value.length==0) {
                    alert("姓名不能为空！")
                    document.validateForm.fd_name1.focus();
                    return false;
                }
                if(document.validateForm.fd_mobile1.value.length==0) {
                    alert("您的手机号不能为空！")
                    document.validateForm.fd_mobile1.focus();
                    return false;
                }
                if(document.validateForm.fd_mobile1.value.length!=11)
                {
                    alert("您的手机号不正确哦！")
                    document.validateForm.fd_mobile1.focus();
                    return false;
                }
                var fd_mobile1=document.validateForm.fd_mobile1.value;
                var reg0=/^13\d{5,9}$/; //130--139。至少7位
                var reg1=/^15\d{5,9}$/; //15至少7位
                var reg2=/^18\d{5,9}$/; //18

                var my=false;
                if (reg0.test(fd_mobile1))my=true;
                if (reg1.test(fd_mobile1))my=true;
                if (reg2.test(fd_mobile1))my=true;

                if (!my){
                    alert("您的手机号不正确哦")
                    document.validateForm.fd_mobile1.focus();
                    return false;
                }

				if(captcha==""){
		alert("验证码不能为空");
		return false;
	}

            }
</script>   
 </body>
</html>