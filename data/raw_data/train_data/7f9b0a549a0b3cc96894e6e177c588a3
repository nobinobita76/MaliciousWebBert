<script>
jQuery(function(){
_avatar = jQuery('#top-hero li.avatar');
_data = _avatar.find('.level-data');
_btn = _data.find('.J_btnClose');

_avatar.click(function(event){
event.stopPropagation();
_thisData = jQuery(this).find('.level-data');
if(jQuery(this).hasClass('hover')){
jQuery(this).removeClass('hover');
_thisData.hide();
}else{
_avatar.removeClass('hover');
_data.hide();
jQuery(this).addClass('hover').find('.level-data').show();
}
});

_btn.click(function(){
_avatar.removeClass('hover');
_data.hide();
return false;
});

jQuery(document).click(function(event){
_avatar.removeClass('hover');
_data.hide();
});


});
</script>
