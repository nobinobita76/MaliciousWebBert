requirejs(['jquery'],function($){
$('.leftnav_bg li:not(.ln_04) a').click(function(e){
	  if($(this).hasClass('active'))return;
	  $('.leftnav_bg li a').removeClass('active');
	  $(this).addClass('active');
})
$('.leftnav_bg li a').hover(function(e){
	  $(this).addClass('hover');
},function(){
	  $(this).removeClass('hover');
	  }
)	
});

