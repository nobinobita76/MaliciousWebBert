    $(".propose_box ul li").mouseover(function(){
      $(this).addClass("active").siblings().removeClass("active");
       $(".searchlist .recomm_list").eq($(".propose_box ul li").index(this)).addClass("active").siblings().removeClass("active")
    })


function get_the_top(o){
	var iTimer=	null;
	var obj_top=$(o).offset().top;
	if(typeof $('#show_result').get(0)=='object'){
		$('html,body').scrollTop(obj_top);
	}else{
		iTimer=setTimeout(function(){
			$('html,body').animate({scrollTop:obj_top},'150');
		},10);
	}
};
