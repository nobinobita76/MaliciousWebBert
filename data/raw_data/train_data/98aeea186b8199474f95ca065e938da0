requirejs(['jquery'],function($){
	$('.energy_nav li:not(.ln_04) a').click(function(e){
		if($(this).hasClass('active'))return;
		$('.energy_nav li a').removeClass('active');
		$(this).addClass('active');
	})
	$('.energy_nav li a').hover(function(e){
		$(this).addClass('hover');
	  },function(){
		  $(this).removeClass('hover');
		  }
	  )	
});
	
