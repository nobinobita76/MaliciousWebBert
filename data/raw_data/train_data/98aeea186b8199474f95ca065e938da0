<script>
//导航
$(function(){
	if(window.screen.width > 1360) {
		var oS1=$(document).scrollTop();
		var oH1=$(window).height();
		if(oS1<510){
			$('.leftnav').hide();
		}else{
			$('.leftnav').show();
		};
		$(window).bind('scroll',function(){
			var oS=$(document).scrollTop();
			var oH=$(window).height();
			if(oS<510){
				$('.leftnav').hide();
			}else{
				$('.leftnav').show();	
			}
		});
	}else{
	
		$(".leftnav").hide();
	}
})
//关注功能
$(function(){
	(function(){
	    var types = [0,1];
	    setCookie("cartools_"+types[0],"",600,'.xcar.com.cn');
	    setCookie("cartools_"+types[1],"",600,'.xcar.com.cn');
	})()
	//关注所有车辆初始化判断
	function carToolInitGz(){
	   var uid=getCookie('_discuz_uid');
	   if (uid && uid!=null)
	   {
	       $.getJSON("http://my.xcar.com.cn/interface/cms/get_user_wantcar.php?uid="+uid+"&callback=?",function(msg){

	           for (var i in msg.data){
	              $("#at_car_"+msg.data[i].pserid).undelegate("click");
	              $("#at_car_"+msg.data[i].pserid).addClass("btn_right_active").html("已关注").attr("title","已关注");
	           }
	       })
	   }
	}
	//carToolInitGz();
	
	//所有关注按钮点击事件
	$("ul.clearfix").delegate("a[id^=at_car_]",'click',function(e){
		var e = e || window.event;
		at_car($(this).attr('id').replace("at_car_",""),$(this));
		e.preventDefault();
	});
	
	//关注按钮
	function at_car(data,ele){
		
		if(ele.attr('class').indexOf("btn_right_active") > -1){
			return;
		}
		var carurl = "http://hezuo.xcar.com.cn/news/attentionv2.php?pserid=";
		var isUrl = carurl + data +'&action=isguanzhu&c=insert&callback=?&rnd=' + Math.random();
		uid=(typeof(uid)=="undefined")?getCookie("_discuz_uid"):uid;
		if(null == getCookie("_discuz_uid")){
			$("#at_car_"+data).xLoginBox({'show':true,func : function(){
					carToolInitGz.call(null);
					$.getJSON(isUrl,{},function(msg){
                        //console.log(msg.code);
                         if (msg.code==1||msg.code==2){
                             $("a#at_car_"+data).addClass("btn_right_active").html("已关注").attr("title","已关注");
                             ele.parent(".list_btn").find(".attentionHover").eq(0).hide();
                             ele.parent(".list_btn").find(".attentionHover").eq(1).show();
                             ele.parent(".list_btn").find(".attentionHover").eq(2).hide();
                             ele.unbind('click');
                        }else if (msg.code==30){
                             ele.parent(".list_btn").find(".attentionHover").eq(0).hide();
                             ele.parent(".list_btn").find(".attentionHover").eq(1).hide();
                             ele.parent(".list_btn").find(".attentionHover").eq(2).show();

                        }
                   })				
			}})
			return false;
		}
		$.getJSON(isUrl,{},function(msg){
			 if (msg.code==1||msg.code==2){
				$("a#at_car_"+data).addClass("btn_right_active").html("已关注").attr("title","已关注");
				ele.parent(".list_btn").find(".attentionHover").eq(0).hide();
				ele.parent(".list_btn").find(".attentionHover").eq(1).show();
				ele.parent(".list_btn").find(".attentionHover").eq(2).hide();
				ele.unbind('click');
			}else if (msg.code==30){
				ele.parent(".list_btn").find(".attentionHover").eq(0).hide();
				ele.parent(".list_btn").find(".attentionHover").eq(1).hide();
				ele.parent(".list_btn").find(".attentionHover").eq(2).show();
	
			}
		})
	}
	
	//所有关注按钮点击事件
	var attentions = $("a[id^=at_car_]");
	/*attentions.live('click',function(e){
	    var e = e || window.event;
	    at_car($(this).attr('id').replace("at_car_",""),$(this));
	    e.preventDefault();
	});*/
	attentions.live('mouseover',function(){
	     $(".attentionHover").hide();
	     if($(this).attr('class').indexOf('btn_right_active') == -1){
	         //$(this).parent(".list_btn").find(".attentionHover").eq(0).show();
	     }
	})
	//关注浮上移除效果  
	var url_floor = $('.models_right li');
	var list_btn = url_floor.find(".list_btn");
	list_btn.live('mouseover',function(){
		 var obj = $(this).parents(".list_btn");
	     //obj.find(".attentionHover").eq(0).show();
	     obj.find(".attentionHover").eq(1).hide();
	     obj.find(".attentionHover").eq(2).hide();
	})
	
	list_btn.live('mouseleave',function(){
	    var obj = $(this);
	    obj.parents().find(".attentionHover").eq(0).hide();
	    obj.parents().find(".attentionHover").eq(1).hide();
	    obj.parents().find(".attentionHover").eq(2).hide();
	})
	
	 url_floor.delegate(".killme","click",function(){
	     $(this).parents(".attentionHover").hide();
	 })
	
	
	$(".killme").live('click',function(){
		$(this).parents(".attentionHover").hide();
	})
 
})

$(function(){
    //关注所有车辆初始化判断
    function carToolInitGz(){
        var uid=getCookie('_discuz_uid');
        if (uid && uid!=null)
        {
            $.getJSON("http://my.xcar.com.cn/interface/cms/get_user_wantcar.php?uid="+uid+"&callback=?",function(msg){
                for (var i in msg.data){
                   $("#at_car_"+msg.data[i].pserid).unbind("click");
                   $("#at_car_"+msg.data[i].pserid).addClass("btn_right_active").html("已关注").attr("title","已关注");
                }
            })
        }
    }
    carToolInitGz();
	//搜索begin 
	
	
	//clicka();
	
	function zhMacth(selectStr, attrName, jsonArray)
    {
        $(selectStr).each(function() {
            var pbValue = $(this).attr(attrName);
          
            if ($.inArray(pbValue, jsonArray) >= 0) {
                $(this).removeClass("fcb5");
            } else {
                $(this).addClass("fcb5");
                $(this).unbind('click');
            }
        })
    }
	    
	//显示置灰
	function searchCondtions(jsonData,counter,boxer){
		var typeArray = jsonData.energy;
		var pbArray = jsonData.pb;
		var priceArray = jsonData.price_qj;
		var type_a = $(".nev_type").find("li");
				
		//品牌
		//(pbArray);
		
	    /*if(typeof(pbArray) == "undefined"){
       		$(".choose_linehe a ").removeClass('fcb5');
       		return;
        }*/
//        if(typeArray)
		if( boxer =='pb' && counter == 1){
			//不置灰
			('不置灰');
			$(".choose_linehe a").removeClass("fcb5");
	        
		}else{
			//置灰  
			$(".choose_linehe a").each(function() {
	            var pbValue = $(this).attr("pb");
	          
	            if ($.inArray(pbValue, pbArray) >= 0) {
	                $(this).removeClass("fcb5");
	            } else {
	                $(this).addClass("fcb5");
	                $(this).unbind('click');
	            }
	        })
		}
       
        //动力类型
        if(boxer == 'type' && counter == 1){
        	$(".nev_type a").removeClass("fcb5");
         }else{
        /*	if(typeof(typeArray) == "undefined"){
           		$(".nev_type a ").removeClass('fcb5');
           		return;
            }*/
    		$(".nev_type a").each(function() {
                var typeValue = $(this).attr("type");
                if ($.inArray(typeValue, typeArray) >= 0) {
                    $(this).removeClass("fcb5");
                } else {
                    $(this).addClass("fcb5");
                    $(this).unbind('click');
                }
            })
     }
        
        //价格
        //(priceArray);
        if(boxer == 'price' && counter == 1){
        	$(".choose_price a ").removeClass("fcb5");
            }else{
        	/*if(typeof(priceArray) == "undefined"){
           		$(".choose_price a ").removeClass('fcb5');
           		return;
            }*/
            $(".choose_price a ").each(function(){
            	var priceValue = $(this).attr("price");
                if ($.inArray(priceValue, priceArray) >= 0) {
                    $(this).removeClass("fcb5");
                } else {
                    $(this).addClass("fcb5");
                    $(this).unbind('click');
                }
            })
        }

        //续航里程  
        var pedmArray = jsonData.pedm;
        zhMacth($(".xuhang dd").find('a'), 'pedm', jsonData.pedm)
		
	}
	//获取筛选条件
	function getCondition(){
		var data = {};
		data.type = $('.dongli').find('a.choose_active').attr('type') || 0;
		data.price = $('.choose_price').find('a.choose_active').attr('price') || 0;
		data.pb = $('.nev_pinp').find('a.choose_active').attr('pb') || 0 ;
		data.pedm  = $('.xuhang').find('a.choose_active').attr('pedm') || 0;
		
		return data;
	}
	
	//显示条件
	function showCondition(){
		var jsonData = getCondition();
		var price = jsonData.price;
		var pb = jsonData.pb;
		var type = jsonData.type;
		var pedm = jsonData.pedm;
		var showPos = $(".choose_condition").find('a');
		var showCon = $(".choose_condition");
		if(type > 0){
			var showType = $(".nev_type").eq(0).find("a[type='"+type+"']").text();
			showPos.eq(0).show().html(showType+'<span></span>').attr('type',type);
		}else{
			showPos.eq(0).hide();
		}
		if(price > 0){
			var showPrice = $(".choose_price").eq(0).find("a[price='"+price+"']").text();
			showPos.eq(1).show().html(showPrice+'<span></span>').attr('price',price);
		}else{
			showPos.eq(1).hide()
		}
		if(pb > 0){
			var showPb = $(".nev_pinp").eq(0).find("a[pb='"+pb+"']").text();
			showPos.eq(2).show().html(showPb+'<span></span>').attr('pb',pb);
			
		}else{
			showPos.eq(2).hide();
		}
		if(pedm > 0){
			var showPe = $(".xuhang").eq(0).find("a[pedm='"+pedm+"']").text();
			showPos.eq(3).show().html(showPe+'<span></span>').attr('pedm',pedm);
			
		}else{
			showPos.eq(3).hide();
		}
		
		$("div .choose_condition").show();
		//所有选择条件都不显示
		if($(".choose_condition a:visible").length == 0){
			
			$("div .choose_condition").hide();
			$("#show_result").hide();
		}
	}
	showCondition();
	//去除筛选条件
	function clickCond(_this){
		if(_this.css('display') == 'inline-block'){
			if(_this.attr('type')){
				var _val = _this.attr('type');
				_this.removeAttr('type');
				$(".dongli").find('a.choose_active').removeClass('choose_active');
				$(".dongli").find('a').removeClass('fcb5'); 
				$(".dongli a").eq(0).addClass('choose_active');
				
			}else if(_this.attr('price'))
			{
				var _val = _this.attr('price');
				_this.removeAttr('price');
				$(".jiage").find('a.choose_active').removeClass('choose_active');
				$(".jiage a").eq(0).addClass('choose_active');
			}else if(_this.attr('pb'))
			{
				var _val = _this.attr('pb');
				_this.removeAttr('pb');
				$(".pinp").find('a.choose_active').removeClass('choose_active');
				$(".pinp a").eq(0).addClass('choose_active');
			}else if(_this.attr('pedm'))
			{
				var _val = _this.attr('pedm');
				_this.removeAttr('pedm');
				$(".xuhang").find('a.choose_active').removeClass('choose_active');
				$(".xuhang a").eq(0).addClass('choose_active');
			}
		}
	}
	
})

//车型下拉框js

$(document).ready(function(){
	/* 下拉插件JS */
	$(".selectbox").hover(
	  function () {
		$(this).addClass("selectbox_hover");
	  },
	  function () {
		$(this).removeClass("selectbox_hover");
	  }
	);
	$(".option li").hover(
	  function () {
		$(this).addClass("active");
	  },
	  function () {
		$(this).removeClass("active");
	  }
	);
	$(".selecttxt").click(function(event){
		//停止冒泡
		event.stopPropagation();

		/* 定义下拉内容最大高度 */
		if($(this).siblings(".option").height()>=300){
			$(this).siblings(".option").height(300);
		}else{
			$(this).siblings(".option").height('auto');
		}

		//判断当前状态如果打开则关闭
		if($(this).parent().is('.selectbox_on')){
			$(this).parent().find(".option").hide();
			$(this).parent().removeClass("selectbox_on");
			return;
		}

		//普通点击事件
		$(".selectbox").removeClass("selectbox_on");
		$(this).parent().addClass("selectbox_on");

		$(".selectbox").find('.option').hide();
		$(this).parent().find(".option").show();
	});


	/* 点击页面其他地方，清除当前下拉框状态 */
	$(document).click(function(event){
		var eo=$(event.target);
		if($(".selectbox").is(":visible") && eo.attr("class")!="option" && !eo.parent(".option").length)
		$('.option').hide();
		$(".selectbox").removeClass("selectbox_on");
	});
	/*赋值给文本框*/
	/*$(".option li").not(".onblur").click(function(){
		var value=$(this).children("a").text();
		$(this).parents(".option").siblings(".selecttxt").children(".selectem").text(value);
		$("#select_value").val(value);
		$('.option').hide();
		$(".selectbox").removeClass("selectbox_on");
		$(this).parents(".option").siblings(".selecttxt").css("color","#666");
	 })*/
	 /* 下拉插件JS end */
});

//loationinfo 当地降价信息
var select_city = {
		//init初始化
		cheshi_city_id:0,
		cheshi_city_name:'',
		cheshi_province_id:0,
		cheshi_province_name:'',
		cheshi_bbs_id:'',
		cheshi_bbs_name:'',
		initselect:false,

		//获取地区
		getUserLocation : function(cityid){
			if(__ipLocationInfo.city_id){
				return __ipLocationInfo.city_id;
			}else{
				return 475;
			}
		},

		//获取经销商推荐信息
		getDealerRecInfo : function(city_id){
			city_id = city_id ? city_id : 475;

               var pserids = new Array();
               $('a[id^=at_car_]').each(function(){
                   var pserid = $(this).attr('id').replace("at_car_","");
                   if(pserid)
                   {
                       pserids.push(pserid);

                   }
               });

               var ids = pserids.join();

               $.getJSON(
                   'http://dealer.xcar.com.cn/dealerdp_index.php?r=inner/priceCenter/psLow/parnter/2001/city_id/'+city_id+'/pserid/'+ids+'/flag/0&callback=?',function(datas){
                    if(pserids.length>1){
                        for(var pserid in datas)
                        {
                           var $_self = $('#at_car_' + pserid);
                           var data = $_self.attr('id').replace("at_car_","");
                           if(data == pserid)
                           {
                               if(datas[pserid]['ps_info'] !==null){
                                    var low = datas[pserid]['ps_info']['preferential'];
                                    if(Number(low) > 0)
                                    {                                        
                                         $_self.parent().parent().find('.list_info').append('<span class="list_drop">' + Number(low) + '万</span>');
                                    }
                               }
                           }
                        }
                    }else{
                        var $_self = $('a[id^=at_car_]');
                        if(datas['ps_info'] !==null){
                            var low = datas['ps_info']['preferential'];                            
                            if(Number(low) > 0)
                            {
                                $_self.parent().parent().find('.list_info').append('<span class="list_drop">' + Number(low) + '万</span>');
                            }
                        }
                    }
               })
		},
		getCityInfo : function(cityid){
			var tempid;
			if(cityid){
				tempid = cityid;
			}else{
				tempid = select_city.getUserLocation();
			}

			if(typeof tempid=='undefined' || tempid==0){
				select_city.cheshi_city_id=475;
			}else{
				select_city.cheshi_city_id=tempid;
			}

			select_city.getDealerRecInfo(tempid);

		},

};

function resWriteCheshiname(){
  if(__ipLocationInfo.city_id != 475){
		var _str = __ipLocationInfo.city_name+"车市";
		document.getElementById("xHeaderIplocation").innerHTML ='<a href="'+__ipLocationInfo.url+'" target="_blank">'+_str+'</a>';
     	document.getElementById("xHeaderIplocation").parentNode.style.width=(_str.length * 18) + 60 + "px";
  }
}
var _ipLocationInfo = getCookie("_locationInfo_");
if(_ipLocationInfo){
	__ipLocationInfo=_ipLocationInfo = eval("("+_ipLocationInfo+")");
	__ipLocationInfo.city_name = decodeURIComponent(_ipLocationInfo.city_name);
	__ipLocationInfo.url=_ipLocationInfo.url;
	setTimeout(function(){
		select_city.getCityInfo();
	},1000)

	delete _tmp,_ipLocationInfo;
}else{
	  loadScript("http://tools.xcar.com.cn/ip2city/ip2getcity.php?_t=json&s="+Date.parse(new Date()),function(){
		_ipLocationInfo = __ipLocationInfo;
		__ipLocationInfo.url=_ipLocationInfo.url[0];
		var _tmp='{url:"'+__ipLocationInfo.url+'",city_id:"'+__ipLocationInfo.city_id+'",province_id:"'+__ipLocationInfo.province_id+'", city_name:"'+encodeURIComponent(__ipLocationInfo.city_name)+'"}';
		setCookie("_locationInfo_",_tmp,1000*60*6,"/",".xcar.com.cn");
		resWriteCheshiname();
			setTimeout(function(){
				select_city.getCityInfo();
			},100)
	  delete _tmp,_ipLocationInfo;
	 });
}


