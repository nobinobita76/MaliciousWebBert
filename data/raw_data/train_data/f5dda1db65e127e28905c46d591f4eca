        $('.zxask-answer-loadapp').on('click',function(){
            if(recordptag.indexOf('2_3_10_1207') === -1){
                (typeof clickStream !== 'undefined') && clickStream.getCvParams('2_3_10_1207');
                recordptag.push('2_3_10_1207');
            }
        });
        $(document).scroll(function(){
            canSeeIt('2_3_10_1206');
        });
        // 曝光ptag
        function canSeeIt(ptag){
            // 当banner显示时记录曝光ptag
            // 当页面把banner滚到可视区时
            if($(document).scrollTop()+$(window).height()>$('.zxask-answer-loadapp').offset().top+$('.shade-click').height()){
                if(recordptag.indexOf(ptag) === -1){
                    (typeof clickStream !== 'undefined') && clickStream.getCvParams(ptag);
                    recordptag.push(ptag);
                }
            }
        }
});

</script>
