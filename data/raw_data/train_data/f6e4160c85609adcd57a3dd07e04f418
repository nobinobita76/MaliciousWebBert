<script type="text/javascript" src="http://p.inte.sogou.com/wap/js/wp.js"></script><script type="text/javascript">
$(function(){
	$('.my_bottom .close').bind('click',function(event) {
		$('.my_bottom').css('display','none');
	});
	$('.Toquick_open').click(function(){
		if($(this).hasClass('sel')){
			$('.quick_loading').show();
			$('.bg').show();
		}else{
			$('.quick_loading').show();
			$('.bg').show();
		}
	});
	$('.toapk_open').click(function(){
		$('.FanYe_Area').hide();
		location.href = "/Home/Book/content/readapk/1/bookid/267538.html"+"&menuid="+(parseInt($('#next_content').data('menu_id'))-1);
	});
});
$(function(){
	$('#next_content').data('menu_id',13);
	$('.page_txt').val(parseInt($('#next_content').data('menu_id')));
	$('.page_btn').click(function(){
		location.href = "/content/"+267538+"/"+$('.page_txt').val()+".html?useridshenma=";
	});
	$('.js_page_up').click(function(){
		location.href = "/content/"+267538+"/"+(12-1)+".html?useridshenma=";
	});
	$('.js_page_down').click(function(){
		location.href = "/content/"+267538+"/"+(12+1)+".html?useridshenma=";
	});	
	next_content(13);
});
function next_content(menuid){
	var url = "/Home/Book/content/bookid/267538.html";
	$.ajax({
		type: "POST",
		url: url,
		data: "menuid="+menuid+"&format=json",
		dataType : 'json',
		success: function(data) {
			var dataobj = eval(data);
			if (dataobj.status == 1) {
				$('#next_content').data('status',data.status);
				$('#next_content').data('title',data.title);
				$('#next_content').data('menu_id',data.menu_id);
				$('#next_content').data('menu_name',data.menu_name);
				$('#next_content').data('content',data.content);
				$('.page_txt').val(data.menu_id);
			} else {
				$('#next_content').data('status',data.status);
				$('#next_content').data('content',data.content);
				$('#next_content').data('title','');
				$('#next_content').data('menu_name','');
				$('.page_txt').val(parseInt($('#next_content').data('menu_id')));
			}
		}
	});
}
</script>
