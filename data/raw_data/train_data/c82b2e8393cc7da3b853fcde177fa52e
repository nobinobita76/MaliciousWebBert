<script type="text/javascript">
var zoom = document.cookie.match(/zoom=([^=;]*)(?=;?)/);
var chapterAElements = document.getElementById('answer-chapters').getElementsByTagName('a');
var length = chapterAElements.length;
var pictureIndex;
$(document).ready(function(){
    if(zoom&&zoom[1]=='in') {
        document.cookie = 'zoom' + "=; expires=Thu, 01 Jan 1970 00:00:00 GMT";
        zoomPicture($("#answer .minifier")[0]);
        
    }
    $("#answer .minifier").click(function(){
        zoomPicture(this);
    });
});
function zoomPicture(node) {
    var lightBoxDivElement = document.createElement('div');
    lightBoxDivElement.className = 'light-box';
    lightBoxDivElement.innerHTML = '<div class = "tools"><a id = "rotate-clockwise" class = "rotate" href = "javascript:;">左转</a>'
                                 +'<a id = "rotate-anticlockwise" class = "rotate" style = "margin:0 0.4em;" href = "javascript:;">右转</a>'
                                 +'<a class = "close"  href = "javascript:;">关闭</a></div>' 
                                 + '<div class = "content"><a id = "left-button" class = "arrow" href = "javascript:;"></a><a id = "right-button" class = "arrow" href = "javascript:;"></a><div class="lightBox"><div class="lightBox1"></div></div></div>';
    lightBoxDivElement.style.zIndex = 1;
    var imgElement = document.createElement('img');
    imgElement.src = node.src;
    imgElement.alt = node.alt + ' 大图';
    lightBoxDivElement.getElementsByTagName('div')[1].appendChild(imgElement);
    document.getElementsByTagName('body')[0].insertBefore(lightBoxDivElement,document.getElementById('logo-intro').nextSibling.nextSibling.nextSibling);
    var angleNum=0;
    $("a.rotate").click(function() {
        var direction = $(this).attr("id");
		direction=='rotate-anticlockwise'?angleNum+=90:angleNum-=90;
		var roatt="rotate("+angleNum+"deg)";
		$(imgElement).css({
			"transform":roatt,
			"-ms-transform":roatt, /* Internet Explorer */
			"-moz-transform":roatt, /* Firefox */
			"-webkit-transform":roatt, /* Safari 和 Chrome */
			"-o-transform":roatt /* Opera */
		});
	});
    $('.light-box a.close').click(function(){
        document.getElementsByTagName('body')[0].removeChild(lightBoxDivElement);
        return false;
    });
    for(var i=0;i<length;i++) {
        if(chapterAElements[i].className=="selected") {
            pictureIndex = i;
            break;
        }
    }
    if(pictureIndex==0) document.getElementById('left-button').style.visibility = 'hidden';
    if(pictureIndex==length-1) document.getElementById('right-button').style.visibility = 'hidden';
    $('.light-box a.arrow').click(function(){
        switch(this.id) {
            case 'left-button': {
                --pictureIndex;
                break;
            }
            case 'right-button': {
                ++pictureIndex;
                break;
            }
            default:break;
        }
        var date = new Date();
        date.setTime(date.getTime()+3600*1000);
        document.cookie = 'zoom=in;expires=' + date.toUTCString();
        window.location = chapterAElements[pictureIndex].href;
        
    });
    document.body.onkeydown = function(e) {
        var currentKey,event = e || window.event;
        currentKey = event.keyCode || event.which || event.charCode;
        var date = new Date();
        date.setTime(date.getTime()+3600*1000);
        if(pictureIndex>0&&currentKey==37) {
            --pictureIndex;
            document.cookie = 'zoom=in;expires=' + date.toUTCString();
            window.location = chapterAElements[pictureIndex].href;
        } else if (pictureIndex<length-1&&currentKey==39) {
            ++pictureIndex;
            document.cookie = 'zoom=in;expires=' + date.toUTCString();
            window.location = chapterAElements[pictureIndex].href;
        }
        if(currentKey==37||currentKey==39) return false;
    }       
}



$(function(){
     $(document).on('click','#answer .minifier,#answer .zoom',function(){
     if($(this).attr('class')=='minifier') $(this).attr('class','zoom');
     else $(this).attr('class','minifier');
		 fangda();
     });

     var angleNum=0;
     $(".rotate").click(function(){
     var action=$(this).attr("do");
     action=='r'?angleNum+=90:angleNum-=90;
     var roatt="rotate("+angleNum+"deg)";
     $("#answer img:eq(0)").css({
     "transform":roatt,
     "-ms-transform":roatt, // Internet Explorer
     "-moz-transform":roatt, // Firefox
     "-webkit-transform":roatt, // Safari 和 Chrome
     "-o-transform":roatt // Opera
     });
     });
          });
//setTimeout('$.get(DT_STATIC+"ajax/index.php?cat=login&mod=record_chapterid&bookid=178725&chapterid=2245155")',30000);//记录页面停留超过30秒的
</script>
<style>
.wrapbox{
        position:relative; 
		clear:both;
}
.textbox{
    filter: alpha(opacity=70);
    -moz-opacity: 0.7;
    opacity: 0.7;
}
</style>
<script>
function textbox(obj,text,top,left,fontsize,color){
    obj.append("<div class='textbox'>"+text+"</div>");
	$('.textbox').css({'position':'absolute','top':top+'px','left':left+'px','font-size':fontsize+'px','color':color});
}


function shuiyin(obj){
    obj.wrap("<div class='wrapbox'></div>");
   var wrapbox = $('.wrapbox');
   textbox(wrapbox,'请关注公众号“青夏精英家教网”',100,200,12,'red');
}
function fangda(){
     var imgs1 = $('.lightBox1');
     textbox(imgs1,'请关注公众号“青夏精英家教网”',100,200,12,'red');
}

 $(function(){
    var imgs = $('.minifier');
     shuiyin(imgs);
	 fangda();
	 
 });
    
</script>
