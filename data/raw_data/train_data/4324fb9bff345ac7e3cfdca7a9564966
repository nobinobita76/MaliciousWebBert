  data["landingAsinColor"] = 'initial';
  data["shouldApplyResizeFix"] = false;
  data["lazyLoadExperienceDisabled"] = true;
  data["lazyLoadExperienceOnHoverDisabled"] = false;

  return data;
});

</script>



