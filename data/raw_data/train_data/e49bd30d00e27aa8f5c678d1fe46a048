	var pid,floor,receiveuid="wtshr8";

	function setpf(obj){
		pid   = obj.getAttribute('pid'); 
		floor = "36";
		receiveuid = obj.getAttribute('receiveuid');
		document.getElementById('textarea1').value = "@"+receiveuid+":";
		var checklogin = "0";
		if(checklogin>0){
			var obj = document.getElementById('textarea1');
			obj.focus();
			var len = obj.value.length;
		    if (document.selection) {
		        var sel = obj.createTextRange();
		        sel.moveStart('character',len);
		        sel.collapse();
		        sel.select();
		    } else if (typeof obj.selectionStart == 'number' && typeof obj.selectionEnd == 'number') {
		        obj.selectionStart = obj.selectionEnd = len;
		    }
		}
	
	}
	//判断是否为空
	function checkIsnull(str){
		var reg = /^\s*$/;
		var result = str.match(reg);
		if(result){return true;}else{return false;}
	}
	function dosubmit(){
		var uid 		= '0';
		var username 	= '';
		var content  	= document.getElementById('textarea1').value;
		var fid      	= '7';
		var tid      	= '6494528';
		var subject     = '想问下姐妹们，为什么淘宝专柜代购比商场便宜很多？';
		var dateline    = '1321885389';
		var image_path 	= 0;
		if(undefined  == pid && undefined  == floor){
			var apid = 0;
			var afloor = 0;
		}else{
			var apid      = pid;
			var afloor    = floor;
		}
		if(checkIsnull(content)){
			alert('回复内容不能为空!');
			return;	
		}
		$.ajax({
			url :'/ajax/wapnew.php?hf=1&m='+Math.random() ,
			type : 'post',
			dataType : "json",
			data : {'uid':uid,'username':username,'content':content,'fid':fid,'tid':tid,'image_path':image_path,'pid':apid,'floor':"36",'receiveuid':receiveuid,'subject':subject,'dateline':dateline},
			success : function(result){
				if( result.code == 1){
					alert('回复成功!');
					window.location.href = "http://m.55bbs.com/bbs_6494528_4/";
					//document.getElementById('textarea1').value ='';
					return;
				}else if(result.code == 2 ){
					alert('回复失败!');return;
				}else if(result.code == 3){
					alert('该帖已关闭!');return;
				}else if(result.code == 4){
					alert('您已经被禁言!');return;
				}
			}
		});
	}
	//获取id 对象
	function getobj(id){
		return document.getElementById(id);
	}
	function getHtmlObj(htm){
		return document.getElementsByTagName(htm);
	}
	if(0){
		var submit = getobj("submit");
		submit.onclick=dosubmit;
	}
