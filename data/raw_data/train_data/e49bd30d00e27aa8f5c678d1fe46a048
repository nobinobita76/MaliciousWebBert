		jump.addEventListener('change',function(){
			var jumpurl = "/bbs_6494528_[page]/";
			var jumppage = jump.value;
			if( jumppage > 4 ||  jumppage<=0 ){
				alert("输入页数不对!");
				return false;
			}
			if(jumppage == curpage){
				return false;
			}
			jumpurl = jumpurl.replace('[page]',jumppage);
			window.location.href=jumpurl;
		});
	}
	
