	// 提示弹窗
	function popTips(t){
		var self_class = t.className,links = t.getAttribute('data-url'),popLayer = document.getElementById('pop-tips');
		var gotoUrl = function(u){
			popLayer.style.display = 'block';
			setTimeout(function(){
				popLayer.style.display = 'none';
				location.href=u;
			},2E3);	
		}
		switch(self_class){
			case 'poster':
				popLayer.innerHTML = '\u53EA\u770B\u697C\u4E3B\u6A21\u5F0F';
				gotoUrl(links);
				break;
			case 'allposter':
				popLayer.innerHTML = '\u67E5\u770B\u5168\u8D34\u6A21\u5F0F';
				gotoUrl(links);
				break;	
		};
	}
