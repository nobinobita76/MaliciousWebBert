          <tr> 
           <td height="18">db file single write</td> 
           <td>db single block write</td> 
           <td>User I/O</td> 
          </tr> 
          <tr> 
           <td height="18">log buffer space</td> 
           <td>log buffer full C LGWR bottleneck</td> 
           <td>Configuration</td> 
          </tr> 
          <tr> 
           <td height="18">log file parallel write</td> 
           <td>log file redo write</td> 
           <td>System I/O</td> 
          </tr> 
          <tr> 
           <td height="18">log file sequential read</td> 
