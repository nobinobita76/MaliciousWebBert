	$(document).ready(function(e) {
		$(".nav_dl").css("left","-"+($(".logo").width()+10)+"px");
        $(".nav li").hover(function(){
			if($(this).find("dl").length>=1)
			{				
				$(this).find(".first_a").addClass("nav_li_a");
