           <li style="vertical-align: middle;height:22px; line-height:22px;" id="cbFilter"> <input type="checkbox" value="0" id="cbEmpty" style="vertical-align:middle;" /><label for="cbEmpty">空</label>&nbsp;&nbsp;&nbsp;<input type="checkbox" value="0" id="cbJump" checked="checked" style="vertical-align:middle;" /><label for="cbJump">跳过</label> <script type="text/javascript">
                                  var cbFilter=document.getElementById("cbFilter");
                                  var filterInputs=cbFilter.getElementsByTagName("input");
                                  var isFiterEmpty=0; var isFiterJump=1;
                                  filterInputs[0].checked=isFiterEmpty;
                                  filterInputs[1].checked=isFiterJump;
                                  filterInputs[0].onclick=function(){
                                      var href=window.location.href;
                                      var addData=this.checked?1:0;
                                      addData="ieec="+addData;
                                      if(href.indexOf("ieec=")>-1)
                                          href=href.replace(/ieec=\d/,addData);
                                      else{
                                          if(href.indexOf("?")==-1)
                                              href=href+"?"+addData;
                                          else
                                              href=href+"&"+addData;
                                      }
                                      window.location.href=href;
                                  }
                                  filterInputs[1].onclick=function(){
                                      var href=window.location.href;
                                      var addData=this.checked?0:1;
                                      addData="njjc="+addData;
                                      if(href.indexOf("njjc=")>-1)
                                          href=href.replace(/njjc=\d/,addData);
                                      else{
                                          if(href.indexOf("?")==-1)
                                              href=href+"?"+addData;
                                          else
                                              href=href+"&"+addData;
                                      }
                                      window.location.href=href;
                                  }
                              </script> </li> 
