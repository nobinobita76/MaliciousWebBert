       <script type="text/javascript">
   
    Array.prototype.indexOf = function (vItem) {
        for (var i = 0, l = this.length; i < l; i++) {
            if (this[i] == vItem) {
                return i;
            }
        }
        return -1;
    };
    Array.prototype.remove = function (vItem) {
        this.removeAt(this.indexOf(vItem));
        return vItem;
    };

    Array.prototype.removeAt = function (iIndex) {
        var vItem = this[iIndex];
        if (vItem) {
            this.splice(iIndex, 1);
        }
        return vItem;
    }; 
     var hideU = "";
     var hide = '';
     if (hide == "u") hideU = "&hide=u";
    var yangBen = ["性别", "年龄", "收入", "教育", "学历", "婚姻", "部门", "年级", "班级","身份","年限","职务"];
    function isSample(qText){
        var finded=false;
        for (var j = 0; j < yangBen.length; j++) {
            if (qText.indexOf(yangBen[j]) > -1) {
                finded = true;
                break;
            }
        }
        return finded;
    }
    function needUpgrad()
    {
       if(confirm("您需要升级才能使用此功能，是否立即升级？")){
         window.location.href="/register/usertype.aspx?returnurl="+ encodeURIComponent(window.location);
          if (window.event)
            window.event.returnValue = false;
       }
    }

    var prevTab = null;
    var prevLi = null;
    var divTabs = [document.getElementById("divFenLei"), document.getElementById("divJiaoCha"), document.getElementById("divQuery")];
    var divLis=["liFenLei", "liJiaoCha", "liQuery"];
  
    var tabmenu = document.getElementById("tabmenu");
    var liTabs = tabmenu.getElementsByTagName("li");
    var liDefault=document.getElementById("ctl02_ContentPlaceHolder1_ViewReport1_liDefault");
    var isCeping = "";
      var htData=new Object();
    for(var i=0;i<divLis.length;i++){
      htData[divLis[i]]=divTabs[i];
    }
    if(isCeping){
        htData["ctl02_ContentPlaceHolder1_ViewReport1_liCeping"]=document.getElementById("divFenLei");
    }
    var liCeping="ctl02_ContentPlaceHolder1_ViewReport1_liCeping";
    function changeTab(index) {
        var currentTab = htData[index];
        if(index==liCeping)
            currentTab=divTabs[0];
        else if(document.getElementById(liCeping) && prevLi==document.getElementById(liCeping) )
        {
//           var tab=3;if(index=="lijiaocha")tab=2;else if(index=="liquery")tab=1;
//           window.location="/report/"+activityid+".aspx?tab="+tab;
//           return false;
        }
        var currentLi = document.getElementById(index);
         if(currentLi==prevLi)
          return;
        currentTab.style.display = "";
        currentLi.className = "current";
       
        if (prevTab&&prevTab!=currentTab)
            prevTab.style.display = "none";
        prevTab = currentTab;
        if (prevLi)
            prevLi.className = "";
        prevLi = currentLi;
        liDefault.className="";
        var divSumData=document.getElementById("divSumData");
        if(divSumData)divSumData.style.display="none";
         var divFilter=document.getElementById("divFilter");
        if(divFilter)divFilter.style.display="none";
    }
    var activityId = "9073821";    
    var reportid = '';
    var reportName="";
    var feiNeiCond='';
    var qCond = '';
    var eCond = '';
    var divData = document.getElementById("ctl02_ContentPlaceHolder1_ViewReport1_divData"); //元数据
    var qs = divData.innerHTML.split("¤"); //题目数组
    var divDataQuery=document.getElementById("ctl02_ContentPlaceHolder1_ViewReport1_divDataQuery");
    var qsQuery=divDataQuery.innerHTML.split("¤"); //题目数组
    var reportType=-1;
    if (reportType==3 && isCeping)//分类统计
        changeTab("ctl02_ContentPlaceHolder1_ViewReport1_liCeping");
    else if(reportType==3 && !isCeping)
        changeTab('liFenLei');
    else if (reportType==2)//交叉分析
        changeTab('liJiaoCha');
    else if(reportType==1)//自定义查询
        changeTab('liQuery');
   var htJoinData=new Object(); 
   function getXmlHttp() {
    var xmlhttp;
    try {
        xmlhttp = new ActiveXObject('Msxml2.XMLHTTP');
    }
    catch (e) {
        try {
            xmlhttp = new XMLHttpRequest();
        }
        catch (e) {
            try {
                xmlhttp = new ActiveXObject('Microsoft.XMLHTTP');
            }
            catch (e) {
            }
        }
    }
    return xmlhttp;
    }
   function loadData(type){
	    if(htJoinData[type])
		  return htJoinData[type];
	    var xmlhttp = getXmlHttp();
	    var url="/Handler/getjoindata.ashx?activityId=" + activityId + "&type="+type;
        xmlhttp.open("get", url,false);
        xmlhttp.send(null);
		 var data=xmlhttp.responseText;
		htJoinData[type]=data;
        return data;
	}
    var isVip=0;
