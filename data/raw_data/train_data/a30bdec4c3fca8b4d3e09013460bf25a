                                                });

                                                $('#divComment' + id).html(sRet);
                                            } else {
                                                alert('没有找到评论');
                                                $('#divComment' + id).hide();
                                            }
                                           
                                        }
                                    },
                                    error: function () {
                                        alert('获取失败');

                                    },
                                    timeout: 15000
