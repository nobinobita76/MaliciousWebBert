                $(function() {
                    $('.btnShowComment').on('click', function() {
                        var id = $(this).data('id');

                        if ($('#divComment' + id).is(':hidden')) {
                            $('#divComment' + id).show();
                            if ($('#divComment' + id).html().trim() == '正在加载...') {
                                

                                $.ajax({
                                    dataType: 'jsonp',
                                    url: '/mob/json/weibo/weiboindex.aspx?type=1&contentid='+id,
