  <script>
    var channel = getQueryString('channel');
    channel = channel.replace(/(\?.*)/g,'');

    if (!$('.description').html()) {
        $('.description').remove();
    }

    $('#link-scheme').attr('href', $.url.scheme({
        scheme: 'Topic?id=945'+'&md5='+md5('邻居'),
        channel: channel ? channel : 'wap_topic'
    }));

    topbar({
        id: 'openapp-wrap',
        city: PAGE.sCityName,
        scheme: 'Topic?id=945'+'&md5='+md5('邻居'),
        channel: channel ? channel : 'wap_top'
    });

    $("#mod-says .item").each(function(){
        var itemhref = $(this).attr('data-href')+"?channel="+channel;
        $(this).attr('data-href',itemhref);
        $(this).find('.noteInfo').attr('href',itemhref);
    });

    new staticSays({
        wrap: $('#mod-says'),
        servDate: 1490733133
        });

    (function(){
        if(channel){
            var $a = $("a");
            $a.each(function(){
                var href = $(this).attr('href');
                if( !!href && href.indexOf("javascript:") == -1 && href.indexOf('channel=') == -1 ){
                    if(href.indexOf('?') != -1){
                        href = href + "&channel=" + channel;
                    }else{
                        href = href + "?channel=" + channel;
                    }
                    $(this).attr('href',href);
                }
            });
        }
    })();


    function getQueryString(k, s) {
        var r, t, s = s || '?';
        k = k.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
        r = new RegExp("[\\" + s + "&]" + k + "=([^&#]*)");
        t = r.exec(window.location.href);
        return (null == t) ? "": t[1];
    }
