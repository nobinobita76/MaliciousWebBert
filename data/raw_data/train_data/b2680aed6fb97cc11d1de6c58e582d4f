		//获取用户登录状态及信息后的回调方法（一定要放在footer.jsp之前）
		function callback_getUserInfo(){
			//已保存搜索初始化
			var fromSaveSearch = getParamValueFromUrlByKey("from") == "saveSearch";
			if(fromSaveSearch){
				$("#btn_saveSearch").addClass("disabled"); //如果页面从已保存搜索条件点击进入，则［保存搜索］按钮disabled
			}
			$("#btn_saveSearch").css({display:'inline-block'}); //［保存搜索］按钮显示
		}
