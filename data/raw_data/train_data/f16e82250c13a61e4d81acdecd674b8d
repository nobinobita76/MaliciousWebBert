	var t = $('.fixed').offset().top;
	var fh = $('.fixed').height();
	$(window).scroll(function(e){
		var s = $(document).scrollTop();	
		if(s > t - 10){					
			$('.fixed').css('position','fixed').css('top','10px');
		}else{
			$('.fixed').css('position','');
		}
	})
});
