<script language="javascript">

function OnShowQiGuaStyle()	//点击起卦方式，显示相应控件
{
	var		sel,i;
	
	for (i=0; i<6; i++)
	{
		if (document.all.rdoQiGua[i].checked)
		{
			sel=i; break;
		}
	}
	switch (sel)
	{
		case 0:		//自动起卦
			document.all.divOther.style.display='none';
			document.all.divTongQian.style.display='none';
			document.all.divYaoGua.style.display='none';
			document.all.divNumber.style.display='none';
			document.all.divDate.style.display='none';
			break;
		case 1:		//手工指定
			document.all.divOther.style.display='none';
			document.all.divTongQian.style.display='';
			document.all.divYaoGua.style.display='none';
			document.all.divNumber.style.display='none';
			document.all.divDate.style.display='none';
			break;
		case 2:		//电脑摇卦
			document.all.divOther.style.display='none';
			document.all.divTongQian.style.display='';
			document.all.divYaoGua.style.display='';
			document.all.divNumber.style.display='none';
			document.all.divDate.style.display='none';
			break;
		case 3:		//数字起卦
			document.all.divOther.style.display='none';
			document.all.divTongQian.style.display='none';
			document.all.divYaoGua.style.display='none';
			document.all.divNumber.style.display='';
			document.all.divDate.style.display='none';
			break;
		case 4:		//时间起卦
			document.all.divOther.style.display='none';
			document.all.divTongQian.style.display='none';
			document.all.divYaoGua.style.display='none';
			document.all.divNumber.style.display='none';
			document.all.divDate.style.display='';
			break; 
		case 5:		//其它方式
			document.all.divOther.style.display='';
			document.all.divTongQian.style.display='none';
			document.all.divYaoGua.style.display='none';
			document.all.divNumber.style.display='none';
			document.all.divDate.style.display='none';
			break;  
	}
}

function selectLiFa()	//选择公、农历，相应显示农历闰月
{
	if (document.all.rdoLiFa[0].checked)
	{
		document.all.chkRun.style.display='none';
		document.all.labRunYue.style.display='none';
	} else {
		document.all.chkRun.style.display='';
		document.all.labRunYue.style.display='';
	}	
}

function ZuGua(pind,sgua)
{
    var t1,j;
    var t2;
	
    t1=7-pind;
    for (j=0; j<3; j++) 
	{
        if (t1%2==1) 
			t2=1; 
		else 
			t2=2;
        sgua[j]=t2;
        t1=Math.floor(t1/2);
    }
    sgua[j]=-1;
}

function OtherQiGua()		//其他方式起卦
{
	var	t1,i,d;
    var f1;
    var ctmp=new Array(3);
    var	guaStr=new Array(14);
	
	ZuGua(document.all.cboGua1.selectedIndex,guaStr);
    ZuGua(document.all.cboGua2.selectedIndex,ctmp);
	for (i=0; i<3; i++) guaStr[i+3]=ctmp[i]; 
    for (i=0; i<6; i++) guaStr[i+6]=guaStr[i];
	//alert(guaStr.toString());
    for (i=0; i<6; i++) 
	{
        switch (i) {
            case 0: f1=document.all.chkYao5.checked;break;
            case 1: f1=document.all.chkYao4.checked;break;
            case 2: f1=document.all.chkYao3.checked;break;
            case 3: f1=document.all.chkYao2.checked;break;
            case 4: f1=document.all.chkYao1.checked;break;
            case 5: f1=document.all.chkYao0.checked;break;
        }
        if (f1) 
		{
            d=6-i;
            t1=(guaStr[d-1]);
            guaStr[d-1]=(t1+2);
            guaStr[d-1+6]=(3-t1);
        }
    }
	document.all.guaXiang.value=guaStr;
}

function AutoQiGua()
{
	document.all.guaXiang.value=Math.round(Math.random()*100000+100);
}

function TongQianQiGua()		//手工指定
{
	var	guaStr=new Array(14);
	var	i,t1;
	
    for (i=0; i<6; i++) 
	{
        switch(i) {
            case 0:t1=document.all.cboYao0.selectedIndex+1;break;
            case 1:t1=document.all.cboYao1.selectedIndex+1;break;
            case 2:t1=document.all.cboYao2.selectedIndex+1;break;
            case 3:t1=document.all.cboYao3.selectedIndex+1;break;
            case 4:t1=document.all.cboYao4.selectedIndex+1;break;
            case 5:t1=document.all.cboYao5.selectedIndex+1;break;
        }
        guaStr[i]=t1;
        switch(t1) 
		{
            case 3:guaStr[i+6]=2;break;
            case 4:guaStr[i+6]=1;break;
            default: guaStr[i+6]=t1; break;
        }
    }
    guaStr[12]=-1;
	document.all.guaXiang.value=guaStr;	
}

function GetNow()
{
	var		now;
	
	now=new Date();
	document.all.cboYear.selectedIndex=now.getYear()-1800;
	document.all.cboMonth.selectedIndex=now.getMonth();
	document.all.cboDay.selectedIndex=now.getDate()-1;
	document.all.cboHour.selectedIndex=now.getHours();
	document.all.cboMinute.selectedIndex=now.getMinutes();
}

function GetNow2()
{
	var		now;
	
	now=new Date();
	document.all.txtYear2.value=now.getYear();
	document.all.cboMonth2.selectedIndex=now.getMonth();
	document.all.cboDay2.selectedIndex=now.getDate()-1;
	document.all.cboHour2.selectedIndex=now.getHours();
	document.all.cboMinute2.selectedIndex=now.getMinutes();
}

function TongBuGuaDate()
{
	document.all.txtYear2.value=document.all.cboYear.selectedIndex+1800;
	document.all.cboMonth2.selectedIndex=document.all.cboMonth.selectedIndex;
	document.all.cboDay2.selectedIndex=document.all.cboDay.selectedIndex;
	document.all.cboHour2.selectedIndex=document.all.cboHour.selectedIndex;
	document.all.cboMinute2.selectedIndex=document.all.cboMinute.selectedIndex;
}
//===============================================
//铜钱摇卦
//===============================================
var		m_yaoCount=0;
var		m_timeId=0;
var		m_yao=new Array(3);

function YaoGua_Init()
{
	if (m_timeId!=0) clearInterval(m_timeId);
	m_yaoCount=0;
	document.FrmLiuYao.BtoYaoGua.value="第1次摇";
	m_timeId=0;
}

function YaoGua_Run()
{
    if (m_timeId!=0) return;
	
	m_yaoCount++;
    if (m_yaoCount==7) 
	{
		TongQianQiGua();
		document.FrmLiuYao.submit();
        return;
    }
    m_timeId=setInterval("YaoGua_Flash()",100);
}

function YaoGua_Stop()
{
    var		t1=0;
	
    if (m_timeId==0) return;
    clearInterval(m_timeId);
	m_timeId=0;
	
	t1+=m_yao[0];
    t1+=m_yao[1];
    t1+=m_yao[2];
    if (t1==0) t1=4;
    switch(m_yaoCount) 
	{
        case 1:document.all.cboYao5.selectedIndex=t1-1;break;
        case 2:document.all.cboYao4.selectedIndex=t1-1;break;
        case 3:document.all.cboYao3.selectedIndex=t1-1;break;
        case 4:document.all.cboYao2.selectedIndex=t1-1;break;
        case 5:document.all.cboYao1.selectedIndex=t1-1;break;
        case 6:document.all.cboYao0.selectedIndex=t1-1;break;
    }
    if (m_yaoCount<6)
        document.all.BtoYaoGua.value="第"+(m_yaoCount+1)+"次摇";
    else 
		document.all.BtoYaoGua.value="排盘";   	
}

function YaoGua_Flash()
{
    var		i;
    
	for (i=0; i<3; i++) 
	{
        switch(i) 
		{
            case 0:
                m_yao[0]=Math.floor(Math.random()*2);
                if (m_yao[0]==1)
                    document.all.Qian1.src="b.jpg";
                else 
					document.all.Qian1.src="z.jpg";
                break;
            case 1:
                m_yao[1]=Math.floor(Math.random()*2);
                if (m_yao[1]==1)
                    document.all.Qian2.src="b.jpg";
                else 
					document.all.Qian2.src="z.jpg";
                break;
            case 2:
                m_yao[2]=Math.floor(Math.random()*2);
                if (m_yao[2]==1)
                    document.all.Qian3.src="b.jpg";
                else 
					document.all.Qian3.src="z.jpg";
                break;
        }
    }
}

function OnOkPost()
{
	var		sel,i;
	
	for (i=0; i<6; i++)
	{
		if (document.all.rdoQiGua[i].checked)
		{
			sel=i; break;
		}
	}
	//起对应的方式
	if (sel==2)
	{
		if (m_timeId!=0) clearInterval(m_timeId);	//关闭定时器
	}
	switch (sel)
	{
		case 0:	//自动起卦，也是随机起卦
			AutoQiGua();
			break;
		case 1:	 case 2://手工指定、电脑摇卦
			TongQianQiGua();
			break;
		case 3:	//数字起卦
			break;	
		case 5:	//其它方式
			OtherQiGua();
			break;
	}	
	return true;
}

</script>
