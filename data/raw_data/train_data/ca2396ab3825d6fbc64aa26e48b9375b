	    			function playNext() {
	    				if (G_CID) {
	    					$.getJSON('http://t.tv.idol001.com/video_v2.php?callback=?&action=get_next&id=58de1ebbcd4e70911c8b46ba&cid='+G_CID, function(rep){
		    					if (rep.url_source) {
		    						$('#player_box iframe').attr('src', 'https://tv.idol001.com/flash_player.html?url='+rep.url_source);
		    						G_CID = rep.cid;
		    						$('#hjlist_related a').removeClass('on');
		    						$('#hjlist_related a').each(function(){
		    							if ($(this).data('cid') == G_CID) {
		    								$(this).addClass('on');
		    								var top = $(this).position().top;
		    								if (top > $('#hjlist_related').height()) {
		    									var newTop = top + $(this).height() - $('#hjlist_related').height();
		    									$('#hjlist_related').mCustomScrollbar("scrollTo", newTop);
		    								} else {
		    									$('#hjlist_related').mCustomScrollbar("scrollTo", 0);
		    								}
		    								
		    							}
		    						});
		    					}
		    				});
	    				} else {
	    					if (G_related.length > 0) {
	    						window.location.href = 'https://tv.idol001.com/video_pc/'+G_related[0]+'.html';
	    					}
	    				}
	    			}

	    			$('#hjlist_related a').click(function(){
	    				$.getJSON('http://t.tv.idol001.com/video_v2.php?callback=?&action=get_cid_single&id=58de1ebbcd4e70911c8b46ba&cid='+$(this).data('cid'), function(rep){
	    					if (rep.url_source) {
	    						$('#player_box iframe').attr('src', 'https://tv.idol001.com/flash_player.html?url='+rep.url_source);
	    						G_CID = rep.cid;
	    						$('#hjlist_related a').removeClass('on');
	    						$('#hjlist_related a').each(function(){
	    							if ($(this).data('cid') == G_CID) {
	    								$(this).addClass('on');
	    								var top = $(this).position().top;
	    								if (top > $('#hjlist_related').height()) {
	    									var newTop = top + $(this).height() - $('#hjlist_related').height();
	    									$('#hjlist_related').mCustomScrollbar("scrollTo", newTop);
	    								}
	    								
	    							}
	    						});
	    					}
	    				});
	    			});

	    			$('.close_side').click(function(){
	    				$('.videobox').addClass('fullsize');
	    				$('#player_box iframe').width($('.m_video').width());
	                    $('#player_box iframe').height($('.m_video').height());
	                    
	    			});

	    			$('.open_side').click(function(){
	    				$('.videobox').removeClass('fullsize');
	    				$('#player_box iframe').width($('.m_video').width());
	                    $('#player_box iframe').height($('.m_video').height());
	    			});

	    			$('#lnk_intro_more').click(function(){
	    				$(this).parent().removeClass('dt');
	    				$(this).hide();
	    				$('#lnk_intro_more_hide').show();
	    			});

	    			$('#lnk_intro_more_hide').click(function(){
	    				$(this).parent().addClass('dt');
	    				$(this).hide();
	    				$('#lnk_intro_more').show();
	    			});

	    			$(".hjlist_wp").mCustomScrollbar({
	                	theme:"inset"
	                });
