var IdolClub = angular.module("IdolClub", ["IdolClub.ctrl", "IdolClub.directive", "IdolClub.filter", "IdolClub.service"], function () {
});

var IdolClubCtrl = angular.module("IdolClub.ctrl", ['ngSanitize']);
var IdolClubDirective = angular.module("IdolClub.directive", []);
var IdolClubFilter = angular.module("IdolClub.filter", []);
var IdolClubService = angular.module("IdolClub.service", []);

IdolClubService.service( 'Club', ['$rootScope', '$window', '$http', function( $rootScope, $win, $http ) {
    this.loginSina = function() {
        location.href = 'https://api.weibo.com/oauth2/authorize?forcelogin=true&client_id=3462420625&redirect_uri=http%3A%2F%2Fuser.idol001.com%2Fweibo_callback.php%3Ffrom%3Dweibo%26redirect_uri%3D'+encodeURIComponent(encodeURIComponent(location.href))+'&response_type=code';
        
    }

    this.loginQQ = function() {
        location.href = 'https://graph.qq.com/oauth2.0/authorize?response_type=code&client_id=101221908&redirect_uri=http%3A%2F%2Fuser.idol001.com%2Fqq_callback.php%3Ffrom%3Dweibo%26redirect_uri%3D'+encodeURIComponent(encodeURIComponent(location.href))+'&state=d29c61ba4166065869216c2111cae4f2&scope=get_user_info,get_simple_userinfo,get_vip_info,get_vip_rich_info,list_album,upload_pic,add_album,list_photo,check_page_fans,get_info,add_t,del_t,add_pic_t,get_repost_list,get_other_info,get_fanslist,get_idollist,add_idol,del_idol,get_tenpay_addr';
    }

    this.loginCheck = function(){
        return $http.jsonp("https://user.idol001.com/weibo_callback.php?callback=JSON_CALLBACK");
    }
}]);

IdolClubDirective.directive( "loginBtn", [ 'Club', function( Club ) {
    return {
        restrict: "A",
            link: function( scope, element, attrs ) {
            element.bind( "click", function() {
                scope.mainLogin();
            });
        }
    }
}]);

IdolClubDirective.directive( "loginBtnSina", [ 'Club', function( Club ) {
    return {
        restrict: "A",
            link: function( scope, element, attrs ) {
            element.bind( "click", function() {
                Club.loginSina();
            });
        }
    }
}]);

IdolClubDirective.directive( "loginBtnQq", [ 'Club', function( Club ) {
    return {
        restrict: "A",
            link: function( scope, element, attrs ) {
            element.bind( "click", function() {
                Club.loginQQ();
            });
        }
    }
}]);

IdolClubCtrl.controller('ClubContainer', ['$window', '$scope', '$rootScope', '$timeout', 'Club', function($win, $scope, $rootScope, $timeout, Club){

    $scope.showMainLogin = false;
    $scope.userinfo = null;

    $scope.logout = function() {
        Common_Tool.delCookie('PHPSESSID');
        $scope.userinfo = null;
    }

    $win.loginCallBack = function (response){
        $timeout(function() {
            jQuery('#player_box').css('text-indent','');
            try{
                if(response['error']){
                    $(".login-error").html(response['error']).show();
                    document.getElementById('img_vcode').src='https://user.idol001.com/web_login.php?action=vcode&_r='+new Date().getTime()
                }else{
                    $(".login-error").hide();
                    $scope.userinfo = response;
                    $scope.showMainLogin = false;
                }
            }catch(e){
                $(".login-error").show();
                //alert("登陆失败");
            }
        }, 0);
    }
    
    Club.loginCheck().success(
        function(response, status, header, config){
            
            $timeout(function() {
                $scope.userinfo = response;
                $scope.showMainLogin = false;
            }, 0);
        }).error(
        function(data){
        }
    );

}]);
    </script>
