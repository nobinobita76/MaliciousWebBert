        <script type="text/javascript">
            //统计脚本加载失败
            (function() {
                if (!window["listJsExecuteSuccessMark"]) {
                    var ajaxRequest = getAjaxRequest();
                    ajaxRequest.open("POST", "/ScriptFailLoad.ashx", true);
                    ajaxRequest.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
                    ajaxRequest.setRequestHeader("Connection", "close");
                    var data = {"name" : "js_list_foot", "pageUrl":location.href};
                    var arr = [];
                    for (var i in data) {
                        arr.push(i + "=" + data[i]);
                    }
                    ajaxRequest.send(arr.join("&"));
                }

                function getAjaxRequest (callback) {
                    var ajaxRequest;
                    try {
                        ajaxRequest = new XMLHttpRequest();
                    } catch (e) {
                        try {
                            ajaxRequest = new ActiveXObject("Msxml2.XMLHTTP");
                        } catch (e) {
                            try {
                                ajaxRequest = new ActiveXObject("Microsoft.XMLHTTP");
                            } catch (e) {
                                return null;
                            }
                        }
                    }
                    ajaxRequest.onreadystatechange = function () {
                        if (ajaxRequest.readyState == 4 && callback) {
                            // Prob want to do some error or response checking, but for
                            // this example just pass the responseText to our callback function
                            callback(ajaxRequest.responseText);
                        }
                    };

                    return ajaxRequest;
                }
            })();

        </script>
        
