<script type="text/javascript">
    $(window).load(function () {
        // 页面执行时间，页面加载时间
        load_data.end_time  = load_data.now();
        load_data.load_time = (load_data.end_time - load_data.start_time) / 1000;
        $(".load_time").text(load_data.load_time);
        $.ajax({
            type    : 'post',
            url     : 'http://ajax.699pic.com/?c=efficiency&a=getPageTime',
            data    : 'type=4&run_time=0.1061&load_time=' + load_data.load_time + '&ip=1707568248',
            success : function(res){}
        });
    })
</script>



