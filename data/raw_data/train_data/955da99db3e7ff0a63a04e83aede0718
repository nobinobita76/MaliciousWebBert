<script type="text/javascript">
    $(function(){
        $('.iwm-close').click(function(){
            $(this).parent().parent().parent().fadeOut()
        })

        $('#idea').click(function(){
            $(".ideaBack-win").show();
        })

        $('.iwm-subBtn').click(function(){
            var content = $('.iwm-cont textarea').val();
            var qq = $(".iwm-cont input[name='qq']").val();
            var email = $(".iwm-cont input[name='email']").val();

            if (content == "") {
                alert("请输入内容");
                return false;
            } else if (qq == "") {
                alert("请输入QQ号码");
                return false;
            }

            if (!/^[0-9]{5,11}$/.test(qq)) {
                alert('QQ格式不正确!');
                return false;
            }

            if (email != '' && !/^([a-zA-Z0-9]{1,20})(([\_\-\.])?([a-zA-Z0-9]{1,20}))*@([a-zA-Z0-9]{1,20})(([\-\_])?([a-zA-Z0-9]{1,20}))*(\.[a-z]{2,4}){1,2}$/.test(email)) {
                alert('邮箱格式不正确!');
                return false;
            }


            $.ajax({
                type:'post',
                url:"http://699pic.com/index.php?m=Statistics&c=Idea&a=addIdea",
                data:{content:content, qq:qq, email:email},
                success:function(data){
                    if (data.state == 1) {
                        $(".iwm-cont input[type='submit']").attr("value", "提交成功");
                        $(".ideaBack-win").animate({
                            opacity: "hide"
                        }, "slow");
                    }
                }
            })

        })
    })
</script>
