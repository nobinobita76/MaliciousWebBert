<script>
    $(function() {
        //右侧悬浮按钮触发的JS
        //意见反馈弹窗
        $('#fixed-nversion .idea').on('click',function(){
            $('.ideaBack-win').fadeIn();
        });
        $('#fixed-nversion .record').on('click',function(){
            window.open('http://699pic.com/community/download');
        });

        //返回顶部
        //点击回到顶部的元素
        $("#fixed-nversion .gotop").click(function(e) {
            $('body,html').animate({scrollTop:0},500);
        });

        //回到顶部元素的渐显与渐隐
        $(window).scroll(function(e) {
            //若滚动条离顶部大于100元素
            if($(window).scrollTop()>100)
                $("#fixed-nversion .gotop").fadeIn(200);
            else
                $("#fixed-nversion .gotop").fadeOut(200);
        });
    });
</script>

