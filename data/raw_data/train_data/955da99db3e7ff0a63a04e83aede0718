                    <input type="text" name="qq" class="fl" placeholder="QQ号">
                    <input type="text" name="email" class="fr" value="" placeholder="E-mail">
                </div>
                <input type="submit" value="提交" class="iwm-subBtn">
            <!--</form>-->
