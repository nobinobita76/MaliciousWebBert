         <tr height="36" align="center"> 
          <td colspan="3"><strong><font color="#000000"><h4>美国空间分享通信历史大事件</h4></font></strong></td> 
         </tr> 
         <tr align="center"> 
          <td width="250">作者: 纵横数据</td> 
          <td>时间:2014-2-24</td> 
          <td width="250" align="right">
           <!-- Baidu Button BEGIN --> 
           <div id="bdshare" class="bdshare_b" style="line-height: 12px;"> 
            <img src="http://bdimg.share.baidu.com/static/images/type-button-5.jpg?cdnversion=20120831" /> 
            <a class="shareCount"></a> 
           </div> <script type="text/javascript" id="bdshare_js" data="type=button&amp;uid=683878"></script> <script type="text/javascript" id="bdshell_js"></script> <script type="text/javascript">
