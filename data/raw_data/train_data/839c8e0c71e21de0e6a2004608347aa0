  <script language="javascript" type="text/javascript">

        //代截图分类信息及图片区域初始化
        var isBizofferHasScreenShot= false;
        if(!isBizofferHasScreenShot)
        {//发布单未购买代截图服务

            $.fed.tabs("#goods-imgwrap", { event: "live", currindex: 0, interval: 0,
                onSuccess: function () {
                    $(this).find('.goods-imgwrap a:not(.hide)').trigger('lazyload');
                }
            });

            //图片容器初始化
            if (picImgData && picImgData.length > 0) {
                $.fed.imagegallery('#goods-imgwrap', {
                    'imgData': picImgData,
                    'imgsTitle': { 'cls': '', 'name': '', 'url': '' }
                });
            }
            else {
                if ($("#hNewShowPic").length > 0)
                    $("#hNewShowPic").remove();
            }

				
        }

        $(function () {
		   
            if(!isBizofferHasScreenShot)
            {//发布单未购买代截图服务
                $("#tab5").hide();

            }
            else{
                var isDealingScreenShot=false;    
                if(isDealingScreenShot ){
                    $("#dvNoScreenShot").show();
                    $("#tab5").show();
                }
                //初始化分类信息及图片
                if(screenShotSortData && screenShotSortData.length >0)
                {
                    var sort_Li="";
                    var sort_DivPics="";
                    for(var i=0;i<screenShotSortData.length;i++ )
                    {
                        var temp_pic='';

                        var sortItem=screenShotSortData[i];
                        sort_Li+= '<li><a href="#renzheng_pic'+ (i+1) +'">'+sortItem.name+'</a></li>';

                        if(sortItem.pics.length>0){
                            for(var p=0;p<sortItem.pics.length;p++){
                                temp_pic+='<li><img src="'+sortItem.pics[p] +'" alt="诚心售17段 绝版圣装纳米瓷&圣装祷告背饰...稀有天赋mod..." /><span class="watermark"></span></li>';
                            }
                        }

                        sort_DivPics+= '<div class="renzheng_pic" id="renzheng_pic'+ (i+1) +'" name="tabpanel">';
                        sort_DivPics+= '<label class="renzheng-pic-sort">'+ sortItem.name +'</label><ul class="renzheng-pic-list">';
                        sort_DivPics+= temp_pic;
                        sort_DivPics+= '</ul></div>';
                    }

                    $("#ulScreenShotSort").html(sort_Li);
                    $("#renzheng_img").html(sort_DivPics);
                }
            }
        });

