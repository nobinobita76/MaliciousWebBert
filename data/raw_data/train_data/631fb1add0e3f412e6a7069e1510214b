<script>
var userid = "";
var username = "";
var uid = "";
var cityPositon = "";
function getDeviceId_an() {
	var jsonstr='{"action":"control_get_deviceinfo","pagetype":"def"}';
	if(getUrlParam('from')=='lyric'||getCookie('pfrom')=='lyric'){
		return;
	}
	window.KuwoInterface.jsCallNative(jsonstr);
}
function feedback_ardeviceinfo(data){
	userinfo= eval('('+data+')');
	username = userinfo.uname;
	uid = userinfo.device_uid;
	cityPositon = userinfo.user_city;
}
function getDeviceId_ios() {
	var jsonstr='{"pagetype":"def"}';
	if(window.location.href.indexOf("debug=1")==-1){
		var iosUrl = "kwip://kwplayerhd/control_get_deviceinfo?param="+encodeURIComponent(jsonstr);
		window.location.href=iosUrl;
	}
}
function feedback_deviceinfo(data){
	userinfo= eval('('+data+')');
	username=userinfo.uname;
	userid=userinfo.userid;
	uid=userinfo.uid;
}
function getPlat(){
	var plat = navigator.platform.toLowerCase();
	if(plat.indexOf('arm')>=0){
		return 'an';
	}else if(plat.indexOf('win')>=0){
		return 'win';
	}else if(plat.indexOf('mac')>=0){
		return 'mac';
	}else if(plat.indexOf('iphone')>=0||plat.indexOf('ipad')>=0){
		return 'ios';
	}
	return plat.substr(0,5);
}
function getUrlParam(name){
	var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)"); 
	var r = window.location.search.substr(1).match(reg);
	if (r!=null) return unescape(r[2]); return null;
}
//滚动条在Y轴上的滚动距离
function getScrollTop(){
　　var scrollTop = 0, bodyScrollTop = 0, documentScrollTop = 0;
　　if(document.body){
　　　　bodyScrollTop = document.body.scrollTop;
　　}
　　if(document.documentElement){
　　　　documentScrollTop = document.documentElement.scrollTop;
　　}
　　scrollTop = (bodyScrollTop - documentScrollTop > 0) ? bodyScrollTop : documentScrollTop;
　　return scrollTop;
}
//文档的总高度
function getScrollHeight(){
　　var scrollHeight = 0, bodyScrollHeight = 0, documentScrollHeight = 0;
　　if(document.body){
　　　　bodyScrollHeight = document.body.scrollHeight;
　　}
　　if(document.documentElement){
　　　　documentScrollHeight = document.documentElement.scrollHeight;
　　}
　　scrollHeight = (bodyScrollHeight - documentScrollHeight > 0) ? bodyScrollHeight : documentScrollHeight;
　　return scrollHeight;
}
//浏览器视口的高度
function getWindowHeight(){
　　var windowHeight = 0;
　　if(document.compatMode == "CSS1Compat"){
　　　　windowHeight = document.documentElement.clientHeight;
　　}else{
　　　　windowHeight = document.body.clientHeight;
　　}
　　return windowHeight;
}
//浏览器视口的宽度
function getWindowWidth(){
　　var windowWidth = 0;
　　if(document.compatMode == "CSS1Compat"){
　　　　windowWidth = document.documentElement.clientWidth;
　　}else{
　　　　windowWidth = document.body.clientWidth;
　　}
　　return windowWidth;
}
function addCookie(cookieName, cookieValue){
    var expdate = new Date();
    var argv = addCookie.arguments;
    var argc = addCookie.arguments.length;
    var expires = (argc > 2 && argv[2] != 0) ? argv[2] : null;
    var path = (argc > 3) ? argv[3] : '/';
    var domain = (argc > 4) ? argv[4] : null;
    var secure = (argc > 5) ? argv[5] : false;
    if (expires != null) {
        expdate.setTime(expdate.getTime() + expires*60*1000);
    }else{
	   	expdate.setTime(expdate.getTime() + (3600*24*365*1000));
    }
    domain = "kuwo.cn";
    document.cookie = cookieName + "=" + escape(cookieValue) +
    "; expires=" + expdate.toGMTString() +
    ((path == null) ? "" : ("; path=" + path)) +
    ((domain == null) ? "" : ("; domain=" + domain)) +
    ((secure == true) ? "; secure" : "");
}
function getCookie(cookieName){
    var arg = cookieName + "=";
    var alen = arg.length;
    var clen = document.cookie.length;
    var i = 0;
    while (i < clen) {
        var j = i + alen;
        if (document.cookie.substring(i, j) == arg) {
            var endstr = document.cookie.indexOf(";", j);
            if (endstr == -1) {
                endstr = document.cookie.length;
            }
            var ret = unescape(document.cookie.substring(j, endstr));
            if (ret != "") {
                return ret;
            }
        }
        i = document.cookie.indexOf(" ", i) + 1;
        if (i == 0) 
            break;
    }
    return "";
}
var from = getUrlParam('from');
if(from!=null&&from.length>0){
	addCookie('pfrom',from);
}else{
	from = getCookie('pfrom');
}
if(from.length<1){
	from = 'direct';
	addCookie('pfrom',from);
} 
var ptimes = getCookie('ptimes');
if(!ptimes) ptimes = 0;
addCookie('ptimes',parseInt(ptimes)+1);
var plast =  getCookie('plast');
var today = new Date();
var vstat = 'first';
if(plast){
	plast = new Date(plast);
	var tmp = today.getTime() - plast.getTime(); 
	if(tmp>4*7*24*3600*1000){
		vstat = 'lastmonth';
	}else if(tmp>7*24*3600*1000){
		vstat = 'lastweek';
	}else if(tmp>12*3600*1000){
		vstat = 'yesterday';
	}else if(tmp >= 0){
		vstat = 'today';
	}else{
		vstat = 'impossible';
	}
}
addCookie('plast',today);
function kuizhiqinglog(a,b,c,d,e){// /from/page/time/action/info/
	new Image().src = 'http://document.kuwo.cn/kuizhiqing/'+a+'/'+b+'/'+c+'/'+d+'/'+e+'/'+Math.random();
}
function showclicklog(id){
	kuizhiqinglog(from,"search",vstat,'showclick',id);
}
function actionlog(act,id){
	kuizhiqinglog(from,"search",vstat,act,id);
}
kuizhiqinglog(from,"search",vstat,'view','');
function searchKeyLog(key,uid) {
	new Image().src = 'http://document.kuwo.cn/kuizhiqing/searchUid/'+key+'/'+uid+'/'+Math.random();// /searchUid/key/uid
}
