<script type="text/javascript">
document.onscroll = function(){
	var sTop = getScrollTop();
	var sHeight = getScrollHeight();
//	var aHeight = window.screen.availHeight;
	var aHeight = getWindowHeight();
	if ((sHeight - sTop) == aHeight) {
		loadmore();
	}
}
/*判断，是否执行自主支付流程--start*/
var ff = getUrlParam('ff');
if (ff != null && ff.length > 0) {
	addCookie('ff',ff);
} else {
	ff = getCookie('ff');
	if (ff != 'ticket') {
		addCookie('ff','none');
	}
}
/*判断，是否执行自主支付流程--end*/
if (from != "direct") {
	var os = getPlat();
	if (os == 'an') {
		getDeviceId_an();
	}
	if (os == 'ios') {
		getDeviceId_ios();
	}
}

var schcook = [];
var PARAM = {'pn':1,'ps':20};

//存在station就添加cookie，切换城市cookie update    && getCookie('station') == ''
var station = '';
if (station == '' && getCookie('station') != '') {
	station = getCookie('station');
} else if (getCookie('station') != station) {
	addCookie('station',station);
}

$(document).ready(function(){
	var winWidth = getWindowWidth();
	if (winWidth > '400') {
		$('.singTex').css('width','72%');
	}
	freshHissearch();
});
setTimeout("autoPosition()", 100);//自动定位当前城市，目前只有安卓
function autoPosition() {
	var first = '';
	if (first == '' || first == '0' || first == 'undefined') {
		return;
	}
	if (station == '' || station == 0) {
		var cities = '';
		if(cities.length>0){
			var cities = (cities).split(',');
 			for(i in cities){
				if ((cities[i]+"市") == cityPositon) {
					location.href = '/piao/index?station='+i;
					$('span.titLe').html(cities[i]);
				}
			} 
		}
	}
}

function gopage(url){
	if(getUrlParam('from')=='lyric'||getCookie('pfrom')=='lyric'){
		location.href = url;
	}else if(navigator.platform.toLowerCase().indexOf('arm')>=0){//android
		controlInappUrl(url,"酷我演出","");
	}else{
		location.href = url;
	}
}

function goSelfTicketPage(showId, type, yl_url){
	$("#loading_img").show();
	var url = "";
	if (type=='my') {
		if (getCookie('ff') != 'none') {
			url = 'http://ticket.kuwo.cn/ticket/my/personalManage.html?';	
		} else {
			url = yl_url;
		}
	} 
	if (type=='detail') {
		if (getCookie('ff') != 'none') {
			url = 'http://ticket.kuwo.cn/ticket/show/info.html?id=' + showId;	
		} else {
			url = yl_url;
		}
	}
 	if(getUrlParam('from')=='lyric'||getCookie('pfrom')=='lyric'){
		location.href = url;
	}else if(navigator.platform.toLowerCase().indexOf('arm')>=0){//android
		controlInappUrl(url,"酷我演出","");
	}else{
		location.href = url;
	} 
}

function controlInappUrl(url,title,type){
	if(url.indexOf('http://')!=0){
		url = "http://piao.kuwo.cn"+url;
	}
	var jsonstr='{"action":"control_inapp_url","url":"'+url+'","title":"'+title+'","pagetype":"'+type+'"}';
	if(window.KuwoInterface){
		try{
			window.KuwoInterface.jsCallNative(jsonstr);
		}catch(e){
			location.href = url;
		}
	}else{
		location.href = url;
	}
}

function loadmore(){
	var filename = "search";
	if(filename=='index'){
		var param = "station=&pn="+(PARAM.pn+1)+"&ps="+PARAM.ps;//(parseInt()+1);
		AjaxJsonp('/piao/index',param,'addShowList');
	}else if(filename=='piece'){
		var param = "station=&pn="+(PARAM.pn+1)+"&ps="+PARAM.ps;
		AjaxJsonp('/piao/piece',param,'addShowList');
	}
}
function addShowList(data){
	var preindex = "";
	if(data.ret>0){
		PARAM.pn++;
		var show = data.data;
		html = "";
		for(i in show){
			if(preindex.indexOf('|'+show[i].id+'|')>0) continue;
			html += '<div class="singBg" onclick="showclicklog('+show[i].id+'); gopage(\''+show[i].link+'&uniontype=iswap\');">';
			html += '<p class="singImg"><img src="'+show[i].pic1+'" width="85"></p>';
			html += '<div class="singTex">';
			html += '<p class="singTa">'+show[i].title+'<p>';
			html += '<p class="h5"></p>';
			html += '<p class="singTb"><img src="http://image.kuwo.cn/piao/adress.png" width="10" align="absmiddle">'+show[i].city +'-'+show[i].venue+'</p>';
			html += '<p class="h5"></p>';
			html += '<p class="singTb"><img src="http://image.kuwo.cn/piao/clockb.png" width="15" align="absmiddle">'+show[i].showdate+'</p>';
			html += '<p class="h10"></p>';
			html += '<div class="singBtn">';
			html += '<p class="sBtnLe"><span>'+show[i].price+'</span> 元起 ';
/* 			if(show[i].part!=300){
				html += '<em> 限时97折 </em>';
			} */
			html += '</p>';
			if(show[i].cstatus==0){
				html += '<a href="#" class="sBtnRi"><img src="http://image.kuwo.cn/piao/hota.png" width="50"></a>';
			}
			html += '</div>';
			html += '</div>';
			html += '</div><p class="h4c"> </p>';
		}
		$('.showlist').append(html);
		if(show.length<PARAM.ps){
			$('.loadmore').hide();
		}
	}else{
		$('.loadmore').hide();
	}
}
function freshHissearch(){
	var searchs = getCookie('searchs');
	if(searchs.length>1){
		schcook = searchs.split(',');
		html = '';
		for(i in schcook){
//			if(schcook[i]) html += '<a href="/search/'+encodeURI(schcook[i])+'" class="seachLista">'+schcook[i]+'</a>';
			if(schcook[i]) html += '<a href="#@" onclick="goSearchByKey(\''+encodeURI(schcook[i])+'\');" class="seachLista">'+schcook[i]+'</a>';
		}
		html += '<a href="#@" onclick="clearSearch();" class="clears">清除搜索记录</a>';
		$('.hissearchkey').html(html);
	}else{
		$('.hissearchkey').html('');		
	}
	currentSearchKey = '周杰伦';
	if(currentSearchKey.length>0){
		if(schcook.indexOf(currentSearchKey)<0){
			appendCookie('searchs',currentSearchKey);
		}
	}
}
function clearSearch(){
	addCookie('searchs','');
	freshHissearch();
}
function goSearch(){
	var key = $('.seachInt').val();
	if(key.length<=0) return;
	searchKeyLog(key,uid);
	location.href = "/search/"+encodeURI(key);
}
function goSearchByKey(key){
	if (key == 'undefined') {
		key = $('.seachInt').val();
	}
	if(key.length<=0) return;
	searchKeyLog(key,uid);
	location.href = "/search/"+encodeURI(key);
}
function searchShift(tt){
	if(tt==1){
		$('#hotsearch').removeClass('tabLeb').addClass('tabLea');
		$('#hissearch').removeClass('tabRib').addClass('tabRia');
		$('.hotsearchkey').show().siblings().hide();
	}else if(tt==0){
		$('#hotsearch').removeClass('tabLea').addClass('tabLeb');
		$('#hissearch').removeClass('tabRia').addClass('tabRib');
		$('.hissearchkey').show().siblings().hide();
	}
	return;
}
function chooseCity(){
	$('#citySelect').show();
	$('.showlist').css('height','500px');
	//$('.showlist').hide();
}

function goIndex() {
	var station = getCookie('station');
	if (station != '') {
		location.href = '/piao/index?station=' + station;
	} else {
		location.href = '/piao/index';
	}
}

function addCookie(cookieName, cookieValue){
    var expdate = new Date();
    var argv = addCookie.arguments;
    var argc = addCookie.arguments.length;
    var expires = (argc > 2 && argv[2] != 0) ? argv[2] : null;
    var path = (argc > 3) ? argv[3] : '/';
    var domain = (argc > 4) ? argv[4] : null;
    var secure = (argc > 5) ? argv[5] : false;
    if (expires != null) {
        expdate.setTime(expdate.getTime() + expires*60*1000);
    }else{
	   	expdate.setTime(expdate.getTime() + (3600*24*365*1000));
    }
    domain = "kuwo.cn";
    document.cookie = cookieName + "=" + escape(cookieValue) +
    "; expires=" + expdate.toGMTString() +
    ((path == null) ? "" : ("; path=" + path)) +
    ((domain == null) ? "" : ("; domain=" + domain)) +
    ((secure == true) ? "; secure" : "");
}
function getCookie(cookieName){
    var arg = cookieName + "=";
    var alen = arg.length;
    var clen = document.cookie.length;
    var i = 0;
    while (i < clen) {
        var j = i + alen;
        if (document.cookie.substring(i, j) == arg) {
            var endstr = document.cookie.indexOf(";", j);
            if (endstr == -1) {
                endstr = document.cookie.length;
            }
            var ret = unescape(document.cookie.substring(j, endstr));
            if (ret != "") {
                return ret;
            }
        }
        i = document.cookie.indexOf(" ", i) + 1;
        if (i == 0) 
            break;
    }
    return "";
}
function appendCookie(cookieName, cookieValue){
	var tmp = getCookie(cookieName)+","+cookieValue;
	addCookie(cookieName,tmp);
}
function AjaxJsonp(url,params,backcall){
	$.ajax({
		type:"post",
		url:url,
		data:params,
		dataType: "jsonp",
	    jsonpCallback:backcall,
		success:function (data) {
		},
		error:function () {	
		}
	});	
}
