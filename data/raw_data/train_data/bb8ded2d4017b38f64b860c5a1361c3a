    <script type="text/javascript">
        //ip定向 写入cookie

        var url = window.location.href;
        if (url.indexOf("quanguo") < 0) {
            Taoche.IpLocation.init();
            Taoche.IpLocation.afterLocation(function () {
                var lstr = url.split("m.taoche.com");
                if (lstr.length > 0) {
                   window.location.href = "http://" + Taoche.IpLocation.citySpell + "." + url.replace(lstr[0], "");
                }
            });
        }
        else {
            Taoche.IpLocation.saveIpCity(0);
        }
        
    </script>

