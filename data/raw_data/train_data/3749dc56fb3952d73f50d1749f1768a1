 var btns = $(".btn");
 var show = $(".show");
 $(btns).each(function (i) {
     $(this).mousemove(function (e) {
         return;
         var x = e.pageX;
         var y = e.pageY;
         $(show).eq(i).show().css({ "position": "absolute", "left": x + 20, "top": y + 5 });
     }).mouseout(function () {
         $(show).hide();
     })
 })

