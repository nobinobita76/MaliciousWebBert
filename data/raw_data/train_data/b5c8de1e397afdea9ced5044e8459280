function GetQueryString(name)
{
	var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
	var r = window.location.search.substr(1).match(reg);
	if(r!=null) return  unescape(r[2]); return null;
}
	//alert(document.getElementById('domain'));
//	alert(GetQueryString("domain"));
	$(function()
	{
		document.getElementById('domain').value = GetQueryString("domain");
		document.getElementById('req').value = GetQueryString("req");
	});
</script>
