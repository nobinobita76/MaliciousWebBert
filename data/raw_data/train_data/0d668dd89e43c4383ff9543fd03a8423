<script type="text/javascript">
	var cityArrsGroup;
	var cityArrs=[];
	
	//document.createStyleSheet('/traffic/css/searchbox.css');
	var style = document.createElement("link");
	style.href = "http://cdn4.pmpsys.cn/traffic/css/searchbox.css";
	style.rel = "stylesheet";
	style.type = "text/css";
	document.getElementsByTagName("head")[0].appendChild(style);
	// js加载城市数据后回调
	function onLoadCityArrs(){
		cityArrsGroup = cityArrsGroup.split(";");
		for(var i=0;i<cityArrsGroup.length;i++){
		cityArrs[i]=cityArrsGroup[i].split(",");
		}
	}
</script>
