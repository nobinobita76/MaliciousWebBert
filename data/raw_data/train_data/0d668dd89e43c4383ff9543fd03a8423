function mouseOut(the,idx){
	the.bgColor="";
	if(idx) $("td_"+idx).style.display='none';
}
function mouseOver(the,idx){
	the.bgColor="#F0F0F0";
	if(idx)	$("td_"+idx).style.display='block';
}
function showrpdialog(x,y,id,idx){
	$("objid").value=id;
	setReply(x,y,$("rp_content"),document.f_replay,'',idx);
}
