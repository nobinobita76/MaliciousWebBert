     <script type="text/javascript">
         require(['jquery'],function(){
             //判断是否登录
             var location_url = location.host;
             if(location_url.indexOf("pps.tv")>-1&&!config.user_name) {
                 autoLoginNew();
             }
             function autoLoginNew(){
                 $.getJSON('http://game.pps.tv/passport/api2/autologin_jsonp?position=g_header&callback=?',function(data){
                     var code = data.code;
                     if (code == 0) {
                         uid = data.userInfo.user_id;
                         user_name = data.userInfo.user_name;
                         $("#account_id").val(user_name);
                         config.user_name = user_name;
                         location.reload();
                     } else {
//                         window.location.href = "http://g.iqiyi.com";
                     }
                 });
             }
         });
     </script>
