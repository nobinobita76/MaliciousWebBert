     <script type="text/javascript">
 function wechatbus(){
jQuery("#mcover").css("display","block")  // 分享给好友圈按钮触动函数
}
function weChat(){
jQuery("#mcover").css("display","none");  // 点击弹出层，弹出层消失
}
jQuery(function(){
setTimeout(function () {
jQuery("#mcover").hide(); }, 8000);    //8000毫秒是隐藏层
 })
</script> 
