<script type="text/javascript">
    var projectOptions =
            {
                selectFirst: false,
                width: 358,
                minChars: 1,
                max: 100,
                dataType: 'json',
                extraParams: {
                    format: 'json',
                    act: 'hint1'
                },
                parse: function (data) {
                    var parsed = [];
                    for (var i = 0; i < data.length; i++) {
                        parsed[parsed.length] = {
                            data: data[i],
                            value: data[i].Key,
                            result: data[i].Value
                        };
                    }
                    return parsed;
                },
                formatItem: function (item) {
                    return item.Key;
                }
            };
    var doctorOptions =
            {
                selectFirst: false,
                width: 358,
                minChars: 1,
                max: 100,
                dataType: 'json',
                extraParams: {
                    format: 'json',
                    act: 'hint2'
                },
                parse: function (data) {
                    var parsed = [];
                    for (var i = 0; i < data.length; i++) {
                        parsed[parsed.length] = {
                            data: data[i],
                            value: data[i].Key,
                            result: data[i].Value
                        };
                    }
                    return parsed;
                },
                formatItem: function (item) {
                    return item.Key;
                }
            };
    var hospitalOptions =
            {
                selectFirst: false,
                width: 358,
                minChars: 1,
                max: 100,
                dataType: 'json',
                extraParams: {
                    format: 'json',
                    act: 'hint3'
                },
                parse: function (data) {
                    var parsed = [];
                    for (var i = 0; i < data.length; i++) {
                        parsed[parsed.length] = {
                            data: data[i],
                            value: data[i].Key,
                            result: data[i].Value
                        };
                    }
                    return parsed;
                },
                formatItem: function (item) {
                    return item.Key;
                }
            };


    $(document).ready(function () {
        $("#searchText").autocomplete("/SearchHint.ashx", projectOptions);
        $("#seachspan span").click(function () {
            $("#searchText").val("");
            var currentText = $('#seachspan span.now').text();
            $("#searchText").unbind("autocompleteselect");
            $("#searchText").unautocomplete();
            if (equal(currentText, '项目')) {
                $("#searchText").autocomplete("/SearchHint.ashx", projectOptions);
            } else if (equal($(this).text(), '医生')) {
                $("#searchText").autocomplete("/SearchHint.ashx", doctorOptions);
            } else if (equal($(this).text(), '医院')) {
                $("#searchText").autocomplete("/SearchHint.ashx", hospitalOptions);
            }

            $("#searchText").keydown(function (event) {
                if (event.keyCode == '13') {
                    if ($.trim($('#searchText').val()) == "" || $(".ac_results .ac_over").length > 0) {
                        return false;
                    }
                    $("#btnSearch").click();
                    return false;
                }
            });
        });
    });
    $("#searchText").keydown(function (event) {
        if (event.keyCode == '13') {
            if ($.trim($('#searchText').val()) == "" || $(".ac_results .ac_over").length > 0) {
                return false;
            }
            $("#btnSearch").click();
            return false;
        }
    });
    $("#btnSearch").click(function () {
        if ($('#searchText').val() == "" || $('#searchText').val() == $('#seachspan span.now').text()) {
            PopAlert("请输入关键字进行搜索！");
        }
        else {
            gotoWithKeyword($('#searchText').val());
        }
    })


    function gotoWithKeyword(word, url) {
        if (url==undefined)
            url = "http://so.39.net/s?words=" + escape($.trim(word).replace(/_/g, '').replace(/\//g, '').replace(/</g, '').replace(/>/g, ''));
        if (!/*@cc_on!@*/0) {
            var myWindow = window.open(url, '_new');
            myWindow.focus();
        } else {
            var a = document.createElement('a');
            a.href = url;
            a.target = '_self';
            document.body.appendChild(a);
            a.click();
        }
    }
    function gotoWithKeywordAndType(word, t) {
        var url;
        var nowproject = $('#seachspan span.now').text();
        if (nowproject == "项目" || nowproject == "医院" || nowproject == "医生") {
            url = "http://so.39.net/s?words=" + escape($.trim(word).replace(/_/g, '').replace(/\//g, '').replace(/</g, '').replace(/>/g, ''));
        }
        if (!/*@cc_on!@*/0) {
            window.open(url);
        } else {
            var a = document.createElement('a');
            a.href = url;
            a.target = '_blank';
            document.body.appendChild(a);
            a.click();
        }
    }
    function equal(str1, str2) {
        return (str1.indexOf(str2) == 0 || str2.indexOf(str1) == 0) || str1 == str2;
    }
</script>

