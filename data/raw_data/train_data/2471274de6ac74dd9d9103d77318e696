<script type="text/javascript">
    (function ($) {
        $(function () {
            $(".sbtn").hover(function () {
                $(this).addClass("sbtn_hover");
            }, function () {
                $(this).removeClass("sbtn_hover");
            });
        });

        var mesTimer = [];
        $(".n_message").find("em,.message_wrap").mouseover(function () {
            if (!$(".n_message").hasClass("hover")) {
                $(".n_message").addClass("hover"); $(".n_message").find(".message_wrap").stop(true, true).slideDown(230);
            }
            window.clearTimeout(mesTimer[1], mesTimer[2]);
        });
        $(".n_message").mouseleave(function () {
            if ($(this).hasClass("hover")) {
                var _this = this;
                mesTimer[1] = window.setTimeout(function () {
                    $(_this).find(".message_wrap").stop(true, true).slideUp(150);
                }, 300);
                mesTimer[2] = window.setTimeout(function () {
                    $(_this).removeClass("hover");
                }, 450);
            }
        });

        $(function () {
            $(".message_data_list li").hover(function () {
                $(this).addClass("li_hover");
            }, function () {
                $(this).removeClass("li_hover");
            });
        });

        $(function () {
            var tnav = $(".message_tabs dd");
            var tcon = $(".message_data");
            tnav.click(function (e) {
                var index = tnav.index(this);
                $(this).siblings().removeClass("on");
                $(this).addClass("on");
                if (!tcon == "") {
                    tcon.hide();
                    tcon.eq(index).show();
                }
            });
        });
    })(jQuery);
</script>
<script type="text/javascript">
    $(function () {
        var head = document.getElementsByTagName('head')[0];
        linkTag = document.createElement('meta');
        linkTag.setAttribute('http-equiv', 'Cache-Control');
        linkTag.setAttribute('content', 'no-transform');
        head.appendChild(linkTag);
    });
</script>
