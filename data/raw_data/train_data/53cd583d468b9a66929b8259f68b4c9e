      <script type="text/javascript">
$(document).ready(function() {
var obj = $('#searchform');
if(obj.length > 0) {
var getstr = {mod:'search', srchtype:'title', srhfid:0, srhlocality:'forum::viewthread', source:'txt.form_nor.a', wap:'yes', fId:0};getstr.sId = '7149150';
