//重新定义激活弹框的关闭按钮
$('.active_contain .wBox_close').live('click', function(){
    setTimeout(function(){
        callLoginScript();
        reloadMsgInfo();
    }, 1000);
});

function updateCommentIfrsrc2(){

    var ifr1 = document.getElementById('comment');
    var src = ifr1.src;
    var arr = src.split('#');
    ifr1.src = arr[0]+'#b'+Math.random()

}

//登录信息
function doCallLoginBox(){
    $("#login_btn_click").unbind('click');
    $("#login_btn_click").xLoginBox({
        func:function(){
            reloadMsgInfo();
            callLoginScript();
            updateCommentIfrsrc2();
            try{
                LoginHandle.setStats();
            }catch(e){}
        },
        args:[],
        step:2,
        show:true,
        url: window.location.href
    });
}
function callLoginScript(){
    var _script_id="updateHeaderLoginScript";
    var o=document.getElementById(_script_id);
    if(o){
        o.parentNode.removeChild(o);
    }
    var _url='http://www.xcar.com.cn/site_js/new_login_2013.php?t='+Date.parse(new Date());
    loadScript({
        url: _url,
        id : _script_id,
        callback: function(){
            o.parentNode.removeChild(o);
        }
    });
}
/*自主视频start*/
//如果有推荐就没有重播按钮，没有推荐就有重播按钮
function replay()
{
    //如果推荐有的话就没有重播按钮
    /*
     1、 $(".vedio_list").show();
    */
    /*
     2、 $("#video_replay").show();
        $("#video_cover_img").show();
     */
    $("#video_cover").hide();
    $(".vedio_list").show();
    //window.console && console.log('Replay');
}
function show_cover()
{
    $("#video_cover").show();
    $("#video_cover_img").show();
}
function hide_cover()
{
    $("#video_cover").hide();
    $("#video_cover_img").hide();
    $(".vedio_list").hide();
}
//播放按钮事件
$("#video_cover").bind("click", function(){
    if(typeof  player.play2=='function' && (typeof(player.play2) != "undefined")){
		$("#video_cover").hide();
		$("#video_cover_img").hide();
        $(".vedio_list").hide();
		player.play2();
		//window.console && console.log('Let\'s Play11');
	}
});
$(document).bind("keydown", function(e){
    if(e.keyCode==32){
        stopDefault(e);
        if(player.getPlaying()){
            player.pause();
        }else{
            player.play2();
        }
    }
});
function stopDefault(e)
{
    if (e&&e.preventDefault)
    {
        e.preventDefault();
    }
    else
    {
        window.event.returnValue = false;
    }
}

//重播按钮事件
$("#video_replay").bind("click", function(){
	if(typeof  player.play2=='function'){
		$("#video_replay").hide();
		$("#video_cover").hide();
		$("#video_cover_img").hide();
		player.play2();
		//window.console && console.log('Let\'s Play22');
	}
});


/* 优酷商业合作播放器 */
function biz_player(content,cid,vid)
{
    player = new YKU.Player(content,{
          client_id:'39fa61a8c2b6ddd9',
          vid:vid,
          width:'100%',
          height:'100%',
          autoplay: true,
          show_related: false,
          flashext: '<param name="wmode" value="transparent">',
          events:{
            onPlayEnd: function(){location.href='http://xtv.xcar.com.cn/show/201703/id_25484.html';}
          }
      }
    );
}

