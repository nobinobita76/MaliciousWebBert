<script type="text/javascript">
$(document).ready(function(){
  $("#search_text").bind({
    focus:function(){if($.trim($(this).val())=='请输入关键字'){$(this).val('');}},
    blur:function(){if($.trim($(this).val())==''){$(this).val('请输入关键字');}}
  });

	//展开车型
	addHandler($('#auto_list_btn')[0],'mouseover',function(e){
		if(checkHover(e,this)){
			show();
		}
	});
	addHandler($('#auto_lists')[0],'mouseover',function(e){
		if(checkHover(e,this)){
			show();
		}
	});

	//隐藏车型
	addHandler($('#auto_lists')[0],'mouseout',function(e){
		if(checkHover(e,this)){
			hide();
		}
	});
	addHandler($('#auto_list_btn')[0],'mouseout',function(e){
		if(checkHover(e,this)){
			hide();
		}
	});
	var showHand = [];
	var hideHand = [];
	function clear(){
		for(var i=0,len=showHand.length;i<len;i++){
			clearTimeout(showHand[i]);
		}
		for(var i=0,len=hideHand.length;i<len;i++){
			clearTimeout(hideHand[i]);
		}
		showHand.length=0;
		hideHand.length=0;
	}

	function show(){
		clear();
		showHand.push(setTimeout(function(){
			$('#auto_list_btn').addClass('current');
			$('#auto_lists').show();
		},100));
	}
	function hide(){
		clear();
		hideHand.push(setTimeout(function(){
			$('#auto_list_btn').removeClass('current');
			$('#auto_lists').hide();
		},100));
	}

    //视频播放页结束展示推荐列表----start

    $(".vedio_list li").hover(function()
    {
        console.log(1)
        $(this).find(".vedio_mask").addClass("mask_hover");
        $(this).find(".bo_intro").removeClass("intro_hover");
        $(this).find(".type_z b").hide();
        $(this).find(".icon_play").css("background-position","-25px top");
    },function(){
        $(this).find(".vedio_mask").removeClass("mask_hover");
        $(this).find(".bo_intro").addClass("intro_hover");
        $(this).find(".type_z b").show();
        $(this).find(".icon_play").css("background-position","left top");
    })

    var now = 0;
    $(".vedio_tab a").click(function()
    {
        var aIndex = $(this).index(".vedio_tab a");
        startMove(aIndex)
    });
    function startMove(aIndex)
    {
        $(".vedio_main .main_list").eq(aIndex).fadeIn(200).siblings().fadeOut(200);
        $(".vedio_tab a").eq(aIndex).addClass("tab_current").siblings().removeClass("tab_current");
    };
    $(".next").click(function() {
        now++;
        if( now > $(".vedio_tab a").length-1 ) {
            now = 0;
        };
        startMove(now)
    });
    $(".prev").click(function() {
        now--;
        if( now < 0 ) {
            now = $(".vedio_tab a").length-1;
        };
        startMove(now)
    });
    //视频播放页结束展示推荐列表----end
});
