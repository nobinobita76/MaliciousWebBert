    <script type="text/javascript">
        //新增公交线路展开收起
        jQuery(document).ready(function () {
            var lineList = jQuery('.lineList')
            var totalLine = lineList.children().length;
            var tabBtn = jQuery('.tabBtn');
            if (totalLine > 5) {
                lineList.addClass('hide');
                tabBtn.addClass('hdBtn');
                tabBtn.toggle(
				  function () {
				      jQuery(this).removeClass('hdBtn');
				      jQuery(this).text('收起');
				      lineList.removeClass('hide');
				  }, function () {
				      jQuery(this).addClass('hdBtn');
				      jQuery(this).text('展开');
				      lineList.addClass('hide');
				  }
				);
            }
            else {
                tabBtn.parent().hide();
            }
        });
    
</script>

