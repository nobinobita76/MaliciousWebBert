<script type="text/javascript">
    jQuery(function () {
        if (window.location.href.toString().indexOf("b6")>-1) {
            jQuery("#select_rent_xing").show();//.attr("display", "");
            jQuery("#select_rent_mate").show(); //.attr("display", "");
            jQuery("#select_rent_huxing").hide();//.attr("display", "none");
            
        } else {
            jQuery("#select_rent_xing").hide(); //.attr("display", "none");
            jQuery("#select_rent_mate").hide(); //.attr("display", "none");
            jQuery("#select_rent_huxing").show(); //.attr("display", "");
        }
    });
</script>

          
        
