<script type="text/javascript">
    window.onresize = function () {
        var widt = $(window).width();
        var rigt = (widt - 1190) / 2 - 70;
        $(".goTop").css("right", rigt + "px");
        if (rigt <= 0) {
            $(".goTop").css("right", "20px");
        }
    }
    $(window).resize();
    $(window).scroll(function () {
        var top = $(window).scrollTop();
        if (top <= 400) {
            $(".goTop").fadeOut();
        } else {
            $(".goTop").fadeIn();

        }
    })
</script>
