    $(function ()
    {
        $("#sousuo").click(
            function ()
            {
                var text=$("#word").val();
                if(!text)
                {
                    alert('对不起,请输入您要搜索的关键词');
                    return false;
                }   
                else
                {
                    window.location.href="index.php?p=news_list&word="+text;    
                }
            }
        )
    })
</script> 
