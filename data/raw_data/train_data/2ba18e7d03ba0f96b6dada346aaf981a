function get_seller()
{
	jQuery.ajax({
		type: "post",
		dataType: "json",
		data: null,
		url: "/ajax/getseller?nick=cima%E6%97%97%E8%88%B0%E5%BA%97",
		success: function(data){
			$(".rankimage").attr("src", data.rankimage);
                        			//get_other_info(540750600076,data.sellerid);
                        			get_item_comments(540750600076,data.sellerid,1);
			var pagesize=10;
			// 同店铺商品
			jQuery.ajax({
			        type: "get",
                                dataType: "json",
                                data: null,
                                url: "/ajax/get_seller_items?start="+0+"&seller_id="+data.sellerid+"&pagesize="+pagesize,
				success: function(dataitems){
				    var dataitems=dataitems['API.SSInshopApi']['auctions'];
					var item_iid='540750600076';
					var j=0;
					$.each(dataitems, function(i, dataitem){
						//console.log(dataitem);
						if(dataitem.nid!=item_iid && j<6)
						{
							$(".searching").remove();
							$(".same-seller-product-items").append("<li style=\"width:80px;height:80px;float:left;text-align:center;margin:0 10px 10px 0\"><a rel='nofollow' href='/item/"+ dataitem.nid +".html'><img src='" + dataitem.ext_pict_url + "_80x80.jpg' onload=AutoResizeImage(80,80,this)></a></li>"); 
							j++;
						}else{
                                                    $(".searching").remove();
                                                }
					}); 
				},
				error: function(msg){
					//get_seller();
					
					$(".searching").remove();
					$(".sellerinfo_viewmore").hide();
					$(".sellerinfo_other_c").append("<table width=\"220\" border=\"0\" align=\"center\" cellpadding=\"0\" cellspacing=\"0\"><tr><td height=\"170\" align=\"center\" style=\"color:#999; line-height:30px;\"><img src=\"/Images/icon0603wu.png\" width=\"37\" height=\"38\" /><br />此卖家暂无其他商品</td></tr></table>");
					//$(".sellerinfo").hide();
				}
			});
		},
		error: function(msg){
			//get_seller();
			$(".sellerinfo").hide();
		}
	});
}


function check_and_buy(){
	if(parseInt($(".buy_num").val())>thispage.option.num) return false;
	var price = fceil($("#price").val(),2);
	//price = price.toFixed(2);
        var num = $("#form_next_2").val();
		
		//检测是否选择数量
		if(isNaN(parseInt($(".buy_num").val())) || parseInt($(".buy_num").val())<=0){
			is_submit = false;
		}
        var note = '';var note1 = '';
        var n=0;
		var is_submit = true;
		//检测是否选择属性
		$(".skurows > div.cu_2").each(function(i){
