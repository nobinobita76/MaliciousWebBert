  <script>
    (function () {
        var serverTime = '1490646842';
        var timeOffset = serverTime * 1000 - new Date().getTime();

        function ShowCountDown(dom, serverTimeEnd, timeOffset) {
            var ele = document.getElementById(dom);
            if (ele == null) {
                clearInterval(dTimer);
                return false;
            }

            var leftsecond = parseInt(serverTimeEnd * 1000 - new Date().getTime() - timeOffset);
            var day1 = Math.floor(leftsecond / (60 * 60 * 24 * 1000));
            var hour = Math.floor((leftsecond / 1000 - day1 * 24 * 60 * 60) / 3600);
            var minute = Math.floor((leftsecond / 1000 - day1 * 24 * 60 * 60 - hour * 3600) / 60);
            var second = Math.floor(leftsecond / 1000 - day1 * 24 * 60 * 60 - hour * 3600 - minute * 60);
            var ms = Math.floor(leftsecond - day1 * 24 * 60 * 60 * 1000 - hour * 3600 * 1000 - minute * 60 * 1000 - second * 1000);

            if (hour < 10) hour = "0" + hour;
            if (minute < 10) minute = "0" + minute;
            if (second < 10) second = "0" + second;
            if (ms < 100) {
                ms = "0" + ms;
            } else if (ms < 10) {
                ms = "00" + ms;
            }
            ele.innerHTML = day1 + "天" + hour + "时" + minute + "分" + second + "秒" + ms;
            if (leftsecond <= 0) {
                clearInterval(dTimer);
                ele.innerHTML = "0天00时00分00秒000";
                if (localStorage.getItem("isEnd3") == null) {
                    localStorage.setItem("isEnd3", "true");
                    window.location.reload();
                }
            }
        }

        
       var serverTimeEnd = '1490716800';
        if (serverTime > serverTimeEnd) {
            $('#start_end').text('距离活动结束还有：');
            serverTimeEnd = '1490716800';
        } else {
            $('#start_end').text('距离活动结束还有：');
        }

        
        var dTimer = window.setInterval(function () {
            ShowCountDown('countdown', serverTimeEnd, timeOffset);
        }, 1);
    }());

</script> 
