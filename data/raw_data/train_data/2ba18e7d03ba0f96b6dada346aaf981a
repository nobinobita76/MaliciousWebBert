            $(".sel_wrap label").on("click", function () {
                if ($(".sel_wrap div").css("display") == "block") $(".sel_wrap div").hide();
                else $(".sel_wrap div").show();
            })
            $(".sel_wrap span").on("click", function () {
                var o;
                var opt = $(".sel_wrap").find('span');
                $(".sel_wrap div").hide();
                $(".sel_wrap span").attr("selected", false);
                $(this).attr("selected", true);
                $(".sel_wrap label").html($(this).html());
