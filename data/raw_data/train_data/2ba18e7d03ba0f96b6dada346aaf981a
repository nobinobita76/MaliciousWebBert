$(".country-list").children('ul').children('li').click(function()
{
   var abbr=$(this).children('a').children('.english').attr('data-abbr');
   var name=$(this).children('a').children('.english').html();
   $('.curCountry').attr('data-country-name',name);
   $('.curCountry').html(name);
   $('.curCountry').attr('data-abbr',abbr);
   $('.curCountry').siblings('em').attr('class','litb-icon-flag '+abbr);
   $('.dropdown-menu').hide();
   $('.country-selector').removeClass("open");
   var ch_name=$(this).children('a').children('.chinese').html();
   $('#curCountry_name').html(ch_name);
   var position=$(this).parent().parent().parent().attr('position');
   if(position!='popup')
   {     
	  record_visit(name,ch_name,abbr,0);
   }
});

function show_countries(val)
{
  var keywords=val.trim().toUpperCase();
  $(".reco-country-list").children('li').each(function()
  {
     var name=$(this).children('a').children('.english').html().toUpperCase();
	 var ch_name=$(this).children('a').children('.chinese').html();
	 if(name.indexOf(keywords)==0||ch_name.indexOf(keywords)==0)
	 {
	    $(this).show();
	 }
	 else
	 {
	    $(this).hide();
	 }
  
  });

   $(".full-country-list").children('li').each(function()
  {
     var name=$(this).children('a').children('.english').html().toUpperCase();
	 var ch_name=$(this).children('a').children('.chinese').html();
	
	 if(name.indexOf(keywords)==0||ch_name.indexOf(keywords)==0)
	 {
	    $(this).show();
	 }
	 else
	 {
	    $(this).hide();
	 }
  
  });
  
}

function close_country_popup()
{
    var name=$("#popup_curCountry").html();
	var abbr=$("#popup_curCountry").attr('data-abbr');
    var ch_name=$("#popup_curCountry_ch").html();
	record_visit(name,ch_name,abbr,1);
	$('.fcc20150128-box1').hide();
	$('#zhezhao_bg').hide();
}

function record_visit(name,ch_name,abbr,is_popup)
{
   		$.ajax({
			type: "post",
			dataType: "json",
			url: "/ajax/record_visit",
			data:"name="+name+"&abbr="+abbr+"&ch_name="+ch_name+"&is_popup="+is_popup,
			success: function(data){
			},
			error: function(msg){
			}
	   });
}
</script> 
  <script type="text/javascript">
function AutoResizeImage(maxWidth,maxHeight,objImg){
	var img = new Image();
	img.src = objImg.src;
	var hRatio;
	var wRatio;
	var Ratio = 1;
	var w = img.width;
	var h = img.height;
	wRatio = maxWidth / w;
	hRatio = maxHeight / h;
	if (maxWidth ==0 && maxHeight==0){
		Ratio = 1;
	}else if (maxWidth==0){
		if (hRatio<1) Ratio = hRatio;
	}else if (maxHeight==0){
		if (wRatio<1) Ratio = wRatio;
	}else if (wRatio<1 || hRatio<1){
		Ratio = (wRatio<=hRatio?wRatio:hRatio);
	}
	if (Ratio<1){
		w = w * Ratio;
		h = h * Ratio;
	}
	objImg.height = h;
	objImg.width = w;
}



$(".right_menu").click(function()
{
   $(".right_menu").children('a').attr('class','');
   $(this).children('a').attr('class','selected');
   var index=$(this).index();
   $('.QAnr2015-box-1').hide();
   $('.QAnr2015-box-1').eq(index).show();
}
);

</script> 
