  <script type="text/javascript">
function showDate(){
	var zone_time = -new Date().getTimezoneOffset() / 60;
    var zone_time_offset = (8 - zone_time) * 3600;

	nowTime.setTime((Math.round(new Date().getTime()/1000) + time_offset + zone_time_offset) * 1000);
	hs = nowTime.getHours().toString(); //时
	ms = nowTime.getMinutes().toString(); //分
	ss = nowTime.getSeconds().toString(); //秒

	if(hs<10)
	{
		h1=0;
		h2=hs;
	}
	else
	{
		h1=hs.substring(0,1);
		h2=hs.substring(1);
	}
	if(ms<10)
	{
		m1=0;
		m2=ms;
	}
	else
	{
		m1=ms.substring(0,1);
		m2=ms.substring(1);
	}
	if(ss<10)
	{
		s1=0;
		s2=ss;
	}
	else
	{
		s1=ss.substring(0,1);
		s2=ss.substring(1);
	}

	global_h1.html(h1);
	global_h2.html(h2);
	global_m1.html(m1);
	global_m2.html(m2);
	global_s1.html(s1);
	global_s2.html(s2);

	window.setTimeout( showDate, 1000);
}

//获取首次加载时服务器与客户端时间差值
var server_time='1490646842';
var local_time = Math.round(new Date().getTime()/1000);
var time_offset = server_time - local_time;
var currentDT = new Date();
var y = currentDT.getFullYear(); //四位整数表示的年份
var m = currentDT.getMonth()+1; //月
	m = m/10<1?"0"+m:m;
var date = currentDT.getDate(); //日
	date = date/10<1?"0"+date:date;
$(".clock_date").html(y+"-"+  m +"-"+date);
var nowTime = new Date();
var hs,ms,ss,h1,h2,m1,m2,s1,s2;
var global_h1 = $(".global_hour_time1");
var global_h2 = $(".global_hour_time2");
var global_m1 = $(".global_min_time1");
var global_m2 = $(".global_min_time2");
var global_s1 = $(".global_sec_time1");
var global_s2 = $(".global_sec_time2");

showDate();
</script> 
