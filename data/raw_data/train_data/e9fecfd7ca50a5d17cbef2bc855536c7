<script>
  function Percentage(_num, _total) { 
    return (Math.round(_num / _total * 10000) / 100.00 + "%"); 
  }

  $(function(){
    //相关资讯
    var _tag=$('.article_tag').val();
    $.get("/index.php?c=product&m=ajax_product_article&tag="+_tag, function(html){
      $('.pro-news').html(html);
    }, 'html');
    //网友评论
    $.get("/index.php?c=product&m=ajax_get_comment&id=366419", function(data){
      var _score_html="", _comment_html="", extend=data.result.extend, comment=data.result.comment;
      if(extend){
        //评分概况
        _score_html +="<div class=\"total-comment-left\"><div class=\"star2\"><div class=\"star\" style=\"width: "+Percentage(extend.average_num, 5)+";\"></div></div><div class=\"total-score\"><em>"+extend.average_num+"</em>/满分5.0</div><div class=\"total-discuss\">共有<em>"+extend.pinglun_num+"</em>条网友点评</div><div class=\"compose-bg\"><a href=\"#comment\" class=\"compose\">我来点评</a></div></div>";
        _score_html +="<div class=\"total-comment-right\">";
        $.each(extend.starts_num, function(k, v){
            _score_html +="<div class=\"comment-content star-one clearfix\"><div class=\"star-s2\"><div class=\"star-s1\" style=\"width:"+Percentage(k, 5)+";\"></div></div><em class=\"score\">"+k+".0</em><span class=\"w-bar2\"><span class=\"w-bar1\" style=\"width: "+Percentage(v, extend.pinglun_num)+";\"></span></span><div class=\"user-num\"><span>"+v+"</span>人</div></div>";
        });
        _score_html +="</div>";
      }
      if(comment){
        //精华评论
        _comment_html += "<div class=\"user-pic\"><img src=\""+comment.add_head+"\" data-bd-imgshare-binded=\"1\"></div><ul class=\"comment-detail-show\"><li class=\"clearfix\"><div class=\"user-time\"><em>"+comment.add_name+"</em><span>"+comment.add_time+"</span></div><div class=\"user-star-num\"><div class=\"star-s2\"><div class=\"star-s1\" style=\"width: "+Percentage(comment.starts, 5)+";\"></div></div><em class=\"score\">"+comment.starts+"</em></div></li><li><h3 class=\"h3-sty\">"+comment.title+"</h3></li>";
        if(comment.advantage){
          _comment_html +=  "<li class=\"comment-list\"><label class=\"comment-title red-txt\">优点：</label><p>"+comment.advantage+"</p></li>";
        }
        if(comment.shortcoming){
          _comment_html +=  "<li class=\"comment-list\"><label class=\"comment-title green-txt\">缺点：</label><p>"+comment.shortcoming+"</p></li>";
        }
        _comment_html +=  "<li class=\"comment-list\"><label class=\"comment-title\">总结：</label><p>"+comment.content+"</p></li></ul><div class=\"to-do\" _id=\""+comment._id+"\"><a class=\"reply\" href=\"/vga/comment/366419.html#comment_"+comment._id+"\">回复(<span>"+comment.reply_num+"</span>)</a><a class=\"thumbs-up\">有帮助(<span>"+comment.ding_num+"</span>)</a><a class=\"thumbs-down\">无帮助(<span>"+comment.cai_num+"</span>)</a></div>";
      }
      
      if(_score_html){
        $('.total-comment').html(_score_html);
      }
      if(_comment_html){
        $('.comment-detail').html(_comment_html);
        //like unlink
        $('.thumbs-up, .thumbs-down').on('click', function(){
                    alert("请登录后进行相关操作");
                  });
      }
    }, 'json');
    //简单评论
    $('.submit-btn').on("click", function(){
      var _score=$('.score').text(), _feeling=$('.feeling').val();
      if(_feeling){
        $.post("/index.php?c=product&m=ajax_post_comment", {id:366419, score:_score, feeling:_feeling}, function(data){
          console.info(data);
          alert(data.errmsg);
        }, 'json');
      }else{
        alert("请填写您的使用感受");
      }
    });
    //
    $('.go-detail').on('click', function(){
      window.location.href="/vga/comment/366419.html"
    });
  });
</script>
