  <script type="text/javascript">
    if(("#hyg-f-djs")){
        var role = $("#hyg-f-djs").find("span").attr("time");
        timer($("#hyg-f-djs").find("span"),role);

    }
    function timer(obj, leftsecond) {
        leftsecond = Math.ceil(leftsecond / 1000);
        if (leftsecond > 0) {
            var t = setInterval(function () {
                leftsecond--;
                var day = Math.floor(leftsecond / (60 * 60 * 24));
                var hour = Math.floor((leftsecond - day * 24 * 60 * 60) / 3600);
                var minute = Math.floor((leftsecond - day * 24 * 60 * 60 - hour * 3600) / 60);
                var second = Math.floor(leftsecond - day * 24 * 60 * 60 - hour * 3600 - minute * 60);
                function timeTest(s) {
                    return s = s >= 10 ? s : "0" + s;
                }
                $(obj).html( String(timeTest(hour)) + ":" + String(timeTest(minute)) +":" + String(timeTest(second)));
                if (leftsecond == 0) {
                    clearInterval(t);
                    //延迟1s后刷新
                    setTimeout(function(){ 
                        window.location.reload();
                    },1000)
                }
            }, 1000)
        }
    }
    // 查看夺宝码
    function layerOpen(code) {
        layer.open({
            type: 1,
            closeBtn: 1,
            skin: "layer-pad",
            title: ["夺宝码", "background:#DF231C;color:#fff"],
            shift: 2,
            area: ["300px"],
            shadeClose: true,
            content: code
        });
    }
    function layerTips(e, msg) {
        layer.tips(msg, $(e), {
            tips: [3, '#df231c']
        });
    }

    $(function () {
        function Loadingfirst(url,size,callback){
            $.ajax({
                url: url,
                type: 'POST',
                dataType: 'json',
                data: {
                    shopid:114546,
                    pageindex:1,
                    pagesize:size
                },
                success:callback
            })
        }
        //夺宝记录-加载第一页
        Loadingfirst("/ShopDetail/GetCanyuRecord",50,function(data){
                if(data.list.length>0){
                    var template = Handlebars.compile($("#box2").html());
                    var html = template(data);
                    $(".hyg-f-itm2").html(html);
                    $(".hyg-f-itm2").append('<div class="hyg-f-page dark-pagination-one" id="dark-pagination"></div>')
                    paging(".dark-pagination-one", data.allpages, callback);
                    allcodeshow();
                }else{
                    $(".hyg-f-itm2").html('<div class="norecordshow"><img src="http://img1.97hyg.com/pc/images/noRecord.png" alt="" /><span>暂无往期记录</span></div>');
                }
            }
        )
        //往期夺宝-加载第一页
        Loadingfirst("/ShopDetail/GetHistoryDuobao",10,function(data){
            if(data.list.length>0){
                var template = Handlebars.compile($("#box4").html());
                var html = template(data);
                $(".hyg-f-itm4 ul.hyg-f-html").html(html);
                paging(".dark-pagination-three", data.allpages, callback3);
            }else{
                $(".hyg-f-itm4").html('<div class="norecordshow"><img src="http://img1.97hyg.com/pc/images/noRecord.png" alt="" /><span>暂无夺宝记录</span></div>');
            }
        })
        //晒单-加载第一页
        Loadingfirst("/ShopDetail/GetSdShareList",10,function(data){
            if(data.list.length>0){
                var template = Handlebars.compile($("#box3").html());
                var html = template(data);
                $(".hyg-f-itm3 ul.hyg-f-html").html(html);
                var $sd_llist = $(".hyg-f-itm3").find(".shai_time");
                for(var i=0;i<$sd_llist.length;i++){
                    var old_time = $sd_llist.eq(i).html();
                    $sd_llist.eq(i).html(getCurrentFormatDate(old_time));
                }
                paging(".dark-pagination-two", data.allpages, callback2);
            }else{
                $(".hyg-f-itm3").html('<div class="norecordshow"><img src="http://img1.97hyg.com/pc/images/noRecord.png" alt="" /><span>暂无晒单记录</span></div>');
            }
        })
        //分页
        var sval,shopid=114546;
        var pageindex=1,pagesize=1,tab;
        
        function callback(page){
            //夺宝记录 tab区分回调
            tab=2;
            pageindex=page;
            pagesize=50;
            recordAjax("/ShopDetail/GetCanyuRecord");
        }
        function callback2(page){
            //晒单
            tab=3;
            pageindex=page;
            pagesize=10
            recordAjax("/ShopDetail/GetSdShareList");
        }
        function callback3(page){
            //往期夺宝
            tab=4;
            pageindex=page;
            pagesize=10
            recordAjax("/ShopDetail/GetHistoryDuobao");
        }
        //夺宝记录，晒单，往期夺宝ajax请求
        function recordAjax(url){
            sval={
                shopid:shopid,//商品ID
                pageindex:pageindex,
                pagesize:pagesize
            }
            $.ajax({
                url: url,
                type: 'POST',
                dataType: 'json',
                data: sval,
                success:recordcallback
            })
        }
        //夺宝记录，晒单，往期夺宝ajax请求回调
        function recordcallback(data){
            if(data.list.length){
                var template = Handlebars.compile($("#box"+tab).html());
                var html = template(data);
                if(tab==2){
                    //夺宝记录
                    $(".hyg-f-itm2 img").remove();
                    $(".hyg-f-itm2 p").remove();
                    $(".hyg-f-itm2").prepend(html);
                    //$(".hyg-f-itm2").html(html);
                    allcodeshow();
                }else{
                    $(".hyg-f-itm"+tab).find("ul.hyg-f-html").html(html);
                }
                if(tab==3){
                    var $sd_llist = $(".hyg-f-itm3").find(".shai_time");
                    for(var i=0;i<$sd_llist.length;i++){
                        var old_time = $sd_llist.eq(i).html();
                        $sd_llist.eq(i).html(getCurrentFormatDate(old_time));
                    }
                }
            }
        }
        // 商品展示图片的切换
        $(".hyg-f-details_tu ul.hyg-f-cptulist li").on("click", function () {
            $(".hyg-f-details_tu div img").attr("src", $(this).children("img").attr("src"));
            $(this).addClass("hyg-f-active");
            $(this).siblings().removeClass("hyg-f-active")
        })
        // 商品详情，参与夺宝，晒单，往期夺宝的切换
        $(".hyg-f-dateiltab>p:first span").on("click", function () {
            $(this).addClass("hyg-f-tabactive");
            $(this).siblings().removeClass("hyg-f-tabactive");
            $(".hyg-f-dateiltab>div>div").eq($(this).index()).show();
            $(".hyg-f-dateiltab>div>div").eq($(this).index()).siblings().hide();
        })
        // 减人次
        $(".hyg-f-add p span").eq(0).on("click", function () {
            var n = $(this).siblings("input").val();
            if (n > 1) {
                n--;
                $(this).siblings("input").val(n);
            }
        })
        // 加人次
        $(".hyg-f-add p span").eq(1).on("click", function () {
            var n = $(this).siblings("input").val();
            n++;
            change(n);
        })
        // 手动输入人次
        $(".hyg-f-add p input").on("keydown", function () {
            setTimeout(function () {
                var n = $(".hyg-f-add p input").val();
                n = n.replace(/[^\d]/g, '');
                change(n);
            }, 10)
        })
        // 失去焦点时input为""时，设置为默认的1
        $(".hyg-f-add p input").on("blur", function () {
            var n = $(".hyg-f-add p input").val();
            if (n == "") {
                $(".hyg-f-add p input").val(1);
            }
        })
        // input的value值改变时
        function change(n) {
            var max = parseInt($("#shengyu").html());
            if (n > max) {
                $(".hyg-f-add p input").val(max);
            }
            else {
                $(".hyg-f-add p input").val(n);
            }
        }
        // 禁止复制
        $(".hyg-f-add p input").on("paste", function () {
            return false;
        })

        //加载商品详情
        //loadGoodsDescribe(shopid);
    })

    function loadGoodsDescribe(shopid){
        $.post('PostGetGoodsDescribe',{"shopId":shopid},function(data){
            if(data){
                $('#GoodsDescribeContent').append(data.content);
            }
        });
    }
    //全价购买未登录-弹窗登录
    function pricelayer(){
        $.ajax({
            url: '/Account/GetCurrPaymentInfo',
            type: 'POST',
            dataType: 'json',
            data: {},
            success:function(data){
                if(data.code==0){
                    quickLogin();
                }else{
                    window.location.href="/Order/ShopPayList?shopids=114546&bfullprice=1";
                }
            }
        })
    }
    //点击所有夺宝码
    //allcodeshow();
    function allcodeshow(){
        var $list = $(".hyg-f-itm2").find(".get-time");
        for(var i=0;i<$list.length;i++){
            var old_time = $list.eq(i).html();
            $list.eq(i).html(getCurrentFormatDate(old_time));
        }
        //夺宝记录-所有夺宝码
        $(".hyg-f-itm2 p").mouseenter(function(){
            $(this).find(".hyg-allcode").show();
        }).mouseleave(function(){
            $(this).find(".hyg-allcode").hide();
        })
        $(".hyg-allcode").on("click",function(){
            $(this).siblings(".hyg-codehide").addClass("hyg-codeshow");
            var text = $(this).siblings(".hyg-codeshow").html();
            $(this).siblings(".hyg-codehide").html(text.replace(/,/g,"  "));
            close();
            var height = $(this).siblings(".hyg-codeshow").height();
            var i = $(this).parent().index();
            var h = (i==1?36:46);
            $(this).parent().height(height+h);
        })
    }
    function close(){
        $(".hyg-f-itm2 .close").on("click",function(){
            $(this).parent().removeClass("hyg-codeshow");
            var i = $(this).parent().parent().index();
            var h = (i==1?36:46);
            $(this).parent().parent().height(h);
        })
    }
    function immediatelyBuy(title,price,num,residue,shopid){
        // title 商品标题 price 云价格  num 购买的数量  residue 剩余人次  shopid  商品ID
        $.ajax({
            url: '/Account/GetCurrPaymentInfo',
            type: 'POST',
            dataType: 'json',
            data: {},
            success:function(data){
                if(data.code==0){
                    quickLogin();
                    // window.location.href="/Account/Login";
                }else{
                    var fullbuy=0;
                    var shoplist=new Array();
                    var arr = {'shopid':shopid,'num':num}
                    shoplist.push(arr);
                    shoplist = JSON.stringify(shoplist);
                    var gong = price*num;
                    $.ajax({
                        url: '/Order/PostCreateOrder',
                        type: 'POST',
                        dataType: 'json',
                        data: {
                            fullbuy:fullbuy,
                            shopinfo:shoplist,
                            money:gong,
                        },
                        success:function(data){
                            if(data.code==1){
                                window.location.href="/Order/OrderPayWay?orderid="+data.orderid;
                            }

                        }
                    })
                }
            }
    })
}
</script> 
