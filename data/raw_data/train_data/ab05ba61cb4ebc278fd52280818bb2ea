      <script>        $('.page-part').click(function(){
            var turn_page = $('.turn-page').val();
            if(turn_page == '') {
                return false;
            }
            var url = $.setUrlParam('page', turn_page);
            location.href=url;
            return false;
        });
       $(document).keydown(function(event){ 
		if (event.keyCode == 13){
			if($('.page .turn-page').is(':focus') == true){
				var turn_page = $('.turn-page').val();
				if(turn_page == '') {
		            return false;
		        }else{
		        var url = $.setUrlParam('page', turn_page);
		        location.href=url;
		        }
			}
		};
	});

    //分页下拉选择
    $('select[name="per-page"]').change(function(){
        var url = $.setUrlParam('per-page', $(this).val());
        location.href=url;
    })	</script> 
