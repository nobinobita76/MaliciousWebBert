   <script>
 		$('.group-link').click(function(){
 			var obj = $(this);
 			obj.ajax({
 				url: '/site/change-group?default_group_id='+obj.data('group_id'),
 				success: function(){
 					console.log('caca');
 					location.reload();
 				}
 			})
 		});
 		$('.login-btn').click(function(){
 			var obj = $(this);
 			var ajaxData = {
 				"account": $("#username").val(),
 				"password":$("#password").val()
 			};

 			obj.ajax({
 				url: '/shop/login-account',
 				type: 'post',
 				data: ajaxData,
 				noLoad:true,
 				success: function(data){
                               //登录成功
                               if(data.status=="1")
                               {
                               	for ( var src in data.crossDomain ) {
                               		var img = $("<img src='"+data.crossDomain[src]+"'/>");
                               		$("body").append(img);
                               	}

                               	setTimeout(function(){
                               		location.reload();
                               	},500);
                               }
                               else
                               {
                               	$('.error-tip').html('登录失败，用户名或密码错误');
                               	$('.error-tip').show();
                               }
				//
			}
		})
 		});
 	</script>
