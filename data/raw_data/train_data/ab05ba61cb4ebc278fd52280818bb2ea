   <script>
	function isJson (obj){
		var isjson = typeof(obj) == "object" && Object.prototype.toString.call(obj).toLowerCase() == "[object object]" && !obj.length;    
		return isjson;
	}
	$('.take-stock').click();
	$('.area-stock').each( function(){
		var obj = $(this).attr('area_stock_str');
		obj =  "\"" + obj + "\"";
		var obj1 = eval('('+obj+')');
		var newObj =  eval('('+obj1+')');
		var str = '';
		for(var i=0; i<newObj.length; i++){
			if ( str != '' ) {
				str = str + '<br>';
			}
			str = str + newObj[i].location + '(' + newObj[i].inventory + ')'
		}
		$(this).html(str);
	});
</script>
