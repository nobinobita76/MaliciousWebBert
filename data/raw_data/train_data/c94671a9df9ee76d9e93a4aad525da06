<script>
                data = [{'videoURL':'http://rstu.tuxiaobei.com/mp4/271.mp4','Thumbnail':'images/mmwd.jpg'},{'videoURL':'http://rstu.tuxiaobei.com/mp4/277.mp4','Thumbnail':'images/xb.jpg'},{'videoURL':'http://rstu.tuxiaobei.com/mp4/011.mp4','Thumbnail':'images/std.jpg'}];
	function playTo(n){
		if($("#videoWrap").length){
		                $("#videoWrap").attr("src",data[n].videoURL);	
                                                $("#videoWrap").attr("poster",data[n].Thumbnail);	
		}else{
			$(".play_video").html('<video id="videoWrap" src="'+data[n].videoURL+'" poster="'+data[n].Thumbnail+'" style="width:100%;height:100%;" controls="controls"></video>');
                               }	
                      $('html,body').animate({scrollTop: '0px'}, 500);
                  };  

//window.setTimeout(show,15000); 
function show() 
{ 
Media = document.getElementById("videoWrap"); 
if(Media.currentTime == 0)
{
document.getElementById("jumplink").click();
}
} 
		
