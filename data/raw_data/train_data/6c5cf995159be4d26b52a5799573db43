    <!-- 页面加载 -->
    <script>
        (function () {
            var href = location.href,
                label = "";

            if(/\/novel\//.test(href)) {
                label = "介绍页";
            } else if (/\/chaplist\//.test(href)) {
                label = "目录页";
            } else if (/\/chapter\//.test(href)) {
                label = "阅读页";
            } else if (/\/abook\/[a-zA-Z0-9]*\.html/.test(href)) {
                label = '有声阅读页';
            } else {
                label = document.title;
            }

            $(window).load(function() {
                // 延迟发送
                setTimeout(function () {
                    if (Statistics) {
                        // 页面所有内容加载完成之后（包括图片等）
                        Statistics.trackEvent('页面加载', '页面', label, (new Date()).getTime() - cnzzLoadStartTime, '');
                    }
                }, 0);
            });

            $(function() {
                // 延迟发送
                setTimeout(function () {
                    if (Statistics) {
                        // DOM结构绘制完毕
                        Statistics.trackEvent('页面加载', 'DOM', label, (new Date()).getTime() - cnzzLoadStartTime, '');
                    }
                }, 0);
            });
        })();
    </script>


