    ,btn: ['\u786e\u8ba4\u652f\u4ed8', '\u5173\u95ed\u7a97\u53e3']
    ,success: function(index){
      document.getElementById("rewardvalue").focus();
    }
    ,yes: function(index){
$.getJSON('plugin.php?id=mapp_reward&act=showqrcode&formhash=c5236e2e&tid='+tid+'&price='+document.getElementById("rewardvalue").value,function(data){
        if(data.status == 1){
            data.wxapiparameters = eval('('+data.wxapiparameters+')');
            if(data.type == 'jsApiCall'){
                WeixinJSBridge.invoke(
                        'getBrandWCPayRequest',
                        data.wxapiparameters,
                        function(res){
                            if(res.err_msg == "get_brand_wcpay_request:ok"){
                                window.location.href='forum.php?mod=viewthread&tid='+tid;
                            }
//alert(data.wxapiparameters);
                            alert(res.err_code+res.err_desc+res.err_msg);
                        }
                );
            }
        }else{
tips = eval("'" + data.tips + "'");
            alert(tips);
        }

});
     
    }
  });
}

 
