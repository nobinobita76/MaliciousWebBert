$(".img-rank").each(function(i){
		$(this).find('li').each(function(j){
			 j=j+1;
			$('em', $(this)).text(j);
				})
			$(this).find('li:lt(3)').find("em").addClass("top");
		$(this).find('li').hover(
			function () {
				$("li",$(this).parent()).removeClass("big");
				$(this).addClass("big");
			});
		$(this).find('li:eq(0)').addClass("big");
}); 
