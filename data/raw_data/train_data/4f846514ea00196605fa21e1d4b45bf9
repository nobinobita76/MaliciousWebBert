function fliter(){
        var b=[];
		var input=document.getElementById('q');
		var fliter=new Array('script','alert','img','javascript:alert','iframe','msgbox','vbscript','<','>');
		var vv=input.value;
		for(var i=0;i<fliter.length;i++){
			var a=vv.indexOf(fliter[i]);
			if(a!=-1){
			 	b.push(a);
			}
		}
		if(b.length!=0){
		    alert('您输入的内容含有非法字符，请重新输入!');
