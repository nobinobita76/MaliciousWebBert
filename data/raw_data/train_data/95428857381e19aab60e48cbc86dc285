  <script type="text/javascript">
	
$(function(){
	
	var u = navigator.userAgent;
	var anbtnhide=document.getElementById('anbtnhide');
	var iosbtnhide=document.getElementById('iosbtnhide');
	var m_down_andzz=document.getElementById('m_down_andzz');
	var m_down_iosdzz=document.getElementById('m_down_iosdzz');
	
	if (u.indexOf('Android') > -1 || u.indexOf('Linux') > -1) {
		if(anbtnhide){
			anbtnhide.style.display="block";
		}
		
		m_down_andzz.style.display="block";
	}else if(u.indexOf('iPad') > -1 || u.indexOf('iPhone') > -1 || u.indexOf('iPod') > -1){
		if(anbtnhide){
			iosbtnhide.style.display="block";
		}
		
		m_down_iosdzz.style.display="block";  
	}else{
		if(anbtnhide){
			anbtnhide.style.display="block";
		}
		m_down_andzz.style.display="block";	
	}
	

	if(isWeiXin()){
			$('#m_down_and').attr('href','');
                        $('#m_down_ios').attr('href','');
			
			
	}
	$('#m_down_and').click(function(){
	
	if(isWeiXin()){
			
			$('#mask').show();
			return false;
	}	
	
	});
      $('#m_down_ios').click(function(){
	
	if(isWeiXin()){
			
			$('#mask').show();
			return false;
	}	
	
	});
      $('#mask').click(function(){
	
		$(this).hide();
	
	})
})
