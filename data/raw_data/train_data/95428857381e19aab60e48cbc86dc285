$(function(){
	//顶部互动菜单
	(function(){
		$('.guide-icon-nav i').click(function(){
		var iDocuH=$(document).height();
		$('.mmaskbox2').height(iDocuH);
			$('.mmaskbox2').toggle();
			if($('.mmask .leftmeun').hasClass('show')){
	
				$('.mmask .leftmeun').removeClass('show');
			}else{
				$('.mmask .leftmeun').addClass('show');
			}
		})	
	})();
$('.hidetable td').attr('type','btn')	
 $('.bGameD .close').click(function(){$('.bGameD').hide()});	
		var aslider=$('#slider .div1');
		aslider.eq(0).addClass('show');
		var num=-1;
		$('#next').click(function(){
				num++;
				if(num==aslider.length-1){
					aslider.eq(0).addClass('show').siblings().removeClass('show');
					num=-1;
					}else{
			   aslider.eq(num).next().addClass('show').siblings().removeClass('show');
					}
		})
	
	
		
})

			
function isWeiXin(){
var ua = window.navigator.userAgent.toLowerCase();
if(ua.match(/MicroMessenger/i) == 'micromessenger'){
return true;
}else{
return false;
}
}


