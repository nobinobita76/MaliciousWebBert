<script type="text/javascript">
    $(function() {
        $('.top').click(function() {
            $(this).parent().find('td').find('.Triangle_con').removeClass('show').removeClass('t').addClass('hide');
            $(this).find('.Triangle_con').removeClass('hide').addClass('show').addClass('t');
        });
        $('.top').hover(function() {
            if ($(this).find('span').attr('class').indexOf('hide') > 0) {
                $(this).find('.hide').removeClass('hide').addClass('show');
            }
        }, function() {
            if ($(this).find('span').attr('class').indexOf('t') < 0) {
                $(this).find('.show').removeClass('show').addClass('hide');
            }
        });
    });
</script>
