<script type="text/javascript">
function friendUrlChange(urlType)
{
    if(urlType=="2")
    {
        if($("#innerHref option:selected").val()!="")
        {
            //window.location.href=$("#innerHref option:selected").val();
             window.open($("#innerHref option:selected").val());
        }
       
    }
    else if(urlType=="3")
    {
        if($("#outerHref option:selected").val()!="")
        {
            //window.open($("#outerHref option:selected").val());
             window.location.href=$("#innerHref option:selected").val();
             window.open($("#outerHref option:selected").val());
        }
    }
}
</script>
