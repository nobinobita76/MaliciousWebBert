	<script>
	
		(function(rc){
			rc.onpageload && rc.onpageload({
				/**sourcelist-start**/
				resources: [
					"/resources/css/build/global/combine.min.css",
					"/resources/css/index.min.css",
					"/resources/css/build/page.css",
					"/resources/css/dev/check.css",
	                '/resources/js/heightchart.js',
	                {
						depend: ['/resources/js/heightchart.js'],
						path: '/static/app.min.js'
					}
				],
				/**js-sourcelist-end**/
				default_url: '#/check/index/',
				before_load: function(next) {
					next && next();
				},
				onload: function(next) {
					next && next();
				},
				complete: function(next) {
					$('#J_app-mask-app-load').hide();
					next && next();
					//console.log(route_conf);
					rc.init({
						route_conf: route_conf,
						user_data: {}
					});
					if (!location.search) {
						return;
					}
					var match = location.search.match(/nick=([^=&]+)/)
							, nick = (match && match[1]) || '';
	
					$.cookie('nick', decodeURIComponent(nick));
				}
			}, location.href);
		})(RayCloud);
	
	
	
	</script>
