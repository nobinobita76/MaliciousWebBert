    </div>
	 <script>
        $(function(){
            //获取章节id
            function getLastString(str){
                var arr = str.split("/"),
                newStr = arr[arr.length-1];
                return newStr;
            }
            var url =  window.location.href;
            var chapterid = getLastString(url);
            $(".book-chapter a").each(function(i){
                var url = $(this).attr("href"),
                        chapterID = getLastString(url);
                if(chapterid == chapterID){
                    $(this).parent().addClass("current");
                    var chapterTop = $(this).parent().offset().top;
                    $('html,body').animate({ scrollTop: chapterTop },1);
                }
            });
        });
    </script>

</body></html>