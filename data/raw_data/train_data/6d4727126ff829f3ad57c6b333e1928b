function sel_area(city_no,city_name){
	Service.excuteNoLoading("WB_SEL_AREA",false,{"CITY_NO":city_no,"CITY_NAME":city_name,"HAS_SEL":"1"},function(reply){
		var ret = reply.SEL_AREA_FLAG;
		if(ret && ret=='1'){window.location.reload();}
	})
}

