<script type="text/javascript">
$(function() {
	$(".shopping").hover(
		function(){
			$(this).addClass("index2");
			$(this).find(".shopping_frame").show();
		},
		function(){
			$(this).removeClass("index2");
			$(this).find(".shopping_frame").hide();  
		}
	);
	
	$.ajax({
		 type:"POST",
		 url:"/public/shop_car_det.jsp",
		 beforeSend:function(){
			 $(".shopping_frame_center").empty();
			 $(".shopping_frame_center").mask("正在加载...");
		 },
		 success:function(msg){
			 $(".shopping_frame_center").html($.trim(msg));
		 },
		 complete:function(){
			 $(".shopping_frame_center").unmask();
		 }
	});
});
</script>
