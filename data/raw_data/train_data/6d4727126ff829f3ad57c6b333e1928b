<script language="JavaScript" type="text/JavaScript">
<!--	
//控制辅助填写框	
function ShowDetails(){
	//alert(document.getElementById("selectDetails").checked);
	if(document.getElementById("selectDetails").checked){
		$(".details").show();
	}else{
		$(".details").hide();
	}
}	
function doValid(){
	var hForm = document.getElementById("frmReg");
	if(hForm==null) {
		return false;
	}
	if(document.getElementById("agr").checked!=true){
		//alert("请仔细阅读注册用户协议内容并确认!");
		pop({text:'请仔细阅读注册用户协议内容并确认!'},3);
		return false;
	}
	var strLogName = hForm.elements["LOGON_NAME"].value;
	var rule = /^[\w-\.]+$/;
	//网站帐号
	if($.trim($("#LOGON_NAME").val())==""){
		hForm.elements["LOGON_NAME"].focus();
		$("#LOGON_NAME").addClass("input_failed");
		$("#LOGON_NAME_vtip").addClass("uname_fail"); 
		$("#LOGON_NAME_vtip").text("网站帐号不能为空。");
		return false;
	}
	if($("#LOGON_NAME").val().length>20 || $("#LOGON_NAME").val().length<4){
		hForm.elements["LOGON_NAME"].focus();
		$("#LOGON_NAME").addClass("input_failed");
		$("#LOGON_NAME_vtip").addClass("uname_fail"); 
		$("#LOGON_NAME_vtip").text("网站帐号仅包含(a-z A-Z 0-9 _)且长度不小于4位，不大于20位。");
		return false;
	}	
	if (strLogName.match(rule) == null){
		hForm.elements["LOGON_NAME"].focus();
		$("#LOGON_NAME").addClass("input_failed");
		$("#LOGON_NAME_vtip").addClass("uname_fail"); 
		$("#LOGON_NAME_vtip").text("网站帐号仅包含(a-z A-Z 0-9 _)且长度不小于4位，不大于20位。");
		return false;
	}else{
		$("#LOGON_NAME").removeClass("input_failed");
		$("#LOGON_NAME_vtip").removeClass("uname_fail"); 
		$("#LOGON_NAME_vtip").text(""); 
	}
	
	//登录密码
	var strNewpwd = $("#PASSWD").val();
	
	if(strNewpwd=='321321'){
		hForm.elements["PASSWD"].focus();
		$("#PASSWD").addClass("input_failed");
		$("#PASSWD_vtip").addClass("uname_fail");
		$("#PASSWD_vtip").text("您的密码太过简单！不能设为321321！请重新设置您的密码！");
		return false;
	}
	//alert(strNewpwd);
	var valSampPwd = fucJudgeSampleString(strNewpwd,6);
	//alert(valSampPwd.msgTxt);
	if(valSampPwd.boolVal){
		hForm.elements["PASSWD"].focus();
		$("#PASSWD").addClass("input_failed");
		$("#PASSWD_vtip").addClass("uname_fail");
		$("#PASSWD_vtip").text("登录密码为简单密码，请重新定义密码:"+valSampPwd.msgTxt);
		return false;
	}
	else{
		$("#PASSWD").removeClass("input_failed");
		$("#PASSWD_vtip").removeClass("uname_fail"); 
		$("#PASSWD_vtip").text(""); 
	}
	
	
	//登录密码
	/*
	if ($("#PASSWD").val().match(rule) == null || $("#PASSWD").val().length < 6 || $.trim($("#PASSWD").val())==""){
		hForm.elements["PASSWD"].focus();
		$("#PASSWD").addClass("input_failed");
		$("#PASSWD_vtip").addClass("uname_fail"); 
		$("#PASSWD_vtip").text("登录密码仅包含数字和字母，且长度不小于6位。");
		return false;
	}else{
		$("#PASSWD").removeClass("input_failed");
		$("#PASSWD_vtip").removeClass("uname_fail"); 
		$("#PASSWD_vtip").text(""); 
	}
	*/
	
	
	//确认密码
	if ($("#PASSWD").val() != $("#PASSWD2").val()){
		hForm.elements["PASSWD2"].focus();
		$("#PASSWD2").addClass("input_failed");
		$("#PASSWD2_vtip").addClass("uname_fail"); 
		$("#PASSWD2_vtip").text("两次输入密码不一致。");
		return false;
	}else{
		$("#PASSWD2").removeClass("input_failed");
		$("#PASSWD2_vtip").removeClass("uname_fail"); 
		$("#PASSWD2_vtip").text(""); 
	}
	//E-Mail
	if (!Validator.Email.test($("#MAIL").val()) || $.trim($("#LOGON_NAME").val())==""){
		hForm.elements["MAIL"].focus();
		$("#MAIL").addClass("input_failed");
		$("#MAIL_vtip").addClass("uname_fail"); 
		$("#MAIL_vtip").text("E-Mail格式输入有误。");
		return false;
	}else{
		$("#MAIL").removeClass("input_failed");
		$("#MAIL_vtip").removeClass("uname_fail"); 
		$("#MAIL_vtip").text(""); 
	}
	//联系人
	if ($.trim($("#RELA_MAN").val())==""){
		hForm.elements["RELA_MAN"].focus();
		$("#RELA_MAN").addClass("input_failed");
		$("#RELA_MAN_vtip").addClass("uname_fail"); 
		$("#RELA_MAN_vtip").text("联系人不能为空。");
		return false;
	}else{
		$("#RELA_MAN").removeClass("input_failed");
		$("#RELA_MAN_vtip").removeClass("uname_fail"); 
		$("#RELA_MAN_vtip").text(""); 
	}
	//主要电话
	if ($.trim($("#RELA_TEL").val())=="" || !Validator.AllPhone.test($("#RELA_TEL").val())){
		hForm.elements["RELA_TEL"].focus();
		$("#RELA_TEL").addClass("input_failed");
		$("#RELA_TEL_vtip").addClass("uname_fail"); 
		$("#RELA_TEL_vtip").text("主要电话格式输入有误。");
		return false;
	}else{
		$("#RELA_TEL").removeClass("input_failed");
		$("#RELA_TEL_vtip").removeClass("uname_fail"); 
		$("#RELA_TEL_vtip").text(""); 
	}
	//密码取回问题
	if ($.trim($("#PASSWD_QUESTION").val())==""){
		hForm.elements["PASSWD_QUESTION"].focus();
		$("#PASSWD_QUESTION").addClass("input_failed");
		$("#PASSWD_QUESTION_vtip").addClass("uname_fail"); 
		$("#PASSWD_QUESTION_vtip").text("密码取回问题不能为空。");
		return false;
	}else{
		$("#PASSWD_QUESTION").removeClass("input_failed");
		$("#PASSWD_QUESTION_vtip").removeClass("uname_fail"); 
		$("#PASSWD_QUESTION_vtip").text(""); 
	}
	//密码取回答案
	if ($.trim($("#PASSWD_ANSWER").val())==""){
		hForm.elements["PASSWD_ANSWER"].focus();
		$("#PASSWD_ANSWER").addClass("input_failed");
		$("#PASSWD_ANSWER_vtip").addClass("uname_fail"); 
		$("#PASSWD_ANSWER_vtip").text("密码取回答案不能为空。");
		return false;
	}else{
		$("#PASSWD_ANSWER").removeClass("input_failed");
		$("#PASSWD_ANSWER_vtip").removeClass("uname_fail"); 
		$("#PASSWD_ANSWER_vtip").text(""); 
	}
	//验证码
	if ($.trim($("#AREA_CODE").val())==""){
		hForm.elements["AREA_CODE"].focus();
		$("#AREA_CODE").addClass("input_failed");
		$("#AREA_CODE_vtip").addClass("uname_fail"); 
		$("#AREA_CODE_vtip").text("验证码不能为空。");
		return false;
	}else{
		$("#AREA_CODE").removeClass("input_failed");
		$("#AREA_CODE_vtip").removeClass("uname_fail"); 
		$("#AREA_CODE_vtip").text(""); 
	}
	
	//身份证
	if ($("#CARD_TYPE").val()=="01"){
		if(!Validator.IdCard.test($("#CARD_NO").val())){
			$("#CARD_NO").addClass("input_failed");
			$("#CARD_NO_vtip").addClass("uname_fail"); 
			$("#CARD_NO_vtip").text("身份证号有误。");
			return false;
		}else{
			$("#CARD_NO").removeClass("input_failed");
			$("#CARD_NO_vtip").removeClass("uname_fail"); 
			$("#CARD_NO_vtip").text(""); 
		}
	}
	//验证码
	if ($.trim($("#VALID_CODE").val())==""){
		hForm.elements["VALID_CODE"].focus();
		$("#VALID_CODE").addClass("input_failed");
		$("#VALID_CODE_vtip").addClass("uname_fail"); 
		$("#VALID_CODE_vtip").text("验证码不能为空。");
		return false;
	}else{
		$("#VALID_CODE").removeClass("input_failed");
		$("#VALID_CODE_vtip").removeClass("uname_fail"); 
		$("#VALID_CODE_vtip").text(""); 
	}
	return true; 
}
function doOnSubmit(){
	//alert("123");
	if(doValid()){//验证表单
	  var options = {
		  target:'#regArea',
		  type:'POST',
		  url:'/public/pages/doAction.jsp',
		  dataType:'xml', 
		  beforeSubmit:function(){
			  $("#regArea").mask("数据处理中...");
		  },
		  success:function processXml(responseXML) { 
			  var actionFlag = $('actionFlag', responseXML).text(); 
			  var actionMsg = $('actionMsg', responseXML).text();
			  //str=responseXML.getElementsByTagName('actionFlag')[0].firstChild.data; 
			  if(actionFlag==0){
				  pop({
					  title:'注册成功！',
					  text:'尊敬的客户，恭喜您成为中国电信网上营业厅·河南注册用户！<br/>请您使用该帐号进行<a href="/index.jsp"><B><FONT color=#ff0000>登录</font></a><br/><font color="red">您尚未关联任何电信产品，请登录后，关联您自己的电信产品。</font>'
					  },1,function(){location.href='/index.jsp'});
			  }else{
				  pop({title:'注册失败！',text:'尊敬的客户，注册失败！<br/>错误信息：<font color=red>'+actionMsg+'</font>'},3);
			  }
		  }, 
		  complete:function(){
			  $("#regArea").unmask();
		  },
		  error:function(){
			  //alert("系统异常，请稍候重试！");
			  pop({title:'注册失败！',text:'系统正忙，请稍候重试！'},2);
		  }
	  }; 
	  $('#frmReg').ajaxSubmit(options);  
	  return false; 
	}
}

function buildImg(e){
	var key = document.getElementById("check_img1");
	var time = new Date();
	key.setAttribute("src","/public/image.jsp?date="+ time);
	document.getElementById('VALID_CODE').focus();
}

