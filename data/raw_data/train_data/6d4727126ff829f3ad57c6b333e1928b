function doSearch(key){
	if(!key){key = $("#SEARCH_KEYWORD").val();}
	if(!key){
		pop({title:"站内搜索", text:'请输入关键字进行查询...'},3);
		return;
	}
	if(key=='您可以搜索产品/活动/问题等'){
		pop({title:"站内搜索", text:'请输入关键字进行查询...'},3);
		return;
	}
	//location.href="/search/?SEARCH_KEYWORD_HEARDER="+encodeURIComponent(key);
	var hForm = document.getElementById("searchForm");
	if(hForm == null)return false;
	hForm.target = "_blank";
	hForm["SEARCH_KEYWORD_HEARDER"].value=key;
	hForm.action = "/10000zhidao/iask.jsp";
	hForm.submit();
}
