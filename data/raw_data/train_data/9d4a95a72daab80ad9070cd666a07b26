  <script type="text/javascript">
$(function(){
var imgWid = 0 ;
var imgHei = 0 ; //变量初始化
$(".fangdaimg fangda1").hover(
function(){
$(this).find("img").stop(true,true);
var imgWid2 = 0;var imgHei2 = 0;
var big = 2;//放大倍数
var bigHave=$(this).find("img").attr("big");
if(bigHave){big=$(this).find("img").attr("big")};
var src1=$(this).find("img").attr("src");
var src2=$(this).find("img").attr("src2");
imgWid = $(this).find("img").width();
imgHei = $(this).find("img").height();//局部变量
imgWid2 = imgWid * big;
imgHei2 = imgHei * big;
if(src2){imgWid2=$(this).find("img").attr("w2");imgHei2=$(this).find("img").attr("h2");};
$(this).find("img").css({"z-index":200});
if(src2){$(this).find("img").attr("src",src2);$(this).find("img").attr("src2",src1)}
$(this).find("img").animate({"width":imgWid2,"height":imgHei2,"margin-left":-(imgWid2-imgWid)/2,"margin-top":-(imgHei2-imgHei)/2});
},
function(){
var src1=$(this).find("img").attr("src");var src2=$(this).find("img").attr("src2");
if(src2){$(this).find("img").attr("src",src2);$(this).find("img").attr("src2",src1);};
$(this).find("img").stop().animate({"width":imgWid,"height":imgHei,"margin-left":0,"margin-top":0,"z-index":1});});
})
</script> 
