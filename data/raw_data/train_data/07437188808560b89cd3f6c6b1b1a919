function tabHover($tabLi,$tabCont){
    $tabLi.eq(0).addClass('on');
    $tabCont.eq(0).show();
    $tabLi.each(function(i){
        $(this).mouseover(function(){
            $tabLi.removeClass('on');
            $tabLi.eq(i).addClass('on');
            $tabCont.hide();
            $tabCont.eq(i).show();
        });
    });
}
function tabClick($tabLi,$tabCont,itemList,itemName){
    $tabLi.eq(0).addClass('on');
    $tabCont.eq(0).show();
    $tabLi.each(function(i){
        $(this).click(function(){
            $tabLi.removeClass('on');
            $tabLi.eq(i).addClass('on');
            $tabCont.hide();
            $tabCont.eq(i).show();
            if(itemList){
                addSWF(itemList[i],itemName, 1920, 865);
            }
        });
    }).eq(0).click();
}
tabClick($('.tab_role li'),$('.role_cont'));
$('.tab_role li').eq(2).click();
tabClick($('.tab_ren li'),$('.cont_ren'),itemList1,'item1');
tabClick($('.tab_shen li'),$('.cont_shen'),itemList2,'item2');
tabClick($('.tab_tian li'),$('.cont_tian'),itemList3,'item3');
</script>


