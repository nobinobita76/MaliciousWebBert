  <script type="text/javascript">
(function(){
	var n=0;
	var fe=document.getElementById("adLayer");
	if(!fe){return false;}
	var x=0;
	window.onscroll=function(){
		x=(document.body.scrollTop||document.documentElement.scrollTop)+n;
		if(x==0){fe.style.display = "none"}else{fe.style.display="block"};
	};
})();
function qbm_vods(){
	document.documentElement.scrollTop = 0;
	document.body.scrollTop = 0;
};
function qbm_closed(){
	var closeId = document.getElementById(id);
	var closeIdi = closeId[i]
};
function qbm_setTab(name,cursel,n){
	for(i=1;i<=n;i++){
		var menu=document.getElementById(name+i);
		var con=document.getElementById("con_"+name+"_"+i);
		con.style.display=i==cursel?"block":"none";
	}
};
function qbm_closeHide(id,none){
	var closeH = document.getElementById(id);
	if(none==1){
		closeH.style.display="none";
	}else{
		closeH.style.display="block";
	}
};
function qbm_get() {
    var xmlHttp;
    try {
        // Firefox, Opera 8.0+, Safari
        xmlHttp = new XMLHttpRequest();
     } catch (e) {
        // Internet Explorer
        try {
            xmlHttp = new ActiveXObject("Msxml2.XMLHTTP");
        } catch (e) {
            try {
                xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
            } catch (e) {
                return false;
            }
        }
    }
    return xmlHttp;
}
function qbm_success()
{
	var name = document.getElementById('q_name').value;
	var phone = document.getElementById('q_phone').value;
	var yixiang = document.getElementById('q_objective').value;
	if(name == "" || name == "姓名:") {
		alert("请输入你的姓名");
		return false;
	}
	if(phone == "" || phone == "手机号:") {
		alert("请输入你的手机号");
		return false;
	}
	if(yixiang == "" || yixiang == "求职意向:") {
		alert("请输入意向岗位，如：焊工、电工等");
		return false;
	}
	
	    var xmlhttp = qbm_get();

	var url="/ajax/qbm.aspx?name=" + escape(name) + "&phone=" + escape(phone) + "&yixiang=" + escape(yixiang);

        xmlhttp.onreadystatechange = function() {
	        if (4 == xmlhttp.readyState) {
		        if (200 == xmlhttp.status) {
				//报名成功
				var data = xmlhttp.responseText;
				if(data > 0) {
					document.getElementById('q_name').value = "姓名:";
					document.getElementById('q_phone').value = "手机号:";
					document.getElementById('q_objective').value = "求职意向:";
					document.getElementById("bm_fixed_con").style.display = "none";
					document.getElementById("bm_fixed_suc").style.display = "block";
				}
				else if(data == -1)
				{
					alert("请输入你的姓名");
				}
				else if(data == -2)
				{
					alert("请输入你的手机号");
				}
				else if(data == -3)
				{
					alert("请输入正确的手机号");
				}
			}
	        }
        }
        xmlhttp.open("POST",url,true);
	xmlhttp.send("");
}
</script> 
