    <script type="text/javascript">
        function showUserinfo() {
            //获取用户基本信息
            $.ajax({
                type: "get",
                url: "/Home/Personal/GetPersonalBaseInfo",
                async: true,
                cache: false,
                dataType: "json",
                success: function (json) {
                    if (json != null) {
                        var loginDiv = $("#onlogin");
                        if (json.PhotoUrl != null && json.PhotoUrl != "") {
                            loginDiv.find("img").attr("src", json.PhotoUrl);
                        }
                        var showName = json.Name; //"123456123456123456"; 
                        if (showName != null && showName != "") {
                            showName = showName.length > 12 ? (showName.substr(0, 9) + "...") : showName;
                        }
                        loginDiv.find(".skip_username").text(showName);
                        loginDiv.find(".skip_username").attr("title", showName);
                        $("#unlogin").hide();
                        loginDiv.slideDown(100);
                    }
                }
            });
        }
        $(function () {
            isLogin() && showUserinfo();
            $("#page_cab,#page_cab_hide").find("li:gt(0)").click(function () {
                var isLin = isLogin();
                !isLin && OpenLoginOrRegister();
                return isLin;
                //eduLayerPage();
                //return false;
            });
            $(".review a").click(function () {
                var isLin = isLogin();
                !isLin && OpenLoginOrRegister();
                return isLin;
            });
            $(".foldingbtn").click(function () {
                $(".skip_leftWrapper").animate({ left: '-152px' });
                $(".skip_pickWrapper").show();
                $(".skip_rightWarpper").css("margin-left", "37px");
            });
            $(".packupbtn").click(function () {
                $(".skip_leftWrapper").animate({ left: '0' });
                $(".skip_pickWrapper").hide();
                $(".skip_rightWarpper").css("margin-left", "152px");
            });
        });
    </script>
