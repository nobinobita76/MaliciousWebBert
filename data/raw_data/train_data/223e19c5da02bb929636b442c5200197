	    			version: function(idx) {
	    				return getVersion(data.browser.version, idx);
	    			}
	    		};
	    	};

	    	var app = function() {
	    		return {
	    			webview : data.app.webview === 'true'
	    		};
	    	};

	    	return {
	    		platform : platform,
	    		os : os,
	    		browser: browser,
	    		app: app
	    	};
	    })();
    })();
