	    	var getVersion = function(arr, idx) {
	    		try {
		    		if (idx === undefined) {
		    			return arr.join('.');
		    		} else {
						return parseInt(arr[idx], 10);
					}
				} catch (error) {
					return null;
				}
	    	};

	    	var platform = function() {
	    		return {
	    			mobile: data.platform === 'mobile',
	    			tablet: data.platform === 'tablet',
	    			pc: data.platform == 'pc',
	    			unknown: data.platform === 'unknown'
	    		};
	    	};

	    	var os = function() {
	    		return {
	    			android: data.os.name === 'android',
	    			ios: data.os.name === 'ios',
					windows: data.os.name === 'windows',
					version: function(idx) {
	    				return getVersion(data.os.version, idx);
	    			}
	    		};
	    	};

	    	var browser = function() {
	    		return {
	    			msie: data.browser.name === 'msie',
	    			chrome: data.browser.name === 'chrome',
	    			firefox: data.browser.name === 'firefox',
	    			safari: !!data.userAgent.match(/WebKit/i) && !data.userAgent.match(/CriOS/i),
