  <script>
    $("img.lazy_load").click(function(){
        var ua = window.navigator.userAgent;
        var url = $(this).data('src');
        if(/iPhone|iPad/.test(ua)){
            window.location.href=url;
        }
        else{
            window.open(url);
        }
    });
</script> 
