            {[
                var avatarUrl = "http://7u2f5g.com2.z0.glb.qiniucdn.com/img/noname.png?v=20170217";
                var username = '匿名用户';
                if(it.user){
                    avatarUrl = it.user.avatar;
                    username = it.user.username;
                }
                else if(it.currentUser){
                    username = '我';
                }
            ]}
            <span class="username username_ios">{[= username ]}</span>
            <img class="avatar border" src="{[= avatarUrl ]}">
            {[ if(it.reader_num > 0){ ]}
            <span class="num">{[= it.reader_num ]}</span>
            {[ } ]}
        </div>
