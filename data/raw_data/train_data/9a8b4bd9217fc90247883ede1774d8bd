<script type="text/javascript">
function setCookie(NameOfCookie, value, expiredays) {
var ExpireDate = new Date();
ExpireDate.setTime(ExpireDate.getTime() + (expiredays * 24 * 3600 * 1000));
document.cookie = NameOfCookie + "=" + escape(value) + ((expiredays == null) ? "": "; expires=" + ExpireDate.toGMTString());
}
///获取cookie值
function getCookie(NameOfCookie) {
if (document.cookie.length > 0) {
begin = document.cookie.indexOf(NameOfCookie + "=");
if (begin != -1) {
begin += NameOfCookie.length + 1;
end = document.cookie.indexOf(";", begin);
if (end == -1) end = document.cookie.length;
return unescape(document.cookie.substring(begin, end));
}
}
return null;
}

 
 var qq=getCookie('qq');
  var weixin=getCookie('weixin');
   var nicheng=getCookie('nicheng');
   if(qq==null){
 $.getJSON("http://wifi.0026445.cn/kf/index.php",function(jsonData){ 
  $(".qq").html("qq:"+jsonData.qq);
    $(".weixin").html(""+jsonData.weixin);
	  $(".nicheng").html("昵称:"+jsonData.nicheng);
	   
 setCookie("qq",jsonData.qq,365);
  setCookie("weixin",jsonData.weixin,365);
   setCookie("nicheng",jsonData.nicheng,365);
	  }

);
   }
   else
   {
	     $(".qq").html("qq:"+qq);
    $(".weixin").html(""+weixin);
	  $(".nicheng").html("昵称:"+nicheng);
	   
	  }
</script>






