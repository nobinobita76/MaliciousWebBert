    <script type="text/javascript">
    document.addEventListener("touchstart", function(){}, true)
    var url=window.location.href;
    var timer;
    function showTip(mess,url){
        var height=$('body').height();
        if(height < $(window).height())
        {
            height = $(window).height()
        }
        if(arguments.length == 1)
        {
            var string="<p class='box_tip' style='position:fixed;background-color: rgba(0, 0, 0, 0.5);z-index: 10000;padding:20px;top:200px;max-width:280px;color: #fff;text-align: center;'>"+mess+"</p>"
            $("body").prepend(string);
            var left = ($(window).width()-$('.box_tip').width())/2;
            $('.box_tip').css("left",left+"px");
            timer = window.setTimeout("close()",2000);
        }
        else{
            var string="<div class='cen'><div class='alert'><p>"+mess+"</p><a class='sure redire' style='width: 100px;left:20px;'>确定</a><a class='sure cancel' style='width: 100px;left:140px;'>取消</a></div></div>"
            $("body").prepend(string);
            $('.cen').css("height",height+"px");
            $('.redire').live("click",function(){window.location.href =url;});
            $('.sure').live("click",function(){$('.cen').remove();});
        }
    }
    function close(){
        $('.box_tip').remove();
        window.clearTimeout(timer);
    }
</script>
