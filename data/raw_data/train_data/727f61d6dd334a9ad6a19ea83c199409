            AM(function(){
                //  头部菜单展示
                $("#menu-link").click(function(event){
                    $(this).addClass("zk")
                    $(".menu-list").show();
                    var e=window.event || event;
                    if(e.stopPropagation){
                        e.stopPropagation();
                    }else{
                        e.cancelBubble = true;
                    }
                })

                $(document).click(function(){
                    $("#menu-link").removeClass("zk")
                    $(".menu-list").hide();
                })
                $(window).scroll(function(){
                    $("#menu-link").removeClass("zk")
                    $(".menu-list").hide();
                })
            })
        </script>
