        $(".J_rate").click(function() {
            if($(this).hasClass("rated")){
                showTip("您已经点过赞了");
            }else{
                if(rate_post){
                    rate_post = false;
                    var _this = $(this) ;
                    var isPicRate = _this.hasClass("picRate");
                    var prestigeCount=_this.attr("prestigeCount");
                    var islogin=_this.attr("islogin");
                    var tid=_this.attr("tid");
                    var bid=_this.attr("bid");
                    var url=_this.attr("ToUrl");
                    var siteName=_this.attr("siteName");

                    if (islogin=="yes"){
                        if(prestigeCount==0){
                            showTip("当前不能评分"); return false;
                        }else if(prestigeCount>0){
                            var reason = "来自 19楼手机版 wap.19lou.com";
                            var score = 1;
                            var pid = _this.attr("pid");
                            var num = (_this.text()=="赞")?1:parseInt(_this.text())+1;
                            var incentiveScores = [];
                            incentiveScores.push( {incentiveType: {id: 1}, value:score} );
                            var incentiveScoresStr = JSON.stringify(incentiveScores);
                            $.ajax({
                                type: 'POST',
                                url: "/board/rate2/submit",
                                dataType: 'json',
                                data: {tid:tid,incentiveScores: incentiveScoresStr,pid:pid,reason:reason,bid:bid,board_flag:1},
                                success: function(res){
                                    if(res.success){
                                        if(isPicRate){
                                            _this.removeClass("picRate");
                                            _this.addClass("picRated");
                                            _this.addClass("rated");
                                            _this.find(".rateNum").text("" + num);
                                        }else {
                                            if(_this.hasClass("conzan")){
                                                _this.addClass("conzaned");
                                                _this.addClass("rated");
                                                $(".zanMun").text("" + num)
                                            }else{
                                                _this.addClass("zaned");
                                                _this.addClass("rated");
                                                _this.text("" + num);
                                            }


                                        }
                                    }else{
                                        var code = res.code;
                                        var msg = "";
                                        switch(code){
