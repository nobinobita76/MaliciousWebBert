<script type="text/javascript">
//分页确认按钮

    function pageOKButton(){
        var pageNumber = $(".page_txt").attr("value");
        var r = /^\d+$/　　//正整数
        //pageNumber = parseInt($.trim(pageNumber));
        if(r.test(pageNumber)){
            if(pageNumber < 1){
                pageNumber = 1;
            }else if(pageNumber> 1){
                pageNumber = 1;
            }
            var url = "/wap/board-36251456480067455-thread-76351482930542368-{page}.html";
            url = url.replace("\{page\}", pageNumber);
            window.location = url;

        }else{
            showTip("请输入正确的数字！");
        }
}
</script>
