        //首张图片添加app图集提示
        function createAppPicTip(){
            //添加图片底部app提示
            var ttname = wap.platform.cityName == "chongqing"?"wapcq_detailtu_app":"wap_detailtu_app";
            var bdname = wap.platform.cityName == "chongqing"?"24078":"23555";
            var goAppDiv = '<a href="javascript:void(0);" class="openAPP openThread " ttname="' + ttname + '" bdname="' + bdname+'">';
            goAppDiv += '<span class="upLoadApp-icon"></span>';
            goAppDiv += '打开APP,查看更多消息进展</a>';

            var imgStickDiv = '<a href="javascript:;" class="openAPP openThread tiezhi" ttname="sticker_thread_wap" bdname="'+bdname+'">';
            imgStickDiv += '<span class="phoneTiezhi"></span>';
            imgStickDiv += '用APP发帖，贴纸随心换</a>';

            var h_stick = false;
            var h_app = false;
            var h_pic = false;
            var allimg = $(".view .center").find("img");
            allimg.each(function(){
                var _this = $(this);
                if(_this.is(":visible")) {
                    if(_this.parents(".img_stick").length >0 && !h_stick){
                        if($(".view .bigImg").find("img").length >1 && !h_pic){
                            imgStickDiv += '<span class="tj">图集</span>';
                            h_pic = true;
                        }
                        setTimeout(function(){
                            _this.after(imgStickDiv)
                            _this.parent().addClass("photoCollect");
                        },450)
                        h_stick = true;
                    }else if(!h_app && !h_stick){
                        if($(".view .bigImg").find("img").length >1 && !h_pic){
                            goAppDiv += '<span class="tj">图集</span>';
                            h_pic = true;
                        }
                        setTimeout(function(){
                            _this.after(goAppDiv)
                            _this.parent().addClass("photoCollect");
                        },450)
                        h_app = true;
                    }
                    if(h_stick) return false;
                }
            });
        }

        function deleteC(tid){
            var ids = [];
            ids.push(tid);
            $.ajax({
                type:'POST',
                dataType:'json',
                url:'/user/myinfo/favthread/delete',
                data:{checkedThreads:ids},
                success:function(res){
                    if(res.success){
                        showTip("已取消收藏");
                        if($(".J_bookMark").hasClass("concelected")){
                            $(".J_bookMark").removeClass("concelected");
                        }
                        if($(".J_bookMark").hasClass("collect-btned")){
                            $(".J_bookMark").removeClass("collect-btned");
                        }
                        if($(".J_bookMark").hasClass("collected")){
                            $(".J_bookMark").removeClass("collected");
                        }
                        $(".J_bookMark").attr("ttname","wap_qzdetail_collect");
                        $(".picView").attr("ttname","wap_pic_collect");
                        $(".J_bookMark").each(function() {
                            if($(this).parent().hasClass("manage-btn")) $(this).html("<span></span>收藏")
                        })
                    }
                },
                error:function(){
                    showTip("取消收藏失败");
                }
            })
        }

        $(".J_bookMark").live("click",function(e){
            var _this=$(this);
            var tag = _this.attr("tag");
            var pid =_this.attr("pid");
            var data = "";
            var action = {
                mes:{
                    "success":"收藏成功",
                    "error":"提交异常"
                },
                success:function(obj){
                    if($(".J_bookMark").hasClass("concelect")){
                        $(".J_bookMark").addClass("concelected");
                    }
                    if($(".J_bookMark").hasClass("collect-btn")){
                        $(".J_bookMark").addClass("collect-btned");
                    }
                    if($(".J_bookMark").hasClass("collect")){
                        $(".J_bookMark").addClass("collected");
                    }
                    $(".J_bookMark").attr("ttname","wap_qzdetail_collect_cancel");
                    $(".picView").attr("ttname","wap_pic_cancel");
                    $(".J_bookMark").each(function() {
                        if($(this).parent().hasClass("manage-btn")) $(this).html("<span></span>已收藏")
                    })
                },
                jump:function(obj){
                    obj.attr("href","/wap/u/myinfo/favthread");
                }
            }
            if (pid!="" & pid!=null) {
                data = "fid=36251456480067455&tid=76351482930542368&pid="+pid;
            }
            else{
                data = "fid=36251456480067455&tid=76351482930542368"
            }
            $.ajax({
                hold: false,
                type:'POST',
                dataType:'json',
                url:'/u/myinfo/favthread/myfavorites',
                data:data,
                success:function(res){
                    if(res.success){
                        showTip("收藏成功");
                        action.success(_this);
                    }else if(false){
                        window.location.href='/util/active';
                    }else {
                        switch (res.code) {
                            case 101:
                                var tid="76351482930542368";
                                deleteC(tid);
                                break;
                            case 102:
                                showTip("自己不能收藏自己的帖子哦！");
                                break;
                            default:
                                showTip("收藏出现异常,请稍候重试...");
                        }
                    }
                },
                error:function(){
                    showTip("提交异常");
                }
            })
        })
