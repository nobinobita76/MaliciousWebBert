        //回复定位
        $(".act-replay").click(function(){
            var topReplay=$("#content_replay").offset().top;
            $('html,body').animate({scrollTop: topReplay+"px"},500);
            $("#content_replay").focus();
        });

        //圈子推荐
        var LOCAL_KEY = "cacheData_cc_none"
        var LOCAL_STOR_SUPPORT = isLocalStorageSupported()
        var recPerpage = 15,
                durHour = 6,
                recMaxPage
        var site = "{huzhou:'huzhou.19lou.com',anji:'anji.19lou.com',hefei:'hefei.19lou.com',guangzhou:'guangzhou.19lou.com',sansha:'sansha.19lou.com',chunan:'chunan.19lou.com',suzhou:'suzhou.19lou.com',yangzhou:'yangzhou.19lou.com',yuhang:'yuhang.19lou.com',rizhao:'rizhao.19lou.com',putian:'putian.19lou.com',shijiazhuang:'shijiazhuang.19lou.com',shaoxing:'shaoxing.19lou.com',fuyang:'fuyang.19lou.com',qz:'www.0595bbs.cn',ningbo:'ningbo.19lou.com',zhoushan:'zhoushan.19lou.com',shh:'www.shanghaining.com',wuhan:'wuhan.19lou.com',taiyuan:'taiyuan.19lou.com',tonglu:'tonglu.19lou.com',support:'support.19lou.com',zz:'zz.19lou.com',quzhou:'quzhou.19lou.com',nt:'nt.19lou.com',jiaju:'home.19lou.com',beijing:'beijing.19lou.com',demo:'demo.19lou.com',luban:'luban.19lou.com',wenzhou:'wenzhou.19lou.com',fz:'www.ihome99.com',hangzhou:'www.19lou.com',yiwu:'yiwu.19lou.com',jinhua:'jinhua.19lou.com',linan:'linan.19lou.com',nanjing:'nanjing.19lou.com',cc:'cc.19lou.com',taizhou:'taizhou.19lou.com',lishui:'lishui.19lou.com',chongqing:'go.cqmmgo.com',chuzhou:'chuzhou.19lou.com',tiaozao:'tiaozao.19lou.com',xiaoshan:'xiaoshan.19lou.com',fanwai:'fanwai.19lou.cn',tj:'www.qvbuy.com',jiande:'jiande.19lou.com',jiaxing:'jiaxing.19lou.com',novel:'www.19yuedu.com',}";
