        function getBroadcastImgData() {
            $.ajax({
                type: "get",
                url: "/wap/thread/getBroadcastImgData",
                data: {fid: bid},
                success: function (res) {
                    var data = res.data;
                    if (data != null && data != "") {
                        if (data.broadcast == "1") {
                            var broadcastImgData = data.broadcastImgData;
                            for (var i = 0; i < broadcastImgData.length && i < 15; i++) {
                                var param = broadcastImgData[i];
                                param.url = param.url.replace("http:", "");
                                if ([1, 3, 6, 8, 11, 13].indexOf(i) > -1) {
                                    param.openApp = true
                                } else {
                                    param.openApp = false
                                }

                                param.bdname = "23557"
                                param.url = broadcastImgData[i].url.replace("forum", "wap/forum");
                                if (broadcastImgData[i].imgSrcs != null && broadcastImgData[i].imgSrcs != "" && (broadcastImgData[i].imgSrcs.split(",").length > 3)) {
                                    broadcastImgData[i].imgSrcs=broadcastImgData[i].imgSrcs.replace(/https?:/g,"").replace(/160x160/g,"220x160");
                                    param.imgSrc = broadcastImgData[i].imgSrcs.split(",").slice(0, 3);
                                    $("#lookList").append(Mustache.render(temp.join(""), param));
                                } else {
                                    broadcastImgData[i].imgSrcs=broadcastImgData[i].imgSrcs.replace(/https?:/g,"").replace(/160x160/g,"220x160");
                                    param.imgSrc=broadcastImgData[i].imgSrcs.split(",").slice(0, 1);
                                    $("#lookList").append(Mustache.render(temp2.join(""), param));
                                }
                            }
                        } else {
                            $(".look-wrap").remove();
                        }

                    }
                    // 大家都知道加载更多
                    var sumHeight = 0, sumHeight2 = 0, sumHeight3 = 0;
                    $("#lookList").find("li").each(function (index) {
                        if (index < 5) {
                            sumHeight += $(this).outerHeight();
                        } else if (index < 10) {
                            sumHeight2 += $(this).outerHeight();
                        } else {
                            sumHeight3 += $(this).outerHeight();
                        }
                    })
                    $("#lookList").height(sumHeight);
                    var bH = $(".bottom_nav").outerHeight() + $(".footer").outerHeight() - 50;
                    $(window).scroll(function () {
                        if ($(document).height() - $(this).scrollTop() - $(this).height() < bH) {
                            var hasClass = $("#lookList").hasClass("hei3");
                            if (!hasClass) {
                                $("#lookList").height(sumHeight + sumHeight2);
                                $("#lookList").addClass("hei3");
                            } else {
                                $("#lookList").height(sumHeight + sumHeight2 + sumHeight3);
                                $(".listTips").remove();
                                $("#lookListAPP").show();
                            }
                        }
                    });
                }
            })
            $("#jz-more").live("click", function () {
                var lookLiH = $(".look-wrap li").outerHeight();//72
                var looWrapHeight = $(".look-wrap").height();//360
                var maxHeight = $(".look-wrap .list").outerHeight();
                if (looWrapHeight < maxHeight) {
                    if (maxHeight - looWrapHeight <= 360) {
                        $(".look-wrap").height(maxHeight)
                        $(this).hide();
                        $(".download_app").css("display", "block");
                    }
                    else {
                        $(".look-wrap").height(looWrapHeight + lookLiH * 5)
                    }
                }
            })
            $(".openApp").live("click", function () {
                window.location.href = "http://m.19lou.com/d/?showType=3&threadUrl=" + $(this).attr("data-url") + "&bdname=" + $(this).attr("bdname");
            })
        }
        //大家都在看
        getBroadcastImgData()
    })

