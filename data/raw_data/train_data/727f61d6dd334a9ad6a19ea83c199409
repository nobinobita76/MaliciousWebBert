        $(window).scroll(function(){
            // $(".act-replay").hide();
            // setTimeout(function(){
            var topReplay=$(".publish-text").offset().top;
            var winHeight=$(window).height();
            var winTop=$(window).scrollTop();
            if(topReplay-winTop>0 && topReplay-winTop<winHeight){
                $(".act-replay").hide();
            }else{
                $(".act-replay").show();
            }//},500);
        });
