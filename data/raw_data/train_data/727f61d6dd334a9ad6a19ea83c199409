<script type="text/javascript">
    AM.ready('ajax','showTips','captcha','//js2.citysbs.com/0.8.3.84/forum/App.showTip.wap.js?v=0.7.6.080706-2017011301','ajax','showTips','captcha','lazyLoad','//js2.citysbs.com/0.8.3.84/forum/pm.js',function(){

        /*图集*/
        $(".view-data .img img").click(function(){
            $("#atkas").css("left","0");
        })
        //图片懒加载;
        $(".J_lazyImgs").lazyload({effect: "fadeIn"});

        //  头部菜单展示
        $("#menu-link").click(function(){
            $(this).addClass("zk")
            $(".menu-list").show();
        })
        $("#menu-link").click(function(event){
            var e=window.event || event;
            if(e.stopPropagation){
                e.stopPropagation();
            }else{
                e.cancelBubble = true;
            }
        });
        $(document).click(function(){
            $("#menu-link").removeClass("zk")
            $(".menu-list").hide();
        })
        $(window).scroll(function(){
            $("#menu-link").removeClass("zk")
            $(".menu-list").hide();
        })
        //回复按钮展示与隐藏
        $(window).scroll(function(){
            var topReplay=$(".publish-text").offset().top;
            var winHeight=$(window).height();
            var winTop=$(window).scrollTop();
            if(topReplay-winTop>0 && topReplay-winTop<winHeight){
                $(".act-replay").hide();
            }else{
                $(".act-replay").css("display","block");
            }
        });
        $(window).scroll();
        //上传图片删除图片功能
        $(".avater-del").click(function(){
            $(this).parents("li").remove();
        })

        //投票菜单展示
        var off=false;
        $('.voteUserList').click(function(){
            if(off==false){
                $(this).parents(".poll-result").siblings(".namelist").fadeIn();
                $(this).text("收起");
                off=true;
            }else{
                $(this).parents(".poll-result").siblings(".namelist").fadeOut();
                $(this).text("查看");
                off=false;
            }
        });
        //底部广告删除功能
        $(".adv-close").click(function(){
            $(".adv-fixed").remove();
