        //hotRec constructor
        function HotRecommend(threadList,perpage,totalcount,maxpage) {
            this.timeStamp = Date.parse(new Date());
            this.returnList = threadList;
            this.perPage = perpage;
            this.totalCount = totalcount;
            this.maxPage = maxpage;
        }

        function getRecommendGroups(){
            if(LOCAL_STOR_SUPPORT) {
                var localData = JSON.parse(window.localStorage.getItem(LOCAL_KEY))
                var returnList = []
                if(!!localData) {
                    returnList = localData.returnList ? localData.returnList : []
                }
                if(returnList.length > 0) {
                    var timeDif = Date.parse(new Date()) - localData.timeStamp
                    if(timeDif > durHour*60*60*1000) {
                        reLoadRecData()
                    } else {
                        returnList.forEach(function(item,index) {
                            if(item.bidStr == "36251456480067455") returnList.splice(index,1)
                        })
                        if(returnList.length > 2) returnList = ranDomData(returnList)
                        renderHot(returnList)
                    }
                } else {
                    ajaxRecData()
                }
            } else {
                ajaxRecData()
            }
        }

        function ajaxRecData() {
            $.ajax({
                type: 'get',
                url: '/wap/board/circle/getRecommendGroups',
                dataType: 'json',
                success: function(res){
                    if(res.success){
                        var returnList = res.data.returnList ? res.data.returnList : []
                        if(returnList.length > 0) {
                            recMaxPage = Math.ceil(res.data.returnList.length / recPerpage);
                            var hotRecommend = new HotRecommend(returnList, recPerpage, res.data.returnList.length, recMaxPage)

                            if(LOCAL_STOR_SUPPORT) {
                                window.localStorage.setItem(LOCAL_KEY, JSON.stringify(hotRecommend));
                            }
                            returnList.forEach(function (item, index) {
                                if(item.bidStr == "36251456480067455") {
                                    returnList.splice(index, 1)
                                }
                            })
                            if(returnList.length > 2) {
                                returnList = ranDomData(returnList)
                            }

                            renderHot(returnList)
                        }
                    }
                    return true;
                },
                error: function() {}
            });
        }

        function renderHot(data) {
            $(".relate-tite").show();
            var list = $(".relate-quan");
            var html="";
            html+= "<div class='qz-jion'>";
            for(var i=0;i<data.length;i++){
                html+="<dl>";
                html+="<dt class='fl' ttname='wap_board_"+data[i].name+"'>";
                var src = data[i].recomType == 1 ? "/wap/forum-" + data[i].fid + "-1.html" : "/wap/board-" + data[i].bidStr + "-1.html";
                if (site != "" && site[data[i].cityName] != undefined)src = "http://" + site[data[i].cityName] + src;
                var totalTidNum=(data[i].totalTidNum<=9999)?data[i].totalTidNum:(data[i].totalTidNum / 10000).toFixed(1) + "万";
                var subscribeNum=(data[i].subscribeNum<=9999)?data[i].subscribeNum:(data[i].subscribeNum / 10000).toFixed(1) + "万"
                html+="<a href='" + src.replace(/^https?:/i,'') + "'>";
                html+="<img src='"+data[i].cover.replace(/^https?:/i,'')+"' alt=''></a></dt>";
                html+="   <dd>";
                html+="<a href='" + src.replace(/^https?:/i,'') + "' class='qz'>";
                html+="       <span>"+data[i].name+"</span>";
                html+="       <p>帖子："+totalTidNum+"&nbsp;&nbsp;&nbsp;";
                html+="       成员：<em class='members' data-num='"+ data[i].subscribeNum + "'>"+subscribeNum+"</em>&nbsp;</p>";
                html+="  </a>";
                if(data[i].subscribe == true) {
                    html+=" <a href='javascript:void(0);' rel='nofollow' class='relate-joinbtn join-btned'>已加入</a>";
                } else {
                    if (data[i].recomType==1) {
                        html+="  <a rel='nofollow' href='https://cc.19lou.com/wap/login' ttname='wap_qzdetail_join_" + data[i].fid + "' class='relate-joinbtn ' data-fid='"+data[i].fid +"'>+加入</a>";
                    } else {
                        html+="  <a rel='nofollow' href='https://cc.19lou.com/wap/login' ttname='wap_qzdetail_join_" + data[i].bidStr + "' class='relate-joinbtn ' data-bid='" +data[i].bidStr +"'>+加入</a>";
                    }
                }
                html+="</dd>";
                html+=" </dl>";
            }
            html+=" </div>";
            list.append(html);
        }

        function reLoadRecData() {
            window.localStorage.removeItem(LOCAL_KEY)
            ajaxRecData()
        }

        //随机取两条数据
        function ranDomData(arr) {
            if(arr.length <= 0) return;
            var resultArr = [],
                    index = parseInt(Math.random()*arr.length,10)

            resultArr.push(arr[index]);
            arr.splice(index,1);
            index = parseInt(Math.random()*arr.length,10);
            resultArr.push(arr[index])

            return resultArr
        }

        //查看是否支持localStorage
        function isLocalStorageSupported() {
            var enabled = true;
            if(!!window.localStorage && !!window.localStorage.getItem && !!window.localStorage.setItem) {
                try {
                    var testKey = '__testStore__'
                    window.localStorage.setItem(testKey,testKey)
                    if(window.localStorage.getItem(testKey) != testKey) {enabled = false}
                    window.localStorage.removeItem(testKey)
                } catch(err) {
                    console.log(err)
                    enabled = false
                }
            } else {
                enabled = false;
            }
            return enabled
        }


        $(".join-btn-forum").live('click',function(){
            if(0==0){
                window.location.href = "https://" + window.location.host + "/wap/login";
            }else{
                var $add = $(this),
                        fid = $add.attr("data-fid");
                $.when(
                        $.ajax({
                            type: "post",
                            url: "/u/myinfo/favforum/batchfavorite",
                            data:{fids:[fid]}
                        })
                ).done(function (res) {
                            if (res.success) {
                                $add.html('已加入').addClass("join-btned").removeClass("join-btn-forum").removeAttr("ttname")
                                var number=$add.parent("dd").find(".members").attr("data-num");
                                var subscribeNum=((parseInt(number)+1)<=9999)?(parseInt(number)+1):((parseInt(number)+1) / 10000).toFixed(1) + "万"
                                var mes=subscribeNum;
                                $add.parent("dd").find(".members").html(mes);

                                if(LOCAL_STOR_SUPPORT) {
                                    var localData = JSON.parse(window.localStorage.getItem(LOCAL_KEY))
                                    var returnList = []
                                    if(localData) {
                                        returnList = localData.returnList ? localData.returnList : []
                                    }
                                    if(returnList.length > 0) {
                                        var updateData = localData
                                        updateData.returnList.forEach(function(item) {
                                            if(fid == item.fid.toString()) {
                                                item.subscribe = true
                                                item.subscribeNum += 1
                                            }
                                        })
                                        window.localStorage.removeItem(LOCAL_KEY)
                                        window.localStorage.setItem(LOCAL_KEY,JSON.stringify(updateData))
                                    }
                                }
                                showTip("加入成功！");
                            } else {
                                switch (res.code) {
                                    case 301:
                                        showTip("你已经加入该板块！");
                                        break;
                                    case 999:
                                        window.location.href = "https://" + window.location.host + "/wap/login";
                                        break;
                                    default:
                                        $add.show();
                                        showTip("加入失败，请稍后再试！");
                                        break;
                                }
                            }
                        }).fail(function () {
                            showTip("服务器正忙，请稍后再试");
                        });
            }
        })

        $(".join-btn-circle").live('click',function(){
            if(0==0){
                window.location.href = "https://" + window.location.host + "/wap/login";
            } else {
                var $add = $(this);
                bid = $add.attr("data-bid");
                $.when(
                        $.ajax({
                            type: "post",
                            url: "/u/board/subscribe/add",
                            data:{bids:[bid]}
                        })
                ).done(function(res) {
                            if(res.success) {
                                $add.html("已加入").addClass("join-btned").removeClass("join-btn-circle").removeAttr("ttname")
                                var number=$add.parent("dd").find(".members").attr("data-num");
                                var subscribeNum=((parseInt(number)+1)<=9999)?(parseInt(number)+1):((parseInt(number)+1) / 10000).toFixed(1) + "万"
                                var mes = subscribeNum;
                                $add.parent("dd").find(".members").html(mes);
                                showTip("加入成功！");
                                if(LOCAL_STOR_SUPPORT) {
                                    var localData = JSON.parse(window.localStorage.getItem(LOCAL_KEY))
                                    var returnList = []
                                    if(localData) {
                                        returnList = localData.returnList ? localData.returnList : []
                                    }
                                    if(returnList.length > 0) {
                                        var updateData = localData
                                        updateData.returnList.forEach(function(item) {
                                            if(bid == item.bidStr) {
                                                item.subscribe = true
                                                item.subscribeNum += 1
                                            }
                                        })
                                        window.localStorage.removeItem(LOCAL_KEY)
                                        window.localStorage.setItem(LOCAL_KEY,JSON.stringify(updateData))
                                    }
                                }
                            } else {
                                switch (res.code) {
                                    case 999:
                                        window.location.href = "https://" + window.location.host + "/wap/login";
                                        break;
                                    case 101:
                                        $add.show();
                                        showTip("该兴趣圈已经被关闭！");
                                        break;
                                    case 102:
                                        $add.show();
                                        showTip("该兴趣圈已经被关闭！");
                                        break;
                                    case 103:
                                        $add.show();
                                        showTip("你已经加入该兴趣圈！");
                                        break;
                                    case 104:
                                        $add.show();
                                        showTip("你没有权限加入该兴趣圈！");
                                        break;
                                    default:
                                        $add.show();
                                        showTip("加入失败，请稍后再试！");
                                        break;
                                }
                            }
                        }).fail(function() {
                            showTip("服务器正忙，请稍后再试");
                        })

            }
        })

