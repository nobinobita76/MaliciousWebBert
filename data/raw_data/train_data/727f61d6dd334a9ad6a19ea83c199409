    AM('ajax','showTips','captcha','//js2.citysbs.com/0.8.3.84/forum/App.showTip.wap.js?v=0.7.6.080706-2017011301','ajax','showTips','captcha',"//js2.citysbs.com/0.8.3.84/forum/webupload/wapUploadConf.js","//js2.citysbs.com/0.8.3.84/forum/webupload/webuploader.min.js",function() {
        //回复定位

    webUpload({fileNum:3,thumbNum:3,width:60, height:60,url: "/upload/?uid=0&channel=cc&appname=forum&time=1491049284032&t=44b7c61ab9460032b211382e077067e6&info=fid-36251456480067455",
        queueCb:function(file,fileCount){
            $(".uploader-list").show();
            var imglen=$(".file-item").length;
            $(".Z_img").show();
            $(".Z_img").html(imglen);
        },
        delectCb:function($this){
            var imglen=$(".file-item").length;
            $(".Z_img").html(imglen);
            if(imglen==0){
                $(".Z_img").hide();
                $(".uploader-list").hide();
            }
        },
        successCb: function(file, response){
            var data = response.data;
            var origName = $.trim(file.name);
            var htmlData = "<input type='text' name='fileType' value='" + data.fileType + "'>";
            htmlData += "<input type='text' name='aid' value='" + data.aid + "'>";
            htmlData += "<input type='text' name='fileSize' value='" + data.fileSize + "'>";
            htmlData += "<input type='text' name='fileDomain' value='" + data.domain + "'>";
            htmlData += "<input type='text' name='filePath' value='" + data.path + "'>";
            htmlData += "<input type='text' name='fileName' value='" + data.name + "'>";
            htmlData += "<input type='text' name='source' value='" + data.source + "'>";
            htmlData += "<input type='text' name='fileOrigName' value='" + origName + "'>";
            htmlData += "<input type='text' name='width' value='" + data.width + "'>";
            htmlData += "<input type='text' name='height' value='" + data.height + "'>";
            $("#"+file.id).find(".pic-data").html(htmlData);
        },
        errorCb:function(file){
            var imglen=$(".file-item").length;
            $(".Z_img").html(imglen);
            if(imglen==0){
                $(".Z_img").hide();
                $(".uploader-list").hide();
            }
            $("#"+file.id).find(".loadTip").html("上传失败").show();
        },
    });

        window.onload=function(){
            $(".index-act").show();
            $(window).scroll(function(){
                var topReplay=$(".publish-text").offset().top;
                var winHeight=$(window).height();
                var winTop=$(window).scrollTop();
                if(topReplay-winTop>0 && topReplay-winTop<winHeight){
                    $(".act-replay").hide();
                }else{
                    $(".act-replay").css("display","block");
                }
            });
            $(window).scroll();
        }

        $("#content_replay").click(function(){
            $(this).attr("placeholder","输入回复内容（2个汉字以上）")
        })


        function publishAttachments(elem) {
             var $imagesPop = elem,
                 attachments = [];
             $imagesPop.each(function () {
                 var _this = $(this);
                 var fileType = _this.find("[name=fileType]").val();
                 var aid = _this.find("[name=aid]").val();
                 var fileSize = _this.find("[name=fileSize]").val();
                 var domain = _this.find("[name=fileDomain]").val();
                 var path = _this.find("[name=filePath]").val();
                 var name = _this.find("[name=fileName]").val();
                 var source = _this.find("[name=source]").val();
                 var origName = _this.find("[name=fileOrigName]").val();
                 var width = _this.find("[name=width]").val();
                 var height = _this.find("[name=height]").val();
                 var attach = {fileType: fileType, aid: aid, fileSize: fileSize, domain: domain, path: path, name: name, source: source, origName: origName, width: width, height: height};
                 attachments.push(attach);
             });
             return JSON.stringify(attachments);
        };

            var checkCapture = false;

        $("#publishButton").click(function(){
            var _this = $(this)
            var c = _this.attr("class")
            if(c.indexOf("disable") == -1){
                _this.addClass("disable")
                var content = $.trim($("#content_replay").val());
                var subject = $.trim($("#subject").val());
                var anonymous = false;
                if($("#anonymous:checked").length>0){
                    anonymous=true;
                }
                if(content.replace(/[^\x00-\xff]/g,"**").length<4){
                    $(".replay_note").html("回复不能少于2个汉字");
                     _this.removeClass("disable")
                    return false;
                }
                if( content.replace(/[^\x00-\xff]/g,"**").length>100000){
                    $(".replay_note").html("回复不能多于50000个汉字");
                     _this.removeClass("disable")
                    return false;
                }
                var actionUrl="/board/post/reply"
                content= content.replace(/<script.*?>.*?<\/script>/ig, '');//禁止?本

                if(checkCapture) {
                    _this.removeClass("disable");
                    window.verification.loadGeetest(function (validate) {
                        submitContent(validate);
                    });
                }else {
                    submitContent();
                }

                function submitContent(validate){
                    validate = validate?validate:{};
                    var postData = $.extend(validate, {
                        fid: '36251456480067455',
                        tid:'76351482930542368',
                        bid:'36251456480067455',
                        content:$.trim(content),
                        boardthreadreply: 1 ,
                        source :0,
                        anonymous:anonymous ,
                        repquote:0,
                        attachments:publishAttachments($(".pic-data")),
                        attentionUserNames:$.trim($("#attentionUserNames").val())
                    });

                    $.ajax({
                        hold : true,
                        type:'POST',
                        dataType:'json',
                        url:actionUrl,
                        data:postData,
                        success:function(res){
                            if(res.success){
                                if(3%10==0){
                                    window.location.href = '/wap/board-36251456480067455-thread-76351482930542368-2.html?#replay';
                                }
                                else if(1==1){
                                    window.location.reload();
                                }
                                else{
                                    window.location.href = '/wap/board-36251456480067455-thread-76351482930542368-1.html?#replay';
                                }
                            }else{
                                var message = res.data;
                                if (message == ""||message ==null){
                                    message = "回复未成功，请再试试！";
                                }
                                if(res.code == 119){
                                    message = "验证码校验失败，请重试！";
                                }else if(res.code == 203)
                                    message = "帖子含有敏感信息或外网链接，请重新编辑"
                                $(".replay_note").html(message);

                            }
                            _this.removeClass("disable")
                        },
                        error:function(){
                            $(".replay_note").html("提交异常，请重试");
                            _this.removeClass("disable")
                        }
                    })
                }
            }
        })
    })
