<script type="text/javascript">
    AM.ready('ajax','showTips','captcha','//js2.citysbs.com/0.8.3.84/forum/App.showTip.wap.js?v=0.7.6.080706-2017011301','ajax','showTips','captcha',"//js2.citysbs.com/0.8.3.84/platform/wap/swiper.min.js","//js2.citysbs.com/0.8.3.84/platform/wap/picView.js",function() {
        var LOCAL_KEY = "cacheData_cc_none"
        var LOCAL_STOR_SUPPORT = isLocalStorageSupported()

        var rate_post = true;
        var isApp = wap.isApp;
        var bid = '36251456480067455',tid = '76351482930542368';
        wap.pic.shareTitle="大润发嘉兴店服务台招聘服务人员一名";
        if(wap.pic.isPicView == "true"){
            getMorePicView(bid,tid);
        }else{
            $(".view-data .bigImg img").live("click",function(){
                if($.trim($(".lastView .pic-list ul").html()) == "") {
                    getMorePicView(bid,tid);
                }
            })
        }

        if(isApp == "false"){
            picView.init();      
        }

        function getMorePicView(bid,tid){
            $.ajax({
                type:"get",
                url:"/wap/board/getMoreBoardPicView",
                data:{bid:bid,tid:tid},
                success: function(res2) {
                    var data=res2.data;
                    var html="";
                    if(data!=null&&data!=""){
                        if(data.oneLine!=undefined){
                            var lineOne=data.oneLine;
                            for(var i=0;i<lineOne.length ;i++) {
                                html += "<li ttname='wap_pic_tj'>";
                                html += "<a href='"+lineOne[i].url.replace(/^https?:/i,'')+"?picView=true' ttname='wap_pic_tj'>";
                                html += "       <img src='"+lineOne[i].pic.replace("90x60","400x400").replace(/^https?:/i,'')+"' alt='$"+lineOne[i].boardName+"' title='$"+lineOne[i].boardName+"'>";
                                html += "        <p class='p_t' title='$"+lineOne[i].boardName+"'>"+lineOne[i].boardName+"</p>";
                                html += "<span class='p_n'>"+lineOne[i].picCount+"图</span>";
                                html += "</a>";
                                html += "</li>";
                            }
                        }
                        if(data.twoLine!=undefined){
                            var lineTwo=data.twoLine;
                            for(var i=0;i<lineTwo.length;i++) {
                                html += "<li ttname='wap_pic_tj'>";
                                html += "<a href='"+lineTwo[i].url.replace(/^https?:/i,'')+"?picView=true' ttname='wap_pic_tj'>";
                                html += "       <img src='"+lineTwo[i].pic.replace("90x60","400x400").replace(/^https?:/i,'')+"' alt='$"+lineTwo[i].boardName+"' title='$"+lineTwo[i].boardName+"'>";
                                html += "        <p class='p_t' title='$"+lineTwo[i].boardName+"'>"+lineTwo[i].boardName+"</p>";
                                html += "<span class='p_n'>"+lineTwo[i].picCount+"图</span>";
                                html += "</a>";
                                html += "</li>";
                            }
                        }
                    }
                    $(".pic-list ul").append(html);
                }
            })
        }

        //查看是否支持localStorage
        function isLocalStorageSupported() {
            var enabled = true;
            if(!!window.localStorage && !!window.localStorage.getItem && !!window.localStorage.setItem) {
                try {
                    var testKey = '__testStore__'
                    window.localStorage.setItem(testKey,testKey)
                    if(window.localStorage.getItem(testKey) != testKey) {enabled = false}
                    window.localStorage.removeItem(testKey)
                } catch(err) {
                    console.log(err)
                    enabled = false
                }
            } else {
                enabled = false;
            }
            return enabled
        }

        $(".join-btn").live('click',function(){
            var $this=$(this);
            if(0==0){
                window.location.href = $(this).attr("toUrl");
            }else{
                var bid = $this.attr("data-bid")
                $this.hide();
                $.when(
                        $.ajax({
                            type: "post",
                            url: "/u/board/subscribe/add",
                            data:{bids:[bid]}
                        })
                ).done(
                        function (res) {
                            if (res.success) {
                                var number=$this.parent("dd").find(".subscribeNum").attr("data-num");
                                var subscribeNum=((parseInt(number)+1)<=9999)?(parseInt(number)+1):((parseInt(number)+1) / 10000).toFixed(1) + "万"
                                var mes="成员:"+subscribeNum;
                                $this.parent("dd").find(".subscribeNum").html(mes);
                                $this.html("").removeClass("join-btn");
                                if(LOCAL_STOR_SUPPORT) {
                                    var localData = JSON.parse(window.localStorage.getItem(LOCAL_KEY))
                                    var returnList = []
                                    if(localData) {
                                        returnList = localData.returnList ? localData.returnList : []
                                    }
                                    if(returnList.length > 0) {
                                        var updateData = localData
                                        updateData.returnList.forEach(function(item) {
                                            if(bid == item.bidStr) {
                                                item.subscribe = true
                                                item.subscribeNum += 1
                                            }
                                        })
                                        window.localStorage.removeItem(LOCAL_KEY)
                                        window.localStorage.setItem(LOCAL_KEY,JSON.stringify(updateData))
                                    }
                                }
                                showTip("加入成功！");

                            }else {
                                switch (res.code) {
                                    case 999:
                                        window.location.href = "https://"+window.location.host+"/wap/login";
                                        break;
                                    case 101:
                                        $this.show();
                                        showTip("该兴趣圈已经被关闭！");
                                        break;
                                    case 102:
                                        $this.show();
                                        showTip("该兴趣圈已经被关闭！");
                                        break;
                                    case 103:
                                        $this.show();
                                        showTip("你已经加入该兴趣圈！");
                                        break;
                                    case 104:
                                        $this.show();
                                        showTip("你没有权限加入该兴趣圈！");
                                        break;
                                    default:
                                        $this.show();
                                        showTip("加入失败，请稍后再试！");
                                        break;
                                }
                            }
                        }).fail(function () {
                            showTip("服务器正忙，请稍后再试");
                        });
            }
        })

    })
