<script type="text/javascript">
   QueryString = (function (a) {
        if (a == "") return {};
        var b = {};
        for (var i = 0; i < a.length; ++i) {
            var p = a[i].split('=');
            if (p.length != 2) continue;
            b[p[0]] = decodeURIComponent(p[1].replace(/\+/g, " "));
        }
        return b;
    })(window.location.search.substr(1).split('&'));

    //实例化
    var passport = new hjpassport();
    //设置 source,点击注册时该 source 将一起传递
    passport.source = QueryString['source'] || 'cichang';  //'quick_login';
    //登录处理页，一般不需要更改
    passport.baseUrl = 'http://pass.hujiang.com/ulogin/quick';
    //实现跨域的代理文件，需要放在应用方服务器上，并在这里声明 url 地址
    passport.agent = 'http://cichang.hujiang.com/agent.html';
    //用户登录成功以后的回调函数
    passport.validationSuccess = 'loginok()';
    //自定义的回调函数
    function loginok() {
        window.location.reload();
    };
</script>
    

<script type="text/javascript">
   

    var IngFrame = {
        lastHeight: 0,
        setHeight: function (h) {

            if (h < 360) {
                h = 360;
            }

            $('#appframe').animate({ 'height': h+30}, function () {
                IngFrame.lastHeight = h;
            });
        },
        getHeight: function () {
            return parseInt($.cookie('ingFrameHeight') || 0);
        }
    };
    IngFrame.init = function () {
        var self = this;

        self.lastHeight = self.getHeight();
        self.setHeight(self.lastHeight);

        setInterval(function () {
            var h = self.getHeight();

            if (IngFrame.lastHeight < h) {
                IngFrame.setHeight(h);
            }
        }, 400);
    }



    var ingSenderBox = null;
    $(function () {

        IngFrame.init();

        $("#trialtip").dialog({
            modal: true,
            dialogClass: 'noTitleStuff',
            width: 400,
            height: 230,
            resizable: false,
            autoOpen: false
        });
        iscontain(10403,'零基础音标班词场');
        FirstLoad(10403);
      
        ingSenderBox = new ingSender();
        ingSenderBox.callback_success = function () {
            ingSenderBox.showSuccess("发送成功！ <a style='margin:0px 5px 0px 10px' href='http://t.hujiang.com/0' class='noline' target='_blank'>查看碎碎&raquo;</a>", true, 3000);
        };

        //送课
        
      
    });



    function wordtest(bid, uid, type) {
        

        if (type)
            centerWin("/" + bid + "/wordtest" + uid + "/t" + type, 1020, 700);
        else
            centerWin("/" + bid + "/wordtest" + uid + "?from=book", 1020, 700);
        
    }

    function wordpk(bid) {
        var isLogin="False";
        if(isLogin=="True")
        {
            centerWin("/" + bid + "/wordpk/", 1020, 700);
        }else
        {
            window.hjquicklogin.doLogin();
        }
    }

    function firstView(bid, uid, type) {
        var url = "/SetWordPlan.aspx?bookid=" + "10403";
        $.jBox("get:" + url, {
            "title": "设置计划",
            "width": 400,
            "height": 200,
            opacity: 0.1,
            top: "20%",
            "showIcon": false,
            "buttons": { "设置": 1, "不制定计划直接背词": 0 },
            submit: function(v, h, f) {
                if (v) {
                    if (typeof window.setPlan === "function") {
                        var res = window.setPlan();
                        if (res < 0) {
                            location.href = '/reminder';
                            return true;
                        } else if (res == 0) {
                            return false;
                        }
                    }
                }
                $.post('/services/HomeService.asmx/HadFirstRecitePlan', { bookid: "10403" });
                if (type) {
                    centerWin("/" + bid + "/wordtest" + uid + "/t" + type, 1020, 700);
                } else {
                    centerWin("/" + bid + "/wordtest" + uid + "?from=book", 1020, 700);
                }
                return true;
            }
        });
    }
   
</script>
