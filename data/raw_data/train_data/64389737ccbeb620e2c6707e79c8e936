<script type="text/javascript">
    $(function () {
        $("#login_form input").keydown(function (e) {
            if (e.keyCode == 13) {
                login();
                return false;
            }
        });
        $("#login_form").dialog({
            width: 400,
            height: 200,
            modal: true,
            dialogClass: 'noTitleBackground',
            resizable: false,
            autoOpen: false,
            title: '亲，先登录一下吧，词场君才能为你保存背词记录哦！'
        });
    });
    function hj_register() {
        window.location.href = "http://pass.hujiang.com/signup/?source=cichang&template=cichang&r=1&url=" + window.location.href;
    }

    function hj_show_login() {
        //window.location.href = "http://www.hjclass.com/login?source=cichang&template=cichang&r=1&returnurl=" + window.location.href;
        window.location.href = "http://pass.hujiang.com/?url=" + window.location.href;
        //$("#login_form").dialog("open");
    }

    function login() {
        var userName = document.getElementById("txtName").value;
        var passwort = document.getElementById("txtPassword").value;
        if (userName == '' || passwort == '') return false;
        hjpass.OnInitialize();

        // 是否记住密码
        var processCookie = "0";
        if (!document.getElementById("chkRember").checked) {
            processCookie = "1";
        }

        hjpass.OnSignIn(
            userName,
            passwort,
            2,
            processCookie
        );
        // 正在登录，请稍候...
        var loadingImg = $("<img src='/images/ajax-loader.gif'/>");
        $("#login_form").block({
            message: loadingImg,
            css: {
                width: '32px',
                border: 'none',
                background: 'none'
            },
            overlayCSS: { backgroundColor: '#FFF', opacity: 0.8 }
        });
        return false;
    }
</script>

