function submitBut(){
    var catid = Number($("#catid").val());
    var kw = $("#kw_input").val();
    if( catid == 0 && kw == ''){
        alert("请填写关键词");
        $("#kw_input").focus();
        return false;
    }
    var url = "http://www.pibu.com/news/search-htm";
    if( catid ) url += "-catid-"+catid;
    if( kw ) url += "-kw-"+kw;
    url = url+".html";
    location.href = url;
}
</script> 
