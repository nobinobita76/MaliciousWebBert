<script type="text/javascript" language="javascript">
var $tp_obj = null;
function lxfEndtime(){
	$tp_t=$('#tp_t').html();
	if($tp_t!=0){
		$('#tp_t').html($tp_t-1);
		$tp_obj=setTimeout("lxfEndtime()",1000);
	}else{		
		$('.tp_box').hide();
		$('#tp_t').html(6);
		$('.tp_ad_time').css({'width':'640px','height':'480px'});
		clearTimeout($tp_obj);
	}
};
$(document).ready(function(){
	$('.tp_close').click(function(){
		$('.tp_box').hide();
		$('#tp_t').html(6);
		$('.tp_ad_time').css({'width':'640px','height':'480px'});
		clearTimeout($tp_obj);
	});
	//开始显示
	$('.tp_box').show();	
	$('.tp_ad_time').animate({width:110,height:18},'slow');
	lxfEndtime();
	

});
</script>
