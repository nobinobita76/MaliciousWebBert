<script>
    $(function(){
        if( !isPhone() && oldMedia != 'xs' ){
            $('.index-wrap a').hover(function(){
                $(this).siblings('span').stop(true,true).fadeIn(200)
            },function(){
                $(this).siblings('span').stop(true,true).fadeOut(200)
            })
        }
    })
    
    $(function(){
	click5();
     clickPopu('#click1','.popu-01');   
     clickPopu('#click2','.popu-2'); 
     clickPopu('#click3','.popu-3'); 
     clickPopu('#click4','.popu-4'); 
     clickPopu('#click43','.popu-4'); 
     clickPopu('#click5','.popu-5'); 
      clickPopu('#click6','.popu-6');
     clickPopu('#click7','.popu-7');	  
	 	 
     clickClose('#close-1','.popu-01');
     clickClose('#close-2','.popu-2');
     clickClose('#close-3','.popu-3');
      clickClose('#close-4','.popu-4');
       clickClose('#close-5','.popu-5');
        clickClose('#close-6','.popu-6');
		clickClose('#close-7','.popu-7');
		function click5(){
			$('#click5').hover(function(){
					$(this).css('opacity','0.5');
			},function(){
					$(this).css('opacity','1');
				})
		}
        function clickPopu(clickId,showClass){
            $(clickId).click(function(){
            $(showClass).show();
            $('#TB_overlayBG').show();
        });
    }
        function clickClose(clickId,closeClass){
            $(clickId).click(function(){
            $(closeClass).hide();
            $('#TB_overlayBG').hide();
            });
        }  
        
        $(".close").click(function(){
            $("#TB_overlayBG").css("display","none");
            $(".login-Popup ").css("display","none");
        });
  $('.popu-suc .close').click(function(){
     $('.popu-suc').hide();
  })
         $('.popu-6 ul li').hover(function(){
            $(this).find('img').stop(true,true).animate({marginTop:'-5px'},0);   
        },function(){
            $('.popu-6 ul li').find('img').stop(true,true).animate({marginTop:'0px'},0);
        }) 
        $('.popu-01').find('input').focus(function(){
                $(this).css('color','#000').val('');
                $(this).blur(function(){
                var text_val = $(this).val();
                var $d_dom = $(this);
                var d_dom = $d_dom[0];
                if (text_val == d_dom.defaultValue || text_val == '') {
                    $(this).css('color','#ccc').val(d_dom.defaultValue);
                    //$(this).val('');
                } else {
                $(this).css('color','#000');
                }
             })
        })
    }) 
</script>

<script>
 $('.paly-sidler').flexslider({
        animation: "slide",
        animationLoop: false,
        itemWidth: 225,
        itemMargin: 0,
        minItems: 2,
        maxItems: 3,
        start: function(slider){
          $('.tech-car-list').find('.flex-direction-nav').addClass('load-nav');;
          $('.flex-prev').addClass('load-prev');
          $('.flex-next').addClass('load-next');
          $('.paly-sidler').find('.flex-control-nav').hide();
        }
      })
 $(function(){
 
    $(".show-Popup").click(function(){

        $("#TB_overlayBG").css({
            display:"block",height:$(document).height()
        });
        $(".login-Popup").css({
            left:($("body").width()-$(".login-Popup").width())/2+"px",
            top:($(window).height()-$(".login-Popup").height())/2+$(window).scrollTop()+"px",
            display:"block"
        });
    });
    
    $(".close").click(function(){
        $("#TB_overlayBG").css("display","none");
        $(".login-Popup ").css("display","none");
    });

    /*登陆JS*/
   $('.login-Popup').find('input').focus(function(){
        $(this).css('color','#000').val('');
        $(this).blur(function(){
			var text_val = $(this).val();
			var $d_dom = $(this);
			var d_dom = $d_dom[0];
			if (text_val == d_dom.defaultValue || text_val == '') {
				$(this).css('color','#ccc').val(d_dom.defaultValue);
				//$(this).val('');
			} else {
			$(this).css('color','#000');
			}
		})
	})
})
    $(function(){
        if( !isPhone() && oldMedia != 'xs' ){
            $('.index-wrap a').hover(function(){
                $(this).siblings('span').stop(true,true).fadeIn(200)
            },function(){
                $(this).siblings('span').stop(true,true).fadeOut(200)
            })
        }
        showMenu();
    })
    function showMenu(){
        var showMenu=$('.container .sel-menu li');
        var index=0;
        var thisMenu=$('.index-car-Popup');
        var levaeMune=$('.show-menu');
         var t=0;
        showMenu.hover(function(){
            index=$(this).index() - 1;
            if(!thisMenu.is(":animated")){
                if(index >= 0){
                thisMenu.eq(index).delay(200).animate({height:"show"},500).siblings(thisMenu).stop().hide();
               } 
            } 
            if(index >= 3 || index < 0)
                thisMenu.stop().hide(); 
        })
       $('.show-menu,.nav').mouseleave(function(){
             thisMenu.stop().hide();
       }) 
   }
	
	    $('.splendid-down span').css('visibility','hidden')
    $('.splendid-down img').hover(function(){
        $(this).parent('.splendid-down').find('span').css('visibility','visible');
    },function(){
        $(this).parent('.splendid-down').find('span').css('visibility','hidden');
    })
$('.header .login').hover(function(){
         $(this).find('.exit').show();    
},function(){
        $(this).find('.exit').hide(); 
})
$(function(){
    var str=$('.login a').text();
    if($('.login a').text().length>=6){
    var newStr=str.substr(0,5)+"...";
   $('.login a').text(newStr);
 }
})

