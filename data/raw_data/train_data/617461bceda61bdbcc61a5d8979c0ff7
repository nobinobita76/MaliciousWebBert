  <script>
<!--登录框-->
$(document).delegate(".login","click",function(){
    $('.login_password').attr('type','password');
    $('.black_bg').width($(window).width()).height($(window).height()).fadeTo(0,0.4);
    $(".login_box").show();
});
$(".close_btn,.black_bg").click(function(){
    $(".black_bg").fadeOut();
    $(".login_box").hide();
});
//*********star 验证码*********//
//随机reqid
var reqid='';
function randomId(phone){
    var timestamp=new Date().getTime();
    reqid=phone+'_'+timestamp;
    return reqid;
}
//请求图片验证码
function picYzmAjaxR(phone){
    reqid=randomId(phone);
    var imgUrl='/m/sms_img_cap.json?reqid='+reqid;
    $('#yzm_img').attr('src',imgUrl);
}
//初始图片验证码
function yzmCyrl(){
    var phoneVal=$('.login_name').val();
    if(phoneVal.length==11){
        if($('.yzm_cont').is(':hidden')){
            $('.yzm_cont').show();
            picYzmAjaxR(phoneVal);
        }else{
            return false;
        }
    }else{
        $('.yzm_input').val('');
        $('.yzm_cont').hide();
        return false;
    }
}
$('.login_name').keyup(function(){
    yzmCyrl();
});
$('.login_name').blur(function(){
    yzmCyrl();
});
//图片验证码点击切换
$('#yzm_img').click(function(){
    var phoneVal=$('.login_name').val();
    picYzmAjaxR(phoneVal);//获取图片验证码
});
$(".login_btn").click(function(){
    var url="/cu/login";
    var isChecked=$("#remember_input").is(":checked");
    var phones=$("#phone").val();
    var pwds=hex_md5($("#pwd").val());
    var phs=$("#ph").val();
    var cap=$('.yzm_input').val();
    var passwordVal=$("#pwd").val();
    var patternPhone=/^1[3|4|5|7|8][0-9]\d{8}$/;
    if(!patternPhone.test(phones)){
        alert('手机号格式有误');
        return false;
    }else if(passwordVal==''||cap==''){
        alert('密码或验证码不能为空');
        return false;
    }else{
    //登录
        $.getJSON(url,{phone:phones,pwd:pwds,checked:isChecked,ph:phs,reqid:reqid,cap:cap},function(data){
            img_vip='';
            if(data.status){
                if (data.talent_v && data.talent_v>=1) {
                    if(data.talent_v == 1){
                        img_vip="<img src='http://wscdn.miaopai.com/static20131031/miaopai20140729/pc-static/images/vip.png' class='vip_header'>";
                    }else if(data.talent_v == 2){
                        img_vip="<img src='http://wscdn.miaopai.com/static20131031/miaopai20140729/img/v2.png' class='vip_header'>";
                    }
                }
                html='<div class="login_yes"><a href="'+data.url+'"><p style="background-image:url('+data.icon+')" class="header_perPic left">'+img_vip+'</p></a><a href="'+data.url+'"><sapn class="header_name">'+data.nick+'</sapn></a><a href="javascript:;" class="sign_out">登出</a></div>';
                $(".personal").html(html);
                $(".close_btn").click();
            }else{
                alert(data.msg);
            }
        })
    }
});
