        <script>
            function openWin(title, url, width, height) {
                var cont = 'width=' + width + ',height=' + height + ',top=' + (screen.height - height) / 2 + ',left=' + (screen.width - width) / 2 + ',toolbar=no,menubar=no,scrollbars=yes,resizable=no,location=no,status=no';
                var childWin = window.open(url, "_blank", cont);
                if (childWin.document) {
                    childWin.document.title = title;
                }
            }

            function openOnlineService() {
                openWin('在线客服', 'http://care3.live800.com/live800/chatClient/chatbox.jsp?companyID=8080&configID=787&info=', 800, 550);
                log.send({
                    actionTag: 'service_entrance'
                });
            }

            function toAddFeedback() {
                location.href = '/college/v1/feedback/toAdd';
                log.send({
                    actionTag: 'feedback_entrance'
                });
            }
            $(function() {
                var backTop = function() {
                    if ($(window).scrollTop() >= 300) {
                        $(".fix-backtop").css('display', 'block');
                        $(".fix-backtop").fadeIn(300);
                    } else {
                        $(".fix-backtop").fadeOut(300);
                    }
                };
                $(window).scroll(function() {
                    backTop();
                });
                $(".fix-top").click(function() {
                    $("html,body").animate({
                        scrollTop: "0px"
                    }, 500);
                });
                var isIe = /MSIE|Trident/gi.test(navigator.userAgent);
                if (isIe) {
                    $('.course-list.jin-course-main').css({
                        'min-height': 'auto'
                    });
                }
            });
        </script>
        <div class="right-fixed" style="display: block;">
            <a href="javascript:void(0);" class="fix-call v-middle" onclick="openOnlineService();">
                <div>
                    <i class="iconfix-call"></i>
                    <span>在线客服</span>
                </div>
            </a>
            <a href="javascript:void(0);" class="fix-feedback v-middle" onclick="toAddFeedback();">
                <div>
                    <i class="iconfix-feedback"></i>
                    <span>意见反馈</span>
                </div>
            </a>
            <a href="javascript:void(0);" class="fix-top v-middle">
