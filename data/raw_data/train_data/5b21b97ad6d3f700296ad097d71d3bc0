	$(document).ready(function(){
		$("#input_a").click(function(){
			var keyword = $("#input_text").val();
			if(keyword == ''){
				alert("搜索内容不能为空");
				return false;
			}else{
				window.location.href='index.php?keyword='+keyword;
			}
		});
		$("#input_text").keydown(function(e){
			if(e.which == 13){
				var keyword = $("#input_text").val();
				if(keyword == ''){
					alert("搜索内容不能为空");
					return false;
				}else{
					window.location.href='index.php?keyword='+keyword;
				}
			}			
		});
	});
	$(document).ready(function(){
		var cid,letter;
				if(cid){
			$("#a_"+cid).addClass("current_category");
			$("#child_list").html("<h3>"+$("#a_"+cid).text()+"</h3>");
		}else if(letter){
			$("#child_list").html("<h3>"+letter.toUpperCase()+"</h3>");
		}
	});
</script> 
