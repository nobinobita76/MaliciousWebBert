var signup_phone_default = '请输入手机号';
var signup_all_count = '请输入报名人数';
var signup_name_default = '请输入用户名';
var signup_captcha_default = '请输入验证码';
var signup_authimg_default = '请输入图文验证码';
var signup_email_default = '请输入邮箱';
var bind_phone = '';

$(document).ready(function() {
	
	var ajaxUrl = "/house/ajaxrequest/isCorrectMobileUser.php?rand="+Math.random();
	$.ajax({
		type:'get',
		dataType:'json',
		url:ajaxUrl,
		data:'',
		complete:function(){},
		error:function(){},
		success:function(msg){
			//如果用户已经登录
			if(msg.uid) {
				$("input#hide_cell").val(msg.bindMobile);
				//signup_phone_default = msg.bindMobile;
				bind_phone = msg.bindMobile;
				//alert(bind_phone);
				//初始化表框
				signUpFormInit();
				signUpRefresh();
				yy_signUpRefresh();
				if (msg.bindMobile){
					//隐藏获取手机号验证码的对话框
					$("input#signup_captcha").parent("li").css("display","none");
					$("input#tg_getValCode").css("display","none");
					$("input#tg_signup_captcha").parent("li").css("display","none");
					$("input#tg_getValCode_2").css("display","none");
					$("dl.yy_captcha_dl").css("display","none");
				}else {
					//显示获取手机号验证码的对话框
					$("input#signup_captcha").parent("li").css("display","block");
					$("input#tg_getValCode").css("display","block");
					$("input#tg_signup_captcha").parent("li").css("display","block");
					$("input#tg_getValCode_2").css("display","block");
					$("dl.yy_captcha_dl").css("display","block");
				}
				
			}else {
				//初始化表框
				signUpFormInit();
				signUpRefresh();
				yy_signUpRefresh();
			}
		}
	});
})
//给手机号输入框添加事件
$("input#signup_phone").keyup(function() {
	//是否绑定手机号
	if($("input#signup_phone").val()==bind_phone&&bind_phone!=signup_phone_default&&bind_phone!='') {
		//隐藏获取手机号验证码的对话框
		$("input#signup_captcha").parent("li").css("display","none");
		$("input#tg_getValCode").css("display","none");
	}else {
		//显示获取手机号验证码的对话框
		$("input#signup_captcha").parent("li").css("display","block");
		$("input#tg_getValCode").css("display","block");
	}
})
//===添加表框的动作事件
$("input#signup_phone").focus(function() {
	//检查是否和默认值相同，如果相同，那么直接清空
	if($("input#signup_phone").val()==signup_phone_default) {
		$("input#signup_phone").val('');
	}
})

$("input#signup_name").focus(function() {
	//检查是否和默认值相同，如果相同，那么直接清空
	if($("input#signup_name").val()==signup_name_default) {
		$("input#signup_name").val('');
	}
})

//失去焦点动作
$("input#signup_phone").blur(function() {
	//检查是否为空，如果为空那么直接等于默认值
	if($("input#signup_phone").val()=='') {
		$("input#signup_phone").val(signup_phone_default);
	}
})
$("input#signup_name").blur(function() {
	//检查是否为空，如果为空那么直接等于默认值
	if($("input#signup_name").val()=='') {
		$("input#signup_name").val(signup_name_default);
	}
})

//===初始化表框
function signUpFormInit() {
	$("input#signup_phone").val((bind_phone!='')?bind_phone:signup_phone_default);
	$("input#signup_name").val(signup_name_default);
	$("input#signup_count").val(signup_all_count);
}
//===验证码发送函数
var kftHintNum = 60;

var kftClockCodeHint;
    
function kftValCodeHint() {
    $("#tg_getValCode").val(kftHintNum+"秒后重新获取");
    kftClockCodeHint = setInterval(kftClockCodeHintFun, 1000);
}

function kftClockCodeHintFun() {
    if(kftHintNum > 0) {
        kftHintNum--;
        $("#tg_getValCode").val(kftHintNum+"秒后重新获取");
    } else {
        clearInterval(kftClockCodeHint);
        $("#tg_getValCode").val("重新获取验证码");
        $("#tg_getValCode").attr("disabled", false);
        kftHintNum = 60;
    }
}
