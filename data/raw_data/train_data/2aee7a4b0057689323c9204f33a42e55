                    mainCell:'.bd',
                    autoPlay:'true',
                    titCell:'.sw-box li',
                    effect:'leftLoop'
                });
                $('.slidebox2').slide({
                    mainCell:'.bd2',
                    autoPlay:'true',
                    titCell:'.hd-ul li',
                    effect:'leftLoop'
                })
                $('.ct-opn').mouseover(function(){
                    $(this).addClass('op-selected').siblings().removeClass('op-selected')
                    $('.ydi_1').eq($(this).index()).show().siblings('.ydi_1').hide();
                });
                $('.f-inp').focus(function(){
                    $(this).siblings('.placeholder').hide();
                });
                $('.f-inp').blur(function(){
                    if($(this).val()!=''){
                        $(this).siblings('.placeholder').hide();
                    }else{
                        $(this).siblings('.placeholder').show();
                    }

                });
                $('.pop-close').click(function(){
                    $(this).parents('.kft-popup').hide()
                })
                $('.xy').click(function(){
                    $('.xy-detail').toggle();
                })
            })
