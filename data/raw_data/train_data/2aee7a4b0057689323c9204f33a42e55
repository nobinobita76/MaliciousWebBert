var kftHintNum_3 = 60;

var kftClockCodeHint_3;

    // 手机验证码发送点击动作绑定
    


function kftValCodeHint_2() {
    $("#yy_getValCode").val(kftHintNum_3+"秒后重新获取");
    kftClockCodeHint_3 = setInterval(kftClockCodeHint_3Fun, 1000);
}

function kftClockCodeHint_3Fun() {
    if(kftHintNum_3 > 0) {
        kftHintNum_3--;
        $("#yy_getValCode").val(kftHintNum_3+"秒后重新获取");
    } else {
        clearInterval(kftClockCodeHint_3);
        $("#yy_getValCode").val("重新获取验证码");
        $("#yy_getValCode").attr("disabled", false);
        kftHintNum_3 = 60;
    }
}

var yy_name_default = "请输入用户名";
var yy_phone_default = "请输入手机号";
var yy_projname_default = "请输入楼盘名称";
var yy_captcha_default = "验证码";

function yy_signUpRefresh(){
	var x = '';
	if(bind_phone!='') {
		x = bind_phone;
	}else {
		x = yy_phone_default;
	}
	$("input#yy_phone").val(x);
    $("input#yy_name").val(yy_name_default);
    $("input#yy_captcha").val(yy_captcha_default);
    $("input#yy_projname").val(yy_projname_default);
}
//看房团预约
function yy_submitYuYue(){
	var ajaxUrl = "/house/web/kft_yuyue_ajax.php";
	var phone = $("#yy_phone").val();
	var name = $("#yy_name").val();
	var projname = $("#yy_projname").val();
	var phonecode= $("#yy_captcha").val();
	var newcode = ''
	if($("#yuyue_newcode").val()!='')
	{
		newcode = $("#yuyue_newcode").val();
	}
	if(!projname || projname == yy_projname_default ){
		alert('请输入楼盘名称'); return false;
	}
	else if(!name || name == yy_name_default){
		alert('请输入姓名'); return false;
	}			
	else if(!phone|| phone == yy_phone_default){
		alert('请输入手机号码'); return false;
	}	
	else if(!checkPhone('yy_phone')){
		alert('请输入正确的手机号码'); return false;
	}				

	phonecode = (phonecode==''||phonecode==yy_captcha_default)?'':$("#yy_captcha").val();
	$.ajax({
		type:'get',
		dataType:'jsonp',
		data:{mobilephone:phone,service:"newhouse-open-web",mobilecode:phonecode},
		url:"https://passport.fang.com/loginverifysms.api",
		error:function(){
			alert("验证码错误，请重试");},
		success:function(msg){
			if(msg.Message == "Success" || bind_phone){
				$.ajax({
					type:'get',
					dataType:'json',
					url:ajaxUrl,
					data:'phone='+phone+'&projname='+urlencode(projname)+'&name='+urlencode(name)+'&phonecode='+phonecode+'&city='+urlencode(city)+'&newcode='+newcode,
					//complete:function(){},
					success:function(msg){	
						if(msg.result==100){
							bind_phone = msg.bind_mobile;
						   // 设置报名成功弹出框的位置
							var thisTop = $(".kftyybtn").offset().top;  var thisLeft = $(".kftyybtn").offset().left;
							var tipTop = thisTop-170+"px";  var tipLeft = thisLeft -120 +"px";
							$("#yuyue_success").css({position:"absolute",top:tipTop,left:tipLeft});
							$("#yuyue_success").show();	
						}else{
							alert(msg.msg);
						}

						if(msg.captcha=='pass') {
							bind_phone = msg.bind_mobile;
							Xml.Request("/house/esf/getusernewhouse.php", null, doHeadloginuser);
						}
						yy_signUpRefresh();
					}
				});
			}else{
				alert(msg.Tip);
				return false;
			}
	    }
	});	
}		
</script>
	

<!--看房团列表页浏览埋码-->

<script type="text/javascript">
//路线搜索
$(function(){
_ub.city = '武汉';
		_ub.biz = 'n';
		_ub.location = 0/1;
		_ub.collect(0,{'vwg.page':'nhkftlist'}); 
})
</script>


<!--看房团热门团购搜索埋码-->

<script type="text/javascript">
//区域
	function hotPurchase()
	{		
		var district = $("#bs_strDistrict option:selected").text();
		var Price = $("#bs_strPrice option:selected").text();
		var Purpose = $("#bs_strPurpose option:selected").text();
		if(Price.substr(-2) == '以下')
		{
			var smallPrice = parseInt(Price);
			Price = '0-'+smallPrice;
		}

		if(Price.substr(-2) == '以上')
		{
			var bigPrice = parseInt(Price);
			Price = bigPrice+'-99999';
		}
		if(Price == '价格')
		{
			_ub.collect(1,{'vwn.position':encodeURIComponent(district),'vwn.genre':encodeURIComponent(Purpose)});
		}else {
			_ub.collect(1,{'vwn.position':encodeURIComponent(district),'vwn.unitprice':encodeURIComponent(Price),'vwn.genre':encodeURIComponent(Purpose)});
		}
	}
</script>



</body></html>