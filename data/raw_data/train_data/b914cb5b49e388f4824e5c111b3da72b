        //插件部分
        (function ($) {
            $.fn.fedFollow = function (id, opt) {
                var o = { left: 0, top: 0 };
                if (opt != null) { o = $.extend(o, opt); }
                var offset = $(id).offset();
                var height = $(id).height();

                $(this).css({ "position": "absolute", "left": offset.left + o.left, "top": offset.top + height + o.top }).show();
                return this;
            }

            $.famsg = $.famsg || {};
            $.extend($.famsg, {

                floatdiv: function (id, opt) {
                    var o = { offset: 513, top: 700 };
                    if (opt != null) {
                        o = $.extend(o, opt);
                    }

                    var offset = o.offset;
                    if (offset < 0) {
                        offset = offset - $(id).width();
                    }

                },
                goTop: function (id) {
                    $gotoTop = $(id).hide();

                    $.famsg.floatdiv(id, { offset: 513 });

                    $gotoTop.click(function (e) {
                        $(window).scrollTop(0);
                        e.preventDefault();
                    });
                    $(window).scroll(function () {
                        if ($(window).scrollTop() > 0) {
                            $gotoTop.show();
                        } else {
                            $gotoTop.hide();
                        }
                    });

                }

            });


        })(jQuery);


