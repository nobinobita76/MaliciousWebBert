    <div class="mainbody"> 
     <script type="text/javascript">
    var ParamArray = new Array();
    var price1, price2;



    function pageName() {
        var a = location.href.split('?')[0];
        var b = a.split("/");
        var c = b.slice(b.length - 1, b.length).toString(String).split(".shtml");
        return c.slice(0, 1).toString(String);
    }

    function GetHrefUrl(backCall, strParam) {
   
        var hrefUrl = "/" ;
    
        ParamArray = pageName().split('-');
        ParamArray[ParamArray.length-1]="0";
        ParamArray[ParamArray.length] = ".shtml";
        if (strParam != null) { backCall(strParam); } else
            var IsExec = backCall();
        if (IsExec) {
            return false;
        }
        for (var i = 0; i < ParamArray.length; i++) {
            if ((i + 2) >= ParamArray.length)
                hrefUrl = hrefUrl + ParamArray[i]
            else
                hrefUrl = hrefUrl + ParamArray[i] + "-";
        }
        var keyword = $.trim($("#SearchCondition1_txtKeyword").val());

        if (keyword != "") {
            hrefUrl += "?keyword=" +escape(keyword);
        }
       
        window.location.href = hrefUrl;
    }
    function GoSearch() {
        GetHrefUrl(NavSearch);
    }
    function SetRange(p1, p2) {
        price1 = p1; price2 = p2;
        GetHrefUrl(SetPriceRange);
    }
    function NavSearch() {
        $(".submit_btn").attr({ "href": "#" });

        var price1 = $("#SearchCondition1_txt_Price1").val();
        var price2 = $("#SearchCondition1_txt_Price2").val();
        ParamArray[12] = "a"; ParamArray[13] = "a";
        if (price1 != "" && price2 != "") {
            if (Check_Digital("SearchCondition1_txt_Price1") && Check_Digital("SearchCondition1_txt_Price2")) {
                if (parseFloat(price1) > parseFloat(price2))
                { ParamArray[12] = price2; ParamArray[13] = price1; }
                else { ParamArray[12] = price1; ParamArray[13] = price2; }
            }
        }
        else if (price1 != "" && price2 == "") {
            if (Check_Digital("SearchCondition1_txt_Price1"))
                ParamArray[12] = price1;

        }
        else if (price1 == "" && price2 != "") {
            if (Check_Digital("SearchCondition1_txt_Price2"))
                ParamArray[13] = price2;
        }
        ParamArray[12] = ParamArray[12].replace(".","$"); ParamArray[13]=ParamArray[13].replace(".","$"); //当前url不支持小数
        ParamArray[16] = "1"; ParamArray[ParamArray.length - 2] = "0";
    }

    function SetPriceRange() {
        if (price1 != "min")
            ParamArray[12] = price1;
            else
            ParamArray[12] = "a";

        if (price2 != "max")
            ParamArray[13] = price2;
            else
            ParamArray[13] = "a";

        ParamArray[16] = "1";
    }

    function AddUrlParam(str) {
        if (ParamArray[ParamArray.length - 1].indexOf("?") != -1)
            ParamArray[ParamArray.length - 1] += "&" + str;
        else
            ParamArray[ParamArray.length - 1] += "?" + str;
    }

    //输入的是否是价格数字
    function Check_Digital(id) {
        var re = new RegExp("(^[\\d]+\\.[\\d]{1,2}$)|([\\d]+$)", "g");
        //var re = new RegExp("([\\d]+$)", "g");
        
        var num = $("#" + id).val();

        if (num.match(re) == num || num == "") {
            return true
        }
        else {
            alert("请输入正确的价格");
            $("#" + id).val("");
            $("#" + id).focus();
            return false;
        }
    }

    //金额文本框限制
    function regInput(obj, reg, inputStr) {
        if (event.keyCode == 13) {
            event.keyCode = 9;
            event.returnValue = false;
            document.getElementById("btnSearch").click();
        }

        var docSel = document.selection.createRange();

        if (docSel.parentElement().tagName != "INPUT")
            return false;

        oSel = docSel.duplicate();
        oSel.text = "";

        var srcRange = obj.createTextRange();
        oSel.setEndPoint("StartToStart", srcRange);
        var str = oSel.text + inputStr + srcRange.text.substr(oSel.text.length);

        return reg.test(str);
    }

    function setPara(key, value) {


        GetHrefUrl(setCheckParam, { k: key, v: value });



        //  location.href = setUrlParam(key, value, RemovePageIndex(location.href));
    }
    function setCheckParam(strParam) {
        switch (strParam["k"]) {
            case "att":
                if(ParamArray[6]=="0"||ParamArray[6]=="")
                    ParamArray[6] = strParam["v"];
                else
                    ParamArray[6]+="$"+ strParam["v"];
                break;
            case "subatt":
                if (ParamArray[8] == "0" || ParamArray[8] == "")
                    ParamArray[8] = strParam["v"];
                else
                    ParamArray[8] += "$" + strParam["v"];
                break;
            case "ts":
                ParamArray[11] = strParam["v"];
                break;
            case "items":
                ParamArray[14] = strParam["v"];
                break;
            case "dktype":
                ParamArray[10] = strParam["v"];
                break;
            case "sort":
                ParamArray[15] = strParam["v"];
                break;
        }
        // return true;
    }
    function KeyWordEnter(obj, id) {
        if (event.keyCode == 13) {
            event.keyCode = 9;
            event.returnValue = false;
            document.getElementById(id).click();
        }
    }

    $(document).ready(function () {
        $('#SearchCondition1_txtKeyword').bind('focus', function () {
            $('#btnSearch').addClass('highlight_submit_btn');
        });
        $('#SearchCondition1_txt_Price1').bind('focus', function () {
            $('#btnSearchRange').addClass('highlight_submit_btn');
        });
        $('#SearchCondition1_txt_Price2').bind('focus', function () {
            $('#btnSearchRange').addClass('highlight_submit_btn');
        });
        $('#SearchCondition1_txtKeyword').bind('blur', function () {
            $('#btnSearch').removeClass('highlight_submit_btn');
        });
        $('#SearchCondition1_txt_Price1').bind('blur', function () {
            $('#btnSearchRange').removeClass('highlight_submit_btn');
        });
        $('#SearchCondition1_txt_Price2').bind('blur', function () {
            $('#btnSearchRange').removeClass('highlight_submit_btn');
        });
    });
</script> 
