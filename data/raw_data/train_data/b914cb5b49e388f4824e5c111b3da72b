        function GetCookie(name) {
            var arg = name + "=";
            var alen = arg.length;
            var clen = document.cookie.length;
            var i = 0;
            while (i < clen) {
                var j = i + alen;
                if (document.cookie.substring(i, j) == arg) return getCookieVal(j);
                i = document.cookie.indexOf(" ", i) + 1;
                if (i == 0) break;
            }
            return null;
        }
        /** 设置COOKIE */
        function SetCookie(name, value, expires) {

            document.cookie = name + "=" + value + ";expires=" + expires + ";path=/;domain=5173.com";
        }
        function getCookieVal(offset) {
            var endstr = document.cookie.indexOf(";", offset);
            if (endstr == -1) endstr = document.cookie.length;
            return unescape(document.cookie.substring(offset, endstr));
        }

        function CookieHistoryRecoder() {
            document.domain = "5173.com";
            var setCookieValue = "";;
            var cookieValue = GetCookie("cokGames");
            if (cookieValue != null) {
                var cookieList = cookieValue.split(',');
                var cookieCount = 0;
                for (var i = 0 ; i < cookieList.length ; i++) {
                    if (cookieList[i] != serachBarData.game.id)
                    { setCookieValue += "," + cookieList[i]; cookieCount++; if (cookieCount == 3) break; }
                }
            }
            var date = new Date();
            setCookieValue = serachBarData.game.id + setCookieValue
            date.setTime(date.getTime() + 3600 * 24 * 30 * 100);
            SetCookie("cokGames", setCookieValue, date.toGMTString());

        }
        CookieHistoryRecoder();
