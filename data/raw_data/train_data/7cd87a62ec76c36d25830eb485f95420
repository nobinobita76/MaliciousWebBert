<script language="javascript">
var submit_options = {
    dataType: 'json',
    beforeSubmit : function(){
      if($("#content2").val() == '') {
      	alert('请输入留言内容');
      	return false;
      }
      $("#failureDiv").empty().hide();
      login.show('succDiv');moveit('succDiv');
    },
    success : function(data){
    	modiP('#buqing');
        var message = "";
            if(typeof(data.message) == "string"){
                message = "<p>"+data.message+"</p>";
            }else{
                $.each(data.message, function(i, item){
                    message += "<p>"+item+"</p>";
                })
            }
            $("#failureDiv").html(message).show();
			if(data.flag){
				$("textarea[name='ret[content]']").empty().val('');
			}
			$("input[name='captcha_code']").empty().val('');
	        login.hide('succDiv');
    }
};
function modiP(pic){
	var b=Math.floor(Math.random() * 1000);
	var picRel=$(pic).attr('rel')+'&'+b;
	$(pic).parents('p').find('img').attr('src',picRel);
}

function check_form(){
	$('#add_message').ajaxSubmit(submit_options);
	return false;
}
</script>
