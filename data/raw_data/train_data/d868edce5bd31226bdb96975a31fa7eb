            window.onerror = killErrors;
            function showTab(n1,n2){
                var h=document.getElementById("tab"+n1).getElementsByTagName("h3");
                var d=document.getElementById("tab"+n1).getElementsByTagName("ul");
                for(var i=0;i<h.length;i++){
                    if(n2-1==i){
                        h[i].className+=" selected";
                        d[i].className+=" show";
                    }
                    else {
                        h[i].className=" ";
                        d[i].className=" ";
                    }
                }
            }
