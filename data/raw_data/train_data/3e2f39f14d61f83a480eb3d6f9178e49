var editor = null;
var attachConfig = {
		'attachnum' : parseInt('500'),
		'list' : {},
		'postData' : {fid : '148'},
		'filetype' : {"gif":810000,"png":810000,"zip":810000,"rar":810000,"jpg":810000,"txt":810000,"mp3":810000,"apk":2010000}
