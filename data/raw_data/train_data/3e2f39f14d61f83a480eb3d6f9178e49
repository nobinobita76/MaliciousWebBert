    <script>
// 手机端将浮动回复按钮改成本页面回复
function isMobile (){
	var ua = window.navigator.userAgent,
		mobile = false,
		m;
	if (/ Mobile\//.test(ua) && ua.match(/iPad|iPod|iPhone/)){
		mobile = 'apple';
	} else if (/ Android/i.test(ua)) {
		mobile = 'android';
	}  else if ((m = ua.match(/NokiaN[^\/]*|Android \d\.\d|webOS\/\d\.\d/))) {
        mobile = m[0].toLowerCase(); // Nokia N-series, Android, webOS, ex: NokiaN95
    } else {
    	if ((m = ua.match(/Presto\/([\d.]*)/)) && m[1]) {
    		if ((m = ua.match(/Opera\/([\d.]*)/)) && m[1]) {
      if ((m = ua.match(/Opera Mini[^;]*/)) && m) {
                        mobile = m[0].toLowerCase(); // ex: Opera Mini/2.0.4509/1316
                    } else if ((m = ua.match(/Opera Mobi[^;]*/)) && m) {
                        mobile = m[0];
                    }
    		}
    	}
    }

    return mobile;
}
function modifyMobileReplyHref () {
	if (isMobile() && document.querySelector && document.addEventListener) {
		var replyEle = document.querySelector('.reply'),
		loc = window.location.href;
		loc = loc.indexOf('#') === -1 ? loc : loc.split('#')[0];
		replyEle.addEventListener('click', function(e){
e.preventDefault();
// document.getElementById('textarea').focus();
window.location.href = loc + '#fp_editor';
if (!document.querySelector('.login_tips') && document.getElementById('textarea')) {
	document.getElementById('textarea').focus();
}
		});
	}
}
modifyMobileReplyHref();
// 移动端移动到底部时 回复按钮隐藏
function hideReplyBtn(){
	if (isMobile() && document.addEventListener) {
		var btn = document.querySelector('#side-nav');
		window.addEventListener('scroll', function(){
var wh = window.innerHeight,
	h = document.body.clientHeight,
	diff = 100,
	scrollTop = document.body.scrollTop;
if (scrollTop + diff + wh >= h ) {
	btn.style.display = 'none';	
} else {
	btn.style.display = 'block';	
}
		}, false);
	}
}
//hideReplyBtn();

// 去掉手机端回复楼层用户名的链接
function moblieReadName(){
	if (isMobile()) {
		var eles = document.querySelectorAll('.readName');
		for (var i = 0; i < eles.length; i++) {
var href = eles[i].getElementsByTagName('a')[0];
href && (href.setAttribute('href', 'javascript:;'));
		}
	}
}
moblieReadName();
</script> 
