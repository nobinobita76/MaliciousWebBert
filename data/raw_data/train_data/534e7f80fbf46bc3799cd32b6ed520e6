<script type="text/javascript">
	$(document).ready(function(){
	
	if(document.getElementById("ke_demo2").offsetHeight >= 248){
			document.getElementById("ke_demo3").innerHTML=document.getElementById("ke_demo2").innerHTML;
      function forul(){
      
      if(document.getElementById("ke_demo2").offsetHeight - document.getElementById("ke_demo").scrollTop <=0){
      
      document.getElementById("ke_demo").scrollTop -=document.getElementById("ke_demo3").offsetHeight;
      
      
      }else{
      document.getElementById("ke_demo").scrollTop = document.getElementById("ke_demo").scrollTop+1;

      
      }
	 }
      
      
      var setclear = setInterval(forul,30);
      
      document.getElementById("ke_demo").onmouseover=function(){ clearInterval(setclear)};
      document.getElementById("ke_demo").onmouseout=function(){ setclear = setInterval(forul,30)};
	  
	} 	
});
</script>

