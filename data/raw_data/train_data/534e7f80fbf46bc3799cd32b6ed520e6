<script>
var marqueeContent=new Array();   //滚动新闻
/*
*/
var marqueeId=0;
marqueeContent[marqueeId++]='<p><a href=/a/yiyuandongtai/gongshigonggao/20170324/561.html target=_blank class="f12red">【预评】重庆太岳新材料科技有限公司新型多功能复合材料生产项目</a></p>';
marqueeContent[marqueeId++]='<p><a href=/a/yiyuandongtai/gongshigonggao/20170324/560.html target=_blank class="f12red">【控评】重庆万里新能源股份有限公司整体搬迁变更项目</a></p>';
marqueeContent[marqueeId++]='<p><a href=/a/yiyuandongtai/gongshigonggao/20170322/558.html target=_blank class="f12red">重庆长安汽车股份有限公司10万辆R103焊接线扩能项目预评价网上公开内容</a></p>';
marqueeContent[marqueeId++]='<p><a href=/a/yiyuandongtai/gongshigonggao/20170322/557.html target=_blank class="f12red">重庆万国半导体科技有限公司12英寸功率半导体芯片制造及封装测试生产基地项</a></p>';
marqueeContent[marqueeId++]='<p><a href=/a/yiyuandongtai/gongshigonggao/20170322/556.html target=_blank class="f12red">太极集团重庆桐君阁药厂有限公司现状评价网上公开内容</a></p>';
marqueeContent[marqueeId++]='<p><a href=/a/yiyuandongtai/gongshigonggao/20170322/555.html target=_blank class="f12red">中科力泰现评网上公开内容</a></p>';
marqueeContent[marqueeId++]='<p><a href=/a/yiyuandongtai/gongshigonggao/20170322/554.html target=_blank class="f12red">中法水务技改项目评网上公开内容</a></p>';
marqueeContent[marqueeId++]='<p><a href=/a/yiyuandongtai/gongshigonggao/20170322/553.html target=_blank class="f12red">秋田化工现评网上公开内容</a></p>';
marqueeContent[marqueeId++]='<p><a href=/a/yiyuandongtai/gongshigonggao/20170322/552.html target=_blank class="f12red">惠源水务现评网上公开内容</a></p>';
marqueeContent[marqueeId++]='<p><a href=/a/yiyuandongtai/gongshigonggao/20170322/551.html target=_blank class="f12red">秋田化工技改项目预评网上公开内容</a></p>';


var marqueeInterval=new Array();  //定义一些常用而且要经常用到的变量
marqueeId=0;
var marqueeDelay=2000;
var marqueeHeight=20;
//接下来的是定义一些要使用到的函数
function initMarquee() {
    var str=marqueeContent[0];
    document.write('<div style="float:left;" class="index_spcs"><strong>最新公告：</strong></div><div id=marqueeBox style="float:left; margin-top:18px; line-height:20px; width:455px; color:#666; overflow:hidden;height:'+marqueeHeight+'px" onmouseover="clearInterval(marqueeInterval[0])" onmouseout="marqueeInterval[0]=setInterval(\'startMarquee()\',marqueeDelay)"><div>'+str+'</div></div>');
    marqueeId++;
    marqueeInterval[0]=setInterval("startMarquee()",marqueeDelay);
    }
function startMarquee() {
    var str=marqueeContent[marqueeId];
        marqueeId++;
    if(marqueeId>=marqueeContent.length) marqueeId=0;
    if(marqueeBox.childNodes.length==1) {
        var nextLine=document.createElement('DIV');
        nextLine.innerHTML=str;
        marqueeBox.appendChild(nextLine);
        }
    else {
        marqueeBox.childNodes[0].innerHTML=str;
        marqueeBox.appendChild(marqueeBox.childNodes[0]);
        marqueeBox.scrollTop=0;
        }
    clearInterval(marqueeInterval[1]);
    marqueeInterval[1]=setInterval("scrollMarquee()",20);
    }
function scrollMarquee() {
    marqueeBox.scrollTop++;
    if(marqueeBox.scrollTop%marqueeHeight==(marqueeHeight-1)){
        clearInterval(marqueeInterval[1]);
        }
    }
initMarquee();
</script>

	
