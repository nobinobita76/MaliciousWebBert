	$(document).ready(function(){
	
		document.getElementById("demo3").innerHTML = document.getElementById("demo2").innerHTML;
 
  function tabfor3(){
   if(document.getElementById("demo2").offsetWidth - document.getElementById("demo").scrollLeft <=0){
    document.getElementById("demo").scrollLeft -=document.getElementById("demo2").offsetWidth;
	
   }else{
    document.getElementById("demo").scrollLeft+=1;
   }
  }
 
     var setclear3 = setInterval(tabfor3,20);
  document.getElementById("demo").onmouseover=function(){ clearInterval(setclear3)};
  document.getElementById("demo").onmouseout=function(){ setclear3=setInterval(tabfor3,20)};
  
  
  
  document.getElementById("jkzx3").innerHTML = document.getElementById("jkzx2").innerHTML;
 
  function tabfor4(){
   if(document.getElementById("jkzx2").offsetWidth - document.getElementById("jkzx").scrollLeft <=0){
    document.getElementById("jkzx").scrollLeft -=document.getElementById("jkzx2").offsetWidth;
	
   }else{
    document.getElementById("jkzx").scrollLeft+=1;
   }
  }
 
     var setclear4 = setInterval(tabfor4,20);
