<script type="text/javascript">
	$("body").mouseover( function()
	{
		var scrollheight = $(document).scrollTop();
			
			if(scrollheight >= $(window).height() / 2 )
			{
				
				$("#goto_top").show();
				
			}else{
				$("#goto_top").hide();
			}

	});
</script>
