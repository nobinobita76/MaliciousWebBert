if(rDevice.test(navigator.userAgent) ) {
	//krpano_xml=krpano_xml.replace('vtourskin.xml','vtourskin_html5.xml');
}
/*在ipod上底部更换模型被隐藏*/
if(window.innerHeight<document.body.clientHeight){
	//document.body.style.height=window.innerHeight+'px';
}
