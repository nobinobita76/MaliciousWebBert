/*二维码显示控制*/
function qrod_show(args){
	var weixin = document.getElementById('weixin');
	if(args=='1'){
		weixin.style.display = 'block';
	}else{
		weixin.style.display = 'none';
	}
}
/*全景图加载完成，处理html显示元素*/
function loaded(){
	if(!rDevice.test(navigator.userAgent) ) {//检测到是电脑端
		qrod_show('1');
	}
	//隐藏启动图
	document.getElementById('start-img').style.display = 'none';
	//显示logo
	document.getElementById('logo').style.display = 'block';
}

var py_design=py;
var roam_id='25587';
var type=0;//普通漫游页面
var uid='15557';
var auth='WmVIL3QwZ1hlRGhDaWloS3VDK0RkUT09';
var img_id='',spot_id='',ath=0,atv=0,html='';
var krpano = document.getElementById("krpanoSWFObject");
var py_list = new Object();
/*添加已有的评语热点到插件显示*/
function add_py(){
	krpano.call("hide_all_py(); sync_imgid_to_js();");
}
/*执行添加*/
function run_add_py(){
	img_id = krpano.get("skin_settings.py_img_id");
	
	$.each(py_list,function(k,v){
		if(v.img_id == img_id){
			var str = '\'' + v.img_id + '\'';
			str += ',\'' + v.ath + '\'';
			str += ',\'' + v.atv + '\'';
			str += ',\'' + v.spot_id + '\'';
			str += ',\'' + v.desc + '\'';
			krpano.call("add_py_js("+ str +");");
		}
	});
}

/*初始化已存在的评语热点信息*/
function init_py(){
	/*获取初始数据*/
	$.ajax({
		type: "get",
		async: false,
		url: "//flashapi.to8to.com/fileapi/roampy/get",
		data: {'roam_id':roam_id,'type':type,'uid':uid,'auth':auth},
		dataType: "jsonp",
		jsonp: 'callback',
		success: function(data){
			py_list = data.data;
			krpano.set("skin_settings.py_max_spot_id", data.py_max_spot_id);
			add_py();
			if(py_design == 0 && py_list.length <= 0){//没有数据时隐藏设计说明按钮
				krpano.set("skin_settings.py_design_no_data", '1');
				krpano.call("skin_show_btn_py(1)");
			}
		}
	});
	
	krpano.set("skin_settings.py_design_btt_init", '1');
	krpano.set("skin_settings.width", $(krpano).width());
	krpano.set("skin_settings.height", $(krpano).height());
}



/*保存评语热点信息，位置，内容等*/
function save_dragspot(){
	if(roam_id<=0){
		alert('参数错误！');
		return false;
	}
	html=$('#py .textarea textarea').val();
	if(!html || html == '请输入你的设计优点，特色等... 最多30个字符'){
		alert('请输入内容');
		return false;
	}
	
	$.ajax({
		type: "get",
		async: false,
		url: "//flashapi.to8to.com/fileapi/roampy/save",
		data: {'roam_id':roam_id,'type':type,'uid':uid,'img_id':img_id,'spot_id':spot_id,'ath':ath,'atv':atv,'desc':html,'auth':auth},
		dataType: "jsonp",
		jsonp: 'callback',
		success: function(data){
			//把数据加到列表
			var t = new Object();
			t.img_id = img_id;
			t.spot_id = spot_id;
			t.ath = ath;
			t.atv = atv;
			t.desc = html;
			py_list.push(t);
			alert(data.msg);
		},error:function(){
			alert('网络异常');
		}
	});
}
/*删除评语热点信息*/
function del_dragspot(id){
	if(roam_id<=0){
		alert('参数错误！');
		return false;
	}
	$.ajax({
		type: "get",
		async: false,
		url: "//flashapi.to8to.com/fileapi/roampy/del",
		data: {'roam_id':roam_id,'type':type,'uid':uid,'img_id':img_id,'spot_id':spot_id,'auth':auth},
		dataType: "jsonp",
		jsonp: 'callback',
		success: function(data){
			krpano.set("hotspot[dragspot_"+img_id+"_"+spot_id+"].visible", 'false' );
			$('#py').hide();
			alert(data.msg);
		},error:function(){
			alert('网络异常');
		}
	});
}

/*打开评语输入*/
function show_py_input(){
	$('#py').show();
	//同步屏幕宽度高度
	krpano.set("skin_settings.width", $(krpano).width());
	krpano.set("skin_settings.height", $(krpano).height());
}
/*隐藏评语输入*/
function hide_py_input(){
	$('#py').hide();
}

/*同步评语热点信息，位置，内容等*/
function sync_dragspot(withhtml){
	img_id=krpano.get("skin_settings.py_img_id");
	spot_id=krpano.get("skin_settings.py_spot_id");
	ath=krpano.get("skin_settings.py_ath");
	atv=krpano.get("skin_settings.py_atv");
	if(withhtml==1){
		/*同步flash内容到文本框内*/
		$('#py .textarea textarea').val('');
		var desc = krpano.get("skin_settings.py_desc");
		if(desc != '请输入你的设计优点，特色等... 最多30个字符'){
			$('#py .textarea textarea').val(krpano.get("skin_settings.py_desc") );
		}
		$('#py .textarea textarea').focus();
	}
}

/*绑定文本框文本更新操作，同步内容到插件内显示*/
function rsyc_html(obj){
	if(img_id){
		krpano.set("hotspot[dragspot_"+img_id+"_"+spot_id+"].desc", obj.val() );
		krpano.set("plugin[tooltip_dragspot_"+img_id+"_"+spot_id+"].html", obj.val() );
	}
}
/*点击事件上报*/
function click_event_up(id){
	if(id<=0){
		return false;
	}
	$.ajax({
		type: "get",
		async: false,
		url: "//flashapi.to8to.com/count/page?type=0&id=" + id,
		dataType: "jsonp",
		jsonp: 'callback',
		success: function(data){}
	});
}
</script>

</body></html>