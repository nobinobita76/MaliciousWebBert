<script type="text/javascript">

    function hidesearch() {
        jQuery("#p_search").hide();
    }
    function showsearch() {

        jQuery("#p_search").show();
    }
    function getJiancaiCateogry(categoryid, categoryName) {
        jQuery("#jiancaicategory").val(categoryid);
        jQuery("#p_search").hide();
        jQuery("#span_search").html(categoryName);
    }
    var adkey = '请输入建材关键词';
    var adlink = '';
    adsearchlink();
    function adsearchlink() {
        var isAdSearch = '0';
        if (parseInt(isAdSearch) == 1) {
            var adSearchKey1 = '请输入建材关键词';
            var adSearchKeyLink1 = '';
            var adSearchKey2 = '请输入建材关键词';
            var adSearchKeyLink2 = '';
            if (adSearchKeyLink2 != '') {
                var numid = 0;
                var adcookie = getCookie('homeadsearch');
                if (adcookie != null) {
                    numid = adcookie;
                }
                if (parseInt(numid) % 2 == 0) {
                    adkey = adSearchKey1;
                    adlink = adSearchKeyLink1;
                    //numid = 1;
                    addCookie('homeadsearch', '1', '1');

                } else {
                    adkey = adSearchKey2;
                    adlink = adSearchKeyLink2;
                    //numid = 0;
                    addCookie('homeadsearch', '0', '1');

                }
            }
            jQuery("#jiancaisearchkey").val(adkey);
        } else {
            jQuery("#jiancaisearchkey").val('请输入建材关键词');
        }
    }
    function adsearchfocus() {
        if (jQuery("#jiancaisearchkey").val() == adkey) {
            jQuery("#jiancaisearchkey").val('');
        }
    }
    function adsearchblur() {
        if (jQuery("#jiancaisearchkey").val() == '') {
            jQuery("#jiancaisearchkey").val(adkey);
        }
    }
    function searchSubmitNew() {
        var caseKey = jQuery("#jiancaisearchkey").val();

        if (adlink != '') {
            if (caseKey == '' || caseKey == '请输入建材关键词' || caseKey == adkey) {
                window.open(adlink);
                return false;
            }
        }

        if (caseKey == "请输入建材关键词") {
            caseKey = "";
        }
        var category = jQuery("#jiancaicategory").val();
        window.open("/jiancai/searchlist.aspx?q=" + caseKey + "&category=" + category);
        if (caseKey != null && caseKey != "") {
            jQuery.ajax({
                type: "GET",
                url: "/jiancai/Ajaxs/AddSearchKey.aspx?key=" + escape(caseKey),
                success: function(msg) {
                }
            });
        }
    }

    function addCookie(name, value, days) {
        var str = name + "=" + escape(value);
        if (days > 0) {
            var date = new Date();
            var ms = days * 24 * 3600 * 1000;
            date.setTime(date.getTime() + ms);
            str += ";path=/;domain=" + document.domain + ";expires=" + date.toGMTString();
        }
        document.cookie = str;
    }
    function getCookie(name) {
        var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
        if (arr = document.cookie.match(reg))
            return unescape(arr[2]);
        else return null;
    }
</script>

   
