  <script>
window['jifenneme'] = '金币';
window['islazyImg'] = true; //是否开启了延迟加载图片
var nowdomain ="http://www.dawuren.net/";
window['Default_tplPath'] = 'http://www.dawuren.net/template/pc/main/default/';

var ueConfig = {toolbars: [['fullscreen', 'emotion', 'simpleupload', 'insertimage', 'scrawl', 'music', 'insertvideo', 'snapscreen', 'map']],theme:'defaulti',wordCount:true,maximumWords:10000,autoHeightEnabled:false};
var ueSize1 = '0';
ueSize1 === '0'&&(ueConfig.wordCount = false);
ueConfig.maximumWords = ueSize1;
var ue = UE.getEditor('chrcontent',ueConfig);
$(function() {
	$('#imgcodeid').iAuthcode();
	$('#mySle').selectbox();
	$(document).modCity();
	$('#fabu').showMore();
	$('#weixin').showMore();
	$('#rmenu').rmenuShow();
	setTimeout(function(){lazyImg('#pagingList .n_img',false);},50);
	$(window).bind("scroll",function(){
		lazyImg('#pagingList .n_img',false);
	});
	$('.n_img').myAlbum();
	if('0' !== '0'){
		$('#s_a_0').addClass('cur');
	}else{
		$('#s_a_0').addClass('cur');
	}
	$('#mySearch').submit(function(e){
		e.preventDefault();
		getPagingGlobal({'_key':$('#keyword').val()});
	});
	$('#allZhuti,#myShare').hover(function(){
		$(this).toggleClass('open');
	});
	$('#seniorSend').click(function(e){
		e.preventDefault();
		isfabukill()&&window.open('http://www.dawuren.net/tieba/addedit.aspx?bigcategoryid='+keyvalues['a']+'&categoryid='+keyvalues['b']);
	});
	$('#fabu_title').focus(function(){
		isfabukill();
	});
	ue.addListener('focus',function(){
		isfabukill();
	});
	function isfabukill(){
		if($('#isfabukill').val() === '0'){
			MSGwindowShow('tieba','1','请登录后再发表新帖！',nowdomain+'member/login.html?from='+encodeURIComponent('http://www.dawuren.net/tieba/addedit.aspx?bigcategoryid='+keyvalues['a']+'&categoryid='+keyvalues['b']),'');
			return false;
		}
		return true;
	}
});
var keyvalues = {"table_id":"25",
	"pagesize":"20",
	"tplpath":"tieba",
	"tplname":"paging_tieba_index_list.html",
	"strlen":"120",	
	"titleLen":"80",
	"isjson":"0",
	"p":"1",
	"_key":"",
	"a":"0",//板块
	"b":"0",//小类
	"e":"0",//排序(0默认回复时间倒,1发帖时间倒,2:精华前,3回复数,4:浏览数)
	"g":"0",//只查看某用户贴(userid)
	"h":"0",//只看精华
	"i":"0"//有图
};
function checkmyfabu(){
	$('#fabu_bigcategoryid').val(keyvalues.a);
	$('#fabu_categoryid').val(keyvalues.b);
	if($('#fabu_title').val() === ''){
		MSGwindowShow('tieba','4','请输入帖子标题！','','');
		return false;
	}
	if(!ue.hasContents()){
		MSGwindowShow('tieba','4','请输入帖子内容！','','');
		return false;
	}
	if($('#i_code1').attr('data-isopen') === '1'){
		if($('#codeindex').val()==""){
			MSGwindowShow('tieba','4','对不起,请点击对应字符激活发帖!','','');
			return false;
		}
	}
}
function showCatA(mm,obj,node){
	getPagingGlobal(obj,node);
	$('#smallCatTxt').html('全部主题分类');
	$('#tiebaList').removeClass('showManage');
	if(!$('#'+mm)[0] || $('#'+mm).html() === ''){
		$('#allZhuti').hide();
	}else{
		$('#smallCat').html($('#'+mm).html());
		$('#allZhuti').show();
	}
	var bazhu = $(node).attr('data-bazhu');
	if(bazhu !== '0'){
		$('#bazhuNode').html('版主：'+bazhu);
	}else{
		$('#bazhuNode').html('');
	}
	window['Bigcategory'] = obj.a;
	getUserState();
	$("html,body").animate({scrollTop: $('#qiandaoWrap').offset().top},300);
	return false;
}
function showCatB(obj,sid,node){
	getPagingGlobal(obj,node);
	$(sid).html($(node).html());
	return false;
}

