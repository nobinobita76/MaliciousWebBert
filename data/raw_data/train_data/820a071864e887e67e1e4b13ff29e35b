<script>
	//    define global & prototype
	if (!String.prototype.format) {
		String.prototype.format = function () {
			var str = this.toString();
			if (!arguments.length)
				return str;
			var args = typeof arguments[0],
					args = (("string" == args || "number" == args) ? arguments : arguments[0]);
			for (arg in args)
				str = str.replace(RegExp("\\{" + arg + "\\}", "gi"), args[arg]);
			return str;
		}
	}

	window.requestAFrame = (function () {
		return window.requestAnimationFrame ||
				window.webkitRequestAnimationFrame ||
				window.mozRequestAnimationFrame ||
				window.oRequestAnimationFrame ||
				function (callback) {
					return window.setTimeout(callback, 40);
				};
	})();
</script>
<script>
	function runFunc(funcGroup) {
		for (var func in funcGroup) {
			try {
				funcGroup[func]();
			}catch(e) {
				console.log(e);
			}
		}

	}

	var groupOfFunction = {
		addFunctionSwipeableToSlot: function () {
			var elScroll = document.querySelectorAll('.grid_scroll');
			Array.prototype.slice.call(elScroll).forEach(addSwipeAtSlot)

        function addSwipeAtSlot (slot) {
            new IScroll(slot, {eventPassthrough: true, scrollX: true, scrollY: false, preventDefault: true});
        }
    },
    imageLazyLoader: function () {
        var elImg = document.querySelectorAll('img');
        var elImgArr = Array.prototype.slice.call(elImg);
        elImgArr.forEach(function (el) {
            if (!!el.dataset.src) {
                el.src = el.dataset.src;
                delete el.dataset.src;
            }
        });
        window.requestAFrame(function () {
            elImgArr.forEach(function (el) {
                if (!!el.dataset.src) {
                    el.src = el.dataset.src;
                    delete el.dataset.src;
                }
            });
        });
    },
    launchDaumApp: function () {
         document.querySelector('#launchDaumApp').addEventListener('click', function (e) {
             launch();
             e.preventDefault();
         });
        function launch() {
            var ua = daumtools.userAgent();
            if (ua.platform === 'pc') {
                alert('모바일 기기에서 이용 가능한 기능입니다.\r\n' + ' 다음앱이 설치된 스마트폰에서 이용해 주세요.');
                return;
            }
            var appInfo = {
                appName: '다음앱',
                urlScheme: ua.os.ios ? 'daumapps://web?url=http://m.daum.net' : 'intent://web?url=' + encodeURIComponent(document.location.href) + '#Intent;scheme=daumapps;action=android.intent.action.VIEW;category=android.intent.category.BROWSABLE;package=net.daum.android.daum;end',
                storeURL: ua.os.ios ? 'itms-apps://itunes.apple.com/app/id365494029' : 'market://details?id=net.daum.android.daum&amp;referrer=param%3D' + encodeURIComponent('daumapps://web?url=' + encodeURIComponent(document.location.href)),
                intentURI: 'intent://web?url=' + encodeURIComponent(document.location.href) + '#Intent;scheme=daumapps;action=android.intent.action.VIEW;category=android.intent.category.BROWSABLE;package=net.daum.android.daum;end'
            };
			send('skt daumapp_gate');
			daumtools.web2app(appInfo);
        }
    },
    drawAd: function (){
		var opts = {
			forceWidth: "100%",
			callback: function (filled) {
				if (filled) {
					console.log("ad loaded");
				} else {
					console.log("no ad");
				}
			}
		}
        //광고
		VP.init({clientId: "400b9ae054fc3cc48013ada75832c905"});
		VP.ad('twifi_daum_ad_top', 'twifi_daum_ad_top',  320, 100, opts);
		VP.ad('twifi_daum_ad_bottom', 'twifi_daum_ad_bottom',  320, 300, opts);

    },
    drawChannelSwipe: function () {
        function removeChannelSwipeSlot(){
            var slot = document.querySelector('.box_chnscroll');
            slot.className += ' hide';
        }

        $.ajax({
            method: 'GET',
            url: 'http://{0}//api/channel/all?recommendation=true'.format(scriptInfo.pradaHost),
            dataType: 'json',
            timeout: 1000,
            success: function(data, textStatus, jqXHR) {
                var result = data.resultData.channelList;
                var liString = '';
                result.forEach(function (e) {
                    liString += '<li><a href="http://m.daum.net/channel/{code}" class="link_item  #skt#ch_gate#{code}_swipe_move"><span class="thumb_g"><img src="{djImage}" class="thumb_img" alt=""></span><strong class="tit_swipe">{name}</strong><span class="txt_sbjnum">읽을거리 {contentCount}</span></a></li>'.format(e);
                });
                var ulEl = document.querySelector('#channel_swipe div ul');
                requestAFrame(function () {
                    ulEl.insertAdjacentHTML('beforeend', liString);
                });
            },
            error: function (jqXHR, textStatus, errorThrown) {
                console.log('drawChannelSwipe function error -> {0}'.format(errorThrown));
                removeChannelSwipeSlot();
            }


			})
		},
		addFunctionSuggestToSearchInput: function () {
			(function () {
				var $ = daumtools.$,
						$$ = daumtools.$$,
						Event = daumtools.event;

				daum.suggest.SuggestListView = daum.suggest.SuggestListView.extend({
					_bindEvent: function () {
						this._super();
						var self = this;
						Event.delegate(this.el, '.btn_close', 'click', function () {

							self.hide();
						});
					}
				});

				daum.suggest.SuggestUrlView = daum.suggest.SuggestUrlView.extend({
					show: function () {
					}
				});

				daum.suggest.SuggestStorage = daum.suggest.SuggestStorage.extend({
					init: function () {
					}
				});

			}());

			var initOnload = function () {
				var el = document.getElementById("daumHead");
				if (typeof el === "undefined") {
					return;
				}


				var suggest = new daum.Suggest(el, {
					formEl: "form_totalsearch",
					serviceProfile: 'suggest_top',
					searchURL: "https://m.search.daum.net/search",
					xDomainURL: "https://m.search.daum.net",
					suggestNumber: 10,
					enableLatestKeyword: false,
					enableDeleteRecentItem: true,
					enableLatestKeywordLayer: false
				});

				suggest.disableRecentQuery();
			};
			initOnload();
		},
		scrollToTop: function () {
			var el = document.querySelector('.btn_pageup');
			el.addEventListener('click', function (e) {
				e.preventDefault();
				window.scrollTo(0, 0);
			});
			window.addEventListener('scroll', function () {
				if (window.scrollY > 150) {
					el.style.display = 'block';
				} else {
					el.style.display = 'none';
				}
			})
		}
	};

	// main 초기화 및 실행
	runFunc(groupOfFunction);
</script>
<script>
	function sendPageLog() {
		var dummy = new Date().getTime() + Math.ceil(Math.random() * 2147483647),
				url = window.location.href.split("#")[0], // remove hash
				startTs = tiaraInfo.onStartedAt,
				endTs = new Date().getTime(),
				loadingTime = (typeof startTs == 'number') ? endTs - startTs : '',
				beacon;

		beacon = new Image(1, 1); // 1 x 1
		beacon.onerror = function () {
			var beacon2 = new Image(1, 1); // 1 x 1
			beacon2.src = 'https://track.tiara.daum.net/queen/footsteps' +
					'?dummy=' + encodeURIComponent(dummy) +
					'&referer=' + encodeURIComponent(document.referrer) +
					'&title=' + encodeURIComponent(document.title) +
					'&param1=' + encodeURIComponent(dummy) +
					'&param2=bk_n' +
					'&url=' + encodeURIComponent(url) +
					'&loadtime=' + encodeURIComponent(loadingTime) +
					'&cke=' + (window.navigator.cookieEnabled ? 'Y' : 'N');
		}
		beacon.src = 'https://track.tiara.daum.net/queen/footsteps' +
				'?dummy=' + encodeURIComponent(dummy) +
				'&referer=' + encodeURIComponent(document.referrer) +
				'&title=' + encodeURIComponent(document.title) +
				'&param1=' + encodeURIComponent(dummy) +
				'&param2=bk_n' +
				'&url=' + encodeURIComponent(url) +
				'&loadtime=' + encodeURIComponent(loadingTime) +
				'&cke=' + (window.navigator.cookieEnabled ? 'Y' : 'N');
	}

	sendPageLog();


	function send(log, options) {
		try {
			var beacon = new Image(),
					url = "https://click.tiara.daum.net/queen/touch?",
					queryObj = {
						"curl": encodeURIComponent(window.location.href),
						"dpath": encodeURIComponent(log || ''),
						"sections": "t_wifi",
						"charset": "UTF-8",
						"puid": tiaraInfo.onCreatePageAt
					};

			for (var key in options) {
				if (options.hasOwnProperty(key)) {
					if (key === 'img' && typeof options.img === "string") {
						queryObj["img"] = encodeURIComponent(options.img);
					} else if (key === 'cno' && typeof options.cno === "number") {
						queryObj["cno"] = encodeURIComponent(options.cno);
					} else if (typeof options[key] !== 'undefined') {
						queryObj[key] = encodeURIComponent(options[key]);
					}
				}
			}
			var paramArr = [];
			for (var key in queryObj) {
				paramArr.push(key + '=' + queryObj[key]);
			}
			var param = "?" + paramArr.join("&");

                beacon.src = url + param;

            } catch(e) {
                console.log(e, "tiara error");
            }
    }

</script>

</body></html>