                if (document.cookie.indexOf("_trackEvent") == '-1') 
                {
                    setCookie("_trackEvent", 1); 
                    _hmt.push(['_trackEvent', '电话', '点击', '电话号码', $(this).attr('data-phone')]); 
                }
            });
