    $.ajax({
        type:'get',
        url:"http://weihai.liebiao.com/ajax/postclicks/?postid=341200577",
        cache:false,
        dataType:'json',
        success: function(json) {
            if (!json.error) {
                $('#post_visitors').html(json.msg.postclicks);
                $('#post_visitors_div').attr('title', '已被'+json.msg.postclicks+'人浏览');
            }
        }
    });

    function changeZimu_area () {
        $('.zimu_active').mousemove(function () {
            var zimu = $(this).attr('zimu-rel');
            var div = $('.zimuzuhe-con').find('div');
            for (var i = 0;i< div.length; i++) {
                if(div[i].className == 'zimuzuhe-' + zimu) {
                    $('.zimuzuhe-' + zimu).css('display','');
                } else {
                    $('.' + div[i].className).css('display','none');
                }
            }
        }).click(function(){
            return false;
        });
    }
    changeZimu_area();
</script>



