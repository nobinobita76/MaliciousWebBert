           <script type="text/javascript">
                                                (function ($) {

                                                    //设置小图分页个数
                                                    var minPicVis = 4;
                                                    var isFirst = true;
                                                    var isHover = false;

                                                    jQuery("#slidePic2Bd").hover(function () { isHover = true }, function () { isHover = false });

                                                    //小图滚动
                                                    jQuery("#slidePic2Bd").slide({
                                                        mainCell: "ul", effect: "left", vis: minPicVis, autoPlay: false, scroll: minPicVis, autoPage: true,
                                                        prevCell: ".sPrev", nextCell: ".sNext"
                                                    });

                                                    //大图切换
                                                    jQuery(".slidePic2").slide({
                                                        titCell: ".bd li", mainCell: ".focus", effect: "fade", vis: 1, autoPlay: true,
                                                        prevCell: ".page_big_prev", nextCell: ".page_big_next", startFun: function (i, p) {
                                                            if (i % 4 == 0 && !isHover) {
                                                                if (!isFirst) { $(".sNext").click(); }
                                                                isFirst = false;
                                                            }
                                                        }
                                                    });


                                                })(jQuery)
                                            </script> 
