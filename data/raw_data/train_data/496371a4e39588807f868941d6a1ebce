<!--app页面不会显示返回按钮，只有非app页面，才进行事件绑定-->

    <script type="text/javascript">
        (function(){
            var elButton = document.getElementById('js-top-banner-return');
            elButton.addEventListener('click', function (e) {
                e.preventDefault();
                var refer = document.referrer;
                if (refer) {
                    history.back();
                } else {
                    location.href = '/';
                }
            });
        })();
    </script>





