<script>
$(document).ready(function(){
	var cutil = bevans("beva.common.util"), tip=$("#js-login-error"), un=$("#js-login-username"), 
		pw=$("#js-login-password"), ca=$("#js-login-captcha"), img=$("#js-login-capthaImg"), btn=$("#js-login-btn"), 
		jsData={"source":0,"returnUrl":"http:\/\/account.beva.com\/newMy\/personal"}, errorCnt=0, check=false, size=cutil.getDocRect(), diff=size.scrollHeight-340;
	function subLoginData()
	{
		var unv, pwv, cav, d;
		if(btn.hasClass("none"))
		{
			alert("请勿重复提交");
			return false;
		}
		unv=un.val();
		if(unv=="")
		{
			un.addClass("on");
			tip.text("邮箱或手机号不能为空");
			return false;
		}
		if(unv.indexOf("@")<0)
		{
			if(!cutil.isMobileNumber(unv))
			{
				un.addClass("on");
				tip.text(/^\d+$/.test(unv) ? "手机号格式有误" : "邮箱格式有误");
				return false;
			}
		}
		else
		{
			if(!cutil.isEmail(unv))
			{
				un.addClass("on");
				tip.text("邮箱格式有误");
				return false;
			}
		}

		pwv=pw.val().length;
		if(pwv==0)
		{
			pw.addClass("on");
			tip.text("密码不能为空");
			return false;
		}

		if(check)
		{
			cav=ca.val().length;
			if(cav!=4)
			{
				ca.addClass("on");
				tip.text("请输入四位验证码");
				return false;
			}
			d = $("#js-login-form").serialize() + "&captcha=on";
		}
		else
		{
			d= "beva_username=" + unv + "&beva_password=" + pw.val();
			d=$("#js-login-rememberMe").attr("checked") ? d+"&beva_rememberMe=on" : d;
		}
		d+="&source="+jsData.source;
		
		btn.attr("disabled", true).addClass("none").text("登录中...");
		$.ajax({
			url: "/newAccount/ajaxLogin",
			type: "POST",
			data: d,
			dataType: "json",
			success: function(data){
				if(data.errorCode!=0)
				{
					if(++errorCnt>=3)
					{
						check=true;
						$("#js-login-capthali").show();
						img.trigger("click");
					}
					tip.text(data.message);
					btn.attr("disabled", false).removeClass("none").text("登录");
				}
				else
				{
					window.location.href=jsData.returnUrl;
				}
			},
			error: function(){
				btn.attr("disabled", false).removeClass("none").text("登录");
			}
		});
		
	}
	
	$("input").focus(function(){$(this).addClass("on");}).blur(function(){$(this).removeClass("on");tip.text("");});
	btn.click(subLoginData);
	pw.keyup(function(event){if(event.which == 13){subLoginData();}});
	ca.keyup(function(event){if(event.which == 13){subLoginData();}});
});
</script>	<!--footer begin-->
