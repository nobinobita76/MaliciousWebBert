function checkstar(frm)
{
frm=document.getElementById(frm);
if (frm.Horoscope.value =="null") 
{ 
alert('请选择您的星座!!');
frm.Horoscope.focus();
return false;
}
else if (frm.Horoscope2.value=="null")
{
alert('请选择他（她）的星座~！');
frm.Horoscope2.focus();
return false;
}   
frm.submit();
}
function autosubmit(v)
{
if(v==1)
{
document.getElementById("select_star").name='star_a';
}else{
document.getElementById("select_star").name='star_b';
}  
document.fastquery.submit();         
}
function check(){
if (document.goBornTag.yourborntag.value == '-1'){
alert('请选择您与对方的生肖！');
return false;
}
if (document.goBornTag.theyborntag.value == '-1'){
alert('请选择您与对方的生肖！');
return false;
}
var iSex=document.goBornTag.iSex.value;
var iSex1=document.goBornTag.iSex1.value;
if(iSex==iSex1)
{
alert('您与对方的性别不能是一样的哦！请重新输入');
return false;
}
document.goBornTag.submit();
}
function check1(){
if (document.goBlood.Blood.value == '-1'){
alert('请选择您与对方的血型！');
return false;
}
if (document.goBlood.Blood1.value == '-1'){
alert('请选择您与对方的血型！');
return false;
}
var iSex=document.goBlood.iSex.value;
var iSex1=document.goBlood.iSex1.value;
if(iSex==iSex1)
{
alert('您与对方的性别不能是一样的哦！请重新输入');
return false;
}
document.goBlood.submit();
}
