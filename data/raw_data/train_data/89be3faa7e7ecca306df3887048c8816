function checkTop(){
var sideExt = document.getElementById("sideExt");sideExt.style.marginTop="";
var artExt = document.getElementById("artExt");artExt.style.marginTop="";
var artMain = document.getElementById("artMain");
var sideMain = document.getElementById("sideMain");
var minusHeight=artMain.scrollHeight-sideMain.scrollHeight;
if(artMain.scrollHeight>sideMain.scrollHeight){sideExt.style.marginTop=-minusHeight+"px";}
else{artExt.style.marginTop="0px";}
if(!pageLoaded) window.setTimeout(arguments.callee,85);
}
var pageLoaded = false;
function clearTimer(){pageLoaded = true; tanFn(checkTop)}
function tanFn(f){var d=[99,300,999,3000,9999];for(var i=0; i<d.length; i++)window.setTimeout(f,d[i]);}
