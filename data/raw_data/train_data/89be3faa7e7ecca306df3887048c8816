<script type="text/javascript">
var radios = pc.getElems('#zhys input'),
ys_year = pc.getElem('#ys_year'),
ys_month = pc.getElem('#ys_month'),
ys_day = pc.getElem('#ys_day'),
ys_hour = pc.getElem('#ys_hour'),
ys_sex = pc.getElem('#ys_sex');
pc.each(radios,function(item){
pc.addEvent(item,'click',function(){
var req = this.value;
var year = ys_year.value,
month = ys_month.value,
day = ys_day.value,
hour = ys_hour.value,
sex = ys_sex.value;
if(year == -1 || month == -1 || day == -1 || hour == -1){
alert("请选择您的生日！");return false;
}else if(sex == -1){
alert("请选择您的性别！");return false;
}
if(req !=''){
MM_openBrWindow(req+'?iYear='+year+'&iMonth='+month+'&iDay='+day+'&iHour='+hour+'&iSex='+sex,'color','toolbar=yes,location=yes,status=yes,menubar=yes,scrollbars=yes,resizable=yes');
}
})
});
