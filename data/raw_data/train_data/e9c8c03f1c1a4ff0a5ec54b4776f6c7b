        } else if (matchs = ua.match(/(iPhone|iPad|iPod)/)) {
            that.name = matchs[1];
            matchs = ua.match(/OS ([\d_\.]+) like Mac OS X/);
            that.isIPhone = that.name === 'iPhone' || that.name === 'iPod';
            that.isIPad = that.name === 'iPad';
            that.isIOS = !0;
            that.version = matchs[1].split("_").join(".");
        } else {
            that.name = 'unknown';
            that.version = '0.0.0';
        }
    }

    OS.prototype = {
        gte: function gte(v) {
            return this.compareTo(v) > -1;
        },
        lt: function lt(v) {
            return this.compareTo(v) < 0;
        },
        compareTo: function compareTo(version) {
            var cvs = this.version.toString().split("."),
                vs = version.toString().split("."),
                i = 0,
                k,
                j;
            for (; i < cvs.length || i < vs.length; i++) {
                k = parseInt(cvs[i], 10);
                j = parseInt(vs[i], 10);
                isNaN(k) && (k = 0);
                isNaN(j) && (j = 0);
                if (j > k) {
                    return -1;
                }
                if (k > j) {
                    return 1;
                }
            }
            return 0;
        }
    };

    exports.default = new OS();
    // added by tanxiangyuan 2016-04-13

    module.exports = exports['default'];

