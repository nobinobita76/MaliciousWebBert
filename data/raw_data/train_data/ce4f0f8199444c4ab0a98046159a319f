<script type="text/javascript">
    var successPageName = "wyhb";
    $(function(){
        var activity_status = $("#activity_status").val();
        if(activity_status == "2"){
            $(".past-tip-box").show();
        }else{
            $(".past-tip-box").hide();
        }
    });
    //判断IE浏览器版本号
    function IETester(userAgent){
        var UA =  userAgent || navigator.userAgent;
        if(/msie/i.test(UA)){
            //IE10及以下版本获取版本号
            return UA.match(/msie (\d+\.\d+)/i)[1];
        }else if(~UA.toLowerCase().indexOf('trident') && ~UA.indexOf('rv')){
            //IE11获取版本号
            return UA.match(/rv:(\d+\.\d+)/)[1];
        }
        return false;
    }
    if(IETester() && IETester() < 10 ){
        document.write('<script src="https://res.360jie.com.cn/dir_mkteditor/activity/common/jquery.placeholder.min.js"><\/script>');
        $(document).ready(function(){
            $("input").placeholder();
        });
    }
</script>



