      <script type="text/javascript">
  var divTab = document.getElementById("divTab");
  if (window.top != window && divTab && divTab.scrollIntoView) {
    try{
      divTab.scrollIntoView(); //嵌入页面，滚动条滚动到标题处
    }
    catch(ex){
    }
  }
  function postHeight() {
      if (window == window.top)
          return;
      try {
          var pmp = parent.postMessage ? parent : parent.document.postMessage ? parent.document : null;
          if (pmp != null)
              pmp.postMessage("heightChanged," + ((document.documentElement.scrollHeight || document.body.scrollHeight) + 50), "*");
      } catch (e) {
      }
  }
  postHeight();
    </script> 
