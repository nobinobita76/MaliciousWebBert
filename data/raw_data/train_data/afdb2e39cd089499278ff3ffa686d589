                    function showPromote(){
                         var promoteid="ctl01_ContentPlaceHolder1_divProMsg";
                        var divProMsg=document.getElementById(promoteid);
                        if(divProMsg){
                            if(!divProMsg.pDiv){
                                var newDiv=document.createElement("div");
                                newDiv.innerHTML="<div style='margin:20px;text-align:left;'>"+divProMsg.innerHTML+"</div>";
                                newDiv.style.display="none";
                                newDiv.id="divPro1234";
                                divProMsg.parentNode.appendChild(newDiv);
                                divProMsg.pDiv=newDiv;
                            }
                            PDF_launch("divPro1234",660,400);
                        }
                    }
