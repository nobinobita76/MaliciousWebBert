    seajs.use(['//static.360buyimg.com/devfe/toolbar/1.0.0/js/main'], function(toolbar) {
        var sid = pageConfig.product.cat[2] === 832 ? '737542' : '992349';

        var phoneNetwork = pageConfig.product.phoneNetwork
            ? pageConfig.product.phoneNetwork.join(',')
            : '';

        var hallEnable = pageConfig.product.cat[2] === 655;
        var hallUrl = {
            url: '//ctc.jd.com/hall/index?',
            param: {
                sku: pageConfig.product.skuid,
                cat: pageConfig.product.cat.join(','),
                mode: phoneNetwork
            }
        };
   
        var cat = pageConfig.product.cat;
        var ad_entry = { enabled: false };

        if (pageConfig.product.pTag === 423886) {
/*
            ad_entry = {
                enabled: true,
                id: "0_0_7209",
                startTime: +new Date(2017, 3, 1, 0, 0, 1) / 1000,
                endTime: +new Date(2017, 4, 3, 0, 0, 0) / 1000
            }
*/
        }

        pageConfig.toolbar = new toolbar({
            pType: 'item',
            bars: {
                hall: {
                    index: 0.5,
                    title: '营业厅',
                    login: true,
                    enabled: hallEnable,
                    iframe: hallUrl.url + $.param(hallUrl.param)
                },
                cart: {
                    enabled: true
                },
                coupon: {
                    index: 1.5,
                    enabled: true,
                    title: '优惠券',
                    login: true,
                    iframe: '//cd.jd.com/coupons?' + $.param({
                        skuId: pageConfig.product.skuid,
                        cat: pageConfig.product.cat.join(','),
                        venderId: pageConfig.product.venderId
                    })
                },
                jimi: {
                    iframe: '//jimi.jd.com/index.action?productId='+ pageConfig.product.skuid +'&source=jdhome'
                }
            },
            links: {
                feedback: {
                    href: '//surveys.jd.com/index.php?r=survey/index/sid/323814/newtest/Y/lang/zh-Hans'
                },
                top:{ anchor:"#" }
            },
            ad: ad_entry
        });
    });
