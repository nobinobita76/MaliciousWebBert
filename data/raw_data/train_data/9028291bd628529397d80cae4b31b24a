var tid = '3888';
var fid = '17';
var mt;
var totalpage = parseInt('1');
var db_ajax = '0';
var page = parseInt('1');
var jurl = 'read.php?tid=3888&fpage=0&ds=1&toread=0&page=';
var db_htmifopen = '1';
var db_dir = '-htm-';
var db_ext = '.html';
var db_bbsurl = 'http://www.safe10000.com/bbs';
var copyurl = '大银行门禁联网统一管理系统|' + location.href;

var db_urlcheck = 'safe10000.com';

