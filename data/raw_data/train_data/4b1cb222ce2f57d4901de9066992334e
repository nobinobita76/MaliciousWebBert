                <script>
                    var isvote = false;
                    function VoteA(id) {
                        if (!isvote) {
                            isvote = true;
                            var s = document.createElement('script');
                            s.src = "/shouji/api/do?action=/vote/soft/&act=votea&id=" + id + "&callback=fillVote($data)";
                            document.body.appendChild(s);
                        }
                    }

                    function VoteB(id) {
                        if (!isvote) {
                            isvote = true;
                            var s = document.createElement('script');
                            s.src = "/shouji/api/do?action=/vote/soft/&act=voteb&id=" + id + "&callback=fillVote($data)";
                            document.body.appendChild(s);
                        }
                    }

                    function fillVote(data) {
                        var q = function (id) { return document.getElementById(id); }

                        var numaObj = q('box_votea');
                        var numbObj = q('box_voteb');
                        var scoreObj = q('box_score');


                        numaObj.innerHTML = '<em>(' + data.votea + ')</em>';
                        numbObj.innerHTML = '<em>(' + data.voteb + ')</em>';

                        var total = data.votea + data.voteb;

                        var prefix = '';
                        var suffix = '';
                        var sc = data.score.toString() + '.0';


                        var arr = sc.split('.');

                        prefix = arr[0];
                        suffix = arr[1];
                        scoreObj.innerHTML = '' + prefix + '.' + suffix + '';
                    }

                </script>




