function onLoginSuccess(userName){
   var currDate = new Date();
   $("#memberlogin").html('<div class="list"><ul><li>欢迎您！'+userName+'</li><li>当前时间是：'+currDate.getFullYear()+'年'+(currDate.getMonth()+1)+'月'+currDate.getDate()+'日    星期'+covertDigitToCn(currDate.getDay())+'</li><li>'+
		   '<input type="button" value="退&nbsp;&nbsp;出" class="submit" onclick=window.location="/log_out" /><a href="/member/index.action" class="underline">进入会员中心</a></div>');
   $("#welcomeTop").html('欢迎您！'+userName);
   $("#welcomeTop").css("width","300px");
   $("#memberCenter").css("display","inline-block");
   $("#loginHref").css("display","none");
   $("#memberMenuTop a").each(function(){
       $(this).attr("href","/member/index.action");
    });
   $("#isLogin").val("true");
}

document.onkeypress=function(e){
	var code;
	if(!e){
		var e=window.event;
	}
	if(e.keyCode){
		code = e.keyCode;
	}else if(e.which){
		code = e.which;
	}
	if(code==13){
		if(document.getElementById("loginTopDiv").style.display=='none'){
			doLogin();
		}
	}
}
