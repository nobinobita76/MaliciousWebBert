                var ebook_support_count = 5;
                if(iswidth == true){
                    if(ebook_support_count > 7){
                        document.write("<div class=\"btn_wrap\"><span id=\"more_support\" class=\"btn_out\" title=\"点击向下展开\"></span></div>");
                    }
                }else{
                    if(ebook_support_count > 7){
                        document.write("<div class=\"btn_wrap\"><span id=\"more_support\" class=\"btn_out\" title=\"点击向下展开\"></span></div>");
                    }
                }
                function more_support(){
                    $("#more_support").click(function(){
                        var text = $("#more_support").attr("title");
                        if(text == "点击向上收起"){
                            $(this).parents(".media").find(".media_list").animate({height:80 }, "slow");
                            $(this).removeClass("btn_in").attr({ "title": "点击向下展开" });
                        }else{
                            $(this).parents(".media").find(".media_list").animate({height:170},"slow");
                            $(this).addClass("btn_in").attr({ "title": "点击向上收起" });
                        }
