                {@if item.append_review.length>0}
                <div class="comment_add">
                    <div class="comment_add_line"><span>【${item.append_date >= 180 ? 180 : item.append_date}天后追评】</span>${item.append_review}
                    </div>
					{@if item.append_pics.length>0}
							 <ul class="pic_show">
                            {@each item.append_pics as img}
                            <li><a href="javascript:;"><img src="${img.small_pic_url}" big="${img.big_pic_url}" alt=""/>
                                </a><b class="pic_show_arrow"></b></li>
                            {@/each}
                        </ul>{@/if}
						<div class="pic_show_big" style="display: none">
                            <div class="big_pic_wrap">
                                <img class="big_pic load"/>
                                <a href="javascript:;" class="big_pic_close"></a>
                            </div>
