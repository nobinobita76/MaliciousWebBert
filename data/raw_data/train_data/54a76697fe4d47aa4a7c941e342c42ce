function preloadImages(a) {
  var d=document; if(d.images){ if(!d.p) d.p=new Array();
    var i,j=d.p.length; for(i=0; i<a.length; i++)
    if (a[i].indexOf("#")!=0){ d.p[j]=new Image; d.p[j++].src=a[i];}}
}
