var current_data = "sjk";
function switch_data(obj)
{
	var now_data = obj.className.replace("_active", "").replace("_normal", "").toString();
	if(current_data == now_data)return false;
	obj.className = now_data + "_active";
	document.getElementById(current_data+"_tab").className = current_data+"_normal";
	document.getElementById(current_data).style.display = "none";
	document.getElementById(now_data).style.display = "";
	current_data = now_data;
	obj.blur();
}
