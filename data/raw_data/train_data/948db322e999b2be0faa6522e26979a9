<script>
function changeCaptcha() {
	document.getElementById("imgv").src = "http://captcha.pconline.com.cn/captcha/v.jpg?" + new Date().getTime();
}
$(function(){changeCaptcha();});

var _ajxpurl='http://m.pconline.com.cn/ypcompany/message_login.jsp';
function doSubmitfrm(frm){
	if (checkForm(frm)){	
		if (!document.getElementById('_hidden').checked){
			if (!!document.cookie.match(/(^|; )common_session_id=[^;]+/)){return doAjxCheckCaptcha(); }
			if($("#_username").val()==''){
				alert("请输入用户、密码，或者选择匿名发表");
				$("#_username").focus(); 
				return false;
			}
			if($("#_password").val()==''){
				alert("请输入用户、密码，或者选择匿名发表");
				$("#_password").focus(); 
				return false;
			}
			var flagLogin = doAjxCheckLogon();
			var flagCaptcha = doAjxCheckCaptcha();
			return flagLogin&&flagCaptcha;		
	 	}else{
	 		return doAjxCheckCaptcha();;
	 	}	 
 	}else{
 		return false;
 	}	
}

function doAjxCheckLogon(){
	$.post(_ajxpurl, {
	    op:'cl',
		r:Math.random()
	}, function(responseText, statusText) {
		var shtml=$.trim(responseText);
		if(!$.trim(responseText)){
			return doLogon();
		}else{
			$("#_msgForm").submit();
	      	return true;
		}
 	})
	return false;
}

function doLogon(){
	$.ajax({
	   type: "POST",
	   url: _ajxpurl,
	   data: 'op=login&username='+ escape($("#_username").val())+'&password='+$("#_password").val()+'&r='+Math.random(),	   
	   success: function(responseText,statusText) {
		      	var shtml=$.trim(responseText);
		      	if ($.trim(responseText)){
		      		alert(shtml); 
		      		return false;
		      	}else{
		      		$("#_msgForm").submit();
	      			return true;
		      	}
		 	}
   	});  
}
</script>
<script>
var _ajxaCaptchaUrl='http://m.pconline.com.cn/ajax_check_captcha.jsp';
function doAjxCheckCaptcha(){
	var html = $.ajax({
		   type: "POST",
		   url: _ajxaCaptchaUrl,
		   data: 'captcha='+ escape($("#_captcha").val())+'&r='+Math.random(),	   
		   async: false
	   	}).responseText;
   	if(html.indexOf('fail')>-1){
   	   	alert('验证码错误,请输入正确验证码!');
   	 	changeCaptcha();
   		return false;
   	}
   	return true;
}
</script>


