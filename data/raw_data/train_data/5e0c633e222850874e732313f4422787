<script type="text/javascript">
$(document).ready(function(e) {
	var menutype = 'menu';
	$("#menu>ul>li").each(function(){
		if($(this).attr("type") == menutype){
			$(this).addClass("mdq");
		}else{
			$(this).removeClass("mdq");
		}
	});
    $("#menu>ul>li").click(function(){
		$(this).addClass("mdq").siblings().removeClass("mdq");
	});
});

	function showMessage(message){
		if(message == "" || message == "null"){
			return;
		}
		$("#showMessage").text(message);
		$("#pop_div").show();
		//$("#trans_div").width("100%").height("100%").css("bottom","0px");
		
		//定时执 行，5秒后执行hideMessage() 
		window.setTimeout(function(){ 
			hideMessage();
		},2000); 
		
	}
	
	function hideMessage() 
	{ 
		$("#pop_div").hide();
		//$("#trans_div").width("0%");
	} 
	
</script>
