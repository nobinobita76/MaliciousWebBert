  <div id="place"></div> 
  <script> 
/******************************
 *Desc   ：获取子菜单
 *creator: sunliang 
 *Date   : 20160223 
 ******************************/
$(function(){
  function PlaceSubMenu(){}
  PlaceSubMenu.prototype={
   init : function(data){
      var that = this;
      that.doAjaxGetSubMenu(data,that.doResponse); 
   },
   doAjaxGetSubMenu : function(data,callBack){
     var that = this;
     $.ajax({
			url: data.ajax.url,
			type: data.ajax.type || 'POST',
			dataType : data.ajax.dataType ||"json",
			data: data.ajax.reqParams,
			async:true,
			success:function(responseData){ 
			 callBack && callBack(data,responseData);
			},
			error:function(responseData){
			   console.log(responseData); 
			}
	  });
   },
   doResponse : function(data,responseData){
	   if(responseData){
		  var menuSize = Number(responseData.menuSize),
		      list = responseData.list,
			  tid = data.ajax.reqParams.tid,
			  arr = new Array();
		  arr.push("<strong>当前位置：</strong><a href='"+responseData.homeUrl+"'>首页</a>>");
		  if(menuSize == 0){
             arr.push("<a href='articleChancel.php?tid="+tid+"'>"+(list && list[0].name)+"</a>>");
		  }else if(menuSize > 0){
		    var meunArr = new Array();
		    var getCurrentMenu = function(pid){
			    $.each(list,function(){
				  if(this.id == pid){
				     meunArr.push("<a href='articleChancel.php?tid="+this.id+"'>"+this.name+"</a>>");
				     (this.pid != '0') && getCurrentMenu(this.pid);
				  } 
		        }); 
			};
		    getCurrentMenu(tid);
			arr.push(meunArr.reverse().join(""));
		  }
		  (menuSize >=0 && data.ajax.reqParams.id) && arr.push("<font>正文</font>");
		  $("#place").html("").append("<span>"+arr.join("")+"</span>");
	   }
   }
  }

  var subMenu  = new PlaceSubMenu(),
      data = {'ajax': {'url':'getSubMenu.php',
	                   'reqParams': {'pMethod':'subMenu',
					                 'tid': '1',
									 'id':'4043'
								    }
					  }
	         };
    subMenu.init(data);
	subMenu = null;
});  
</script> 
