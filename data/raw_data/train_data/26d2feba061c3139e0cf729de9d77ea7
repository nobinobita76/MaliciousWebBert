function delPingLog(fid, tid, pid, pingid) {
showDialog({type:'confirm',message:"ÄãÈ·¶¨ÒªÉ¾³ıÕâÌõÆÀ·Ö¶¯Ì¬Ã´£¿",okText:'È·ÈÏ',onOk:function(){
read.guide();
