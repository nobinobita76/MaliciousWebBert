function ajaxPingcp(href) {
ajax.send(href,'',function(){
//	console.log(ajax.runscript(ajax.request.responseText));
/pid=(\d+)/.exec(href);
var pid = RegExp.$1;
if(pid == 0) pid = 'tpc';
 getObj('pingtable' + pid).innerHTML = ajax.runscript(ajax.request.responseText);
});
return false;
}
