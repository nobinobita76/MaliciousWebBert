					$(".caidan").on("click", function(e){ 
							if($("table").is(":hidden")){ 
								$("table").show(); 
							}else{ 
			
								$("table").show(); 
							} 
							$(document).one("click", function(){ 
								$("table").hide(); 
								$(".menu").hide(); 
							});
							 e.stopPropagation(); 
						}); 
