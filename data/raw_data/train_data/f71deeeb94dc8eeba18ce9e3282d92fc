        <script type="text/javascript">
        document.addEventListener('DOMContentLoaded', function () {
            var mScripts = Array.prototype.map.call(document.querySelectorAll('mscript'), function (el) {
                        return el.getAttribute('src');
                    }),
                    toLoad = mScripts.length,
                    LOCAL_KEY_PRE = 'mscript_',
                    reScriptMeta = /^(.+)\?t=(\w+)$/,
                    localStorage = window.localStorage,
                    mobileConfig = window.mobileConfig,
                    isProd = mobileConfig && mobileConfig.isProd;
            mScripts.forEach(function (mScript, i) {
                var scriptMeta = mScript.match(reScriptMeta),
                        pathId = scriptMeta[1],
                        page = isProd ?  mobileConfig.pages[pathId] : null,
                        filePath = isProd ? page.path : pathId,
                        ver = isProd ? page.v : scriptMeta[2],
                        local = localStorage && localStorage.getItem(LOCAL_KEY_PRE + pathId),
                        localVer = local && local.substr(0, ver.length);
                if (local && localVer == ver) {
                    mScripts[i] = local.substr(ver.length);
                    lessToLoad();
                } else {
                    var oReq = new XMLHttpRequest();
                    oReq.addEventListener("load", function () {
                        mScripts[i] = this.responseText;
                        lessToLoad();
                        if (localStorage && (this.status == 200 || this.status == 304)) {
                            try {
                                localStorage.setItem(LOCAL_KEY_PRE + pathId,
                                        ver + mScripts[i]);
                            } catch (e) {}
                        }
                    });
                    oReq.open("GET", filePath + '?t=' + ver);
                    oReq.send();
                }
            });

            function lessToLoad() {
                toLoad--;
                if (toLoad < 1) {
                    var script = document.createElement('script');
                    script.innerHTML = mScripts.join(';');
                    document.body.appendChild(script);
                }
            }
        });
    </script>


