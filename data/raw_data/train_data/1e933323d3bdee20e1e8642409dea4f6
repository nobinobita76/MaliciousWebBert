function ad_window(config){
	this.obj=document.getElementById(config.id);
	this.tag=config.tag;
	this.wrap=document.getElementById('poptipWrap');
	this.box=document.getElementById('poptipBox');
	this.span=document.getElementById('poptipBox_span');
	this.loading=document.getElementById('poptiploading');
	this.startTime=null;
	this.endTime=null;
	this.height=document.body.offsetHeight;
	this.height1=document.documentElement.clientHeight;
};
ad_window.prototype={
	init : function(){
		var re=/^\./;
		var _this=this;
		if(this.tag.match(re)!=null){
			this.tag=this.tag.substr(1);
			this.tag=this.getClass(this.obj,this.tag);
		}else{
			this.tag=this.obj.getElementsByTagName(this.tag);
		};
		for(var i=0;i<this.tag.length;i++){
			if(_this.tag[i].childNodes[0].nodeType==3){
				_this.tag[i].onmouseover=function(e){
					var e=e || event;
					var x=e.clientX;
					var y=e.clientY;
					_this.showAd(this,x,y);
				};
				_this.tag[i].onmouseout=function(){
					_this.hideAd(this);
				}
			}
		};
		this.wrap.onmouseover=function(){
			clearTimeout(_this.endTime);
		};
		this.wrap.onmouseout=function(){
			_this.hideAd();
		};

	},
	getClass : function(o,n){
		var oTag=o.getElementsByTagName('*');
		var arr=[];
		for(var i=0;i<oTag.length;i++){
			var iTag=oTag[i].className.split(' ');
			for(var j=0;j<iTag.length;j++){
				if(iTag[j]==n){
					arr.push(oTag[i]);
				}
			}
		};
		return arr;
	},
	showAd : function(o,x,y){
		var theTop=document.documentElement.scrollTop || document.body.scrollTop;
		var toTop=this.getTop(o);
		var iTop=o.offsetTop;
		var _this=this;
		var theY=y;
		var theX=x;
		var theObj=o;
		var tid = o.getAttribute('name');	//取当前文本的自定义属性name    
		this.getClass(_this.wrap,'tit')[0].innerHTML='';		
		this.box.getElementsByTagName('p')[0].innerHTML='';
		this.loading.style.display='block';
		this.box.getElementsByTagName('p')[0].style.display='none';
		this.getClass(_this.wrap,'tit')[0].style.display='none';
		this.getClass(_this.wrap,'adaptive')[0].style.diaplay='none';
		$("#poptipWrap div[id^='div_newd0_js_']").css('display','none');
		var bodybox=document.getElementById('newsbody');
		clickP('229',tid,'');
		_this.wrap.style.display='none';
		clearTimeout(_this.startTime);
		clearTimeout(_this.endTime);
		$('#adaptive').hide();
		document.getElementById('adaptive_ul').innerHTML='';
		this.startTime=setTimeout(function(e){
			_this.wrap.style.display='block';
			_this.getPosition(theObj,theX,theY);
			$.ajax({
				url:'http://info.xcar.com.cn/ajax/wiki_interface.php',
				type:'GET',
				dataType:'jsonp',
				data:{'did':tid},
				jsonp:'callback',
				success:function(data){

					if(data.code==1){
						var bjtitle = o.innerHTML;	//标题
						var bjdes = data.des;		//描述
						var bjlink = data.link;		//链接
						var bjad = data.ad;
						var ad_id = data.ad_id;
						var chexi =data.wiki_che;
						_this.loading.style.display='none';
						_this.box.getElementsByTagName('p')[0].style.display='block';
						_this.getClass(_this.wrap,'tit')[0].style.display='block';
						_this.getClass(_this.wrap,'tit')[0].innerHTML='<span>'+bjtitle+'</span><em class="more"><a target="_blank" href="'+bjlink+'">查看词条</a></em>';
						_this.box.getElementsByTagName('p')[0].innerHTML=bjdes;

						if(bjad==2){
							$("#div_newd0_js_"+ad_id).css('display','block');

						}else{
							//非售卖有车系
							if(chexi== null &&  chexi==undefined){
								$('#adaptive').hide();
							}else{
								$('#adaptive').show();
								//填充车系内容
								for(var i=0;i<chexi.length;i++){
									document.getElementById('adaptive_ul').innerHTML+='<li><a target="_blank" href="http://newcar.xcar.com.cn/'+chexi[i]["pserid"]+'/" title="'+chexi[i]["psname"]+'" ><img width="88" height="65" src="http://img1.xcarimg.com/PicLib/s/s'+chexi[i]["serid"]+'_80.jpg"></a><a target="_blank" href="http://newcar.xcar.com.cn/'+chexi[i]["pserid"]+'/" title="'+chexi[i]["psname"]+'" >'+chexi[i]["psname"]+'</a></li>';
								}
							}
							$("#div_newd0_js_"+ad_id).css('display','none');
						}
						_this.getPosition(theObj,theX,theY);

					}
				}
			});

		},500);
	},
	hideAd : function(){
		var _this=this;
		clearTimeout(_this.startTime);
		clearTimeout(_this.endTime);
		this.endTime=setTimeout(function(){
			_this.wrap.style.display='none';
		},50)
	},
	getTop : function(obj){
		var result = 0;
		while(obj) {
			result += obj.offsetTop;
			obj = obj.offsetParent;
		}
		return result;
	},
	getLeft : function(obj){
		var result = 0;
		while(obj) {
			result += obj.offsetLeft;
			obj = obj.offsetParent;
		}
		return result;
	},
	getPosition : function(o,x,y){
		var bodybox=document.getElementById('newsbody');
		var theTop=document.documentElement.scrollTop || document.body.scrollTop;
		var toTop=this.getTop(o);
		var iTop=o.offsetTop;
		var _this=this;
		var toLeft=x-this.getLeft(this.obj);
		var theY=y;
		var theX=x;
		var theObj=o;
		if(toTop-theTop<=240){
			_this.wrap.removeAttribute("style");
			if(o.offsetHeight>30){
				_this.wrap.style.top=theY+theTop+10+'px';
			}else{
				_this.wrap.style.top=toTop+16+'px';
			}
			_this.box.className='poptip line2';
			_this.span.className='poptip-arrow poptip-arrow-top';
		}else{
			_this.wrap.removeAttribute("style");
			if(o.offsetHeight>30){
				_this.wrap.style.bottom=_this.height1-theTop-theY+10+'px';
			}else{
				_this.wrap.style.top=toTop-_this.wrap.offsetHeight+'px';
			}
			_this.box.className='poptip line1';
			_this.span.className='poptip-arrow poptip-arrow-bottom';
		};
		if(toLeft<100){
			_this.wrap.style.left=theX-34+'px'
			_this.span.style.left=30+'px';
		}
		else if(toLeft>=100 && toLeft<400){
			_this.wrap.style.left=theX-84+'px'
			_this.span.style.left=78+'px';
		}else{
			_this.wrap.style.left=(671-340)+_this.getLeft(bodybox)+'px';
			_this.span.style.left=theX-((671-340)+_this.getLeft(bodybox)+10)+'px';
		}
	}

};
window.onload=function(){
	var theAd=new ad_window({ id : 'newsbody' , tag : '.wiki'});
	theAd.init();
};
window.onresize=function(){
	var theAd=new ad_window({ id : 'newsbody' , tag : '.wiki'});
	theAd.init();
}


//热门车关注排行
$(function(){
	function newCar_tab(id){
		$('#'+id+' li').mouseover(function(){
			$('#'+id+' li').removeClass('hot_top_active').find('div .topdiv').hide();
			$('#'+id+' li').find('div .botmdiv').css("display","block");
			$(this).addClass('hot_top_active').find('div .botmdiv').hide();
			$(this).find('div .topdiv').css("display","block");
		})
	};
	newCar_tab('hot_top');

});




