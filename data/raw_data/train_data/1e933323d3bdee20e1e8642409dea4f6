$(function(){
	// 新增分享按钮
	$('#share_weixin').hover(function(){
		var $t = $('#w_new');
      	if ($(".weixinImg").attr("src")=="http://icon.xcar.com.cn/2011newcar/images/auto/ps_logo.jpg")
      	{
          	$(".weixinImg").attr("src","http://hezuo.xcar.com.cn/index.php?partner=qrcode&url=http%3A%2F%2Finfo.xcar.com.cn%2F201703%2Fnews_1970442_1.html");
      	}
		$t.show();
	},function(){
		$('#w_new').hide();
	});
	$('#w_new').hover(function(){
		$(this).show();
	},function(){
		$(this).hide();
	});

	//新增收藏里的微信
	$('div.whynewcon1').hover(function(){
		var $t = $('#w_new1');
      	if ($(".weixinImg").attr("src")=="http://icon.xcar.com.cn/2011newcar/images/auto/ps_logo.jpg")
      	{
          	$(".weixinImg").attr("src","http://hezuo.xcar.com.cn/index.php?partner=qrcode&url=http%3A%2F%2Finfo.xcar.com.cn%2F201703%2Fnews_1970442_1.html");
      	}
		$t.show();
	},function(){
		$('#w_new1').hide();
	});
	$('#w_new1').hover(function(){
		$(this).show();
	},function(){
		$(this).hide();
	});

	//微信

	$("div.whynewcon").hover(function(ev){
		var $t = $('#w_new0');
		var tpos=$(this).offset().top - $(document).scrollTop(),
		bpos=$(window).height()-tpos;
        if ($(".weixinImg").attr("src")=="http://icon.xcar.com.cn/2011newcar/images/auto/ps_logo.jpg")
        {
            $(".weixinImg").attr("src","http://hezuo.xcar.com.cn/index.php?partner=qrcode&url=http%3A%2F%2Finfo.xcar.com.cn%2F201703%2Fnews_1970442_1.html");
        }
		$t.show();
		if(tpos > 321 || tpos> bpos ) {
			$t.removeClass('w_newTop').addClass('w_newBottom');
		}else{
			$t.removeClass('w_newBottom').addClass('w_newTop');
		}

		ev.stopPropagation();
	},function(){
		$('#w_new0').hide();
	});
	$('#w_new0').hover(function(){
		$(this).show();
	},function(){
		$(this).hide();
	});


	$(document).click(function(ev){
		$('#w_new0').hide();
		img_comment.hiddenbox();
		updateCommentIfrsrc();//更新评论iframe地址
	});


	$(".whyclose a").click(function(){
		$(this).closest(".w_new").hide();
	});

	$('div.whyknow > a').click(weixin_url);
	$('.bds_weixin')[0].onclick = weixin_url;
	function weixin_url(){
		var url='http://hezuo.xcar.com.cn/index.php?partner=weixin_share';
		url+='&url='+encodeURIComponent(location.href);
		url+='&title='+encodeURIComponent(document.title);
		url+='&zoneclick='+this.id.substr('zoneclickid_'.length);
		if(this.tagName.toLowerCase() === 'a') {
			url+='#user_guide';
		}
		window.open(url);
		return false;
	}


	$(".commentpic_btn").click(function(){
		//get_img_comment();
	// get_img_commentNum();
	});
});
//============================================点击收藏结束======================================//

</script>

