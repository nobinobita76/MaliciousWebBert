<script>
$(function(){
	$(".new17_close").click(function(){
		if($(this).hasClass('setnickname')){
			$.ajax({
				dataType : 'json',
				url  : '/foreuser/setnickname/',
			});
		}
		$(".new17_pop").hide();
		$(".new17_tc_mb").hide();
	});
	
	})
	$('.login_button').live('click',function(){
		$(".new17_pop").hide();
		$('#tc_login').show();
		$(".new17_tc_mb").show();
		changfy();
	});
	$('.register_button').live('click',function(){
		$(".new17_pop").hide();
		$('#tc_register').show();
		$(".new17_tc_mb").show();
		changmsgpic();
	});
    var pe = '1';
    var demo = $("#form_register").Validform({
        btnSubmit:".submit_register", 
        showAllError:true,
        ajaxPost:true,
        tiptype:function(msg,o,cssctl){
            if(o.type==2){
                $('#J_'+o.obj.context.name+'Tip').html('');
            }else if(o.type==3){
                if(o.obj.context.value==''){
                    $('#J_'+o.obj.context.name+'Tip').html(msg);
                }else{
                    $('#J_'+o.obj.context.name+'Tip').html(msg);
                    if(pe!='1'){
                    changmsgpic(); 
                    }
                }
            } 
        },
        callback:function(data){
            if(data.status=='y'){
                $.ajax({
                    type : 'POST',
                    dataType : 'json',
                    url  : '/foreuser/getuinfotest/',
                    data : {data:data.loginkey,url_home:url},
                    success:function(result){
                        if(result.status == 'y'){
							$(".new17_pop").hide();
							$(".new17_tc_mb").hide();
							$("#login_block").css('display', '');
							$("#nickname_flag").html(result.nickname);
							$("#loginout_block").css('display', 'none');
							login_flag = true;
							if(result.headpic){
								$("#eUserpic img").attr('src',result.headpic);
								$("#header_pic").attr('src',result.headpic);
							}
                        }
                    }
                });
            }else{
                $('#J_usernameTip').html(data.info);
                if(pe!='1'){
                    changmsgpic();
                }
                return false;
            }                        
        }
	});
	
	demo.addRule([{
        ele:'input[name="nickname"]',
        datatype:"/^[0-9a-zA-Z\u4e00-\u9fa5_]{2,10}$/",
        ajaxurl:"/foreusertest/isnickname/type/10/?callback=?",
        nullmsg:"请输入昵称",
        validform_valid:"false",
        errormsg:"昵称不正确！"
	},
    {
        ele:'#re_username',
        datatype:"m",
        ajaxurl:"/foreusertest/existence/exis/2/type/10/?callback=?",
        nullmsg:"请输入手机",
        validform_valid:"false",
        errormsg:"账号格式不正确！"
	},
	{
        ele:'#re_password',
        datatype:"*6-16",
        nullmsg:"请输入密码",
        errormsg:"密码应该为6-20位之间！"
	},
	{
        ele:'re_repassword',
        datatype:"*6-16",
        recheck:"password",
        nullmsg:"请再次输入密码！",
        errormsg:"两次密码不一致！"
	},
	{
        ele:'input[name="verify"]',
        datatype:"n5-6",
        nullmsg:"请输入验证码",
        errormsg:"验证码不正确！"
	}]);

    var isess = 0;
    var dldemo = $(".form_login").Validform({
        btnSubmit:".submit_login", 
        showAllError:true,
        ajaxPost:true,
        tiptype:function(msg,o,cssctl){
            if(o.type==2){
                $('#J_dl'+o.obj.context.name+'Tip').html('');
            }else if(o.type==3){
                isess++;
                if(isess>2){
                    $("#loginyzm").show();
                    $("#isecode").val(isess);
					dldemo.addRule([{
						ele:'input[name="secode"]',
						datatype:"n5-5",
						nullmsg:"请输入验证码！",
						errormsg:"验证码不正确！"
					}]);
                }                        
                if(o.obj.context.value==''){
                    $('#J_dl'+o.obj.context.name+'Tip').html(msg);
                }else{
                    $('#J_dl'+o.obj.context.name+'Tip').html(msg);
                }
            } 
        },
		callback:function(res){
            if(res.status=='y'){
                $.ajax({
                    type : 'POST',
                    dataType : 'json',
                    url  : '/foreuser/getuinfotest/',
                    data : {data:res.loginkey,url_home:url},
                    success:function(result){
                        if(result.status == 'y'){
							$(".new17_pop").hide();
							$(".new17_tc_mb").hide();
                            $("#login_block").css('display', '');
							$("#nickname_flag").html(result.nickname);
							$("#loginout_block").css('display', 'none');
															// GetAjaxShareInfo();
                                GetgetTodayVideo();//新首页
														login_flag = true;
							if(result.headpic){
								$("#eUserpic img").attr('src',result.headpic);
								$("#header_pic").attr('src',result.headpic);
							}
                                                    }
                    }
                });
            }else{
                changfy();
                $('#J_dlusernameTip').html(res.info); 
                return false;
            }                        
        }
	});

	dldemo.addRule([{
            ele:'input[name="username"]',
            datatype:"m",
            nullmsg:"请输入手机号！",
            errormsg:"账号格式不正确！"
        },
        {
            ele:'input[name="password"]',
            datatype:"*6-16",
            nullmsg:"请输入密码！",
            errormsg:"密码应该为6-20位之间！"
        },
    ]);

    $("#J_phone").click(function(){
		var nick = $('input[name="nickname"]').val();
        if(!nick){
            $('#J_nicknameTip').html('请输入昵称');
            return false;
        }
		
        var user = $('#re_username').val();
        if(user.length<11){
            $('#J_usernameTip').html('请输入手机号');
            return false;
        }
    
        var msgpic = $('#msgpicverify').val();
        if(msgpic.length!=5){
            $('#J_verifymsg').html('请输入验证码');
            return false;
        }
        $.ajax({
            type : 'POST',
            cache : false,
            dataType : 'jsonp',
            url  : "/send_message/setphonezc/type/10/",
            data : {user:user,msgpic:msgpic},
            success:function(result){
                if(result.recode!=2){
                     $('#J_verifyTip').html(result.mag); 
                }else{
                    $('.login_tip1').html('');
                    phoneTimes(60);
                    $("#J_phone").hide();
                    $('#J_yzm3').show();                        
                }
            }
        });          
    });
	
    function phoneTimes(miao){
        miao--;
        $('#J_yzm3').html(miao+'秒后重新获得');
        if(miao>0){
            setTimeout("phoneTimes("+miao+")", 1000);
        }else{
            $('#J_phone').show();
            $('#J_yzm3').hide();
        }
    }
    		
function changmsgpic(){
    var url = "/send_message/seccode/?"+Math.random();
    $('#msgpic').attr('src',url);
}
function changfy(){
    var url = "/foreusertest/seccode/?"+Math.random();
    $('#verifyimg_dl').attr('src',url);
}
