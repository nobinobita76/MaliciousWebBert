	$(".zg_hoed dl").hover(function(){
		$(this).find('p.bot a').addClass('a_on');
	},function(){
		$(this).find('p.bot a').removeClass('a_on');
	});



	var Lwid=$('#yq_banner_list').width();
		$('.zg_17outsi').css('width',Lwid+36)
	})
</script>
