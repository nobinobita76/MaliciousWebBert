	var showarea = $.cookie('showarea');
	if(!areaId){
		$(".zg17_fix,.zg17_fix_bj").show();
	}
	$('.showarea').html(showarea);
    $('.showarea').attr('classData',areaId);
    GetAjaxShareInfo();
    GetFreeCourse();
    GetVipCourse();
