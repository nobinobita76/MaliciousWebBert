<script type="text/javascript">
        function switchGridViewIntrospection(gridview)
        {
            
            document.getElementById("ti_div1").style.display="none";
            document.getElementById("ti_div2").style.display="none";
            document.getElementById("ti_div3").style.display="none";
            document.getElementById("ti_div4").style.display="none";
//            document.getElementById("div5").style.display="none";
            document.getElementById("ti_div"+gridview).style.display="";
//            document.getElementById("ti_t1").className="link7";
//            document.getElementById("ti_t2").className="link7";
//            document.getElementById("ti_t3").className="link7";
//            document.getElementById("ti_t4").className="link7";
//            document.getElementById("t5").className="link7";
//            document.getElementById("ti_t"+gridview).className="txt6";
            document.getElementById("ti_t1").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
            document.getElementById("ti_t2").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
            document.getElementById("ti_t3").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
            document.getElementById("ti_t4").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
//            document.getElementById("t5").background="../images/nav_bg03.gif";
            document.getElementById("ti_t"+gridview).background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg042.gif";   
            
             document.getElementById("ti_t1").innerHTML="<a>最新</a>";
            document.getElementById("ti_t2").innerHTML="<a>热门</a>";
            document.getElementById("ti_t3").innerHTML="<a>热评</a>";
            document.getElementById("ti_t4").innerHTML="<a>班级推荐</a>";
            if(gridview=="1")
            {
                document.getElementById("ti_t1").innerHTML="最新";
            }
            else if(gridview=="2")
            {
                document.getElementById("ti_t2").innerHTML="热门";
            }
            else if(gridview=="3")
            {
                document.getElementById("ti_t3").innerHTML="热评";
            }
            else if(gridview="4")
            {
                document.getElementById("ti_t4").innerHTML="班级推荐";
            }
            var url=document.getElementById("more_userIntrospection").href;
            url= url.substring(url.indexOf("&Level"),url.length);
            document.getElementById("more_userIntrospection").href="../IndexPageMore/TeachingIntrospectionMore.aspx?type="+gridview+url;

                     
        }

</script>
