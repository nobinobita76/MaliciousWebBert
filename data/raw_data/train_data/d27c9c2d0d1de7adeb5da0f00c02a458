<script type="text/javascript">
function switchHomework(gridview,ht,_type,_level,_id)
        {  
            document.getElementById("divNewHomework").style.display="none";
            document.getElementById("divHotHomework").style.display="none"; 
            document.getElementById("divBetterHomework").style.display="none";    
            document.getElementById("divHomework").style.display="none";        
            document.getElementById(gridview).style.display="";
    
            var pageName = "HomeWorkStudentMore";
            if(ht=="ht1")
            {
                document.getElementById("ht4").innerHTML="<a>作业布置</a>";  
                document.getElementById("ht1").innerHTML="最新";
                document.getElementById("ht2").innerHTML="<a>热评</a>";
                document.getElementById("ht3").innerHTML="<a>班级推荐</a>";  
            }
            else if(ht=="ht2")
            {
                document.getElementById("ht1").innerHTML="<a>最新</a>";
                document.getElementById("ht2").innerHTML="热评";
                document.getElementById("ht3").innerHTML="<a>班级推荐</a>"; 
                document.getElementById("ht4").innerHTML="<a>作业布置</a>";   
            }
            else if(ht=="ht3")
            {
                document.getElementById("ht1").innerHTML="<a>最新</a>";
                document.getElementById("ht2").innerHTML="<a>热评</a>";
                document.getElementById("ht3").innerHTML="班级推荐";
                document.getElementById("ht4").innerHTML="<a>作业布置</a>";  
            }
            else 
            {
                document.getElementById("ht1").innerHTML="<a>最新</a>";
                document.getElementById("ht2").innerHTML="<a>热评</a>";
                document.getElementById("ht3").innerHTML="<a>班级推荐</a>";
                document.getElementById("ht4").innerHTML="作业布置";
                
                pageName = "HomeWorkIndexMore";
            }
            
            document.getElementById("ht1").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
            document.getElementById("ht2").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
            document.getElementById("ht3").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
            document.getElementById("ht4").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
            
            document.getElementById(ht).background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg042.gif";
            document.getElementById("more_homework").href="../IndexPageMore/"+pageName+".aspx?type="+_type+"&Level="+_level+"&ID="+_id;
                        
        }
</script>

