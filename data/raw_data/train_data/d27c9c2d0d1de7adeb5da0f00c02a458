<script type="text/javascript">
function switchTrainingActivity(gridview,ht,_type,_level,_id)
        {  
            document.getElementById("divNewTrainingActivity").style.display="none";
            document.getElementById("divHotTrainingActivity").style.display="none"; 
            document.getElementById("divCommentTrainingActivity").style.display="none"; 
            //document.getElementById("divBetterTrainingActivity").style.display="none";   
            document.getElementById("divRecommededTrainingActivity").style.display="none";  
            document.getElementById("divRequestTrainingActivity").style.display="none";  
            
                    
            document.getElementById(gridview).style.display="";
            
            document.getElementById("hht1").className="link7";
            document.getElementById("hht2").className="link7";
            document.getElementById("hht3").className="link7";
            //document.getElementById("hht4").className="link7";
            document.getElementById("hht5").className="link7";
            document.getElementById("hht6").className="link7";
            
          
            document.getElementById("hht1").innerHTML="<a>"+document.getElementById("hht1").innerText+"</a>";
            document.getElementById("hht2").innerHTML="<a>"+document.getElementById("hht2").innerText+"</a>";
            document.getElementById("hht3").innerHTML="<a>"+document.getElementById("hht3").innerText+"</a>";  
           // document.getElementById("hht4").innerHTML="<a>"+document.getElementById("hht4").innerText+"</a>";  
            document.getElementById("hht5").innerHTML="<a>"+document.getElementById("hht5").innerText+"</a>";  
            document.getElementById("hht6").innerHTML="<a>"+document.getElementById("hht6").innerText+"</a>";  
            
                
            document.getElementById(ht).innerHTML=document.getElementById(ht).innerText; 
              
            document.getElementById("hht1").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
            document.getElementById("hht2").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
            document.getElementById("hht3").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
           // document.getElementById("hht4").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";   
            document.getElementById("hht5").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif"; 
            document.getElementById("hht6").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";           
            

            document.getElementById(gridview).style.display="";
            document.getElementById("hht1").className="link7";
            document.getElementById("hht2").className="link7";
            document.getElementById("hht3").className="link7";
            //document.getElementById("hht4").className="link7";
            document.getElementById("hht5").className="link7";
            document.getElementById("hht6").className="link7";
            
            
            document.getElementById("hht1").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
            document.getElementById("hht2").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
            document.getElementById("hht3").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
            //document.getElementById("hht4").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
            document.getElementById("hht5").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
            document.getElementById("hht6").background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg032.gif";
            document.getElementById(ht).background="http://images1.teacher.com.cn/zhengshuty/images/nav_bg042.gif";
            
            var pageName="TrainingActivityStudentMore";
            if(ht=="hht6")
            {
                pageName="TrainActivityIndex";
            }
            document.getElementById("more_href").href="../IndexPageMore/"+pageName+".aspx?type="+_type+"&Level="+_level+"&ID="+_id;
        }
</script>

