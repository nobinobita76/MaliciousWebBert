<!--所有页面用到的js脚本都必须放到此位置，包括外链js和内嵌js-->
<script src="http://js.3conline.com/min/temp/v1/lib-jquery1.4.2.js"></script>
<script type="text/javascript">
;(function(){
$.fn.scrollFixed = function(o){
return this.each(function(){
var c,offsetT,offsetH,scrollT,pos,top,left,mT;
c = $(this);
offsetT = c.offset().top;
offsetH = c.outerHeight(true);
pos = c.css('position');
top = c.css('top');
left = c.css('left');
mT = parseInt(c.css('margin-top'));
c.wrap('<div></div>').parent().css('height',offsetH);//固定位置，避免页面抖动
$(window).bind("scroll resize",function(){
scrollT = $(document).scrollTop();
if(scrollT>=offsetT){
c[0].style.cssText = 'position:fixed;_position:absolute;top:'+(-mT)+'px;_top:'+(scrollT-parseInt(mT))+'px;width:'+c.width()+'px;z-index:100';
}else{
c.css({
position:pos,
top:top,
left:left
});
}
});
});
}
})(jQuery)
$(function(){
//滚动到一定高度，层固定在顶部
$('.scroll-fixed').scrollFixed();
})
</script>
