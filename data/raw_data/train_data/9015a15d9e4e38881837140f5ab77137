									document.write('<img style=\"display:none;\" src=\"http://tracker.tgbus.com/count/?type=' + STYPE + '&id=' + IDENTIFY + '\" />');
								</script><img style="display:none;" src="http://tracker.tgbus.com/count/?type=olgame&amp;id=1255018">
								</center></div>
