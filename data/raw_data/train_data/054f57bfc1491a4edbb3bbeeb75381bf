    document.onkeydown=function(){
      if (event.keyCode == 13){
        addSelect();
      }
    }
    function addSelect(){
            var region =  $('#add-region').val();
            if(region == ''){
                return false;
            }
                        var myurl = '/sell/index'+'/location/'+region+'.html';            window.location.href=myurl;
    }

