		// 返回顶部
		K.scrollTop=function(){
			var scrollTop=0;
			K("scrollTop").bind("click",function(){window.scrollTo(0,0)});
			window.onscroll=function(){
				scrollTop=document.documentElement.scrollTop||document.body.scrollTop;
				if(scrollTop<580){K("sideTool").css({display:"none"});}else{K("sideTool").css({display:"block"});}
				if(scrollTop<200){K("scrollTop").css({display:"none"});}else{K("scrollTop").css({display:"block"});}
			}
			if(K.B.ie6){
				K("scrollTop").css("position:absolute;");
				window.onscroll = function (){      scrollTop=document.documentElement.scrollTop||document.body.scrollTop;
					if(scrollTop<580){K("sideTool").css({display:"none"});}else{K("sideTool").css({display:"block"});}
					if(scrollTop<200){K("scrollTop").css({display:"none"});}else{K("scrollTop").css({display:"block"});}
					K("scrollTop").node.className=K("scrollTop").node.className; //ie6 scroll bug
				}
			}
		};
		K.scrollTop();
	})

