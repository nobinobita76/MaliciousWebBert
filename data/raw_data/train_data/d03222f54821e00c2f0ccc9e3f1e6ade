<script>
	var coopBtn = false;
	$(".status_btn").on("click",function(){
		if(!coopBtn){
			$(".more_coop").show();
			$("#area12,#area13").show();
			coopBtn = true;
		}else{
			$(".more_coop").hide();
			$("#area12,#area13").hide();
			coopBtn = false;
		}
	});
</script>


