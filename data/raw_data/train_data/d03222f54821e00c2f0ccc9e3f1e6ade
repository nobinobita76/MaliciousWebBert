    if(!l_t || (n_t - l_t > 1000*3600 )) {
      crystal.cookie.set("dsp_cookiemapping1",(new Date).getTime(),{expires:0.04,path:"/"});
      var img = new Image(1,1);
      img.src = "http://cm.l.qq.com/?Bid=9d973ca36d55db8598045165535d64de";
      refs.push(img);
    }
  })();
</script>
<script type="text/javascript">
  (function() {
    var ratio = 30;
    if(Math.random() * 100 >= ratio) return;
    var l_t = crystal.cookie.get("dsp_cookiemapping2");
    var n_t = (new Date).getTime();
    var refs = [];
    if(!l_t || (n_t - l_t > 1000*3600 )) {
