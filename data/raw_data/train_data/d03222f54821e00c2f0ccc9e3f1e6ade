<script>
    $.ajax({
        url: "http://data.v.qq.com/videocms/getNewsvideoList.php?ref=pclient&appkey=6UkwV9DeHr9_PC&vsite=tv&ename=tv_global_after_play",
        type: "get",
        dataType: "jsonp"
    }).done(function(e) {
        var data = e.data.content,
            t = "",
            tag = "";
        for(var i = 0;i<6;i++){
tag = "";
            if(i%2==0){
                t += '<li class="emli3 emlilt">';
            }else{
                t += '<li class="emli3 emlirt">';
            }
            if(data[i].corner){
                tag = '<a class="emtxtabs">'+data[i].corner.tag_3.text+'</a>';
            }
            t += '<a target="_blank" href="'+data[i].link_addr+'"><img src="'+data[i].pic_url+'"></a>'+tag+'<a class="emtxt" target="_blank" href="'+data[i].link_addr+'"><div style="height:30px;overflow:hidden;">'+data[i].title+'</div><span>'+data[i].subtitle+'</span></a></li>';
        }
        $(".tvrb ul").html(t);
    });
