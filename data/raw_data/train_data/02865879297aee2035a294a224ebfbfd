<script type="text/javascript">
    window.staticpath = "http://imc.aiyuke.com/";
    window.webSocketHost = "http://ws.tiyushe.com/";
    window.VideoLiveInfoid = '507';
    //sso登录成功的回调
    window.addonSSOLoggedCallback = function(){
        $('#js-isLogin').val(1);    //刷新评论框的状态

        setTimeout(function(){
            avalon.vmodels['live-chat'].isLogin = true
        },1000);
    };
    // 播放路线
    window.playlist = [{"title":"\u5148\u950b\u4e52\u7fbd","code":"<iframe src=\"http:\/\/www.aiyuke.com\/bztemplate\/zhibo\/zijianzhibo\/letv-live.html?activityId=A2016111400001w4\" frameborder=\"0\" width=\"860\" height=\"500\"><\/iframe>"}];
    var liveTimeStart = '1488187800';
    var liveTimeEnd = '1861977540';
    $(function(){
        var curHref = window.location.href;
        var routeNum = 0;
        var curTime= new Date().getTime();
        if (parseInt(curHref.substr(-2)) >= 10) {
            routeNum = parseInt(curHref.substr(-2));
        } else {
            routeNum = parseInt(curHref.substr(-1));
        }
        if (isNaN(routeNum)) routeNum = 0;
        $("a[id^='route']").attr("class", "g-btn gray");
        $("#route_"+routeNum).attr("class", "g-btn default cur");
        if (curTime < parseInt(liveTimeStart)*1000) {
            //还未开始
            $("#liveStatus").append("<span style='color: blue;'>(还未开始)</span>");
        } else if (curTime > parseInt(liveTimeEnd)*1000) {
            //已结束
            $("#liveStatus").append("<span style='color: gray;'>(已结束)</span>");
        } else {
            //正在进行中
            $("#liveStatus").append("<span style='color: darkred;'>(正在进行中)</span>");
        }
    })
</script>


