<script>
$(document).ready(function(){
	//电梯导航
	var win_H = $(window).height();
	$(".sy_rfloat").stop().fadeIn(1500);
	$(window).scroll(function(){
		var scroll_H = $(window).scrollTop();
		if(scroll_H>=250){
			$(".sy_rfloat").stop().animate({top:(win_H-462)/2},500);
		}else if(scroll_H<250){
			$(".sy_rfloat").stop().animate({top:117},500);
		}
	});
	$(".rfloat_sub.rfloat_sub6").click(function(){
		$('body,html').animate({scrollTop:0},500); 
	});
	//收回、展开效果
	$(".rfloat_sub7").click(function(){
		$(".rfloat_sub1,.rfloat_sub2,.rfloat_sub3,.rfloat_sub4,.rfloat_sub5,.rfloat_sub6,.rfloat_sub7").animate({top:389},500);
		$(".rfloat_sub8_bg").animate({top:0},500);
		$(".rfloat_sub8").animate({top:389},500);
	});
	$(".rfloat_sub8").click(function(){
		$(".rfloat_sub1").animate({top:0},500);
		$(".rfloat_sub2").animate({top:71},500);
		$(".rfloat_sub3").animate({top:142},500);
		$(".rfloat_sub4").animate({top:213},500);
		$(".rfloat_sub5").animate({top:284},500);
		$(".rfloat_sub6").animate({top:355},500);
		$(".rfloat_sub7").animate({top:427},500);
		$(".rfloat_sub8_bg").animate({top:-73},500);
		$(".rfloat_sub8").animate({top:462},500);
	});
});

//其他调用
function openWin(new_bmfw_url){
	var webid=$('#ss_webid').length==0?'0':+$('#ss_webid').text();
	new_bmfw_url=new_bmfw_url+'?webId='+webid;
	window.location.href=new_bmfw_url;
}	
function openNewWin(new_bmfw_url){
	var webid=$('#ss_webid').length==0?'0':+$('#ss_webid').text();
	new_bmfw_url=new_bmfw_url+'?webId='+webid;
	window.open(new_bmfw_url);
}
</script>

