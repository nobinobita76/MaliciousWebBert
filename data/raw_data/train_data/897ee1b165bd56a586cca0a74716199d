	    	jQuery.downloadAppPlugIn(downloadAppPlugIn); 
        }
		

		var closeCallbackFunc = function() {
			
		};
		
		var prvLink=document.referrer;
		if(typeof(prvLink) == "undefined"||prvLink==''){
			$("#goBack").attr("href","/column/photo/");
		}else{
			$("#goBack").attr("href","javascript:window.history.go(-1);");
		}

		
		$(".close").bind("click", function(){
			$(".advice").attr("style","display:none;");
		})
		
		var _articleId = $("input[name='_articleId']").val();
		
		//详情页收藏功能
		var flgFavor = false;
		if (getCookie('article_favors') != null){
			if (getCookie('article_favors').indexOf('*') != -1){
				var idArr = getCookie('article_favors').split('*');
				if ($.inArray(_articleId, idArr) != -1){
					flgFavor = true;
				}
			}else{
				if (getCookie('article_favors') == _articleId){
					flgFavor = true;
				}
			}
		}

	   //处理刷新
	   if(flgFavor){
		$("#addFavorBtn").addClass('new-zan new-cang new-cang-click');
		$("#addFavorBtn").find('span').eq(1).html('已收藏');
		$("#addFavorBtn").attr('disabled',"true");
	   }
		
		$("#addFavorBtn").click(function(){
			if(!flgFavor){
				var _articleId = $("input[name='_articleId']").val();
				var _userId = $("input[name='_userId']").val();
				var _articleTitle = $("input[name='_articleTitle']").val();
				var _articleDesc = $("input[name='_articleDesc']").val();
				var _channelId = $("input[name='_channelId']").val();
				var _icon = $("input[name='_icon']").val();
				if(_userId != ''){
					$.ajax({
						type: 'POST',
						url: 'addCollection.php',
						data: 'uid='+_userId+'&id='+_articleId+'&title='+_articleTitle+'&description='+_articleDesc+'&channelId='+_channelId+'&icon='+_icon+'&time='+new Date().getTime(),
						success: function(resp){
							if(resp.code == 103){

								if (getCookie('article_favors') != null){
									var idArr =getCookie('article_favors').split('*');
									idArr.push(_articleId);
									addCookie('article_favors', idArr.join('*'),365);
								}else{
									addCookie('article_favors', _articleId,365);
								}
								
								$("#addFavorBtn").addClass('new-cang-click');
								$("#addFavorBtn").find('span').eq(1).html('已收藏');
							}else if(resp.code == 104){
								$("#addFavorBtn").removeClass('new-cang-click');
								alert('网络忙，请稍后再试');
							}else if(resp.code == 106){
								$("#addFavorBtn").addClass('new-cang-click');
								$("#addFavorBtn").find('span').eq(1).html('已收藏');
							}
						},
						dataType: 'json'
					 })
				}else{
					location.href = 'http://m.ebrun.com/about/land.php';
				}
			}
		});

	    //点赞功能开始
	    var loveCount=0;


	    getLoveCount();
	    //获取文章点赞数量
	    function getLoveCount(){
	    	jQuery.getJSON( '/wap_loveCount.php','id=196040'+'&timeline='+new Date().getTime(),function (jsondata) {
	    		var loveCntStr = '';
	    		if(jsondata.loveCount>999){
	    		   loveCntStr = '999+';
	    		}else{
	    		   loveCntStr = ''+jsondata.loveCount;
	    		}
	    		loveCount = jsondata.loveCount;
	    		if (loveCount > 0){
	    			$(".mark").append('赞<mark class="mar_ri0"><label class="loveCount" style="color:#E2625A">'+loveCntStr+'</label></mark>');

	    			$("#zanBtn").find('span').eq(1).html(loveCntStr);
	    		}
	       }); 
	    }

	    
		//获取缓存
	    var flg = false;
	    if (getCookie('article_loves') != null){
	    	if (getCookie('article_loves').indexOf('*') != -1){
	    		var idArr = getCookie('article_loves').split('*');
	    		if ($.inArray('196040', idArr) != -1){
	    			flg = true;
	    		}
	    	}else{
	    		if (getCookie('article_loves') == '196040'){
	    			flg = true;
	    		}
	    	}
	    }
	    //处理刷新
	    if(flg){
	      $("#zanBtn").removeClass().addClass('new-zan new-zan-click');
	      $("#zanBtn").attr('disabled',"true");
	      //$("#zanBtn").find('span').eq(1).html('已赞');
	    }
	    //点赞功能
	    function zanfinction(ele)
	    {
	    	if (zanable == false)return;
	    	if (!flg){
	    		zanable=false;
	    		jQuery.getJSON( '/wap_love.php','ac=1&id=196040',function (data) {
	               if (data.code == 1){
	    				if (getCookie('article_loves') != null){
	    					var idArr =getCookie('article_loves').split('*');
	    					idArr.push('196040');
	    					addCookie('article_loves', idArr.join('*'),365);
	    				}else{
	    					addCookie('article_loves', '196040',365);
	    				}
	    				if(ele==2)
	    				{
	    					$("#zanBtn").removeClass().addClass('new-zan new-zan-click');
	    					$("#zanBtn").attr('disabled',"true");
	    				}
	    				flg = true;//禁止再次点击
	    				var loveCntStr = '';
	    				if((parseInt(loveCount)+1)>999){
	    					//$("#zanBtn").find('span').eq(1).html('已赞');
	    					$("#zanBtn").find('span').eq(1).html('999+');
	    					loveCntStr = "999+";
	    				}else{
	    					//$("#zanBtn").find('span').eq(1).html('已赞');
	    					$("#zanBtn").find('span').eq(1).html((parseInt(loveCount)+1));
	    					loveCntStr = ''+(parseInt(loveCount)+1);
	    				}
	    				if (loveCount > 0){
	    					$(".loveCount").html(loveCntStr);
	    				}else{
	    					$(".mark").append('赞<mark class="mar_ri0"><label class="loveCount" style="color:#E2625A">'+loveCntStr+'</label></mark>');
	    				}
	    			}else if (data.code == 2){
	    				alert('网络繁忙，请稍后重试！');
	    				zanable = true;
	    			}
	           }); 
	    	}
	    }
	    var zanable = true;

	  //赞点击事件点赞功能
	    $("#zanBtn").click(function(){
	    	zanfinction(2);
	    });
	    
	    showCommentCnt();
	    //点赞功能结束
	});

	//获取评论数
    function showCommentCnt(){
    	var commentCntStr = $("#changyan_count_unit").html();
    	if (commentCntStr == ''){
    		setTimeout('showCommentCnt()', 100);
    		return;
    	}
    	if (commentCntStr != '0'){
    		$(".mark").prepend('评论<mark style="color:#E2625A">'+commentCntStr+'</mark>');
    	}
    }
	
	if($(".advice_bottom img").length != 0){ //ad bottom
		$(".advice_bottom").attr({style:"margin:0 0 20px 0"});
		$(".advice_bottom img").attr({width:"100%",height:""});
	}

	$('.menu-new-home-tan').on('click',function(){
        if($(this).hasClass('revision_select_bottomt')){
            $(this).removeClass('revision_select_bottomt');
            $('.menu-wang').slideUp();
            $('.menu-fixed').hide()
        }else{
            $(this).addClass('revision_select_bottomt');
            $('.menu-wang').slideDown();
            $('.menu-fixed').show()
        }
    })

    $('.menu-fixed').on('click',function(){
        $(this).removeClass('revision_select_bottomt');
        $('.menu-wang').slideUp();
        $('.menu-fixed').hide()
    })
    
    $('.where-share').on('click',function(){
        $(this).hide()
    })

    $('.new-share-more-tan').on('click',function(){
        $('.where-share').show()
    })
