	<script type="text/javascript">
	function initMap_detail(map_poil){
		maplet = new Maplet("mapbar");
		maplet.centerAndZoom(new MPoint(map_poil), 11);
		var marker = new MMarker(new MPoint(map_poil), new MIcon("http://union.mapbar.com/apidoc/images/detail_mark.gif", 32, 32));
		maplet.addOverlay(marker);
		maplet.showOverview(false);
	}
	</script>

	<script type="text/javascript">
	    function navSearch(){
	        var on = document.getElementById('on');
	        var on_ = on.value;
	        if(on_ == "" || on_ == '输入起点'){
	            alert("请输入起点");
	            on.focus();
	        }else{
	            var oc = encodeURIComponent('商丘市');
	            var dc = oc;
	            var on = on_;
	            var dn = 'HTCJIFWVEBAAG';
	            var ds = encodeURIComponent(document.getElementById("dsname").value);
	            var nav_url = "http://www.mapbar.com/search/#ac=nav&st=n&qnav=1&oc="+oc+"&dc="+dc+"&on="+on+"&dn="+dn+"&ds="+ds+"&wf=navs";
	            window.open(nav_url);
	        }
	    }
	    (function ($) {
	        var tip = '输入起点';
	        var on = $('#on');
	        on.bind('focus', function () {
	            if(this.value === tip) {
	                this.value = '';
	            }
	        });
	        on.bind('blur', function () {
	            if(!this.value) this.value = tip;
	        });
	    })(jQuery);
	
	</script>

