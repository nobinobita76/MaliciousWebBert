  	function dobook(a, b, c) {
  		var doBook = document.doBook;
  		doBook.action = "http://hotel.mapbar.com/woyaoyuding/" + b + "/"
  		doBook.hid.value = a;
  		doBook.rid.value = b;
  		doBook.pid.value = c;
  		doBook.submit();
  	}
  	function refreshprice(id, Order_api) {
  		try {
  			var tm1 = $2("txtComeDate").value;
  			var tm2 = $2("txtOutDate").value;
  			document.doBook.tm1.value = tm1;
  			document.doBook.tm2.value = tm2;
  			if (tm1 > tm2) {
  				alert("离店日期应该在入店日期之前，请重新输入");

  			}
  			_v1 = null, dn = 0, tb1 = '', wks = '', d, hotels = null, n1 = 0;
  			if (tm1 != '' && tm2 != '') {
  				$2("h" + id + "").innerHTML = "<p style='text-align:center;'><img src='http://www.api.zhuna.cn/price/default/loading.gif'/></p>";
  				var file = Order_api + "e/json.php?hid=" + id + "&tm1=" + tm1 + "&tm2=" + tm2 + "&call=callback";
  				var head = document.getElementsByTagName("head")[0] || document.documentElement;
  				var js = document.createElement("script");
  				js.src = file;
  				js.charset = "utf-8";
  				head.insertBefore(js, head.firstChild);
  			}
  		} catch (e) {
  		}
  	}
  	function like(userName) {
  		var poiName = document.getElementById("poiName").innerHTML;
  			var url = "/services/comment/like?poiName="+poiName+"&count=5"+"&userName="+userName+"&method=comment";
  			$.ajax({
  	            cache: false,
  	            type: "GET",
  	            url:url,
  	            data:$('#commontForm').serialize(),
  	            async: false,
  	            error: function(request) {
  	                alert("Connection error");
  	            },
  	            success: function(data) {
  	            	if("no" == data){
  	            		var count = document.getElementById(userName).innerHTML;
  	            		document.getElementById(userName).innerHTML=++count;
  	            	} else {
  	            		alert("您已经点过赞了！");
  	            	}
  	            }
  	        });
  		}
