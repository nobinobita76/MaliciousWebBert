  <script>
    function myGod(id, w, n) {
        var box = document.getElementById(id), can = true, w = w || 1500, fq = fq || 10, n = n == -1 ? -1 : 1;
        box.innerHTML += box.innerHTML;
        box.onmouseover = function () { can = false };
        box.onmouseout = function () { can = true };
        var max = parseInt(box.scrollHeight / 2);
        new function () {
            var stop = box.scrollTop % 18 == 0 && !can;
            if (!stop) {
                var set = n > 0 ? [max, 0] : [0, max];
                box.scrollTop == set[0] ? box.scrollTop = set[1] : box.scrollTop += n;
            };
            setTimeout(arguments.callee, box.scrollTop % 18 ? fq : w);
        };
    };

   //myGod('div4', 1200);
</script> 
  <!-- 滚动效果结束--> 
  <script language="javascript" type="text/javascript">
    function getId(id) {
        return document.getElementById(id)        //本函数用于取得该节点，为的是节省代码书写。如果不用该函数我们将必须每获得一个节点就要写一次document.getElementById
    }

    //左侧展会搜索切换

    function fa_showdiv(o, num1, num2) {
        o.className = "hdqha";     //指定传递过来的对像的class为nav_current
        var j;
        var id;
        var e;

        for (var i = num1; i < num2; i++) {     //用一个for循环来遍历所有的ID
            id = "hm" + i;
            j = getId(id);
            e = getId("subha" + i);
            if (id != o.id) {      //如果ID不等当前传递过来的对像ID那么就把它的CLASS清空，在本例中就是把背景色设为无了。同时把它所对应的子菜单所在的层不显示
                j.className = "";
                e.style.display = "none";
            }
            else {
                e.style.display = "block";    //如果ID等于传递过来的对像ID那么就把它的子菜单显示出来
            }
        }
    }
 
</script> 
  <script language="javascript">
    function tabChange(obj, id) {
        var arrayli = obj.parentNode.getElementsByTagName("li"); //获取li数组（获取obj父节点下的所有li标记）
        var arrayul = document.getElementById(id).getElementsByTagName("ul");  //获取ul数组（获取id下的所有ul标记）
        for (i = 0; i < arrayul.length; i++)//循环 拿整个页面ul的个数做判断
        {
            if (obj == arrayli[i])//判断每个li的值是否和obj的值相等
            {
                arrayli[i].className = "clih"; //把此时的li的class改为cli
                arrayul[i].className = ""; //把此时的ul的class改为空
            }
            else {
                arrayli[i].className = ""; //把此时的li的class改为空
                arrayul[i].className = "hidden"; //把此时的ul的class改为hidden
            }
        }
    }
</script> 
