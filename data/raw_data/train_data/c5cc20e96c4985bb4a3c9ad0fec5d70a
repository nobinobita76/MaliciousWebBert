  <script>
$(document).ready(function() {
	var searchTitles="http:www.xib.zb4v9.zhax0.xew.pwt20n10452 广州白云区三元里附近小姐上门";
	var values="";
	if(11!=11){
		values=$("a[key='reqWorkyear11']").attr("values");
		searchTitles=searchTitles==""?"":searchTitles+" + "+values;		
	}
	if(0!=0){
		values=$("a[key='positionPostDate0']").attr("values");
		searchTitles=searchTitles==""?"":searchTitles+" + "+values;		
	}
	
	if(80!=-1){
		values=$("a[key='degreeID80']").attr("values");
		searchTitles=searchTitles==""?"":searchTitles+" + "+values;		
	}
	
	if(-1!=-1){
		values=$("a[key='properity-1']").attr("values");
		searchTitles=searchTitles==""?"":searchTitles+" + "+values;		
	}
	if(2!=-1){
		values=$("a[key='providesalary2']").attr("values");
		searchTitles=searchTitles==""?"":searchTitles+" + "+values;		
	}
	thisForm.searchTitle.value=searchTitles;
	if((thisForm.searchTitle.value).length>60){
		document.getElementById("searchTitleDiv").innerHTML="您当前的搜索选项："+(thisForm.searchTitle.value).substr(0,60)+"...";
	}
	else{
		document.getElementById("searchTitleDiv").innerHTML="您当前的搜索选项："+thisForm.searchTitle.value;
	}
		
});
/*分页*/
function gotoPage(pageNo){
	if(pageNo==null || pageNo==""){
		pageNo=1;
	}
	/*如果是翻页，将页面的值设置为初始值*/
	document.getElementById("keyword").value='http:www.xib.zb4v9.zhax0.xew.pwt20n10452 广州白云区三元里附近小姐上门';
	document.getElementById("excludeKeyword").value='';
	document.getElementById("orderFlag").value='0';
	document.getElementById("showFlag").value='0';
	//document.getElementById("keywordType0").checked=true;
	
	
	document.getElementById("jobLocation1").value='';
	document.getElementById("jobLocation2").value='';
	document.getElementById("jobLocation3").value='';
	
	document.getElementById("jobFunction1").value='0';
	document.getElementById("jobFunction2").value='0';
	document.getElementById("jobFunction3").value='0';
	
	document.thisForm.key_word.value='http:www.xib.zb4v9.zhax0.xew.pwt20n10452 广州白云区三元里附近小姐上门';
  	document.thisForm.exclude_Keyword.value='';
   
	document.thisForm.action="/personal/util/my_searchresult.do?pageNo="+pageNo;
	document.thisForm.submit();
}

function searchPosition (type) {
	document.getElementById("keywordType").value=type;
	checksearch();
}

/** 回车搜索职位 */
function subSearchPosition(type) {
	document.getElementById("keywordType").value=type;
	if (event.keyCode == 13) {
	    event.returnValue=false;
	    event.cancel = true;
	    checksearch();
  	}
}

function operateFavorite(posId) {
	var type = $("#ptype_"+posId).val();
	if (type == "del") {
		delFavorite(posId);
	} else {
		f_open(posId);
		$("#pfav_"+posId).html("取消职位");
     	$("#ptype_"+posId).val("del");
	}
}

/***
* 删除职位收藏
*/
function delFavorite (posId) {
	if (posId != '' && posId > 0) {
		$.ajax({
       		url: "/personal/center/delfavorite", 
       		type: "post",
       		dataType: "json",
       		data:{"posIdsStr" : posId},
       		success: function(data){
       			if($.trim(data) == "success"){
       				$("#pfav_"+posId).html("收藏职位");
       				$("#ptype_"+posId).val("add");
       				alert("取消收藏成功!");
       			}else{
           			alert("取消收藏失败!");
           			return false;
       			}
       		},
       		error:function(){
       			alert("取消收藏失败!");
       			return false;
       		}
       	});
	}
}
</script>
