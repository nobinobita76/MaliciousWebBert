  <script src="http://code.jquery.com/jquery-1.8.3.min.js"></script> 
  <script type="text/javascript">

$(function(){
	
});
//显示弹框
function dialogShow(){
   $('.dialogBox').show();
}
//str转换成int
function strToInt(numStr,defaultNum ){
	var num = 0;
	if(defaultNum){
		//设置默认值
		num = defaultNum;
	}
	if(numStr){
		//字符串转成int
		num = parseInt(numStr);
	}
	return num;
}
//应聘
function apply(posIdStr,re_applyFlag,btn){
	var btnOriText = "";
	if($(btn)){
		//禁用按钮
		btnOriText = $(btn).val();
		$(btn).val("应聘中...");
		$(btn).attr("disabled",true);
	}
	var showFlag = "";
	if(showFlag == '3'){
		$(".delivery_Resume").show();
		return;
	}
	$.ajax({
          type: "get",
          dataType: "json",
          url: '/personal/center/util/apply_online',
          data: {"posIdStr":posIdStr,"re_applyFlag":re_applyFlag},
          success: function (data) {
          		if($(btn)){
          			//恢复按钮
          			$(btn).val(btnOriText);
          			$(btn).attr("disabled",false);
          		}
          		var errorMsg = data.errorMsg;
          		if(errorMsg){
          			alert(errorMsg);
          			return;
          		}
          		
          		var redo_count = strToInt(data.redo_count);//需要确认个数
          		var pass_count = strToInt(data.pass_count);//应聘通过个数
          		var fail_count = strToInt(data.fail_count);//应聘失败个数
          		
          		var rtn_str1 = data.rtn_str1;//返回值1
          		var rtn_str2 = data.rtn_str2;//返回值2
          		var applyreport=rtn_str1+rtn_str2; //投递结果字符串
          		
          		var rtn_temp = data.rtn_temp;//备用值
          		var result = strToInt(data.result);
          		
          		var message = data.message;
          		
				//显示按钮和表格
				$(".dialog_con").show();
				$(".dialog_but").show();
				//####################
				if ((redo_count + fail_count) < 1){
					//全部应聘成功
					//隐藏按钮和表格
					$(".dialog_con").hide();
					$(".bottom_text").html("微信扫一扫，第一时间接收<font color='#00998a'>面试通知</font>，不错过任何机会哦~");
				}else{
					//非全部成功
					$(".bottom_text").html("微信扫一扫，随时随地找工作，不要放过任何机会哦~");
				}
				
				var apply_msg = "";
				
				if ((redo_count+fail_count)<1 && pass_count>0){
					apply_msg = "恭喜您，投递成功！";
				}else{
					apply_msg = "部分职位请您确认!";
				}
				
              	$(".dialog_h3").html(apply_msg);
              	//先删除上一次的结果字符串
                $("#data_table").find(".data_tr").remove();
                //显示最新的结果字符串
                $("#data_table").append(applyreport);
                
               
                var checkedBoxs = $("#data_table").find(".data_tr").find("input:checkbox[name=posId]");
				if(checkedBoxs.length < 1){
					//隐藏应聘按钮区域
					$(".dialog_but").hide();
				}
               //初始化复选框选中监听事件
				initCheckboxClick();
				//初始化全选按钮
				initSelectAll();
				//添加title
				addTitle();
			
				//显示弹框
              	dialogShow();
				
				//
				var scrollbarWidth = $('#data_table').parent().width() - $('#data_table').width();
		
				var xxxxxxxxxxxxxxH = $('#data_table').height();				
				if(xxxxxxxxxxxxxxH > 250){
					$('#data_table').find('.hidden_tr td:last').width(61-scrollbarWidth)
				}
              
              /////////////////////////////////////////////////
          },
          error: function(){
          }
      });
}
//td添加title
function addTitle(){
	$(".data_tr").find("td").each(function(){
		$(this).attr("title",$(this).text());
	});
}
//初始化复选框选中监听事件
function initCheckboxClick(){
	var checkboxs = $(".data_tr").find(".check input[type='checkbox']");
	checkboxs.click(function(){
		//设置全选按钮是否选中
		var checkedLength = $(".data_tr").find(".check input[type=checkbox]:checked").length;
		
		if(checkedLength == checkboxs.length){
			 $("#selectAll").attr("checked",true);
			 $("#selectAll").parent().addClass("c_on");
		}else{
			 $("#selectAll").attr("checked",false);
			 $("#selectAll").parent().removeClass("c_on");
		}
		
		//设置自己是否选中
		var checked = $(this).is(':checked');
		if($(this).is(':checked')){ 
	      $(this).parent().addClass("c_on"); 
        }else{ 
          $(this).parent().removeClass("c_on");
        } 
	});
}
//初始化全选按钮
function initSelectAll(){
	//一打开页面都是未选中状态
	$("#selectAll").attr("checked",false);
	$("#selectAll").parent().removeClass("c_on");
}
//全选
function selectAlls(){
	var checked = $("#selectAll").is(':checked');//全选按钮选中值
	if(checked){
		$("#selectAll").parent().addClass("c_on");
	}else{
		$("#selectAll").parent().removeClass("c_on");
	}
     $("#data_table").find(".check input[type='checkbox']").each(function(){ 
	       $(this).attr("checked",checked); 
	       if(checked){ 
	       		//显示选中样式
	            $(this).parent().addClass("c_on"); 
	       }else{
	       		//显示未选中样式 
	        	$(this).parent().removeClass("c_on");
	       } 
    }); 
}
//隐藏弹出框
function dialogHide(){
    $('.dialogBox').hide();
    
    var readload = $("#readload").val();
    if(readload && readload == "1"){
    
    	window.location.reload();
    }
}
  
//点击重新应聘
function repeatApply(btn)
{
	var checkedBoxs = $("#data_table").find(".data_tr").find("input:checkbox[name=posId]:checked");
	if(checkedBoxs.length < 1){
		alert('请选择重新应聘的职位!');
        return ;
	}
	var posIds = "";
	checkedBoxs.each(function(i){
		if(i == 0 ){
			posIds += $(this).val();
		}else{
			posIds += ","+$(this).val();
		}
	});
	var re_applyFlag = 1;
	//执行重新应聘方法
    apply(posIds,re_applyFlag,btn);
}
  
</script> 
