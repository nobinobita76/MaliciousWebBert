  <script type="text/javascript">
		//申请职位
		function sendApply2(btn){
			var isLogin = "false";
			if(isLogin == "false"){
				//未登录
				window.location.href = "/personal/my_login.do?url=/public/higherSearch.jsp";
				return;
			}
			var checkedBoxs = $(".result_list").find("input:checkbox[name=posId]:checked");
			
			if(checkedBoxs.length < 1){
				alert('请先选择要应聘的职位!');
		        return ;
			}
			var posIds = "";
			checkedBoxs.each(function(i){
				if(i == 0 ){
					posIds += $(this).val();
				}else{
					posIds += ","+$(this).val();
				}
			});
			//执行申请方法
		    apply(posIds,'0',btn);
		}
		function toApply(posIdStr,re_applyFlag,btn){
			var isLogin = "false";
			if(isLogin == "false"){
				//未登录
				window.location.href = "/personal/my_login.do?url=/public/higherSearch.jsp";
				return;
			}
			apply(posIdStr,re_applyFlag,btn);
		}
	</script> 
