   <script language="javascript">
var cp = 'immwa';

function cis_setcookie(cookieName, cookieValue, seconds, path, domain, secure) {
if(cookieValue == '' || seconds < 0) {
cookieValue = '';
seconds = -2592000;
}
if(seconds) {
var expires = new Date();
expires.setTime(expires.getTime() + seconds * 1000);
}
domain = !domain ? cookiedomain : domain;
path = !path ? cookiepath : path;
document.cookie = escape(cp + '_'+cookieName) + '=' + escape(cookieValue)
+ (expires ? '; expires=' + expires.toGMTString() : '')
+ (path ? '; path=' + path : '/')
+ (domain ? '; domain=' + domain : '')
+ (secure ? '; secure' : '');
}

function shownv(action){
if(action=='show'){
$('#mainnv').fadeIn(300);
$(".sidenv_content").clearQueue().animate({right: '0px'})
                        
                    
}else{
$(".sidenv_content").clearQueue().animate({right: '-414px'});
$('#mainnv').fadeOut(300);
}
}
</script> 
