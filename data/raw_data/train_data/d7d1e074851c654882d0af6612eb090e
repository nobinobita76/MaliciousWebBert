.Reward_pop .btn_confirm {height: 2.93em; width: 80%; background: -webkit-linear-gradient(top,#fd921b 0,#f5821b 100%); background: linear-gradient(to bottom,#fd921b,#f5821b); border: 1px solid #f0732f; border-radius: 8px; margin: 0 auto; }
.Reward_pop .btn_confirm a {display: block; width: 100%; color: #fff; font-size: 1.28em; line-height: 2.28; text-align: center; }
.Reward_pop .notice {color: #999; font-size: .85em; text-align: center; margin: .45em 0; }
.reward_overlay {background: rgba(228,228,228,0.9); position: fixed; width: 100%; height: 100%; top: 0; z-index: 99;display: none; }
