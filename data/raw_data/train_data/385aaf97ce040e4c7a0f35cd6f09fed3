<script type="text/javascript">

function followhospital(flag,hospital_id){
	var login_uid = "";
	if(login_uid <=0){
		location.href="/passport/login?return_url=" + document.location;
		return false;
	}
	$.ajax({
        type:"post",
        url:'/home/followhospital',
        data:{"uid":login_uid,'hospital_id':hospital_id,'flag':flag},
        dataType:"json",
        success:function(result){
            if(result.code == 200){
            	if(flag ==1){
                	$('#no_cer_hos_'+hospital_id).html("<span id=no_cer_hos_"+hospital_id+"><a class='btn_b' style='cursor:pointer;' onclick='followhospital("+2+","+hospital_id+")'>"+"已关注"+"</a></span>");
    	            return false;
                }else if(flag ==2){
                	$('#no_cer_hos_'+hospital_id).html("<span id=no_cer_hos_"+hospital_id+"><a class='btn_d' style='cursor:pointer;' onclick='followhospital("+1+","+hospital_id+")'>"+"+关注"+"</a></span>");
    	            return false;
                }
            }
            if(result.code == 100){
            	if(flag ==1){
                	$('#no_cer_hos_'+hospital_id).html("<span id=no_cer_hos_"+hospital_id+"><a class='btn_b' style='cursor:pointer;' onclick='followhospital("+2+","+hospital_id+")'>"+"已关注"+"</a></span>");
    	            return false;
                }else if(flag ==2){
                	$('#no_cer_hos_'+hospital_id).html("<span id=no_cer_hos_"+hospital_id+"><a class='btn_d' style='cursor:pointer;' onclick='followhospital("+1+","+hospital_id+")'>"+"+关注"+"</a></span>");
    	            return false;
                }
            }
	    }
	});
}
//关注医院--认证的
function followuser_new(flag,fid,hid){
	var login_uid = "";
	if(login_uid <=0){
		location.href="/passport/login?return_url=" + document.location;
		return false;
	}
	$.ajax({
        type:"post",
        url:'/home/followuser',
        data:{"uid":login_uid,'fid':fid,'flag':flag},
        dataType:"json",
        success:function(result){
            if(result.code == 200){
            	if(flag ==1){
                	$('#no_cer_hos_'+hid).html("<span id=no_cer_hos_"+hid+"><a class='btn_b' style='cursor:pointer;' onclick='followuser_new("+2+","+fid+","+hid+")'>"+"已关注"+"</a></span>");
    	            return false;
                }else if(flag ==2){
                	$('#no_cer_hos_'+hid).html("<span id=no_cer_hos_"+hid+"><a class='btn_d' style='cursor:pointer;' onclick='followuser_new("+1+","+fid+","+hid+")'>"+"+关注"+"</a></span>");
     	            return false;
                }
            }
            if(result.code == 100){
            	if(flag ==1){
                	$('#no_cer_hos_'+hid).html("<span id=no_cer_hos_"+hid+"><a class='btn_b' style='cursor:pointer;' onclick='followuser_new("+2+","+fid+","+hid+")'>"+"已关注"+"</a></span>");
    	            return false;
                }else if(flag ==2){
                	$('#no_cer_hos_'+hid).html("<span id=no_cer_hos_"+hid+"><a class='btn_d' style='cursor:pointer;' onclick='followuser_new("+1+","+fid+","+hid+")'>"+"+关注"+"</a></span>");
     	            return false;
                }
            }
	    }
	});
}

</script>

    
