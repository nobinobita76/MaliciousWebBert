//想去、去过
var cur_user_id = '';
var go_uids_list = ''.split(",");
var not_go_uids_list = ''.split(",");

function go_to(){
	//alert(cur_user_id);
	if(!cur_user_id){
		location.href="/passport/login?return_url=" + document.location;
		return false;
	}
	if($.inArray(cur_user_id,go_uids_list)!= -1 || $.inArray(cur_user_id,not_go_uids_list)!= -1){
		alert("抱歉，已经做出过选择，只能选择一次！");  return false;
	}

	$.post("/hospital/edit",{hospital_id:4003, act:'go'},function(data){
		if(data.result=="true"){
			$("#go_to_cnt").text(parseInt($("#go_to_cnt").text())+1);
			go_uids_list.push(cur_user_id);
		}else{
			alert("抱歉，已经做出过选择，只能选择一次！");
		}
		//$(".go_to").prepend('<a href="/home/person/uid/'+cur_user_id+'"><img src="'+data.avatar+'" alt="" width="34" height="34"></a>');
	},"json");
}
function go_to_new(){
	//alert(cur_user_id);	
	if(cur_user_id){		;
			$.post("/hospital/edit",{hospital_id:4003, act:'go'},function(data){
				if(data.result=="true"){
					$("#go_to_cnt").text(parseInt($("#go_to_cnt").text())+1);
				}else{
					alert("抱歉，已经做出过选择，只能选择一次！");
				}
				//$(".go_to").prepend('<a href="/home/person/uid/'+cur_user_id+'"><img src="'+data.avatar+'" alt="" width="34" height="34"></a>');
				
				go_uids_list.push(cur_user_id);
			},"json");
	}
}
function not_go_to(){
	if(!cur_user_id){
		location.href="/passport/login?return_url=" + document.location;
		return false;
	}
	if($.inArray(cur_user_id,go_uids_list)!= -1 || $.inArray(cur_user_id,not_go_uids_list)!= -1){
		alert("抱歉，已经做出过选择，只能选择一次！");  return false;
	}

	$.post("/hospital/edit",{hospital_id:4003, act:'notGo'},function(data){
		if(data.result=="true"){
			$("#not_go_to_cnt").text(parseInt($("#not_go_to_cnt").text())+1);
		}else{
			alert("抱歉，已经做出过选择，只能选择一次！");
		}
 		//$(".not_go_to").prepend('<a href="/home/person/uid/'+cur_user_id+'"><img src="'+data.avatar+'" alt="" width="34" height="34"></a>');
		//$("#not_go_to_cnt").text(parseInt($("#not_go_to_cnt").text())+1);
		not_go_uids_list.push(cur_user_id);
	},"json");
}
function showPhone(){
	$.post("/hospital/tvc",{hid:4003});
	$(".show_phone").hide();
	$(".look_phone").show();
}

