<script type="text/javascript">

function cancelHospital(){
    if (confirm('确定取消注册医院吗？')) {
        $.post('/hospital/unbind',{'hid':4003},function(data){
            if (data.code == 200) {
                alert('取消成功！');
                window.location.href='/';
            }else{
                alert('取消失败');
            };
        },'json')
    };
}

$('#hd .m_con_b .name_box .score').hover(function(){
    $('.score_exp').show().stop().animate({'opacity':'1'},300);
},function(){
  $('.score_exp').stop().animate({'opacity':'0'},300,function(){
      $(this).hide();
  });
});

$('#hd .m_con_b .tools .score').hover(function(){
    $(this).find('.code_box').show();
},function(){
 $(this).find('.code_box').hide();
});
</script>
