        function bBarSelectBtn() {
            var h, strBroker = '',
            strAccessLevel = '',
            strServiceType = '',
            strLogin, strMbrID = '';
            var k = {};
            var l = {};
            h = document.cookie;
            k = h.split(";");
            for (i = 0; i < k.length; i++) {
                if (k[i].substr(0, 8).indexOf("Login=") != -1) {
                    strLogin = k[i].substring(k[i].substr(0, 8).indexOf("Login=") + 6);
                    if (strLogin.indexOf("mbr%5Fid") != -1 || strLogin.indexOf("mbr_id") != -1) {
                        l = strLogin.split("&");
                        for (j = 0; j < l.length; j++) {
                            if (l[j].indexOf("broker") != -1) {
                                strBroker = l[j].substring(7).toUpperCase()
                            }
                            if (l[j].indexOf("ucalc") != -1) {
                                strAccessLevel = l[j].substring(6)
                            }
                            if (l[j].indexOf("service%5Ftype") != -1) {
                                strServiceType = l[j].substring(15)
                            } else if (l[j].indexOf("service_type") != -1) {
                                strServiceType = l[j].substring(13)
                            }
                            if (l[j].indexOf("mbr%5Fid") != -1) {
                                strMbrID = l[j].substring(9)
                            } else if (l[j].indexOf("mbr_id") != -1) {
                                strMbrID = l[j].substring(7)
                            }
                        }
                    }
                    break;
                }
            }

