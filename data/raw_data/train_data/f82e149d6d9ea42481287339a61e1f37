    var isStock = false;

    function symbolChange(symbol) {
        try
        {
            if(parseInt(symbol) > 9999) {
                isStock =  $.inArray(parseInt(symbol.replace(/^0+/, '')) ,s) >= 0;
            } else {
                isStock =  $.inArray(parseInt(symbol.replace(/^0+/, '')) ,s) < 0;
            }
        }
        catch(ex)
        {
            isStock =  false;
        }
        var newUrl = isStock ? String.format( url, "S=Y&", symbol) : String.format( url, "", symbol);
        if(isAllReflesh || !isStock) {
            window.location.href = newUrl;
            return;
        } else if(typeof(loadBNPInIframe) == "function") {        
            loadBNPInIframe(symbol);
        }
        
        try
        {
            symbol = symbol.replace(/^[0]+/g,"");
   
            if(typeof(leftMenuSponsor) != "undefined") {     
                leftMenuSponsor.callback(symbol);
            }
            if(typeof(TopSponsor) != "undefined") {
                TopSponsor.callback(symbol);
            } else if(typeof(document.getElementById("aspnetForm").TopSponsor) != "undefined") {
                 document.getElementById("aspnetForm").TopSponsor.callback(symbol);
            }
        }
        catch(ex)
        {

        }
        return false;
    }
     
    String.format = function(src){
        if (arguments.length == 0) return null;
        var args = Array.prototype.slice.call(arguments, 1);
        return src.replace(/\{(\d+)\}/g, function(m, i){ return args[i]; });
    };
  
    function updateVar(symbol, name, industry, underlying) {
        b_symbol = symbol;
        b_sName = name;
        b_sIndustry = industry;
        b_sUnderlying = underlying;
    }  

