    function GoTo(type, symbol){
    switch (type) {
    
    case 1:window.location.href = '/tc/stocks/quote/detail-quote.aspx?symbol=' + symbol;break;
    case 2:window.location.href= "/tc/stock/BasicChart.aspx?Symbol=" + symbol;break;
    case 3:window.location.href= "/tc/stocks/analysis/stock-aafn/{0}/0/all/1".replace("{0}", symbol);break;
    case 4:window.location.href= '/tc/stocks/analysis/company-fundamental/company-profile/?symbol=' + symbol;break;
    case 5: window.location.href = "http://www.aastocks.com/tc/stocks/analysis/blocktrade.aspx?symbol=" + symbol; break;
    case 6:window.location.href= "/tc/stock/LinkedWarrants.aspx?Symbol=" + symbol;break;
    case 7:window.location.href= "/tc/stock/CBBCInformation.aspx?Symbol=" + symbol;break;
    case 8:window.location.href= "/tc/stocks/analysis/stock-aamm/{0}/0/aamm-all-category".replace("{0}", symbol);break;
    case 9:window.location.href= "/tc/stocks/market/index/hk-index-con.aspx?Index=" + symbol;break;
    case 10:window.location.href= "/tc/market/InterestsDisclosure.aspx?shareholder=" + symbol;break;
    case 11:window.location.href= "/tc/usq/quote/quote.aspx?symbol=" + encodeURIComponent(symbol);break;
    case 12:window.location.href= "/tc/stock/DetailChart.aspx?Symbol=" + symbol;break;
    case 13: window.location.href = "http://www.aastocks.com/tc/stocks/warrant/search.aspx?symbol=" + symbol; break;
    case 14: window.location.href = "http://www.aastocks.com/tc/stocks/cbbc/search.aspx?symbol=" + symbol; break;
    case 15:window.location.href= '/tc/stocks/quote/detail-quote.aspx?symbol=' + symbol;break;
    }
    }
