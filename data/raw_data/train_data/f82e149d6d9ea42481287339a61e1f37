            if (strMbrID !== '') {
                if (strAccessLevel == "3" || strAccessLevel == "2" || (strAccessLevel == "0" && strServiceType == "FW") || strBroker == "AA5" || strBroker == "AA6") {
                    document.writeln(b + "|" + a);
                } else {
                    document.writeln(b);
                }
            } else {
                document.writeln(d + "|" + c);
            }
        }
        
        function ToogleFooterBar() {
            if ($("#btm_bar").is(":visible")) {
                $("#btm_bar").hide();
                $("#footer .footer").removeClass("s1 s2").addClass("s2");
                return 0;
            } else {
                $("#btm_bar").show();
                $("#footer .footer").removeClass("s1 s2").addClass("s1");
                return 1;
            }
        }
        
        $(function () {
            $(".fbar-close").bind("click", function () {
                var FB = ToogleFooterBar();
                SetFooterCookies(FB, 'www.aastocks.com');
            });

            var FB = GetFooterCookies();
            if (FB.value == "0") {
                ToogleFooterBar();
            }
        });
    </script>
