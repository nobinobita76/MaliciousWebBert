            <script language="javascript" type="text/javascript">
            function usa_go(){
                var strSymbol = document.getElementById("UsSymbol").value;
                this.popWindow = window.open('/tc/usq/quote/quote.aspx?symbol=' + encodeURIComponent(strSymbol), '_forex');
            }
