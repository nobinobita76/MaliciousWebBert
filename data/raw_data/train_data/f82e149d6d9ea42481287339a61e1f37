        function bannerDataGetTime() {
            var d = new Date();
            var curr_date = '00' + d.getDate();
            var curr_month = '00' + (d.getMonth() + 1);
            var curr_year = d.getFullYear();
            var curr_hour = '00' + d.getHours();
            var curr_min = d.getMinutes() >= 30 ? '30' : '00';
            return curr_year + '' + curr_month.substr(curr_month.length - 2, 2) + '' + curr_date.substr(curr_date.length - 2, 2) + '' + curr_hour.substr(curr_hour.length - 2, 2) + '' + curr_min;
        }

    </script>
