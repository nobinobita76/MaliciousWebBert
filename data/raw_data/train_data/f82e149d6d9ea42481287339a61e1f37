            <script language="javascript" type="text/javascript">
            var searchBoxText = "代號/中文/拼音";
            var isSupportSH = true;
            function go() {
                var strSymbol = document.getElementById("PY_txt").value;
                var isLikeSHSymbol = (strSymbol.length == 6);
                
                if (isLikeSHSymbol) {
                    if (isSupportSH) {
                        
                            cnquote(strSymbol);
                        
                    } else {
                        alert('此頁不支援A股');
                    }
                } else {
                    strSymbol=hk_symbol_check(strSymbol, '股票代號錯誤，請重新輸入﹗');
                    if (searchBoxText == strSymbol)
                        symbol = "00388";
                    
                    if (strSymbol != "")
                    {
                        if ($("#txtHKQuote")) {
                            $("#txtHKQuote").val(strSymbol);
                        }
                        
                        if ("http://www.aastocks.com/tc/LTP/RTQuote.aspx?".toLowerCase().indexOf("rtquote.aspx") >=0 || "http://www.aastocks.com/tc/LTP/RTQuote.aspx?".toLowerCase().indexOf("detailquote.aspx") >=0 ){
                            SetLTP('www.aastocks.com');
                        }
                        
                        if ("http://www.aastocks.com/tc/LTP/RTQuote.aspx?".indexOf("FlashDetailChart.aspx") >=0 || "http://www.aastocks.com/tc/LTP/RTQuote.aspx?".indexOf("RTQuote.aspx") >=0  || "http://www.aastocks.com/tc/LTP/RTQuote.aspx?".toLowerCase().indexOf("detailchart.aspx") >=0 ){
                            if (typeof(symbolChange) == "function") {
                                symbolChange(strSymbol);SetFocus();
                            } else {
	                            if ("http://www.aastocks.com/tc/LTP/RTQuote.aspx?".indexOf("{0}") >=0)
	                                window.location.href = "http://www.aastocks.com/tc/LTP/RTQuote.aspx?".replace("{0}",  strSymbol);
	                            else
	                                window.location.href = "http://www.aastocks.com/tc/LTP/RTQuote.aspx?&symbol=" + strSymbol;
	                        }  
	                        while(strSymbol.length <5){
	                            strSymbol = "0" + strSymbol; 
	                        }
	                        document.getElementById("PY_txt").value = strSymbol;
	                        document.getElementById("PY_txt").focus();
	                        document.getElementById("PY_txt").select();     
	                    }                
	                    else
	                    {
	                        if ("http://www.aastocks.com/tc/LTP/RTQuote.aspx?".indexOf("{0}") >=0)
	                            window.location.href = "http://www.aastocks.com/tc/LTP/RTQuote.aspx?".replace("{0}",  strSymbol);
	                        else
	                            window.location.href = "http://www.aastocks.com/tc/LTP/RTQuote.aspx?symbol=" + strSymbol;
	                    }
                	}
                }
                    
                return false;
            }
            
            function goTo(link){
                var strSymbol = document.getElementById("PY_txt").value;
                strSymbol=hk_symbol_check(strSymbol, '股票代號錯誤，請重新輸入﹗');
                if (strSymbol != "")
                    window.location = link.replace("{0}", strSymbol);
            }
            </script>
            
