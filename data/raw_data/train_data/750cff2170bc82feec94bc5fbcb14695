<script type="text/javascript">
jQuery(function(){
var urlParser = new LHC.URLParser(window.location.href);
var host = urlParser.getHost();
var dom_header = jQuery('#header-forum');
var ico_href = '/favicon_bdcp.ico';
var link_href = '';
var linkCss = {
'baidu.lecai.com' : '/css/cooperator/baidu/common.css',
'caipiao.hupu.com' : '/css/cooperator/hoopchina/common.css',
'hupu.lecai.com' : '/css/cooperator/hoopchina/common.css',
'caipiao.78500.cn' : '/css/cooperator/78500/common.css',
'hao123.lecai.com' : '/css/cooperator/hao123/common.css',
'youxi.baidu.lecai.com' : '/css/cooperator/baidu_youxi/common.css',
'tieba.lecai.com' : '/css/cooperator/baidu_tieba/common.css',
'58.lecai.com' : '/css/cooperator/58/common.css'
}
var linkCssArr = [];

var url = host.replace('bbs.','');

if(url == 'lecai.com'){
G.baseUrl = 'http://www.lecai.com';
    G.baseMainUrl = 'http://www.lecai.com';
    G.baseDataUrl = 'http://data.lecai.com';
}else{
G.baseUrl = 'http://' + url;
    G.baseMainUrl = 'http://' + url;
    G.baseDataUrl = 'http://data.' + url;
    if(url == 'hao123.lecai.com'){
    	ico_href = '/cooperator/hao123/favicon.ico';
    }
}

if(url in linkCss){
link_href = linkCss[url];
}

for(var attr in linkCss){
linkCssArr.push(linkCss[attr]);
}

jQuery('head link[type="text/css"]').each(function(){
var href = jQuery(this).attr('href').toLocaleLowerCase(); 
for(var i=0; i<linkCssArr.length; i++){
if(new RegExp(linkCssArr[i]).test(href)){
jQuery(this).remove();
return false;
}
}
});

jQuery('head base:first').attr('href','http://' + host + '/');
jQuery('head link[rel="shortcut icon"]').attr('href',G.baseImgUrl + ico_href);
if(link_href != ''){
jQuery('<link>').attr({
'rel':'stylesheet',
'type':'text/css',
'href':G.baseStaticUrl + link_href
}).appendTo(jQuery('head'));
}

jQuery.ajax({
            url: '//' + host + '/api/forum/template.php',
            type: 'GET',
            dataType:'html',
            global: false,
            success: function(html) {
            	dom_header.html(html);
            }
        });	
});
</script>
