                    if ($(this).attr("data") == "close") {
                        $(".measure-con-box").css("display", "");
                        $(this).attr("data", "open").html('<span class="btn-text">收起部分实测</span><i class="icon10 icon10-down1"></i>')
                    } else {
                        $(".measure-con-box:gt(1)").css("display", "none");
                        $(this).attr("data", "close").html('<span class="btn-text">展开全部实测</span><i class="icon10 icon10-top"></i>');
                        scrollFloatFixIE6()
                    }
                })
