            autoseries.che168(seriesid,yearid,auto.area.provinceId,auto.area.cityId);
            //车商城报价
            autoseries.MallPrice(seriesid,auto.area.cityId);
            //购车热线
            var tempHtml = '<div class="fn-fontsize16 fn-clear"><span>购车热线：</span>{StyledPhone}</div><div>{Title}</div>';
            var myurl = "http://dealer.api.autohome.com.cn/dealerrest/Profile/GetRecommendDealers?_appid=cms&_seriesId=" + seriesid + "&cityId=" + auto.area.cityId + "&_rowCount=2";
            $.ajax({
                url: myurl ,
                type:'get',
                dataType: 'jsonp',
                cache: true,
                jsonp: '_callback',
                jsonpCallback: 'isdealer',
                scriptCharset:'utf-8',
                success: function(data) 
                {
                    if (data.returncode > 0 || data.message != "" || !data.result || data.result.rowcount == 0) {
                        return
                    }
                    var dealerhtml = "";
                    for (var i = 0; i < data.result.list.length; i++) {
                        if (!document.getElementById("ad_word_0" + (i + 1))) continue;
                        dealerhtml = tempHtml;
                        if (!data.result.list[i].IsDealer) dealerhtml = dealerhtml.replace("{Title}",  '<a href="'+data.result.list[i].LinkUrl+'">'+data.result.list[i].Company+'</a>');
                        else dealerhtml = dealerhtml.replace("{Title}", '<a href="http://dealer.autohome.com.cn/{DealerId}/#pvareaid={PvId}">{DealerName}</a>');
                        dealerhtml = dealerhtml.replace("{DealerId}", data.result.list[i].DealerId);
                        dealerhtml = dealerhtml.replace("{DealerName}", data.result.list[i].Company);
                        dealerhtml = dealerhtml.replace("{StyledPhone}", data.result.list[i].AsSellPhone);
                        switch (i) {
                            case 0:
                                dealerhtml = dealerhtml.replace("{PvId}", 101546);
                                break;
                            case 1:
                                dealerhtml = dealerhtml.replace("{PvId}", 101547);
                                break
                        }
                        document.getElementById("ad_word_0" + (i + 1)).innerHTML = dealerhtml
                    }
                    DealerResAdmin.resLoader('ad_word_01', '10000', auto.area.cityId, seriesid, 0,false);
                    return;
                }
            })

            //对比
            autoseries.carcompare('1592,1593,1594,1595');
            //$(function(){Car.init({cssText:'right:0; position: fixed; _position:absolute; top: 125px; width:50px;',autoLeft:!1,max:4,pvareaid:101719,psids:'1592,1593,1594,1595',add:function(item){var $ass=$(item);$ass[0]&&($ass.attr("class","added"))&&($ass.text("已加入"));},remove:function(item){var $ass=$(item);$ass[0]&&($ass.attr("class","btn btn-mini"))&&($ass.html("<i class='icon10 icon10-plus'></i>对比"));}});Car.Menu.show();});
        
            //试驾/低价购车/团购
            DealerResAdmin.resLoader('shijia', '10033', auto.area.cityId, seriesid, 0,false);

            //蓝v
            autospec.bluev(seriesid);
               
            //车型列表 
            autoseries.speclist(seriesid,levelid,auto.area.cityId);           

            //经销商参考价
            autoseries.dealerprice(seriesid,levelid,auto.area.provinceId,auto.area.cityId, false,'1npc10000009');
            
            //经销商iframe
            var iframeSrc = 'http://dealer.autohome.com.cn/frame/' + seriesid + '/' + auto.area.provinceId + '/' + auto.area.cityId + '/0.html?source='+auto.area.RefSource;
            $('#dealeriframe').attr("src", iframeSrc);

            //车商城
            if (! ($.browser.msie && $.browser.version == "6.0")) {
                $.ajax({
                    url: 'http://api.mall.autohome.com.cn/getpopseries/' + fctid +'/' + seriesid +'/' + auto.area.cityId + '/?_appid=cms',
                    cache: !($.browser.msie && ($.browser.version == "6.0") && !$.support.style),
                    dataType: 'jsonp',
                    jsonpCallback: 'mallAreaCallback',
                    success: function(data) {
                        if (data.returncode == 0 && data.result.list.length > 0 && data.result.list[0].recommendItemList.length >= 4) {
                            $("#mall").append($("#auto-series-mall-tmpl-main").tmpl(data.result.list[0]))
                        }
                    }
                })
            }
            $(function() {
                var scrollFloat = function(id) {
                    var $elem = $(id),
                    $temp,
                    $nav,
                    create = function() {
                        $temp = $('<div style="width:' + (sup.fixed ? $elem.width() : "1000") + 'px;margin:0 auto;height:0;display:none;"><div id="_tempscrollFloat" class="' + $elem[0].className + '" style="z-index:1000;top:0;height:' + $elem.height() + "px;width:" + $elem.width() + "px;position:" + (sup.fixed ? "fixed": "absolute") + ";" + $elem[0].style.cssText + '">' + $elem.html() + "</div></div>");
                        $temp.appendTo("body");
                        $nav = $temp.children("div")
                    },
                    set = function() {
                        var wT = $(window).scrollTop(),
                        pos = $elem.offset();
                        if (wT > pos.top) {
                            if (sup.fixed) {
                                $temp.show()
                            } else {
                                $nav.css("top", wT + "px");
                                $temp.show()
                            }
                        } else {
                            $temp.hide()
                        }
                    };
                    create();
                    set();
                    $(window).on("scroll resize", set)
                };
                var MallUrl = 'http://api.mall.autohome.com.cn/gethomemallad/spread/'+seriesid+'/' + auto.area.cityId + '?_appid=cms';
                $.ajax({
                    url: MallUrl,
                    type: "get",
                    dataType: "jsonp",
                    jsonpCallback: "mallTabCallback",
                    cache: true,
                    scriptCharset: "utf-8",
                    success: function(data) {
                        if (data.returncode == 0 && data.result.rowcount > 0 && JSON.stringify(data.result.list) != "{}") {
                            $("#SeriesCarMall").html('<a target="_blank" href="' + data.result.list.pcUrl + '#pvareaid=2148101">' + data.result.list.iconText + "</a>")
                        }
                        scrollFloat("#navTop")
                    }
                })
            })
