            //lazyload
            $(function() {
                //车系金融
                var ScrollLock=true;
                //车型列表曝光
                var SpecListShow=true;
                //询价
                var xunjiashow=true;
                //经销商列表曝光
                var DealerListShow=true;
                $(window).bind("scroll", function(event){
                    WindowScroll();
                });
                WindowScroll();
                function WindowScroll(){
                    var thisButtomTop = parseInt($(window).height()) + parseInt($(window).scrollTop());  
                    var thisTop = parseInt($(window).scrollTop()); 
                    if($(".financial").length>0){
                        var PictureTop = parseInt($(".financial").offset().top);  
                        if (PictureTop >= thisTop && PictureTop <= thisButtomTop) {
                            if($('.financial #tab3-1 div.series-financial').html()==""&&ScrollLock){
                                ScrollLock=false;
                                autoseries.financial(seriesid,auto.area.cityId,'index');
                            }
                        }
                    }
                    if($("#speclist").length>0){
                        var speclistTop = parseInt($("#speclist").offset().top);  
                        if (speclistTop >= thisTop && speclistTop <= thisButtomTop) {
                            if(SpecListShow){
                                SpecListShow=false;
                                trackMdaEvent('dlr_enter_source_show', {'esfrom':'1npc10000009','espos':'','business':'CSH','series':''+seriesid+'','city':''+auto.area.cityId+''});
                            }
                        }
                    }
                    if($("#xunjia").length>0){
                        var xunjiaTop = parseInt($("#xunjia").offset().top);  
                        if (xunjiaTop >= thisTop && xunjiaTop <= thisButtomTop) {
                            if(xunjiashow){
                                xunjiashow=false;
                                trackMdaEvent('dlr_enter_source_show', {'esfrom':'1npc10000010','espos':'','business':'CSH','series':''+seriesid+'','city':''+auto.area.cityId+''});
                            }
                        }
                    }
                    if($("#dealeriframe").length>0){
                        var dealeriframeTop = parseInt($("#dealeriframe").offset().top);  
                        if (dealeriframeTop >= thisTop && dealeriframeTop <= thisButtomTop) {
                            if(DealerListShow){
                                DealerListShow=false;
                                trackMdaEvent('dlr_enter_source_show', {'esfrom':'1npc10000011','espos':'','business':'CSH','series':''+seriesid+'','city':''+auto.area.cityId+''});
                                trackMdaEvent('dlr_enter_source_show', {'esfrom':'1npc10000012','espos':'','business':'CSH','series':''+seriesid+'','city':''+auto.area.cityId+''});
                                trackMdaEvent('dlr_enter_source_show', {'esfrom':'1npc10000013','espos':'','business':'CSH','series':''+seriesid+'','city':''+auto.area.cityId+''});
                            }
                        }
                    }
                }
                $(".otherul img").lazyload({
                    threshold: "300",
                    data_attribute: function() {
                        return ri.imageSrc($(this).attr("data-original"), auto.hasWebP())
                    }
                });
                $(".uibox img").lazyload({
                    threshold: "300",
                    data_attribute: function() {
                        return ri.imageSrc($(this).attr("data-original"), auto.hasWebP())
                    }
                });
