            $(function() {
                var scrollFloatFixIE6 = function() {
                    if (!sup.fixed) {
                        var bh = $('body').height(),
                        wh = $(window).height();
                        $("#_tempscrollFloat").css('top', bh - wh);
                        var compObj = $('#divShowCarCompare,#popautoapp,.comparepop,.gotop02');
                        if (compObj.length > 0) {
                            compObj.css('top', bh - wh + 90)
                        }
                    }
                };
