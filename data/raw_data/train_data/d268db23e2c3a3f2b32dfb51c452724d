    <script src="http://i.pengxun.cn/fcontent/plugin/juicer/juicer.min.js"></script>
    <script type="text/javascript">
        var flag = 0;
        $(".vip-topic li").click(function () {
            $(".vip-topic li").each(function (index, obj) {
                $(obj).removeClass();
                $("#" + $(obj).attr("id") + "_block").hide();
            });
            $(this).addClass("vip-topic-active");
            $("#" + $(this).attr("id") + "_block").show();
        });

        $(".vzan-friend-focus").click(function () {
            if ($(this).find("a").text() != "管理") {
                layer.open({
                    content: "<img src='" + $(this).find("a").data("src") + "' width='250px' height='250px'/>",
                });
            }
        });

        var is_show_guanzhu = GetCookies("is_show_guanzhu_616128");
        if (is_show_guanzhu != "1") {
            if(is_show_guanzhu=="2")
                $("#qrcode_attention_block").hide();
            else
                $("#qrcode_attention_block").show();

            if(is_show_guanzhu!="0"&&is_show_guanzhu!="2"){
                $.get("/fajax/getfocus",{ fid: 616128},function(data){
                    if (data && data.isok) {
                        GetCookies("is_show_guanzhu_616128", "2", { path: "/", domain: ".vzan.com", expires: 7 });
                        $("#qrcode_attention_block").hide();
                    }
                    else{
                        GetCookies("is_show_guanzhu_616128", "0", { path: "/", domain:".vzan.com", expires: 1 });
                    }
                });
            }
        }
       
        //加载好友动态
        function loadfrienddt() {
            juicer.set({
                'tag::operationOpen': '{#',
                'tag::operationClose': '}',
                'tag::interpolateOpen': '${',
                'tag::interpolateClose': '}',
                'tag::noneencodeOpen': '$${',
                'tag::noneencodeClose': '}',
                'tag::commentOpen': '{#',
                'tag::commentClose': '}'
            });
            $.ajax({
                url: '/f/getarticlebottom-616128',
                type: 'get',
                dataType: 'json',
                data: {
                    pageIndex: 1,
                    typeId: 0,
                    from: 'wx',
                    rspan: 1,                
                    key:'index'
                },
                success: function(data) {
                    if (data.code == 0) {
                        $('#dynamiclist').hide();
                        return false;
                    }
                    var artlist = data.ArtList;
                    var len = artlist.length;
                    if (len < 1) {
                        flag = 0;
                        $('#dynamiclist').hide();
                        return false;
                    } 
                    TemplateId = "TemplateIndexArtOne";
                    for (var i = 0; i < len; i++) {                       
                        var tpl = document.getElementById(TemplateId).innerHTML;
                        var html = juicer(tpl, artlist[i]);
                        $("#art-content").append(html);
                    }
                    //thissize();
                    bindvoice();
                    $("iframe")
                        .each(function() {
                            var videoUrl = $(this).attr("src");
                            if (videoUrl.indexOf("yuntv.letv.com") > -1) {
                            } else {
                                if (videoUrl.indexOf('width=') > 0 || videoUrl.indexOf('height=') > 0) {
                                    $(this).attr("src", videoUrl.replace("width", "w1").replace("height", "h1"));
                                }
                            }
                        });                
                },
                error: function() {
                    $('#dynamiclist').hide();
                }
            });
        };
    </script>



<script type="text/javascript">
    wx.ready(function () {
        if ('1' == "1") {
            wxShareShow();
        }
        else {
            wx.hideOptionMenu();
        }
    });
    //微信分享(可用于动态修改)
    function wxShareShow() {
        wx.showOptionMenu();
        var title = $(".share_bar").attr("attr-title");
        var descript = $(".share_bar").attr("attr-descript");
        var imgurl = $(".share_bar").attr("attr-pic");
        var shareurl = $(".share_bar").attr("attr-url");
        //分享给朋友
        wx.onMenuShareAppMessage({
            title: title, //  分享标题
            desc: descript, //  分享链接
            link: shareurl, // 分享图标
            imgUrl: imgurl,
            type: 'link',
            dataUrl: '',
            success: function () {
                $(this).minTipsBox({
                    tipsContent: "分享成功",
                    tipsTime: 1
                });  // 用户确认分享后执行的回调函数
            },
            cancel: function () {
                // 用户取消分享后执行的回调函数
            }
        });

        //分享到朋友圈
        wx.onMenuShareTimeline({
            title: title, // 分享标题
            link: shareurl, // 分享链接
            imgUrl: imgurl, // 分享图标
            success: function () {
                $(this).minTipsBox({
                    tipsContent: "分享成功",
                    tipsTime: 1
                });  // 用户确认分享后执行的回调函数
            },
            cancel: function () {
                // 用户取消分享后执行的回调函数
            }
        });

        //分享到QQ
        wx.onMenuShareQQ({
            title: title, // 分享标题
            desc: descript, // 分享描述
            link: shareurl, // 分享链接
            imgUrl: imgurl, // 分享图标
            success: function () {
                $(this).minTipsBox({
                    tipsContent: "分享成功",
                    tipsTime: 1
                });  // 用户确认分享后执行的回调函数
            },
            cancel: function () {
                // 用户取消分享后执行的回调函数
            }
        });

        //分享到腾讯微博
        wx.onMenuShareWeibo({
            title: title, // 分享标题
            desc: descript, // 分享描述
            link: shareurl, // 分享链接
            imgUrl: imgurl, // 分享图标
            success: function () {
                $(this).minTipsBox({
                    tipsContent: "分享成功",
                    tipsTime: 1
                });  // 用户确认分享后执行的回调函数
            },
            cancel: function () {
                // 用户取消分享后执行的回调函数
            }
        });

        //分享到QQ空间
        wx.onMenuShareQZone({
            title: title, // 分享标题
            desc: descript, // 分享描述
            link: shareurl, // 分享链接
            imgUrl: imgurl, // 分享图标
            success: function () {
                $(this).minTipsBox({
                    tipsContent: "分享成功",
                    tipsTime: 1
                });  // 用户确认分享后执行的回调函数
            },
            cancel: function () {
                // 用户取消分享后执行的回调函数
            }
        });
        wx.showOptionMenu();
    }
</script>

