function setqu(id){
	for(var i=1 ;i<=4 ; i++){
		if(i==id) {
			document.getElementById("yqnav"+id).className = "select_nav";
			document.getElementById("con"+id).style.display = "";
		}
		else{
			document.getElementById("yqnav"+i).className = "no_select_nav";
			document.getElementById("con"+i).style.display = "none";
		}
	}
}
