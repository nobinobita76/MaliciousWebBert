<script language="javascript">
function getCurDate(){	
	var date = new Date();
	var str = date.getFullYear()+'年'+(date.getMonth()+1)+'月'+date.getDate()+'日';	
	return str;
}

function AddFavorite(sURL, sTitle) {   
    try {   
        window.external.addFavorite(sURL, sTitle);   
    } catch (e) {   
        try {   
            window.sidebar.addPanel(sTitle, sURL, "");   
        } catch (e) {   
            alert("加入收藏失败，请使用Ctrl+D进行添加");   
        }   
    }   
}   

function setFocus(obj)
{
	//var input=document.getElementById("mobilePhone");
	if(obj.value.replace(/ /g,"")=="" || obj.value=="请输入您的手机号码")
	{
		obj.value="";
		obj.style.color="#000000";
		setTimeout(function(){obj.focus();},50);
	}
}
function setBlur(obj)
{
	//var input=document.getElementById("mobilePhone");
	if(obj.value.replace(/ /g,"")=="")
	{
		obj.value="请输入您的手机号码";
		obj.style.color="#666";
	}
}

function resetvalue(obj)
{
 obj.value='';
 obj.focus();
 obj.style.color='#000';
}

function CheckTelType(idstr)
	{
	   var obj= document.getElementById(idstr);
		if(obj.value.replace(/^\s*|\s*$/g,'')=="")
		{
			alert("请填写手机号码！");
			obj.focus();
			return false;
		}
		
		if(obj.value.search(/^[0-9]{8,12}$/)==-1)
		{
			alert("请填写正确的手机号码！")
			obj.focus();
			obj.select();
			return false;
		}
		return true;
	}
	
	function checkStockCode(idstr){
		var stockCodeValue = document.getElementById(idstr).value;	
		var isEnterStock = false;
		
		if(/^\d{6}$/.test(stockCodeValue))
		{
			isEnterStock = true;		
		}	
		if(!isEnterStock){
			alert('请正确输入股票代码!');
			document.getElementById("stockCode").select();
			document.getElementById("stockCode").focus();
			return false;
		}
		return true;
	}
	
	function show_mask(div1,mask2)
	{
	   	document.getElementById(div1).style.left = document.body.clientWidth/2-154+'px';	
		//document.getElementById(div1).style.top = document.body.scrollTop+154+'px';	
		document.getElementById(div1).style.display = 'block';
		document.getElementById(mask2).style.top=0+'px';
		document.getElementById(mask2).style.display = 'block';
		document.getElementById(mask2).style.height=parseInt(document.body.clientHeight)>parseInt(document.documentElement.clientHeight)?parseInt(document.body.scrollHeight)+"px":parseInt(document.documentElement.clientHeight)+"px";
	}
	function shut_mask(div1,mask2){
		document.getElementById(div1).style.display = 'none';
		document.getElementById(mask2).style.display = 'none';
	}
</script>
