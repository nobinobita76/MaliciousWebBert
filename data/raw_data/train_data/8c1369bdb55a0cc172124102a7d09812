<script language="javascript" type="text/javascript">
function showList(li,index)
{
	var navList=li.parentNode.getElementsByTagName("li");
	var ulList=document.getElementById("listContainer").getElementsByTagName("ul");
	for(var i=0;i<navList.length;i++)
	{
		navList[i].className="";
	}
	for(var i=0;i<ulList.length;i++)
	{
		ulList[i].style.display="none";
	}
	li.className="active";
	ulList[index].style.display="block";
}
function addLinkHit(id)
{
	var request=new ajax();
	if(!request)
	{
		return;
	}
	var url="/link/addHit.asp?id="+id+"&rnd="+Math.random();
	request.open("get",url,true);
	request.send();
}
var ajax=function()
{
	var request=false;
	try
	{
		request=new XMLHttpRequest();
	}
	catch(microsoft)
	{
		try
		{
			request=new ActiveXObject("Msxml2.XMLHTTP");
		}
		catch(others)
		{
			try
			{
				request=new ActiveXObject("Micosoft.XMLHTTP");
			}
			catch(failed)
			{
				request=false;
			}
		}
	}
	return request;
}
</script>

