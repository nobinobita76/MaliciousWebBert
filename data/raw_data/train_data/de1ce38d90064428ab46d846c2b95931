<script type="text/javascript">
function kmss_onsubmit(){
	var loginInput = "请输入用户名和密码";
	if(document.forms[0].j_username.value==""){
		alert(loginInput);
		document.forms[0].j_username.focus();
		return false;
	}
	if(document.forms[0].password.value==""){
		alert(loginInput);
		document.forms[0].password.focus();
		return false;
	}
	document.getElementsByName("btn_submit")[0].disabled = true;	 
	encryptPassword();
	return true;
}
window.onload = function(){
	var j_username = document.getElementsByName('j_username')[0].value;
	if(j_username==""){ 
		document.getElementsByName('j_username')[0].focus();
	}
	else{
		document.getElementsByName('password')[0].focus();
	}
	$(document).keypress(function(e) {
		if (e.which == 13)
			$("form").submit();
	});
}
function encryptPassword(){ 
	var pw = document.forms[0].password.value;
	if(pw.charAt(0)=='['&&pw.charAt(pw.length-1)==']'){
		var idx = pw.indexOf("\u4445\u5320\u4d45");
		var key = pw.substring(1,idx);
		var pw = pw.substring(idx,pw.length-1);
		document.forms[0].j_password.value = desEncrypt(deEncrypt(pw,key));
		document.forms[0].password.value = "";
	}else{
		document.forms[0].j_password.value = desEncrypt(document.forms[0].password.value);
		var keyObj = DESKEY.get();
		document.forms[0].password.value = "[" + keyObj.key + "-" + keyObj.iv + document.forms[0].j_password.value + "]";		
	}
	
}
</script>

