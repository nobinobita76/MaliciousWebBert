function pageSizeAction(picBoxId, seatBoxId){
	$("#"+picBoxId).focusImageFade({
		seatBoxId : seatBoxId
	});
	
	var windowWidth = function(){
			return $("body").width();
		},
		setPicBoxSize = function(){
			var move = (1900-windowWidth())/2,
				move2 = (windowWidth()-940)/2;
			$("#"+picBoxId).parent().width(windowWidth());
			$("#"+picBoxId).css("left", -move+"px");
			$("#"+seatBoxId).css("right", move2+"px");
		};
	setPicBoxSize();
	$(window).resize(function(){
		setPicBoxSize();
	});
}
pageSizeAction("focus_pic", "focus_seat");
</script>
	


