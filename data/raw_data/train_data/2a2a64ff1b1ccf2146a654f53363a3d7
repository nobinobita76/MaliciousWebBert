       <script type="text/javascript">
				jQuery(function(jq){
					jq("?p=sele-id").hover(function(){
						jq(this).find("ul").show();
					},function(){
						jq(this).find("ul").hide();
					});
					
					var searchId = jq("?p=logo-intro .search-txt");
					var searchValue = searchId.val();
					searchId.focus(function(){
						jq(this).val("").css("color","?p=333");
					}).blur(function(){
						if(jq(this).val() == ""){
							jq(this).val(searchValue).css("color","?p=C7C7C7");
						} 
					});
				});
				</script> 
