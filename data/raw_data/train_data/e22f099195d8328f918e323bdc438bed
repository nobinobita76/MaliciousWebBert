   <script type="text/javascript">
$(".tabPage").find("li a").each(function(){
	if($(this).find("span").text().match(/\(([0-9]*)\)/ig) != null){
		$(this).find("span").css({"width":"auto", "min-width":"50px", "text-align":"center", "padding-right":"10px"}).parent().css({"width":"auto"});
	}
});
function displayTabPage()
{
	var oDiv = document.getElementById("tabPage");
	if(oDiv != null)
	{
		var aLi = oDiv.getElementsByTagName("li");
		for(i = 0; i < aLi.length - 1; i++)
		{
			if(aLi[i].style.display == "none")
			{
				aLi[i].style.display = "inline";
			}
		}
		aLi[aLi.length - 1].style.display = "none";
	}
}
</script> 
