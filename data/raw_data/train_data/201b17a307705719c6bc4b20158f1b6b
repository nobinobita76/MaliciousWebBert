  <script language="javascript"> 

var obj = document.getElementById("frameContent");  //获取内容层 
var pages = document.getElementById("pages");         //获取翻页层 
var pgindex=1;    
                      //当前页 
window.onload = function()                             //重写窗体加载的事件 
{ 
    var allpages = Math.ceil(parseInt(obj.scrollHeight)/parseInt(obj. offsetHeight));//获取页面数量 
	 //alert(parseInt(obj.scrollHeight)/parseInt(obj. offsetHeight)); 
    //pages.innerHTML = "<b>共"+allpages+"页</b>";     //输出页面数量 
	pages.innerHTML += "<li class=\"prev-li\"> <a href=\"javascript:gotopage('-1');\"><i></i></a></li>" 
    for (var i=1;i<=allpages;i++){ 
	if(i == 1){
        pages.innerHTML += "<li><a class=\"current\"  href=\"javascript:showPage('"+i+"');\">"+i+"</a></li> "; 
		}else{
		pages.innerHTML += "<li><a  href=\"javascript:showPage('"+i+"');\">"+i+"</a></li> "; 
		}
//循环输出第几页 
    } 
    pages.innerHTML += "     <li class=\"next-li\"> <a href=\"javascript:gotopage('1');\"><i></i></a></li>" 
} 
function gotopage(value){ 
try{ 
 value=="-1"?showPage(pgindex-1):showPage(pgindex+1); 
 }catch(e){ 
  
 } 
} 
function showPage(pageINdex) 
{ 
   
   
	  var allpages = Math.ceil(parseInt(obj.scrollHeight)/parseInt(obj. offsetHeight));
	 
	  var cc = '';
	cc += "<li class=\"prev-li\"> <a href=\"javascript:gotopage('-1');\"><i></i></a></li>" 
    for (var i=1;i<=allpages;i++){ 
	if(pageINdex == i){
        cc += "<li><a class=\"current\"  href=\"javascript:showPage('"+i+"');\">"+i+"</a></li> "; 
		}else{
		 cc += "<li><a   href=\"javascript:showPage('"+i+"');\">"+i+"</a></li> "; 
		}
//循环输出第几页 
    } 
   cc += " <li class=\"next-li\"> <a href=\"javascript:gotopage('1');\"><i></i></a></li>" 
	 $('#pages').html(cc);
	  window.onload = function(){ 
 var oTop = document.getElementById("to_top");
 var screenw = document.documentElement.clientWidth || document.body.clientWidth;
 var screenh = document.documentElement.clientHeight || document.body.clientHeight; 
 oTop.style.left = screenw - oTop.offsetWidth +"px"; 
 oTop.style.top = screenh - oTop.offsetHeight + "px";
 window.onscroll = function(){
   var scrolltop = document.documentElement.scrollTop || document.body.scrollTop;
   oTop.style.top = screenh - oTop.offsetHeight + scrolltop +"px"; 
  }  
 oTop.onclick = function(){ 
  document.documentElement.scrollTop = document.body.scrollTop =0;
  }
} 
	 jQuery("html,body").animate({'scrollTop':250},800)
    obj.scrollTop=(pageINdex-1)*parseInt(obj.offsetHeight);                     //根据高度，输出指定的页 
    pgindex=pageINdex; 
} 

	//获得点击赞
function subbtn(obj,ding)
{  
              var dingobj= 'zan'+obj;
			  var ding1 = ding+1;
			 
		    $.getJSON('index.php?s=/Home/Index/ding/commentid/'+obj, function(data){
			//  layer.msg(data);exit;
				if(data == "1") {
					  layer.msg('英雄略见所同，谢谢您的赞同');
					$('#'+dingobj).html(ding1);
			}
			if(data == '3') {
					  layer.msg('您已经赞过了,一个人只能赞一次额，或者明天再来');
			}
			if(data == '0') {
					  layer.msg('操作失败');
			}
				});
		}
		
//发布评论
	 $("#fabu").click(function(){
		    var content = $("#pinglun_textarea").val();
			var qid = 901;
		var name = $("#username").val();
			$.ajax({
             url:'index.php?s=/Home/Index/pinglun',
             type: 'POST',
             dataType: 'json',
             data: {qid:qid,content:content,name:name},
             success:function(r){
                     if (r.status==1) {
					   layer.msg(r.info);
                       //$('#comment').html(r.info);
					   document.location.href='index.php?s=/Home/Index/news/id/'+qid;
                      }else{
					    layer.msg(r.info);
					  }
            }
         });

		});
		
		
		 $("#cloadmore").click(function(){
		   var qid = 901;
		    var pagenum = $('#pagenum').val();
		    $("#cloadmore").html("加载中");
			$.ajax({
             url:'index.php?s=/Home/Index/morepinglun',
             type: 'POST',
             dataType: 'json',
             data: {qid:qid,statnum:pagenum},
             success:function(r){
                     if (r.status==1) {
					 $(".jiazaimore ul:last").after(r.info);
                       $("#pagenum").val(r.num);
					   
	                     $("#cloadmore").html("加载更多");
			
                      }else{
					 $("#cloadmore").html("没有更多了");
					  }
            }
         });

		});
		//获得回复的框
		$(function () {

        var mask_theme = $('.theme-popover-mask');
        var close = $('.close');
        var commMask=$('.commMask');
        $('.close,.theme-popover-mask').click(function () {
            mask_theme.hide();
            commMask.hide();
        });
    })
	
     function   subbtn1(obj) {
	 
	$('.theme-popover-mask').show();
    $.ajax({
             url:'index.php?s=/Home/Index/huifushouw',
             type: 'POST',
             dataType: 'json',
             data: {qid:obj},
             success:function(r){
                     if (r.status==1) {
					 $(".huifushouw").html(r.info);
                     $("#eeeee").html(r.name);
					   $("#qidii").val(obj);
					 
                      }else{
					 $(".huifushouw").html("还没有回复额~立即回复他把");
					  $("#eeeee").html(r.name);
					   $("#qidii").val(obj);
					  }
            }
         });
    $('.commMask').show();
        }
		
		
