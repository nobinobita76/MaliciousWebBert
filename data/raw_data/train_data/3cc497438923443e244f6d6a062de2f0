$(function() {
    var offset = $("#sidenav").offset();
    var topPadding = 47;
    var noscrollHight = 480;
    if(offset){
        $(window).scroll(function() {
            if ($(window).scrollTop() > offset.top + noscrollHight) {
                $("#sidenav").css("position","absolute");
                $("#sidenav").stop().animate({top: $(window).scrollTop() - offset.top + topPadding},500);
            } else {
                $("#sidenav").stop().animate({top:0},1000,function(){$("#sidenav").css("position","");});
            };
        });
    }
});
</script>
