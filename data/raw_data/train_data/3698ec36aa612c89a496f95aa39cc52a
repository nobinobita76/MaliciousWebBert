<script type="text/javascript">

/* 用户登录 */
function cnfolLogin(data) {
    if($.trim(C.G(data[0][0]).value)=='') {
        C.G(data[0][0]).className = data[0][3];
        C.G(data[0][1]).className = data[0][2];
        C.G(data[0][1]).innerHTML = '请输入您的帐号！';
        return false;
    }
    if($.trim(C.G(data[1][0]).value)=='') {
        C.G(data[1][0]).className = data[1][3];
        C.G(data[1][1]).className = data[1][2];
        C.G(data[1][1]).innerHTML = '请输入您的密码！';
        return false;
    }
	C.G('LoginForm').submit();
}

function login() {
    var $login_arr = [['username_d','error','Rsp No','WrtErr'],['password_d','error','Rsp No','WrtErr'],['LoginForm']];
    cnfolLogin($login_arr);
}

function ClsErr(){
	document.getElementById('error').className = 'Rsp';
	document.getElementById('username_d').className = '';
	document.getElementById('password_d').className = '';
}


</script>
