<script type="text/javascript">
var diffMonth = 24;
var groupID = 7533;
var isRenew = 0;

GetPrice(1);
function GetPrice(months) {
	var userID="0";
	var userGolden;
	var userMoney;
	if(parseInt(userID)>0)
	{
		userGolden="0";
		userMoney="0";
	}
	else
	{
		userGolden=0;
		userMoney=0;
	}
	//alert(accMul(userMoney,100));
	userGolden=parseInt(userGolden);
	userMoney=parseInt(accMul(userMoney,100));
	
	$('#buyMonthsinfo').html("");
	$.getJSON(baseUrl+'group/Monthprice?groupID='+groupID+'&months='+months+'&jsoncallback=?', function(data) {
		$('#buyMonthsinfo').html(data.info);
		
		buymoney=accMul($('#buyMoney').html(),100);
		//buymoney=(data.Price)*100;
		
		buymoney=parseInt(buymoney);
		if(userGolden>=buymoney)
		{
			$('.BePay').css('display','none');
			$('#how_money').html('');
		}
		else
		{
			$('.BePay').css('display','block');
			if($('#Moneychk').is(':checked'))
			{
				var totalMoney=userGolden+userMoney;
				//alert(totalMoney);
				//alert(buyMoney);
				if(totalMoney>=buymoney)
				{
					$('#Moneychk').val('buyMoney');
					$('#how_money').html('');
				}
				else
				{
					
					
					$('#Moneychk').val('buyMoney');
					blance=(buymoney-totalMoney)/100;
					
					var msg='您还需要支付'+blance+'元';
					$('#Moneychk').val('buyMoney');
					$('#how_money').html(msg);
				}
			}
			else
			{
				
				blance=(buymoney-userGolden)/100;
				$('#Moneychk').val('nobuyMoney');
				var msg='您还需要支付'+blance+'元';
				$('#how_money').html(msg);
			}
			
		}
		
	});
}
function payMoneyChk(obj)
{
	var buyMoney,userMoney,userGolden,userID;
	buyMoney=parseInt(accMul($('#buyMoney').html(),100));
	userID="0";
	if(parseInt(userID)>0)
	{
		userGolden="0";
		userMoney="0";
	}
	else
	{
		userGolden=0;
		userMoney=0;
	}
	userGolden=parseInt(userGolden);
	userMoney=parseInt(accMul(userMoney,100));
	if($(obj).is(':checked'))
	{
		$('#Moneychk').val('buyMoney');
		if(userMoney>=buyMoney)
		{
			$('#how_money').html('');
		}
		else
		{
			var totalMoney=userGolden+userMoney;
			if(totalMoney<buyMoney)
			{
				buymoney=(buyMoney-userGolden-userMoney)/100;
				var msg='您还需要支付'+buymoney+'元';
				$('#how_money').html(msg);
			}
			else
			{
				$('#how_money').html('');
			}
			
		}
	}
	else
	{
		$('#Moneychk').val('nobuyMoney');
		buymoney=(buyMoney-userGolden)/100;
		var msg='您还需要支付'+buymoney+'元';
		$('#how_money').html(msg);
	}
}
function check_join() {
	var month = $("select[name='buyMonths']").attr("value");
	$("#btncheckJoinGroup").attr("disabled", true);

	$.ajax({
		url:baseUrl + "group/check_buy.html?groupID=" + groupID + "&month=" + month,
		type:"get",
		dataType:"json",
		success:function(result)
		{
			if (errCodeSuccess != result["errorCode"])
			{
				//是否为余额不足?
				if ("5002" == result["errorCode"])
				{
					Dialog("WBg", "noMoney", $("#btnJoinGroup").get(0));
					Dialog.Close("WBg", "Renew");
				}else if ('20003' == result['errorCode'])
				{
					Dialog.Close("WBg", "Renew");
					Dialog("WBg", "LoginDialog");
				}
				else if('20017'==result['errorCode'])
				{
					alert(result.jumpUrl);
					return;
					//self.location.href = result['jumpUrl'];
					
				}
				else
				{
					showErrorMsg(result["errorDescription"], 1, $("#btnJoinGroup").get(0));
					Dialog.Close("WBg", "Renew");
				}
				
				return;
			}

			$("#tip_price").html(result["price"]/10+"元");
			Dialog.Close("WBg", "Renew");
			$("#btncheckJoinGroup").attr("disabled", false);
			Dialog("WBg", "tip");
		}
	});

}

function joinGroup()
{
	var month = $("select[name='buyMonths']").attr("value");
	var autoRenew = $("#autoRenew").attr("checked");
	var isuseMoney=$('#Moneychk').val();
	if (autoRenew)
		autoRenew = '1';
	else
		autoRenew = '0';
	$("#btnJoinGroup").attr("disabled", true);
	$("#btnJoinGroup").attr("display", 'none');
	$.ajax({
		url:baseUrl + "group/buygroup.html?groupID=" + groupID + "&month=" + month + "&isRenew=" + isRenew + "&isuseMoney="+isuseMoney+"&autoRenew=" + autoRenew,
		type:"get",
		dataType:"json",
		success:function(result)
		{
			if (errCodeSuccess != result["errorCode"])
			{
				//是否为余额不足?
				if ("5002" == result["errorCode"])
				{
					Dialog("WBg", "noMoney", $("#btnJoinGroup").get(0));
					Dialog.Close("WBg", "tip");
				}else if("5011" == result["errorCode"])
				{
					showErrorMsg("您好,该商品目前仅支持1-24个月的购买及续费,你还能续费" + result.data[0].BuyMonth + "个月,请重新选择!", 1, $("#btnJoinGroup").get(0));
				}else if ('20003' == result['errorCode'])
				{
					Dialog.Close("WBg", "tip");
					Dialog("WBg", "LoginDialog");
					/*
					alert('请先注册或登录!');
					self.location.href = 'https://passport.cnfol.com/login?rt=aHR0cDovL3ZpcC5nLmNuZm9sLmNvbS83NTMzLGpvaW4=';
					*/
				}
				else if('20017'==result['errorCode'])
				{
					self.location.href = result['jumpUrl'];
				}
				else
				{
					showErrorMsg(result["errorDescription"], 1, $("#btnJoinGroup").get(0));
					Dialog.Close("WBg", "tip");
				}
				$("#btnJoinGroup").attr("disabled", false);
				return;
			}
			diffMonth -= parseInt(month);
			isRenew = "1";
			if ("1" == autoRenew)
			{
				$("#sucBuy_content").html("您已购买成功");
			}
			Tn.SetCookie("cookie[passport][money]", result['Golden'], 7, "/", ".cnfol.com");
			C.G("GoldNum").innerHTML = "(" + result['Golden'] + ")";
			Dialog.Close();
			Dialog("WBg", "sucBuy", $(".vipImgR4").get(0));
			setTimeout("conter_location()",1000);
		}
	});
}

function conter_location() {
	var objId = "suc_time_num";
	var num = $("#suc_time_num").html();
	var num = parseInt(num);
	if (num > 0)
	{
		document.getElementById(objId).innerHTML = --num;
		setTimeout("conter_location()",1000);
	}
	else {
		window.location.href = '/shaoyevip';
	}
}

