        SewisePlayer.onPlayTime(function(time, id){
          var playtime = parseInt(time);
          var duration = parseInt(SewisePlayer.duration());
          // console.log(playtime + ' / ' + duration);
          // 循环播放
          if (playtime == duration) {
            loop++;            
            if (loop == play.length) {
              loop = 0;
              SewisePlayer.toPlay(play[loop], '【醉美三八女神节】吉林省九台阳光艺术团醉情歌表演个人版', 0, false);
              setTimeout(function(){
                SewisePlayer.doPause();
              },200);              
            }else{
              SewisePlayer.toPlay(play[loop], '【醉美三八女神节】吉林省九台阳光艺术团醉情歌表演个人版', 0, true);
            }
          }
          // 循环播放---
        });
        SewisePlayer.onPause(function(name){
        });
