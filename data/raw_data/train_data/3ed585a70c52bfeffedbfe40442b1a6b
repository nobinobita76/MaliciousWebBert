    <script>
     var system = {
        win: false,
        mac: false,
        xll: false
    };
    //检测平台
    var p = navigator.platform;
    system.win = p.indexOf("Win") == 0;
    system.mac = p.indexOf("Mac") == 0;
    system.x11 = (p == "X11") || (p.indexOf("Linux") == 0);
    //跳转语句，如果是手机访问就自动跳转到wap.baidu.com页面
    if (system.win || system.mac || system.xll) {
            window.auth = $.cookie('auth');
            if (window.auth) {
              $('.header-operator').hide();
              $('.header-user-center').show();
              if($.cookie('truename')){
                $('#user').text(decodeURIComponent($.cookie('truename')));
              }else{
                 $('#user').text('99用户');
              }
              // else{
              //   $('.header-operator').hide();
              //   $('.header-user-center').hide();
              // }
          }
    }
    //验证是否为移动端
    if((isApp||isiOS||isWechat||AndroidVersion)&&(!system.mac)){
      $('.header-operator').hide();
      $('.header-user-center').hide();
    }


      var greeting, hour = (new Date()).getHours();
      if(hour>=6 && hour<12){
          greeting="上午好!";
      }else if(hour>=12 && hour<18){
          greeting="下午好!";
      }else if(hour>=18 && hour<23){
          greeting="晚上好!";
      }else{
          greeting="晚上好!";
      }
      $('#tips').text(greeting);
    </script>


