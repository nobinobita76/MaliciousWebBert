    <script>

    //验证是否登录
    function js2app_passport(message) {
      $.cookie
        if(!$.cookie('auth')){
            if(confirm(message)){
                 if(!isApp){
                    redirect_login();
                }else{
                    try{
                        window.gcw.passport();//去app登录
                    }catch(e){
                        redirect_login();
                    }
                }
            }else{
                 // 点击取消后的回调函数
                 console.log('esc');
            }

            return false;
        }else{
            return true;
        }
    }

//去qq微信登录
    function redirect_login() {
        location.href = 'http://user.999d.com//plugin/login?code=' + (isWechat ? 'wxapp' : 'qq') + ' &redirect=' + encodeURIComponent(EVENTFILEURL);
    }
    </script>
