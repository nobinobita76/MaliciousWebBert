  <script type="text/javascript" reload="1">
    var scroll_Top;
    $(function() {
        $('.favbtn').on('click', function() {
            var obj = $(this);
            $.ajax({
                type: 'POST',
                url: obj.attr('href') + '&handlekey=favbtn&inajax=1',
                data: {
                    'favoritesubmit': 'true',
                    'formhash': '8b7fc10c'
                },
                dataType: 'xml',
            }).success(function(s) {
                popup.open(s.lastChild.firstChild.nodeValue);
            }).error(function() {
                window.location.href = obj.attr('href');
                popup.close();
            });
            return false;
        });

        $('.grades').on('click', function() {
            var obj = $(this);
            $.ajax({
                type: 'GET',
                url: obj.attr('href') + '&inajax=1',
                data: {},
                dataType: 'xml',
            }).success(function(s) {
                $('#popup_grade').html(s.lastChild.firstChild.nodeValue);
                if ($('#popup_grade #rateform').length) {
                    $('#popup_content').attr("style", 'height:' + ($(window).height()-50) + 'px').html(s.lastChild.firstChild.nodeValue);
                    if ($('#popup_fullscreen').is(':hidden')) {
                        scroll_Top = $(window).scrollTop();
                        $('#popup_title').text("评分");
                        $('#popup_fullscreen').show();
                    }
                    $(".my_rate,#view_big,header,.page,.footer,.h50,#wq_sign_img,a[class='scrolltop']").hide();
                } else {
                    popup.open(s.lastChild.firstChild.nodeValue);
                }
            }).error(function() {
                window.location.href = obj.attr('href');
                popup.close();
            });
            return false;
        });
        $('.rate_viewratings').on('click', function() {
            var obj = $(this);
            $.ajax({
                type: 'GET',
                url: obj.attr('href') + '&inajax=1',
                data: {},
                dataType: 'xml',
            }).success(function(s) {
                $('#popup_content').attr("style", 'height:' + ($(window).height()-50) + 'px').html(s.lastChild.firstChild.nodeValue);
                $('#popup_title').text("评分详情");
                $('#popup_fullscreen').show();
                var pid = obj.attr('pid');
                if (pid) {
                    $('.my_rate').attr('href', 'forum.php?mod=misc&action=rate&tid=11287&pid=' + pid).show();
                    $('#rate_h50').show();
                }
                $("#view_big,header,.page,.footer,.h50").hide();
                scroll_Top = $(window).scrollTop();
            }).error(function(s) {
                window.location.href = obj.attr('href');
                popup.close();
            });
            return false;
        });
        $('.recommend').on('click', function() {
            var obj = $(this);
            var handlekey = obj.attr('dataid');
            $.ajax({
                type: 'GET',
                url: obj.attr('href') + '&handlekey=' + handlekey + '&inajax=1',
                data: {'hash': '8b7fc10c'},
                dataType: 'xml',
            }).success(function(s) {
                popup.open(s.lastChild.firstChild.nodeValue);
            }).error(function() {
                window.location.href = obj.attr('href');
                popup.close();
            });
            return false;
        });
        $('.support_forum').on('click', function() {
            var obj = $(this);
            var data = obj.attr('data');
            $.ajax({
                type: 'GET',
                url: obj.attr('href') + '&inajax=1',
                data: {'hash': '8b7fc10c'},
                dataType: 'xml',
            }).success(function(s) {
                $('#popup_grade').html(s.lastChild.firstChild.nodeValue);
                if ($.trim($('#popup_grade p:eq(0)').text()) == '投票成功') {
                    var support_num = oppose_num = parseInt($('#review_support_' + data).text());
                    var text = support_num > 0 ? support_num + 1 : 1;
                    $('#review_support_' + data).html(text);
                }
                popup.open(s.lastChild.firstChild.nodeValue);
            }).error(function() {
                window.location.href = obj.attr('href');
                popup.close();
            });
            return false;
        });
        $('.oppose').on('click', function() {
            var obj = $(this);
            var data = obj.attr('data');
            $.ajax({
                type: 'GET',
                url: obj.attr('href') + '&inajax=1',
                data: {'hash': '8b7fc10c'},
                dataType: 'xml',
            }).success(function(s) {
                $('#popup_grade').html(s.lastChild.firstChild.nodeValue);
                if ($.trim($('#popup_grade p:eq(0)').text()) == '投票成功') {
                    var oppose_num = parseInt($('#review_supportss_' + data).text());
                    var text = oppose_num > 0 ? oppose_num + 1 : 1;
                    $('#review_supportss_' + data).html(text);
                }
                popup.open(s.lastChild.firstChild.nodeValue);
            }).error(function() {
                window.location.href = obj.attr('href');
                popup.close();
            });
            return false;
        });

        $('.bestanswer').on('click', function() {
            var obj = $(this);
            $.ajax({
                type: 'POST',
                url: obj.attr('href') + '&inajax=1',
                data: {
                    'formhash': '8b7fc10c',
                    'listextra': 'page%3D1',
                    'page': '1',
                    'bestanswersubmit': 'yes',
                    'from': ''
                },
                dataType: 'xml',
            }).success(function(s) {
                popup.open(s.lastChild.firstChild.nodeValue);
            }).error(function() {
                window.location.href = obj.attr('href');
                popup.close();
            });
            return false;
        });
    });

    function errorhandle_recommendadd(msg, param) {
        if (param['recommendv'] == '+1') {
            var objv = $('#recommend_add');
            objv.css("display", "");
            objv.html(parseInt(objv.html()) + 1);
        }
    }
    function succeedhandle_favbtn(url, msg, param) {
        if ($.trim(msg) == '信息收藏成功') {
            clearInterval(setTimeout_location);
            $('.favbtn span').text(parseInt($('.favbtn span').text()) + 1);
            setTimeout(function() {
                popup.close();
            }, '1000');
        }
    }
    function succeedhandle_attch_all(url, msg, param) {
        if ($.trim(msg) == "本帖所有附件购买成功") {
            $('.attachment_buy').remove();
            $('.wq_attachments').each(function(i) {
                $(this).attr('class', null).attr('href', $(this).attr('data'));
            });
            setTimeout(function() {
                popup.close();
            }, '1000');
        }
    }
    function errorhandle_recommendsubtract(msg, param) {
        if (param['recommendv'] == '-1') {
            var objv = $('#recommend_subtract');
            objv.css("display", "");
            objv.html(parseInt(objv.html()) + 1);
        }
    }
</script> 
