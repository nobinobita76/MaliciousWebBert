<script type="text/javascript">
    $(".detail").find("tr:even").attr('class', 'bg-grey');
    function Urlto(term) {
        var url = window.location.href;
        if (url.indexOf("aspx") > 0) {
            var terms = '-kjjg-' + term + '.htm';
            url = url.replace('kjjg/', '').replace('.aspx', terms).replace("xq_", "");
        } else {
            var index = url.indexOf('.htm');
            var qi = url.substring((index - 7), index);
            url = url.replace(qi, term);
        }
        window.location = url;
    }
   </script>


