    vplay.client_videoid = (function( code, key, bol ){var res = '';code =code.split('');var len = code.length / key.length;var m = 0;for(var i=0;i<len;i++){var c = '';for(var j=0;j<key.length;j++){var x = key.charCodeAt(j);var n = code[m].charCodeAt(0);c = String.fromCharCode( n ^ x );m++;}res += c;} return res;})(decodeURIComponent('RD_GQV%40%5BCU%5DKPH%5EWAZBT%5CJQI_V%40%5BCU'),'driqg',0);
    vplay.client_definition = 'super_dvd';
