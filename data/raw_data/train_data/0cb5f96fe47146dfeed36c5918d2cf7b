    /**
     * 通用弹层
     * @param txt
     */
    var popFun = function(txt){
        $('#alert').find('p').html(txt);
        $('#alert,#pop-bg').show();
        $('[data-close="true"]').on('click',function(){
            $('#pop-bg,#alert').hide()
        })
    }
</script>
