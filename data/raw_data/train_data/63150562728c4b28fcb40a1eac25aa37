     <script>
    $('#addComment').submit(function(){
    	if(!ue.hasContents()){
    		alert('您还没输入内容呢！');return false;
    	}
    	if($('#notLogin').text() != '' && !$('#anonymouse').is(':checked')){
    		alert('登陆后再发表吧！');ShowLoginBox(1);return false;
    	}
    	$.post('/index.php?g=Web&m=Index&a=addComment', $(this).serialize(), function(data){
    		alert($.parseJSON(data).info);
    		if($.parseJSON(data).status == 1){
    			location.reload();
    		}else if($.parseJSON(data).status == 2){
    			ShowLoginBox(1);
    		}
    	});
    	return false;
    });
    </script> 
