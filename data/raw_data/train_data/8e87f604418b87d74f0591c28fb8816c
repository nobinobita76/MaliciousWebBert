$("#info_top_con ul").find("li:eq(1) span").each(function(){if(!!$(this).html())$(this).addClass("yx").css({"background-color":""+("#"+rancolor())+""}).mouseover(function(){$(this).css({"background-color":"#1F78D0"})}).mouseout(function(){$(this).css({"background-color":"#"+rancolor()})})});
function rollto(v){
this.roll=function(s){$('html').animate({scrollTop:$(".info_box").eq(s).offset().top},'fast');$('body').animate({scrollTop:$(".info_box").eq(s).offset().top},'fast');}
switch(v){
case "酒店介绍":this.roll(0);break;
case "酒店图片":this.roll(1);break;
case "客人点评":this.roll(2);break;
case "交通地图":this.roll(3);break;
case "酒店问答":this.roll(4);break;
case "周边酒店":this.roll(5);break;}
}


var maplet = null,mData=[[37160,'张家界迎宾宾馆','http://p.zhuna.cn//Hotel_Images/37160/160x120_1a2619b5-767f-455f-9090-e7517bf3ab65.png','HTDFRFZUEWVRG','张家界火车站边即新汽车站车辆入站口正对面（近张家界中心汽车站）','98','9'],[41639,'张家界东方宾馆','http://p.zhuna.cn//Hotel_Images/41639/160x120_9c5e498c-40cc-42dc-b35a-99a60d2adcd8.jpg','HTDGCUZUEWVST','张家界市永定区官黎坪办事处（近张家界中心汽车站）','88','9'],[43165,'张家界木头人客栈','http://p.zhuna.cn//Hotel_Images/43165/160x120_73bcaab5-d598-4da1-ae0d-5061242e48c3.jpg','HTDFIHZUEWUWR','张家界市永定区汽车站旁边（近汽车站）','98','9'],[46960,'张家界驴途客栈','http://p.zhuna.cn//Hotel_Images/46960/160x120_c611e6e7-a403-4c05-8271-c3996b3af20d.jpg','HTDFSRZUEWVDD','张家界永定区新火车站、中心汽车站、麦当劳左边直进','78','9'],[50734,'张家界惟景商务酒店（中心汽车站...','http://p.zhuna.cn//Hotel_Images/50734/160x120_5e73c5ad-eca9-4084-bc23-f0526eb54925.jpg','HTDFCWZUEWVAE','张家界市汽车站斜对面100米（彭家铺小区内）','149','9'],[104023,'张家界好运来宾馆','http://p.zhuna.cn//images/nophoto_1.gif','HTDFJDZUEWVRC','张家界永定区官黎路（中心汽车站对面）','88','9'],[115920,'张家界中天公寓','http://p.zhuna.cn//Hotel_Images/115920/160x120_2d9f7371-e2e4-481a-9ba8-44da4147078a.jpg','HTDFFTZUEWUVS','官黎路新火车站旁','88','9'],[117656,'张家界微公馆连锁酒店','http://p.zhuna.cn//Hotel_Images/117656/160x120_abb78800-bcba-4bd0-90b3-2694db7539bb.jpg','HTDFSUZUEWUWC','永定区官黎路彭家铺小区D11栋（火车站、汽车站斜对面）','128','9']],mks=new Array(),polyline;
function initMap(){
	maplet = new Maplet("DP_Map_Show");
	maplet.centerAndZoom(new MPoint('北京市'),8);
	maplet.addControl(new MStandardControl());
	maplet.showOverview(false);	
	var mk=new MMarker(   
				new MPoint("HTDFHTZUEWVGJ"),   
				new MIcon("images/0.gif",34,36), 
				null,
				new MLabel("张家界遇见特色主题客栈",10,-8,80)
			);
	maplet.addOverlay(mk);
	mks.push(mk)
	for(var i=0;i<mData.length;i++){
		var mk=new MMarker(   
				new MPoint(mData[i][3]),   
				new MIcon("images/No/"+(i+1)+".gif",17,22),   

				new MInfoWindow(mData[i][1],'<img src="'+mData[i][2]+'" width="86" height="68" align="left" style="margin:5px;">名称：'+mData[i][1].substring(0,12)+'<br>价格：最低'+mData[i][5]+'元起订<br>地址：'+mData[i][4].substring(0,25)+'<br><a href="hotelinfo.asp?id='+mData[i][0]+'&types=0" target="_blank">了解酒店详情>></a>'),
			
				null); 
		maplet.setIwStdSize(300,150); 
		maplet.addOverlay(mk);
		mks.push(mk);
	}
	maplet.setAutoZoom();
};
function showhotel(id,i){
	maplet.removeOverlay(polyline); 
	mks[i].openInfoWindow();
	for(var i=0;i<mData.length;i++){
		if(mData[i][0]==id){
			    var brush = new MBrush("blue");brush.stroke = 15;brush.overlap.enable = true;   
				polyline = new MPolyline([new MPoint("HTDFHTZUEWVGJ"),new MPoint(mData[i][3])],brush);
				polyline.setEditable(true);
				maplet.addOverlay(polyline); 
				mks.push(polyline);
				maplet.setAutoZoom();
		}
	}
}
</script>
 </body>
</html>