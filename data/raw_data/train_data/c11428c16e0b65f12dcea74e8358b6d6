<script type="text/javascript">
	function Support_Search() {
		var key = document.getElementById("support_ssUserText").value;
		doSearch(key);
	}

	function doSearch(keyvalue) {
		var url = null;
		if ((!keyvalue) || ($.trim(keyvalue) == '请输入关键词')
				|| keyvalue == '') {
			url = '/carriersearch/advancedSearch!advancedInit?searchModelType=elasticsearch&hedexLang=zh';
		} else {
			keyvalue = encodeURIComponent(keyvalue);
			url = '/carriersearch/?hedexLang=zh#origin=headSearch&resLang=zh&searchModelType=elasticsearch&searchString='
					+ keyvalue + '&sortType=Relevance';
		}
		window.location = url;
	}

	function Support_EnterClick(e) {
		var key = window.event ? e.keyCode : e.which;
		if (key == 13) {
			Support_Search();
		}
	}
</script>

