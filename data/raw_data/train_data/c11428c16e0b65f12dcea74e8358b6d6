<script language="javascript">  
var timeOut = 0;
    		$(document).ready(function() {
    			var alink = $("#alink").val();
    			var msgtype = $("#msgtype").val();
    			var msg = $("#msg").val();
    			alink = decodeURIComponent(alink);
    			var i = 0;
    			//3秒倒计时 方法
    			function timer(count){
    				timeOut = window.setInterval(function(){
    					var r = count - i;
    					if(r == 0){
    						location.href = alink;
    						clearInterval(timeOut);
    					}else{
    						$("#last").text(r);
    						i++;
    					}    					
    				},1000);
    			}
    			
        		if('autotips' == msgtype || 'autoerror' == msgtype || 'autosuccess' == msgtype || 'sendMess' == msgtype || 'autoAndClickTips' == msgtype){
        			timer(3);
        		}
        		$('.Jumpwindow a').click(function(){
        			clearInterval(timeOut);
        		});
        		if(msg ==null || msg == ""){
        			$(".JumpWord").css("margin-top","15px");
        		}
   			 });
	</script> 

