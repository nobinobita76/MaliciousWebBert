	<div id="layout_content" class="layout_content" style="min-height: 450px;box-sizing:content-box ! important;margin:0 auto;">
		

<input style="display:none" id="noTipsKnowledgeBase" value="noTips">
<input style="display:none" id="msgtype" value="tips">
<input style="display:none" id="alink" value="">
<input style="display:none" id="msg" value="Sup.Exception.SystemError">
<input style="display:none" id="blink" value="">
<span id="noException" style="display:none">系统异常</span>
<span id="returnBack" style="display:none">秒后返回上一层节点</span>


	<div style="height: 20px;"></div>
	
	<div class="spt_main">
		<div class="Jumpwindow">
			<div class="Jumppic">
				
				
				
					<img id="a" src="pages/messagetips/img/attention_icon.jpg">
				
				
			<div class="backTips">系统异常</div><div class="Jumpbottom"><button class="btn btn-danger" id="backHomeBtn">返回首页</button></div></div>
		</div>
	</div>
	<div class="clear" style="height: 20px;"></div>

    <script>
    var msgtips = "";
    var msgtype1 =$("#msgtype").val();
    var is3Second = ('autotips' == msgtype1 || 'autoerror' == msgtype1 || 'autosuccess' == msgtype1 || 'sendMess' == msgtype1 || 'autoAndClickTips' == msgtype1);
    var lang=$("#sysLang").val();
    //加入兼容性写法 3秒有使用跳转 10秒返回上一层

    if(lang == "zh"){
    	if(is3Second){
    		msgtips += '<div class="backTips"><span id="last">3</span>'+$("#noException").html()+'</div>';	
    	}
    	else if(!is3Second && history.length > 2){
    	  msgtips += '<div class="backTips">'+$("#noException").html()+'</div>';
    	  msgtips += '<div class="backTips"><span id="spanNumber">10</span>'+$("#returnBack").html()+'</div>'; 
    	}
    	else{
    	  msgtips += '<div class="backTips">'+$("#noException").html()+'</div>';
    	}
    }else{
    	if(is3Second){
        	msgtips += '<div class="backTips"><span id="last">3</span>&nbsp;'+$("#noException").html()+'</div>';
    	}
    	else if(!is3Second && history.length > 2){
    	  msgtips += '<div class="backTips">'+$("#noException").html()+'</div>';
    	  msgtips +='<div class="backTips">'+$("#returnBack").html()+" "+"<span id='spanNumber'>10</span>"+" seconds"+'</div>';
    	}else{
    	  msgtips += '<div class="backTips">'+$("#noException").html()+'</div>';
    	} 
    }
    msgtips += '<div class="Jumpbottom"><button class="btn btn-danger" id="backHomeBtn">返回首页</button></div>';
    $(".Jumppic").append(msgtips);
    //优化无资源节点页面
    if(!is3Second){
    	var start=10;
        noSourceRead(start);
    }
    
	$("#backHomeBtn").off("click").on("click",function(){
		window.location.href = "carrier";
	});
    function noSourceRead(start){
    	//尾部增加返回首页的按钮
    	timer=setInterval(function(){
    		$(".backTips #spanNumber").text(start);
    		start--;
    		if (start == 0){
    			clearInterval(timer);
                timer = 0;
    	  		/* var newHref=window.location.href.substring(0,window.location.href.lastIndexOf('/'));
    	  		window.location.href=encodeURI(newHref); */
    	  		if(history.length > 2){
    	  			history.go(-1);
    	  		}	
    	  		
    		}
    	},1000);
        window.onhashchange=function(){
            clearInterval(timer);
            timer = null;     	
        };
    }
    </script>

	</div>
