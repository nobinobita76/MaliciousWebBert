  <script type="text/javascript">
$('#nav a').each(function(){
	if ($(this).text()=="PPT课件") {
		$(this).css({'color':'#fff'}).parent().addClass("hover");
	}
})
refreshScore();
function refreshScore(){
	$.get( "/e/action/score.php?vtype=list&id=26233",{ "time": new Date().getTime() }, function( data ) {
		$("#like_100").html(data.like_100);
		$("#dislike_100").html(data.dislike_100);
		$("#score").html(data.score);
		$("#like_score").html(data.good);
		$("#dislike_score").html(data.bad);
		$(".like_width").css( "width", data.like_100+"%" );
		$(".dislike_width").css( "width", data.dislike_100+"%" );
	}, "json");
}
//顶踩
$(".like_or_dislike").click(function(){
	$.ajax({
		type: "GET",
		dataType: "json",
		data: { id : 26233 , classid : 160 ,dotop : $(this).attr('action')},
		url: "/e/public/digg/",
		beforeSend: function() {
		    //TODO
		}
	}).done(function( data ) {
		if ( 1 == data.success ) {
			if ('1' == data.action) {
				$("#like_score").html(data.good);
			}else if('0' == data.action){
				$("#dislike_score").html(data.bad);
			}
			refreshScore();//刷新分数
		}else if( 2 == data.success ){
			alert("您已经投过票了！");
		};
	}).fail(function() {
    	alert( "投票失败！" );
  	});
})
//报错

$(".feedback-btn").click(function(){
	var content=$("#feedback-text").val();
	$.ajax({
	url: "/e/enews/index.php",
	data: { id: 26233, cid: 0, enews: "AddError", classid: 160, errortext: ""+content+""},
	type: "POST",
	dataType: "json"
	}).done(function( data ) {
		if('1' == data.success){
		  	alert("谢谢反馈！");
		  	$('.feedback-form,#feedback-box').fadeOut();
		}else if('2' == data.success){
		  	alert("已经反馈过了！");
		  	$('.feedback-form,#feedback-box').fadeOut();
		}
	});
});
</script>  
