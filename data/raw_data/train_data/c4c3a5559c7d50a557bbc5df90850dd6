function getRelProducts(pname){
	var s = document.getElementById("productSelect")
	return $.ajax({
		type:"GET",
		url:baseurl + "service/product/detail/list",			  		  
		dataType:"jsonp",
		jsonp:"jsonp",
		data:{"siteCode":SITE_CODE,"productCode":pname},
		success: function(data){
			if(data.length > 0){
				for(var i = 0; i < data.length; i++){
					var sn = data[i].productTypeName?data[i].productTypeName:data[i].productCode
					s.options[i] = new Option(sn, data[i].productCode)
				}
				getFAQTypes(data[0].productCode)
			}else{
				s.options[0] = new Option($("#product_text").text(), pname)
				getFAQTypes($("#productSelect").val())
			}
			$(s).bind("change",changeProduct)
			$(s).selectbox('detach').selectbox('attach')
		},
		error:function(){}
	});
}
function changeProduct(){ 
	$("#productFaq,.pop_software ul,.pop_manuals ul,.help-filter-close ul").empty();
	$(".help-filter-close span").eq(0).click();
	$("#manual-list,#soft-list").attr("data","0");
	getFAQTypes($("#productSelect").val());
	setTimeout("getManuals($('#productdown'))",500);
	setTimeout("getSoft()",500);
}

function getFAQTypes(productCode){
	$("#cbg-help").hide();
	$("#productFaq *").remove();
	$.ajax({
		async:false, 
		type:"GET",
		url:baseurl + "service/faq/type/list",
		data:{"siteCode":SITE_CODE, "productCode": productCode},
		dataType:"jsonp", jsonp:"jsonp",
		success:function(data){
			var str = ""; 
			if(data.length > 0){
				var datafaqcode = "";
				for(var i =0;i<data.length;i++){
					datafaqcode += data[i].faqTypeCode+",";
					str = str +"<li data='0' class='item"+i+"' tcode='" +data[i].faqTypeCode+ "'><i class='g-ico g-ico-118'></i>"+data[i].faqTypeName+"</li>";
				}
				if(datafaqcode!=null&&datafaqcode.indexOf(",")>0){
					datafaqcode = datafaqcode.substring(0,datafaqcode.length-1);
				}
				//$("#faqTypeCode").val(datafaqcode);
				$("#productFaq").append(str);
				$("#cbg-help").show();
				setAnchor();
			}else{
				/*$("#productFaq").append("<h1>"+ LAG_CON_NO_RESULTS +"</h1>");*/
				$(".help-filter").css("display","none");
				$("#cbg-help").show();
			}
		},
		complete:function(){
			setCbgScroll()
			getFaqTypeDataList("all")
			$("#productFaq .faq_expand").click(function(){
				$(this).toggleClass("cbg-current")
				$("#productFaq a").not($(this)).removeClass("cbg-current")
				var $this_ul = $(this).next("ul")
				var $other_ul = $("#productFaq .cbg-faq-list").not($this_ul)
				$this_ul.slideToggle(function () {
					if($(this).css("display") == "block")	$('html,body').animate({ scrollTop: $(this).offset().top - 210 + 'px' });
      			})
				$other_ul.slideUp()
				$(".flist").click(function(){
					if($(this).attr("data") == 0){
						$(this).attr("data",1)
						f_data.mcCurPage = 1
						f_data.productCode = $("#productSelect").val()
						f_data.pageSize = 15
						f_data.queryMatch = "support:faqs@#" + $(this).attr("tcode")
						getFaqs(f_data,$(this))
					}
				});
			});
		},
		error:function(){}
	});	
}
function getFaqs(_data,obj){	
	$.ajax({
		async:false, 
		type:"GET",
		url:baseurl + faqToFourList,
		data:_data,
		dataType:"jsonp", 
		jsonp:"jsonp",
		success:function(data){
			var j_data = data.list[0]['alllist']
			if(data && j_data.length > 0){
				var f_type = ""
				var _l = ""
				for(var i = 0; i < j_data.length; i++){
					f_type = getFAQContentType(j_data[i].RESERVE_FIELD25)
					_l = "<li class='clearfix'><a class='a-1' href='"+returnVariable(j_data[i].DOC_URL)+"' target='_blank'><span class='fwb'>[" + f_type + "]" + getValue(j_data[i].DRETITLE) +"</a><span></span></li>";
					$(obj).find(".cbg-faq-list").show().prepend(_l)
				}
				if(data.list[0].totalNum > _data.mcCurPage * _data.pageSize){
					$(obj).find(".cbg-troubleshooting-expand").show()
				}else{
					$(obj).find(".cbg-troubleshooting-expand").hide()
				}
			}else{
				$(obj).find(".cbg-faq-list").show(LAG_CON_NO_RESULTS)
			}
		},	
		error:function(){$(obj).find(".cbg-faq-list").show(LAG_CON_NO_RESULTS)}
		
	});
}
function getMoreFaqs(obj){
	f_data.mcCurPage = $(obj).attr("page")
	var p = parseInt($(obj).attr("page")) + 1
	$(obj).attr("page",p)
	f_data.productCode = $("#productSelect").val()
	f_data.queryMatch = "support:faqs@#" +$(obj).parents(".flist").attr("tcode")
	var o = $(obj).parents(".flist")
	getFaqs(f_data,o)
}

function getFAQContentType(val){
	return val == "video" ? '视频' : '图文';
}
function getNewbieGuide(pname){
	$.ajax({
  		type: 'GET', 
  		async:false,
  		url:baseurl + "service/simulator/listinfo",
  		contentType: "application/x-www-form-urlencoded; charset=utf-8",
  		data:{"siteCode":SITE_CODE,"productCode":pname,"servicesType":"newproGuide"},
  		dataType: 'jsonp', 
  		jsonp: 'jsonp', 
        success: function(data) {
          	if(data.length>0 && data[0].returnCode == 0){
            	$("#newbie-guide,#cbg-newbie-guid").show()
            	$("#cbg-newbie-guid iframe").attr("src",data[0].url)
            }                  
        },
  	    error: function(){}
	});	
}
function getSimilator(pname){
	$.ajax({
		async:false, 
		type:"GET",
		url:baseurl + "service/simulator/info",
		data:{"siteCode":SITE_CODE,"productCode":pname},
		dataType:"jsonp",
		jsonp:"jsonp",
		success:function(data){
			if(data.url){
				$("#id_sim_parent").attr("src",data.url);
				$("#cbg-simulator,#is_simulator").show(); 
			}	
		},
		complete:function(){
			setAnchor()
			setCbgScroll()
		},
		error:function(){}
	});
}
function getFaultDebar(pname){
	$.ajax({
  		type: 'GET', 
  		async:false,
  		url: baseurl + "service/simulator/listinfo",  
  		contentType: "application/x-www-form-urlencoded; charset=utf-8",
  		data:{"siteCode":SITE_CODE,"productCode":pname,"servicesType":"faultDebar"},
  		dataType: 'jsonp', 
  		jsonp: 'jsonp', 
        success: function(data) {
          	if(data.length>0  && data[0].returnCode == 0){	
            	for(var i =0;i<data.length;i++){
            		$("#cbg-troubleshooting ul").append("<li><a target='_blank' href='"+data[i].url+"'>"+data[i].simulatorName+"</a></li>")
            	}
            	$("#fault_product,#cbg-troubleshooting").show();
            	$("#fault_product").parent("li").show();
            }else{
            	$("#fault_product,#cbg-troubleshooting").hide();
            	$("#fault_product").parent("li").remove();
            }                   
        },
        complete: function(){setCbgScroll()},
  	    error: function(){;setCbgScroll()}
	});
}
function getManuals(obj){	
	$(".pop_manuals ul").empty()
	$(obj).attr("data",1)
	m_data.ssUserText = $("#productSelect").val()
	m_data.queryMatch="support:manual" 
	isearchByProductCode("manual",1,m_data)
}
function getSoft(){	
	if(true){
		$(".pop_software ul").empty()
		m_data.ssUserText = $("#productSelect").val()
		m_data.queryMatch="support:software" 
		isearchByProductCode("software",1,m_data)
	}else{
		if($(".pop_software ul li").length > 0){
			$(".pop_software").show()
		}
	}
}
function isearchByProductCode(type,c,_data){
	$.ajax({
		type:"GET",
		async: false,
		url:isearchUrl,  
		dataType:"jsonp",
		jsonp:"jsonp",
		data:_data,
		success: function(data){		
			var _data = data.list[0].alllist
			if(type=="manual"){
				if(_data!=null && _data.length > 0){
					$("#productdown h2").eq(0).text(LAG_CON_MANUALS);
					addInfo(_data,".cbg-labeled-icon-list.doc")
				}else{
					$("#productdown h2").eq(0).hide();
				}	
			}else if(type=="software"){
				if(_data!=null && _data.length > 0){
					$(".wrap_firmware").show()
					$("#productdown h2").eq(1).text(LAG_CON_DOWNLOADS);
					addInfo(_data,".cbg-labeled-icon-list.down")
				}else{
					$("#productdown h2").eq(1).hide();
					if(c == '0'){
						$("#cbg-downloads,#downloads_product").remove()
					}
				}
			}
			setCbgScroll()
		},
		error: function(){}	
	});
}
$(".pop_div .close").click(function(){
	$(".pop_div").hide()
});
function setCbgScroll(){
  $('#cbg-support-menu').attr('data-fixed-scrolltop', $('#cbg-support-menu').offset().top - 55);
  $(window).on('scroll', function () {
    $('#cbg-support-menu a').each(function (i) { 
   		var sort = $(this).attr("sort")
   		for(var j = 0; j <  $(".cbg-navx").length; j++){
   			if($(".cbg-navx").eq(j).attr("sort") == sort){
   				var $section =  $(".cbg-navx").eq(j)
   				break
   			}	
   		}	
   	  if($section ){
   	  	 if ($section.length == 0) {
        	return;
      	 }
      	 var scrollTop = $(window).scrollTop();
      	 var offsetTop = $section.offset().top;
      	 if (scrollTop > offsetTop - 120 && scrollTop < offsetTop + 120) {     	
        	$('#cbg-support-menu li').removeClass('cbg-current').eq(i).addClass('cbg-current');
      	 }
   	  }	
    });
  });
  setTimeout(function () { $(window).trigger('scroll'); }, 1000);
  $('#cbg-troubleshooting-expand').click(function (e) {
    var $btn = $(this);
    var $list = $('#cbg-troubleshooting ul.cbg-faq-list-more');
    if ($btn.hasClass('cbg-expanded')) {
      $list.slideUp(function () {
        $btn.text($btn.data('expand-text')).removeClass('cbg-expanded');
      });
    }
    else {
      $list.slideDown(function () {
        $btn.text($btn.data('close-text')).addClass('cbg-expanded');
      });
    }
    e.preventDefault();
  }).attr('data-expand-text', $('#cbg-troubleshooting-expand').text());
}
function setAnchor(){
	var _u = window.location.href;
	var anchor = _u.substr(_u.lastIndexOf("#"))
	if(anchor == "#newpro"){
		$("#newPhone").click()
	}else if(anchor == "#sim"){
		$("#is_simulator").click()
	}else if(anchor == "#faq"){
		$("#help_product").click()
	}	
}
function goSupportList(index){
	if(index == 16739){
		var anchor = "#mp"
	}else if(index == 16741){
		var anchor = "#tb"
	}else if(index == 16769){
		var anchor = "#acc"
	}else if(index == 16770){
		var anchor = "#mbd"
	}else if(index == 16742){
		var anchor = "#smho"
	}else if(index == 16740){
		var anchor = "#wear"
	}
	window.location.href = $("#support-index").attr("href")+ anchor
}

$("#cbg-help").on("click", '.help-filter-close p span',function(){
	  var $this=$(this);
      $('#cbg-help .help-filter-close p span').removeClass('active');
      if(!$this.hasClass('active')) {
        $this.addClass('active');
      }
});
$("#cbg-help").on("click", '.help-filter li',function(){
	$('#cbg-help .help-filter-close p span').removeClass('active');
	  var $this=$(this);
	  var tcode = $this.attr("tcode");
	  var colsText = " <li class='"+$this.attr("class")+"' tcode='"+$this.attr("tcode")+"'><i class='g-ico g-ico-21'></i><span>"+$this.text()+"</span></li>";
	  $(".help-filter-close ul").append(colsText);
      var $thisClose = $('#cbg-help .help-filter-close li.'+$this.attr('class').split(' ')[0]);
      if (!$this.hasClass('active')) {
        $this.addClass('active');
      }else{
        $this.removeClass('active');
        $thisClose.remove();
      }
  	  searchQuerykey();
});
$("#cbg-help").on("click", '.help-filter-close li',function(){
      var $this=$(this);
      var $thisFilter = $('#cbg-help .help-filter li.'+$this.attr("class"));
      $thisFilter.removeClass('active');
      $this.remove();
	  searchQuerykey();
});
$("#cbg-help").on("click", '.help-search-submit',function(){
	chickSerchKey($("#serchkey").val());
});
$("#serchkey").keydown(function(event){
    if(event.which==13){
    	chickSerchKey($("#serchkey").val());
    }
});
var alllist;
var pagenum = 10;
var pageindex = 0;
var faqpagesize = 0;
 function getFaqTypeDataList(typecode){
	var faqcodelist = $("#faqTypeCode").val();
	var querycode = "";
	if(faqcodelist!=null&&faqcodelist.length>0){
		var list = faqcodelist.split(",");
		querycode = "support:faqs@#"+list[0];
		for(var n=1;n<list.length;n++){
			querycode+= "@-"+list[n];
		}
	}else{
		querycode = "support:faqs";
	}
	var skey = $("#serchkey").val()==$("#serchkey").attr("placeholder")?"":$("#serchkey").val();
	var param_faq = {}
	param_faq.siteCode = SITE_CODE;
	param_faq.APP_NUM = APP_NUM;
	param_faq.pageSize	= "10";
	param_faq.tagMatch = "same";
	param_faq.topFlag = "n";
	param_faq.isTop = 1;
	param_faq.mcCurPage = 1;
	param_faq.queryMatch = querycode;
	param_faq.ssUserText = skey;
	param_faq.productCode = $("#productSelect").val();
	$.ajax({ 
		async:false, 
		type:"GET",
		url: baseurl + faqToFourList,
		data:param_faq,
		dataType:"jsonp", 
		jsonp:"jsonp",
		success:function(data){		
			analysisData(data);
		},
		error:function(){}
	});		
} 
function getFaqTypeDataListType(typecode,mcCurPage){
	$("#faqTypeCode").val("")
	$("#productFaq li").removeClass("active")
	$(".help-filter-close ul,.help-detail-cont,.help-detail-pagenum").empty();
	var querycode = "support:faqs";
	if(typecode!=null&&typecode=="text"){
		querycode += "@@text";
	}
	if(typecode!=null&&typecode=="video"){
		querycode += "@@video";
	}
	    var param_faq = {}
		param_faq.siteCode = SITE_CODE;
		param_faq.APP_NUM = APP_NUM;
		param_faq.pageSize	= "10";
		param_faq.tagMatch = "same";
		param_faq.topFlag = "n";
		param_faq.isTop = 1;
		param_faq.mcCurPage = mcCurPage;
		param_faq.queryMatch = querycode;
		param_faq.productCode = $("#productSelect").val();
		$.ajax({ 
			async:false, 
			type:"GET",
			url: baseurl + faqToThreeList,
			data:param_faq,
			dataType:"jsonp", 
			jsonp:"jsonp",
			success:function(data){		
				analysisData(data);
			},
			error:function(){}
		});		
} 
function searchQuerykey(){
	  var itemsli = $('#cbg-help .help-filter-close li');
      var pcode ="";
      for(var n = 0;n<itemsli.length;n++){
      	pcode += $(itemsli[n]).attr("tcode")+",";
      }
      if(pcode!=null&&pcode.indexOf(",")>0){
      	pcode = pcode.substring(0,pcode.length-1);
      }
      $("#faqTypeCode").val(pcode);
      getFaqTypeDataList($('.help-filter-close p span.active').attr("tcode"));
}
function chickSerchKey(key){
	if(key!=null&&key.length>0&&key!=$("#serchkey").attr("placeholder")){
		var faqcodelist = $("#faqTypeCode").val();
		var typecode = $('.help-filter-close p span.active').attr("tcode");
		if(faqcodelist!=null&&faqcodelist.length>0){
			var list = faqcodelist.split(",");
			var querycode = "support:faqs@#"+list[0];
			for(var n=1;n<list.length;n++){
				querycode+= "@-"+list[n];
			}
			if(typecode!=null&&typecode=="text"){
				querycode+= "@@text";
			}
			if(typecode!=null&&typecode=="video"){
				querycode+= "@@video";
			}
		}else{
			querycode = "support:faqs";
		}
		var i_data = {
			"url": baseurl + faqToFourList, 
			"data":{"siteCode":SITE_CODE, "APP_NUM":APP_NUM, "queryMatch":querycode,"productCode":$("#productSelect").val(),"tagMatch":"same", "topFlag":"n", "mcCurPage":"1", "pageSize":"1000","isTop":"1","ssUserText":key}
		};
		$.ajax({ 
			async:false, 
			type:"GET",
			url: i_data.url,
			data:i_data.data,
			dataType:"jsonp", 
			jsonp:"jsonp",
			success:function(data){	
				analysisData(data);
			}
		});
	}else{
		 getFaqTypeDataList($('.help-filter-close p span.active').attr("tcode"));
	}
}

$("#cbg-downloads").on("mouseover","ul li .text-right .more",function(e){
    e.preventDefault();
    $(this).each(function(){
      $(this).parent().parent().find(".des").css({"whiteSpace":"normal"})
    })
});
$("#cbg-downloads").on("mouseout","ul li .text-right .more",function(e){
    e.preventDefault();
    $(this).each(function(){
      $(this).parent().parent().find(".des").css({"whiteSpace":"nowrap"})
    })
});
function facebookBlog(){
	var share = window.share || {};
	share.openShareWindow = function (site, text, linkUrl, imageUrl, title) {
		var shareUrl;
		text = encodeURIComponent(text || document.title);
		linkUrl = encodeURIComponent(linkUrl || window.location.href);
		shareUrl = shareUrl = "https://www.facebook.com/sharer/sharer.php?u=" + linkUrl;
		window.open(shareUrl, "sw", "width=800,height=500,menubar=0,scrollbars=1,resizable=1,status=1,titlebar=0,toolbar=0,location=1");
	}
	share.openShareWindow("facebook");
}
function twitterBlog(){
	var share = window.share || {};
	share.openShareWindow = function (site, text, linkUrl, imageUrl, title) {
		var shareUrl;
		text = encodeURIComponent(text || document.title);
		linkUrl = encodeURIComponent(linkUrl || window.location.href);
		shareUrl = "https://twitter.com/intent/tweet?text=" + text + encodeURIComponent(' ') + linkUrl;
		window.open(shareUrl, "sw", "width=800,height=500,menubar=0,scrollbars=1,resizable=1,status=1,titlebar=0,toolbar=0,location=1");
	}
	share.openShareWindow("twitter");
}
function googleBlog(){
	var share = window.share || {};
	share.openShareWindow = function (site, text, linkUrl, imageUrl, title) {
		var shareUrl;
		text = encodeURIComponent(text || document.title);
		linkUrl = encodeURIComponent(linkUrl || window.location.href);
		shareUrl = "https://plus.google.com/u/0/share?text=" + text + encodeURIComponent(' ') + linkUrl;
		window.open(shareUrl, "sw", "width=800,height=500,menubar=0,scrollbars=1,resizable=1,status=1,titlebar=0,toolbar=0,location=1");
	}
	share.openShareWindow("google");
}
