function setDownloadsHide(){
	if($("#productdown h2").eq(0).css("display")=="none"&&$("#productdown h2").eq(1).css("display")=="none"){
		$("#downloads_product").remove();
		$("#cbg-downloads").hide();
	}
}
$(document).ready(function(){	
	//getRelProducts(pbi3name);
	getRelProducts(pbi3name).then(function(data, err){
		setTimeout("getManuals($('#productdown'))",300);
	});
	if(pbi3!=null && pbi3.indexOf("31001")>-1){
		getNewbieGuide(pbi3name);
	}
	getSimilator(pbi3name);
	getFaultDebar(pbi3name);
	//setTimeout("getManuals($('#productdown'))",300);
