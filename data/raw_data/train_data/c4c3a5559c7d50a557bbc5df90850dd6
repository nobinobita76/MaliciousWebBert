	$('#cbg-support-menu li').click(function (e) {
		var _s = $(this).find("a").attr("sort")
		var $section = ''
		for(var j = 0; j <  $(".cbg-navx").length; j++){
			if($(".cbg-navx").eq(j).attr("sort") == _s){
				$section =  $(".cbg-navx").eq(j)
				break
			}	
		}
		e.preventDefault();
		$('html,body').animate({ scrollTop: $section.offset().top - 95 + 'px' });
	});
	
	if(window.location.host.indexOf("consumer")> -1){
		$(".marketing_url").each(function(){
			var _url = $(this).attr("_u")
			_url = _url.replace(g_ssSourceSiteId ,"cn")
			if(_url.indexOf("index.htm")>0){
				_url = _url.substring(0,_url.indexOf("index.htm"))
			}else{
				_url = _url.substring(0,_url.lastIndexOf("/")+1)
			}		
			_url = _url + $(this).attr("name").toLowerCase() + ".htm"
			$(this).attr("href",_url)
		});
	}else{
		$(".marketing_url").each(function(){
			var _url = $(this).attr("_u")
			_url = _url.substring(0,_url.lastIndexOf("/")+1);
			_url = _url + $(this).attr("name")
			$(this).attr("href",_url)
		});
	}
	$(".help-detail").on("click",".help-detail-cont .item1",function(){
