<script type="text/javascript">
    function usererror() {
    $(".user_error").append("<span></span>"); 
    $(".user_error").append("<a>[关闭]</a>"); 
    userclose();
    closeal();
    }
    var cc = 6;
    istop = 0;
    function userclose(){
      if(istop == 1){istop = 0;cc = 6; return;}
        if (cc == 0) { cc = 6; $(".user_error").hide(); return; }
            cc--;
            $(".user_error").show().find("span").html(cc + "s后提示自动关闭");
            clearstime = setTimeout(function(){
              userclose();
            }, 1000)
        smallclose();
    }
    function smallclose(){
      $(".user_error a").click(function(){
        $(".user_error").hide();
        istop = 1;
       })
    }
    function closeall(){
      $(".user_error").hide();
      clearTimeout(clearstime); 
      istop = 0;cc = 6; return;
    }
    function closeal(){
      $("#lx_nav1 a").click(function(){
        closeall();
      })
    }
    new TextMagnifier({
        inputElem: '.enlarge',
        align: 'bottom'
    });
    new TextMagnifier({
        inputElem: '.enlarge1',
        align: 'bottom'
    });
</script>
