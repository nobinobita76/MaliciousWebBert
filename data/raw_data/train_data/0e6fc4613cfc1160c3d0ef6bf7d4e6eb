		  dataType: "json",
		  success:function(data)
		  {
			  $("#liulangnum").html(data.NewsViews);
			  $("#lovesee font").html(data.lovesee);
			  $("#nolovesee font").html(data.nolovesee);
			  $("#NewsCommentNum").html(data.NewsCommentNum);
			  if(data.NewsCommentNum=="0")
			  	$(".title-join-w").hide();
		  }
		});	
	})
  </script> 
  <script>document.body.onselectstart=document.body.oncontextmenu=function(){return false;};</script>   
 </body>
</html>