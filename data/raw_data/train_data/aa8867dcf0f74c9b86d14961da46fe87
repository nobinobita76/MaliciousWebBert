<script type="text/javascript">
function checkQySearch(){
	var textFlag = false;
	var selectFlag = false;
	$("#qySearchForm :text").each(function(){
		if($.trim($(this).val()) != ""){
			textFlag = true;
			return;
		}
	});
	
	$("#qySearchForm select").each(function(){
	if($.trim($(this).val()) != ""){
			selectFlag = true;
			return;
		}
	});
	if(textFlag == false && selectFlag == false){
		alert("请先填写或选择内容再进行搜索！");
		return false;
	}
	return true;
}
</script>
