<script type="text/javascript">
function checkNumber(str){ 
	if(""==str){ 
		return false; 
	} 
	var reg = /^\d*$/; 
	return reg.test(str); 
}
function gotoPage(){
	try{
		var gotoNum = document.getElementById("pageGotoNo").value;
		var pageCount = 6752;
		if(gotoNum == "" || !checkNumber(gotoNum)){
			alert("请输入您所需跳转的页码数，且只能是数字！");
			return;
		}
		gotoNum = parseInt(gotoNum);
		if(gotoNum>pageCount || gotoNum<1){
			alert("您请输入的跳转页码数超过范围(1-6752)！");
			return;
		}
		window.location.href = "corpList.shtml?pageNo=" + document.getElementById("pageGotoNo").value;
	}catch(e){
	}
}
</script>
