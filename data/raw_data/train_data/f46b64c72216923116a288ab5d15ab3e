                            alert(data);
                            $("#btn_submitmsg").attr("class", "cBtn1");
                            $("#btn_submitmsg").removeAttr("disabled");
                        }
                    },
                    error: function (data) {
                        $(".btnck").text(data);
                    }
                });
            });
        });
