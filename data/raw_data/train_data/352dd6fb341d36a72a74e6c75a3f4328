<script>var imageurls= ''; var wapurl= 'http://m.chinaiiss.com';var source_dir='http://static.chinaiiss.com/wap/chinaiiss_touch';var artnumber = '3';var artid = '5453691'</script>
<script type="text/javascript">

$("#tuijian_ad").html($('#div_tuijian').html());
$(document).ready(function(){
	$('#number').attr('value',0);
	var maxwidth = $(document.body).width();
	$(".zhengwen img").attr('width',maxwidth);
	
	$(".close").click(function(){
		$("#bg").hide();
		$(".tankuang").hide();
	});
});

//上一页
function previous(){
	var num = document.getElementById("number").value;
	var num2 =Number(num)-Number(1);
	if(num2<0)
	       num2=0;
	if(num2<artnumber){
		$.get(wapurl+"/do.php", {inajax:1,do:'touch', ac:'getmorearticle',number:num2,vtype:'touch',tempid:artid}, function(data){
			
				var articlelist = eval("(" + data + ")");
			    $('.zhengwen').html(articlelist);
			    $("#curpage").text(parseInt(num2)+1);
			    $('#number').attr('value',num2);
				window.scrollTo(0,100);
		});
	}
}
//下一页
function next(){
	var num = document.getElementById("number").value;
	var num2 =Number(num)+Number(1);
	
	if(num2<artnumber){
		$.get(wapurl+"/do.php", {inajax:1,do:'touch', ac:'getmorearticle',number:num2,vtype:'touch',tempid:artid}, function(data){
			
			var articlelist = eval("(" + data + ")");
			$('.zhengwen').html(articlelist);
			$("#curpage").text(parseInt(num2)+1);
			$('#number').attr('value',num2);
			window.scrollTo(0,100);
		});
	}
}
//退出
function quit()
{
	
	$.post(wapurl+"/do.php?do=login&ac=quit&vtype=touch", {inajax:1}, function(data){
		alert("退出成功，重新刷新");
		var list = '<p class="qing_dl"><span>请您<a href="'+wapurl+'/touch/user/login">登陆</a>/<a href="'+wapurl+'/touch/user/reg">注册</a>后发表评论</span></p>';
		document.getElementById("#p").innerHTML = list;
		
	});
}

function summ(){
	
	document.getElementById("myform").submit();
}
//导航栏
function navi(){
	
    if($(".tuan_nav").attr("style"))
	     $(".tuan_nav").removeAttr("style");
	else
	     $(".tuan_nav").attr("style","display:none;");
}
function showWin()
{
	$("#bg").height($(document).height());
	$("#bg").show();
	$(".tankuang").show();
	$('.tankuang').offset({top:window.scrollY+$(window).height()/2-100});
}

</script>

