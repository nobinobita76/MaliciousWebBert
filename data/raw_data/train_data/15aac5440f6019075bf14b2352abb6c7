    <script type="text/javascript">
        function resize1(obj, wsize, hsize, index, total) {
            var w = obj.width || 0;
            var h = obj.height || 0;
            var iebug = false;
            if (w == 0 || h == 0) {
                iebug = true;
                var clone = obj.cloneNode(true);
                clone.style.visibility = 'hidden';
                document.body.appendChild(clone);
                w = clone.width;
                h = clone.height;
                document.body.removeChild(clone);
            }

            if (w > wsize && h > hsize) {
                if (w / h <= wsize / hsize) { //resize by height
                    obj.style.width = wsize + "px";
                    obj.style.height = "auto";
                }
                else {
                    obj.style.height = hsize + "px";
                    obj.style.width = "auto";
                }
            }
            //	obj.height=180;
            obj.style.marginLeft = 'auto';
            obj.style.marginRight = 'auto';
            obj.style.visibility = 'visible';
            obj.style.display = 'block';
            if (index == 0) {
                for (var i = 1; i < total; i++) {
                    var e = document.getElementById("slideimg_" + i);
                    if (typeof e !== 'undefined' && e.getAttribute("lazy-src") !== null) {
                        e.setAttribute("src", e.getAttribute("lazy-src"));
                    }
                }
            }
        }
        function loadDefault(obj, src) {
            if (typeof obj !== 'undefined') {
                obj.src = src;
                obj.style.visibility = 'visible';
            }
        }
        function tp(d) {
            if (document.searchForm.query.value.length > 0) {
                var a = document.searchForm.query.value;
                if (d.href.indexOf("wenwen") != -1) {
                    d.href = "http://wenwen.sogou.com/s/?cid=news2ww&w=" + encodeURIComponent(a)
                } else if (d.href.indexOf("baike") != -1) {
                    d.href = "http://baike.sogou.com/Search.e?cid=news2bk&sp=S" + encodeURIComponent(a)
                }
            }
        }
        // 高级搜索功能将查询词带给高级搜索界面
        window.s = function (o, query, p, po) {
            var w = document.searchForm.query,
                    h = o.attributes["hrefmock"].value,
                    q = encodeURIComponent(w.value);

            if (q == "") return;
            if (h.indexOf("?") > 0) {
                o.href = h + "&p=" + p + "&" + query + q
            } else {
                o.href = h + "?p=" + p + "&" + query + q
            }

            if (po && po.length > 0) {
                o.href = h + "#" + po;
            }
        };
    </script>

