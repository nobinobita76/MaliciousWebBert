                <script type="text/javascript">
                    function adjustKeywords() {
                        var keywords = document.getElementById("keywords");
                        var lis = keywords.children;
                        for (var i = 0; i < lis.length; i++) {
                            var oParent = lis[i].firstChild;
                            var span = oParent.firstChild;
                            if (span.clientHeight > 40) {
                                span.clientHeight = 40;
                            }
                            span.style.width = oParent.clientWidth - 10 + 'px';
                            span.style.top = (oParent.clientHeight - span.clientHeight) / 2 + 'px';
                            span.style.display = 'block';
                        }
                    }
                    adjustKeywords();
                </script>
