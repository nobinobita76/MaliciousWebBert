    <script type="text/javascript">

        $(".querytype").click(function () {
            $(".qborder .select-querytype").css("display", "block");
        });

        $(".select-querytype li").click(function () {
            var index = $(this).index();
            if (index == 0) {
                $(".select-querytype li:eq(0)").addClass("active");
                $(".select-querytype li:eq(1)").removeClass("active");
                $(".querytype").html('全文<i class="triangle"></i>');
                $("#mode-text").val(1);
            } else if (index == 1) {
                $(".select-querytype li:eq(1)").addClass("active");
                $(".select-querytype li:eq(0)").removeClass("active");
                $(".querytype").html('标题<i class="triangle"></i>');
                $("#mode-text").val(2);
            }
            $(".qborder .select-querytype").css("display", "none");
        });


        (function () {
            var init = function () {
                if ($(window).scrollTop() == 0) {
                    $("a.go_top").hide();
                } else {
                    $("a.go_top").show();
                }
            };
            var gotop = function () {
                $(window).scrollTop(0);
                return false;
            };
            /*function m_s() {
                 var w = document.searchForm.query;
                 if (new RegExp("kw=([^&]+)").test(location.search)) {
                     if (w.value.length == 0) w.value = decodeURIComponent(RegExp.$1)
                 }
             };*/
            $(document).ready(init);
            //$(document).ready(m_s);
            var a = document.getElementById("go_top");
            if (typeof a == 'undefined' || a == null)
                return;
            if (typeof window.addEventListener != "undefined") {
                a.addEventListener("click", gotop, false);
            } else {
                a.attachEvent("onclick", gotop);
            }
        })();
    </script>
<script language='javascript'>
(function(){
    var bp = document.createElement('script');
    var curProtocol = window.location.protocol.split(':')[0];
    if (curProtocol === 'https') {
        bp.src = 'https://zz.bdstatic.com/linksubmit/push.js';        
    }
    else {
        bp.src = 'http://push.zhanzhang.baidu.com/push.js';
    }
    var s = document.getElementsByTagName("script")[0];
    s.parentNode.insertBefore(bp, s);
})();
</script>
   

