var _text = document.getElementById('text');
function formatPic(){var _imgs = _text.getElementsByTagName("IMG");
var _tabs = _text.getElementsByTagName("TABLE");
for (i=0;i<_imgs.length;i++){if (_imgs[i].scrollWidth > 620){
_imgs[i].width = "620";_imgs[i].onclick= showPic;_imgs[i].style.cursor="pointer";_imgs[i].alt="点击查看原图";}}
for (i=0;i<_tabs.length;i++){if (_tabs[i].width > 620){_tabs[i].style.width = "620";}}}
function showPic(){window.open(this.src);}
if (_text){window.onload = formatPic;}
