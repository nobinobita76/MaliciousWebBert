<script type="text/javascript">
    $(function(){
        $("#types").change(function(){
            var i = $(this).val();
            $(".service_center_box .FP").hide()
            $(".service_center_box .FP").eq(i).show();
        }).change();

        $("#searchbtn").click(
            function(e){
                    $("#search").fadeToggle(300);
                    e.stopPropagation();
                }
        );
        $("#shareBtn").click(
            function(e){
                    $(".shareAll").fadeIn(300);
                    e.stopPropagation();
                }
        );  
        $(".shareAll .close").click(
            function(e){
                    $(".shareAll").fadeOut(300);
                    e.stopPropagation();
                }
        );  
        $(".more img").width(230)
    });

    function changeTag(element){
        $(".inside a").removeClass("cur");
        $(element).addClass("cur");
        var index = $(".inside a").index(element);
        $(".detail-warp").hide().eq(index).show();
    }
    //分享文章（分享类型，分享链接，分享标题，分享摘要，分享图片）
    function shareArchive(type, url, title, content, pic){
        url = url.indexOf("http://") == 0 ? url : (location.origin + url);
        pic = pic.indexOf("http://") == 0 ? pic : (location.origin + pic);
        var shareUrl = "http://www.jiathis.com/send/?webid=" + type + "&url=" + url + "&title=" + title + "&summary=" + content + "&pic=" + pic;
        window.open(shareUrl);
        $(".shareAll .close").click();
    }
</script>
