$("#info_top_con ul").find("li:eq(1) span").each(function(){if(!!$(this).html())$(this).addClass("yx").css({"background-color":""+("#"+rancolor())+""}).mouseover(function(){$(this).css({"background-color":"#1F78D0"})}).mouseout(function(){$(this).css({"background-color":"#"+rancolor()})})});
function rollto(v){
this.roll=function(s){$('html').animate({scrollTop:$(".info_box").eq(s).offset().top},'fast');$('body').animate({scrollTop:$(".info_box").eq(s).offset().top},'fast');}
switch(v){
case "酒店介绍":this.roll(0);break;
case "酒店图片":this.roll(1);break;
case "客人点评":this.roll(2);break;
case "交通地图":this.roll(3);break;
case "酒店问答":this.roll(4);break;
case "周边酒店":this.roll(5);break;}
}

var maplet = null,mData=[[20072,'澳门东望洋酒店','http://p.zhuna.cn//hotel_images/20072/160x120_33301022_0_5_0_3.jpg','ISHBWVZTWCBDD','澳门松山马路1-5号','614','9'],[20073,'澳门维景酒店(原京澳酒店)','http://p.zhuna.cn//hotel_images/20073/160x120_43301002_0_10_0_1.jpg','ISHDGCZTWBWJS','澳门新口岸北京街199号','845','9'],[20074,'澳门新丽华酒店','http://p.zhuna.cn//hotel_images/20074/160x120_43301003_0_5_0_1.jpg','ISHBCHZTWBTGV','澳门约翰四世大马路','747','9'],[20081,'澳门金沙酒店','http://p.zhuna.cn//hotel_images/20081/160x120_50101462_0_5_0_1.jpg','ISHFCJZTWCBTT','澳门蒙地卡罗前地203号','1722','9'],[20084,'澳门葡京酒店','http://p.zhuna.cn//hotel_images/20084/160x120_53301003_0_5_0_1.jpg','ISHCTVZTWBTVB','澳门葡京路2-4号','977','9'],[20086,'永利澳门酒店','http://p.zhuna.cn//hotel_images/20086/160x120_53301015_0_10_0_1.jpg','ISHDUCZTWBUCB','澳门外港填海区仙德丽街','2056','9'],[20090,'澳门新葡京酒店','http://p.zhuna.cn//hotel_images/20090/160x120_53301023_0_5_0_1.jpg','ISHCBFZTWBUAA','澳门葡京路','1989','9'],[20094,'澳门皇都酒店','http://p.zhuna.cn//Hotel_Images/160x120_2010102610421510613.jpg','ISHBEWZTWCASR','澳门得胜马路2-4号','1100','9'],[27396,'澳门帝濠酒店','http://p.zhuna.cn//hotel_images/27396/160x120_33301023_0_5_0_1.jpg','ISHDRHZTWBWCE','澳门上海街51号','850','9']],mks=new Array(),polyline;
function initMap(){
	maplet = new Maplet("DP_Map_Show");
	maplet.centerAndZoom(new MPoint('北京市'),8);
	maplet.addControl(new MStandardControl());
	maplet.showOverview(false);	
	var mk=new MMarker(   
				new MPoint("ISHDSEZTWCAHB"),   
				new MIcon("images/0.gif",34,36), 
				null,
				new MLabel("澳门利澳酒店",10,-8,80)
			);
	maplet.addOverlay(mk);
	mks.push(mk)
	for(var i=0;i<mData.length;i++){
		var mk=new MMarker(   
				new MPoint(mData[i][3]),   
				new MIcon("images/No/"+(i+1)+".gif",17,22),   

				new MInfoWindow(mData[i][1],'<img src="'+mData[i][2]+'" width="86" height="68" align="left" style="margin:5px;">名称：'+mData[i][1].substring(0,12)+'<br>价格：最低'+mData[i][5]+'元起订<br>地址：'+mData[i][4].substring(0,25)+'<br><a href="/hotel/hotelinfo.asp?hotelid='+mData[i][0]+'" target="_blank">了解酒店详情>></a>'),
				
				null); 
		maplet.setIwStdSize(300,150); 
		maplet.addOverlay(mk);
		mks.push(mk);
	}
	maplet.setAutoZoom();
};
function showhotel(id,i){
	maplet.removeOverlay(polyline); 
	mks[i].openInfoWindow();
	for(var i=0;i<mData.length;i++){
		if(mData[i][0]==id){
			    var brush = new MBrush("blue");brush.stroke = 15;brush.overlap.enable = true;   
				polyline = new MPolyline([new MPoint("ISHDSEZTWCAHB"),new MPoint(mData[i][3])],brush);
				polyline.setEditable(true);
				maplet.addOverlay(polyline); 
				mks.push(polyline);
				maplet.setAutoZoom();
		}
	}
}
</script>
 </body>
</html>