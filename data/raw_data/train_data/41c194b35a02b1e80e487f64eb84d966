      <script language="javascript" type="text/javascript" src="/include/clicks.aspx?name=articles&amp;id=69359"></script> 
      <h1>高考录取通知书变成“大礼包” 其中暗藏的利益链</h1> 
      <div class="title_content"> 
       <p style="padding-top:10px;"> <span>来源：网络整理</span> <span style="padding-left:10px;">作者：佚名</span> <span style="padding-left:10px;">更新时间：2015/8/6</span> <span id="font_sl" style="padding-left:10px;"> 字号：<a id="small" href="#">小</a> <a id="large" href="#">大</a></span> </p> 
       <div id="ding" style="position:absolute"> 
        <!-- 将此标记放在您希望显示like按钮的位置 --> 
        <div class="bdlikebutton"></div> 
        <!-- 将此代码放在适当的位置，建议在body结束前 --> 
        <script id="bdlike_shell"></script> 
        <script type="text/javascript">
			//获取控件左绝对位置


			var bdShare_config = {
			"type":"small",
			"color":"blue",
			"share":"yes"
			};
			document.getElementById("bdlike_shell").src="http://bdimg.share.baidu.com/static/js/like_shell.js?t=" + Math.ceil(new Date()/3600000);

			function getAbsoluteLeft(o) {
			oLeft = o.offsetLeft
			while(o.offsetParent!=null) {
			oParent = o.offsetParent
			oLeft += oParent.offsetLeft
			o = oParent
			}
			return oLeft
			}
			//获取控件上绝对位置
			function getAbsoluteTop(o) {
			//o = document.getElementById(objectId);
			oTop = o.offsetTop;
			while(o.offsetParent!=null)
			{
			oParent = o.offsetParent
			oTop += oParent.offsetTop  // Add parent top position
			o = oParent
			}
			return oTop
			}

			var left=getAbsoluteLeft(document.getElementById("font_sl"));
			var top=getAbsoluteTop(document.getElementById("font_sl"));
			document.getElementById("ding").style.top = "439px";
			document.getElementById("ding").style.left = (left+80)+"px";
			//alert(document.getElementById("ding").style.top);
			//alert(document.getElementById("ding").style.left);

			function wresize() {

			var left=getAbsoluteLeft(document.getElementById("font_sl"));
			var top=getAbsoluteTop(document.getElementById("font_sl"));
			document.getElementById("ding").style.top = "439px";
			document.getElementById("ding").style.left = (left+90)+"px";
			//alert(document.getElementById("ding").style.top);
			}
			function addResizeEvent(func) {

			var oldonload = window.onresize;
			if (typeof window.onresize != 'function') {
			window.onresize = func;
			} else {
			window.onresize = function() {
			oldonload;
			func();
			}
			}
			}

			addResizeEvent(wresize);
		</script> 
       </div> 
