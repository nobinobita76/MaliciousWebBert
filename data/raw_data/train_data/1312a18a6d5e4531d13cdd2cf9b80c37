<script language="javascript"> 
    var regionCode="999";
	if(regionCode!=null&&regionCode!=''){
		$("#change").html($('#sx'+regionCode).html());
	}
	
function setRegionCode(url,regionCode){
	    if(regionCode!=null&&regionCode!=""){
	        $.ajax({
				type:"post",
				cache: false,
				async:false, 
				url:"/setRegionCode.jspx",
				data:"regionCode="+regionCode,
				dataType:"json",
				async:false, //同步
				success:function(json){
				     //alert(json.user.areaCode);
				},
				error:function(xmlhttprequest,msg,errorobj){
				     alert("切换地市出错，请稍后重试");
				}
			});			
		    if($("#sx"+regionCode).html()!=null&&$("#sx"+regionCode).html()!=''){
		        $("#change").html($("#sx"+regionCode).html());
		    }
		    window.location=""+url;  
	     }
	}
</script> <div class="box-iframe" style="float:left">
