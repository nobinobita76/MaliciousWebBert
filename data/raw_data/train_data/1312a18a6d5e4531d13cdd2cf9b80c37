<script type="text/javascript">
$(function(){
	$("div.tab div>a").each(function(n){this.index=n;}).mouseover(
		function(){
			$("div.tab div.tab-content").hide().eq(this.index).show();
			$("div.tab div>a").removeClass("on").eq(this.index).addClass("on"); 
			return false;
		}
	);
	
	$("div.round").round();//圆角
		$("div.round-t").round({corner:"top"});
	$("div.round-b").round({corner:"bottom"});//圆角
});
</script>
<script type="text/javascript">
$(document).ready(function(){
	$(".nav ul li").hover(function(){
		$(this).addClass("hover").siblings().removeClass("hover");
		},function(){$(this).removeClass("hover")});//主导航背景切换
	
	$(".table-form-1 td.delete").click(function(){
		$(this).parent().hide();
		});	//购物车中删除产品

	$(".cart-list .total a").click(function(){
		$(".cart-list").hide();
		$("#arrow").removeClass("tip-fold");
		});	
	$("#arrow").click(function(){
		$(this).toggleClass("tip-fold");
		$(".cart-list").toggle();
		});//展开折叠购物车中产品详情
	
	$(".nav-quick-2").hover(function(){
		$(".sub-menu-2").show();
		},function(){
			$(".sub-menu-2").hide();
			});
	$(".sub-menu-3 li").hover(function(){
			$(this).addClass("on");
			$(this).find("div").show();
		},function(){
			$(this).removeClass("on");	
			$(this).find("div").hide();	
		});	
	$(".sub-menu li").hover(function(){
		$(this).find("div").show();
		},function(){
		$(this).find("div").hide();	
		});//左侧二级菜单展开树功能
	
	$(".tab-caption li:first").addClass("on");	
	$(".tab-content>div").hide();
	$(".tab-content div.on").show();
	$(".tab-caption li").click(function(){
		var jf=$(this).index();
		$(this).addClass("on").siblings().removeClass("on");
		$(this).parent().parent().next().children("div").hide().eq(jf).show();
		});//登录口tab切换功能
	
	$(".tab-con .box:not(:first)").hide();
	$(".show-tab li").click(function(){
		var intjtk=$(this).index();
		$(this).addClass("on").siblings().removeClass("on");
		$(".tab-con").show();
		$(".tab-con .box").filter(":visible").hide().parent().children().eq(intjtk).show();

		});
	$(".box h5 span").click(function(){
		$(".tab-con").hide();
		$(".show-tab li").removeClass("on");
		});//业务推荐切换
	
	////$("#message").Show(256,185);浮动提示层
	
	window.onload=function(){for(var ii=0; ii<document.links.length; ii++)document.links[ii].onfocus=function(){this.blur()}};
	});
</script>
<script type="text/javascript">
$(document).ready(function(){
	$("a.login2").click(function(){
		var handle=$(this);
		handle.addClass("on");
		var panel=$("div.login-content-2");
		panel.fadeIn().find("a.close").click(function(){panel.fadeOut();handle.removeClass("on");});
	});
})
</script>
