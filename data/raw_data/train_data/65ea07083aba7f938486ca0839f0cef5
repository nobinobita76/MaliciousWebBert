<script language=javascript>
function sent()
{	    
    $error = "";		
    if(form1.user.value.replace(/(^\s*)|(\s*$)/g,'')=='')
	$error = "用户名不能空缺!\r\n";
    if(form1.pwd.value.replace(/(^\s*)|(\s*$)/g,'')=='')
	$error += "密码不能空缺!";
    if($error!="")
    {
	alert($error); 		 
	document.form1.user.focus();
    }
    else
    {		
	document.form1.submit();
    }
}

function po_login()
{
	document.form1.action="login_transfer.php";
	document.form1.submit();
	return true;
}
</script>
<script language="javascript">
  //验证表单
  function loginAction()
  {
      var df = document.loginForm;
      var userName=df.userName.value;
      var password=df.password.value;
      var domain=df.domainName.value;
      if(domain && domain!="" && domain!="null")
      {
        df.user.value = userName+"@"+domain; 
      }
      else
      {
          df.user.value = userName; 
      }
      df.user.value=Validator.Trim(df.user.value);
      if( !Validator.Email.test(df.user.value))
      {
        //info("请输入合法的帐户！","Please input valid account!","入合法的簦");
        return false;
      } else if(Validator.IsNull(password)){
          //info("请输入邮箱密码！","Please input  password!","入]箱密a！");
          return false;
      } else {
         return true;
      }
  }
</script>

