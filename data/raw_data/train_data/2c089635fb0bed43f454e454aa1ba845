$(document).ready(function(){
    var cfpy="A,B,C,D,E,F,G,H,I,J,K,L,M,N,P,Q,R,S,T,W,X,Y,Z";
    var arr=cfpy.split(",");
    $("#tblcatefpy td").each(function(){
        if(cfpy.indexOf($.trim($(this).html()))>-1){
            $(this).mouseover(function(){
                 for(var i=0;i<arr.length;i++){
                    $("#tdc"+arr[i]).hide();
                 }          
                $("#tdc"+$.trim($(this).html())).show();
           })
        }
    })
    for(var i=0;i<arr.length;i++){
        $("#tdc"+arr[i]).mouseover(function(){
            $(this).show();
        })
    }
    for(var i=0;i<arr.length;i++){
        $("#tdc"+arr[i]).mouseout(function(){
            $(this).hide();
        })
    }
})
</script>
