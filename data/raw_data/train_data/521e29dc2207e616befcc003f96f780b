<script type="text/javascript">
function category_highlight(){
}
category_highlight();

function category_related(box,cid,showkline){
  category_related_hide();
  if(!jq("#kline_box_loader").length){
    /*
    os=box.offset();//元素位置
    sc=jq(window).scrollTop();//滚动条位置
    wh=document.documentElement.clientHeight;//窗口高度
    if( (wh + sc -os.top >= 300) || (os.top-sc < 300)){
      //向下显示
      newtop=os.top + box.height() + 5;
    }else{
      //向上显示
      newtop=os.top - 335;
    }
    newleft=os.left + 50;
    */
    os=box.offset();//元素位置
    sc=jq(window).scrollTop();//滚动条位置
    wh=document.documentElement.clientHeight;//窗口高度
    var boxclass='left';

    newtop=os.top + box.height();
    if(jq(window).width() - os.left < 470){
      newleft=os.left -470;
      boxclass='right';
    }else{
      newleft=os.left + box.width()/2 -30;
    }
    newleft=newleft>30 ? newleft : 30;

    jq("body").prepend("<div id='kline_box_loader' class='kline_boxes kline_boxes_" + boxclass + "' style='display:none'><img src='./images/ajax-loader.gif'/></div>");
    jq("body").prepend("<div id='kline_box' class='kline_boxes kline_boxes_" + boxclass + "' style='display:none'><img src='./images/ajax-loader.gif'/></div>");

    jq(".kline_boxes").css({ top: newtop + 'px', left: newleft + 'px' }); 

    jq("#kline_box_loader").fadeIn('slow');
    jq("#kline_box").load("category_related.php", {cid:cid, tid:5112479, showkline:showkline} , function(data){
      //jq("#kline_box").html(data).mouseleave(function(){category_related_hide();});
      jq("#kline_box").html(data).mouseleave(category_related_hide);
    });

    jq("#kline_box_loader").hide();
    jq("#kline_box").fadeIn('slow');

//    setTimeout(function(){
//      hidekline();
//    },6000);
  }
}

function category_related_hide(){
  jq("#kline_box_loader").remove();
  jq("#kline_box").remove();
}

//-->
</script>
