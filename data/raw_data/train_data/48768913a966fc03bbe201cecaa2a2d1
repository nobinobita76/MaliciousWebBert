    var wq_navs_scrolls_width=$('#wq_navs_scrolls ul').width();
    $('#wq_navs_scrolls ul').parent().css('width', wq_navs_scrolls_width);
    if (wq_navs_scrolls_width>wq_window_width) {
        var myscroll_wq_navs_scrolls = new iScroll("wq_navs_scrolls", {
            hScrollbar: false,
            vScroll: false,
            bounce: true,
        });
        if ($('#wq_navs_scrolls .a').length) {
            var wq_wq_navs_scrolls = $('#wq_navs_scrolls .a').offset().left;
            if (wq_wq_navs_scrolls > wq_window_width / 2) {
                var scrollTo_left=wq_wq_navs_scrolls - (wq_window_width / 2);
                var wq_left=wq_navs_scrolls_width - wq_window_width;
                var scrollleft=wq_left<scrollTo_left?wq_left:scrollTo_left;
                myscroll_wq_navs_scrolls.scrollTo(-scrollleft, 0, 1500);
            }
        }
    }
</script> 
