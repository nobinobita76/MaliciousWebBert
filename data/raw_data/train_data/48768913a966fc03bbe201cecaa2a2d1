        .video_molie #wq{background:f051d2b3e34ab93d}
    </style> 
  <script>
		function AC_FL_RunContent() {
			var str = '';
			var ret = AC_GetArgs(arguments, "clsid:d27cdb6e-ae6d-11cf-96b8-444553540000", "application/x-shockwave-flash");
			str += '<embed class="video_molie" ';
			for (var i in ret.embedAttrs) {
				str += i + '="' + ret.embedAttrs[i] + '" ';
			}
			str += '></embed>';
			return str;
		}
		function AC_GetArgs(args, classid, mimeType) {
			var ret = new Object();
			ret.embedAttrs = new Object();
			ret.params = new Object();
			ret.objAttrs = new Object();
			for (var i = 0; i < args.length; i = i + 2) {
				var currArg = args[i].toLowerCase();
				switch (currArg) {
					case "classid":
						break;
					case "pluginspage":
						ret.embedAttrs[args[i]] = 'http://www.macromedia.com/go/getflashplayer';
						break;
					case "src":
						ret.embedAttrs[args[i]] = args[i + 1];
						ret.params["movie"] = args[i + 1];
						break;
					case "codebase":
						ret.objAttrs[args[i]] = 'http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=9,0,0,0';
						break;
					case "onafterupdate":
					case "onbeforeupdate":
					case "onblur":
					case "oncellchange":
					case "onclick":
					case "ondblclick":
					case "ondrag":
					case "ondragend":
					case "ondragenter":
					case "ondragleave":
					case "ondragover":
					case "ondrop":
					case "onfinish":
					case "onfocus":
					case "onhelp":
					case "onmousedown":
					case "onmouseup":
					case "onmouseover":
					case "onmousemove":
					case "onmouseout":
					case "onkeypress":
					case "onkeydown":
					case "onkeyup":
					case "onload":
					case "onlosecapture":
					case "onpropertychange":
					case "onreadystatechange":
					case "onrowsdelete":
					case "onrowenter":
					case "onrowexit":
					case "onrowsinserted":
					case "onstart":
					case "onscroll":
					case "onbeforeeditfocus":
					case "onactivate":
					case "onbeforedeactivate":
					case "ondeactivate":
					case "type":
					case "id":
						ret.objAttrs[args[i]] = args[i + 1];
						break;
					case "width":
					case "height":
					case "align":
					case "vspace":
					case "hspace":
					case "class":
					case "title":
					case "accesskey":
					case "name":
					case "tabindex":
						ret.embedAttrs[args[i]] = ret.objAttrs[args[i]] = args[i + 1];
						break;
					default:
						ret.embedAttrs[args[i]] = ret.params[args[i]] = args[i + 1];
				}
			}
			ret.objAttrs["classid"] = classid;
			if (mimeType) {
				ret.embedAttrs["type"] = mimeType;
			}
			return ret;
		}
		$(function() {
			$(".video_molie").each(function() {
				var w = $(this).width();
				var b = parseFloat('1.5');
				var h = w / b;
				$(this).attr("style", 'height:' + h + 'px');
			});
		});

	</script>
  <script type="text/javascript" reload="1">
    var scroll_Top;
    $(function () {
        $('.favbtn').on('click', function () {
            var obj = $(this);
            $.ajax({
                type: 'POST',
                url: obj.attr('href') + '&handlekey=favbtn&inajax=1',
                data: {
                    'favoritesubmit': 'true',
                    'formhash': 'f59c8f45'
                },
                dataType: 'xml',
            }).success(function (s) {
                popup.open(s.lastChild.firstChild.nodeValue);
            }).error(function () {
                window.location.href = obj.attr('href');
                popup.close();
            });
            return false;
        });

        var pcontent_height = $(window).height() - 50;
        $('.grades').on('click', function () {
            var obj = $(this);
            $.ajax({
                type: 'GET',
                url: obj.attr('href') + '&inajax=1',
                data: {},
                dataType: 'xml',
            }).success(function (s) {
                $('#popup_grade').html(s.lastChild.firstChild.nodeValue);
                if ($('#rateform').length) {
                    $('#popup_content').attr("style", 'height:' + pcontent_height + 'px').html(s.lastChild.firstChild.nodeValue);
                    if ($('#popup_fullscreen').is(':hidden')) {
                        $('#popup_title').text("评分");
                        $('#popup_fullscreen').slideDown();
                        scroll_Top = $(window).scrollTop();
                        $('html,body').addClass("wq_ovhidden");
                    }
                    $('.my_rate').hide();
                } else {
                    popup.open(s.lastChild.firstChild.nodeValue);
                }
            }).error(function () {
                window.location.href = obj.attr('href');
                popup.close();
            });
            return false;
        });
        $('.rate_viewratings').on('click', function () {
            var obj = $(this);
            $.ajax({
                type: 'GET',
                url: obj.attr('href') + '&inajax=1',
                data: {},
                dataType: 'xml',
            }).success(function (s) {
                $('#popup_content').attr("style", 'height:' + pcontent_height + 'px').html(s.lastChild.firstChild.nodeValue);
                $('#popup_title').text("评分详情");
                $('#popup_fullscreen').slideDown();
                var pid = obj.attr('pid');
                if (pid) {
                    $('.my_rate').attr('href', 'forum.php?mod=misc&action=rate&tid=80944&pid=' + pid).slideDown();
                    $('#rate_h50').show();
                }
                scroll_Top = $(window).scrollTop();
                $('html,body').addClass("wq_ovhidden");
            }).error(function (s) {
                window.location.href = obj.attr('href');
                popup.close();
            });
            return false;
        });
        $('.recommend').on('click', function () {
            var obj = $(this);
            var handlekey = obj.attr('dataid');
            $.ajax({
                type: 'GET',
                url: obj.attr('href') + '&handlekey=' + handlekey + '&inajax=1',
                data: {'hash': 'f59c8f45'},
                dataType: 'xml',
            }).success(function (s) {
                popup.open(s.lastChild.firstChild.nodeValue);
            }).error(function () {
                window.location.href = obj.attr('href');
                popup.close();
            });
            return false;
        });
        $('.support_forum').on('click', function () {
            var obj = $(this);
            var data = obj.attr('data');
            $.ajax({
                type: 'GET',
                url: obj.attr('href') + '&inajax=1',
                data: {'hash': 'f59c8f45'},
                dataType: 'xml',
            }).success(function (s) {
                $('#popup_grade').html(s.lastChild.firstChild.nodeValue);
                if ($.trim($('#popup_grade p:eq(0)').text()) == '投票成功') {
                    var text = $('#review_support_' + data).text() > 0 ? $('#review_support_' + data).text() + 1 : 1;
                    $('#review_support_' + data).html(text);
                }
                popup.open(s.lastChild.firstChild.nodeValue);
            }).error(function () {
                window.location.href = obj.attr('href');
                popup.close();
            });
            return false;
        });
        $('.oppose').on('click', function () {
            var obj = $(this);
            var data = obj.attr('data');
            $.ajax({
                type: 'GET',
                url: obj.attr('href') + '&inajax=1',
                data: {'hash': 'f59c8f45'},
                dataType: 'xml',
            }).success(function (s) {
                $('#popup_grade').html(s.lastChild.firstChild.nodeValue);
