  <script>
    $(function () {
        //城市悬停展开
        $(document).click(function(e){
            e.stopPropagation();
            $(".city_tab").hide().removeClass("on");
        });
        $(".city_btn").click(function(e){
            e.stopPropagation();
            if($(".city_tab").hasClass('on')){ 
                $(".city_tab").hide().removeClass("on");
            }else { 
                $(".city_tab").show().addClass("on") 
            } 
        });
 


        //图片轮播
        //单体宽度1120
        var w = $(".banBox").width();
        //图片推进&创建圆点
        var i = 0;
        var clone = $(".banBox .img li").first().clone();
        $(".banBox .img").append(clone);
        var size = $(".banBox .img li").size();
        for (var j = 0; j < size - 1; j++) { $(".num").append("<li></li>"); }
        $(".banBox .num li").first().addClass("on");
        /*鼠标点击圆点*/
        $(".banBox .num li").click(function () {
            var index = $(this).index();
            i = index;
            $(".banBox .img").stop().animate({ left: -index * w }, 500)
            $(this).addClass("on").siblings().removeClass("on")
        });
        /*自动轮播*/
        var t = setInterval(function () { i++; move() }, 4000)
        /*悬停时解除自动轮播*/
        $(".banner").hover(
            function () { clearInterval(t); },
            function () { t = setInterval(function () { i++; move() }, 4000) }
        )
        //轮播函数
        function move() {
            if (i == size) { $(".banBox .img").css({ left: 0 }); i = 1; }
            if (i == -1) { $(".banBox .img").css({ left: -(size - 1) * w }); i = size - 2; }
            $(".banBox .img").stop().animate({ left: -i * w }, 500);
            if (i == size - 1) { $(".banBox .num li").eq(0).addClass("on").siblings().removeClass("on"); }
            else { $(".banBox .num li").eq(i).addClass("on").siblings().removeClass("on"); }
        }
        //图片轮播结束


        //首页-鼠标移入移出效果(含灵敏度)
        var li_in;
        var li_out;
        $(".checkbox >ul>li").hover(
            function () {
                //第一次移入不会延时
                var firtst_IN = $(".checkbox>ul>li.hovers").length;
                if (firtst_IN == 0) {
                    $(this).addClass("hovers");
                    $(this).find("ul").show();
                    return;
                }
                //移入延时
                clearTimeout(li_out);
                clearTimeout(li_in);
                li_in = setTimeout(function () {
                    $(".checkbox>ul>li").removeClass("hovers");
                    $(this).addClass("hovers");
                    $(".checkbox>ul>li>ul").hide();
                    $(this).find("ul").show();
                }.bind(this), 200);
            }, function () {
                //移出延时
                clearTimeout(li_in);
                clearTimeout(li_out);
                li_out = setTimeout(function () {
                    $(".checkbox>ul>li").removeClass("hovers");
                    $(".checkbox>ul>li>ul").hide();
                }, 200);
            }
        );


        

    });
</script> 
