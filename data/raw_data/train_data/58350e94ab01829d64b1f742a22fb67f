function lihover(obj){
var oP=document.getElementById(obj);
if(oP){
var obj1=oP.getElementsByTagName("li");
}
if(obj1){ 
for(var i =obj1.length-1;i>=0;i--){
obj1[i].onmouseover=function(){
this.className="current";
}
obj1[i].onmouseout=function(){
this.className=""; 
}
}}
}
lihover("bDlist");
lihover("sTitlist");
</script> 
