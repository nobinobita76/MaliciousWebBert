<script type="text/javascript">
    //图片延迟加载，延迟加载的图片的obj
    function imgLazy(obj) {
        var imgs = Fn(obj).tag('img');
        if (typeof(imgs[0].attributes["thissrc"]) == 'undefined') {
            return;
        }
        imgs.each(function () {
            if (this.attributes["thissrc"]) {
                if (this.src.indexOf('image_blank.gif') != -1) {
                    var osrc = this.attributes["thissrc"].nodeValue;
                    this.src = osrc;
                    this.removeAttribute("thissrc", 0);
                }
            }
        })
    }
    //选项卡
    var tab1 = new M.tab({
        trigger: ["trig-tab-1", "li"],
        panel: ["tab-panel-1", "ul", "mod_plistA"],
        fun: imgLazy,//切换li标签的时候相应的函数
        switchCur: "current",
        triggerType: "mouseover",
        delay : 300
    });
    var tab2 = new M.tab({
        trigger: ["trig-tab-2", "li"],
        panel: ["tab-panel-2", "ul", "mod_plistA"],
        fun: imgLazy,//切换li标签的时候相应的函数
        switchCur: "current",
        triggerType: "mouseover",
        delay : 300
    });
    var tab3 = new M.tab({
        trigger: ["trig-tab-3", "li"],
        panel: ["tab-panel-3", "ol", "mod_shutterA"],
        switchCur: "current",
        triggerType: "click"
    });
    var tab4 = new M.tab({
        trigger: ["trig-tab-4", "li"],
        panel: ["tab-panel-4", "ol", "mod_shutterA"],
        switchCur: "current",
        triggerType: "click"
    });
    var tab5 = new M.tab({
        trigger: ["trig-tab-5", "li"],
        panel: ["tab-panel-5", "ol", "mod_shutterA"],
        switchCur: "current",
        triggerType: "click"
    });
    var tab_link = new M.tab({
        trigger: ["trig-tab-link", "li"],
        panel: ["tab-panel-link", "div", "mod_split"],
        switchCur: "current",
        triggerType: "click"
    });

    //百叶窗,后三个的参数：是否图片延迟加载，真是图片存储的地址，空白图片名
    M.shutter("shutter-1", "li", "current", true, 'thissrc', 'image_blank.gif');
    M.shutter("shutter-2", "li", "current", true, 'thissrc', 'image_blank.gif');
    M.shutter("shutter-3", "li", "current", true, 'thissrc', 'image_blank.gif');
    M.shutter("shutter-4", "li", "current", true, 'thissrc', 'image_blank.gif');
    M.shutter("shutter-5", "li", "current", true, 'thissrc', 'image_blank.gif');
    M.shutter("shutter-6", "li", "current", true, 'thissrc', 'image_blank.gif');
</script>
