<!--[if IE 6]>
<script type="text/javascript">
    var trig_tab_1 = document.getElementById("trig-tab-1").getElementsByTagName("li"),
            trig_tab_1_len = trig_tab_1.length;
    for (var i = 0; i < trig_tab_1_len; i++) {
        trig_tab_1[i].onmouseover = function (i) {
            return function () {
                this.className += " hover";
            }
        }(i);
        trig_tab_1[i].onmouseout = function (i) {
            return function () {
                this.className = this.className.replace("hover", "");
            }
        }(i);
    }
</script>
<![endif]-->

