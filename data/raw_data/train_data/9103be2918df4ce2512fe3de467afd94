<script type="text/javascript">
var HeMvDownloadCount = 0;
var isVip = false;
var totalMarks = false;
var userId = 0;

require(["modules/yinyuetai/player/loader", "loginBox", "user"], function(loader, loginBox, user) {
	$(window).load(function() {
		userId = user.get("userId");
		var mv1 = {
			click : function() {
				if (user.get("userId") > 0) {
					return $('#videoUrl').text();
				} else {
					user.login();
				}
			},
			hover : function(status) {
				var mvdown1 = $('.mv1');
				if (status == 'mouseOver') {
					mvdown1.addClass('hover');
				} else if (status == 'mouseDown') {
					mvdown1.addClass('active');
				} else if (status == 'mouseUp') {
					mvdown1.removeClass('hover');
					mvdown1.removeClass('active');
				} else if (status == 'mouseLeave') {
					mvdown1.removeClass('hover');
				}
			},
			complete : function(status) {
				if (status) {
					confirm('下载地址已复制到剪贴板，如果您已下载并开启"音悦台PC客户端"，"音悦台PC客户端"会自动弹出下载窗口。\u000d\u000d提示：流畅版MV，可免积分无限次下载！');
				}
			},
			getUserInfo : function(result) {
				$.ajax({
					url : '/v2/person/he-user-info',
					method : 'get',
					data : 'json=true&t=' + (new Date()).getTime(),
					success : function(result) {
						if (!result.error) {
							HeMvDownloadCount = result.hecount;
							isVip = result.isVip;
							totalMarks = result.totalMarks;
						}
					}
				});
			}
		};
		window.mv1 = mv1;
		mv1.getUserInfo();

		Y.partner = {
			iframes : {},

			loadEnabled : (function() {
				var result = true;
				try {
					if (window.parent != window.self) {
						result = false;
					}
				} catch (e) {
				}
				return result;
			})()
		};

		/*
		 * Flash加载函数
		 * */

		function loadFlash(id, vars) {
			loader.load(id + 'box', {
				swfUrl : Y.domains.urlStatic + '/swf/main/copyyyt.swf?t=20151112',
				properties : {
					id : id,
					name : id
				},
				vars : vars,
				params : {
					wmode : 'transparent',
					IEwmode : 'transparent'
				}
			});
		}


		$('#J_download_btns').css('display', 'block');
		loadFlash("mv1", {
			swfName : "mv1",
			clipboardComplete : "mv1.complete",
			getClipboardContent : "mv1.click",
			mouseStateEventTypeList : "mouseDown,mouseUp,mouseOver,mouseLeave",
			mouseStateCallback : "mv1.hover"
		});


		
		loadFlash("mv2", {
			swfName : "mv2",
			clipboardComplete : "mv2.complete",
			getClipboardContent : "mv2.click",
			mouseStateEventTypeList : "mouseDown,mouseUp,mouseOver,mouseLeave",
			mouseStateCallback : "mv2.hover"
		});
		var mv2 = {
			click : function() {
				if (user.get("userId") > 0) {
					if (!isVip && totalMarks < 10) {
						confirm('您的积分不够，请去获取积分或者成为会员！');
						return;
					}
					return $('#videoUrl').text() + '?videoFileType=hd';
				} else {
					user.login();
				}
			},
			hover : function(status) {
				var mvdown2 = $('.mv2');
				if (status == 'mouseOver') {
					mvdown2.addClass('hover');
				} else if (status == 'mouseDown') {
					mvdown2.addClass('active');
				} else if (status == 'mouseUp') {
					mvdown2.removeClass('hover');
					mvdown2.removeClass('active');
				} else if (status == 'mouseLeave') {
					mvdown2.removeClass('hover');
				}
			},
			complete : function(status) {
				if (status) {
					if (!isVip) {
						confirm('下载地址已复制到剪贴板，如果您已下载并开启"音悦台PC客户端"，"音悦台PC客户端"会自动弹出下载窗口。\u000d\u000d提示：下载高清版MV，您需要花10积分！');
					} else {
						confirm('下载地址已复制到剪贴板，如果您已下载并开启"音悦台PC客户端"，"音悦台PC客户端"会自动弹出下载窗口。\u000d\u000d提示：尊敬的会员，您可以免积分且无限次下载高清视频！');
					}
				}
			}
		};
		window.mv2 = mv2;
		
		
		loadFlash("mv3", {
			swfName : "mv3",
			clipboardComplete : "mv3.complete",
			getClipboardContent : "mv3.click",
			mouseStateEventTypeList : "mouseDown,mouseUp,mouseOver,mouseLeave",
			mouseStateCallback : "mv3.hover"
		});
		var mv3 = {
			click : function() {
				if (user.get("userId") > 0) {
					if (isVip && HeMvDownloadCount > 0) {
						return $('#videoUrl').text() + '?videoFileType=he';
					} else {
						if (!isVip) {
							confirm('需要成为会员才能下载超清版MV！');
						} else if (HeMvDownloadCount == 0) {
							confirm('尊敬的会员，这个月超清版MV下载权限已用光！');
						}
					}
				} else {
					user.login();
				}
			},
			hover : function(status) {
				var mvdown1 = $('.mv3');
				if (status == 'mouseOver') {
					mvdown1.addClass('hover');
				} else if (status == 'mouseDown') {
					mvdown1.addClass('active');
				} else if (status == 'mouseUp') {
					mvdown1.removeClass('hover');
					mvdown1.removeClass('active');
				} else if (status == 'mouseLeave') {
					mvdown1.removeClass('hover');
				}
			},
			complete : function(status) {
				if (status) {
					confirm('下载地址已复制到剪贴板，如果您已下载并开启"音悦台PC客户端"，"音悦台PC客户端"会自动弹出下载窗口。\u000d\u000d提示：尊敬的会员，您还可以免积分下载超清版版MV：' + HeMvDownloadCount + '首！');
				}
			}
		};
		window.mv3 = mv3;
		
		
		loadFlash("mv4", {
			swfName : "mv4",
			clipboardComplete : "mv4.complete",
			getClipboardContent : "mv4.click",
			mouseStateEventTypeList : "mouseDown,mouseUp,mouseOver,mouseLeave",
			mouseStateCallback : "mv4.hover"
		});
		var mv4 = {
			click : function() {
				if (user.get("userId") > 0) {
					if (isVip && HeMvDownloadCount > 0) {
						return $('#videoUrl').text() + '?videoFileType=sh';
					} else {
						if (!isVip) {
							confirm('需要成为会员才能下载1080P版MV！');
						} else if (HeMvDownloadCount <= 0) {
							confirm('尊敬的会员，这个月1080P版MV下载权限已用光！');
						}
					}
				} else {
					user.login();
				}
			},
			hover : function(status) {
				var mvdown1 = $('.mv4');
				if (status == 'mouseOver') {
					mvdown1.addClass('hover');
				} else if (status == 'mouseDown') {
					mvdown1.addClass('active');
				} else if (status == 'mouseUp') {
					mvdown1.removeClass('hover');
					mvdown1.removeClass('active');
				} else if (status == 'mouseLeave') {
					mvdown1.removeClass('hover');
				}
			},
			complete : function(status) {
				if (status) {
					confirm('下载地址已复制到剪贴板，如果您已下载并开启"音悦台PC客户端"，"音悦台PC客户端"会自动弹出下载窗口。\u000d\u000d提示：尊敬的会员，您还可以免积分下载1080P版MV：' + HeMvDownloadCount + '首！');
				}
			}
		};
		window.mv4 = mv4;
		
	});

})
