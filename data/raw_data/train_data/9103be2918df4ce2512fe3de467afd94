<script type="text/javascript">
	
	$(window).load(function() {
		$('#subNavTriggers a').on('click', function() {
			var index = $(this).index();
			$(this).addClass('current').siblings().removeClass('current');
//            $('.toggle_cont').eq(index).show().siblings('.toggle_cont').hide();
			$('.toggle_cont').eq(index).removeClass('tab_hide').addClass('tab_show').siblings('.toggle_cont').removeClass('tab_show').addClass('tab_hide');
		});
		var videoContent = $('#videoContent');
		if (videoContent.height() > 66) {
			videoContent.attr({
				'height' : '66px',
				'overflow' : 'hidden',
				'position' : 'relative'
			});
			$('#videoContentMore').attr({
				'position' : 'absolute',
				'bottom' : '0',
				'right' : '0',
				'display' : 'block',
				'width' : '73px',
				'background' : '#fff',
				'text-align' : 'center'
			});
			$('#videoContentMore').on('click', function() {
				var h = videoContent.attr('height');
				if (h != '66px') {
					videoContent.attr('height', '66px');
					this.attr({
						position : 'absolute',
						display : 'block'
					});
					this.html(' [<a href="#">查看全部</a>]</span>');
				} else {
					videoContent.attr('height', 'auto');
					this.attr({
						position : 'static',
						display : 'inline',
						'float' : 'none'
					});
					this.html(' [<a href="javascript:void(0)">收回描述</a>]</span>');
				}
				return false;
			});
		}
	});
