(function(cfg, doc) {
    var el = doc.getElementById('gcSlider');
    var templateWrap = '\
    <div class="slider-wrap">\
        <ul>{item}</ul>\
    </div>\
    <div class="slider-trigger">\
        <div class="slider-nav">{trigger}</div>\
    </div>';
    var resultItems = [];
    var resultTriggers = [];

    var i, curr,
    len = cfg.DATA_MSlide.length;

    for (i = 0; i < len; i++ ) {
        var curr = cfg.DATA_MSlide[i];
        var currClass = i==0?'curr':'';
        var currSrc = i==0?'src':'data-src';
        //var clstag= 'secondtype|keycount|newbook|nb_focus' + (i+1);

        var templateItem = '\
        <li class="fore'+ i +' item ui-slider-item '+ currClass +'" >\
            <div class="i-wrap">\
                <div class="i-inner">\
                    <a target="_blank" class="i-link" href="'+ curr.href +'">\
                        <img class="" '+ currSrc +'="'+ curr.src +'" width="'+ curr.width +'" height="'+ curr.height +'" alt="'+ curr.alt +'" />\
                    </a>\
                </div>\
            </div>\
        </li>';
        var templateTrigger = '<a class="ui-slider-trigger '+ currClass +'" href="#none"></a>';

        resultItems.push(templateItem);
        resultTriggers.push(templateTrigger);
    }

    if (el) {
        el.innerHTML = templateWrap.replace('{item}', resultItems.join('')).replace('{trigger}', resultTriggers.join(''));
    };

})(pageConfig, document);
</script>
