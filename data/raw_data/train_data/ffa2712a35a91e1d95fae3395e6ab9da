</section>


    <script src="http://newhouse.js.soufunimg.com/ju/TaoWap/Resource/seckill/script/jquery-1.11.1.min.js"></script>
    <script src="http://newhouse.js.soufunimg.com/ju/TaoWap/Resource/seckill/script/swiper.3.1.7.jquery.min.js"></script>
    
    <script>
        var count = 5;
        function CountDown() {
            count--;
            if (count == -1) {
                self.location = "http://m.fang.com";
            }
            $("#counter").text(count < 0 ? 0 : count);
        }
        $(function () {
            self.setInterval("CountDown()", 1000)
        })
    </script>



