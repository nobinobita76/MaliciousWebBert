<script>
                   $(function(){
    var $uli=$(".video ul li");
    var $em=$(".video ul li em");
    
    var $reson=$(".video .beside_1");
    var $wq=$(".video ul li");
//  console.log($reson.index());
    $reson.eq(0).addClass("show_1").removeClass("beside_1");
    $em.eq(0).addClass("em_1").removeClass("em");
    $uli.bind("mouseover",function(){
    var num=$(this).index();
     
      $reson.addClass("none_1");
$reson.eq(num/2).addClass("show_1").removeClass("none_1");
      
$em.addClass("em");
$em.eq(num/2).addClass("em_1").removeClass("em");
    });
});
