var url = document.location.href;
var urlarray = url.split("_");
if (urlarray.length==1) {
	var nexturl = url.substring(0,url.length-5)+"_2.html";
} else {
	if (parseInt(urlarray[1])==5) {
		var nexturl = urlarray[0]+".html";
	} else {
		var nexturl = urlarray[0]+"_"+(parseInt(urlarray[1])+1)+".html";
	}
}
function addHref(element)
{
    var imglink = document.createElement("a");
    imglink.href = nexturl;
    imglink.title = "点击查看下一页";
    element.parentNode.insertBefore(imglink,element);
    imglink.appendChild(element);
}
var imgobj = document.getElementById("article_content").getElementsByTagName("img");
