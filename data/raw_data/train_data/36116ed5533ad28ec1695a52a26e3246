<script> 
  var displayModel = function(value, id, flag, type, params)
  {
    var brand_nbr = value;
    var model_name = "phone";
    var product_id = "654589";
    var keyword_id = "1708";
    jQuery.ajax({
      url:'../../model',   //为请求的url
      type: 'GET',     //暂用post，如果用get服务器上需要做出相应的改变
      data:{
        product_id:product_id,
        brand_nbr:brand_nbr,
        keyword_id:keyword_id
      },
      success:function(msg){ //其中msg是服务器上返回的数据，可以alert查看一下
        var tmp=eval(msg);   //将字符串转化成为数组
        var str='';           //存储拼凑出来的html字符串  
        $(tmp).each(function(){
          var url = 'http://www.9game.cn/sys/prd/prd/' + brand_nbr + '-' + this.ename + '-djb-' + product_id + '.html';
          if(type == "mdl")
          {
            var order_type = "t";
            if(params.order_type != undefined)
                order_type = params.order_type;
            url = 'http://www.9game.cn/sys/list/mdl/' + brand_nbr + '-' + this.ename + '-djb-' + order_type + '-0.html';
          }
          if(this.ename == model_name)
              str += '<option value="' + url + '" selected>' + this.cname +'</option>';
          else
              str += '<option value="' + url + '">' + this.cname +'</option>';
        }); 
        $('#' + id).html(str);
        if(flag && flag == true)
          $('#' + id).append("<option selected>请选择机型</option>");
      }
    });
  }
  $('a').click(function(event){
      var href = event.target.href;//获取触发事件的<a>元素的href属性值
      //console.log('href:' + href);
      var click_fn = event.target.onclick;
        if((href == null) || (href.indexOf('http://www.9game.cn/sys') == 0))
            addTime('9game');
        else if(click_fn == null)
            addTimeAndStat('9game');
  });
  var stat = function(prd, category_name, from){
      var url_prefix = 'http://www.9game.cn/sys/stat?brand_nbr=android&model_ename=phone&keyword_name=单机版&prd=' + prd + '&category_name=' + category_name + "&from=" + from;
      commonStat(url_prefix, '9game', 'download');
  };
  //添加我的品牌展示函数
  var displayBrand = function(id, cascade)
  {
    jQuery.ajax({
      url:'http://www.9game.cn/sys/my_brand',   //为请求的url
      type: 'GET',     //暂用post，如果用get服务器上需要做出相应的改变
      data:{
        product_id:"654589",
        type:"1",
        la:"chinese"
      },
      success:function(msg){ //其中msg是服务器上返回的数据，可以alert查看一下
        var tmp=eval(msg);   //将字符串转化成为数组
        var str='';           //存储拼凑出来的html字符串  
        $(tmp).each(function(){
          var url = this.nbr;
          if(cascade === false)
            {
                              url = 'http://www.9game.cn/sys/nav/nav/' + this.nbr + '-djb-654589.html';
                          }
            if(this.nbr.toLocaleLowerCase() == "android".toLocaleLowerCase())
              str += '<option value="' + url + '" selected>' + this.cname +'</option>';
          else
              str += '<option value="' + url + '">' + this.cname +'</option>';
        });
        $('#' + id).html(str);
      }
    });
  }
</script>
