var setting = {
group_id: "58158221374"
};
$(function(){
// 进入页面时就检查是否安装了段子，安装的话直接跳到段子中打开
var obj = {
'app': Common.Util.getUrlParam(location.href, 'app') || 'joke_essay',
'id': setting.group_id
};
try{
setTimeout(function () {
jumpToNativeapp(obj);
}, 1000);
} catch(e){
console.log(e);
}
var getGroupData = function(){
return $.ajax({
url: Common.Util.packURL('/group/' + setting.group_id + '/'),
type: 'GET',
dataType: 'json',
data: {
is_json: 1
},
timeout: 5000
});
};
//导航条先渲染
require('neihan_common/pagelet/nav/nav').create();
getGroupData().then(function(groupDataRes){
if(groupDataRes.the_data){
setting.group_data = groupDataRes.the_data;
var title = $.trim(setting.group_data.text);
if(title) document.title = title;
}else{
//Handle no group data
}
try {
require('neihan_common/pagelet/detail-content/detail-content').create($.extend({}, setting, {
opts: {
noUserLink: true,
noCategoryLink: false,
detailPage: true,
isWap2app: true
},
updateTypeSourceMap: {
'ESSAY': 'jokeshare',
'IMAGE': 'picshare',
'VIDEO': 'videoshare',
'GIF': 'gifshare'
}
}));
require('neihan_common/pagelet/comment/comment').create($.extend({}, setting, {
}));
//init all pagelet
require('pagelet/relate-video/relate-video').create({
isWap2app: true
});
require('neihan_common/pagelet/banner/banner').extend({el: '#pageletTopBanner'}).create();
require('neihan_common/pagelet/banner/banner').extend({el: '#pageletBottomBanner'}).create();
require('neihan_common/pagelet/popup/popup').create();
require('neihan_common/pagelet/bonus/bonus').create();
} catch(e){
console.log(e);
}
$('body').removeClass('loading');
$('.opacity0').animate({'opacity': 1}, 'fast');
//init weixinShare
if($.browser.weixin){
try{
require('neihan_common/common/util/weixinUtil').init();
} catch(e) {
console.log(e);
}
}
// 下载位处理，先尝试调起客户端，再尝试下载
