var updateDownloadUrlBySource = function(sourceName, positionNames) {
if(!sourceName) return false;
var targetURL = SOURCE_URL_SETTING[sourceName];
if(!targetURL) return false;
if(positionNames){
positionNames = $.isArray(positionNames) ? positionNames : [positionNames];
for(key in DOWNLOAD_URL_SETTING) {
if(DOWNLOAD_URL_SETTING.hasOwnProperty(key) && $.inArray(key, positionNames) > -1) {
DOWNLOAD_URL_SETTING[key] = targetURL;
}
}
}else{
for(key in DOWNLOAD_URL_SETTING) {
if(DOWNLOAD_URL_SETTING.hasOwnProperty(key)) {
DOWNLOAD_URL_SETTING[key] = targetURL;
}
}
}
};
