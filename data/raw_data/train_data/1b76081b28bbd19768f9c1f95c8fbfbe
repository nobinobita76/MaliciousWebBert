var length=liNumber*130;
var isno = true;

//小图滑动效果	

$(".pop_img_list .next").live('click',function()
{
	$(".pop_img_list .con ul").css("width",length + "px");
	var me = $(".pop_img_list .con ul");
	var l= parseFloat(me.css('left'))


	if( l-650 > -length)
	{
		if(isno)
		{
			if(isno)
            {
    			isno = false;
    			me.animate({left:"-=130px"},300,"linear",function()
    			{
    				isno = true;
    			});
		    }
	    }
	}
});
		 
$(".pop_img_list .back").live('click',function(){
	var me = $(".pop_img_list .con ul");
	var l= parseFloat(me.css('left'))
	if(l<0){
		if(isno)
		{
				if(isno){
				isno = false;
				me.animate({left:"+=130px"},300,"linear",function()
				{
					isno = true;
				});
		}
	}
	}
});

//点击照片左右滑动
