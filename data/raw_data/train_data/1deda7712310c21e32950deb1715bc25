<script>

window.pageid = 1;
var ename = "weifang" || '';
var channel = location.search.replace(/^.*(\?|&)channel=([\-\w]+).*$/, "$2");
channel = channel == location.search ? '' : channel;

$.ajax({
    url: '/apis/apis_car/home_recommend/',
    type: 'get',
    data: { cityid: '2105', cityname :ename, uid : $.getCookie('XIN_UID_CK') || '',_t :new Date().getTime()},
    dataType: 'json',
    success: function(data){
        var data = data.data;
        var recommend_series = '';
        if(data && $.isArray(data)){
            var show_num = data.length>3 ? 3 : data.length;
            for(var i = 0,len = show_num; i < len; i++){
                var price = data[i]['price'];
                var car_price = (!price || isNaN(price)) ? '' : (parseInt(price * 100) / 100).toFixed(2);
                var car_name = data[i]['scname'];
                recommend_series += '<dd><a abevent="home/half/'+ i +'/brand/'+data[i]['brandid']+'/series/'+data[i]['scid']+'" onclick="zhuge.track(\'C-首页-付一半位置-'+i+'\');" href="/weifang'+data[i]['series_url']+'sn_v5/' + (channel ? '?channel=' + channel : '') + '"><h3>'+
                        car_name+'</h3><span><strong>'+car_price+'</strong>万起</span>'+
                        '<img src="//s5.xinstatic.com/xinwap/images/index/carbw/'+data[i]['standar_scid']+'.png"/></a></dd>';
            }
            $('#recommend_series').html(recommend_series);
            $('div.half-wrap,div.quality-bar').show();
        } else {
            $('div.half-wrap,div.quality-bar').hide();
        }
    }
});


