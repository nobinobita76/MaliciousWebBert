    //M端全站诸葛打点
    function zhuge_track(event, properties,href) {
    	if (typeof(event)=='string' && event.length>0 && event.length<=32) {
    		if (properties && typeof(properties)=='object') {
                if(href){
                    zhuge.track(event, properties,function(){
                        window.location.href = href
                    });
                }else{
                    zhuge.track(event,properties);
                }
    		} else {
    			zhuge.track(event);
    		}
    	}
    	return true;
    }
    
    (function ($){
        if(!$) {
          console.warn('缺少zepto或者jquery库');return;
        }
        $(function(){
            if(!zhuge || !zhuge.track ) return;
            $(document).on('click','.xinStatistics',function(ev){
                var $this = $(this),data;
                if(data = $this.attr('data-statistics')){
                    try{
                        data = $.parseJSON(data);
                    }catch(e){
                        console.warn('JSON格式的字符串解析错误',e);
                        return;
                    }
                    var event = data.title || '';
                    var properties = data.data || '';
                    var href =  $this.attr('href');
                    if(event){
                        if(properties){
                            if(href){
                                zhuge.track(event, properties ,function(){
                                    window.location.href = href;
                                });
                                return false;
                            }
                            zhuge.track(event, properties);
                        }else{
                            zhuge.track(event);
                        }
                    }
                }
            });
        });
