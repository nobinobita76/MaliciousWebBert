    var goto_car_detail_flg = /goto_car_detail_flg\s*=\s*([^;]+)\;/.exec(document.cookie);
    if (goto_car_detail_flg && goto_car_detail_flg.length > 1 && goto_car_detail_flg[1] == 1) {
        document.cookie = "goto_car_detail_flg=0;path=/;expires:0;domain:'.xin.com'";
    }
    // 常见问题
    $('.qa-list dd').click(function() {
        if($(this).find('a').hasClass('open')){
            $(this).find('p,a').removeClass('open').addClass('close');
        }else{
            $('.qa-list dd').find('p,a').removeClass('open').addClass('close');
            $(this).find('p,a').removeClass('close').addClass('open');
        }
