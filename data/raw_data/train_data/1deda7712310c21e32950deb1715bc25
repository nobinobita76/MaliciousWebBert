    $(".buy_tab").click(function(event) {
        var tab_name = $(this).attr("tab_name");        
        if(tab_name == '付一半购车'){        
            $(".buy-wrap .mores").attr("href","/"+city+"/sn_v5/").attr("onclick","zhuge.track('C-首页-超值购车-更多',{ '城市':'"+city+"','类别':'付一半购车' });");
        }else if(tab_name == '优信认证'){
            $(".buy-wrap .mores").attr("href","/"+city+"/sn_v9/").attr("onclick","zhuge.track('C-首页-超值购车-更多',{ '城市':'"+city+"','类别':'优信认证'});");
        }
