    //home search seo-url 暂定方法
    $('.search-btn').click(function(event) {
        //seo-v5修改URL。|s/b62s269/?q=奥迪A6L
        var history_arr = [];
        var cityer = ename || 'quanguo';
        setTimeout(function(){
            //将查询串收集一次发送
            $('.hot-cars-list dd a').each(function(i,v){
                var o_url = $(this).attr('data-query-url');
                var regx = /\/s\/(.*)/;
                if(regx.test(o_url)) {
                    history_arr.push($(this).attr('data-query-url'));
                }
            });
            if(history_arr.length > 0) {
                $.get('/apis/apis_car/get_home_seo_url', { 'hist_url':history_arr, 'cityer':cityer }, function(newUrl){
                    $.each(newUrl,function(i,v){
                        $('.hot-cars-list dd a[data-query-url="'+v.old_url+'"]').attr('data-query-url',v.new_url);
                    });
                },'json');
            }
            
        },1000); 
        //seo-v5修改URLend    
    });
});
