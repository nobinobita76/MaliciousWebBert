<script>
    $(function() {
        
        /* TODO:rebuild search.js begin */
        var SUGGEST_QUERY_URL = {
            car: ('https:' == document.location.protocol ? 'https://' : 'http://') + 'www.' + (location.hostname == 'm.xin.com' ? '' : 'test.') + 'xin.com/apis/apis_suggest/match',
            store: ('https:' == document.location.protocol ? 'https://' : 'http://') + 'www.' + (location.hostname == 'm.xin.com' ? '' : 'test.') + 'xin.com/apis/apis_suggest/shop',
            hot: ('https:' == document.location.protocol ? 'https://' : 'http://') + 'www.' + (location.hostname == 'm.xin.com' ? '' : 'test.') + 'xin.com/apis/apis_suggest/hot'
        };

        function resetSuggestSearch() {
            var defaultCategory = $('._suggest-category-list a[data-default-suggest="1"]'),
                    category = defaultCategory.attr('data-suggest-category'),
                    categoryText = defaultCategory.attr('data-suggest-category-text'),
                    placeholder = defaultCategory.attr('data-placeholder');

            $('._suggest-category-list-btn').text(categoryText);
            $('._suggest-keyword').attr('placeholder', placeholder);
            $('._suggest-keyword').attr('data-current-suggest-category', category);

            $('._suggest-keyword').val('');

            $('._suggest-category-list').css('display', 'none');

            $('._suggest-list dd').remove();
            $('._suggest-list').css('display', 'none');

            $('._suggest-clean-btn').css('display', 'none');
        };

        function getQueryURL(url, keyword, type) {
            return url + '?q=' + keyword;
        };
        function saveCarHistoryCookie(url, keyword, num) {
            // 与列表页cookie保持一致
            var history = /XIN_SUGGEST_HISTORY\s*=\s*([^;]+)\;/.exec(document.cookie),
                    data = {};

            if (history && history[1]) {
                try {
                    var jsonData = JSON.parse(decodeURIComponent(history[1]));
                    data = jsonData ? jsonData : {};
                } catch (e) {
                }
            }
            data[keyword] = {
                query: url,
                label: keyword,
                num: num
            };
            document.cookie = 'XIN_SUGGEST_HISTORY=' + encodeURIComponent(JSON.stringify(data)) + ';path=/';
        };
        function saveStoreHistoryCookie(url, keyword) {
            var history = /XIN_SUGGEST_SHOP_HISTORY\s*=\s*([^;]+)\;/.exec(document.cookie),
                    data;

            if (history && history.length > 1 && JSON.parse(decodeURIComponent(history[1]))) {
                data = JSON.parse(decodeURIComponent(history[1]));

                if (!$.isArray(data)) {
                    data = $.map(data, function (el) {
                        return el
                    });
                }
                ;

                for (var i = data.length - 1; i >= 0; i--) {
                    if (decodeURIComponent(data[i].label) == keyword) data.splice(i, 1);
                }

                data.unshift({
                    query: url,
                    label: keyword
                });
            } else {
                data = [{
                    query: url,
                    label: keyword
                }];
            }
            document.cookie = 'XIN_SUGGEST_SHOP_HISTORY=' + encodeURIComponent(JSON.stringify(data)) + ';path=/';
        };
        function getCarHistoryCookie() {
            var history = /XIN_SUGGEST_HISTORY\s*=\s*([^;]+)\;/.exec(document.cookie);
            return history && history.length > 1 ? history[1] : '';
        };
        function getStoreHistoryCookie() {
            // var history = /SUGGEST_STORE_HISTORY\s*=\s*([^;]+)\;/.exec(document.cookie);
            var history = /XIN_SUGGEST_SHOP_HISTORY\s*=\s*([^;]+)\;/.exec(document.cookie);
            return history && history.length > 1 ? history[1] : '';
        };
        //显示建议列表
        function renderSuggestList() {
            $.ajax({
                type: 'GET',
                url: SUGGEST_QUERY_URL[$('._suggest-keyword').attr('data-current-suggest-category')],
                data: {
                    w: $('._suggest-keyword').val(),
                    c: $('._city').attr('data-city-id'),
                    s: 'wap',
                    h: /sn_[\w\-\.]*v5[\w\-\.]*/.test(location.pathname) ? 1 : 0
                    // source: /v5/.test(location.pathname)?'halfm':'wap'
                },
                dataType: 'jsonp',
                jsonp: 'jsoncallback',
                success: function (data) {
                    if (!data || (data && data.length == 0)) {
                        $('._suggest-list').css('display', 'none');
                        $('._suggest-list dd').remove();
                        return;
                    }
                    $('._suggest-list dd').remove();
                    var list = [],
                            template = $('#_suggest-list-item').html(),
                            len = data.length;
                    var type = $('a._suggest-category-list-btn').text();
                    for (var i = 0; i < len; i++) {
                        var text = data[i].label,
                                queryUrl = getQueryURL(data[i].query, text, type),
                                count = data[i].num,
                                html = template;
                        if (type == '店铺') {
                            queryUrl = queryUrl.replace(/.*\/d\//, '/d/');
                            html = html.replace(/data-query-url=\"\/.*?\"/, 'data-query-url="{$queryUrl$}"');
                        }
                        html = html.replace(/{\$abevent\$}/g, '/m_home/search_click/rank/' + i + '/word/' + text);
                        html = html.replace(/{\$queryUrl\$}/g, queryUrl)
                        html = html.replace(/{\$text\$}/g, text);
                        html = html.replace(/{\$count\$}/g, count);
                        html = html.replace(/{\$countText\$}/g, type == '店铺' ? '' : ('约' + count + '条车源'));

                        list.push(html);
                    }
                    $('._suggest-list').append($(list.join('')));
                    $('._suggest-list').css('display', 'block');
                }
            });
        };
        // 显示历史搜索记录
        function renderCarHistoryList() {
            renderHistoryList(getCarHistoryCookie())
        };
        function renderStoreHistoryList() {
            renderHistoryList(getStoreHistoryCookie());
        };
        function renderHistoryList(history) {
            try {
                var data = history ? JSON.parse(decodeURIComponent(history)) : {};
            } catch (e) {
                return;
            }
            $('._history-list dd').remove();

            var list = [],
                    tmpData = [],
                    template = $('#_history-list-item').html();
            // len = data.length;

            for (var name in data) {
                if (data.hasOwnProperty(name)) {
                    tmpData.push(data[name]);
                }
            }

            if (tmpData.length == 0) {
                $('.history-list-wrap,.SearchCookies-history-top').css('display', 'none');
                $('._history-list dd').remove();
                return;
            }

            for (var i = 0, len = tmpData.length; i < len; i++) {
                if(i >= 20){
                    break;
                }
                var text = decodeURIComponent(tmpData[i].label),
                        queryUrl = getQueryURL(decodeURIComponent(tmpData[i].query), text),
                        num = decodeURIComponent(tmpData[i].num || ''),
                        html = template;

                html = html.replace(/{\$queryUrl\$}/g, queryUrl);
                html = html.replace(/{\$text\$}/g, text);
                list.push(html);
            }
            $('._history-list').append($(list.join('')));
            $('.history-list-wrap,.SearchCookies-history-top').css('display', 'block');
        };

        function displaySuggestCleanBtn() {
            if ($('._suggest-keyword').val().trim()) {
                $('._suggest-clean-btn').css('display', 'block');
            } else {
                $('._suggest-clean-btn').css('display', 'none');
            }
        };
         $('._search-btn').on('click',function(){
             var category = $('._suggest-keyword').attr('data-current-suggest-category');

             if (category == 'car'){
                 renderCarHistoryList();
             }else if(category == 'store'){
                 renderStoreHistoryList();
             }
             $('._hot-cars-wrap').css('display', 'block');

             // TODO:remove window scroll
             $('._suggest-layer').css('display','block');
             $('._download-bar,.header-wrap,.banner-wrap,.deal-bar,.half-wrap,.wrap,._service-bar,footer,._return-service-btn').hide();

             // TODO:default focus on input
             $('._suggest-keyword').focus();
             $('._suggest-keyword').val('');

         });
        $('._suggest-category-list-btn').on('click', function () {
            $('._suggest-category-list').css('display', 'block');
        });
        $('._suggest-category-list a').on('click', function () {
            var that = this,
                    $currentCateBtn = $('._suggest-category-list-btn'),
                    category = $(that).attr('data-suggest-category'),
                    categoryText = $(that).attr('data-suggest-category-text'),
                    placeholder = $(that).attr('data-placeholder');

            var cuurentCateText = $currentCateBtn.text();
            if (cuurentCateText != categoryText) {
                $currentCateBtn.text(categoryText);
                $('dl._suggest-list.suggest-list').hide();
                $('._suggest-keyword').val('');
            }

            $('._suggest-keyword').attr('placeholder', placeholder);
            $('._suggest-keyword').attr('data-current-suggest-category', category);

            $('._suggest-category-list').css('display', 'none');

            if (category == 'car') {
                $('._hot-cars-wrap').show();
                renderCarHistoryList();
            } else if (category == 'store') {
                $('._hot-cars-wrap').hide();
                renderStoreHistoryList();
            }

        });
        $('._suggest-cancel-btn').on('click', function () {
            $('._suggest-layer').css('display', 'none');
            $('._download-bar,.header-wrap,.banner-wrap,.deal-bar,.half-wrap,.wrap,._service-bar,footer,._return-service-btn').show();
            resetSuggestSearch();
        });
        $('._suggest-keyword').on('focus', function (e) {
            $('._suggest-category-list').css('display', 'none');
        });
        $('._suggest-keyword').on('keyup', function (e) {
            var that = this, category = $(that).attr('data-current-suggest-category');
            displaySuggestCleanBtn();
            renderSuggestList();
            var keyword, queryUrl;
            if (e.keyCode == 13) {
                var kw_reg = new RegExp("[!${}=<>?&']");  // 过滤特殊字符
                var channel_param = channel ? 'channel=' + channel : '';
                var input_word = $('.suggest-keyword').val();
                if (!kw_reg.test($.trim(input_word))) {
                    if ($('._suggest-list dd').length > 0) {
                        var firstRecord = $('._suggest-list dd:nth-of-type(1) a');

                        keyword = firstRecord.attr('data-keyword');
                        queryUrl = firstRecord.attr('data-query-url');
                        var patn_keyword = new RegExp("[&\?]q=" + keyword);
                        if (channel) {
                            channel_param = (queryUrl.indexOf('?') === -1 ? '?' : '&') + channel_param;
                        }
                        channel_param = queryUrl.indexOf('channel=') === -1 ? channel_param : '';
                        if (category == 'car') {
                            saveCarHistoryCookie(queryUrl.replace(patn_keyword, ''), keyword);
                            window.location.href = queryUrl + channel_param;
                        } else if (category == 'store') {
                            saveStoreHistoryCookie(queryUrl.replace(patn_keyword, ''), keyword);
                            window.location.href = queryUrl + channel_param;
                        }
                    } else {
                        keyword = $(that).val();
                        queryUrl = location.pathname + 's/';
                        if (channel) {
                            channel_param = (queryUrl.indexOf('?') === -1 ? '?' : '&') + channel_param;
                        }
                        channel_param = queryUrl.indexOf('channel=') === -1 ? channel_param : '';
                        var patn_keyword = new RegExp("[&\?]q=" + keyword);
                        if (category == 'car') {
                            saveCarHistoryCookie(queryUrl, keyword);

                            window.location.href = queryUrl + '?q=' + keyword + channel_param;
                        } else if (category == 'store') {
                            //saveStoreHistoryCookie(queryUrl, keyword);
                        }
                    }

                    uxl_track('m_home/search_input/word_search/' + $.trim(input_word) + '/word_result/' + keyword);
                }
            }
        });
        $('._suggest-clean-btn').on('click', function () {
            $('._suggest-keyword').val('');

            $('._suggest-list dd').remove();

            displaySuggestCleanBtn();
        });
        // suggest下拉、选择history
        $('._suggest-layer').on('click', '._suggest-list a,._history-list a', function (ev) {
            var category = $('._suggest-keyword').attr('data-current-suggest-category'),
                    keyword = $(this).attr('data-keyword'),
                    queryUrl = $(this).attr('data-query-url'),
                    num = $(this).attr('data-num');
            var patn_keyword = new RegExp("[&\?]q=" + keyword);
            var channel_param = channel ? 'channel=' + channel : '';
            if (channel) {
                channel_param = (queryUrl.indexOf('?') === -1 ? '?' : '&') + channel_param;
            }
            channel_param = queryUrl.indexOf('channel=') === -1 ? channel_param : '';
            if (category == 'car') {
                saveCarHistoryCookie(queryUrl, keyword, num);
                window.location.href = queryUrl + channel_param;
            } else if (category == 'store') {
                saveStoreHistoryCookie(queryUrl, keyword);
                window.location.href = queryUrl + channel_param;
            }
        });
        $('._suggest-layer').on('click', '._hot-cars-list a', function () {
            var keyword = $(this).attr('data-keyword'),
                    queryUrl = $(this).attr('data-query-url');
            var patn_keyword = new RegExp("[&\?]q=" + keyword);
            saveCarHistoryCookie(queryUrl.replace(patn_keyword, ''), keyword);

            var channel_param = channel ? 'channel=' + channel : '';
            if (channel) {
                channel_param = (queryUrl.indexOf('?') === -1 ? '?' : '&') + channel_param;
            }
            channel_param = queryUrl.indexOf('channel=') === -1 ? channel_param : '';
            window.location.href = '/' + location.pathname.split('/')[1] + queryUrl + channel_param;
        });
        $('._empty-history-btn').on('click', function () {
            var category = $('._suggest-keyword').attr('data-current-suggest-category');

            if (category == 'car') {
                document.cookie = 'XIN_SUGGEST_HISTORY=' + JSON.stringify('') + ';path=/';
            } else if (category == 'store') {
                document.cookie = 'XIN_SUGGEST_SHOP_HISTORY=' + JSON.stringify('') + ';path=/';
            }

            //window.location.reload();
            $('.SearchCookies-history-top,.history-list-wrap').hide();
            $(".mask,.removeHistory").hide();
        });

        $(".DelBox").click(function(){
            $(".mask,.removeHistory").show();
        })
        $(".cancel").click(function(){
            $(".mask,.removeHistory").hide();
        })


        /* TODO:rebuild search.js end */
    });
    
</script>


<script>

(function(){
    var Cookie = {
        set: function(e, t, o, i, s, n) {
            document.cookie = e + "=" + (n ? t : encodeURIComponent(t)) + (s ? "; expires=" + s.toGMTString() : "") + (i ? "; path=" + i : "; path=/") + (o ? "; domain=" + o : "")
        },
        get: function(e, t) {
            var o = document.cookie.match(new RegExp("(^| )" + e + "=([^;]*)(;|$)"));
            return null != o ? decodeURIComponent(o[2]) : t
        },
        clear: function(e, t, o) {
            this.get(e) && (document.cookie = e + "=" + (t ? "; path=" + t : "; path=/") + (o ? "; domain=" + o : "") + ";expires=Fri, 02-Jan-1970 00:00:00 GMT")
        }
    };
    //compatible IE8
    var JSON = JSON || {};
    JSON.stringify = JSON.stringify || function (obj) {
        var t = typeof (obj);
        if (t != "object" || obj === null) {
            if (t == "string") obj = '"'+obj+'"';
            return String(obj);
        }
        else {
            var n, v, json = [], arr = (obj && obj.constructor == Array);
            for (n in obj) {
                v = obj[n]; t = typeof(v);
                if (t == "string"){
                    v = '"'+v+'"';
                } else if(t == "object" && v !== null) {
                    v = JSON.stringify(v)
                }
                json.push((arr ? "" : '"' + n + '":') + String(v));
            }
            return (arr ? "[" : "{") + String(json) + (arr ? "]" : "}");
        }
    };
    //data format => {"case":"xxx","page":"xx"};
    var cid = Cookie.get('XIN_UID_CK'),timer= null;
    if(!cid){
        timer = setInterval(function(){
            cid = Cookie.get('XIN_UID_CK');
            if(cid){
                setTranferCookie(cid)
                clearInterval(timer);
            }
        },200);
        setTimeout(function(){
            clearInterval(timer);
        },5000)
    }else{
        setTranferCookie(cid);
    }
    function setTranferCookie(cid){
        cid = cid || Cookie.get('XIN_UID_CK');
        var transAdd = Cookie.get('XIN_transfer_spoor'),
            transNow = Cookie.get('XIN_transfer_'+cid);
        if(cid && transAdd){
            transAdd = eval('(' + transAdd + ')');
            transNow = transNow ? eval('('+transNow+')') :[];
            if(transNow.length == 0){
                transNow.push(transAdd);
            }else{
                if(transAdd.case == 0) return;
                for(var i =0 ;i<transNow.length;i++){
                    if(transNow[i].case == transAdd.case){
                        return;
                    }
                }
                transNow.push(transAdd);  
            }
            Cookie.set('XIN_transfer_'+cid, JSON.stringify(transNow) , '.xin.com', '/' ,new Date(9999,11,30));
        }   
    }
})();

</script>

        
        <script>function fix_channel_div(e,t){var channel="{$channel}";channel=location.search.replace(/.*?channel=([^\s&?]+).*/,"$1");channel=channel==location.search?"":channel;if(channel){var ela="";if(t=="id"){ela=("#"+e+" ")}else{if(t=="class"){ela=("."+e+" ")}}$(ela+"a").each(function(){if($(this).attr("href")&&$(this).attr("href").indexOf("channel=")===-1){if(!(($(this).attr("href")=="#")||($(this).attr("href").indexOf("javascript"))==0)){$(this).attr("href",$(this).attr("href")+($(this).attr("href").indexOf("?")===-1?"?":"&")+"channel="+channel)}}})}}function fix_channel_url(url){var channel="{$channel}";channel=location.search.replace(/.*?channel=([^\s&?]+).*/,"$1");channel=channel==location.search?"":channel;if(channel&&url&&(url.indexOf("channel=")===-1)){if(!((url=="#")||(url.indexOf("javascript")==0))){url+=(url.indexOf("?")===-1?"?":"&")+"channel="+channel}}return url}function fix_multi_channel_div(e,t){var channel="{$channel}";channel=location.search.replace(/.*?channel=([^\s&?]+).*/,"$1");channel=channel==location.search?"":channel;if(channel){var ela="";if(t=="id"){ela=("#"+e+" ")}else{if(t=="class"){ela=("."+e+" ")}}$(ela+"a").each(function(){if($(this).attr("href")&&$(this).attr("href").indexOf("channel=")===-1){if(!(($(this).attr("href")=="#")||($(this).attr("href").indexOf("javascript"))==0)){$(this).attr("href",$(this).attr("href")+($(this).attr("href").indexOf("?")===-1?"?":"&")+"channel="+channel)}}else{if($(this).attr("href")&&$(this).attr("href").indexOf("channel="+channel+"&channel="+channel)!==-1){$(this).attr("href",$(this).attr("href").replace("channel="+channel+"&channel="+channel,"channel="+channel))}}})}}fix_channel_div(0,0);</script>
        
        <script>
$(function(){
    $(document).on('click', function(e){
        if(!window._abq) return;
        
        var target = $(e.target);

        while(target.length > 0 && target.get(0) != document){
            var abevent = target.attr('abevent') || target.attr('uxevent');

            if(abevent){
                if(abevent.indexOf('/') == 0){
                    if(abevent) _abq.push(['_track'].concat(abevent.substr(1).split('/')));
                }else{
                    if(abevent) _abq.push(['_track'].concat(abevent.split('/')));
                }
            }

            target = target.parent();
        }
    });
});


