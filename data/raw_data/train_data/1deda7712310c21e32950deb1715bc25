<script type="text/javascript">

    var ua_agent = get_agent().toLowerCase();//获取浏览器agent

    var dl_jump_url = '//' + document.domain + '/download/show_dl_page/'; //下载中间地址
    var ios_app_home_url = 'xinapp://';
    var ios_app_detail_url = 'xinapp://mobile/YXUCarDetailVC2?carID=';
    var android_app_home_url = 'uxin://uxin.app/openWith';
    var android_app_detail_url = 'uxin://uxin.app/openVehicleDetailsActivity?from=wap&&car_id=';

    //跳转到app
    function _getSkipToAppUrl() {
        var skip_url = '';
        if (/ios/gi.test(ua_agent)) {  // ios
            if (_isHomeOrDetail() === 'home') {
                skip_url = ios_app_home_url;
            } else {
                skip_url = ios_app_detail_url + _getCarId();
            }
        } else if (/android/gi.test(ua_agent)) { //安卓
            if (_isHomeOrDetail() === 'home') {
                skip_url = android_app_home_url;
            } else {
                skip_url = android_app_detail_url + _getCarId();
            }
        }
        return skip_url;
    }

    //判断是首页还是详情页
    function _isHomeOrDetail() {
                return "home";
            }

    //获取车辆id
    function _getCarId() {
                return "";
            }

    //跳转到下载页面
    function _getSkipToDownloadUrl(type) {
        if(type == 'weixin'){
            return dl_url_weixin; //微信下载
        }else if (type == 'android'){
            return dl_url_android; //安卓下载
        }else {
            return dl_url; //ios下载
        }
    }

    function _getJumpUrl(){
        //channel
        var channel = '';
        channel = location.search.replace(/^.*(\?|&)channel=([\-\w]+).*$/, "$2");
        channel = channel == location.search ? '' : channel;

        var jump_url = dl_jump_url
        if(channel  != ''){
            jump_url = dl_jump_url + '?channel=' + channel;

            //页面类型
            
            return jump_url;
        }else{
            //页面类型
            
            return jump_url;
        }
    }

    $(document).ready(function () {
        //初始化
        openApp.init({
            downAppAndroid: _getJumpUrl(),
            downAppIos: _getJumpUrl(),
            downWeixin: _getJumpUrl(),
            schema: _getSkipToAppUrl()
        });

        $('._download-btn').on("click", function () {
            dl_app_pro();
        });

    });

    //启动（打开或下载）
    function dl_app_pro() {
        openApp.open();
    }

</script>

