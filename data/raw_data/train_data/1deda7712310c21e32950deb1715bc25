    // C-车辆列表页-车辆位置

    function uxl_carlist_rank(e, type, car_id) {
        var uxe = '';
        if (parseInt(type) == 1) { //付一半
            uxe = 'halfprice_carlist/rank/';
        } else if(parseInt(type) == 2) { //全价车下的付一半
            uxe = 'carlist_half/rank/';
        } else { //默认全价车
            uxe = 'carlist/rank/';
        }
        uxe = uxe + parseInt(e) + '/carid/' + car_id;
        uxl_track(uxe);
    }
    //----------- ab end -----------
    
    // 修复channel链接，针对div
    function fix_channel_div(e,t) {
    	var channel = "{$channel}";
        channel = location.search.replace(/^.*(\?|&)channel=([\-\w]+).*$/, "$2");
        channel = channel == location.search ? '' : channel;
    	if (channel) {
    		var ela = '';
    		if (t=='id') {
    			ela = ('#'+e + ' ');
    		} else if (t=='class') {
    			ela = ('.'+e + ' ');
    		}
    	    $(ela + 'a').each(function () {
    	        if($(this).attr('href') && $(this).attr('href').indexOf('channel=') === -1) {
    	            if(!(($(this).attr('href')=='#') || ($(this).attr('href').indexOf('javascript'))==0)){
    	                $(this).attr('href', $(this).attr('href') + ($(this).attr('href').indexOf('?') === -1 ? '?' : '&') + 'channel=' + channel);
    	            }
    	        }
    	    });
    	}
    }
    
    // 修复channel链接，针对单独的url
    function fix_channel_url(url) {
        var channel = "{$channel}";
        channel = location.search.replace(/^.*(\?|&)channel=([\-\w]+).*$/, "$2");
        channel = channel == location.search ? '' : channel;
        if(channel && url && (url.indexOf('channel=') === -1)) {
            if(!((url=='#') || (url.indexOf('javascript')==0))){
            	url += (url.indexOf('?') === -1 ? '?' : '&') + 'channel=' + channel;
            }
        }
        return url;
    }
    
    // 修复channel链接，针对div
    function fix_multi_channel_div(e,t) {
        var channel = "{$channel}";
        channel = location.search.replace(/^.*(\?|&)channel=([\-\w]+).*$/, "$2");
        channel = channel == location.search ? '' : channel;
        if (channel) {
            var ela = '';
            if (t=='id') {
                ela = ('#'+e + ' ');
            } else if (t=='class') {
                ela = ('.'+e + ' ');
            }
            $(ela + 'a').each(function () {
                if($(this).attr('href') && $(this).attr('href').indexOf('channel=') === -1) {
                    if(!(($(this).attr('href')=='#') || ($(this).attr('href').indexOf('javascript'))==0)){
                        $(this).attr('href', $(this).attr('href') + ($(this).attr('href').indexOf('?') === -1 ? '?' : '&') + 'channel=' + channel);
                    }
                } else if ($(this).attr('href') && $(this).attr('href').indexOf('channel='+channel+'&channel='+channel)!==-1) {
                    $(this).attr('href', $(this).attr('href').replace('channel='+channel+'&channel='+channel, 'channel='+channel));
                }
            });
        }
    }

</script>


