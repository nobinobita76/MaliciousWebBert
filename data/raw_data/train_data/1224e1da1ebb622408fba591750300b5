		function zan(aid,bqid){
			$.ajax({
				type	:	'GET',
				url		: 	'/news/zanNews/aid/'+aid+'/bid/'+bqid,
				success	:	function(data){
					$("a").removeAttr("onclick");
					//if(data == 'success'){
					num = $("#bq"+bqid).text();
					for(i=1 ;i<7;i++){
						num_i = $("#bq"+i).text();
						if(i == bqid){
							if(num_i == ''){
								num_i = 1;
							}else{
								num_i = parseInt(num)+1;
							}

						}else{
							if(num_i == ''){
								num_i = 0;
							}
						}
						$("#bq"+i).html(num_i);
					}
					//}
				},
				error : function(data){
					//alert('系统异常');
				}
			});
		}

