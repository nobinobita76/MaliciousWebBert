    }else{
        $('#forPopupVideo').on('click', function(){
            var $this = $(this);
            var $popupVideo = $('#popupVideo');
            var url = video_playlist_source[0]['file'];
            var $video = '<video src="'+url+'" preload></video>';
            $('.black-drop').show();
            $popupVideo.html($video);
            $video = $popupVideo.find('video').get(0);
            setTimeout(function(){
                $video.play();
            }, 10);
            return false;
        })
        $('.black-drop').on('click', function(){
            $(this).hide();
            $('#popupVideo').html('');
        }).on('click', 'video', function(e){
            e.stopPropagation();
        })
        $('body').on('touchmove', function(e){
            if($('.black-drop').is(':visible')) {
                $('.black-drop').trigger('click')
            }
