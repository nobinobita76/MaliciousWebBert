<script type="text/javascript">
function getstr(str) { 
  if (str.indexOf("\n")>0) {  
    etag=str.indexOf('\n'); 
    rstr=str.substring(0,etag-1) + " (转自雅图www.yatu.tv)\r\n"; 
    rstr=rstr+str.substring(str.indexOf("\n")+1,str.length); 
    return rstr   	
  }	
  else
  {
   rstr=str
   return rstr 
  }
} 

document.getElementById("contents_more").oncopy = function () { 
	setTimeout( function () { 
		var text = clipboardData.getData("text"); 
		
		if (text) { 
		    text=getstr(text) + "「本文转自雅图在线yatu.tv」";
			clipboardData.setData("text", text)
			//text = "「yatour.cn」 "+ text + " 「文章来自:雅图在线 www.yatour.cn」"; 
			//clipboardData.setData("text", text); 
		} 
	}, 100 ) 
}
</script>

