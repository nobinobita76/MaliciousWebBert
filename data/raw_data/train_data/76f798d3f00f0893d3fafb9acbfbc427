<script>
$(function(){
	var i=0;
	$(".ci1 a").eq(0).show();
	setInterval("changeImg1()",3000);
	changeImg1=function(){
		i++;
		if(i==5){i=0;}
		$(".ci1 a").eq(i).fadeIn("slow").siblings().hide();
	}
	var n=0;
	$(".ci2 a").eq(0).show();
	setInterval("changeImg2()",3000);
	changeImg2=function(){
		n++;
		if(i==4){n=0;}
		$(".ci2 a").eq(n).fadeIn("slow").siblings().hide();
	}
});
</script>


