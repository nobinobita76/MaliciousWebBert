$(function(){
	var index=0;
	setInterval(function(){
	    index=index+1;
		index=index>3?0:index;
		$(".snapshot ul").animate({left:-236*index+"px"});
    },5000);
	
	$("input[name='mobile']").focus(function(){
	    //if($(this).val()=="输入手机号免费发送") {
		    $(this).val("");
		//}
	});

	$("input[name='mobile']").blur(function(){
	    if($(this).val()=="") {
		    $(this).val("输入手机号免费发送");
		}
	});
	
	$("#sms-button").click(function() {
        var mobile = $("input[name='mobile']").val();
        $("input[name='mobile']").val("正在发送，请稍候。");
        
        $.ajax({
            type: 'get',
            url: './tel.ajax.php',
            data: {
			    'type': "tel",
                'mobile' : mobile
            },
            dataType: 'json',
			success: function(data){
				$("input[name='mobile']").val(data.title);
            }
        });
    });
});

