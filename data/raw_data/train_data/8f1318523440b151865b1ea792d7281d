    $(function() {

        layer.config({
            extend: ['skin/myskin/style.css'], //加载新皮肤
        });

        $('#span_favor').click(function(){
            var pid = "324267920";
            var timestamp=new Date().getTime();
            $.ajax({
                type: 'get',
                dataType: 'json',
                async : false,
                data: {action:'add', postid:pid},
                url: "/cgi-bin/ajax/ajax_user_favor.php?"+timestamp,
                success: function(data) {
                    if (data.error == 0) {
                        //收藏成功 junping
                        layer.open({
                            type: 1,
                            shift: false,
                            skin: 'layer-ext-custom', //加上边框
                            area: ['auto', 'auto'],
                            shade: [0.3, '#000'],
                            title: '收藏信息',
                            content: $("#favorite_pop"),
                            end: function () {
                                //判断是否已登录
                                if($("#usercenter").css('display') == 'none') {
                                    location.reload();
                                }
                            }
                        });
                    } else if (data.error == 2){
                        //已收藏
                        $("#favorite_pop .sc-box .sc-state").html("<i class='success-icon1'></i>已收藏过本条信息！");
                        layer.open({
                            type: 1,
                            shift: false,
                            skin: 'layer-ext-custom', //加上边框
                            area: ['auto', 'auto'],
                            shade: [0.3, '#000'],
                            title: '收藏信息',
                            content: $("#favorite_pop"),
                            end: function () {
                                //判断是否已登录
                                if($("#usercenter").css('display') == 'none') {
                                    location.reload();
                                }
                            }
                        });
                    }
                    else {
                        //用户未登录 junping
                        if (data.data == '99') {
                            var pid = 324267920;
                        var redir = '/cgi-bin/ajax/ajax_user_favor.php?add='+pid;
                        $('#redir').val(redir);
                        layer.open({
                            type: 1,
                            shift: false,
                            skin: 'layer-ext-custom', //加上边框
                            area: ['auto', 'auto'],
                            shade: [0.3, '#000'],
                            title: '登录',
                            content:$("#Login_registration_reminder")
                        });
                    }
                }
            }
        });
        return false;
    });

    });
</script> 
  <script>


    $(function () {
        var $tip_text = '用户名 / 邮箱 / 手机号';
        $('#account').focus(
                function () {
                    if ($('#account').val() == $tip_text) {
                        $('#account').val('');
                        $('#account').removeClass('tip-text');
                    }
                }
        ).blur(
                function () {
                    if ($('#account').val().length == 0) {
                        $('#account').addClass('tip-text');
                        $('#account').val($tip_text);
                    }
                }
        );
        if ($('#account').val().length == 0) {
            $('#account').val($tip_text);
            $('#account').addClass('tip-text');
        }

        $('#login_form').submit(function() {
            if(!login_validate()) {
                return false;
            }
            var keeplogin = false;
            if($('#keeplogin').attr('checked')=='checked') {
                keeplogin = true;
            }
            var form_data = {
                login_username:$('#account').val(),
                login_password:$('#password').val(),
                login_type:$('#login_type').val(),
                auto_login:keeplogin
            };
            var redir = $('#redir').val();
            var pid = "324267920";
            var redir1 = '/cgi-bin/ajax/ajax_user_favor.php?add='+pid;
            $('#redir').val(redir);
            $.ajax({
                type:'post',
                url:'/article/check/',
                data:form_data,
                success:function(e) {
                    if(e.error) {
                        $('#error_msg').text(e.data.notice);
                    }else {
                        if (redir1 == redir) {
                            layer.closeAll();
                            $('#span_favor').click();
                        } else {
                            location.href = redir;
                        }
                    }
                }
            });
            return false;
        });

        function login_validate() {
            if($('#account').val() == $tip_text) {
                $('#account').val('');
            }
            if ($('#account').val().length == 0 || $('#password').val().length == 0) {
                if($('#account').val().length == 0) {
                    $('#account').focus();
                    $('#error_msg').text('您输入的账号不能为空');
                }else if($('#password').val().length == 0){
                    $('#password').focus();
                    $('#error_msg').text('您输入的密码不能为空');
                }
                return false;
            }
            return true;
        }

    });
</script> 
  <script>

    $(window).load(function() {
        var search_kw = '';
        var selected_index = -1;
        $('#tips_detail').on('click', 'li', function (){
            $('#article_search_keyword').val($(this).text());

            // hide the tip box junping
            $('#tips_detail').hide().children().remove();
            search_kw = '';
            selected_index = -1;
        }).on('mouseover', 'li', function(){
            $('#tips_detail > li').css({'background' : '#ffffff'});
            selected_index = $(this).index();
            $(this).css({'background' : '#ebebeb'});
        }).on('mouseout', 'li', function(){
            selected_index = $(this).index();
            $(this).css({'background' : '#ffffff'});
        });

        /*详情页右侧广告下边的相关行业周边推荐跟随代码*/
        function right_bottom($div_obj, $bottom_obj){
            if($div_obj.length == 0 || $bottom_obj.length == 0) {
                return false;
            }
            var div_ScrollTop = $div_obj.offset().top;
            var div_height = $div_obj.height();
            var bottom_SrollTop = $bottom_obj.offset().top;
            var min_height = $('.main-l').offset().top + $('.main-l').height() + 23;  //23为$('.main-l')上下margin与上下border的值之和
            $(window).on('scroll', function (){
                var doc_ScrollTop = $(this).scrollTop();
                if(doc_ScrollTop > div_ScrollTop && (div_ScrollTop + div_height < min_height)){

                    if (doc_ScrollTop + div_height >= bottom_SrollTop) {
                        $div_obj.fadeOut();
                    } else {
                        $div_obj.fadeIn();
                        $div_obj.css({
                            position:'absolute',
                            top:doc_ScrollTop
                        });
                    }
                }else if(doc_ScrollTop <= div_ScrollTop){
                    $div_obj.fadeIn();
                    $div_obj.css({
                        position:'static'
                    })
                }
            });
        }
        right_bottom($('.jpShop-info'), $(".bottomBd"));
    });
</script> 
