<script type="text/javascript">
$(function(){
	$('#register').find("input[name=username]").first().blur(function(){
		var val = $(this).val();
		if( val.length <6){
			alert('用户名必须不少于6个字符');
			return false;
		}else{
			checkUsername( val);

		}
	});
})
function checkUsername( val){
	$.ajax({
		type: "POST",
		url: "/cn/check-username/"+val,
		dataType: "json",
		cache:false,
		success: function(msg){
			if( msg.result){
				alert(val + " 已经被注册,请更换!");
			}
		}
	});
}
function getCaptcha(){    
    var img = document.getElementById("imgCap");    
    img.src = "/cn/captcha?cap=" + new Date().getTime();    
} 
</script>
