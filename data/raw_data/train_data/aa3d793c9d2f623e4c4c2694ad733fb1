	};
	Y.comment = {
		listUrl : 'http://comment.yinyuetai.com/mv/video-comment-list',
		hotListUrl : 'http://comment.yinyuetai.com/mv/video-promo-comment-list',
		createUrl : 'http://comment.yinyuetai.com/mv/video-comment-create',
		replyUrl : 'http://comment.yinyuetai.com/mv/video-comment-reply',
		supportUrl : 'http://comment.yinyuetai.com/comment/support-comment'
	};
	require(['app/video2/main', 'app/video/videocomment'], function(app, comment) {
		new app();
		comment.init(videoId);
	});
	require(['h5mvplayer'], function(YYTVideo) {
		var v = new YYTVideo(201373, {
			$container : $("#vPlay"),
			controlbar : "over",
			switchVideo : "auto",
			autoplay : deviceIsIOS ? false : true
		});
		v.on("media_list_complete", function() {
			if (videoId == 2311504 || videoId == "2311504") {
				return;
			}
			isPlayerClosed = true;
			setTimeout(yytVideoComplete, 5000);
		});
		v.on("video_error", function(msg) {
			$(".message_box").show();
			$(".message_content").html(msg);
		});
		v.startPlay();
	});
	require(['yytStatis'], function(yytStatis) {
		yytStatis.init();
	});

</script>



