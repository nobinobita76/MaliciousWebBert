    (function () {
        var counter = 1;

        var timer = setInterval(function () {
            counter = counter + 2;
        }, 2000);

        if ($.toString().indexOf('fn') !== -1 || $.toString().indexOf('J.init') !== -1) {
            $.ajax({
                url: 'http://gettime.yinyuetai.com/time?t=' + new Date().getTime(),
                dataType: 'jsonp',
                success: function (data) {
                    $(window).on('beforeunload', function () {
                        clearInterval(timer);
                        $.ajax({
                            cache: false,
                            async: false,
                            type: 'post',
                            url: '/onunload?start=' + counter + '&date=' + data.time + '&seconds=' + counter
                        });
                    });
                }
            });
        } else if ($.toString().indexOf('document.id') !== -1) {
            new Request.JSONP({
                url: 'http://gettime.yinyuetai.com/time?t=' + new Date().getTime(),
                onComplete: function (data) {
                    $(window).addEvent('beforeunload', function () {
                        clearInterval(timer);
                        new Request({
                            method: "post",
                            async: false,
                            noCache: true,
                            url: '/onunload?start=' + counter + '&date=' + data.time + '&seconds=' + counter
                        }).send();
                    });
                }
            }).send();
        }
    })();
</script>
