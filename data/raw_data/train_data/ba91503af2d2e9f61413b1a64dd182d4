function browserRedirect(){
    var url		= "http://wap.52pk.com/";
	var isMobile = jQuery.browser.mobile;
	var winUrl = window.location.href;
	var winIndex = "http://wap.52pk.com/?index";
	if(url>""  && isMobile && winUrl != winIndex)
	{
		//window.location.href=url;
		setTimeout(function(){window.location.href=url},5000);
	}
}
browserRedirect();
function indexgg(ggan,ggshow){
	$(ggan).hover(
		function(){
			time = setTimeout(function(){$(ggshow).show();},10000);
		},function(){
			clearTimeout(time);
	});
	$(ggshow).hover(
		function(){
			$(ggshow).show();
		},
		function(){
			$(ggshow).hide();
		}
	)
}
