$(function(){
	var url=document.URL;
	var url_reg='http://china.makepolo.com/misc/interface/stat2.php?rf=2e02eb06064cde0319a798f4e3f70d03&url=http://my.b2b.makepolo.com/ucenter/reg/register_new.php'
