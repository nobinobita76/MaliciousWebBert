					init:function(title,box){
						box.innerHTML = "是否确定要删除该日志？";
					},
					success:function() {
						common.ajax({
							url:'/blog/act.tc',
							data:{
								topicId : topicId,
								type : 'db'
							},
							success:function(datas) {
								datas = common.parseJson(datas);
								if(datas.result == 0) {
									fn.alert('删除日志成功！');
									window.location.href = "/blog/myBlog.tc?cateId=0&index=0&fromurl=";
								} else {
									fn.alert('删除日志失败！');
								}
							},
							failure:function(datas){
								fn.alert('删除日志失败！');
							}
						});
					}
				});
	    	}
		
	</script>
			<script>

			changeLogo('csjy', 'http://s13.jsyxw.cn/res/web/2.0/img/newyanxiu/changsha_logo2.jpg', 'http://www.hncsteacher.cn/');
			changeLogo('gx', 'http://s13.jsyxw.cn/res/web/2.0/img/newyanxiu/guangxi_logo2.jpg', 'http://gx.yanxiu.com/');
			changeLogo('zgjiaoyan', 'http://s1.jsyxw.cn/res/web/2.0/img/newyanxiu/zgjiaoyan_logo2.png', 'http://www.zgjiaoyan.com');
			//changeLogo('sdxx', 'http://s1.jsyxw.cn/res/web/2.0/img/newyanxiu/sdxx_logo2.png', 'http://2014sdxx.yanxiu.com');
			var intervalWdsqObjId;
			function changeLogo(logoValue, src, domain){
				var fromurl = '';
				if(fromurl == logoValue){
					var logoObj = document.getElementById("logo");
					logoObj.setAttribute("src", src);
					logoObj.parentNode.setAttribute("href", domain);
					updateCsjyCss();
					var formDivObj = document.getElementById("ss_form_div");
					formDivObj.parentNode.removeChild(formDivObj);
					intervalWdsqObjId = window.setInterval(delWdsqObj,500); 
					readLoad(fromurl, domain);
				}
			}
			function readLoad(fromurl, domain){
				if (document.readyState && document.readyState == 'complete') {
					if(fromurl!="" && domain!="")iteratorAHref(fromurl,domain);
				} else {
					setTimeout(function(){readLoad(fromurl, domain);},50);
				}
			}
			function delWdsqObj() {
					var wdsqObj = document.getElementById("wdsq");
					if(wdsqObj != null) {
						wdsqObj.parentNode.removeChild(wdsqObj);
						//updateCsjyCss();
						window.clearInterval(intervalWdsqObjId);
					}
			}
			function updateCsjyCss() {
				var htmlStyle = '';
				htmlStyle = htmlStyle + '<style>';
				htmlStyle = htmlStyle + '#header{background-color:#ebebeb;height:36px;}';
				htmlStyle = htmlStyle + '.navigation li a{font-size:12px;color:#666;font-weight:normal;}';
				htmlStyle = htmlStyle + '.navigation a:hover{border-top:none;}';
				htmlStyle = htmlStyle + '.navigation li,.navigation li a,.navigation li a:hover{height:36px;line-height:36px;border:0;color:#666;}';
				htmlStyle = htmlStyle + '.message span.message_count_bg{background:none;color:#666;margin:9px 0 0 0;}';
				htmlStyle = htmlStyle + '.navigation .message:hover span.message_count_bg{background:none;color:#666;margin:9px 0 0 0;}';
				htmlStyle = htmlStyle + '.navigation .message{background:url(http://portal.yanxiu.com/statics/area_csyxw/images/public_icon.png) no-repeat 50px -43px;padding-right:10px;float:left}';
				htmlStyle = htmlStyle + '.navigation .message:hover{background:url(http://portal.yanxiu.com/statics/area_csyxw/images/public_icon.png) #fff no-repeat 50px -43px;padding-right:10px;float:left}';
				htmlStyle = htmlStyle + '.navigation li a strong{font-size:12px;color:#666;font-weight:normal;}';
				htmlStyle = htmlStyle + '</style>';
				document.write(htmlStyle);
			}
			function iteratorAHref(fromurl, domain) {
				var aArr = document.getElementsByTagName("a");
				for(var i = 0; i < aArr.length; i++) {
					var hrefAttr = aArr[i].getAttribute("href");
					var hrefaddrAttr = aArr[i].getAttribute("hrefaddr");
					if(hrefaddrAttr != null && hrefaddrAttr.length > 0 && hrefaddrAttr.indexOf('fromurl=' + fromurl) < 0) {
						setAHrefAddrCsjy(aArr[i], hrefaddrAttr, fromurl);
					}
					if(hrefAttr && hrefAttr.indexOf('fromurl=' + fromurl) < 0) {
						if(hrefAttr == 'q.yanxiu.com' || hrefAttr == 'http://q.yanxiu.com' || hrefAttr == 'q.yanxiu.com/' || hrefAttr == 'http://q.yanxiu.com/') {
							setAHrefWithDomain(aArr[i], hrefAttr, domain);
						} else if(hrefAttr.indexOf('i.yanxiu.com') >= 0) {
							setAHref(aArr[i], hrefAttr, fromurl);
						} else if(hrefAttr.indexOf('q.yanxiu.com') >= 0) {
							setAHref(aArr[i], hrefAttr, fromurl);
						} else if(hrefAttr.indexOf('/blog/') >= 0) {
							setAHref(aArr[i], hrefAttr, fromurl);
						} else if(hrefAttr.indexOf('/u/') >= 0) {
							setAHref(aArr[i], hrefAttr, fromurl);
						} else if(hrefAttr.indexOf('/user/') >= 0) {
							setAHref(aArr[i], hrefAttr, fromurl);
						} else if(hrefAttr.indexOf('javascript:Passport.doLogout();') >= 0) {
							setAHrefAction(aArr[i], hrefAttr, domain);
						}
					}
				}
			}
			function setAHref(aObj, hrefAttr, fromurl) {
				if(hrefAttr.indexOf('?') >= 0) {
					aObj.setAttribute("href", hrefAttr+"&fromurl=" + fromurl);
				} else {
					aObj.setAttribute("href", hrefAttr+"?fromurl=" + fromurl);
				}
			}
			
			function setAHrefWithDomain (aObj, hrefAttr, domain) {
				aObj.setAttribute("href", domain);
			}
			
			function setAHrefCsjy(aObj, hrefAttr) {
				aObj.setAttribute("href", "http://csjy.yanxiu.com/");
			}
			
			function setAHrefAddrCsjy(aObj, hrefAttr, fromurl) {
				if(hrefAttr.indexOf('?') >= 0) {
					aObj.setAttribute("hrefaddr", hrefAttr+"&fromurl=" + fromurl);
				} else {
					aObj.setAttribute("hrefaddr", hrefAttr+"?fromurl=" + fromurl);
				}
			}
			
			function setAHrefAction(aObj, hrefAttr, domain) {
				aObj.setAttribute("href", domain + "?action=logout");
			}
		</script>
