<script type="text/javascript">
function checkAnswer(){
	var answer = $('#textarea_answer_add').val();
	answer = $.trim(answer);
	if(answer==''){
		alert("请输入您的回答");
		return false;
	}else if(answer.length<5){
		alert("您输入的回答太短");
		return false;
	}
	return true;
}
function checkContentShow(){
        var cont = $('#question_cont').val();
        cont = $.trim(cont);
        if(cont == ''){
                alert("请输入您的问题");
                return false;
        }else if(cont.length <5){
                alert("请完整您的问题");
                return false;
        }
	$("#quickask").slideUp();
	$("#number").slideDown();
}
function checkContent(){
        var cont = $('#question_cont').val();
        cont = $.trim(cont);
        if(cont == ''){
                alert("请输入您的问题");
                return false;
        }else if(cont.length <5){
                alert("请完整您的问题");
                return false;
        }
        return true;
}
function checkPhone(){
        var phone = $('#phone_num').val();
        phone = $.trim(phone);
        var reg = "^0{0,1}(13|15|18)[0-9]{9}$";
        var isMobile = phone.match(reg);

        if(!isMobile){
                alert("不正确输入手机号将无法及时收到回答通知");
                return false;
        }
        if(phone == ''){
                alert("不输入手机号将无法及时收到回答通知");
                return false;
        }else if(phone.length!=11){
                alert("不正确输入手机号将无法及时收到回答通知");
                return false;
        }else{
        return true;
        }

}
function goToLogin(){
	window.location="http://m.ci123.com/user/login";
}
function quickAskButton(){
	var q=document.getElementById("qucikask_box").style.display;
	if(q == "none")
	{
		$("#qucikask_box").slideDown();
		$("#quick_close").removeClass("close");
	}
	document.quickaskForm.question_cont.focus();
}
</script>

