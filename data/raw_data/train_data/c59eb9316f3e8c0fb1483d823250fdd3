<script type="text/javascript">
var aastate = 0;

function isUser(is_uid){
	if(is_uid == ''){
		window.location="http://m.ci123.com/user/login";	
	}	
}

function AddAnswer() {
	if (aastate) {
		return false;
	}
	var nickname = $('#nickname').val();
	var content = $('#textarea_answer_content').val();
	var question_id = $('#question_id').val();
	var reg = [[/\&amp;/gi,"&"],[/\&ldquo;/gi,"“"],[/\&rdquo;/gi,"”"],[/\&bull;/gi,"•"],[/\&mdash;/gi,"—"],[/\&hellip;/gi,"…"],[/\&ndash;/gi,"–"],[/\&lsquo;/gi,"‘"],[/\&rsquo;/gi,"’"],[/\&sbquo;/gi,"‚"],[/\&bdquo;/gi,"„"],[/\&tilde;/gi,"˜"],["</scr ipt>","</scr"+"ipt>"]];
	for(var r in reg) {
		content = content.replace(reg[r][0],reg[r][1]);
	}
	if (content.length <= 0) {
		alert('请输入回答内容');
		return false;
	}
	aastate = 1;
	$.post('/andask/sub/answer_add2.php', {textarea_answer_content:content,question_id:question_id}, function(data) {
		if(data == 'ok') {
			$('.answer_con').append('<li><div class="li_border"><p>'+content+'</p><div class="author clearfix" ><div class="uname">'+nickname+'</div><div class="time"></div></div></div></li>');
			$('#textarea_answer_content').val('');
		} else {
			alert(data);
		}
		aastate = 0;
	});
	return false;
}
</script>
