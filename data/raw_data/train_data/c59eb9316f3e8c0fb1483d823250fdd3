//广告文案切换-xyj
$(function () {
    var tie,
        $ele = $('.btm-text2'),$ele2 = $('.mmsq-text2'),
        i = 0,
        arr = ['千万妈妈的选择  伴您孕育每一天', '一键查询  孕期饮食禁忌'];arr2 = ['倾听妈妈的声音，及时在线交流', '妈妈的小管家小天地,妈妈社区'];
    $ele.html(arr[0]);
    $ele2.html(arr2[0]);
    time = setInterval(change, 3000);
    function change() {
        i++;
        if (i > 1) {
            i = 0;
         }
        $ele.html(arr[i]);
		$ele2.html(arr2[i]);
    }
})

</script>
