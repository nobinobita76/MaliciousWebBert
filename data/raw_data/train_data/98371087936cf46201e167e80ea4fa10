(function() {
	//获取url地址中的参数
	function getQueryString(name) {
	     var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
	     var r = window.location.search.substr(1).match(reg);
	     if(r!=null)return  unescape(r[2]); return null;
	}
	//来自头条的显示 易车APP
	if (getQueryString("WT.mc_id") == "mjricdq") {
		DownloadPriceApp.init('app_download_ycapp', 'closedPriceApp');
	} else {
               DownloadPriceApp.init('app_download_price', 'closeYcApp');
	}
})();
