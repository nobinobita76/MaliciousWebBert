var now=10;
var size=10;
var sum=$(".deals").find("div").length;
//copy www.dianru.net 2014-08-15
$(function(){
    $(".loadingImg").each(function(){
        lazyload(this);
    });
    $("#loadMore").click(function(){
        now=now+size;
        $(".deals div:lt("+now+")").fadeIn(500);
        if(now>=sum)
        {
            $("#loadMore").unbind("click").html("共"+sum+"个特价宝贝");
        }
    })
})
