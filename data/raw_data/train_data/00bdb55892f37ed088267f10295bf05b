<script type="text/javascript" src="http://js.lotour.net/jquery.tmpl.min.js"></script>
<script id="articleTmpl" type="text/x-jquery-tmpl">
<ul class="lg_list clearfix">
    <li class="active">
        <div class="imgwrap">
            <a href="${Url}" target="_blank">
                <img src="http://img.lotour.net/zip/20110411/index_mrtu.jpg">
                <p class="lg_info"></p>
            </a>
            <ul class="tag_list">
                {{each(j, tag) Tags}}
                    {{if tag._id == 0}}
                        <li class="fm {{if tag._id == lttag}}active2{{/if}}" tagid="0" pic="${tag.Pic}" reason="${tag.Reason}" data-id="fm">${tag.Name}</li>
                    {{else}}
                        <li class="{{if tag._id == lttag}}act{{/if}}" pic="${tag.Pic}" tagid="${tag._id}" reason="${tag.Reason}" data-id="${tag.TagClass}"><i></i>${tag.Name}</li>
                    {{/if}}
                {{/each}}
            </ul> 
        </div>
        <div class="author_info" id="${AuthorId}">
            <div class="info_nr">
                <a href="${AuthorUrl}" target="_blank">
                    <img src="${AuthorHeadPic}" alt="" title=""/>
                    <span>${NickName}</span>
                </a>
                <div class="dy_num">
                    <p class="gx">
                        <i>浏览量</i><span class="zl${AuthorId}" authortype="${AuthorType}">0</span>
                    </p>
                </div>
                <p class="sign">${Introduction}</p>
            </div>
        </div>
    </li>
</ul>
</script> 
<script type="text/javascript">
var lttag = 3750;
var regionArray = [];
var mySwiperVer;
$.each($(".title"), function() {
	regionArray.push($(this).attr("regionId"));
});
var regions = regionArray.join(",");
//动态判断搜索框的宽
    var show_num = 0;
if($(window).innerWidth() > 1680){
	show_num = 14.6;
}else if($(window).innerWidth() > 1440 && $(window).innerWidth() <= 1680){
	show_num = 13.6;
}else if($(window).innerWidth() > 1366 && $(window).innerWidth() <= 1440){
	show_num = 12.3;
}else if($(window).innerWidth() > 1280 && $(window).innerWidth() <= 1366){
	show_num = 12.4;
}else if($(window).innerWidth() > 1024 && $(window).innerWidth() <= 1280){
	show_num = 11.25;
}else if($(window).innerWidth() <= 1024){
	if($('.hc>div').length > 3){
		$('.hc>div').each(function(){
			if($(this).index() >= 3){
				$(this).hide()
			}
		})
	}
		show_num = 9;
}
//图上兴趣标签
GetTagsByRegions({regions: regions}, function(data) {
	var data = data.split(',');
	$.each(data, function(idx, item) {
		var r = item.split('$');
		var id = r[0],h=r[1].split('^');
		$("#region" + id).find(".choose_ul").html(h[0]);
		if($("#region" + id).find(".choose_ul ul li").length <= 5){
			$("#region" + id).find(".choose_pre,.choose_next").hide();
			if($("#region" + id).find(".choose_ul ul li").length == 0 || parseInt($("#region" + id).find(".include i").html()) == 1){
				$("#region" + id).find(".choose").hide();
			}
		}
		$("#region" + id).next().find(".else").hide();
	});
});

//排名靠前的目的地
GetRankAreas({}, function(data) {
	var sDest = "";
	$.each(data.param, function(idx, item) {
		var name = item.destinationName;
		var url = item.destinationUrl;
		sDest += '<li><a href="' + url + '" target="_blank">' + name + '</a></li>';
	});
	$(".mdd").append(sDest);
});

//目的地汇总
GetRecommondTags({}, function(data) {
	var sTags = "", flag = false;
    $.each(data.param, function(idx, item) {//如果数量全部为1，则目的地汇总不显示
	    sTags += '<div class="swiper-slide">';
	    sTags +=     '<a tagId="' + item.id + '" href="' + item.url + '">';
	    sTags +=         '<img src="' + item.pic + '"/>';
	    sTags +=         '<div class="info">';
	    sTags +=             '<span>' + item.name + '</span> <i>' + item.count + '</i>';
	    sTags +=         '</div>';
	    sTags +=     '</a>';
	    sTags += '</div>';
	    flag = item.count != 1 ? true : flag;
    });
	if(flag) {
	    $(".swiper-wrapper").append(sTags);
	    mySwiperVer = new Swiper('.swiper-conter', {
		    onlyExternal : true,
		    slidesPerView :show_num,
		    slidesPerGroup : 3.38,
		    spaceBetween :14,
		    prevButton:'.prev',
		    nextButton:'.next'
	    });
	    $('.prev').click(function(){
		    mySwiperVer.swipePrev(); 
	    })
	    $('.next').click(function(){
		    mySwiperVer.swipeNext(); 
	    });
        if ($(".swiper-wrapper").width() - $(".swiper-conter").width() == 0) {
            $(".prev,.next").hide();
        }
	}
    else {//无兴趣标签推荐
      $('.header_B').addClass('disnone');
      $('.content').css('margin-top', '110px');
    }	
});

//选择或取消兴趣词（二级筛选）
var theChoose, _theText, theXz, _str, theWidth, PreWidth;
$('.choose_ul li').unbind('click').live('click', function(){
	theChoose = $(this).parents('.choose');
	PreWidth = theChoose.find('.choose_ul').width();
	theChoose.find('.choose_ul').width(PreWidth - $(this).innerWidth() - 8);
	_theText = $(this).html();
	var _id = $(this).attr("id");
	//remove之前赋值
	var $title = $(this).parents(".title");
	$(this).remove();
	_str = '<li id=\''+_id+'\'>' +_theText+ '<em title="取消选中"></em></li>';
	theChoose.find('.xz').append(_str);
	
	var regionId = $title.attr("regionId");
	var tags = "";
	$title.find(".xz li").each(function(i){
		tags += $(this).attr("id") + ",";
	});
	var that = $title.next().find(".lg_next");
	GetInspirations(that, regionId, 1, tags);
	
	// 重新汇总兴趣标签
	$title.find(".xz").remove();
	$title.find(".choose_ul ul").remove();
	GetTagsByOptions({regionId: regionId, tags: tags}, function(result) {
	    var unTags = result.unselectedTags.split(',');
	    $.each(unTags, function(idx, item) {
	        var r = item.split(':');
	        var id = r[0];
	        $("#region" + id).find(".choose_ul").html(r[1]);
	    });
	    
		var tags = result.selectedTags.split(',');
		$.each(tags, function(idx, item) {
		    var r = item.split(':');
		    var id = r[0];
		    $("#region" + id).find(".choose_ul").before(r[1]);
		});
		
	    chooseLength();
	});
});
$('.xz li').unbind('click').live('click', function(){
	theXz = $(this).parents('.choose');
	i = Math.ceil(theXz.find('.choose_ul>ul').width() / theXz.find('.choose_ul').width());
	PreWidth = theXz.find('.choose_ul').width();
	theWidth = $(this).innerWidth();
	_theText = $(this).text();
	_id=$(this).attr("id");
	//remove之前赋值
	var $title=$(this).parents(".title");
	$(this).remove();
	_str = '<li id=\''+_id+'\'>' +_theText+ '</li>';
	theXz.find('.choose_ul').width(PreWidth + theWidth + 8);
	theXz.find('.choose_ul ul').prepend(_str);
	
	var regionId = $title.attr("regionId");
	var tags = "";
	$title.find(".xz li").each(function(i){
		tags += $(this).attr("id") + ",";
	});
	var that = $title.next().find(".lg_next");
	GetInspirations(that, regionId, 1, tags);
	
	// 重新汇总兴趣标签
	$title.find(".xz").remove();
    $title.find(".choose_ul ul").remove();
    GetTagsByOptions({regionId: regionId, tags: tags}, function(result) {
        var unTags = result.unselectedTags.split(',');
        $.each(unTags, function(idx, item) {
            var r = item.split(':');
            var id = r[0];
            $("#region" + id).find(".choose_ul").html(r[1]);
        });
        
        var tags = result.selectedTags.split(',');
        $.each(tags, function(idx, item) {
            var r = item.split(':');
            var id = r[0];
            $("#region" + id).find(".choose_ul").before(r[1]);
        });
        chooseLength();
    });
});

$(".hr .gr").on('click', function(){
	if ((typeof loginUserData == "undefined" || loginUserData.uid == 0)) {
		$.getScript("http://js.lotour.net/login/floatlogin_v3_utf8.js", function() {
		     var loginReg = new LoginRegV3();
		     loginReg.init();
	         loginReg.showLogin();
	         $("#layerBg_lg").css("position", "fixed");
		     loginReg.regCallBack = function(data) {
		         loginReg.closeLayer();
		     };
		     loginReg.loginCallBack = function(data) {
		         loginReg.closeLayer();
		         location.reload();
		     };
		})
	}
});

//可选兴趣标签为0的时候
function chooseLength() {
    $('.choose').each(function() {
        if ($(this).find('li').length == 0) {
            $(this).hide();
        } else if ($(this).find('li').length <= 5) {
        	 console.log(2)
            $(this).find('.choose_pre,.choose_next').hide()
        } else {
            $(this).find('.choose_pre,.choose_next').show()
        }
    })
}
//标签中的最后一个Li去掉下面的底条
function unBottomBorder() {
    $('.tag_list').each(function() {
        $(this).find('li').eq($(this).find('li').length - 1).addClass('last');
    })
}
</script>
<script type="text/javascript">
var loginUserData;
$(function () {
    jQuery.getJSON('http://my.lotour.com/login/login/islogin?random=' + (new Date()) + '&callback=?',function(data){
        loginUserData = data;
        if (parseInt(data.uid) > 0){
        	$(".hr .gr").addClass("login");
        	$(".hr .gr img").attr("src", "http://img.lotour.com/UserPhoto/" + data.photo);
        	
        	$("#mypage a").attr("href", "http://my.lotour.com/" + data.uid + "/");
        	$("#myhome a").attr("href", "http://my.lotour.com/" + data.uid + "/user/profile");
        	$("#logout").click(function(){
        	    $.getJSON('http://my.lotour.com/Login/Login/Loginout?callback=?&rn=' + Math.random(), function (data) {
        	        location.reload();
        	    });
        	});
        }
    });
    $("li[tagid="+lttag+"]").trigger("click");
});
</script>
