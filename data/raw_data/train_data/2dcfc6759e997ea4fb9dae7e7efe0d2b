	function rightMenuMove() {
		var oLeftMenu = document.getElementById("S_left_menu");
		var oLeftMenu2 = document.getElementById("S_left_menu2");

		var aLinks = oLeftMenu.getElementsByTagName("li");
		var aLinks2 = oLeftMenu2.getElementsByTagName("li");

		bindEvents(aLinks, 0);
		bindEvents(aLinks2, 4);
	};

	function bindEvents(aLinks, startIndex) {
		var oMerweima = document.getElementById("S_m_erweima");
		var basePath = $('#project_url_cli').attr('data-tpUrl');
		//console.log(aLinks.length);
		for (var i = 0, len = aLinks.length; i < len; i++) {
			(function(index) {
				aLinks[index].onmouseover = function() {
					var oImg = this.getElementsByTagName("img")[0];
					var src=oImg.src;
					oImg.style.opacity = 0;
					oImg.style.opacity = 'filter:alpha(opacity=0)';
					oImg.style.width = 138 + 'px';
					var active_src=src.replace("left-menu","left-menu-active");
					var current = startIndex + index + 1;
					oImg.src = active_src;
					startMove(oImg, {
						"opacity": 100
					}, function() {});

					if(current === 4) {
						oMerweima.style.display = '';
					}
				}
			})(i);
			(function(index) {
				aLinks[index].onmouseout = function() {
					var oImg = this.getElementsByTagName("img")[0];
					var active_src=oImg.src;

					oImg.style.opacity = 0;
					oImg.style.opacity = 'filter:alpha(opacity=0)';
					oImg.style.width = 40 + 'px';

					var current = startIndex + index + 1;
					var src=active_src.replace("left-menu-active","left-menu");

					oImg.src = src;

					startMove(oImg, {
						"opacity": 100
					}, function() {});

					if(current === 4) {
						oMerweima.style.display = 'none';
					}
				}
			})(i);
		}
	}

	rightMenuMove();
	
	</script>





</body></html>