<script type="text/javascript">
   
//通过ACTION地址跳转到其它页�?
function goToPageByAction(action,moduleAndTabId) {
	 var areaCode = window.location.href;
	  //从URL中获得acreCode�?
	 if(areaCode.indexOf("areaCode=") >0){//说明含有areaCode变量
		 areaCode = areaCode.split("areaCode=")[1];
	 	if(areaCode.indexOf("&") >0){//说明areaCode后边还有参数 
	 		areaCode = areaCode.split("&")[0];
	 	}
	 }else if(areaCode.indexOf("OldInsurance_") >0){
		 areaCode = areaCode.split("OldInsurance_")[1].split(".shtml")[0];
	 }else if(areaCode.indexOf("NewInsurance_") >0){
		 areaCode = areaCode.split("NewInsurance_")[1].split(".shtml")[0];
	 }else{
		 areaCode = '';
	 }
	 //拼接URL 
	if(areaCode !=null && areaCode !=''){
		window.location.href= action+"_"+areaCode+".shtml";
	}else{
		window.location.href= action+".shtml";
	}
	
	 //window.location.href=action+(moduleAndTabId!=null?"?selectedModuleId="+moduleAndTabId+"&areacode=" :"");
}

var selectedModuleId="";
var selectedTabId="";

//设置所选模块及TAB页的ID
function setSelectedModuleTabId(){
	
	var positonStr="";
	if (positonStr!="")
	{
		var moduleId = positonStr.split(".");
		selectedModuleId = moduleId[0];
		if (moduleId.length>1) selectedTabId=moduleId[1];
	}
}

setSelectedModuleTabId();

