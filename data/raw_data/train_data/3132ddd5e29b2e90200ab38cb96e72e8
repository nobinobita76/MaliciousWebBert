    /*tab*/

    $("a.Aclose").click(function() {

        var str = $(this).attr('attr');

        location.href = $('#' + str).attr('href');

    });

    $("#tagCtr").click(function() {

        if ($(this).hasClass("foldbox on")) {

            $(this).attr('class', 'foldbox hov');

        } else {

            $(this).attr('class', 'foldbox on');

        }

        $('#HideSoy').toggle();

    });

    $('div.soyall').show();

    $('dl.m_dl').hide();

    //$('div.info2_box').hide();

    var o_listr = $('a.h_on'),o_listg = $('a.v_view');

    function listr(o) {

        $('dl.m_dl').show();
        $('div.info2_box').hide();

        o_listr.attr('class', 'h_on');

        o_listg.attr('class', 'v_view');

        setPageUrl('g');

        var e = new Date()

        e.setFullYear(e.getFullYear() + 1);

        LETV.cookie("j-listview", 'listr', {expires:e,domain:".so.letv.com",path:"/"});
    }

    function listg(o) {

        $('dl.m_dl').hide();
        $('div.info2_box').show();

        o_listr.attr('class', 'h_view');

        o_listg.attr('class', 'v_on');

        setPageUrl('r');

        var e = new Date()
        e.setFullYear(e.getFullYear() + 1);
        LETV.cookie("j-listview", 'listg', {expires:e,domain:".so.letv.com",path:"/"});

    }

    function setPageUrl(l) {

        $('div.page>a').each(function() {

            var link = $(this).attr('href');

            if (typeof(link) != 'undefined') {

                link = link.replace(/_l(\w+?)/, "_l" + l);

                $(this).attr('href', link);

            }

        });

    }
    if (LETV.cookie("j-listview") == "listg") {
        listg();
    } else {
        listr();
    }
    $("a.h_on").click();
</script>
<script type="text/javascript">
    (function() {
        var ui = LETV.using("UI");
        ui.SimpleTab("#j-tab-b", {tabs: ".title>h2>span>a",tabsFor: ".sort div",switchEvent: "click"});
    })();


    function $TAB(x) {
        return document.getElementById(x);
    }
    function tab_show(a, b, c, d) {
        x = a,max_i = b,tn = c,tc = d;
        for (var i = 1; i <= max_i; i++) {
            $TAB(tn + i).className = $TAB(tc + i).className = "";
        }
        $TAB(tn + x).className = $TAB(tc + x).className = "on";
    }
    (function() {
        $(".j-morebtn").click(function(ev) {
            $(this).hide();
            $(".j-morebox").show();
            return false;
        });
        $(".j-retractbtn").click(function(ev) {
            $(".j-morebox").hide();
            $(".j-morebtn").show();
            return false;
        });
    })();


    (function() {
        var Logon = Spirit.UserValidate, //用户登录验证
                userLogin = Spirit.userLogin; //弹出层对象
        if (LETV.Plugin.DownLoader != undefined) {
            var DL = LETV.Plugin.DownLoader.getInstance();
            var a = LETV.cookie('m'), b = LETV.cookie('u');
            $('.btDown').click(function() {
                if (Logon.getUserInfo()) {
                    DL.downVideo(__INFO__, 'special');
                } else {
                    userLogin.openLetvLogin();
                    var event = window['Spirit'].Event;
                    var eventid = event.addEvent('loginSuccess', function(data) {
                        DL.downVideo(__INFO__, 'special');
                    });
                }
            });
        }
    })();


</script>

