            if (OpDvc(objEps)) {
                ClsDvc(objEps);
            } else {
                alert('没有密虎设备!或者您IE禁用了ActiveX组件!');
            }
        } else {
            $('#dlInputEpsPin').hide();
        }
    });
    $('#txtFirstCode').keyup(function() {
        if ($(this).val().length > 1) {
            $('#txtSecondCode').val('').focus();
        }
    }).focus(function() { $(this).val('') });
    $('#txtSecondCode').keyup(function() {
        if ($(this).val().length > 1) {
            $('#txtThirdCode').val('').focus();
        }
    });
});
