        <script type="text/javascript">
            $(document).ready(function () {

                $("#kinMaxShow").kinMaxShow({
                    button: {
                        normal: { width: '10px', height: '10px', marginRight: '8px', border: '0', right: '50%', bottom: '40px' },
                        focus: { background: "#32617c", border: "1px solid #33627d", color: "#000000" }
                    }
                });
                if ($("#kinMaxShow ul li").length <= 1) {
                    $(".KMSPrefix_kinMaxShow_button").css("display", "none");
                }
            });
            function PhonePassDown() {
                $("#PhoneDownloadDiv").zxxbox({
                    title: "请选择您的手机系统",
                    width: "525px",
                    fix: true,
                    index: 999999,
                    onclose: function () {

                    }
                });
                $(".wrap_close").css("padding-top", "14px");
                $("#zxxBlank").css("z-index", "99999");
            }

            function setLoginType() {
                var loginType = $("#rbtnlLoginType input:checked").val();
                if (loginType == "0") {
                    $("#liPwd").show();
                    $("#liEnPwd").hide();
                }
                else {
                    $("#liPwd").hide();
                    $("#liEnPwd").show();
                }
            }

            setLoginType();

            $("#rbtnlLoginType input").click(function () {
                setLoginType()
            });

            function checkInput() {
                var loginType = $("#rbtnlLoginType input:checked").val();
                if (loginType == "1") {
                    var length = pgeditor.pwdLength();//获得密码长度
                    if (length == 0 || length == undefined) {
                        alert("密码不能为空");
                        $("#OcxPassword").focus();
                        return false;
                    }
                    if (pgeditor.pwdValid() == 1) {//判断密码是否匹配正则表达式二
                        alert("密码不符合要求");
                        $("#OcxPassword").focus();
                        return false;
                    }
                    $.ajax({
                        url: "/Modules/SrandNum.aspx",
                        type: "GET",
                        async: false,
                        cache: false,
                        success: function (srand_num) {
                            pgeditor.pwdSetSk(srand_num);//给控件设置随机因子
                        }
                    });
                    var pwdResult = pgeditor.pwdResult();//获取密码AES密文
                    var machineNetwork = pgeditor.machineNetwork();//获取网卡信息密文
                    var machineDisk = pgeditor.machineDisk();//获取硬盘信息密文
                    var machineCPU = pgeditor.machineCPU();//获取CPU信息密文
                    $("#txtLoginPwd").val(pwdResult);//将密码密文赋值给表单
                    $("#hidMachineNetwork").val(machineNetwork);//将网卡和MAC信息密文赋值给表单
                }

                if ($.trim($("#txtLoginPwd").val()) == "") {
                    alert("请输入密码");
                    return false;
                }
                return true;
            }

            $("#lbBtnLogin").click(function () {
                return checkInput();
            })
            function TradeLoginEnter(e) {
                var key = window.event ? e.keyCode : e.which;
                var obj = document.getElementById("lbBtnLogin");
                if (key == 13) {
                    checkInput();
                    obj.click();
                    return false;
                }
            }

            //为空信息提示切换
            if ($.trim($("#txtLoginAccount").val()) != "") {
                $("#txtLoginAccount").prev().hide();
            }
            $(document).ready(function () {
                var arrid = new Array();
                arrid[0] = { id: "jsonTip", name: "汇付宝产品", code: "index" };
                //arrid[1] = { id: "Mer", name: "商户接入" };

                for (var i = 0; i < arrid.length; i++) {
                    $("body").pluginName("jsonstr", arrid[i]);
                }

                //为空信息提示切换
                if ($.trim($("#txtLoginAccount").val()) != "") {
                    $("#txtLoginAccount").prev().hide();
                }
                if ($.trim($("#txtExtCode").val()) == "") {
                    $("#txtExtCode").prev().show();
                    $("#ExtCode").addClass("ExtCode");
                }
                $("#LoginContent input").focus(function () {
                    $(this).prev().hide();
                }).blur(function () {
                    if ($.trim(this.value) == "") {
                        $(this).prev().show();
                    }
                });
                $("#LoginContent label").click(function () {
                    $(this).next().trigger("focus");
                });

                $("#txtExtCode").keyup(function () {
                    $.ajax({
                        url: "Service/LoginRegisterService.aspx",
                        data: "doType=checkExtCode&extCode=" + $(this).val(),
                        cache: false,
                        dataType: "json",
                        success: function (data) {
                            if (data.HasError) {
                                $("#ExtCodeImg").removeClass();
                            } else {
                                $("#ExtCodeImg").removeClass().addClass("MsgIcon06").show();
                            }
                        }
                    });
                });

                $("#close").click(function () {
                    $(".LeftImg").hide();
                });
            });
            function TextBlur(obj) {
                if ($.trim($(obj).val()) != "" && $.trim($(obj).val()) != null) {
                    $(obj).prev().hide();
                }
                else {
                    $(obj).prev().show();
                }
            }
            function TextFocus(obj) {
                $(obj).prev().hide();
            }
            $("#imgBtn").click(function () {
                window.setTimeout("javascript:location.href='Business/solutionGame.aspx'", 100);
            });

            function reLoadExtCode() {
                var ran = Math.round((Math.random()) * 100000000);
                $("#imgRandom").attr("src", "Modules/RandomImage.aspx?name=login&ran=" + ran);
            }


        </script>

    

