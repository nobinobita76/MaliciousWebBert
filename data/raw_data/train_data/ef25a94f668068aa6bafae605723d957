			    <script type="text/javascript">
			            function question(){
			                    var questionUrl=$("#questionUrl").val();
			                    var vtf= $("#visitTypeFa").val();
						        if(vtf=='b2c'){
						           questionUrl+="?visitType=b2c";
						        }else{
						           questionUrl+="?visitType=user";
						        }
			                    window.location.href=questionUrl;
			            }
			            
				        var vtf= $("#visitTypeFa").val();
				        if(vtf=='b2c'){
				            $("#visitType1").val(vtf);
				            $("#visitType2").val(vtf);
				            $("#visitType3").val(vtf);
				            $("#visitType4").val(vtf);
				        }
			    </script>
			    
    






