<script type="text/javascript">
if($('#allQ').val()){
	$("#queryInputId").val($('#allQ').val());
}
$(function(){
	
	var _member_drop_down_is_animating = false,
		_member_drop_down_is_open = false;
	
	$('#member-drop-down-toggle').on('click',function (){
		if (_member_drop_down_is_animating) return false;
		_member_drop_down_is_animating = true;

		if(_member_drop_down_is_open)
			$('#member-drop-down-list').slideUp('fast', function(){
				  _member_drop_down_is_animating = false;
				   _member_drop_down_is_open = false;
			});
		else
			$('#member-drop-down-list').slideDown('fast', function(){
				  _member_drop_down_is_animating = false;
				   _member_drop_down_is_open = true;
			});
	});
	
	
	$('#member-drop-down-toggle').mouseenter( function (){
					if (_member_drop_down_is_animating) return false;
					if (_member_drop_down_is_open) return false;
					
					_member_drop_down_is_animating = true;
					
					$('#member-drop-down-list').slideDown('fast',function(){
						_member_drop_down_is_animating = false;
						_member_drop_down_is_open = true;
					
					});

			
	});
	
	$('#member-drop-down-list').mouseleave(function(){
	
					if (_member_drop_down_is_animating) return false;
					if (!_member_drop_down_is_open) return false;
					
					_member_drop_down_is_animating = true;

					$('#member-drop-down-list').slideUp('fast', function(){
							_member_drop_down_is_animating = false;
							_member_drop_down_is_open = false;
					});
			
	});

	$('#member-drop-down-toggle').click(function () {
		$(this).dropdown('toggle');
	});

	$('#tp-help').on('click',function (){
		showHelp();
	});
});
</script> 
