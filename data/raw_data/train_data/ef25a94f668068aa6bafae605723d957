<script type="text/javascript">
       
		$(function(){
		    
		    /**页面加载默认 隐藏子菜单**/
			$(".li_4,.li_5,.li_17,.li_7,.li_8,.li_10,.li_11,.li_12,.li_15,.li_16").each(
		         function(){
		           $(this).hide();
		         }
	         );
		
		    /**右侧菜单绑定鼠标经过离开事件**/
			$(".li_font,.li_font_1,.li_font_2").hover(
	         function(){
	           $(this).children('a').css("color","blue");//鼠标经过时文字为蓝色
	         },
	         function(){
	           $(this).children('a').css("color","gray");//鼠标离开时文字为灰色
	         }
	         );
	         
	         
	        /**提交按钮绑定鼠标经过离开事件**/
			$(".sbBtn").hover(
	         function(){
	           $(this).removeClass();
	           $(this).addClass('sbBtn_on');
	         },
	         function(){
	           $(this).removeClass();
	           $(this).addClass('sbBtn');
	         }
	         );
	         
	         
	         
		});
		
		
		
		//点击客户服务活动菜单 显示和隐藏其子菜单
		function khfw(){
		$(".li_4,.li_5,.li_17").toggle();
		}
		
		//点击太平VIP俱乐部 显示和隐藏其子菜单
		function tpVip(){
		$(".li_7,.li_8,.li_10,.li_11,.li_12").toggle();
		$(".li_15,.li_16").hide();   //特别处理,关闭 VIP礼遇服务的子菜单
		}
		
		//点击VIP礼遇服务 显示和隐藏其子菜单		
		function vipLy(){
		$(".li_15,.li_16").toggle();
		}
</script>

