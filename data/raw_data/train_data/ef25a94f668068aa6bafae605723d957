function submitForm(){
	var findWay = document.getElementById("findWay").value;
    var plyNo = document.getElementById("plyNo").value;
    var vhlfrm = document.getElementById("vhlfrm").value;
    if(plyNo == ''){
     alert("保单号不能为空！");
     return false;
    }
    if(vhlfrm == ''){
     alert("被保险人证件号不能为空！");
     return false;
    }
    
    document.getElementById("findWay_car").value=findWay;
    document.getElementById("policyNo_car").value=plyNo;
    document.getElementById("insuredIdentifyNumber_car").value=vhlfrm;
    
    var fm = document.getElementById("fm_car");
   	fm.action = "/TPEBizWeb/pages/B2C/xiamenVehicleQuery/queryVechileAll.do";
   	fm.submit();
}
function submitForm1(){
		var visaserialno = document.getElementById("visaserialno").value;
    	var plyNoForUnvechile = document.getElementById("plyNoForUnvechile").value;
    	var insuredName = document.getElementById("insuredName").value;
    	var findWay = document.getElementById("findWay").value;
    	if(plyNoForUnvechile == ''){
     		alert("保单号不能为空");
     		return false;
    	}
    	
    	if(visaserialno == ''&& insuredName ==''){
     		alert("单证流水号和被保险人名称请任选其一填写");
     		return false;
    	}
    	
    	if(visaserialno!=''&&insuredName!=''){
     		alert("单证流水号与被保险人名称只能选择一项");
     		return false;
    	}
    	
    	document.getElementById("findWay_uncar").value=findWay;
    	document.getElementById("policyNo_uncar").value=plyNoForUnvechile;
    	document.getElementById("visaserialno_uncar").value=visaserialno;
    	document.getElementById("insuredName_uncar").value=insuredName;
    	
    	
    	var fm = document.getElementById("fm_uncar");
   		fm.action = "/TPEBizWeb/pages/B2C/anHuiPolicyQuery/queryUnVechileAll.do";
   		fm.submit();
 }
 
 function submitFormYiwai(){
		var visaserialno = document.getElementById("visaserialnoYiWai").value;
    	var plyNoForUnvechile = document.getElementById("plyNoForUnvechileYiWai").value;
    	var insuredName = document.getElementById("insuredNameYiWai").value;
    	var findWay = document.getElementById("findWay").value;
    	if(plyNoForUnvechile == ''){
     		alert("保单号不能为空");
     		return false;
    	}
    	
    	if(visaserialno == ''&& insuredName ==''){
     		alert("单证流水号和被保险人名称请任选其一填写");
     		return false;
    	}
    	
    	if(visaserialno!=''&&insuredName!=''){
     		alert("单证流水号与被保险人名称只能选择一项");
     		return false;
    	}
    	
    	document.getElementById("findWay_uncarYiWai").value=findWay;
    	document.getElementById("policyNo_uncarYiWai").value=plyNoForUnvechile;
    	document.getElementById("visaserialno_uncarYiWai").value=visaserialno;
    	document.getElementById("insuredName_uncarYiWai").value=insuredName;
    	
    	var fm = document.getElementById("fm_yiwai");
    	if(plyNoForUnvechile=='62001101320130000006'||plyNoForUnvechile=='62001101320130000007') {
			insuredName = encodeURI(insuredName);
    		fm.action = "/TPEBizWeb/pages/B2C/show/downLoad.do?insuredName=" + insuredName + "&policyNo=" + plyNoForUnvechile;
   			fm.submit();
    	}else {
    		fm.action = "/TPEBizWeb/pages/B2C/anHuiPolicyQuery/queryUnVechileAll.do";
   			fm.submit();
    	}
 }
 function submitForm11(){
		var findWay = document.getElementById("findWay").value;
   		var policyNo1 =document.getElementById("policyNo1").value;
   		var insuredName1 =document.getElementById("insuredName1").value;
   		var insuredIdentifyNumber =document.getElementById("insuredIdentifyNumber").value;
		if(policyNo1==""){
			alert("保单号不能为空！");
    	  	document.getElementById("policyNo1").focus();
			return false;
   		}
   		if(document.getElementById("insuredName1").value=="" ){
			alert("被保险人名称不能为空！");
			document.getElementById("insuredName1").focus();
      		return false;
   		}
   		if(document.getElementById("insuredIdentifyNumber").value=="" ){
			alert("被保险人证件号码不能为空！");
			document.getElementById("insuredIdentifyNumber").focus();
      		return false;
   		}
   		
   		document.getElementById("findWay_uncarother").value=findWay;
    	document.getElementById("policyNo_uncarother").value=policyNo1;
    	document.getElementById("insuredName_uncarother").value=insuredName1;
   		document.getElementById("insuredIdentifyNumber_uncarother").value=insuredIdentifyNumber;
   		
	   	var fm = document.getElementById("fm_uncarother");
   		fm.action = "/TPEBizWeb/pages/B2C/anHuiPolicyQuery/queryOtherUnVechile.do";
   		fm.submit();
 }
 
 function submitForm2(){
    var policyNo = document.getElementById("policyNo").value;
    var carframeNo = document.getElementById("carframeNo").value;
    	if(policyNo == ''){
     		alert("保单号不能为空");
     		return false;
    	}
    	if(carframeNo == ''){
     		alert("被保险人证件号不能为空");
     		return false;
    	}
    var fm = document.getElementById("car");
	    fm.action = "/TPEBizWeb/pages/B2C/claimQueryForQingdao/vechileClaim.do";
   	fm.submit();
 }
 
 
 
 
 function submitForm3(){
		var policyNo =document.getElementById("cappnme11").value;
		if(document.getElementById("cappnme11").value==""){
			alert("保单号不能为空！");
    	  	document.getElementById("cappnme11").focus();
			return false;
   		}
   		if(document.getElementById("plyno11").value=="" ){
			alert("投保人证件号码不能为空！");
			document.getElementById("plyno11").focus();
      		return false;
   		}
	   	var fm = document.getElementById("unCar");
   		fm.action = "/TPEBizWeb/pages/B2C/claimQueryForQingdao/claimStatusQuery.do";
   		fm.submit();
}
 
 //交易日期转换
function netPolicyDateFill(dateObject,dateStr){
   document.getElementById(dateStr).value = dateObject.value;
}
 
 function reload(field){
    field.setAttribute('src','/TPEBizWeb/CreateImage?'+Math.random());
 }
 
