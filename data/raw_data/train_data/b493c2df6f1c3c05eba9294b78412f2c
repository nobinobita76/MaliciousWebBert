          </div> 
          <!-- 推荐 楼数 --> 
          <div class="o_info" id="first-ad"> 
           <div login_user="" user_id="548603641" rid="111457110" tid="7381557" floor="388" have_set="" style="height:30px;position:absolute;bottom:0;right:78px;z-index:10"> 
