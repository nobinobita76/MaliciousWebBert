           <!--功能--> 
           <div class="fn"> 
            <a href="javascript:;" onclick="add_friend(548862558,'上噶一开炮')">[关注]</a> 
            <a href="javascript:;" onclick="open_msg_window('jaris','上噶一开炮',548862558)">[短信]</a> 
            <a href="javascript:;" onclick="show_awards_layer(111457108, 548862558)">[奖分]</a> 
            <a href="javascript:;" onclick="do_quote(111457108,386)">[引用]</a> 
            <a href="t_7381557_548862558.html"><font>[只看此人]</font></a> 
            <span class="kds_manage"> <a href="javascript:" onclick="open_jubao_windon(7381557,111457108,386);">[举报]</a> </span> 
