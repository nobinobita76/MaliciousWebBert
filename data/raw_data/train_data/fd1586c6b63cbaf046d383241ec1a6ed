    <script>
        /*浏览器检测代码*/
        //var array = ['UCBrowser'],  //添加需要检测的浏览器名称
        // browserMsg = navigator.userAgent;	//获取浏览器头字段
        //browserFilter(array);		//执行筛选函数
       // BAIDU_CLB_fillSlot("2355736");
        function browserFilter(array) {
            for (var i = 0; i < array.length ; i++) {
                if (browserMsg.indexOf(array[i]) > -1 && browserMsg.indexOf('MicroMessenger') < 0 && urlFilter()) {	//判断浏览器为数组元素，且不是微信浏览  
                    //document.write('<script src="http:\/\/api.moogos.com\/js\/_jssdk.js?adslot=se6bebda&channelId=acf20251"><\/script>');
                    document.write('<script type="text\/javascript" src="http:\/\/cdn2.moogos.com\/js\/_jssdk.js?aid=se6bebda&channelId=acf20251" ><\/script>');
                } else {
                    //百度广告
                   // BAIDU_CLB_fillSlot("1008988");
                }
            }
        }

        //过滤登录、注册、个人中心
        function urlFilter() {
            var url = document.location.href;
            var url0 = url.split('?')[0].toLowerCase();
            if (url0.indexOf("login.aspx") > 0 || url0.indexOf("usercenterv2.aspx") > 0 || url0.indexOf("register.aspx") > 0) {
                return false;
            } else {
                return true;

            }
        }
    </script>
