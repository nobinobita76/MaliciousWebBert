        function fnMonitor(Pid, Tid) {
            try {
                var monitorImg = document.createElement('img');
                monitorImg.src = "http://a.itiexue.net/w/4/home/index?pid=" + Pid + "&tid=" + Tid;
                monitorImg.width = '1px';
                monitorImg.height = '1px';
                monitorImg.style.display = 'none';
                document.getElementsByTagName('body')[0].appendChild(monitorImg);
            } catch (e) { }
        }
