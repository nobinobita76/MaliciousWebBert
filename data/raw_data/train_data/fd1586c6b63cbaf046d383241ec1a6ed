    function getQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        var r = window.location.search.substr(1).match(reg);
        if (r != null) return unescape(r[2]); return null;
    }
    function getLogoutUrl() {
        var url;
        if (getQueryString("logout") != null) {
            url = location.href.replace("logout=0", "logout=1");
        }
        else {
            if (location.href.indexOf("?") > 0) {
                url = location.href.replace("?", "?logout=1&");
            }
            else {
                url = location.href + "?logout=1";
            }
        }

        return url;
    }
    var isLogin = "False";
    var url = "http://m.tiexue.net/touch/Login.aspx?retto=" + encodeURI(window.location.href);
    var name = "登录注册";
    var isAdd = "False";
    var isMsg = "False";
    var forum = "0";
    var hasMsg = "False";
    if (_tx.GetUserID() > 0) {
        isLogin = "True";
        name = _tx.GetUserName();
        url = "http://m.tiexue.net/touch/UserCenterV2.aspx";
    } else {
        _tx.R("link").className = "";
    }
    if (hasMsg == "True") {
        url = "http://m.tiexue.net/touch/MessageList.aspx";
    }
    if (isAdd == "True") {
        name = "发帖";
        url = "http://m.tiexue.net/touch/AddPost.aspx?forumID=" + forum;
        if (isLogin != "True") {
            url = "http://m.tiexue.net/touch/Login.aspx?retto=" + escape("AddPost.aspx?forumID=" + forum);
        }
    }
    if (isMsg == "False") {
        if (name.length > 10) {
            name = name.substr(0, 9) + "...<em id='msgNum' style='display:none;'></em>";
        } else {
            name = name + "<em id='msgNum' style='display:none;'></em>";
        }
    }
    else {
        name = "";
        url = "http://m.tiexue.net/touch/AddMessage.aspx";
    }
    if (_tx.R("link")) {
        _tx.R("link").innerHTML = name;
        _tx.R("link").href = url;
