        //回顶部  //滚动加载
        var num = 2; 
        var threeNum=0;

        $(window).scroll(function () {
            if ($(window).scrollTop() > $(window).height()) {
                $('.toTop').show();
            } else {
                $('.toTop').hide();
            }
            if (num > 8) {
                return;
            }
            var bot = 160;
            var scrolltop = $(window).scrollTop();
            var scrollHeight = $(document).height();
            var windowHeight = $(window).height();
            if (scrolltop + bot >= scrollHeight - windowHeight) {
                var url = "http://m.tiexue.net/wapcommon/report/uc/" + num + "_json.html";
                $.ajax({
                    url: url, async: false, cache: false, success: function (datas) {
                        var obj = JSON.parse(datas);
                        var html = '<section>';
                        var fnnum = (3100 + num) * 100 + 1;
                        var fms = '';
                        for (var i = 0; i < obj.length; i++) {
                            if (num <= 8) {
                                var tid = obj[i].URL.match(/thread_(\d+)_(\d+).html/i)[1];
                                fms = 'fnMonitor(' + (fnnum + i) + ',' + tid + ');';
                                if (i == 0) {
                                    html += '<script>fnMonitor(' + '310' + num + '0101' + ',0);<\/script>';
                                }
                            }
                            if (obj[i].Imgs.length == 3) {
                                if (threeNum % 3 == 0) {
                                    html += '<div class="threeImg">';
                                    html += '<a onclick=\'' + fms + '_czc.push(["_trackEvent", "index", "a", "list_' + num + '_' + (i + 1) + '"])\' href="' + formaturl(obj[i].URL) + '" class="listPic">';
                                    html += '<h2>' + decodeURIComponent(RepPlus(obj[i].Title)) + '</h2>';
                                    html += '<div class="imgList"><img src="' + obj[i].Imgs[0] + '"><img src="' + obj[i].Imgs[1] + '"><img src="' + obj[i].Imgs[2] + '"></div>';
                                    html += '<aside><span class="bk_ico">' + decodeURIComponent(obj[i].ForumName).substring(0, 2) + '</span>'
                                }
                                else {
                                    html += '<div class="oneImg">';
                                    html += '<a onclick=\'' + fms + '_czc.push(["_trackEvent", "index", "a", "list_' + num + '_' + (i + 1) + '"])\' href="' + formaturl(obj[i].URL) + '" class="listPic"><img src="' + obj[i].ImgUrl + '">';
                                    html += '<h2>' + decodeURIComponent(RepPlus(obj[i].Title)) + '</h2>';
                                    html += '<aside><span class="bk_ico">' + decodeURIComponent(obj[i].ForumName).substring(0, 2) + '</span> '
                                }
                                threeNum++;
                            } else if (obj[i].Imgs.length == 1) {
                                html += '<div class="oneImg">';
                                html += '<a onclick=\'' + fms + '_czc.push(["_trackEvent", "index", "b", "list_' + num + '_' + (i + 1) + '"])\' href="' + formaturl(obj[i].URL) + '" class="listPic"><img src="' + obj[i].ImgUrl + '">';
                                html += '<h2>' + decodeURIComponent(RepPlus(obj[i].Title)) + '</h2>';
                                html += '<aside><span class="bk_ico">' + decodeURIComponent(obj[i].ForumName).substring(0, 2) + '</span> '
                            }
                            else {
                                html += '<div class="noImg">';
                                html += '<a onclick=\'' + fms + '_czc.push(["_trackEvent", "index", "b", "list_' + num + '_' + (i + 1) + '"])\' href="' + formaturl(obj[i].URL) + '">';
                                html += '<h2>' + decodeURIComponent(RepPlus(obj[i].Title)) + '</h2>';
                                html += '<aside><span class="bk_ico">' + decodeURIComponent(obj[i].ForumName).substring(0, 2) + '</span>'
                            }
                            html += ' <span class="f-c-9eb6d4">' + decodeURIComponent(obj[i].Authors) + '</span>'
                            html += ' <span class="time">' + getAmityTime(obj[i].TimeStamp) + '</span>';
                            html += ' <span>阅读数：' + obj[i].TotalViews + '</span>'
                            //html += '<span class="review">评论：' + obj[i].TotalReplies + '</span>';
                            html += '</aside></a></div>';
                            if (num < 3) {
                                if (i == 0 || i == 3 || i == 6 || i == 9 || i == 12) {
                                    html += '<div id="ad' + num + '_' + i + '"></div>';
                                }
                            }
                            else {
                                var nd = new Date();
                                var tnd = new Date(nd.getFullYear(), nd.getMonth(), nd.getDate(), nd.getHours(), 0, 0, 0);
                                var tvs = tnd.getTime().toString();
                                if (i == 0) {
                                    html += '<div class="g_iframe" style="padding-bottom:0.5rem;padding-top:1rem"><iframe width="100%" height="85px" src="http://m.tiexue.net/zgg/ggb_2557260.htm?' + parseInt(Math.random() * 1000) + '" frameborder="0" scrolling="no"></iframe></div>';
                                }
                                else if (i == 3) {
                                    html += '<div class="g_iframe" style="padding-bottom:0.5rem;padding-top:1rem"><iframe width="100%" height="85px" src="http://m.tiexue.net/zgg/ggb_2557260.htm?t=?' + parseInt(Math.random() * 1000) + '" frameborder="0" scrolling="no"></iframe></div>';
                                }
                            }
                        }
                        html += '</section>';
                        $('#bomNum0').append(html);                         
                        num++;
                        if (num > 8) {
                            $('.over_login').show();
                            $('.loading').hide();
                        }
                    }
                });
            }
        })
       
        function closeCheck(_status) {
            if (_status.code == 1) {
                _tx.R('checkDiv').style.display='none';
            }
        }
        function formaturl(url) {
            if (url.indexOf('?') == -1) {
                return url + '?f=b';
            }
            else {
                return url + '&f=b';
            }
        }
