					<script type="text/javascript">
					COLLECT_MSG_STATUS = 0;
					function collect_msg(){
						if(COLLECT_MSG_STATUS == 1){
							return false;
						}
						
						$.ajax({
							type: "POST",
							dataType: "json",
							url: "http://"+window.location.host+"/my/?m=collect&a=doc&msg_id="+ED.msg.msgid,
							data: "ajax_msg_collect=1",
							success: function(data){
								if(data.code == 1){
									COLLECT_MSG_STATUS = 1;
								}else{
									alert(data.msg);
								}
							}
						});
					}
					</script>
					
