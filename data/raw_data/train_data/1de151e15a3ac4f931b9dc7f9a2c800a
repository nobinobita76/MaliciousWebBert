			var arr = EsApp.getInfo();
			appObj.init(JSON.stringify(arr));
			EsApp.invoke("titlebar", { isShow :0});

			$(".ft_btn_touzhu").on(clickEvent,function(){
				var id = $(this).attr("lotid"),
				cp = $(this).attr("data-type"),
				mhref=$(this).attr("mhref"),
				playid = $(this).attr("playid");
				if(cp=='cp'){
					window.location.href= mhref;
				}else{
					EsApp.invoke('buy',{lotid:id,playid:(playid?playid:'')});
				}
			});
		};
		EsApp.init(); // 初始化API
	},500)
	
}


$('.dropmenu_hd').on(clickEvent,function(e){
    $(this).parents('.dropmenu').find('.dropmenu_bd').toggle();
    e.stopPropagation();
});

$("body").on(clickEvent,function(){
	$(".dropmenu_bd").hide();
});

$(".headernavL").css({"cursor":"pointer"}).click(function(e){
	e.stopPropagation();
	//window.EsApp ? EsApp.invoke('back'):window.location.href = 'http://'+location.host+'/info/kaijiang/';
	window.EsApp ? EsApp.invoke('back'):(window.history.back());
	
});

$(".ui-head-btn3").on(clickEvent,function(){
	window.EsApp ? EsApp.invoke('home') : window.location.href = 'http://'+location.host;
});
$("#div_e a").on(clickEvent,function(){
	window.location.href = $(this).attr("mhref");
});
$(".ft_btn_touzhu").on(clickEvent,function(){//buy
	var id = $(this).attr("lotid"),
	playid = $(this).attr("playid"),
	mhref=$(this).attr("mhref"),
	cp = $(this).attr("data-type");
	if(!window.EsApp|| cp=='cp'){//触屏
		window.location.href = mhref;
	}		
});
//分享
$(".share-icon").on(clickEvent,function(){
	if(window.EsApp){
		EsApp.send('share', {
               common: {
                   title: shareObj.title,
                   url:  shareObj.link,
                   content:  shareObj.desc,
                   icon: shareObj.icon
               }
             },function(channel) {});
	}else if(isWx){
		 getWxShare();
		 $(".js-ui-acttemp-layer,.js-ui-acttemp-wx-alert").removeClass("hide");
	}else{
		if(/MQQBrowser|UCBrowser|iPhone|iPod/i.test(navigator.userAgent)) {
			$(".js-ui-acttemp-layer,.js-ui-acttemp-tuc-alert-2").removeClass("hide");
		}else{
			$(".js-ui-acttemp-layer,.js-ui-acttemp-tuc-alert-3").removeClass("hide");	
		}
	}
});

//关闭弹窗
$(".js-ui-acttemp-layer,.js-ui-acttemp-tuc-alert-2,.js-ui-acttemp-wx-alert,.js-ui-acttemp-tuc-alert-3").on(clickEvent,function(){
	$(".js-ui-acttemp-layer,.js-ui-acttemp-tuc-alert-2,.js-ui-acttemp-wx-alert,.js-ui-acttemp-tuc-alert-3").addClass("hide");	
});
</script>

