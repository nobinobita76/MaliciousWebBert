<script>
    var fyb_api = 'http://api.youxinjinrong.com';
    $(function(){
        // onClick="zhuge_track('付一半方案页-下载APP');"
        //#######打点逻辑开始###############
        //zhuge_track('P-付一半购买方案页(M)', { '城市':"广州" });
        //zhuge_track('付一半方案页');
        /*zhuge_track('C-付一半购买方案页(M)', {
         '城市':_zg_city_name,
         '车辆ID':_zg_carid,
         '是否半价':_zg_is_half
         });*/
                $(".finan_btn").click(function(){
            zhuge_track('C-付一半方案页-提交资料', {
                '城市':_zg_city_name,
                '车辆ID':_zg_carid,
                '是否半价':_zg_is_half
            });
            var _href=$(this).attr('_href');
            window.setTimeout(function(){
                window.parent.top.location.href = _href;
            },100);
            //return false;
        });
                //#######打点逻辑结束###############
        $('#get_tel').click(function(){
            zhuge_track('C-付一半方案页-电话咨询', {
                '城市':_zg_city_name,
                '车辆ID':_zg_carid,
                '是否半价':_zg_is_half
            });
            //alert($(this).attr('href'));
            var carid = 77524749;
            $.getJSON(fyb_api+'/ux_finance/financial_plan/get_tel?carid='+carid+'&os=wap&nb=&jsoncallback=?',function(data){
                //console.log(data);
                if(data.tel){
                	_abq.push(['_track', 'half_plan', 'tel_consulting',  '('+data.tel+')/M']);
                    window.parent.top.location.href = 'tel:'+data.tel;
                }
            });
            // half_plan/tel_consulting/(400_num)/M
            //uxevent='half_plan/tel_consulting/(400_num)/M'

        });
    });
</script>
