        var agent = get_agent();
        if(agent == 'weixin') {
            dl_link = 'http://a.app.qq.com/o/simple.jsp?pkgname=com.uxin.usedcar&g_f=991653';
        } else if(agent == 'ios') {
            dl_link = 'http://app.youxinpai.com/dl/32/';
        }
        // 修改下载链接
        $('#app_dl_s').attr('href', dl_link);
    });


    /**
     * 获取浏览器agent
     **/
    function get_agent()
    {
        var ua = navigator.userAgent.toLowerCase();
        var agent = 'other';
        if(ua.match(/MicroMessenger/i)=="micromessenger") {
            agent = 'weixin';
        } else if(/android/.test(ua)) {
            agent = 'android';
        } else if(/iphone|ipad|ipod/.test(ua)) {
            agent = 'ios';
        }

        return agent;
    }
