  <script type="text/javascript">
    $.yyDialogCreat.callFn = function (dzId, tId) {
        smAjax({
            url: "/Common/ShareMethod",
            data: { "dzId": dzId, "tId": tId },
            type: "post", 
            success: function (data) {
                if (data.code == 1000) {
                    //调用分享动作成功
                    tipFun(data.message);
                } else {

                }
            }
        });
        tipLoadingRemove();
    };
    fnGetImgRef = function(img) {
        var v = img;
        if (typeof(v) != "undefined") {
            if (v.indexOf("http://mmbiz.qpic.cn") == 0 || v.indexOf("https://mmbiz.qpic.cn") == 0 || v.indexOf("http://mmbiz.qlogo.cn") == 0 || v.indexOf("https://mmbiz.qlogo.cn") == 0) {
                v = "http://imgref.ccoo.cn/Ajax/ImgRef.ashx?url=" + img;
            }
        }
        return v;
    };


    $(function () {
        GetRelatedNews();
        $("#js_content img").each(function(e) {
            var v = fnGetImgRef($(this).attr("src"));
            $(this).attr("src", v);
        });
    })
    //分享
    $('.js-share').on('click', function () {
        
        ccooShare.openshare({dzId:110,tId:5087619});
    })

    //点击tool评论 
    $(".toolshare2tool .js2pl").on("click", function () {
        plData();
        var allnum = $(this).data('allnum');
        //alert(allnum)
        $('#js2submit').find('em').text(allnum);
        $('#js2submit textarea').val('');
        $('#js2submit textarea').attr("data-ReplyID", $(this).attr("data-ReplyID"));
        ('#js2submit textarea').nextAll('.inputright').show();
    });
    //点击楼层评论icon或点击楼中楼文章
    

    // 2016-12-28wm textarea自适应高度
    $(function(){
        $("#txtContent").each(function(){
            $(this).on("focus input",function(){
                !$(this).val()==""?$(".inputright").hide():$(".inputright").show();
                
                if(!$(this).prev().hasClass("pre")){
                    $(this).before("<pre class='pre'></pre>");
                    toHeight($(this));
                }else{
                    toHeight($(this))
                }
                if($(this).height()>60){
                    $(this).css({"overflow":"auto","height":"60px"})
                }else{
                    $(this).css("overflow","hidden")
                }
            }).on("blur",function(){
                $(this).prev(".pre").remove();
            });
        })
        function toHeight(ele){
            ele.prev(".pre").width(ele.width()).text(ele.val());
            ele.prev(".pre").text(ele.val());
            ele.height(ele.prev(".pre").height());
        }
    });
    //锚点 动画
    function plData() {
        var newid = $(this).data('newid')
        var uid = $(this).data('uid');
        var toid = ($(this).data('toid') == undefined) ? 0 : $(this).data('toid');
        var loucengid = ($(this).data('loucengid') == undefined) ? 0 : $(this).data('loucengid');
        $('#js2submit input[type="text"],#js2submit textarea').focus();
        $('#js2submit').attr('data-newid', newid);
        $('#js2submit').attr('data-uid', uid);
        $('#js2submit').attr('data-toid', toid);
        $('#js2submit').attr('data-loucengid', loucengid);
    }
    //切换tab
    $('.navtab .tablecell').on("click", function () {
        $('.navtab .tablecell').removeClass('active');
        $(this).addClass('active');
    });

    //赞新闻或赞回复
    //$("#content .ding,.toolshare2tool .ding").on("click", function () {
    $("#mainDiv").on("click", ".ding", function () {
        var $this = $(this);
        var newsID = $(this).attr("data-newsId");
        var replyID = $(this).attr("data-ReplyId");
        var type = $(this).attr("data-Type");//0:赞新闻  1：赞评论
        var _that = $(this);
        var znum=0;
        if ($(".newsZanNum").text()!=""){
            var znum=parseInt($(".newsZanNum").text());
        }
        smAjax({
            type: "Post",
            url: "/News/Zan",
            dataType: 'json',
            data: {
                newsID: newsID,
                replyID: replyID,
                type: type
            },
            success: function (data) {
                if (data.code == 1000) {
                    if (data.priCount > 0) {//一言九鼎
                        if (type == "0") {
                            $('.newsZan').addClass('active');
                            $('.newsZan').find("i").addClass("icon-dianzan2");
                            $('.a_zan').addClass('on');
                            $(".newsZanNum").text(znum + 9);																	
                            $this.find(".toolshare2icon").append("<b class='on'>+9</b>");
                            $this.find(".toolshare2icon b").animate({ "opacity": 0, "bottom": "20px" }, 500);
                            var nick='';
                            if(znum==0){							
                                $(".toolshare2header").html('<span id="cnick" class="f666">'+nick+'</span><span id="cnum" class="f999">等9人赞过</span>');
                            }else{
                                $("#cnick").html(nick+'、'+$("#cnick").html());
                            }
                        }
                        else if (type == "1") {
                            _that.addClass('active');
                            _that.find("i").addClass("icon-dianzan2");
                            $(".replyZanNum" + replyID + "").text(parseInt($(".replyZanNum" + replyID + "").text()) + 9);
                            $this.find(".toolshare2icon b").animate({ "opacity": 0, "bottom": "20px" }, 500);
                            $this.find(".toolshare2icon").append("<b class='on'>+9</b>");
                        }
                    }
                    else {
                        if (type == "0") {
                            $('.newsZan').addClass('active');
                            $('.newsZan').find("i").addClass("icon-dianzan2");
                            $('.a_zan').addClass('on');
                            $(".newsZanNum").text(znum + 1);
                            $this.find(".toolshare2icon").append("<b class='on'>+1</b>");
                            $this.find(".toolshare2icon b").animate({ "opacity": 0, "bottom": "20px" }, 500);
                            var nick='';
                            if(znum==0){							
                                $(".toolshare2header").html('<span id="cnick" class="f666">'+nick+'</span><span id="cnum" class="f999">等1人赞过</span>');
                            }else{
                                $("#cnick").html(nick+'、'+$("#cnick").html());
                            }
                        }
                        else if (type == "1") {
                            _that.addClass('active');
                            _that.find("i").addClass("icon-dianzan2");
                            $(".replyZanNum" + replyID + "").text(parseInt($(".replyZanNum" + replyID + "").text()) + 1);
                            $this.find(".toolshare2icon").append("<b class='on'>+1</b>");
                            $this.find(".toolshare2icon b").animate({ "opacity": 0, "bottom": "20px" }, 500);
                        }
                    }
                    tipFun(data.message);
                }
                else if (data.code == 1002) {
                    tipFun("你已点过赞了~");
                }
                else if (data.code == 1004) {
                    tipFun(data.message);
                    window.location.href = "/login/?reurl=" + window.location.href;
                }
            }
        })
    });

    //发表评论
    $("#js2submit").on("click", ".submit", function () {
        if (!doisLogin()){return false}
        $(this).attr("diasbled", true);
        var newsID = $("#txtContent").attr("data-newsid");
        var replyID = $("#txtContent").attr("data-replyid");
        var content = $("#txtContent").val().trim();
        if (content.trim() == "" || content.trim == null) {
            tipFun("内容不能为空~");
            return false;
        }
        if(content.length>2000){
            tipFun("评论内容不能超过2000字符~");
            return false;
        }
        smAjax({
            type: "Post",
            url: "/News/Reply",
            dataType: 'json',
            data: {
                content: content,
                newsID: newsID,
                parentID: replyID
            },
            success: function (data) {
                $(this).attr("diasbled", false);
                var reUrl = "";
                var isDaka = $("#isDaka").val();
                if (isDaka == "1") {
                    reUrl = "/cnews/news_show.aspx?id=" + newsID + "&type=1&temp=" + Math.random() * 100 + "#rc1";
                } else {
                    reUrl = "/cnews/news_show.aspx?id=" + newsID + "&temp=" + Math.random() * 100 + "#rc1";
                }
                if (data.code == 1000) {
                    tipFun(data.message, function () {
                        location.href = reUrl;
                    });                  
                } else if (data.code == 9) {
                    tipFun(data.message, function () {
                        location.href = reUrl;
                        //location.href = "/cnews/news_show.aspx?id=" + newsID+"&temp="+Math.random()*100+"#rc1";
                    });
                } else if (data.code == 1004) {
                    tipFun(data.message, function () {
                        window.location.href = "/login/?reurl=" + window.location.href;
                    });
                } else {
                    tipFun(data.message);
                    return;
                }
            }
        });
    });

    var hasData = false;
    var pageIndex = 2;
    var pageSize = 10;
    var nowCount = $("#newReplyCount").val();
    if (parseInt(nowCount) > pageSize) {//当数量达到分页条件时，滚动分页
        hasData = true;
    }

    $(window).scroll(function () {
        if (hasData) {
            hasData = false;
            $(".loading").html("<img src='http://res.pccoo.cn/wap/webapp/images/loading.gif'>正在努力加载~");
            getData(pageIndex);
        }
    });

    function getData(index) {
        $.ajax({
            url: '/News/ReplyPaging/',
            type: 'post',
            data: { "type": $("#type").val(), "newsID": $("#newsID").val(), "CurPage": index, "PageSize": pageSize },
            success: function (data) {
                if (data != "") {
                    pageIndex = pageIndex + 1;
                    $(".loading").html("滑动加载更多~");
                    $("#content").append(data);
                    hasData = true;
                } else {
                    hasData = false;
                    $(".loading").html("没有更多内容了~");
                }
            }
        });
    }

    //分享
    $(".more").click(function () {
        $(".more-wrap").slideToggle();
    });

    $(".more-wrap a:first-child").click(function () {
        $(this).find("i").addClass("on");
        $(".more-wrap").slideToggle();
    });

    $(".replyDetail").click(function () {
        var id = $(this).attr("data-Id");
        var medals = $(this).attr("data-Medal");
        window.location.href = "/cnews/reply.aspx?replyid=" + id + "&medal=" + medals;
    });

    //返回按钮，评论信息没有提交给予提示
    $(".back").click(function() {
        var content = $("#txtContent").val();
        if (content.trim() != "" && content != null && content != "undefine") {
            if (confirm("你还未完成发布，确定退出吗？")) {
                fback();
            }
        } else {
            fback();
        }
    });
</script> 
