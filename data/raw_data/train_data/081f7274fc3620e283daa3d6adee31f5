<script type="text/javascript">
  $(function() {
    //滚动图
    $('#carousel ul').carouFredSel({
      prev: '#prev',
      next: '#next',
      pagination: "#pager",
      scroll: 1000
    });
    //鼠标移入出字
    $('#carousel a').hover(
            (function () {
              $(this).find('i').css('display', 'inline-block');
              $(this).find('i').css({height: '152px'});
            }),
            (function () {
              $(this).find('i').css({height: '0px'}, function () {
                $(this).find('i').css("display", "none");
              });
            })
    );
    //快捷方式
    $('.kuai').hover(
            function(){
              
              $(this).find('.arrow_1').addClass("hover");
              $('.box_01').fadeToggle(100);
            },
            function(){
              
              $(this).find('.arrow_1').removeClass("hover");
              $('.box_01').fadeToggle(100);
            }
    )
  })
</script>


