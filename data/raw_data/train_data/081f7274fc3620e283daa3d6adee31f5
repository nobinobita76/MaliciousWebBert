    function check(){
      var name=$("#name").val();
      var mobile=$("#mobile").val();
      if(name==""){
        alert("团干部姓名不能为空");
        return false;
      }else if(mobile==""){
        alert("团干部手机不能为空");
        return false;
      }else{
        $.ajax({
          type: "post",
          url: "/youth/json/check.do?t=" + new Date().getTime(),
          data: $("#formAdd").serialize(),
          success: function (datalogs) {
            var json = $.parseJSON(datalogs);
            if (json.success) {
              $("#formAdd").submit();
            } else {
              alert("查不到对应团干部，请检查您的输入信息！");
            }
          },
          error: function (message) {
          }
        })
      }
    }
    function checkYouth(){
      var name=$("#name1").val();
      var mobile=$("#mobile1").val();
      var phone=$("#phone").val();
      if(name==""){
        alert("团干部姓名不能为空");
        return false;
      }else if(mobile==""){
        alert("团干部手机不能为空");
        return false;
      }else if(phone==""){
        alert("我的手机不能为空");
        return false;
      }else{
        $.ajax({
          type: "post",
          url: "/youth/json/checkYouth.do?t=" + new Date().getTime(),
          data: $("#youth").serialize(),
          success: function (datalogs) {
            var json = $.parseJSON(datalogs);
            if (json.success) {
              $("#youth").submit();
            } else {
              alert("查不到对应团干部，请检查您的输入信息！");
            }
          },
          error: function (message) {
          }
        })
      }
    }
  </script>


