              <!--中间地图开始-->
              <script type="text/javascript">
                function randomData() {
                  return Math.round(Math.random()*100);
                }
                require.config({
                  paths: {
                    'echarts': 'http://s.learning.youth.cn/resources/front/echarts/dist'
                  }
                });
                require(
                        [
                          'echarts',
                          'echarts/chart/map'// 按需加载
                        ], function (ec) {
                          // 基于准备好的dom，初始化echarts图表
                          var myChart = ec.init(document.getElementById('main'));
                          data1=[{name: '北京',value:0.46},{name: '天津',value:1.35},{name: '河北',value:1.38},{name: '山西',value:2.68},{name: '内蒙古',value:1.98},{name: '辽宁',value:2.05},{name: '吉林',value:0.7},{name: '黑龙江',value:1.93},{name: '上海',value:4.49},{name: '江苏',value:0.99},{name: '浙江',value:0.39},{name: '安徽',value:1.3},{name: '福建',value:1.85},{name: '江西',value:10.93},{name: '山东',value:2.97},{name: '河南',value:4.66},{name: '湖北',value:3.79},{name: '湖南',value:3.61},{name: '广东',value:2.31},{name: '海南',value:2.28},{name: '广西',value:3.88},{name: '甘肃',value:9.62},{name: '陕西',value:9.31},{name: '新疆',value:0.66},{name: '青海',value:0.26},{name: '宁夏',value:1.24},{name: '重庆',value:1.96},{name: '四川',value:1.79},{name: '贵州',value:6.81},{name: '云南',value:1.33},{name: '西藏',value:0.0},{name: '兵团',value:1.37},];
                          option = {
                            tooltip : {
                              trigger: 'item',
                              formatter : function (params) {
                                var res = params.seriesName + '<br/>' + params.name + ' : ';
                                for (var i = 0; i < data1.length; i++) {
                                  if(params.name == data1[i].name){
                                    res += data1[i].value;
                                  }
                                }
                                return res;
                              }
                            },
                            dataRange: {
                              min: 0,
                              max: 100,
                              text:['高','低'],
                              splitNumber: 4,
                              splitList: [
                                {start: 75, end: 100},
                                {start: 50, end: 75},
                                {start: 25, end: 50},
                                {end: 0, end: 25}
                              ],
                              color: ['#ff4e50', '#EE7600', '#FFFF00', '#FFE7BA']
                            },
                            series : [
                              {
                                name: '热度指数',
                                type: 'map',
                                mapType: 'china',
                                selectedMode : 'single',
                                itemStyle:{
                                  normal:{label:{show:true}
                                  },
                                  emphasis:{
                                    color: '#08c',
                                    label:{show:true},
                                  }
                                },
                                data:[{name: '北京',value:25},{name: '天津',value:50},{name: '河北',value:50},{name: '山西',value:75},{name: '内蒙古',value:75},{name: '辽宁',value:75},{name: '吉林',value:50},{name: '黑龙江',value:50},{name: '上海',value:75},{name: '江苏',value:50},{name: '浙江',value:25},{name: '安徽',value:50},{name: '福建',value:50},{name: '江西',value:100},{name: '山东',value:75},{name: '河南',value:100},{name: '湖北',value:75},{name: '湖南',value:75},{name: '广东',value:75},{name: '海南',value:75},{name: '广西',value:75},{name: '甘肃',value:100},{name: '陕西',value:100},{name: '新疆',value:25},{name: '青海',value:25},{name: '宁夏',value:50},{name: '重庆',value:50},{name: '四川',value:50},{name: '贵州',value:100},{name: '云南',value:50},{name: '西藏',value:25},{name: '兵团',value:50},]
                              }
                            ]};
                          // 为echarts对象加载数据
                          myChart.setOption(option);
                          myChart.on('click', function (params) {
                            window.open('/youth/front/ranking.do?province=' + encodeURIComponent(params.name));
                          });
                        }
                );
              </script>
            <p class="map_zi3 clearfix" style="top: 560px;"><i class="zhu fl">注：</i><i class="zhushi fl">1、各省热度指数由本省团干部活跃指数平均产生，包括开展活动次数，青年互动情况等。2、点击地图中的省可进入该省的各市联系青年工作热度指数排行榜。<br>3、输入团干部姓名和手机号可以查看该省团干部的联系青年情况。</i></p>
