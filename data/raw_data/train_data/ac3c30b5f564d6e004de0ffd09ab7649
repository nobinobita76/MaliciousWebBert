  <script>
				$(function(){
					//digg赞初始化ajax  纯静态网页使用
					$.post("http://m.kuhuan.net/index.php?m=content&c=digg",{id:152,modelid:2},function(data){
									$("#zan_ck em").html(data.zanbai);
									$("#zan_ck span b").css("width",data.zanbai);
									$("#ruo_ck em").html(data.caibai);
									$("#ruo_ck span b").css("width",data.caibai);
									$(".col-l .p1 span em").html(data.fenshu);
									$("#scoreImg").attr("src","http://www.kuhuan.net/statics/images/pc-start/"+Math.floor(data.fenshu)+".png");	
									
					},"json")
					//digg点击+1
			    $(".zancai").on("click",'',function(){
						//获取data-val值 赞为1 踩为0
						var zhi = $(this).data('val');
						//ajax传输数据
						$.ajax({
							 type: "POST",
				             url: "http://m.kuhuan.net/index.php?m=content&c=digg&a=diggs",
				             data: {id:152,modelid:2,modelid:2,zhi:zhi},//zhi为0表示踩 1为赞
				             dataType: "json",
				             success: function(data){
				             	if(data.flag){
									$("#zan_ck em").html(data.zanbai);
									$("#zan_ck span b").css("width",data.zanbai);
									$("#ruo_ck em").html(data.caibai);
									$("#ruo_ck span b").css("width",data.caibai);
									$(".col-l .p1 span em").html(data.fenshu);
									$("#scoreImg").attr("src","http://www.kuhuan.net/statics/images/pc-start/"+Math.floor(data.fenshu)+".png");
									//投票成功
									alert(data.content);
				             	}else{
				             		//投票失败
				             		alert(data.content);
				             	}
				             }
						})
					})

				})
				
				$(function() {
					var wrong = '<a href="javascript:void(0);" class="modal_close"></a><h3>报错反馈</h3><div class="con"><p>请描述您所遇到的错误,我们将尽快予以修正,谢谢！</p><form method="post" name="myform" id="myform"><textarea name="content" id="content"></textarea><div class="h"></div><input type="button" name="" onclick="replysub();" class="sub" value="提交反馈" />&nbsp;<input type="reset" name="" id="resetBt" class="reset" value="取消"/></form></div>';
					$('#wrong').html(wrong);
					$('#resetBt').click(function() {
						$('.modal_close').click();
					});
				})
				
				function replysub() {
					if ($("#content").val() == "") {
						{
							alert('请输入反馈内容！');
							return false;
						}
					}
					if ($("#content").val().length > 200) {
						{
							alert('对不起,反馈内容不能大于200字！');
							return false;
						}
					}
					$.ajax({
						cache: false,
						url: "http://m.kuhuan.net/index.php?m=guestbook&c=index&a=register&siteid=1",
						type: "POST",
						data: {url: "http://www.kuhuan.net/downinfo/152.html",fenlei: "下载工具",content: $("#content").val()},
						success: function(data) {
							if (data == "error") {
								alert('参数错误,请稍后再试！');
							} else {
								alert('您的反馈已经提交成功！');
								$("#myform textarea").val("");
								$('.modal_close').click();
							}
						}
					});
}
