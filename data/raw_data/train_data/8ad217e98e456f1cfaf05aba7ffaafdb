<script type="text/javascript">
	$(document).ready(function(){
		var catLoaded = false;
		$(document).mousemove(function(){
			  if (!catLoaded){
                  catLoaded = true;
    			  $("#categorys").load('/header/null',function(){
                        if($("#categorys").find("iframe").length > 0){
                            $("#categorys").html('');
                        }  
                  });

                  
			  }
		});
		function showMore(){
			var showMore=$(".show_more");
			showMore.each(function(i,n)
			{
				var _this=$(n);
				$(n).toggle(function(){
					_this.html() == "更多产品 ▼" ? _this.html("收起产品 ▲") : _this.html("收起门票 ▲");
					if (_this.prev().html() == "") {
						_this.prev().html('<center><img src="/ui/images/loading.gif" align="absmiddle"/> 数据载入中......</center>');
						var url = [_this.prev().attr('poi'),_this.prev().attr('classify'),_this.prev().attr('condition')];
						post = {
							'cat':_this.prev().attr('cat'),
							'poi':_this.prev().attr('poi'),
							'classify':_this.prev().attr('classify'),
							'condition':_this.prev().attr('condition'),
							'routes': _this.prev().attr('routes')
						};
				        $.post("/yii.php?r=prdlist/ajax/RecommendedList/",
		        		post,
				        function(d) {
				        	_this.prev().html(d);
				        })
					}
					_this.prev().removeClass("hide");
				},
				function(){
					_this.html() == "收起产品 ▲" ? _this.html("更多产品 ▼") : _this.html("更多门票 ▼");
					_this.prev().addClass("hide");
				})
			}
		)}
		showMore();
	});
</script>
