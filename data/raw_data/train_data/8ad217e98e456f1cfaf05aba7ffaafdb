<script>
prdListGlobalConfig = {
    url: 'http://www.tuniu.com/',
    poi_id: 'zhoubian/erriyou',
    prd_type: 'whole',
    start_city: 'tj'
}
$(function(){
    $('.prop_list').each(function(i,v){
        if(!$('.prop_item.hide',v).length){
            $(v).next('.showallbtn').hide();
        }
    });
});
