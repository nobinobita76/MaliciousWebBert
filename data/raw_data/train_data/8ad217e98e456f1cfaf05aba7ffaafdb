<script type="text/javascript">
function disClick(route_id) {
	$("#favorite_id_"+route_id).addClass("on");
}
$(function() {
	$(".collcet_del_id").click( function () {
		$(this).parents("li").find(".collcet_id").hide();
	});
	$(".del_collcet_del_id").click( function () {
		$(this).parents("li").find(".delete_collcet_id").hide();
	});
	$(".login_del_id").click( function () {
		$(this).parents("li").find(".login_id").hide();
	});

});
function clickAddScroll(obj,routeId,days) {
    $.ajax({
        url: "/yii.php?r=api/attention/attention",
        dataType: "jsonp",
        jsonp: "callFuc",
        data: {
            days: days,
            routeId: routeId
        },
        success: function (data) {
            switch(data) {
                case 1://成功
                    disClick(routeId);
                    $(obj).parents("li").find(".collcet_id").show();
                    $(obj).parents("li").find(".delete_collcet_id").hide();
                    $("#favorite_id_"+routeId).removeClass("focus_bg bg_box_focus bg_box_focus_normal");
                    $("#favorite_id_"+routeId).addClass("on");
                    break;
                case 2://已收藏
                    disClick(routeId);
                    $(obj).parents("li").find(".collcet_id").hide();
                    $(obj).parents("li").find(".delete_collcet_id").show();
                    break;
                case 3://失败
                    alert('添加关注失败!');
                    break;
                case 4://未登录
                    $(obj).parents("li").find(".login_id").show();
                    break;
            }
        }
    });
}
</script>
<script src="http://img2.tuniucdn.com/f/2014122905/ui-lib/layer/layer.min.js"></script>
<script type="text/javascript">
$(function(){
    var temp = {};
    var request = null;
    var showLayer = function(res, sf,target) {
        var str=[];
        var data = res[sf];
        for(var x in data){
            if(data[x].description){
                
                str.push('<p style="margin:5px 0">活动'+(+x+1)+'：'+data[x].description.replace(/\n/g,'<br>')+"<br>活动时间："+data[x].beginDate+"至"+data[x].endDate)+'</p>';

            }
        }
        if (str && str.length > 0) {
            layer.tips(str.join(''), target, {
                guide: 2,
                time: 0,
                style: ['background-color:#fffce5; color:#444444', '#fffce5'],
                maxWidth: 350
            });
        }
    };

    $('.mytip').on('mouseenter', function(e) {
        var _btn = $(this);
        var i = _btn.attr("productId");
        var j = _btn.attr("productType");
        var sf=_btn.attr("tipId");


        var isBoss3 = function(productId) {
            return productId > 210000000 && productId < 211000000;
        }
        var bookCode = 0,
            isBoss3Status = isBoss3(i);
        if (isBoss3Status) {
            bookCode = 3000;
        }

        if(sf=="sf"){
            var res="轻松首付，即刻出发，灵活分期，还款无忧";
            layer.tips(res,this,{
                guide:2,
                time:0,
                style:['background-color:#fffce5; color:#444444', '#fffce5'],
                maxWidth:350
            })
        }
        if(sf=="bdly"){
            var res="提供本地到出发地的推荐交通服务";
            layer.tips(res,this,{
                guide:2,
                time:0,
                style:['background-color:#fffce5; color:#444444', '#fffce5'],
                maxWidth:350
            });
        }
        if(sf=="kly"){
            var res="提供所在地到出发地的推荐交通服务";
            layer.tips(res,this,{
                guide:2,
                time:0,
                style:['background-color:#fffce5; color:#444444', '#fffce5'],
                maxWidth:350
            });
        }
        else{
            var key = i + '_' + j + '_' + bookCode;
            if (!temp[key]) {
                var request = $.ajax({
                    url: '/yii.php?r=search/search/ajaxGetPromotionDesc',
                    dataType: 'json',
                    type: 'post',
                    data:{
                        'productId':i,
                        'productType':j,
                        'isBoss3':(isBoss3Status ? 1 : 0),
                        'bookCode':bookCode
                    },
                    success: function(res) {
                        showLayer(res, sf, _btn);
                        temp[key]=res;
                    },
                    error: function() {}
                });
            } else {
                // 有则用
                showLayer(temp[key],sf,_btn);
            }
        }
        $(e.target).on("mouseleave",function(){
            if(request != null){
                request.abort();
            }
            layer.closeTips();
        })
    });
    
})
window.superPageData = window.superPageData || {};
</script>
