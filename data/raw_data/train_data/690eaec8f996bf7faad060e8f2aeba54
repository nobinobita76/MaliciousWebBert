window.onresize = function()
{
	setPanelSize(appName);
};

function setPanelSize(appName)
{
	if(self!=top&&appName=="IE")
	{
		try
		{
			var popTable = document.getElementById("popTable");
			popTable.height = popTable.parentNode.offsetHeight;
			var popTable1 = document.getElementById("popTable1");
			popTable1.height = popTable1.parentNode.offsetHeight-10;
			
			var popTable2 = document.getElementById("popTable2");
			popTable2.height = popTable2.parentNode.offsetHeight;
			document.getElementById("rightClientCardDiv").style.height = "100%";
			
			if(autoHideInfo==0)
			{
				var popTable5 = document.getElementById("popTable5");
				popTable5.height = popTable5.parentNode.offsetHeight;
				var popTable3 = document.getElementById("popTable3");
				popTable3.height = popTable3.parentNode.offsetHeight;
				var popTable4 = document.getElementById("popTable4");
				popTable4.height = popTable4.parentNode.offsetHeight-10;
				if(infoShowType==0)
				{
					var infoHeight = document.getElementById("infoTd").parentNode.offsetHeight;
					document.getElementById("infoDiv").style.height = infoHeight+"px";
				}
			}
		}
		catch(e)
		{
		}
		

	}
	var contentHeight = document.getElementById("chatContent").offsetHeight;
	if(contentHeight<10)
	{
		document.getElementById("chatContent").height = 300;
	}


}
