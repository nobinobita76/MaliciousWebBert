/**
*功能：给鼠标移动到s-dropdown上面的时候，记得给他多添加一个hover类样式
*目的是兼容ie6
*@author 李峰
*/
$(function(){
	$(".js-header-menu").hover(function(){
		$(this).addClass("nav-menu-hover");
		ec.base.getCartNum();
		ec.base.getOrderNum();
	},function(){
		$(this).removeClass("nav-menu-hover");
	});
});
</script>
