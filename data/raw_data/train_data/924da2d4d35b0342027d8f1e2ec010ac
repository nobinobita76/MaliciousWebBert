<script type="text/javascript">
function getRandomObj(A) {
	var x = 0,R = [];
	for (var i = 0; i < A.length; i++) {
		R.push(1*A[i].rate || 1);
		x +=R[i];
	}
	var y = Math.ceil(Math.random() * x),//1-10
	z = [],
	m = [];
	for (var i = 1; i < x + 1; i++) {
		z.push(i)//1-10
	}
	for (var i = 0; i < A.length; i++) {
		m[i] = z.slice(0, R[i]);//m[0] = [1,2],m[1] = [1,...,9]
		z.splice(0, R[i])
	}
	for (var i = 0; i < m.length; i++) {
		for (var j = 0; j < m[i].length; j++) {
			if (y == m[i][j]) {
				return A[i]
			}
		}
	}
}
(function() {
	var list = [
    {
    	src:(screen.width>=1200)?"//img30.360buyimg.com/da/jfs/t1633/12/1310374395/45027/9972250f/55c48624N6ad6ed97.jpg":"//img13.360buyimg.com/da/jfs/t1390/131/1369809972/45027/9972250f/55c48629N121bf750.jpg",
	    href:"//c-nfa.jd.com/adclick?&cv=2.0&url=//dbd.jd.com/listing/0-13-2000-0-111-0-0-1-0-0.html?cpdad=1DLSUE",
	    height:(screen.width>=1200)?"150":"150",
	    width:(screen.width>=1200)?"1210":"990",
	    rate:""
	}
	];
	var _cho=getRandomObj(list);
    var _imgSrc= '<a href="'+_cho.href+'" target="_blank"><img width="'+_cho.width+'" height="'+_cho.height+'" src="'+_cho.src+'"/></a>';

   document.getElementById("list-ad").innerHTML = _imgSrc;
})();
</script>
