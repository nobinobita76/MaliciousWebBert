    $(".open_surprise").click(function(){
	    $(".surprise").show();
		$(".bg").show();
	});
	$(".bg").click(function(){
	    $(".surprise").hide();
		$(".bg").hide();
	});
	$(".surprise_bg").click(function(){
	    $(".surprise").hide();
		$(".bg").hide();
	});
});
