  <script>
	
	function collect(id){
		var ck=getCookie("cfcpnKey");
		var orgId="";
		if(ck==null || ck==''){
			alert("未登录，请前往首页登录或注册！");
			window.location.href="/";
			return ;
		}else{
			var json=eval("("+ck+")");
			orgId=json.orgId;
			if(orgId==null || orgId==''){
				alert("未登录，请前往首页登录或注册！");
				window.location.href="/";
				return ;
			}
		}
		$.ajax( {  
		      url:"/front/article/collect",// 跳转到 action  
		      data:{"collId":id,"orgId":orgId},  
			  type:'post',  
			  cache:false,  
			  dataType:'json',  
			  success:function(data) {  
				  var flag=data.M;
				  if(flag=='-1'){
					  layer.alert("请完善您的信息！",{offset: 300});
				  }else if(flag=='2'){
					  layer.alert("您已报名，请不要重复报名！",{offset: 300});
					  $("#regOnline").text("已报名").attr("disabled", true).css({"backgroundColor":"#eee","color":"#000","borderColor":"#ccc"});
				  }else if(flag=='1'){
					  layer.alert("报名成功！",{offset: 300});
					  $("#regOnline").text("已报名").attr("disabled", true).css({"backgroundColor":"#eee","color":"#000","borderColor":"#ccc"});
				  }else if(flag=='-2'){
					  layer.alert("操作错误，请重试！",{offset: 300});
				  }else{
					  layer.alert("系统繁忙，请稍后再试！",{offset: 300});
				  }
			  },  
			  error : function() {  
				  alert("异常！");  
			  }  
		 });
	}
	$(function() {
		var ck=getCookie("cfcpnKey");
		var orgId="";
		if(ck==null || ck==''){
			return;
		}else{
			var json=eval("("+ck+")");
			orgId=json.orgId;
			if(orgId==null || orgId==''){
				return;
			}
		}
		$.ajax( {  
		      url:"/front/article/checkRegStatus",// 跳转到 action  
		      data:{"collId":'8a7b9151568d82d0015697d15a7c0107',"orgId":orgId},  
			  type:'post',  
			  cache:false,  
			  dataType:'json',  
			  success:function(data) {  
				  var status=data.status;
				  if(status=='2'){
					  $("#regOnline").text("已报名").attr("disabled", true).css({"backgroundColor":"#eee","color":"#000","borderColor":"#ccc"});
				  }
			  }
		 });
	});
