                pageId = $('body').attr('id');
        switch (pageId) {
            case 'jtsIndex':
                $('.navlevel .jtsIndex').addClass('nav_hover');
                break;
            case 'collectiveComplaints':
                $('.navlevel .collectiveComplaints').addClass('nav_hover');
                break;
            case 'successCase':
                $('.navlevel .successCase').addClass('nav_hover');
                break;
            case 'complainObject':
                $('.navlevel .complainObject').addClass('nav_hover');
                break;
            case 'redBlackList':
                $('.navlevel .redBlackList').addClass('nav_hover');
                break;
            case 'rankingList':
                $('.navlevel .rankingList').addClass('nav_hover');
                break;
            case 'jstReprot':
                $('.navlevel .jstReprot').addClass('nav_hover');
                break;
            case 'recall':
                $('.navlevel .recall').addClass('nav_hover');
                break;
            case 'law':
                $('.navlevel .law').addClass('nav_hover');
                break;
            case 'expertList':
                $('.navlevel .expertList').addClass('nav_hover');
                break;
        }
    </script>


