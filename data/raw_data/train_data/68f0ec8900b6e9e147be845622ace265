    <script type="text/javascript">
        // $(".example_video_1_html5_api").css({"width":"","height":""});
        $(".vjs-big-play-button").click(function(){
            $(this).hide();
        });
        // 加载更多
        var rcb = $(".reply_c_table");
        $(rcb).each(function(){
            var tr = $(this).find("tr");
                len = tr.length;
            if (len >=10) {
                $(this).next(".reply_page").show();
            } else if(4 >= 10){
                $('.reply_c_table2').next(".reply_page").show();
            }
        });

        var pageNo = 1;
        var loadIng = false; //正在加载
        function nextPage(type,i,el){
            if(!loadIng){
                loadIng = true;
                pageNo = pageNo+1;
                $.ajax({
                    type:"post",
                    dataType:"json",
                    url:"/front/api/includePage/postContentMoreReply.do",
                    data:{"pageNo":pageNo,"id":14268,"pageSize":10,"type":type},
                   success:function(data){
                       var code = data.errorCode;
                       if(code>=0){
                            //成功
                            //渲染页面
                            var content = data.message; // 返回内容
                            var cl = ".reply_c_table" +i;
                            $(cl).find("tbody").append(content);
                            // console.log("success");
                            loadIng = false;
                       } else{
                            loadIng = false;
                            $(el).parent().html("没有更多").css({"background":"none","border":"none"});
                       }
                   }
                });
            }
        }
        $(function(){
            large();
            reply_large();
        });
        function large(){
            var img = $("#J_tscontent img");
            img.each(function(){
                var src = $(this).attr("src");
                if(src.indexOf("21cnimg.com") > 0 && src.indexOf("m500") >0 ){
                    var m = src.indexOf('m500');
                    var _m = src.substring(0,m);
                    var m_ = src.substring(m+4);
                    var o = _m+"o"+m_;
                    if(src.indexOf(".jpg") > 0 || src.indexOf(".png") > 0 || src.indexOf(".jpeg") > 0 || src.indexOf(".gif") > 0 || src.indexOf(".bmp") > 0 ){
                        $(this).wrap("<a></a>");
                        $(this).closest("a").attr({"href":o,"rel":"lightbox-demo"});
                    }
                }
                else{
                    if(src.indexOf("jpg") > 0 || src.indexOf("png") > 0 || src.indexOf("jpeg") > 0 || src.indexOf("gif") > 0 || src.indexOf("bmp") > 0 ){
                        $(this).wrap("<a></a>");
                        $(this).closest("a").attr({"href":src,"rel":"lightbox-demo"});
                    }
                }
            });
        }
        function reply_large(){
            var img = $(".chatu img");
            img.each(function(){
                var src = $(this).attr("src");
                if(src.indexOf("21cnimg.com") > 0 && src.indexOf('m190') >0 ){
                    var m = src.indexOf('m190');
                    var _m = src.substring(0,m);
                    var m_ = src.substring(m+4);
                    var o = _m+"o"+m_;
                    if(src.indexOf(".jpg") > 0 || src.indexOf(".png") > 0 || src.indexOf(".jpeg") > 0 || src.indexOf(".gif") > 0 || src.indexOf(".bmp") > 0 ){
                        $(this).wrap("<a></a>");
                        $(this).closest("a").attr({"href":o,"rel":"lightbox-demo"});
                    }
                }
                else{
                    if(src.indexOf("jpg") > 0 || src.indexOf("png") > 0 || src.indexOf("jpeg") > 0 || src.indexOf("gif") > 0 || src.indexOf("bmp") > 0 ){
                        $(this).wrap("<a></a>");
                        $(this).closest("a").attr({"href":src,"rel":"lightbox-demo"});
                    }
                }
            });
        }

        function browser (){
                var ua=window.navigator.userAgent,
                    ret="";
                if(/Firefox/g.test(ua)){
                ua=ua.split(" ");
                ret="Firefox|"+ua[ua.length-1].split("/")[1];
                }else if(/MSIE/g.test(ua)){
                ua=ua.split(";");
                ret="IE|"+ua[1].split(" ")[2];
                }else if(/Opera/g.test(ua)){
                ua=ua.split(" ");
                ret="Opera|"+ua[ua.length-1].split("/")[1];
                }else if(/Chrome/g.test(ua)){
                ua=ua.split(" ");
                ret="Chrome|"+ua[ua.length-2].split("/")[1];
                }else if(/^apple\s+/i.test(navigator.vendor)){
                ua=ua.split(" ");
                ret="Safair|"+ua[ua.length-2].split("/")[1];
                }else{
                    ret="未知浏览器";
                    }
             return ret.split("|");
        }

        //调用函数，返回一个数组,r[0]是浏览器名称，r[1]是版本号
        $(function(){
        var r=browser();
        var rr =r[0]+r[1];
        if(rr.indexOf("IE7") != -1 || rr.indexOf("IE6") != -1|| rr.indexOf("IE8") != -1){
           // alert("当前是IE7系列浏览器，部分功能可能不支持，为了不影响您使用，请更新到最新版本IE或者选择Chrome浏览器查看。")
           console.log(rr);
           $("#ie6update-bar .bd1").show();
           $("#ie6update-bar").show();

        }
    });
        $(document).ready(function(){
            checkCookie();
        });

    </script>
    <script type="text/javascript" src="http://ts.21cn.com/static/js/picbox.js"></script>
    <script>
        var postId,getButton;

        // 居中
        function center(obj) {
            var screenWidth = $(window).width(), screenHeight = $(window).height();  //当前浏览器窗口的 宽高
            var scrolltop = $(document).scrollTop();//获取当前窗口距离页面顶部高度

            var objLeft = (screenWidth - obj.width())/2 ;
            var objTop = (screenHeight - obj.height())/2 + scrolltop;

            obj.css({left: objLeft + 'px', top: objTop + 'px','display': 'block'});
            //浏览器窗口大小改变时
            $(window).resize(function() {
                screenWidth = $(window).width();
                screenHeight = $(window).height();
                scrolltop = $(document).scrollTop();
                objLeft = (screenWidth - obj.width())/2 ;
                objTop = (screenHeight - obj.height())/2 + scrolltop;

                obj.css({left: objLeft + 'px', top: objTop + 'px'});
            });
            //浏览器有滚动条时的操作、
            $(window).scroll(function() {
                screenWidth = $(window).width();
                screenHeight = $(window).height();
                scrolltop = $(document).scrollTop();
                objLeft = (screenWidth - obj.width())/2 ;
                objTop = (screenHeight - obj.height())/2 + scrolltop;

                obj.css({left: objLeft + 'px', top: objTop + 'px'});
            });
        }

        //媒体信息居中
        function meitiCenter(obj) {
            var screenWidth = $(window).width();
            var scrolltop = $(document).scrollTop()
            var objLeft = (screenWidth - obj.width())/2 ;

            obj.css({left: objLeft + 'px', top: scrolltop + 'px','display': 'block'});
            //浏览器窗口大小改变时
            $(window).resize(function() {
                screenWidth = $(window).width();
                objLeft = (screenWidth - obj.width())/2 ;

                obj.css({left: objLeft + 'px', top: scrolltop + 'px'});
            });
            //浏览器有滚动条时的操作、
            $(window).scroll(function() {
                screenWidth = $(window).width();
                objLeft = (screenWidth - obj.width())/2 ;

                obj.css({left: objLeft + 'px', top: scrolltop + 'px'});
            });
        }

        //创建模态框
        function createModal(content,button){
            $('.modalContent').html(content);
            switch (button) {
                case 1:
                    $('.modalButton').html('<div class="btn cancel close">取消</div><div class="btn confirm" id="confirm">确认获取</div>');
                    break;
                case 2:
                    $('.modalButton').html('<div class="btn confirm close">确认</div>');
                    break;
                default:
            }
        }

        function closeModal() {
            $('.modalButton').children().remove();
            $('.modalContent').children().remove();
            $('.mask').hide();
            $('.modal').hide();
        }

        //获取联系方式
        $(document).on('click', '.btn.get', function() {
            var msg = "",
                href = document.location.href,
                postIdIndex = href.lastIndexOf('/') +1;
            postId = href.substr(postIdIndex, href.length-1);
            getButton = $(this);
            $.ajax({
                url: 'http://ts.21cn.com/front/api/meiti/isContact.do',
                dataType:"json",
                method: 'POST',
                data: {"postId":postId},
                success: function(data) {
                    var number = data.haveTime,
                            errorCode = data.errorCode,
                            type = data.type,
                            modalText = {
                                refuse: '<p>投诉人不接受媒体采访。</p><p>您可在投诉帖下面回复，尝试联系投诉人，但无权直接查看该投诉人联系方式。</p>' +
                                '<p>回复时，请勿要求投诉人公开联系方式，可请其与您直接联系</p>',
                                surplus: '<p>本自然月，您尚可获取<span class="quota"> ' + number +' </span>个投诉帖的投诉人联系方式。 </p><br>' +
                                '<p style="font-size: 12px">为防骗子假冒记者联系投诉人，您已获取其联系方式的投诉人，也将查看到您的个人信息。确认获取后，请进入个人中心的“我的采访”页面进行查看。 <p>',
                                empty: '<p>本自然月，您有权获取投诉帖投诉人联系方式的限额已全部用完。请进入个人中心的“我的采访”页面进行查看。</p>'
                            }
                    if(errorCode == 1) {
                        switch (type) {
                            case -1:
                                msg = modalText.empty;
                                createModal(msg, 2);
                                break;
                            case -2:
                                msg = modalText.refuse;
                                createModal(msg, 2);
                                break;
                            case 1:
                                msg = modalText.surplus;
                                createModal(msg, 1);
                                break;
                        }
                        center($('.modal'));
                        $('.mask').show();
                        $('.modal').show();
                    } else{
                        alert(data.message);
                    }
                }
            })
        })

        $(document).on('click', '.btn.got', function() {
            var msg = '<p>您已成功获取该投诉人联系方式，请进入个人中心的“我的采访”页面进行查看。</p>'
            createModal(msg, 2);
            center($('.modal'));
            $('.mask').show();
            $('.modal').show();
        })

        //确认获取
        $('.modalButton').on('click', '#confirm', function(){
            $.ajax({
                url: 'http://ts.21cn.com/front/api/meiti/getContact.do',
                dataType:"json",
                method: 'POST',
                data: {"postId":postId},
                success: function(data) {
                    if(data.errorCode == 1) {
                        getButton.removeClass('get').addClass('got').text('已获取联系方式')
                        closeModal();
                    } else{
                        alert(data.message);
                    }
                }
            })
        })

        //模态框关闭
        $('.modalButton').on('click', '.close', function(){
            closeModal();
        })

        //微信二维码
        $(document).on('mouseover', '.perWx i',function() {
            $(this).closest('.concatInfoBox').next('#qr').show();
        }).on('mouseout', '.perWx i',function() {
            $(this).closest('.concatInfoBox').next('#qr').hide();
        })

        $('.mayContact').click(function(){
            var href = document.location.href,
                postIdIndex = href.lastIndexOf('/') +1;
            postId = href.substr(postIdIndex, href.length-1);
            $.ajax({
                url: 'http://ts.21cn.com/front/api/meiti/getInterview.do',
                dataType: "json",
                method: 'POST',
                data: {"postId": postId},
                success: function (data) {
                    var sex,cardNum,wordNum, reportTpl="",msg = "",reports = [], wbUrl="";
                    var data = data.interview;
                    if(data && data.length > 0) {
                        for(var i = 0 ; i < data.length; i ++) {
                            if(data[i].sex == 1){
                                sex= '男'
                            } else {
                                sex = '女'
                            }
                            if( data[i].cardNum && data[i].cardNum.length >0 ) {
                                cardNum = data[i].cardNum;
                                wordNum = "";
                            } else {
                                cardNum = "无";
                                wordNum = data[i].wordNum;
                            }

                            if(data[i].report.length > 0){
                                for(var j = 0; j < data[i].report.length; j++) {
                                    reportTpl +=  '<div class="fbItem"><div class="title_box"><label>报道标题：</label><span>' +
                                                data[i].report[j].title + '</span></div><div class="link_box"><label>报道链接：</label><a href="'+
                                                data[i].report[j].url + '">' + data[i].report[j].url + '</a></div></div>'
                                }
                                reports.push(reportTpl);
                            } else {
                                reports.push('该媒体对您的投诉暂无报道');
                            }

                            if(data[i].privateWeibo && data[i].privateWeibo.length > 0) {
                                wbUrl = '<a class="wbUrl" href="' + data[i].privateWeibourl + '"></a>';
                            }else {
                                wbUrl = "";
                            }

                            msg += '<div class="meitiInfo"> <div class="title-panel"> <span>媒体资料</span> </div>' +
                                  '<div class="up"><div class="info-tb-item clearfix"><div class="meitiInfoBox">' +
                                  '<div class="logoBox"><img class="meitiLogo" src="'+ data[i].headImg + '" alt="个人头像">' +
                                  '<div class="auth"> <img src="http://ts.21cn.com/static/img/right.png">&nbsp;&nbsp;已认证'+
                                  '</div></div> <div class="meitiText"> <p>媒体名称：' + data[i].title + '</p>' +
                                    '<p>媒体地址：<a href="'+　data[i].url + '">' + data[i].url + '</a></p>' +
                                    '<p>媒体热线：' + data[i].hotLine + '</p><p>官方昵称：' + data[i].onickname + '</p><p>微博地址：<a href=" ' +
                                    data[i].weibo + '">' + data[i].weibo + '</a></p> <p>公众号ID：' +  data[i].wpublicId + '</p><p>公众号名称：' +
                                    data[i].wpublicName + '</p></div> </div> <div class="perInfoBox"><p>姓名：' +
                                    data[i].name + '</p> <p>性别：' + sex + ' </p> <p>所属栏目：' + data[i].lanmu +'</p><p>职务：' +
                                    data[i].job + '</p><p class="pressNum">记者证编号：' + cardNum + ' </p><p>身份证号：保密</p><p>工作证号：' +
                                    wordNum + '</p> </div><div class="concatInfoBox"><p>手机号码：'+ data[i].phone + '</p><p>电子邮箱：' +
                                    data[i].email + '</p><p class="perWx">个人微信号：<strong>' + data[i].wx + '</strong><i></i></p> <p>QQ号：' +
                                    data[i].qq + '</p> <p class="perWb">个人新浪微博：<strong>' + data[i].privateWeibo + '</strong>' + wbUrl + '</p></div><div id="qr"><img src="' + data[i].wxUrl + '" alt="微信二维码"> <i></i> </div> </div> </div>' +
                                    '<div class="title-panel"><span>报道信息</span></div><div class="fbList">' +
                                    reports[i] + '</div></div>';
                        }
                    }
                    msg += '<i class="closeItem"></i>';
                    $('.innerBox').html(msg);
                    meitiCenter($('.fixBox'));
                    $('.fixBox').show();
                    $('.mask').show();
                }
            })
        })
        $(document).on('click', '.closeItem', function(){
            $('.mask').hide();
            $('.fixBox').hide();
        })
    </script>




