    <script type="text/javascript" src="http://ts.21cn.com/static/js/script.min.js"></script>
	<script src="http://ts.21cn.com/static/js/uploadify/jquery.uploadify-3.1.1.min.js" type="text/javascript"></script>
    <script type="text/javascript">
        var picNum = 1;
        $('#fileupload').uploadify({
	        'swf':'http://ts.21cn.com/static/js/uploadify/uploadify.swf',
	        'uploader':'http://ts.21cn.com/front/api/admin/uploadUserPic.do',
	        'formData':{'fieldName':'Filedata'},
	        'buttonText':'上传图片',
	        'fileTypeDesc':'支持格式:jpg/gif/jpeg/png/bmp.', //如果配置了以下的'fileExt'属性，那么这个属性是必须的
	        'fileTypeExts':'*.jpg;*.gif;*.jpeg;*.png;*.bmp;', //允许的格式
	        'fileSizeLimit':'5MB',
	        'multi':true,
	        'onUploadSuccess':function (file, retData, response) {
	            var data = eval('(' + retData + ')');
	        	 var error = data.error;
	             if (error != undefined) {
	                 alert(error);
	                 return;
	             }
	             var successReturn = data.SUCCESS;
	             var failReturn = data.FAIL;
	             if (failReturn.length > 0) {
	                 alert(failReturn[0].meta);
	                 return;
	             }
	             uploadCallback(successReturn[0]);
	        }
	    });

		//上传之后回调，渲染页面
		function uploadCallback(successData){
			if(picNum>3){
				alert("只能上传3张图片");
				return;
			}
			var url = successData.url;
			var html = "";
			html += '<li>';
			html += '	<img src="'+url+'" width="100" height="100">';
			html += '	<span class="close" onclick="removeFileupload(this)"></span>';
			html += '	<input type="hidden" value="'+url+'">';
			html += '</li>';
			$("#uploadImglist ul").append(html);
			picNum++;
		}
		function removeFileupload(el){
			$(el).parent().remove();
			picNum--;
		}
    </script>
    <script>
        function checkLogin(redictUrl){
            gLoginRefUrl = ((typeof(redictUrl) == "undefined") || (redictUrl == "")) ? window.location.href : redictUrl;
            if(Cookie.get("SSON") && Cookie.get("SSONKEY") != 'null'){
                if(redictUrl != "") {
                    window.location.href = redictUrl;
                }
            }else{
                $('#Dialog').show();
                var x = '<iframe width="560px" height="320px" id="udburl" frameborder="0" src="http://passport.21cn.com/api/loginUrl.do"></iframe>';
                $('#main1 ul li:eq(0)').append(x);
            }
        }
