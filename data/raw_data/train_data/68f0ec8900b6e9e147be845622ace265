    <script language="JavaScript">

    var ua = window.navigator.userAgent;
    if(ua.match(/mobile/i)){
         var url=window.location.href;

         var idAdd = url.indexOf('id');

         if(idAdd != -1){
            var myvalue = url.substring(idAdd+3);
            window.location.href = 'http://ts.21cn.com/wap/show/id/'+myvalue;
         } else{
            window.location.href = 'http://ts.21cn.com/wap/show/id/';
         }
    }
    </script>
