        function searchGo() {
            var kw = document.getElementById('keyword').value;
            document.location.href = 'http://s.iliyu.com/cse/search?s=8354323290589432078&entry=1&q=' + encodeURI(kw) + '';
        }
        function Check(event) {
            if (window.event) {
                event = window.event;
            }
            if (event.keyCode == 13) {
                document.getElementById("lkSearch").focus();
                return;
            }
        }
    </script> 
