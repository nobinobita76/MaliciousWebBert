<script type="text/javascript">
			var nowFlag=0;
			var tabFlag=0;  
			var divFlag=0;
			var tabId="treetgirls";
			var showId="app_list";
			function app_list(){
				var obj = $('#treetgirls');
				var offset = $('#treetgirls').offset();
				var top = offset.top;
				var left = offset.left;
				var height = $('#treetgirls').height();
				var width = $('#treetgirls').width();
				var html = app_list_html();
				if($("#app_list").length==0){
					$("body").append(html);
				}
				$("#app_list").width(width);
				$("#app_list").css("top",top+height);
				$("#app_list").css("left",left);
			}
			function app_list_show(nowId){
				if(nowId==tabId)tabFlag=1;
				else if(nowId==showId)divFlag=1;
				else{}
				$("#"+nowId).show();
				nowFlag=tabFlag+divFlag;
			}
			function app_list_hide(nowId){
				if(nowId==tabId)tabFlag=0;
				else if(nowId==showId)divFlag=0;
				else{}
				if(tabFlag+tabFlag==0)
				{
					$("#"+nowId).hide();
				}
				nowFlag=tabFlag+divFlag;
			}
			function app_list_html(){
				var app_html = '<div id=\"app_list\" style=\"z-index:10;position: absolute;background:#fff;font-size:14px;border:solid;border-width:1px;border-color:#ccccff;padding-top:5px;padding-bottom:5px;text-align:center;display:none;\" onmouseout=app_list_hide(\"app_list\") onmouseover=app_list_show(\"app_list\")>';
				var tutu_html = '<a href=\"http://baobao.ci123.com/tgirls/space/600?v=1\" taget=\"_blank\" style=\"width:100px;height:22px;line-height:22px;margin:0 auto;display:block;\" >图图秀</a>';
				var tree_html = '<a href=\"http://baobao.ci123.com/tree/space/600?v=1 \" taget=\"_blank\" style=\"width:100px;height:22px;line-height:22px;margin:0 auto;display:block\">许愿树</a>';
				app_html = app_html+tree_html+tutu_html+'</div>';
				return app_html;
			}
			$(document).ready(function(){
					app_list();
				//$('#treetgirls').bind("mouseover",app_list);		
			});	
		</script></ul>
