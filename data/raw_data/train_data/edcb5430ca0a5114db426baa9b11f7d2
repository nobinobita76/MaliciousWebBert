var tpli;var tptime=0;var tpstime;var tpstime2;
function delBlogTips(){
	tpli=document.createElement("li"); 
	tpli.innerHTML=document.getElementById("tptips").getElementsByTagName("li")[0].innerHTML;
	document.getElementById("tptips").getElementsByTagName("ul")[0].removeChild(document.getElementById("tptips").getElementsByTagName("li")[0]);
	document.getElementById("tptips").getElementsByTagName("ul")[0].appendChild(tpli);
}

function scrollBlogTips(){
	if(tptime==0){clearInterval(tpstime2);}
	document.getElementById("tptips").getElementsByTagName("ul")[0].style.marginTop=-tptime+'px';
	tptime++;
	if(tptime==29){
		clearInterval(tpstime);
		delBlogTips();
		document.getElementById("tptips").getElementsByTagName("ul")[0].style.marginTop='0px';
		tptime=0;
		tpstime2=setInterval(scroolBlogTipsTime2,5000);
	}
}
function scroolBlogTipsTime2(){tpstime=setInterval(scrollBlogTips,30);}
function scroolBlogTipsTime(){tpstime2=setInterval(scroolBlogTipsTime2,4000);}
scroolBlogTipsTime();
</script>
