<script type="text/javascript" src="http://l.bst.126.net/rsc/js/jquery-1.6.2.min.js"></script>
<script type="text/javascript">
var isOldIE = function(){return $.browser.msie && $.browser.version.substr(0,1)<7;}();
$(document).ready(function () {
    $(".list .box img").each(function(){
        $(".photo .img").each(function(){
            var len = $.trim($(this).find(".desc").val()).length;
            if (len <= 0) {
                $(this).find(".desc").remove();
                $(this).find(".descbg").remove();
                return;
            }
            var lh=$(this).find("img").height();
            if(lh !=0 ){
                $(this).find(".desc").css("line-height",lh+"px");
            } else {
                $(this).find(".desc").hide();
            }
        });
        if(isOldIE){
            $(".list li > a").click(function(){
                location.href = $(this).attr('href');
            });
        }
    });
});
</script>

