<script type="text/javascript">

$(function() {

    var elm = $('.newsTnav');
    var startPos = $(elm).offset().top;
    $.event.add(window, "scroll", function() {
        var p = $(window).scrollTop();
        $(elm)[p>startPos?'addClass':'removeClass']('newsTnavFlxe');
    });
    
	if($('.gdTxt .hot').size()>1){
		$('.gdTxt .hot:first').removeClass('hot');
	}
	
});



$(document.body).ready(function(){
	var curr=0;
	var currShowNum=6;
	var ali=$(".gdTxt");
	var a_len = $(".gdTxt a").length;
	var aWidth= $(".gdTxt a").eq(1).width()+23;
	if($('.gdTxt a').size() < 6){
			$('.toRight').hide();
			$('.toLeft').hide();
		}
	
	if($('.gdTxt .hot').attr('data-index')>4){
		$('.gdTxt').css({left:-528});
		curr++;
	}
	
	$('.toRight').click(function(){
		if(curr==a_len-currShowNum){
			return;
		}
		curr++;
		ali.stop().animate({left:curr*aWidth*-6},300);
		
	});
	$('.toLeft').click(function(){
		if(curr<1){
				return;
			}
		curr--;
		ali.stop().animate({left:curr*aWidth*-6},300);
		
	});
});


</script>



