    <script>
        $('.img-lazyload').lazyload({
            threshold: 300
        });
        $(window).on('load', function(){
            setTimeout(function(){
                $(window).trigger('scroll');
            }, 1)
        });
        
        function scroll(){
            var wTop = $(window).scrollTop() + ( ( $(window).height() / 3 ) * 2.5 );
            var $img = $('.target'),
               len  = $img.length;
            for(var i=0;i<len;i++){
               var $current = $img.eq(i),
                   top      = $current.offset().top;
                if(wTop > top){
                    $current.addClass('move');
                }
            }
        }
        $(window).scroll(scroll);
    </script>
