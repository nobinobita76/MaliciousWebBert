<script type="text/javascript">
    function LoadMoreMessage(newsId,lastId,calltype) {

        $.ajax({
            type: 'get',
            url: 'http://app.api.autohome.com.cn/autov5.1.0/News/GetFastMessageList.ashx',
            dataType: 'jsonp',            
            jsonp: "jsonpcallback",
            jsonpCallback: "FillMessageData",
            data: { newsid: newsId, lastid: lastId, model:calltype},
            success: function (msg) {              
            },
            error: function (msg) {               
                $("#amoremessage").html("æŸ¥çœ‹æ›´å¤š<i class=\"iconfont\">&#xf02a9;</i>");
            }
        });
    }

    function FillMessageData(data) {
        if (data != null) {
            var item = eval(data);

            if (item.result.isloadmore) {
                //å°†æœ€åä¸€æ¡çš„lastidèµ‹å€¼åˆ°æ§ä»¶ä¸Š                                         
                $("#amoremessage").attr("lastid", item.result.lastpageid);
                $("#amoremessage").html("æŸ¥çœ‹æ›´å¤š<i class=\"iconfont\">&#xf02a9;</i>");
                 

            } else {
                $("#amoremessage").hide();
            }
            $("li:last").after(item.result.messagehtml);

            var ispc = window.innerWidth > 1152;                                                         
            //pcæ ·å¼æ›¿æ¢
            if (ispc) {
                $(".picbox").removeClass("picbox").addClass("picboxpc");

            } else {
                dealArtileLink(5);
                //å¤„ç†æ–°æ·»åŠ çš„å›¾ç‰‡
                dealImg(window.innerWidth, item.result.imgs);    
            }
            dealVideoImg(window.innerWidth, ispc);   
            lazyloadimg();//è§¦å‘å›¾ç‰‡æ‡’åŠ è½½
            LoadSelfVideo(browser,ispc,window.innerWidth);//å¤„ç†å°è§†é¢‘
            //var newshtml = $('#0')[0].outerHTML;
            //$('#0').remove();
            ////å½“æœ€åä¸€é¡µæ—¶æ˜¾ç¤º
            //if(item.result.isloadmore==false)
            //    $("li:last").after(newshtml);
        }
    }
 
</script>
<!--pcç«¯ä¸æ‰‹æœºç«¯å¤„ç†é€»è¾‘-->
<script type="text/javascript">
    (function(win, browser) {
        if (win.screen === undefined || win.screen.width < 810) {
            if (browser.versions.iPad  || browser.versions.webKit  || browser.versions.mobile  || browser.versions.ios  || browser.versions.iPhone  || browser.versions.ucweb  || browser.versions.ucSpecial ) {
                //ç§»åŠ¨ç«¯è¯·æ±‚           
                //å¤„ç†å›¾ç‰‡
                dealImg(window.innerWidth, '1080');
                //å¤„ç†androidå’ŒiOSçš„ä¸‹è½½é“¾æ¥
                var ischeckbrower = (browser.versions.safari && browser.versions.ios) || browser.versions.chrome;
                dealDownLoad(ischeckbrower, browser.versions.weixinweibo,false, false, 1, 5, 465, '', 0, '');
                //å¤„ç†æ–‡ç« è·³è½¬é“¾æ¥,å°†pcç«¯åœ°å€è½¬æ¢ä¸ºmç«¯åœ°å€
                dealArtileLink(5);
                dealVideoImg(window.innerWidth, false);
            } else {
                //éç§»åŠ¨ç«¯è¯·æ±‚
                $(".picbox").removeClass("picbox").addClass("picboxpc");
                dealDownLoad(false, browser.versions.weixinweibo, true, false, 1, 5, 465, '', 0, '');
                dealVideoImg(window.innerWidth, true);                   

            }
        } else {
            //å½“å±å¹•å®½åº¦<=1152
            var ispc = win.screen.width > 1152;
            if (ispc) {
                $(".picbox").removeClass("picbox").addClass("picboxpc");		                    
                    
            } else {		
                        
                //å¤„ç†æ–‡ç« è·³è½¬é“¾æ¥,å°†pcç«¯åœ°å€è½¬æ¢ä¸ºmç«¯åœ°å€
                dealArtileLink(5);                            
            }
                
            dealDownLoad(false, browser.versions.weixinweibo, ispc, false, 1, 5, 465, '', 0, '');                 
            dealVideoImg(window.innerWidth, ispc);             
        }
        LoadSelfVideo(browser,ispc,win.screen.width);
    })(this, browser);
</script>
<script>
    !function ($) {
        var similarWrap = $('.similar-wrap');
        $(document).on('click', '#toggle-more', function () {
            similarWrap.toggleClass('all');
            if ($(this).hasClass('all')) {
                $(this).removeClass('all').html('æ›´å¤šç›¸å…³è½¦ç³» <i class="iconfont">&#xf02a9;</i>');
                window.scrollTo(0, $('.similar-wrap').offset().top)
            } else {
                $(this).addClass('all').html('æ”¶èµ·æ›´å¤šç›¸å…³è½¦ç³» <i class="iconfont">&#xf02aa;</i>');
            }
        });
    }(jQuery);
</script>


