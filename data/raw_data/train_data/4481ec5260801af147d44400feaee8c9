<script type="text/javascript">
    //图片懒加载
    function lazyloadimg() {        
        var murl = "http://x.autoimg.cn/app/image/tmbg.png";
        $("img").lazyload({
            placeholder: murl,
            //effect      : "fadeIn",
            threshold: 200
        });
    }

    //格式化字符串
    String.prototype.format = function (args) {
        var result = this;
        if (arguments.length > 0) {
            if (arguments.length == 1 && typeof (args) == "object") {
                for (var key in args) {
                    if (args[key] != undefined) {
                        var reg = new RegExp("({" + key + "})", "g");
                        result = result.replace(reg, args[key]);
                    }
                }
            }
            else {
                for (var i = 0; i < arguments.length; i++) {
                    if (arguments[i] != undefined) {
                        var reg = new RegExp("({[" + i + "]})", "g");
                        result = result.replace(reg, arguments[i]);
                    }
                }
            }
        }
        return result;
    };
    
    function launchFullScreen(element) {
        if(element.requestFullScreen) {
            element.requestFullScreen();
        } else if(element.mozRequestFullScreen) {
            element.mozRequestFullScreen();
        } else if(element.webkitRequestFullScreen) {
            element.webkitRequestFullScreen();
        }
    }

    function LoadSelfVideo(browser,ispc,screenWidth) {
       
        //自家视频处理
        (function () {                        
            var videos = $("div[name='clubvideo']");
            if (videos.length > 0) {
                var maxwidth = $(".main-wrap").width()-20;//294;
                var maxheight = $(".main-wrap").width()-20; //220;
                if (!ispc && !browser.versions.ios) {
                    videos.each(function(i) {
                        var v = jQuery(this), pic = v.attr("v_pic"), pk = v.attr("v_pk"), src = v.attr("v_src"), videotype = "video/mp4", v_width = v.attr("v_w"), v_height = v.attr("v_h"), v_isload = v.attr("isload");
                        //将没有加载过的视频填充元素
                        if (v_isload == 0) {

                            v.attr("isload", 1); //将视频置为加载状态
                            if (pic == "")
                                pic = "http://x.autoimg.cn/club/v1Content/player/v1/background.png";
                            //screenWidth = 320;
                            //android 使用视频宽高设置封面最大宽高值                    
                            //maxwidth = v_width > screenWidth ? screenWidth - 20 : v_width;
                            maxheight = maxwidth / v_width * v_height;

                            v.css({ 'width': maxwidth + 'px', 'margin': '15px auto' }); //'height': maxheight + 'px',
                            src = src.replace(/(?:\.wm)?\.mp4/ig, ".mp4");
                            var s = "<div style=\"width:" + maxwidth + "px;height:" + maxheight + "px;margin:0 auto;margin-bottom:20px;position: relative;background:url(" + pic + ");background-size:100% auto;\"><video id =\"video_" + pk + "\" width=\"" + maxwidth + "\" height=\"" + maxheight + "\" src=\"" + src + "\" type=\"" + videotype + "\" controls=\"controls\" poster=\"" + pic + "\" style=\"background-color: #000;\" ,preload:\"none\">    </video></div>";
                            v.append(s);

                            //当设备为mi2时给出提示
                            if (navigator.userAgent.indexOf('MI 2') > -1) {
                                v.append('<p class=\"tip\" style=\"font-size: 10px;color: red !important;text-align: center;\">提示:如不能播放，请点击全屏观看</p>');
                            }
                                //当设备是红米给出提示
                            else if (navigator.userAgent.indexOf('Xiaomi-2013022__weibo') > -1) {
                                v.append('<p class=\"tip\" style=\"font-size: 10px;color: red !important;text-align: center;\">提示:为了视频流畅播放，建议在小屏模式下观看</p>');
                            }
                        }
                    }
                    );                        
                    
                } else {
                    $.getScript("http://x.autoimg.cn/club/v1Content/player/v1/mediaelement-and-player.min.js?11", function() {

                        //ios直接调用video签，android使用A标签实现                                    
                        var css = $("<link>", { "rel": "stylesheet", "type": "text/css", "href": "http://x.autoimg.cn/club/v1Content/player/v1/mediaelementplayer.css?10" })[0];
                        document.getElementsByTagName("head")[0].appendChild(css);
                        //小视频灾备开关
                        var videoButton = false;
                        
                        videos.each(function(i) {
                            var v = jQuery(this), pic = v.attr("v_pic"), pk = v.attr("v_pk"), src = v.attr("v_src"), videotype = "video/mp4", v_width = v.attr("v_w"), v_height = v.attr("v_h"), v_isload = v.attr("isload");
                            //将没有加载过的视频填充元素
                            if (v_isload == 0) {
                                v.attr("isload", 1);//将视频置为加载状态
                                
                                if (ispc) {
                                    if (src.indexOf('.wm.m3u8') > -1)
                                        src = src.replace(".wm.m3u8", ".wm.mp4");

                                } else {
                                    if (browser.versions.ios) {
                                        src = src.replace(/(?:\.wm)?\.mp4/ig, ".wm.m3u8");
                                        videotype = "video/m3u8";
                                    } else {
                                        src = src.replace(/(?:\.wm)?\.mp4/ig, ".mp4");
                                        videotype = "video/mp4";
                                    }
                                }
                                if (videoButton && src != '')
                                    src = src.replace('http://v.autoimg.cn/', 'http://v9.autoimg.cn/');
                                if (pic == "")
                                    pic = "http://x.autoimg.cn/club/v1Content/player/v1/background.png";

                                //pc端直接显示480X480,iOS根据屏幕宽度适配
                                //if (!ispc) {
                                //    maxwidth = v_width > screenWidth ? screenWidth - 20 : v_width;
                                //    maxheight = maxwidth / v_width * v_height;
                                //}

                                v.css({ 'width': maxwidth + 'px', 'height': maxheight + 'px', 'margin': '15px auto' });
                                var s = jQuery("<video />", { width: maxwidth, height: maxheight, id: pk, src: src, poster: pic, type: videotype, controls: "controls", style: "background-color:#000000;width:" + maxwidth + "px;height:" + maxheight + "px", preload: "none" });
                                v.append(s);
                                s.mediaelementplayer({
                                    defaultVideoWidth: maxwidth,
                                    defaultVideoHeight: maxheight,
                                    videoWidth: maxwidth,
                                    videoHeight: maxheight,
                                    alwaysShowControls: false,
                                    iPadUseNativeControls: true,
                                    iPhoneUseNativeControls: true,
                                    AndroidUseNativeControls: true,
                                    success: function(player, node) {
                                        //播放
                                        player.addEventListener('play', function(e) {
                                            jQuery(this).data("mediaelementplayer").enterFullScreen();
                                        }, false);
                                        //异常
                                        player.addEventListener('error', function(e) {
                                            if (e.target && e.target.error.code == 4 && !$(this).data('ErrHandled')) {
                                                e.target.setSrc(e.target.src.replace(/(?:\.wm)?(\.(?:mp4|m3u8))/ig, '$1'));
                                                e.target.play();
                                                $(this).data('ErrHandled', '1');
                                            }
                                        }, false);
                                        //播放完毕
                                        player.addEventListener('ended', function(e) {
                                            $("#mejs-overlay mejs-layer").css("display", "none");
                                        }, false);
                                        //连接意外断开
                                        player.addEventListener('waiting', function(e) {
                                            //jQuery(this).data("mediaelementplayer").pauseVideo();                                                                
                                            $("#mejs-overlay mejs-layer").css("display", "none");
                                        }, false);

                                    }
                                });
                            }
                        });
                    });
                }
            }
        })();              
    }
</script>
<script type="text/javascript">
    $(document).ready(function () {
        //更新页面标题
        document.title = $('h1').text();
        
        lazyloadimg();
        //关闭下面下载图层
        $("#close-app").click(function() {            
            $(".open-app-foot").hide();
        });
        
        //刷新        
        $(".article-head-icon").click(function() {            
            location.reload();
        });
        //文章区域进入文章分享页
        $("#article-content").click(function() {
            location.href = "http://cont.app.autohome.com.cn/autov5.1.0/content/news/newssharedcontent-n"+$("#article-content").attr("value")+".html";
        });
        
        //当有加载更多数据时，将最后一条简介隐藏
        //if ($(".btn-full").attr("lastid") != undefined) {
        //    $('#0').hide();
        //}
        //加载更多逻辑
        $("#amoremessage").click(function() {
            var newsid = $("#amoremessage").attr("newsid");
            var lastid = $("#amoremessage").attr("lastid");

            $("#amoremessage").html("查询中...");
            LoadMoreMessage(newsid, lastid, "jsonp");
        });
        //车系综述页
        $("div[id^='seriessummary_']").click(function(){
            var ssid = this.id.replace("seriessummary_","");
            var pvid = 105489;
            if(parseInt(0)!=1){
                pvid = 105490
            }
            window.location.href = "http://m.autohome.com.cn/" + ssid + "/#pvareaid=" + pvid;
        });
    });

</script>
<script type="text/javascript"> document.title = '';</script>
<script type="text/javascript">
    function LoadMoreMessage(newsId,lastId,calltype) {

        $.ajax({
            type: 'get',
            url: 'http://app.api.autohome.com.cn/autov5.1.0/News/GetFastMessageList.ashx',
            dataType: 'jsonp',            
            jsonp: "jsonpcallback",
            jsonpCallback: "FillMessageData",
            data: { newsid: newsId, lastid: lastId, model:calltype},
            success: function (msg) {              
            },
            error: function (msg) {               
                $("#amoremessage").html("查看更多<i class=\"iconfont\">&#xf02a9;</i>");
            }
        });
    }

    function FillMessageData(data) {
        if (data != null) {
            var item = eval(data);

            if (item.result.isloadmore) {
                //将最后一条的lastid赋值到控件上                                         
                $("#amoremessage").attr("lastid", item.result.lastpageid);
                $("#amoremessage").html("查看更多<i class=\"iconfont\">&#xf02a9;</i>");
                 

            } else {
                $("#amoremessage").hide();
            }
            $("li:last").after(item.result.messagehtml);

            var ispc = window.innerWidth > 1152;                                                         
            //pc样式替换
            if (ispc) {
                $(".picbox").removeClass("picbox").addClass("picboxpc");

            } else {
                dealArtileLink(5);
                //处理新添加的图片
                dealImg(window.innerWidth, item.result.imgs);    
            }
            dealVideoImg(window.innerWidth, ispc);   
            lazyloadimg();//触发图片懒加载
            LoadSelfVideo(browser,ispc,window.innerWidth);//处理小视频
            //var newshtml = $('#0')[0].outerHTML;
            //$('#0').remove();
            ////当最后一页时显示
            //if(item.result.isloadmore==false)
            //    $("li:last").after(newshtml);
        }
    }
 
</script>
<!--pc端与手机端处理逻辑-->
<script type="text/javascript">
    (function(win, browser) {
        if (win.screen === undefined || win.screen.width < 810) {
            if (browser.versions.iPad  || browser.versions.webKit  || browser.versions.mobile  || browser.versions.ios  || browser.versions.iPhone  || browser.versions.ucweb  || browser.versions.ucSpecial ) {
                //移动端请求           
                //处理图片
                dealImg(window.innerWidth, '1080');
                //处理android和iOS的下载链接
                var ischeckbrower = (browser.versions.safari && browser.versions.ios) || browser.versions.chrome;
                dealDownLoad(ischeckbrower, browser.versions.weixinweibo,false, false, 1, 5, 465, '', 0, '');
                //处理文章跳转链接,将pc端地址转换为m端地址
                dealArtileLink(5);
                dealVideoImg(window.innerWidth, false);
            } else {
                //非移动端请求
                $(".picbox").removeClass("picbox").addClass("picboxpc");
                dealDownLoad(false, browser.versions.weixinweibo, true, false, 1, 5, 465, '', 0, '');
                dealVideoImg(window.innerWidth, true);                   

            }
        } else {
            //当屏幕宽度<=1152
            var ispc = win.screen.width > 1152;
            if (ispc) {
                $(".picbox").removeClass("picbox").addClass("picboxpc");		                    
                    
            } else {		
                        
                //处理文章跳转链接,将pc端地址转换为m端地址
                dealArtileLink(5);                            
            }
                
            dealDownLoad(false, browser.versions.weixinweibo, ispc, false, 1, 5, 465, '', 0, '');                 
            dealVideoImg(window.innerWidth, ispc);             
        }
        LoadSelfVideo(browser,ispc,win.screen.width);
    })(this, browser);
</script>
<script>
    !function ($) {
        var similarWrap = $('.similar-wrap');
        $(document).on('click', '#toggle-more', function () {
            similarWrap.toggleClass('all');
            if ($(this).hasClass('all')) {
                $(this).removeClass('all').html('更多相关车系 <i class="iconfont">&#xf02a9;</i>');
                window.scrollTo(0, $('.similar-wrap').offset().top)
            } else {
                $(this).addClass('all').html('收起更多相关车系 <i class="iconfont">&#xf02aa;</i>');
            }
        });
    }(jQuery);
</script>


