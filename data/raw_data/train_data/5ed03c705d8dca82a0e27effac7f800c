//        void function () {
//            var url = window.location.href
//            if (! ~url.indexOf('#body')) {
//                window.location.href = url + '#body'
//            }
//        } ();

        function playNext() {
            var url = document.getElementById('nextUrl').value
            if (! ~url.indexOf('#body')) {
                url += '#body'
            }
            window.location.href = url
        }
        // 专辑分页加载
        function GetPageList() {
            var curpage = $('#hidcurPage').val();
            var appid = "25264";
            var temp = PGCIndex.GetCoumnPageList(appid, curpage, 3).value;
            if (temp != "") {
                var ret = $("#pageList").html();
                ret = ret + temp;
                $('#pageList').html(ret);
                jQuery(".slideBrandBox").slide({ titCell: ".hd ul", mainCell: ".bd ul", autoPage: true, effect: "leftLoop", scroll: 1, vis: 4, delayTime: 1000 });
            }
            curpage = (Number(curpage) + 1).toString();
            $('#hidcurPage').val(curpage);
        }
    </script>
    <script>

        $(".my-space-close").click(function () {
            $(this).parent().hide();
            $(".overlay").hide();
        });
        $(".my-detail-mobile").click(function () {
            $(".my-space-mobiledet").show();
        });
        //初始化选中样式
        $(".item-current").next(".masks").css({ "bottom": "-2px", "right": "-2px" });
        $(".item-current").siblings(".mask").html("播放中").css({ "bottom": "-2px", "right": "-3px" });
        $(".unlogin-input").focus(function () {
            if ($(this).val() == this.defaultValue) {
                $(this).val(""); ;
            }
        }).blur(function () {
            if ($(this).val() == "") {
                $(this).val(this.defaultValue);
            }
        });
        $(".unlogin-password").focus(function () {
            if ($(this).val() == this.defaultValue) {
                $(this).next(".password-tips").hide();
            }
        }).blur(function () {
            if ($(this).val() == "") {
                $(this).next(".password-tips").show();
            }
        });

    </script>
