        function getCookies(name) {
            var arr = document.cookie.match(new RegExp("(^| )" + name + "=([^;]*)(;|$)"));
            if (arr != null)
                return unescape(arr[2]);
            return null;
        }

            //点赞
            function dianzan()
            {
                pvstatClick('dianzan','0|20|1|0|210|0');
                $.ajax({
                    url: 'http://www.baomihua.com/interfaces/GetPrColInfo.ashx',
                    data: { videoid: '36032775',
                                channelid: '25264',
                                userid: userid,
                                datatype:'Praise'
                            },
                dataType: 'jsonp',
                jsonp: 'jsoncallback',
                success: function (data) {
                    var ret=data[0].ret  ;
                    if(ret==1)
                    {
                        var zan=$('#j_my-suNum').html();
                        zan=(Number(zan)+1).toString();
                        $('#j_my-suNum').html(zan);//赞数量加1

                        $('#my-support').addClass("btn-mySupport");//改成已点赞图标
                        alert("点赞成功！");
                        return;
                    }
                    else if(ret==-2)
                    {
                        $('#my-support').addClass("btn-mySupport");//改成已点赞图标
                        alert("您今天已经赞过了哦，请明天再来！");
                        return;
                    }
                    else
                    {
                        alert("点赞失败！");
                        return;
                    }
   
                },
                error: function (err) {
                    alert("error:" + err);
                }
            });
                    }
                    //收藏
                    function shoucang()
                    {
                        pvstatClick('shoucang','0|21|1|0|210|0');
//                        var userid="0";
            if(userid=="" ||userid=="0")
            {
                _B.FRAME.popLogin();
            }
            else{
                $.ajax({
                    url: 'http://www.baomihua.com/interfaces/GetPrColInfo.ashx',
                    data: { videoid: '36032775',
                                channelid: '25264',
                                userid: userid,
                                datatype:'Collection'
                            },
                dataType: 'jsonp',
                jsonp: 'jsoncallback',
                success: function (data) {
                    var ret=data[0].ret  ;
                    if(ret==1)
                    {
                        $('#my-sc').addClass("btn-myCol");
                        alert("收藏成功！");
                        return;
                    }
                    else if(ret==0)
                    {
                        $('#my-sc').addClass("btn-myCol");
                        alert("已收藏过！");
                        return;
                    }
                    else
                    {
                        alert("收藏失败！");
                        return;
                    }
   
                },
                error: function (err) {
                    alert("error:" + err);
                }
            });
                    }
                }

    </script>
