    function IsShouCang()
    {  
//    alert('111');
       //获取是否已经收藏
        $.ajax({
            url: 'http://www.baomihua.com/interfaces/GetPrColInfo.ashx',
            data: { videoid: '36032775',
                channelid: '25264',
                userid: userid,
                datatype: 'GetCollect'
            },
            dataType: 'jsonp',
            jsonp: 'jsoncallback',
            success: function (data) {
                var ret = data[0].ret;
                if (ret == 1) {
                    $('#my-sc').addClass("btn-myCol");
                }
//                else{
//                    alert('未收藏'+ret);
//                }
            },
            error: function (err) {
                alert("error:" + err);
            }
        });
    } 
    
        //获取是否已经点赞
        function IsDianzan() {
//    alert('222');
            var videoid = "36032775";
            var test = getCookies(userid);
            if (test != null && test != undefined) {
                var i = test.indexOf(videoid);
                if (i != -1) {
                    $('#my-support').addClass("btn-mySupport");
                }
//                else{
//                    alert('未点赞'+test);
//                }
            }
        }
    function getDZCount()
    {
        //获取点赞数
        $.ajax({
            url: 'http://www.baomihua.com/interfaces/GetPrColInfo.ashx?datatype=GetCollNum',
            data: 'videoid='+36032775+'&isshowtruepv='+1,
        dataType: 'jsonp',
        jsonp: 'jsoncallback',
        success: function (data) {
            if (typeof data !== 'undefined') {
                var zan = data[0].zancount;
                var collec = data[0].CollectionCount;
                $('#j_my-suNum').html(zan);
            }
        }
        });
      }   

