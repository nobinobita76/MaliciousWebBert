<script>
    $("#content-show img[style!='']").attr("style","");
    $(window).bind("scroll",function() {
            var d = $(document).scrollTop(),
            e = $(window).height();
            a=$(document).height();
            if(a<(e+d+300)){
                $('.section-floatbar-wap').fadeOut();
            }else{
                $('.section-floatbar-wap').fadeIn();
                }
                
       });
