        //全局变量，动态的文章ID
       /* var ShareId = "";
        var ShareTitle = "";
        //绑定所有分享按钮所在A标签的鼠标移入事件，从而获取动态ID
        $(function () {
            $(".bdsharebuttonbox").on('mouseover','a',function () {
                ShareId = $(this).attr("data-id");
                ShareTitle = $(this).attr("data-title");
            });
        });

