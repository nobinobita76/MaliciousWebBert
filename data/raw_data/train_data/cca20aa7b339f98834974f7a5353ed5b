<script>
	//avalon model
  	var model = avalon.define({
      	$id:"player",
      	songArr : []//搜索歌曲数据数组
  	});
  	$(".search input").on("keyup focus",function(){
      	var searchStr = $(this).val();
      	if(searchStr){
          	var songUrl = "http://search.kuwo.cn/r.s?SONGNAME="+searchStr+"&ft=music&rformat=json&encoding=utf8&rn=8&callback=song&vipver=MUSIC_8.0.3.1";
          	$.getScript(songUrl);
          	$(".searchInfo").show();
      	}else{
          	$(".searchInfo").hide();
      	}
  	});
  	// 请求数据回调
  	function song(jsondata){
      	model.songArr = [];
      	var searchData = jsondata;
      	for(var i=0;i<searchData.abslist.length;i++){
          	searchData.abslist[i].id = searchData.abslist[i].MUSICRID.replace('MUSIC_','');
          	model.songArr.push(searchData.abslist[i]);
      	}
      	if(model.songArr.length==0){
          	$(".searchSong").hide();
      	}else{
          	$(".searchSong").show();
      	}
  	}
  	// jsonError
  	function jsonError(){

  	}
  	
  	$(document).live('keydown',function(ev){
		var ev = ev||event;
		var a = $(".search input").val();   
		if(a==""){
			return;
		}
		if(ev && ev.keyCode==13){ 
			window.open("http://sou.kuwo.cn/ws/NSearch?type=all&catalog=yueku20177&key="+a);
  		}
	});
</script>

<script>
	//显示、隐藏更多服务
	jQuery('.more').hover(function(){
	
		jQuery('.more ul').show();
		
		
	},function(){
		
		jQuery('.more ul').hide();
		
	});
	
	jQuery('.more ul li').live('click',function(){
		jQuery('.more ul').hide();
	});
	
	jQuery(function(){  
 	
	  	//搜索歌曲
	  	jQuery(document).live('click',function(ev){
	  		
			$(".searchInfo").hide();
		});
		
		jQuery(".searchBtn").live('click',function(){
			var a = jQuery(".search input").val();
			if(a==""){
				return;
			}
			window.open("http://sou.kuwo.cn/ws/NSearch?type=all&catalog=yueku2016&key="+a);
		});
		
		jQuery('#search_txt').live('click',function(ev){
			
			return false;
			
		});
	  	
	});
	
	//图片延迟加载
	function loadPic(obj){
	    var lazyPic = jQuery(obj).attr("lazy_src");
	    obj.onload = null;
	    if(lazyPic!=null&&lazyPic!=""){
	   	  jQuery(obj).attr("src",lazyPic);
	    }else{
	      jQuery(obj).attr("src","http://image.kuwo.cn/www/default/180-180-person.jpg");
	    }
	}
	
	function loadSmallPic(obj){
	    var lazyPic = jQuery(obj).attr("lazy_src");
	    var width = jQuery(obj).attr("width");
	    var newPic = "";
	    if(lazyPic!=undefined && lazyPic.indexOf(".jpg")>0){
	        var front = lazyPic.substring(0,lazyPic.length-4);
	        if(width=='160'){
	           newPic = front+"sp.jpg";
	        }else if(width=='300'){
	           newPic = front+"bp.jpg";
	        }
	    }
	    obj.onload = null;
	    if(newPic!=""){
	        jQuery(obj).attr("src",newPic);
	    }
	}
	
	//图片加载失败
	function getDefaultPic(obj,width,height,type){
		obj.onerror=null;
		var picUrl="";
		var prefix="http://image.kuwo.cn/www/default/";
		var suffix=".jpg";
		var name="";
		//55*55
		if(width==30&&height==30){
			name="30-30-";
		}else if(width==55&&height==55){
			name="55-55-";
		}else if(width==70&&height==70){
			name="70-70-";
		}else if(width==90&&height==90){
			name="90-90-";
		}else if(width==120&&height==80){
			name="120-80-";
		}else if(width==120&&height==90){
			name="120-90-";
		}else if(width==120&&height==120){
			name="120-120-";
		}else if(width==180&&height==180){
			name="180-180-";
		}else if(width==240&&height==240){
			name="240-240-";
		}else if(width==140&&height==79){
			name="140-79-";
		}
		switch(type){
			case 'person':name=name+"person";break;
			case 'album':name=name+"album";break;
			case 'mv':name=name+"mv";break;
			case 'user':name=name+"person";break;
			case 'pl':name=name+"album";break;
			case 'category':name=name+"album";break;
		}
		picUrl=prefix+name+suffix;
		jQuery(obj).attr("src",picUrl);
	}
	
	function picLog(clickFlag){
		window.setTimeout(function(){
			var playclick = new Image();
			playclick.src="http://webstat.kuwo.cn/logtjsj/commsj/commjstj/www2016/" + clickFlag + ".jpg";
		},300);
	}
</script>



