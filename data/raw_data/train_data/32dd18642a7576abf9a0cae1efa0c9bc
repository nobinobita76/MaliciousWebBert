 type: 'GET', 
 url: apiUrl + '/game-number.html?t=' + new Date().getTime(), 
 data: { 
 id: ueGame.gameId 
 }, 
 dataType: 'jsonp', 
 jsonp: '_callback_', 
 success: function (result) { 
 if (result.code == 100) { 
 var _play = cNum(+result.result.play); 
 var _favorite = cNum(+result.result.favorite); 
 var _candy = +result.result.candy; 
 
 CandyNum = $("#j-candynum"); 
 CandyNum.html(cNum(+_candy)); 
 $("#j-play").html(_play); 
 $("#j-game-favorite em").attr("data-favorite", +result.result.favorite); 
 $("#j-game-favorite em").html(_favorite); 
 if (result.result.share > 0) { 
 if (result.result.share > 9999) { 
 result.result.share = parseFloat(result.result.share / 10000).toFixed(2)+'万' 
 } 
 $('#j-share .num').text( result.result.share); 
 } 
 } 
 } 
 }); 
