	<script>
      var escapeReg = function(source) {return String(source).replace(new RegExp("([.*+?^=!:\x24{}()|[\\]\/\\\\])", "g"), '\\\x241');};
      var getQueryValue = function(key) { var reg = new RegExp("(^|&|\\?|#)" + escapeReg(key) + "=([^&]*)(&|\x24)", "");var match = window.location.search.match(reg);if (match) {return match[2]; }return null;};
      var isJustBuy = 'false';//是否刚购买过
      var orderAct = false;//是否赠送
      var channel = getQueryValue('channel') || '';
      //评论评分
      Do("rankStar", function() {loginRootUrl = $("#J_Nologin").attr('href'), Hash = Do.require('Detail.Hash');rankAndComment = new RankStar('rankCon0', 5, '/comment/add', '/comment/listall/','', '', '', '', '',);rankAndComment.getCommentList(0, 3);});
      
      //显示支付窗口
      function showPayWindow() {$("#modal-buy").modal({hasClose: true});$('#modal-buy').on('hidden', function() {$('#ifr-buy').attr('src', '')});}
      
      //生成免费订单
      function createOrder() {$('#J-downWrap').empty().append('<div class="loading" style=""></div>');
        $.ajax({url: '/free?userId=&moduleId=',type: 'POST',dataType: 'json',
          data: {'productId': '','inAjax': 1,'serviceToken': '','channel':channel},
          success: function(msg) {if (msg.success) {$('#J-downWrap').empty().append($('#J-downInfo').fadeIn());isJustBuy = 'true';} else {$('#J-downWrap').empty().append(msg.error);}}});}
        
      Do("dialog", function() {
        var userId = "", loginRootUrl = $("#J_Nologin").attr('href'), Hash = Do.require('Detail.Hash');    
        
            //购买按钮点击
            $('.btn-buy').click(function(e) {
              var act = $(this).attr('id');orderAct=act;
              
                  if (getQueryValue('orderAct')) {
                    window.location = loginRootUrl.replace('orderAct%3DJ_buy','orderAct%3D'+act);
                    window.location = loginRootUrl.replace('orderAct%3DJ_zs','orderAct%3D'+act);
                  } else {
                    if(window.location.search.match(/\?/)) {
                      window.location = loginRootUrl + '%26'+'orderAct='+act;
                    } else {
                      window.location = loginRootUrl + '?'+'orderAct='+act;
                    }
                  }
                
            });
          
        //页面载入时执行判断，登录跳转回来页面，用来直接弹出支付
        
      });
      //收费主题 并且 没有购买的情况
      console.info('price:','isBuy:');
      
        function createPayOrder() {
          $.post('/order?userId=',
                  {'assemblyId': '',
                    'moduleId': '',
                    'serviceToken': '',
                    'orderAct':orderAct,
                    'channel':channel,
                    'inAjax': 1},
          function(data) {
            if (data['success']) {
              var order = data['order'];
              $('#orderFee').val(order['orderFee']);
              $('#orderId').val(order['orderId']);
              $('#orderDesc').val(order['orderDesc']);
              $('#createTime').val(order['createTime']);
              $('#returnUrl').val(order['returnUrl']);
              $('#senderSign').val(order['senderSign']);
              if(order.hasOwnProperty('useGiftcard')){
            	  $('#useGiftcard').val(order['useGiftcard']);
              }
			  if(order.hasOwnProperty('useMarketGiftcard')){
				  $('#useMarketGiftcard').val(order['useMarketGiftcard']);
			  }
              $('#payform').submit();
            } else {
              alert(data['msg']);
            }
          }, 'json');
        }
      
      function imgrefresh(){
        $('#randomCodeImg').attr('src','/ImageRandom?'+Math.random());
      }
    </script>

