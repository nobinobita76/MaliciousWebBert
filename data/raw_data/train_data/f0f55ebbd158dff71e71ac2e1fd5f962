	         <script language="javascript">
    var currShow = '';
  
    /** 显示产品分类下的商机图片  add by hudan*/
	function showNextSeries(parentSeriesId){
		var url = "http://detail.b2b.hc360.com/detail/turbine/action/ajax.ProSeriesAjaxAction/eventsubmit_doloadsubproseries/doLoadsubproseries";
			var paramData = {"pid":'100012349444',"sid":parentSeriesId,"username":'wwzjbzcy131',"d":'',"whoinput":encodeURI('',"utf-8")};
			var nextSeries = jQuery("#next2_"+parentSeriesId);
			if(nextSeries.html() == ""){
				jQuery.getJSON(url+"?callback=?",paramData,function(data){
					var html = "";
					if(data != ''){
						for(var i =0;i<data.length;i++){
							html +="<li>";
							html += "<a title=\"" + data[i].seriesName + "\" class=\"onelevel_a\" href=\""+data[i].seriesUrl+"\" target=\"_blank\" onmousedown=\"return hcclick('?hcdetail_enterpriselog=classification_pic')\">";
							html += "<span>"+data[i].seriesName+"</span>";
							html += "</a></li>";
						}
					}
					nextSeries.html(html);
					jQuery("#li_"+parentSeriesId+" > h4").toggleClass("h4_shrink");
					nextSeries.toggle();
				});
			}else{
				jQuery("#li_"+parentSeriesId+" > h4").toggleClass("h4_shrink");
				nextSeries.toggle();
			}
	}
    
    /** 隐藏产品分类下的商机图片 */
    function hideProductPic(id){
        var obj = document.getElementById(id);
        if(obj != null)
            obj.style.display = "none";
    }
    function showurl(url){
     window.open(url,'newwindow');
     return false;
    }
</script>
