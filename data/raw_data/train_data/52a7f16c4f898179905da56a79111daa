        jQuery(elm).css('position',((p) > startPos) ? 'fixed' : 'static'); 
        jQuery(elm).css('top',((p) > startPos) ? '69px' : ' '); 
jQuery(elm).css('box-shadow',((p) > startPos) ? '0px 2px 2px 2px #ededed' : ''); 
    }); 
}); 
</script> 
  </div> 
