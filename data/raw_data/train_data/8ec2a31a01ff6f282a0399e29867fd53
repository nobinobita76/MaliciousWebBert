<script type="text/javascript">
    $(window).load(function() {
        $('.flexslider').flexslider({
            animation: "slide",
            start: function(slider) {
                $('body').removeClass('loading');
            }
        });
    });
</script>
<!-- 提示插件  开始 -->
<script type="text/javascript" src="js/jquery-powerFloat.js"></script>
<script>
    $(function() {
        //通过默认rel属性加载
        $(".trigger1").powerFloat({
            target: $(".popup-box")
        });
        $("#trigger2").powerFloat({
            target: $("#targetBox2")
        });

    });
    var date = new Date();
    var times = date.getDate() + "" + date.getHours() + "" + date.getMinutes() + "" + date.getSeconds();
    function goUrl(cid) {

        function OnSuccess(result) {
            if (result != null) {

                var returnvaluearr = result.split('$$$');

                if (returnvaluearr[0] == "ERR") {
                    alert('您尚未登录，请先登录！');
                    window.location.href = "/website/Default.aspx";
                } else {

                    var loadUrl = "/website/Train.ashx?rnd=" + Math.random();

                    $.ajax({
                        type: "POST",
                        url: loadUrl,
                        dataType: "json",
                        data: {
                            action: "Check",
                            cid: cid,
                            userName: $("#fontUserName").html()
                        },
                        success: function(data) {
                            if (data.msg == "true") {
                                window.location.href = learning + "web/BanJi/ClassDefault.aspx?cid=" + cid;
                            }
                            else {
                                alert('您尚未参加此培训，无法查看培训内容！');
                            }
                        }
                    });

                }
            }
        }
        $.ajax({ type: "get", url: "website/NewsList.aspx?c=593266386737&type=10", dataType: "html", data: "varcode=" + times + "&action=checkuser", success: OnSuccess });
    }
</script>
<!-- 提示插件  结束 -->
    </form>

