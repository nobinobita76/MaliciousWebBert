	//动态添加link-异步-不会阻塞
	function addCssByLink(url){
		var link = document.createElement("link");
		link.setAttribute("rel", "stylesheet");
		link.setAttribute("type", "text/css");
		link.setAttribute("href", url);
		var heads = document.getElementsByTagName("head");
		if(heads.length) {
			heads[0].appendChild(link);
		} else {
			document.documentElement.appendChild(link);
		}
	}
	var getTheme	= function() {
		var themeInfo   = [];
		 if (themeInfo = navigator.userAgent.match(/hxtheme\/(\S*)/)) {
			return themeInfo[1] == 1 ? "night" : "day";
		}
		return "day";
	}
	var theme = getTheme();
	var getPhoneType	= function() {
		 var ua	= navigator.userAgent;
		 match   = /(?:\S*) (\S*)\/(\S*) \(Royal Flush(?:; (\S*))?\)/.exec(ua);
		 if (match && match[1]) {
			return match[1] == 'Hexin_Gphone' ? 'Hexin_Gphone' : "IHexin";
		}
		return "Hexin_Gphone";
	}
	var phoneType = getPhoneType();
	if (theme == 'night') {
		 		addCssByLink('//s.thsi.cn/cb?css/basic/stockph/iosdark_v5.20161229.css;css/basic/stockph/gntc-dark.css&20170105');
		}
