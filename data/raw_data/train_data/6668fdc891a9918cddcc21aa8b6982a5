<script>
$(document).ready(function(){

    if (window.location.href.indexOf('#sm=stock') >= 0) {
	idStat('wap_xg','wap_sj',{"ld":"mobile"});
        $('.topfixed').show();
	 $('#smpublist').hide();
       // $('.main-cont').eq(0).css('padding-top', 44);
        $('.gpkh').show();
    }
    if (window.location.href.indexOf('#sm=publist') >= 0) {
        $('#smpublist').show();
        $('.gpkh').show();
    }
    if (window.location.href.indexOf('#thirdboard=publist') >= 0) {
        $('.main-cont .info-cont').hide();
        $('.main-cont .info-cont #pub_new').parent().show();
        $('.main-cont .info-cont h3').hide();
    }
    var divid="opendiv";          
    var divclass="poop";          
    var boxclear= true;          
    var gray=0.5;                 
    var openclass="qwe";         
    var closeclass="close"        
    thisopen="."+openclass;
    gra(divid,divclass,gray);
    $(".menu").click(function(){
        if($("#opendiv").css("display")=="none"){
            $('.poop').css('top', $('.nav').height());
            masking(divid,divclass,boxclear,closeclass);
        }else{
            $("#opendiv").hide();
        }
    })

    function gra(mid,mclass,mgray){
        thisid="#"+mid;
        thisclass="."+mclass;
        h=$(document).height()
        var s=$(thisid).prepend("<div class='mask'></div>")
        $(thisid).find(".mask").css({ "height":"100%","width": "100%","background-color":"#000","position":"fixed","top":"0px","z-index":"19","opacity":mgray});
        $(thisclass).css({ "z-index":1000,"position": "fixed"});
    }

    function masking(mid,mclass,mclear,mclose){
        thisid="#"+mid;
        thisclass="."+mclass;
        thisclose="."+mclose;
        $(thisid).show();
        $(".mask").css({ "height":$(document).height()})
        if(mclear){
            clear(thisid,thisclass);
        }
        $(function(){
            $(thisclose).click(function(){
                $(thisid).hide();
            });
        });
    }

    function clear(ti,tc){
        $(".mask").click(function(){
            $(ti).hide();
        })
    }


    function isShenma(){
        var href = location.href;
        if(href.indexOf('sm=stock') > -1){
            return true;
        }else{
            return false;
        }  
    }

    if(isShenma()){
	$('.main-cont').css('margin-top','33px');
      //  $('#xgpd,.titleback,#gpkh').show();
        $('.low-price-alarm,.module').hide();
        $('#xgpd,.titleback,#gpkh,#xxzl').show();
        var buyBtn = $('#mg2 .buy-btn-three');
        buyBtn.each(function(i){
            if(!(i%2)){
                $(this).hide();
            }
        });
    }
});
</script>


