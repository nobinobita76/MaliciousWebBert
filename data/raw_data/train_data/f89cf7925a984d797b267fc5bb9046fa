    ;(function () {
        //ajax获取服务器端配置
        var URL = window.location.href;
        $.post('/Portal/Jssdk/share', {url:encodeURIComponent(URL)},function (data) {
            if (data.state === 'success') {
                console.log(data);
                wx.config({
                    debug: false, //是否开启debug
                    appId: 'wxb0f6ea4f05e88ae0', //appId
                    timestamp: data.timestamp,
                    nonceStr: data.nonceStr,
                    signature: data.signature,
                    jsApiList: [
                        'onMenuShareTimeline',
                        'onMenuShareAppMessage',
                        'onMenuShareQQ',
                        'onMenuShareWeibo',
                        'onMenuShareQZone'
                    ]
                });
                wx.ready(function () {
                    console.log('wx is ready');
                    var obj = {
                        title: document.title, //分享标体
                        desc: $('.al-article-content p').eq(0).text().slice(0, 30) + '...',
                        link: location.href,
                        imgUrl: 'http://jiqizhixin.com/data/upload/20161118/582e6252da08e.jpg', //分享图标 封面图， 可以页面中存个地址取一下
                        success: function () {

                        },
                        cancel: function () {

                        }
                    };
                    wx.onMenuShareTimeline(obj);
                    wx.onMenuShareAppMessage(obj);
                    wx.onMenuShareQQ(obj);
                    wx.onMenuShareWeibo(obj);
                    wx.onMenuShareQZone(obj);
                });
            }
        })
    })();
</script>  
