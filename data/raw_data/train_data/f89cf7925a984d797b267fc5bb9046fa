  <script>
$(function(){
    $('body').on('touchstart.dropdown', '.dropdown-menu', function (e) { e.stopPropagation(); });

    $("#main-menu li.dropdown").hover(function(){
        $(this).addClass("open");
    },function(){
        $(this).removeClass("open");
    });
    (function($){
        $.fn.totop=function(opt){
            var scrolling=false;
            return this.each(function(){
                var $this=$(this);
                $(window).scroll(function(){
                    if(!scrolling){
                        var sd=$(window).scrollTop();
                        if(sd>100){
                            $this.fadeIn();
                        }else{
                            $this.fadeOut();
                        }
                    }
                });

                $this.click(function(){
                    scrolling=true;
                    $('html, body').animate({
                        scrollTop : 0
                    }, 500,function(){
                        scrolling=false;
                        $this.fadeOut();
                    });
                });
            });
        };
        $('input.search').searchbox({
            url: '/your/search/url',
            param: 'q',
            dom_id: '#thumbnails',
            delay: 250,
            loading_css: '#spinner'
        });
    })(jQuery);
    $('.amz-cse').on({
        'mouseenter': function () {
            $(this).addClass('am-active');
        },
        'mouseleave':function(){
            $(this).removeClass('am-active');
            $(this).find('input').val('');
        }
    });

    $('.amz-cse input[type="text"]').on('blur', function () {
      $(this).closest('.amz-cse').removeClass('am-active');
    });
    });
</script> 
