<script type="text/javascript">
    function Register() {
        seajs.use("register", function (register) {
            register();
        });
        seajs.flush();
    }

    function login() {
        seajs.use("login", function(login) {
            login();
        });
        seajs.flush();
    }

    function formatSeconds(value) {
        var theTime = parseInt(value); // 秒
        var theTime1 = 0; // 分
        var theTime2 = 0; // 小时
        if (theTime > 60) {
            theTime1 = parseInt(theTime / 60);
            theTime = parseInt(theTime % 60);
            if (theTime1 > 60) {
                theTime2 = parseInt(theTime1 / 60);
                theTime1 = parseInt(theTime1 % 60);
            }
        }
        var result = "" + parseInt(theTime) + "秒";
        if (theTime1 > 0) {
            result = "" + parseInt(theTime1) + "分" + result;
        }
        if (theTime2 > 0) {
            result = "" + parseInt(theTime2) + "小时" + result;
        }
        return result;
    }

    seajs.use("jquery", function ($) {
        var hoverTimer;
        $(".quick_item").hover(function () {
            if ($(this).find("div").length > 0) {
                clearTimeout(hoverTimer);
                $(".channel_area li").addClass("top-out").removeClass("top-on");
                $(".quick_item").addClass("top-out");
                $(this).addClass("top-on").removeClass("top-out");
            }
        }, function () {
            if ($(this).find("div").length > 0) {
                var obj = $(this);
                hoverTimer = setTimeout(function () {
                    obj.addClass("top-out").removeClass("top-on");
                }, 200);
            }
        });
        $(".channel_area li").hover(function () {
            if ($(this).find(".mod_pop_action").length > 0) {
                clearTimeout(hoverTimer);
                $(".quick_item").addClass("top-out").removeClass("top-on");
                $(".channel_area li").addClass("top-out");
                $(this).addClass("top-on").removeClass("top-out");
            }
        }, function () {
            if ($(this).find(".mod_pop_action").length > 0) {
                var obj = $(this);
                hoverTimer = setTimeout(function () {
                    obj.addClass("top-out").removeClass("top-on");
                }, 200);
            }
        });

        function getMyPlayAnnal() {
            $.ajax({
                type: "Post",
                url: "/Video/GetMyPlayAnnalByPage/",
                dataType: 'json',
                success: function(result) {
                    if (result.list.length > 0) {
                       
                        var html = "";
                        for (var i = 0; i < result.list.length; i++) {
                            if (result.list[i] != null&&i<5) {
                                html += "<dd>";
                                html += " <a href=\"" + "/Course/" + result.list[i].MyClassID + "/" + result.list[i].DCourseID + "/" + result.list[i].DCourseID
                                    + "\" title='" + result.list[i].MyClassName + "' target=\"_blank\" class=\"video_name\">" + result.list[i].MyClassName + "</a>";
                                html += " <span class=\"video_timestamp\">" + (result.list[i].Progress == "100" ? "已看完" : formatSeconds(result.list[i].Timer)) + "</span>";
                                html += "</dd>";
                            }
                        }
                        $(".history_earlier").append(html); 
                    }
                    else {
                        $(".history_earlier").append("<dd style='text-align:center;'>暂无您的听课记录哦~~~</dd>"); 
                    }
                }
            });
        }
        function getUnReadMessage() {
            $.ajax({
                type: "Post",
                url: "/Message/GetSystemMessage/",
                dataType: 'json',
                success: function (result) {

                    if (result.list.length == 0) {
                        $("#divMessage").hide();
                    } else {
                        var html = "";
                        for (var i = 0; i < result.list.length; i++) {
                            if (result.list[i] != null) {
                                html += " <li><a href='http://a.233.com/usercenter/message/inform'>" + (result.list[i].Content) + "</a></li>";
                            }
                        }
                        $(".msg_list").html(html);
                       // $("#divMessage").show();
                    }
                }
            });
        }

        $(function () {
            getUnReadMessage();
            getMyPlayAnnal();
            $("body").on("click", ".quit", function() {
                $.ajax({
                    type: "Post",
                    url: "/User/ClearVisitor/",
                    dataType: "json",
                    success: function(result) {
                        $.ajax({
                            type: "Post",
                            url: "/User/Logout/",
                            data: {
                                RedirectUrl: location.href
                            },
                            dataType: "json",
                            success: function(result) {
                                window.location.reload();
                            }
                        });
                    },
                    error: function(e) {},
                    async: true,
                    cache: false
                });
            });
            
        }); 
    });
    seajs.flush();  
</script>
