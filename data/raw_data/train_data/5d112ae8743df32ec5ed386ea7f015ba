<script type="text/javascript">

    function getLogin() {
        seajs.use(["jquery183", "login"], function($, login) {
            login();
        });
        seajs.flush();
    }

    function quit() {
        seajs.use(["jquery183"], function($) {
            $.ajax({
                type: "Post",
                url: "/User/ClearVisitor/",
                dataType: 'json',
                success: function(result) {
                    $.ajax({
                        type: "Post",
                        url: "/User/Logout/",
                        data: {
                            RedirectUrl: location.href
                        },
                        dataType: 'json',
                        success: function() {
                            window.location.reload();
                        },
                        error: function(e) {},
                        async: false,
                        cache: false
                    });
                },
                error: function(e) {},
                async: true,
                cache: false
            });
        });
        seajs.flush();
    }

</script>
<script type="text/javascript">
    seajs.use(["jquery"], function($) {
        $(function() {
            $(window).scroll(function() {
                if ($(document).scrollTop() > $(window).height()) {
                    $(".slidebar_backUp").find("a").show();
                } else {
                    $(".slidebar_backUp").find("a").hide();
                }
            });
            $(".slidebar_backUp").click(function() {
                $('body,html').animate({
                    scrollTop: 0
                }, 1000);
                return false;
            });

            //二维码
            $(".trigger_app_btn").hover(function() {
                $(".trigger_app_content").show();
            }, function() {
                $(".trigger_app_content").hide();
            });

            //清除百度背景样式
            $(".bds_more").css("background", "").css("background-image", "");
            $("span .bds_more").css("background", "").css("background-image", "");

        });
    });
    seajs.flush();
</script>
