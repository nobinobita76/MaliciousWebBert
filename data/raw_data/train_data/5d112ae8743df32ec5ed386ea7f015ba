<script type="text/javascript">
 
 
    seajs.use(["jquery"], function ($) {
        var body = $("body");
        var cpid = "80";
        
        $(document).ready(function () {
            updateurl();
        });

        //年级选择
        body.on("click", "#grade dd", function () {
            var grade = $(this).find("a").attr("data-grade");
            $("#hd_grade").val(grade);
            getTitle();
            if (grade == 0) {
                //课后练习 
                $("#course").hide();
                $("#course2").show();
                $("#ztkc").show();
                $("#zjll").hide(); 
                getVideoChapter();
            } else { 
                $("#course").show();
                $("#course2").hide();
                $("#ztkc").hide();
                $("#zjll").show();
                 
                var type = $("#hd_type").val();

                if (type == 1) {
                    //章节练习
                    getChapter(cpid);
                } else if (type == 2) {
                    //模拟试卷
                    getExamPaper(2);
                } else if (type == 3) {
                    //单元测试
                    getExamPaper(3);
                } else if (type == 4) {
                    //专项练习
                    getExamPaper(4);
                }; 
            }
            $(this).siblings().removeClass('this').end().addClass('this');
            updateurl();
        });
         

        body.on("mouseover", "#tips", function() { 
            $(".xxm-poptip").show();
        });
        body.on("mouseout", "#tips", function () {
            $(".xxm-poptip").hide();
        });


        //科目
        body.on("click", "#course dd", function () {
            $("#hd_course").val($(this).find("a").attr("data-course"));
            $("#hd_book").val($(this).find("a").attr("data-book"));
            $(this).siblings().removeClass('this').end().addClass('this'); 
            getTitle();
            var type = $("#hd_type").val();
            if (type == 1) {
                //章节练习
                getChapter(cpid);
            } else if (type == 2) {
                //模拟试卷
                getExamPaper(2);
            } else if (type == 3) {
                //单元测试
                getExamPaper(3);
            } else if (type == 4) {
                //专项练习
                getExamPaper(4);
            };
            updateurl();
        });
        //视频科目
        body.on("click", "#course2 dd", function () {
            $("#hd_course2").val($(this).find("a").attr("data-course"));
            $(this).siblings().removeClass('this').end().addClass('this');
            var index = $(this).index(); 
            $("#ztkc").children().eq(index - 1).siblings().removeClass("this-small").addClass("zan-small").end().addClass("this-small");
            getTitle(); 
            getVideoChapter($(this).find("a").attr("data-course"));
        });

        //展开
        body.on("click", ".td-none", function () {
            $(this).attr("class", "chapter");
            var chapterId = $(this).attr("data-id");

            var count = $("#chapterid_" + chapterId).parent().find("tr").length;
            if (count == 1) { 
                getChapterDetail(chapterId);
            }
            $(this).parent().siblings().show();
        });
        //收缩
        body.on("click", ".chapter", function () {
            $(this).attr("class", "td-none");
            $(this).parent().siblings().hide().end().show();
        });

         
        body.on("click", "#zjll a", function () {
            $(".qu-ping").remove();
            $(".yd-kuang").remove();
            $("#zjll").attr("style", null);

            var type = $(this).attr("data-type"); 
            $(this).siblings().attr('class', 'zan').end().attr('class', 'this');
            $("#hd_type").val(type);
            if (type == 1) {
                //章节练习
                getChapter(cpid);
            } else if (type == 2) {
                //模拟试卷
                getExamPaper(2);
            } else if (type == 3) {
                //单元测试
                getExamPaper(3);
            } else if (type == 4) {
                //专项练习
                getExamPaper(4);
            }
        });

        body.on("click", "#ztkc a", function () {
                var type = $(this).attr("data-type"); 
                $(this).siblings().attr('class', 'zan-small').end().attr('class', 'this-small');
                var index = $(this).index()+1; 
                $("#course2").children().eq(index).siblings().removeClass("this").end().addClass("this");
                $("#hd_course2").val($(this).attr("data-course"));
                //获取视频课程章节
                getVideoChapter();
            })
            .on("click", ".zt", function () {
                var id = $(this).attr("data-id");
                if($(this).html()!="开始做题"){
                    if ($(this).hasClass("btn-green")) {
                        $(this).parent().parent().find(".ico").addClass("open");
                        $(this).removeClass("btn-green").addClass("btn-grey").html("点击收起");

                        var count = 0;
                        $(".ta-three").each(function (index) {
                            if ($(this).attr("data-parent-id") == id) {
                                $(this).show();
                                count += 1;
                            }
                        });
                        if (count == 0) {
                            getVideoChapterDetail(id);
                        }
                        $(this).parent().parent().find(".chapter").find("h2").addClass("f-f18 orange");
                    } else {
                        $(this).parent().parent().find(".ico").removeClass("open");
                        $(this).removeClass("btn-grey").addClass("btn-green").html("展开做题");

                        $(".ta-three").each(function (index) {
                            if ($(this).attr("data-parent-id") == id) {
                                $(this).hide();
                            }
                        });
                        $(this).parent().parent().find(".chapter").find("h2").removeClass("f-f18 orange");
                    }
                }
            });

        //获取章节  
        function getChapter() {
            var type = $("#hd_type").val();
            if (type == 1) {
                //章节练习
                getChapter1(cpid);
            } else if (type == 2) {
                //模拟试卷
                getExamPaper(2);
            } else if (type == 3) {
                //单元测试
                getExamPaper(3);
            } else if (type == 4) {
                //专项练习
                getExamPaper(4);
            }
        }

        function updateurl()
        {
            var grade = $("#hd_grade").val();
            var course = $("#hd_course").val();
            var book = $("#hd_book").val();
            $(".jilu:eq(0)").attr("href", "/questionbank/wrongtopic/?grade=" + grade + "&course=" + course + "&book=" + book);
            $(".jilu:eq(1)").attr("href", "/questionbank/practicerecord/?grade=" + grade + "&course=" + course + "&book=" + book);
            $(".jilu:eq(2)").attr("href", "/questionbank/markreport/?grade=" + grade + "&course=" + course + "&book=" + book);
            $(".jilu:eq(3)").attr("href", "/questionbank/mycollection/?grade=" + grade + "&course=" + course + "&book=" + book);
        }

        //章节练习
        function getChapter1() { 
            var grade = $("#hd_grade").val();
            var book = $("#hd_book").val();
            var course = $("#hd_course").val();

            $.ajax({
                type: "POST",
                url: "/QuestionBank/GetQuestionBankChapter/",
                data: {
                    Grade: grade,
                    Course: course,
                    Book: book 
                },
                dataType: 'json',
                success: function (result) {
                    var html = "";
                    if (result.S == 10006) {
                        if (result.list.length == 0) {
                            html+="<ul class=\"shijuan\">";
                            html+="<li class=\"meiti\">"; 
                            html += "<span>试题正在添加中...</span><br>";
                            html+="敬请期待！";
                            html+="</li>";
                            html+="</ul>"; 
                        }else{ 
                            html += "<div class=\"le-prabg le-question\">";
                            html += "<div class=\"le-title\"><span>章节</span> <span>已做/总题</span> <span class=\"left\">错题";
                            html += "";
                            html += "<div class=\"why\">";
                            html += "<a href=\"javascript:;\" class=\"ico\" id=\"tips\"></a>";
                            html += "<div class=\"xxm-poptip\" style=\"display: none;\" >";
                            html += "<div class=\"xxm-poptip-cont\">";
                            html += "<div class=\"xxm-poptip-arrow\">";
                            html += "<em>◆</em>";
                            html += "<i>◆</i>";
                            html += "</div>";
                            html += "<p>点击数字，可重做错题</p>";
                            html += "</div>";
                            html += "</div>";
                            html += "</div>"; 
                            html += "</span></div>";
                            html += "<div class=\"lo-tablecon xian\">";
                            html += "<table width=\"100%\" border=\"0\" cellspacing=\"0\" class=\"ui-table percentage ui-table-row\" id=\"chapter\">";
                        }
                        for (var i = 0; i < result.list.length; i++) {
                            var style = "td-none";
                            if (i == 0) {
                                style = "chapter"; 
                            }

                            html += "<tbody>";
                            html += "<tr id=\"chapterid_"+result.list[i].ID+"\">";
                            html += "<td class=\""+style+"\" data-id=\"" + result.list[i].ID + "\"><h2>" + result.list[i].ChapterName + "</h2></td>";
                            var rate = 0;
                            if (result.list[i].ExamNumber > 0) {
                                rate = ((result.list[i].RightNum + result.list[i].ErrorNum) / result.list[i].ExamNumber) * 100;
                                rate = rate > 100 ? 100 : rate;
                            } 
                            html += "<td class=\"zt-left\"><span><b class=\"b1\" style=\"width:"+rate+"%;\"></b> </span> " + (result.list[i].RightNum + result.list[i].ErrorNum) + " / " + result.list[i].ExamNumber + "</td>";
                            html += "<td class=\"czct\"><a href=\"" + (result.list[i].ErrorNum == 0 ? "javascript:;" : "http://a.233.com/PracticeRecord/RedoExamination?ChapterID=" + result.list[i].ID) + "\" class=\"wrong\" title=\"" + (result.list[i].ErrorNum > 0 ? "点击可重做错题" : "") + "\">" + result.list[i].ErrorNum + "</a></td>";
                            html += "<td class=\"czct\"> </td>";
                            var chapterId = 0; 
                            chapterId = result.list[i].ID; 
                            html += "<td class=\"czct\"><a href=\"/questionbank/practice?chapterid=" + chapterId + "\" class=\" zt\" >开始做题</a></td>";
                            html += "</tr> "; 
                            html += "</tbody>";

                            if (i == 0) {
                                getChapterDetail(result.list[i].ID);
                            }
                        }
                        if (result.list.length > 0) {
                            html += "</table>";
                            html += "</div>";
                            html += "</div>";
                        }
                    }
                    $("#content").html(html);
                },
                error: function (e) {
                },
                async: true,
                cache: false
            });

            getTitle(); 
        }
        
        function getChapterDetail(chapterId) {
            var grade = 0;
            var course = 0;
            var book = 0;

            grade = $("#hd_grade").val();
            book = $("#hd_book").val();
            course = $("#hd_course").val();

            $.ajax({
                type: "GET",
                url: "/QuestionBank/GetQuestionBankChapterDetail/",
                data: {
                    ChapterID: chapterId
                },
                dataType: 'json',
                success: function (result) {
                    var html = "";
                    if (result.S == 10006) { 
                        for (var i = 0; i < result.list.length; i++) {
                            html += "<tr class=\"ta-three\" data-id=\"" + result.list[i].ID + "\">";
                            html += "<td class=\"teleft\"><h4><a href=\"javascript:;\">" + result.list[i].ChapterName + "</a></h4><div></div></td>";
                            var rate = 0;
                            if (result.list[i].ExamNumber > 0) {
                                rate = (result.list[i].RightNum + result.list[i].ErrorNum) / result.list[i].ExamNumber * 100;
                                rate = rate > 100 ? 100 : rate;
                            }
                            html += "<td class=\"zt-left\"><span><b class=\"b1\" style=\"width:" + rate + "%;\"></b> </span> " + (result.list[i].RightNum + result.list[i].ErrorNum) + " / " + result.list[i].ExamNumber + " </td>";
                            html += "<td class=\"czct\"><a href=\"" + (result.list[i].ErrorNum == 0 ? "javascript:;" : "http://a.233.com/PracticeRecord/RedoExamination?SectionID="+result.list[i].ID) + "\" class=\"wrong\"  title=\"" + (result.list[i].ErrorNum > 0 ? "点击可重做错题" : "") + "\">" + result.list[i].ErrorNum + "</a></td>";
                            html += "<td class=\"czct\"> </td>"; 
                            if (parseInt(result.list[i].IsDo) > 0) {
                                html += "<td class=\"czct\"><a href=\"/questionbank/practice?chapterid=" + result.list[i].ID + "\" class=\" zt\" >再做一次</a></td>";
                            } else {
                                html += "<td class=\"czct\"><a href=\"/questionbank/practice?chapterid=" + result.list[i].ID + "\" class=\" zt\" >开始做题</a></td>";
                            }
                            html += "</tr>"; 
                        } 
                    }
                    $("#chapterid_"+chapterId).after(html);
                },
                error: function (e) {
                },
                async: true,
                cache: false
            });

            getTitle();
        }


        //获取试卷
        //单元测试、模拟试卷
        function getExamPaper(type) {
            var grade = 0;
            var course = 0;
            var book = 0;
            grade = $("#hd_grade").val();
            course = $("#hd_course").val();
            book = $("#hd_book").val();

            $.ajax({
                type: "GET",
                url: "/QuestionBank/GetExamPaper",
                data: {
                    Grade: grade,
                    Course: course,
                    Book: book,
                    Type: type
                },
                dataType: 'json',
                success: function (result) {
                    var html = "";
                    if (result.S == 10006) {
                        if (result.list.length == 0) {
                            html += "<ul class=\"shijuan\">";
                            html += "<li class=\"meiti\">";
                            html += "<span>试题正在添加中...</span><br>";
                            html += "敬请期待！";
                            html += "</li>";
                            html += "</ul>";
                        } else {
                            var courseName = "语";
                            switch (course) {
                                case "1":
                                    courseName = "语";
                                    break;
                                case "2":
                                    courseName = "数";
                                    break;
                                case "3":
                                    courseName = "英";
                                    break;
                            }

                            var html1 = "";
                            for (var i = 0; i < result.list.length; i++) {
                                if (result.list[i].PaperName.indexOf("期中") > 0) {
                                    courseName = "精";
                                    html1 += "<li>";
                                    var isDoName = "开始做题";
                                    if (result.list[i].IsDo > 0) {
                                        isDoName = "再做一次";
                                    }
                                    html1 += "<i class=\"ico expert \">" + courseName + "</i><span class=\"dal-but\"> <a href=\"/QuestionBank/ExamPaper?PaperID=" + result.list[i].ID + "\" class=\"zt-go\">" + isDoName + "</a></span>";
                                    html1 += "<h3><a href=\"/questionbank/exampaperintroduce?paperid=" + result.list[i].ID + "\"  style=\"color: #ff841c;\">" + result.list[i].PaperName + "</a></h3>";
                                    html1 += "<p><span>总题：" + result.list[i].ExamNumber + " 题</span><span>考试时长：" + result.list[i].ExamTime + "分钟</span> <span>总分：" + result.list[i].TotalPoints + "分 </span> <span>" + result.list[i].DoPaperNumber + "人参考</span> <span>更新时间：" + result.list[i].UpdateDate + " </span></p>";

                                    html1 += "</li>";
                                } else {
                                    var isDoName = "开始做题";
                                    if (result.list[i].IsDo > 0) {
                                        isDoName = "再做一次";
                                    }
                                    html += "<li>";
                                    html += "<i class=\"ico expert \">" + courseName + "</i><span class=\"dal-but\"> <a href=\"/questionbank/exampaper?paperid=" + result.list[i].ID + "\" class=\"zt-go\">"+isDoName+"</a></span>";
                                    html += "<h3><a href=\"/questionbank/exampaperintroduce?paperid=" + result.list[i].ID + "\">" + result.list[i].PaperName + "</a></h3>";
                                    html += "<p><span>总题：" + result.list[i].ExamNumber + " 题</span><span>考试时长：" + result.list[i].ExamTime + "分钟</span> <span>总分：" + result.list[i].TotalPoints + "分 </span> <span>" + result.list[i].DoPaperNumber + "人参考</span> <span>更新时间：" + result.list[i].UpdateDate + " </span></p>";

                                    html += "</li>";
                                }
                            }
                            html = "<ul class=\"shijuan\">" + html1 + html + "</ul>";
                            //html += html1 + "</ul>";
                        }
                    }
                    $("#content").html(html);
                },
                error: function (e) {
                },
                async: true,
                cache: false
            });
        }

        //标题
        function getTitle() {
            var grade = 0;
            var course = 0;
            var book = 0;

            grade = $("#hd_grade").val();
            book = $("#hd_book").val();
            course = $("#hd_course").val();

            var title = "";
            switch(grade) {
            case "1":
                title += "一年级";
                break;
            case "2":
                title += "二年级";
                break;
            case "3":
                title += "三年级";
                break;
            case "4":
                title += "四年级";
                break;
            case "5":
                title += "五年级";
                break;
            case "6":
                title += "六年级";
                break;
            }
            switch(course) {
            case "1":
                title += "语文";
                break;
            case "2":
                title += "数学";
                break;
            case "3":
                title += "英语";
                break;
            }
            switch(book) {
            case "1":
                title += "上册";
                break;
            case "2":
                title += "下册";
                break;
            }
            $("#coursetitle").html(title);
        }

        //获取章节
        function getVideoChapter() {
            var course = $("#hd_course2").val();

            $("#coursetitle").html("视频课程练习");
            $.ajax({
                type: "GET",
                url: "/QuestionBank/GetVideoQuestionBankChapter/",
                data: {
                    Course: course
                },
                dataType: 'json',
                success: function (result) {
                    var html = "";
                    
                    if (result.S == 10006) {
                        if (result.list.length == 0) {
                            html += "<ul class=\"shijuan\">";
                            html += "<li class=\"meiti\">";
                            html += "<span>试题正在添加中...</span><br>";
                            html += "敬请期待！";
                            html += "</li>";
                            html += "</ul>";
                        } else {
                            html += "<div class=\"le-prabg le-question\">";
                            html += "<div class=\"le-title\"><span>章节</span> <span>已做/总题</span> <span class=\"left\">重做错题</span></div>";
                            html += "<div class=\"lo-tablecon wuxian\">";
                            html += "<table width=\"100%\" border=\"0\" cellspacing=\"0\" class=\"ui-table percentage ui-table-row\">";
                        }
                        var index = 0;
                        for (var i = 0; i < result.list.length; i++) {
                            var courseName = "语";
                            switch (course) {
                            case "1":
                                courseName = "语";
                                break;
                            case "2":
                                courseName = "数";
                                break;
                            case "3":
                                courseName = "英";
                                break;
                            case "4":
                                courseName = "语";
                                break;
                            case "5":
                                courseName = "英";
                                break;
                            case "6":
                                courseName = "英";
                                break;
                            }

                            html += "";
                            html += "<tbody>";
                            html += "<tr id=\"myclassid_" + result.list[i].MyClassId + "\">";
                            html += "<td class=\"chapter\"><i class=\"ico expert\">" + courseName + "</i><h2 id=\"tdh2\">" + result.list[i].MyClassName + "</h2></td>";
                            var rate = 0;
                            if (result.list[i].TotalNum > 0) {
                                rate = ((result.list[i].RightNum + result.list[i].ErrorNum) / result.list[i].TotalNum) * 100;
                                rate = rate > 100 ? 100 : rate;
                            }
                            html += "<td class=\"zt-left\"><span><b class=\"b1\" style=\"width:" + rate + "%;\"></b> </span> " + (result.list[i].DoNum) + " / " + result.list[i].TotalNum + " </td>";
                            html += "<td class=\"czct\"><a href=\"javascript:;\" class=\"wrong\" title=\"" + (result.list[i].ErrorNum > 0 ? "点击可重做错题" : "") + "\">" + result.list[i].ErrorNum + "</a></td>";
                            html += "<td class=\"czct\"></td>";
                            html += "<td class=\"czct\"><a href=\"javascript:;\"  data-id=\"" + result.list[i].MyClassId + "\" class=\"zt  btn-green\">展开做题</a></td>";
                            html += "</tr>"; 
                            html += "</tbody>";
                        }
                    };
                    if(result.list.length>0){
                        html += "</table>";
                        html += "</div>";
                        html += "</div>";
                    }
                    $("#content").html(html);
                },
                error: function (e) { 
                },
                async: true,
                cache: false
            });
        }

        function getVideoChapterDetail(myClassId) {
            var course = $("#hd_course2").val();
            $.ajax({
                type: "GET",
                url: "/QuestionBank/GetVideoChapterDetail/",
                data: {
                    MyClassID: myClassId
                },
                dataType: 'json',
                success: function (result) {
                    var html = ""; 
                    for (var i = 0; i < result.list.length; i++) { 
                        html += "<tr class=\"ta-three\" data-parent-id=\"" + result.list[i].MyClassID + "\" >";
                        html += "<td class=\"teleft\"><h4><a href=\"http://a.233.com/Course/" + result.list[i].MyClassID + "/" + result.list[i].DCourseID + "/" + result.list[i].DCourseID + "\">" + result.list[i].Subject + "</a></h4></td>";
                        var rate = 0;
                        if (result.list[i].TotalNum > 0) {
                            rate = (result.list[i].RightNum + result.list[i].ErrorNum / result.list[i].TotalNum) * 100;
                            rate = rate > 100 ? 100 : rate
                        }
                        html += "<td class=\"zt-left\"><span><b class=\"b1\" style=\"width:" + rate + "%;\"></b> </span> " + result.list[i].DoNum + " / " + result.list[i].TotalNum + " </td>";
                        html += "<td class=\"czct\"><a href=\"javascript:;\" class=\"wrong\" title=\"" + (result.list[i].ErrorNum > 0 ? "点击可重做错题" : "") + "\">" + result.list[i].ErrorNum + "</a></td>";
                        html += "<td class=\"czct\"><a href=\"http://a.233.com/Course/" + result.list[i].MyClassID + "/" + result.list[i].DCourseID + "/" + result.list[i].DCourseID + "\" target=\"_blank\" class=\"listen\">听课</a></td>";
                        var isDoName = "开始做题";
                        if (result.list[i].IsDo > 0) {
                            isDoName = "再做一次";
                        }
                        html += "<td class=\"czct\"><a href=\"/questionbank/videopractice?chapterid=" + result.list[i].DCourseID + "&course=" + course + "\"  class=\" zt\">" + isDoName + "</a></td>";
                        html += "</tr>";
                    } 
                    $("#myclassid_" + myClassId).after(html);
                },
                error: function (e) {
                },
                async: true,
                cache: false
            });
        }

        $(function () { 
            getTitle();
            var type = $("#hd_type").val();
            var grade = $("#hd_grade").val();
           
            //$("#zjll a").siblings().attr('class', 'zan').end().eq(parseInt(type) - 1).attr('class', 'this');
            if (grade == 0) {
                getVideoChapter();
            } else {

                if (type == 1) {
                    //章节练习
                    getChapter(cpid);
                } else if (type == 2) {
                    //模拟试卷
                    getExamPaper(2);
                } else if (type == 3) {
                    //单元测试
                    getExamPaper(3);
                } else if (type == 4) {
                    //专项练习
                    getExamPaper(4);
                };
            }

        });
    });
    seajs.flush();
</script>



