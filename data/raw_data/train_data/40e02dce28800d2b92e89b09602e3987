  <script type="text/javascript">
    BizQQWPA.addCustom({ aty: '0', a: '0', nameAccount: 4009639119, selector: 'div_im_main' });
    function QQService() {
        ///不管是否统计成功都要执行链接
        //im_main绝对不能设置为a标签，因为a标签的模拟click会被浏览器认为不安全，被浏览器过滤
        $("#div_im_main").click();
    }
</script> 
