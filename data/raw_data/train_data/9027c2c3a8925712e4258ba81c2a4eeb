    var oldDiv = "";
    var Submittype = "1";
    function RendererEvent(ids, wbgid, type) {
        $('#ReplyFSOCID').val(wbgid);
        if (oldDiv != ids) {
            if (oldDiv != "") {
                $('.' + oldDiv).html("");
            }
            $('.' + ids).html(comment_div);
        }
        oldDiv = ids;
        Submittype = type;
    }
    function Replysubmit() {
        var comment = $('.ReplyComments').val(); 
        AddComment(comment, "Reply");
    }

    function AppendComments(com, type) {
        var name = $('#Lname').val();
        var flg = true;
        if (com.indexOf("javascript") > 0 || com.indexOf("script") > 0 || com.indexOf("Script") > 0 || com.indexOf("SCRIPT") > 0 || com.indexOf("alert") > 0) {
            com = "";
        }
        com = com.replace("[E1]", ReplaceSimle("E1"));
        com = com.replace("[E2]", ReplaceSimle("E2"));
        com = com.replace("[E3]", ReplaceSimle("E3"));
        com = com.replace("[E4]", ReplaceSimle("E4"));
        com = com.replace("[E5]", ReplaceSimle("E5"));
        com = com.replace("[E6]", ReplaceSimle("E6"));
        com = com.replace("[E7]", ReplaceSimle("E7"));
        com = com.replace("[E8]", ReplaceSimle("E8"));
        com = com.replace("[E9]", ReplaceSimle("E9"));
        com = com.replace("[E10]", ReplaceSimle("E10"));
        com = com.replace("[E11]", ReplaceSimle("E11"));
        com = com.replace("[E12]", ReplaceSimle("E12"));

        if (type == "1") {
            var order = $('#OrderNum1').val(); if (order == null || order == "") { order = 1; }
            var str = '<li><div class="Forum_Contentbox3_conleft">' + order + '楼<img src="/Statics/Images/WorthBuyGoods/basic/worthbuyn_icon.png"></div><div class="Forum_Contentbox3_conright"><div class="Forum_Contentbox3_conright_tit"><a href="javascript:void(0)">' + name + '<div class="offerrank_stars star20" style="margin-top: -14px; margin-left: 40px;"></div></a><em>刚刚</em></div><p>' + com + '</p></div></li>';
            $('#con_two_1_ul').prepend(str);
        } else {
            var order = $('#OrderNum2').val();
            var str = '<li><div class="Forum_Contentbox3_conleft">' + order + '楼<img src="/Statics/Images/WorthBuyGoods/basic/worthbuyn_icon.png"></div><div class="Forum_Contentbox3_conright"><div class="Forum_Contentbox3_conright_tit"><a href="javascript:void(0)">' + name + '<div class="offerrank_stars star20" style="margin-top: -14px; margin-left: 40px;"></div></a><em>刚刚</em></div><p>' + com + '</p></div></li>';
            $('#con_two_2_ul').prepend(str);
        }
    }

    function InsertSimle(el, val) {
        var text = "";
        if (el == "1") {
            text = $('#Comments').val();
        } else {
            text = $('.ReplyComments').val();
        }

        if (text == msgText) {
            text = "";
        }
        text = text + "[" + val + "]";
        if (el == "1") {
            $('#Comments').val(text)
        } else {
            $('.ReplyComments').val(text);
        }
    }
