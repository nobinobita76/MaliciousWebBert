    setTimeout(function () { PVChange(); }, 500);
    function PVChange() {
        $.ajax({
            type: "POST",
            dataType: "json",
            url: '/ModifyOrderCommentTopOrStep',
            data: {
                FSOID: $('#FSOID').val(),
                FSOCID:'',
                Action: ''
            },
            success: function (json) {

            }
        });
    }
    function setTab(name, cursel, n) {
        for (i = 1; i <= n; i++) {
            var menu = document.getElementById(name + i);
            var con = document.getElementById("con_" + name + "_" + i);
            menu.className = i == cursel ? "hover" : "";
            con.style.display = i == cursel ? "block" : "none";
        }
    }
    $('#submit').on('click', function () {
        var comment = $('#Comments').val();
        Submittype = "1";
        AddComment(comment, "");
    });
    setTimeout(function () {
        if ($('#pageNum').val() != 1) {
            var height = $('#content_tie').height() + "px";
            $("html, body").animate({ scrollTop: height }, 100);
        }
    }, 2000);
    function ReplaceSimle(val) {
        var text = "<img src='/statics/js/ubbeditor/common/theme/default/images/smiley/{0}.gif'>";
        switch (val) {
            case "E1":
                text = text.replace("{0}", "1");
                break;
            case "E2":
                text = text.replace("{0}", "2");
                break;
            case "E3":
                text = text.replace("{0}", "3");
                break;
            case "E4":
                text = text.replace("{0}", "4");
                break;
            case "E5":
                text = text.replace("{0}", "5");
                break;
            case "E6":
                text = text.replace("{0}", "6");
                break;
            case "E7":
                text = text.replace("{0}", "7");
                break;
            case "E8":
                text = text.replace("{0}", "8");
                break;
            case "E9":
                text = text.replace("{0}", "9");
                break;
            case "E10":
                text = text.replace("{0}", "10");
                break;
            case "E11":
                text = text.replace("{0}", "11");
                break;
            case "E12":
                text = text.replace("{0}", "12");
                break;
        }
        return text;
    }
    function AddComment(comment, type) {
        if (comment == "" || comment == "为维护良好的讨论环境，请勿在留言中发布政治话题、买卖返券、加价转让、人身攻击等内容。") {
            return alert("请填写评论内容");
        }
        if (comment.length < 1) {
            return alert("请填写评论内容");
        }
        if (comment.length > 500) {
            return alert("评论内容不能超过500个字符");
        }
        AppendComments(comment, Submittype);
        $.ajax({
            type: "POST",
            dataType: "json",
            url: '/AddPostsComment',
            data: {
                ReplyFSOCID: $('#ReplyFSOCID').val(),
                ShowTitle: $('#ShowTitle').val(),
                FSOID: $('#FSOID').val(),
                Comment: encodeURIComponent(comment)
            },
            success: function (json) {
                if (json.success.toString() == "true") {
                    if (type == "") {
                        $('#submit').css("display", "none");
                    } else {
                        $('.' + oldDiv).html("");
                    }
                }
            }
        });
    }

    function VoteEvent(type, id, num, el) {
        num = parseInt(num) + 1;
        if (type == "Top") {
            $('#' + el).html("顶(" + num + ")");
            //$('#' + el).removeClass('bcicon2 mleft15').addClass('siteup mleft15');
            $('#' + el).removeAttr("onclick");
        } else if (type == "Step") {
            $('#' + el).html("踩(" + num + ")");
            //$('#' + el).removeClass('bcicon3 mleft15').addClass('sitedown');
            $('#' + el).removeAttr("onclick");
        }
        $.ajax({
            type: "POST",
            dataType: "json",
            url: '/ModifyOrderCommentTopOrStep',
            data: {
                FSOCID: id,
                FSOID:"",
                Action: type
            },
            success: function (json) {

            }
        });
    }
    var msgText = "为维护良好的讨论环境，请勿在留言中发布政治话题、买卖返券、加价转让、人身攻击等内容。";
    function focusInputEle(o) {
        if (o.value == msgText) {
            o.value = '';
            o.style.color = '';
        }
    }

    function blurInputEle(o) {
        if (o.value == '') {
            o.value = msgText;
            o.style.color = 'grey';
        }
    }

