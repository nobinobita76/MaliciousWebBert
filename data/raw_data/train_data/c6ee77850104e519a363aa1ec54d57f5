  <script>
            $('.dp-txt .more').on('click', function() {
        $(this).closest('.dp-txt').addClass('on')
        $(this).hide()
        $(this).closest('.dp-txt').find('.hidemore').show()
        return false
    })
    $('.dp-txt .hidemore').on('click', function() {
        $(this).closest('.dp-txt').removeClass('on')
        $(this).closest('.dp-txt').find('.more').show()
        return false
    })
        $(function() {
            $('.kz-data').each(function(k, v) {
                var $len = $(this).find('tr').length,
                    flag = false // 若为真表示 展开
                if ($len <= 3) {
                    return
                } else {
                    $(this).find('tr').slice(4).hide();
                    $(this).find('.kz-more').click(function() {
                        if (flag) {
                            $(this).closest('.kz-data').find('tr').slice(4).hide()
                            flag = !flag
                        } else {
                            $(this).closest('.kz-data').find('tr').show()
                            flag = !flag
                        }
                    })
                }

            })

        })
        function get_comment(page)
        {
            var s=(new Date()).getSeconds();
            $.ajax({
                url: '/ajaxjson/get_comments/'+page+'/'+s , //后台处理程序
                success:function(msg){
                    $('.dp-one').html(msg);
                    window.location.hash = "dianping";
                }
            });
        }
function show_map(uid)
{
    layer.open({
        type: 2,
        title: '地图',
        shadeClose: true,
        shade: 0.8,
        area: ['750px', '600px'],
        content: "http://www.3dkezhan.com/map/tjf/"+uid //iframe的url
    });
}
