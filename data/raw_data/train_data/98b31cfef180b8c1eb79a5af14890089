    &lt;ul&gt;
    {for row in rows}
    &lt;li&gt;
        &lt;p class=&quot;p_info&quot; style=&quot;width:115px;overflow:hidden;white-space:nowrap;&quot;&gt;
            &lt;a href=&quot;/item/${row.id}&quot;&gt;${row.item_name}&lt;/a&gt;&lt;br /&gt;
            封装规格：${row.format}&lt;br /&gt;
            价格：&lt;font class=&quot;red&quot;&gt;￥${row.price}&lt;/font&gt;&lt;br /&gt;
        &lt;/p&gt;
        &lt;button  type=&quot;button&quot; onclick=&quot;delete_history(${row.id});&quot;&gt;&lt;/button&gt;
    &lt;/li&gt;
    {/for}
        &lt;/ul&gt;
</textarea> 
  <script type="text/javascript">
    $(function () {
        $('ul .qqs').hover(function () {
            $('.show_box_qq').show();
        }, function () {
            $('.show_box_qq').hide();
        });
        $('ul .wxs').hover(function () {
            $('.show_box_wx').show();
        }, function () {
            $('.show_box_wx').hide();
        });
        $('ul .history').hover(function () {
            get_history();
            $('.show_box_history').show();
        }, function () {
            $('.show_box_history').hide();
        });   
        $("#online_close img").click(function () {
            $(".r_bj").hide();
            $("#online_open").show();
            $("#online_close").hide();
        });
        $("#online_open img").click(function () {
            $(".r_bj").show();
            $("#online_open").hide();
            $("#online_close").show();
        });
        function get_history() {
            $.ajax({
                type: "POST",
                url: "/post/stock_item:get_history",
                dataType: "json",
                async: true,
                data: {},
                success: function (result) {
                    if(result) {
                        var a = TrimPath.processDOMTemplate('history_tpl',result);
                        $("#history_list").html(a);
                    }
                }
            });
        }
        get_history();
    });
    function delete_history(item_id) {
        $.ajax({
            type: "POST",
            url: "/post/stock_item:delect_viewed_item",
            dataType: "json",
            async: true,
            data: { item_id: item_id},
            success: function (result) {
                if(result) {
                    var a = TrimPath.processDOMTemplate('history_tpl',result);
                    $("#history_list").html(a);
                }
            }
        });
    }
</script> 
