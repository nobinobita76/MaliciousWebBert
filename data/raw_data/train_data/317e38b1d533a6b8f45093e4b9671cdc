	function h_submit(){
	var hsw = document.getElementById("hsw").value
		if(hsw.trim()==""){
			alert("请输入检索词!")
			return false;
		}
		window.open("http://ss.chaoxing.com/ncs?sw=" + encodeURI(hsw)); 
		return false;
	}
