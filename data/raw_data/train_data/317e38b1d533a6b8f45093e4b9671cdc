	            	<script type="text/javascript">
	var order = "";
	$.ajax({
		url:"/ajaxMethod.shtml?urlParam=BASE_COURSE_INFO&num=1&type=1",
		data:{"param":"{courseIds:\'95926646,95926528,95926413,95926278,95926035,95925753,95925535,95925372,95925009,95924866,95924693\'}"},
		type:"get",
		success:function(data){
				var PAGE_COURSE = "1";
	var PAGE_TEACHER = "2";
	var PAGE_SCHOOL = "3";
	var PAGE_EXPERT = "4";
	var str = '';
	var ids = "";
	if(undefined == data && data.type == "0"){ return false;}
	if(PAGE_COURSE == data.type){
		for(var i = 0;i< data.courses.length;i++){
			var obj = data.courses[i];
			if(!obj.succ) continue; 
			var old_name = name = obj.name.replace(/<[^>]+>/g,"");
			if(name.length>12){
				name = name.substring(0,12)+"...";
			}
			if(obj.image.indexOf("star3")>0){
			var _img = obj.image.replace("origin","338_200c");
			}else{
			var _img = obj.image.replace("origin","338_200cc");
			}
			var count = i + 1 ;
			str = str + '<li id="course_'+count+'"';
			//if(count > 6){
				//str = str + ' style="display:none"';
			//}
			str = str + '>'+
			'<a class="img_box" href="http://mooc1.chaoxing.com/course/'+obj.id+'.html"  title="'+obj.name+'" target="_blank"><img class="img_list" src="'+_img+'" /></a>'+
			'<h4><a href="http://mooc1.chaoxing.com/course/'+obj.id+'.html"  title="'+old_name+'" target="_blank">'+name+'</a>'+
			'<span style="text-align: center;float: right;width: 60px;height: 24px;margin-top: 3px;"><span style="float:right;font-size:12px;">'+obj.courseApply+'</span><i style="float:right;margin-top:1px;"><img src="/template/school/szu/images/count_people.png"></i></span> </h4>'
			'<p class="auth">';
			if(obj.speakers.length>0){
				str = str + '<a href="/teacher/schooldetail.html?teacherId='+obj.speakers[0].id+'" target="_blank" >'+obj.speakers[0].name+'</a>&nbsp;&nbsp;';
			}
			
			if(obj.speakers.length>0 && obj.speakers[0].school != ''){
				str = str + '(';
				for(var k = 0 ; k < obj.speakers.length;k++){
					str = str + obj.speakers[k].school;
					break;
				}
				str = str + ')';
			}
		
			str = str + '</p>';
			if(obj.provider == "尔雅" ){
				str = str + '<b class="sign"></b>';
			}
			str = str + '</p>';
			if(null != obj.credit && undefined != obj.credit && "" != obj.credit){
				str = str + '<div class = "schoolIndexDiv1"></div>'+
				'<div class = "schoolIndexDiv2">学分：'+obj.credit+'</div>';
			}
			str = str + '</li>';
		}
	}else if(PAGE_TEACHER == data.type){
		var count = 0;
		for(var i = 0;i< data.teachers.length;i++){
			if(data.teachers.length>0){
			var teacher = data.teachers[i];
			count += 1;
			var info = "";
			try{
				info = teacher.info.replace(/<[^>]+>/g,"");
			}catch(e){
				continue;
			}
			
			if(info.length>141){
				info = info.substring(0,141)+"...";
			}
			str = str + '<li id="teacher_'+count+'" ';
			if(i>3){
				str = str + ' style="display:none"';
			}
			str = str + ' class="clearfix">'+
			'<a href="/teacher/schooldetail.html?teacherId='+teacher.teacherId+'" class="photo leftF" title="'+teacher.name+'" target="_blank"><b></b><img src="'+teacher.photo+'" /></a>'+
			'<div class="leftF info_box">'+
			'<p class="name"><a href="/teacher/schooldetail.html?teacherId='+teacher.teacherId+'" title="'+teacher.name+'" target="_blank">'+teacher.name+'</a></p>'+
			'<p class="info">'+info+'</p>'+
            '</div>'+
            '</li>';
          	}
  		}
	}else if(PAGE_EXPERT == data.type){
		for(var i = 0;i< data.experts.length;i++){
			var expert = data.experts[i];
       		var count = i + 1 ;
       		if(count < 11){
				str = str + '<li class="clearfix">'+
				'<a class="img_box" href="#" title="" target="_blank"><img class="img_list" src="/template/school/temp/list.jpg" /></a>'+
				'<h4><b></b><a href="#" title="" target="_blank">中国文化概论</a></h4>'+
				'<div>'+
				'<a href="/expert/detail.html?teacherId='+expert.id+'" class="leftF"><img src="'+expert.photo+'" alt="'+expert.teacherName+'" title="'+expert.teacherName+'"/><b></b></a>'+
				'<div class="rightF js"><span><a href="/expert/detail.html?teacherId='+expert.id+'">'+expert.teacherName+'</a> '+expert.position.substring(0,3)+'...</span>   <span>'+expert.company+'</span></div>'+
				'</div>'+
				'</li>';
            }
		}
	}else if(PAGE_SCHOOL == data.type){
		for(var i = 0;i< data.schools.length;i++){
			if(i<=5){
				var school = data.schools[i];
				 str = str + '<li><a href="http://'+school.hostName+'" class="school_img" title="'+school.name+'" target="_blank"><img src="'+school.schoollogo+'" /></a><h4><a href="http://'+school.hostName+'" title="'+school.name+'"  target="_blank">'+school.name+'</a></h4></li>';
			}
		}
	}
	
	if(PAGE_COURSE == data.type){
		$("#clearfix"+data.num).html(str);
	}
	else if(PAGE_TEACHER == data.type){
		$("#teacher"+data.num).html(str);
	}
	else if(PAGE_EXPERT == data.type){
		$("#expert"+data.num).html(str);
	}
	else if(PAGE_SCHOOL == data.type){
		$("#school"+data.num).html(str);
	}
		}
	})
	</script>
            	    	
    	    	    	    </div>
	</div>
