<script>
var k,m=0;
function opacityChange(){
	if($('.cartoon').eq(m).css('zIndex')<=-1){
		$('.cartoon').eq(m).css('zIndex',$('.cartoon').length)
	};
	$('.cartoon').eq(m).animate({opacity:0},1000,function(){
		$(this).css({'zIndex':-1-m,'opacity':1});
		if(m<$('.cartoon').length-1){
			m++;
		}else if(m>=$('.cartoon').length-1){
			m=0;
			for(var i=0;i<$('.cartoon').length;i++){
				$('.cartoon').eq(i).css({zIndex:$('.cartoon').length-i});				
				}
		}
		$('.index_num li').removeClass('cur');
		$('.index_num li').eq(m).addClass('cur');
	});
};
k=setInterval(opacityChange,3000);
//全部分类
$(".sort").hover(function(){
	$(".sort_box").show();
},function(){
	$(".sort_box").hide();
});
//搜索
$('.search .search_txt').disappearDefault({
    vals:"搜索课程",
    defaultColor:"#fff", 
    color:"#fff",
    showDefault:true
});
//我的课程
var timeoutID = 0;
$(".my_course").hover(function(){
	clearTimeout(timeoutID);
	$(".my_course_box").show();
},function(){
	timeoutID=setTimeout(function(){
		$(".my_course_box").hide();
	},200);
});
var timeoutID1 = 0;
$(".person").hover(function(){
	clearTimeout(timeoutID1);
	$(".my_info").show();
},function(){
	timeoutID1=setTimeout(function(){
		$(".my_info").hide();
	},200);
});
function showmore(){
	$(".course").find('li').each(function(){$(this).show();});
	$("#showmore").html("缩起内容");
	$("#showmore").attr("onclick","hidemore()");
}
function hidemore(){
	$(".course").find('li').each(function(){$(this).hide();});
	for(var i = 1;i < 7;i++){
		$("#course_"+i).show();
	}
	$("#showmore").html("浏览更多课程内容");
	$("#showmore").attr("onclick","showmore()");
}
function showMoreTeacher(){
	$(".teacher").find('li').each(function(){$(this).show();});
	$("#showMoreTeacher").html("缩起内容");
	$("#showMoreTeacher").attr("onclick","hidemMoreTeacher()");
}
function hidemMoreTeacher(){
	$(".teacher").find('li').each(function(){$(this).hide();});
	for(var i = 1;i < 5;i++){
		$("#teacher_"+i).show();
	}
	$("#showMoreTeacher").html("浏览更多老师");
	$("#showMoreTeacher").attr("onclick","showMoreTeacher()");
}
</script>
    
    
    
