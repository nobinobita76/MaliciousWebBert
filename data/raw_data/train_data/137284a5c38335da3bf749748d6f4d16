<script type="text/javascript">
/*<![CDATA[*/
jQuery(function($) {
jQuery('#tupian').after("<a id=\"refresh\" href=\"\/master\/captcha\/refresh\/1\">\u6362\u4e00\u5f20<\/a>");
jQuery(document).on('click', '#refresh', function(){
	jQuery.ajax({
		url: "\/master\/captcha\/refresh\/1",
		dataType: 'json',
		cache: false,
		success: function(data) {
			jQuery('#tupian').attr('src', data['url']);
			jQuery('body').data('captcha.hash', [data['hash1'], data['hash2']]);
		}
	});
	return false;
});

});
/*]]>*/
</script>


