      $('.glyphicon-chevron-left').click(function(){
        var $this=$('.carousel-innercase .item');
        itemleft($this);
      })

    $('.glyphicon-chevron-right').click(function(){
      var $this=$('.carousel-innercase .item');
      itemright($this);
    })

  });

function itemleft($this){
  var leng=$this.length;
  $.each($this, function (i, item) {
       if($this.eq(i).hasClass('active')){
         $this.eq(i).removeClass('active');
        if(i==0){
           i=leng-1;
           $this.eq(i).addClass('active');
           return false; //跳出循环
         }else{
           i=i-1;
           $this.eq(i).addClass('active');
           return false; //跳出循环
         }
       }
  });

}
function itemright($this){
  var leng=$this.length;
  $.each($this, function (i, item) {
    if($this.eq(i).hasClass('active')){
      $this.eq(i).removeClass('active');
      if(leng-1 == i){
        $this.eq(0).addClass('active');
        return false; //跳出循环
      }else{
        i=i+1;
        $this.eq(i).addClass('active');
        return false; //跳出循环
      }
    }
  });

}

function show_comment_img(obj) {
  var order_id = $(obj).find('img').attr('orderid'),
    order = $(obj).find('img').attr('order'),
    aid = $(obj).find('img').attr('rel'),
    support = $(obj).find('img').attr('support'),
    maid='3739437688',
    loginin = "",
    str = '';

  $.ajax({
    url: '/master/ShowCase',
    type: 'post',
    dataType: 'json',
    data: {id: order_id, rel: aid, maid:maid},
    success: function (res) {
      if (res.status == 1) {
        if (res.list.length > 0) {
          $('.carousel-innercase').empty();
          str = '';
          for (var i in res.list) {
            var l = res.list[i];
            if(l.aid==aid){
              str += '<div class="item cd active">';
            }else{
              str += '<div class="item ad">';
            }
            str += '<div class="ds-tab">';
            str += '<span class="ds-tab-cell">';
            str += '<img data-src="holder.js/900x500/auto/#777:#555/text:First slide" alt="" src="'+ l.path+'" max-width="590px" max-height="360" >';
            str += '</span>';
            str += '</div>';
            str += '</div>';
            $('.servertit').html(res.serve_types)
          }
          $('.carousel-innercase').append(str);
          $('.totalimg').html(res.mun);
        }
        if(res.list.length == 1){
          $('.lrimg').removeClass('carousel-control');
          $('.lrimg').addClass('hide');
        }else{
          $('.lrimg').addClass('carousel-control');
          $('.lrimg').removeClass('hide');
        }
      }
    },
    beforeSend: function ()
    {

      $('.carousel-innercase').empty();
      str += '<div class="item cd active">';
      str += '<div class="ds-tab">';
      str += '<span class="ds-tab-cell">';
      str += '<img  alt="" src="http://www.wanshifu.com/assets/images/loading.gif" max-width="590px" max-height="360" >';
      str += '</span>';
      str += '</div>';
      str += '</div>';
      $('.carousel-innercase').append(str);
      $('#showpic').click();
    }
  })
}


  function checkPhone() {
    var phone = $('#uphone').val();
    if (/^(1[34578]\d{9}|(0[1-9]\d{1,2}(\s|\-)?)?[1-9]\d{6,8})$/g.test(phone) === false) {
      $('#phone_err').html('手机号码格式不正确');
      $('#uphone').focus();
      return false;
    } else {
      $('#phone_err').empty();
    }
  }

  function swap(id) {
    var loginin = "";
    if (!loginin) {
      window.location.href = "/member/account/login?from=" + encodeURIComponent(location.href);
      return false;
    }
    var aid = $('#' + id).attr('rel');
    if (!aid) {
      return false;
    }
    $.ajax({
      url: '/master/support',
      type: 'post',
      dataType: 'json',
      data: {aid: aid, type: 'action'},
      success: function (res) {
        if (res.status == 1) {
          $('#' + id).attr('rel', aid).html("<i class=\"fa fa-thumbs-up\"></i>" + res.msg);
        } else {
          alert(res.msg);
        }
      }
    })
  }

  var photoNum = 0;
  function getMasterJobPhoto(id,title)
  {

    var mid = '3739437689';
    $.ajax({
      url: "/caseImg/" + mid + "/" + encodeURIComponent(title) + '/1',
      type: 'get',
      dataType: 'json',
      async: false,
      data: {},
      success: function (res) {
          photoNum = res.number;
        console.log(photoNum);
      }
    })
  }


  function showProvider(obj, id) {
    $('#masterTab>li,.tab-content>div').removeClass('active');
    $('#masterTab>li:eq(2),.tab-content>div:eq(2)').addClass('active').addClass('in');
    var mid = '3739437689';
    var title = $(obj).text() == '完工照片' ? '全部' : $.trim($(obj).text());
    getMasterJobPhoto(id,title);
    var num = $(obj).attr('rel') ? $(obj).attr('rel') : photoNum;
    id = !id ? 'provider_all' : id;
    $('#case_type>a').removeClass('active');
    if (id == 'provider_all') {
      $('#case_type>a:first').addClass('active');
    } else if (id == 'provider_setup') {
      $('#case_type>a:eq(1)').addClass('active');
    } else if (id == 'provider_repair') {
      $('#case_type>a:last').addClass('active');
    }
    if ($('#' + id).html() == '') {
      $(".tab-content").showLoading();
      $("#" + id + '_page').pagination({
        baseUrl: "/caseImg/" + mid + "/" + encodeURIComponent(title) + '/',
        prevHtml: "上一页",
        nextHtml: "下一页",
        prevClass: "prev",
        nextClass: "next",
        activeClass: "active",
        indexClass: "index",
        showNum: 5,
        jumpHtml: "",
        ajaxCallback: "fill",
        totalPage: Math.ceil(num / 12),
        ajaxAutoTrigger: true,
        ajaxCallbackOptions: id
      });
    }


    $('.provider_sub').addClass('hide');
    $('#' + id).removeClass('hide');
    if (num > 0) {
      $('#' + id + '_page').removeClass('hide');
    }
  }
  function fill(res, id) {
    $(".tab-content").hideLoading();
    $('#' + id).empty();
    var str = '';
    //console.log(res.list.length);
    $('#provider').attr('rel', res.number);
    if (res.list.length > 0) {
      for (var i in res.list) {
        if (!isNaN(i)) {
          var l = res.list[i];
          var image = l.thumbs['120x90'];
          str += '<div>';
          str += '<span><a onclick=\'modalShow(' + l.order_id + ',' + l.aid + ')\'   class="' + l.order_id + '" rel="' + l.aid + '"><img src="' + image + '" alt="案例照片"><span>'+l.totalimg+'</span></a></span>';
          str += '<span>' + l.serve_types + '</span>';
          str += '</div>';
        }
      }
    }
    //console.log(str);
    if (!str) {
      $('#' + id).css({height: '40px'});
      str = '<div style="width:100%;text-align:center">暂无图片</div>';
    }
    $('#' + id).html(str);
  }

  function modalShow(id,rel){

    var $this=$(this);
    var str = '';
    var maid='3739437688';
    $.ajax({
      url: '/master/ShowCase',
      type: 'post',
      dataType: 'json',
      data: {id: id, rel: rel,maid:maid},
      success: function (res) {
        if (res.status == 1) {
          if (res.list.length > 0) {
            $('.carousel-innercase').empty();
            str = '';
            for (var i in res.list) {
                var l = res.list[i];
              if (l.path == false) {
                continue;
              }
              if(l.aid==rel){
                str += '<div class="item active">';
              }else{
                str += '<div class="item">';
              }
                str += '<div class="ds-tab">';
                str += '<span class="ds-tab-cell">';
                str += '<img data-src="holder.js/900x500/auto/#777:#555/text:First slide" alt="" src="'+ l.path+'" max-width="590px" max-height="360" >';
                str += '</span>';
                str += '</div>';
                str += '</div>';
              $('.servertit').html(res.serve_types)
            }

            $('.carousel-innercase').append(str);
            $('.totalimg').html(res.mun);

          }
          if(res.list.length == 1){
            $('.lrimg').removeClass('carousel-control');
            $('.lrimg').addClass('hide');
          }else{
            $('.lrimg').addClass('carousel-control');
            $('.lrimg').removeClass('hide');
          }

        }
      },
      beforeSend: function ()
      {
        $('.carousel-innercase').empty();
        str += '<div class="item active">';
        str += '<div class="ds-tab">';
        str += '<span class="ds-tab-cell">';
        str += '<img  alt="" src="http://www.wanshifu.com/assets/images/loading.gif" max-width="590px" max-height="360" >';
        str += '</span>';
        str += '</div>';
        str += '</div>';
        $('.carousel-innercase').append(str);
        $('#showpic').click();
      }
    })


  }




