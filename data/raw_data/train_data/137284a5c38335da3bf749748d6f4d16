  function judge(txt, count) {
    $('#pagination').empty();
    $("#pagination").pagination({
      baseUrl: baseUrl + '/master/comment/type/' + txt + '/master_id/3739437688/ajax/1/page/',
      prevHtml: "上一页",
      nextHtml: "下一页",
      prevClass: "prev",
      nextClass: "next",
      activeClass: "active",
      indexClass: "index",
      showNum: '10',
      totalPage: Math.ceil(count / 10),
      jumpHtml: "",
      ajaxAutoTrigger: true,
      ajaxCallback: "ajaxCommentCallback"
    });
  }
  function ajaxCommentCallback(res) {
    $('#comment-list').empty();
    var score = '', str = '',
      team_name = "秦师傅";
    if (res.list.length > 0) {
      $('#pagination1').show();
      for (var i in res.list) {
        var l = res.list[i];

        score = ((parseInt(l.serve_quality) + parseInt(l.serve_attitude) + parseInt(l.serve_speed)) / (3 * 5)) * 100;
        str += '<ul>';
        str += '<li>';
        str += '<span>' + l.buyer + '（买家）</span>';
        str += '<span><span style="width:' + score + '%"></span></span>';
        str += '<span>' + l.post_time_m + ' </span>';
        str += '<span>';
        str += '<font>质量' + l.serve_quality + '(<font>' + serve_rate_text(l.serve_quality) + '</font>)</font>';
        str += '<font>态度' + l.serve_attitude + '(<font>' + serve_rate_text(l.serve_attitude) + '</font>)</font>';
        str += '<font>速度' + l.serve_speed + '(<font>' + serve_rate_text(l.serve_speed) + '</font>)</font>';
        str += '</span>';
        str += '</li>';
        str += '<li>' + l.content + '</li>';
        str += '<li>服务类型：<font>' + l.serve_types + '</font></li>';
        if (l.atta && l.atta.length > 0) {
          str += '<li>';
          str += '<span>';
          for (var j in l.atta) {
            if (!isNaN(j)) {
              str += '<span class="comment_img"><a onclick="show_comment_img(this)" ><img src="' + l.atta[j].thumbs['120x90'] + '" rel=' + l.atta[j].aid + ' orderid="' + l.order_id + '" support="' + l.atta[j].support + '" order=' + l.atta[j].order + ' style="width:120px;height:90px"></a></span>';
            }
          }
          str += '</span>';
          str += '<font>照片来源：该订单服务师傅（' + team_name + '）提供。</font>';
          str += '</li>';
        }
        if (l.explaination) {
          str += '<li><span>服务商解释：' + l.explaination + '</span></li>';
        }
        str += '</ul>';
      }
      $('#comment-list').html(str);
    } else {
      $('#pagination1').hide();
      $('#comment-list').html('<div style="width:100%;height:50px;line-height:50px;text-align:center">无数据</div>');
    }
  }

  function serve_rate_text($rate) {
    var status = ['-', '很差', '一般', '好', '很好', '非常好'];
    return status[$rate];
  }

</script>

