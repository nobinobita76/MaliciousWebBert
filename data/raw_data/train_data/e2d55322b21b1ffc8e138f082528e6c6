    <!--名片二维码JS--> 
    <script>
function utf16to8(str) {  //解决中文乱码
   var out, i, len, c;  
   out = "";  
   len = str.length;  
   for(i = 0; i < len; i++) {  
   c = str.charCodeAt(i);  
   if ((c >= 0x0001) && (c <= 0x007F)) {  
       out += str.charAt(i);  
   } else if (c > 0x07FF) {  
       out += String.fromCharCode(0xE0 | ((c >> 12) & 0x0F));  
       out += String.fromCharCode(0x80 | ((c >>  6) & 0x3F));  
       out += String.fromCharCode(0x80 | ((c >>  0) & 0x3F));  
   } else {  
       out += String.fromCharCode(0xC0 | ((c >>  6) & 0x1F));  
       out += String.fromCharCode(0x80 | ((c >>  0) & 0x3F));  
   }  
   }  
   return out;  
}  
</script> 
    <script>//此处生成名片二维码（如要生成普通链接二维码 则 "text"参数值 直接替换成普通链接即可）
var the_text = "BEGIN:VCARD \r\nFN:嘉客 \r\nTEL;CELL,VOICE: \r\nTITLE:\r\nORG:网站运维-张工 \r\nEMAIL;INTERNET,HOME: \r\nADR;WORK,POSTAL: \r\nURL:http://luosiadmin.luotuocj.com/ \r\nEND:VCARD";
the_text = utf16to8(the_text);
//alert(the_text);
jQuery('#qrcodeid').qrcode({
width:120,
