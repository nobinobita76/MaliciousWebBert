    //异步加载收藏房源按钮
    function initShouCangHouse() {
        jQuery.ajax({
            url: "/newsecond/mysoufun.aspx?pagetype=initpage&houseid=" + houseID + "&linkurl=" + window.location.href,
            type: "GET",
            success: function (data) {
                var jsons = eval("(" + data + ")");
                if (jsons.result != "" && jsons.result != "-1")//已经收藏过
                {
                    $(".btnbg .star").addClass("star1");
                    $("#sc").html("已收藏");
                    $("#myselectedid").val(jsons.result);
                }
            }
        });
    }

    function shoucang() {
        var sc = document.getElementById("sc");
        var val = sc.innerHTML;
        if (val == "收藏") {
            jQuery.ajax({
                type: "GET",
                cache: false,
                dataType: "text",
                url: "/newsecond/include/DefaultUserLogin.aspx?method=init",
                success: function (msg) {
                    var Json = eval("(" + msg + ")");
                    if (Json.result == "100") {
                        //ajax保存
                        jQuery.get('/newsecond/mysoufun.aspx?returnid=1&userid=' + Json.userid + '&face=http://cdnsfb.soufunimg.com/1/2017_3/23/M19/24/b06b0a540dc54757bda57d62af83399a.jpg&name=%b4%b4%d2%b5%bd%d6%be%ab%d7%b0%b8%b4%ca%bd+%bd%f4%c1%da%b5%d8%cc%fa%bf%da%c1%bd%d6%a4%c1%bd%c4%ea++%cb%e6%ca%b1%bf%b4%b7%bf%b4%f8%c2%b6%cc%a8&address=%b6%ab%ba%fe%d0%c2%bc%bc%ca%f5%bf%aa%b7%a2%c7%f8%b6%ab%d0%c5%c2%b7&price=85&city=%ce%e4%ba%ba&linkurl=http://esf.wuhan.fang.com/chushou/3_162358947.htm&type=esf&pricetype=%cd%f2%d4%aa%2f%cc%d7&compareid=162358947&projurl=http%3a%2f%2fchuangyejiegg.fang.com%2f&fangdai=http%3a%2f%2f8.fang.com%2fjsq%2f%3funit_price%3d11967%26buildingarea%3d71.03%26cshort%3dWUHAN&room=2&hall=2&floor=19&totalfloor=27&forward=%c4%cf&createtime=2003&area=71.03&inserttime=2017/3/23 19:24:19&unitprice=11967&propertype=0&comarea=%b9%e2%b9%c8&district=%b6%ab%ba%fe%b8%df%d0%c2%c7%f8', function (data) {
                            //弹出到我的选房单提示框
                            var jsons = eval("(" + data + ")");
                            if (jsons.result != "房源成功添加到选房单啦" && jsons.result != "你已经添加过该房源了" && isNaN(jsons.result)) {
                                jErpPopup(jsons.result, '/NewSecond/sale_info/boxes/collection.aspx', 240, 120);
                            }
                            else {
                                $(".btnbg .star").addClass("star1");
                                $("#sc").html("已收藏");
                                if (!isNaN(jsons.result)) {
                                    $("#myselectedid").val(jsons.result);
                                }
                            }
                        });
                        //ub 收藏 
                        AddShouCangUB();
                    }
                    else {
                        //弹出登录
                        jErpPopup("您还没有登录，请先登录", '/NewSecond/sale_info/boxes/dboxLogin.aspx?fun=shoucang', 410, 180);
                    }
                }
            });
        }
        else {
            jQuery.ajax({
                url: "/newsecond/mysoufun.aspx?pagetype=cancelshoucang&Myselectid=" + jQuery("#myselectedid").val(),
                type: "GET",
                success: function (data) {
                    var jsons = eval("(" + data + ")");
                    if (jsons.result != "" && jsons.result == "100")//已经收藏过
                    {
                        $(".btnbg .star").removeClass("star1");
                        $("#sc").html("收藏");
                        $("#myselectedid").val("");
                    }
                }
            });
        }
    }
</script>
