        } 
        jErpPopup('议价', ' /newsecond/sale_info/boxes/yijia.aspx?houseid=162358947&agentID=163739376&agentName=陈莎&projCode=2700060410&projName=光谷创业街&room=2&hall=2&buildarea=71.03&type='+type, 420, 350);
