var dfsj_jq = jQuery.noConflict();
                dfsj_jq(document).ready(function() {
                    dfsj_jq('#top-images-areax div').hover(function() {
                        dfsj_jq(this).animate({
                            opacity: 0.8, '-moz-opacity': 0.8, filter: 'alpha(opacity=80)'
                        }, 400);
                    }, function() {
                        dfsj_jq(this).stop(true).animate({
                            opacity: 1, '-moz-opacity': 1, filter: 'alpha(opacity=100)'
                        }, 400);
                    });

                    dfsj_jq('.fl_g.dfsj_flg .fl_icn_g').hover(function() {
                        dfsj_jq(this).find('img').animate({
                            opacity: 0.8, '-moz-opacity': 0.8, filter: 'alpha(opacity=80)'
                        }, 400);
                    }, function() {
                        dfsj_jq(this).find('img').stop(true).animate({
