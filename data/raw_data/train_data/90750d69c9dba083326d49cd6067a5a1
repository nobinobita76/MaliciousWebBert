  <script>
    $(function () {
        var zwwidth =  $(".header-top .container").width();
        var cz = $(".mains-menu").width() -(($(window).width() - zwwidth)/2) ;
        if (cz > 0) {
        $(".currentmenu").css("margin-left", ($(window).width() - zwwidth) / 2);
        $(".currentmenu").width(zwwidth - cz);
        //alert($(".currentmenu").height());
        }
        var mainh=$(".currentmenu").height();
        if(mainh>40){
            $(".moreli").show().on("click",function(){
                    if($("#header-menu").css("overflow")=="hidden" ){
                        $("#header-menu").css("overflow","visible");
                    }else{
                        $("#header-menu").css("overflow","hidden");
                    }
            });

             $("#sort li.active").insertAfter("#sort li:eq(0)");
        }
    })

</script> 
