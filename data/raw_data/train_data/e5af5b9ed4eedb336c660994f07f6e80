    window.onload = function(){
        var ms = document.getElementById("mysteve");
        var add = document.getElementById("add");
        var abc = document.getElementById("all001");
        //$("#all001");
        ms.onmouseover = function(){
            add.style.display = "block";
        };
        abc.onmouseout = function(e){
            
            evt = window.event || e;
            var obj = evt.toElement || evt.relatedTarget;

            var pa = this;
            if(pa.contains(obj)) return false;

            $("#add").hide();
        
        };

    ms.onmouseout = function(e)
    {
       //$("#add").hide();
    }
    
        resizebody();
    };

function resizebody()
{
    var screenwidth = document.body.scrollWidth;
    if(screenwidth <= 1500 && screenwidth >= 800)
    {
        $(".nav-3").addClass("nav-3-small");    
    $(".top-1").addClass("top-1-small");
    $(".content-con").addClass("content-con-small");
    $(".local-1").addClass("local-1-small");
    $(".menue-1").addClass("menue-1-small");
    $(".link-1").addClass("link-1-small");
    $(".foot-content-1").addClass("foot-content-1-small");
    }else
    {
        $(".nav-3").removeClass("nav-3-small"); 
    $(".top-1").removeClass("top-1-small");
    $(".content-con").removeClass("content-con-small");
    $(".local-1").removeClass("local-1-small");
    $(".menue-1").removeClass("menue-1-small");
    $(".link-1").removeClass("link-1-small");
    $(".foot-content-1").removeClass("foot-content-1-small");
    }
}
</script> 
