function SetWinHeight(obj)
{
 var iComment=obj;
 
 if (document.getElementById)
 {
  if (iComment && !window.opera)
  {
   if (iComment.contentDocument && iComment.contentDocument.body.offsetHeight) 

    iComment.height = iComment.contentDocument.body.offsetHeight+60; 
   else if(iComment.Document && iComment.Document.body.scrollHeight)
    iComment.height = iComment.Document.body.scrollHeight+60;
  }
 }
}
function  ShowComment()  {
 var pars;
 var ModuleName = "news";
 var InfoID = "51298";
  
 var Titlelen = "";  //内容摘要长度，默认是20
 var Tablelen = "";  //调用表格宽度，如100%或760
 var MaxPerPage = 10;  //每页调用数量
 pars="ModuleName="+ModuleName+"&InfoID="+InfoID+"&Titlelen="+Titlelen+"&Tablelen="+Tablelen+"&MaxPerPage="+MaxPerPage;
 new Ajax.Updater(
 "leaveMsgList",
 "/Include/PowerEasy.Comment_Ajax.asp?rnd="+Math.random(),
 {
 method:"get",
 parameters  :pars,
 evalScripts :true,
 asynchronous:true
 });
}
function ajaxPager(pars)
{
 new Ajax.Updater(
 "leaveMsgList",
 "/Include/PowerEasy.Comment_Ajax.asp",
 {
 method:"get",
 parameters: pars
 });
}
