<script type="text/javascript">
    $(function () {
        $.post('/getnewservers', function (data) {
            if (!data) {
                $("#ul_server_list").html('<li><span class="q">系统错误</span><span></span></li>');
                return false;
            }
            if (data.status == 1) {
                var list = data.data;
                if (list.length > 0) {
                    var html = '';
                    for (var i = 0; i < list.length; i++) {
                        html += '<li>&nbsp;<span class="q">' + list[i]['server_name'] + '</span>&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;<span>' + list[i]['op'] + '　' + list[i]['first_opentime'] + '</span></li>';
                    }
                    $("#ul_server_list").html(html);
                    setTimeout('$("#scrollDiv").textSlider({line: 1, speed: 500, timer: 3000})',1000);
                }
            }
            else{
                $("#ul_server_list").html('<li><span class="q">数据异常</span><span></span></li>');
                return false;
            }
        }, "json");
    });
</script>
