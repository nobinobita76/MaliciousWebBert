     <script type="text/javascript">
(function()
{
    window.scroll = function()
    {
        var isIE6 = false;
        if(window.navigator.userAgent.indexOf('MSIE 6.0')!=-1)
        {
                isIE6 = true;
        }
        var oLink = document.getElementById('Link');
        var oLinkNone = document.getElementById('Linknone');

        window.onscroll = function()
        {
            var onScrollTop = document.documentElement.scrollTop||document.body.scrollTop;
            if(onScrollTop > oLink.offsetTop)
            {
                if(isIE6)
                {
                    oLinkNone.style.position = 'absolute';
                }
                else
                {
                    oLinkNone.style.position = 'fixed';
                }
                oLinkNone.style.display = 'block';
            }
            else
            {
                oLink.style.position=""
                oLinkNone.style.display = 'none';
            }
            var scrollBottom = $(document).height() - $(window).height() - $(window).scrollTop();
            if(scrollBottom < 135){
                oLink.style.position=""
                oLinkNone.style.display = 'none';
            }
        }
    }
})();
scroll();
$("#Linknone em").mouseover(function () {
    var index = $(this).index();
    $(this).addClass("remen_em").siblings().removeClass("remen_em");
    $("#Linknone").find('.paihang>div').eq(index / 2 - 1).show().siblings().hide()
})
</script> 
