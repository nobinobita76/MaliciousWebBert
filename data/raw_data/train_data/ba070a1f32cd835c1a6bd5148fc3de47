   <script>
var tabsSwiper = new Swiper('#swiper02',{
	speed:500,
	onSlideChangeStart: function(){
		$(".box05 .tabs .active").removeClass('active')
		$(".box05 .tabs a").eq(tabsSwiper.activeIndex).addClass('active')  
	}
})
$(".box05 .tabs a").on('touchstart mousedown',function(e){
	e.preventDefault()
	$(".box05 .tabs .active").removeClass('active')
	$(this).addClass('active')
	tabsSwiper.swipeTo( $(this).index() )
})
$(".box05 .tabs a").click(function(e){
	e.preventDefault()
})
</script> 
