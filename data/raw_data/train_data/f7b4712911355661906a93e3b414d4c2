            success: function(jsonArray) {
                for (var i=0; i<jsonArray.length; i++){
                    var divJson = jsonArray[i];
                    var divId = divJson.divId;
                    if (divJson != null){
                        $('#'+divId).append(divJson.extend);
                        $('#'+divId).append(divJson.content);
                    }
                }
            }
         });
     });</script>        <!--FUWU-10224新品牌广告系统对接-顶部通栏 end-->
        			
