   function dropNav(){
	var $btn=$(".arr"),
		$Mn=$(".nav_u_down"),
		$true=true;
		$Mn.fadeOut();	
		
	  $btn.bind("click",function (e){
		if($true){
			$Mn.fadeIn();
			$(".nav_box").addClass("nav_box1");
			$true=false;
		}else{
			$Mn.fadeOut();
			$(".nav_box").removeClass("nav_box1");
			$true=true;
		}
		e.preventDefault();
		return false;
	});
	$Mn.bind("click" ,function (e){
		$Mn.fadeIn();
		$(".nav_box").addClass("nav_box1");
		$true=false;
		e.stopPropagation();
	});
	$(document).bind("click" ,function (){
		$Mn.fadeOut();
		$(".nav_box").removeClass("nav_box1");
		$true=true;
	});
};

$(function (){
	dropNav();	
});
