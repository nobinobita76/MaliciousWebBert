function ShowJJZQ(item,item2,item3,listSrc)
{
	var jjgg = document.getElementById(item);//取得基金公告
	var zqgg = document.getElementById(item2);
	var qzgg = document.getElementById(item3);
	if(jjgg.id == "itemJjgg"){
		jjgg.className="m_hover";
		zqgg.className="";
		qzgg.className="";
	}
	if(jjgg.id=="itemZqgg"){
		jjgg.className="m_hover";
		zqgg.className="";
		qzgg.className="";
	}
	if(jjgg.id=="itemQzgg"){
		jjgg.className="m_hover";
		zqgg.className="";
		qzgg.className="";
	}
	var list = document.getElementById("listJjzq");
	list.src = listSrc;
}
</script>
