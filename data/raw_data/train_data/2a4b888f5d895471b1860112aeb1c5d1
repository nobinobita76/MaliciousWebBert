<script type="text/javascript">

/*var oldJjzqItem = document.getElementById("itemJjgg");
var oldFontItem = document.getElementById("font1");
oldFontItem.color = "#FFFFFF";

function ShowJJZQ(item,fontItem, listSrc)
{
	oldJjzqItem.className = "mid_boxt";
	oldFontItem.color = "#000000";
	
	item.className = "td7";
	document.getElementById(fontItem).color = "#FFFFFF";
