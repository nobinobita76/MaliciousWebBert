jQuery(document).ready(function(){
jQuery('.deploy-sidebar, .close-sidebar').click(function(){return false;})
    jQuery(".content").click(function(){
        jQuery('.sidebar').animate({
            right: '-270',
        }, 300, 'easeOutExpo', function () {});
    });
    jQuery(".deploy-sidebar").click(function(){
        jQuery('.sidebar').delay(300).animate({
            right: '0',
        }, 300, 'easeOutExpo', function () {});
        return false;
    });
    jQuery(".close-sidebar").click(function(){        
        jQuery('.sidebar').animate({
            right: '-270px',
        }, 300, 'easeInOutExpo', function () {});
        return false;
    });

jQuery('.show-menu').click(function(){
jQuery('#suvbox').fadeIn(100);
jQuery(this).hide();
jQuery('.hide-menu').show();	
return false;
});


jQuery('.hide-menu').click(function(){
jQuery('#suvbox').fadeOut(100);
jQuery(this).hide();
jQuery('.show-menu').show();	
return false;	
});

jQuery('.clos-icon').click(function(){
jQuery('#suvbox').fadeOut(100);
jQuery('.top').hide();
jQuery('.show-menu').show();	
return false;	
});


});


jQuery(function(){  
 var cookie_style = jQuery.cookie("mystyle");
 if(cookie_style==null){
 jQuery("link[title='default']").removeAttr("disabled");
 jQuery("#styles li#default").addClass("cur");
 }else{
 jQuery("link[title='"+cookie_style+"']").removeAttr("disabled");
 jQuery("#styles li[id='"+cookie_style+"']").addClass("cur");
 jQuery("link[title!='"+cookie_style+"']").attr("disabled","disabled");
 }
jQuery("#styles li").click(function(){
var style = $(this).attr("id");
jQuery("link[title='"+style+"']").removeAttr("disabled");
jQuery("link[title!='"+style+"']").attr("disabled","disabled");
jQuery.cookie("mystyle",style,{expires:30});
jQuery(this).addClass("cur").siblings().removeClass("cur");
});
}); 
jQuery(function(){
        jQuery(".toptip").click(function(){jQuery("#toptipbg").show()
setTimeout("jQuery('#toptipbg').hide()",600);
});
});
