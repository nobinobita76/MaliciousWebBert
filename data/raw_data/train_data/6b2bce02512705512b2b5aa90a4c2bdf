  <script type="text/javascript">
	
	function pageDetail(w,h,c,t)
	{
		var pos = document.documentElement.scrollTop || document.body.scrollTop;
		
		var hh = document.documentElement.scrollHeight || document.body.scrollHeight;
		
		var pos1 = document.documentElement.clientHeight;
		
		var pos2 = document.documentElement.clientWidth;
		
		// 遮罩层
		var _allDiv = document.createElement("DIV");
		
		// 结构层
		var _childDiv = $(c);
		
		// 按钮层
		var _btnDiv = $(t);
		
		// 遮罩层样式
		_allDiv.className="allDiv";

		_childDiv.style.display="block";
		_childDiv.style.width = w+"px";
		_childDiv.style.height = h+"px";
		_childDiv.style.top = pos+((pos1-h)*0.5)+"px";
		_childDiv.style.left =((pos2-w)*0.5)+"px";
		_allDiv.style.height=hh+"px";

		document.body.appendChild(_allDiv);
		
		_btnDiv.onclick = function()
		{

				_childDiv.style.display="none";
				document.body.removeChild(_allDiv);
		}
		
	}
	function closebar(){
document.getElementById("tksbar").style.visibility="hidden"
}
</script> 
