  <script>
var xkjs_str="";
var exp_html='<ul id="bibSaveFormat" class="export-formats">';
exp_html+='<li class="option"><input type="radio" checked="" value="1" name="exportSaveFormat">  直接以 RIS 格式导出（例如 CITAVI、EasyBib、EndNote、ProCite、Reference Manager、Zotero）</li>';
exp_html+='<li class="option"><input type="radio" value="2" name="exportSaveFormat">  导出EndNote</li>';
exp_html+='<li class="option"><input type="radio" value="3" name="exportSaveFormat">  导出NoteExpress</li>';	

/**检索用户登录**/
function check_user()
{
	if($.trim($('#user_id').val()) == '')
	{
		$('#user_id').focus();
	}
	else if($.trim($('#user_pwd').val()) == '')
	{
		$('#user_pwd').focus();
	}
	else
	{
		$('#login_buttom').html("登录中...");
		$.ajax({
        cache: true,
        type: "POST",
        url:'control.html',
        data:$('#user_login').serialize(),// 你的formid
        async: false,
        dataType:'json',
        error: function(request) 
		{
            alert("操作失败");
			$('#login_buttom').html("登&nbsp;&nbsp;录");
        },
        success: function(data) 
		{
			$('#login_buttom').html("登&nbsp;&nbsp;录");
			if(data.status == 'ok')
			{
				$('#user_zc_a').hide();
				$('#user_lo_a').hide();
				$('#user_ou_a').show();
								$('#user_sc_articles').html(data.articles_nums);
				$('#myModal_login').modal('hide');
				window.open(document.URL,'_self');
			}
			else
			{
				alert(data.error_message);
			}
		}
		});
	}
}
</script> 
