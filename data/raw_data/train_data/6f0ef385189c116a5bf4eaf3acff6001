	/******打印****/
function doPrint() 
{ 
	bdhtml	=	window.document.body.innerHTML; 
	sprnstr	=	"<!--startprint-->"; //开始打印标识字符串有17个字符
	eprnstr	=	"<!--endprint-->"; //结束打印标识字符串
	prnhtml	=	bdhtml.substr(bdhtml.indexOf(sprnstr)+17); //从开始打印标识之后的内容
	prnhtml	=	prnhtml.substring(0,prnhtml.indexOf(eprnstr)); //截取开始标识和结束标识之间的内容
	window.document.body.innerHTML=prnhtml; //把需要打印的指定内容赋给body.innerHTML
	window.print(); //调用浏览器的打印功能打印指定区域
	window.document.body.innerHTML=bdhtml; // 最后还原页面
}
/**
* 检查用户输入的是否是正确邮箱格式
*/
function emailCheck (emailStr) 
{
	var emailPat=/^(.+)@(.+)$/;
	var matchArray=emailStr.match(emailPat);
	if (matchArray==null) 
	{
		return false;
	}
	return true;
}

/**
* 获取网络的请求参数
*/
Request = {
	QueryString : function(item){
	  var svalue = location.search.match(new RegExp("[\?\&]" + item + "=([^\&]*)(\&?)","i"));
	  return svalue ? svalue[1] : svalue;
	}
}

/**
* 判断用户输入的是否包含中文
*/
function funcChina(value)
{ 
	if(/.*[\u4e00-\u9fa5]+.*$/.test(value)) 
	{ 
		return true; 
	} 
	return false; 
}
/**
* 打开地址
*/
function window_open_url(url){
	window.open(url,'_self');
}

//更多的显示，聚类使用
function change_jl_div(name)
{
	$("#panel"+name).toggle();
	if($("#plus"+name).html()=='更多&gt;&gt;'){
		$("#plus"+name).html('收起&lt;&lt;');
	}else{
		$("#plus"+name).html('更多&gt;&gt;');
	}
}

//返回顶部
(function() {
    var $backToTopEle = $('<div class="backToTop" title="返回顶部"></div>').appendTo($(".r_fixed_div"))
	$('.backToTop').click(function(){
            $("html, body").animate({ scrollTop: 0 }, 120);
    }), $backToTopFun = function(){
        var st = $(document).scrollTop(), winh = $(window).height();
        (st > 0)? $backToTopEle.show(): $backToTopEle.hide();    
        //IE6下的定位
        if (!window.XMLHttpRequest) {
            $backToTopEle.css("top", st + winh - 166);    
        }
    };
    $(window).bind("scroll", $backToTopFun);
    $(function() { $backToTopFun(); });
})();

//返回顶部



//判断字符的长度
function strlen(str){  
    var len = 0;  
    for (var i=0; i<str.length; i++) {   
     var c = str.charCodeAt(i);   
    //单字节加1    
     if ((c >= 0x0001 && c <= 0x007e) || (0xff60<=c && c<=0xff9f)) {   
       len++;   
     }   
     else {   
      len+=2;   
     }   
    }   
    return len;  
}
//截取字符
function getInterceptedStr(sSource, iLen)
{
    if(sSource.replace(/[^\x00-\xff]/g,"xx").length <= iLen)
    {
        return sSource;
    }

    var str = "";
    var l = 0;
    var schar;
    for(var i=0;schar=sSource.charAt(i);i++)
    {
        str += schar;
        l += (schar.match(/[^\x00-\xff]/) != null ? 2 : 1);
        if(l >= iLen)
        {
            break;
        }
    }

    return str;
}
//关键词解释的隐藏与显示
function show_full(){
	$("#full-wikicon").show() ; 
	$("#wikicon").hide() ; 				
}

function hide_full(){
	$("#full-wikicon").hide() ; 
	$("#wikicon").show() 
}
//关键词解释的隐藏与显示

/**
* 详细页控制与显示固定链接的地方
*/
function switchs(id)
{
	$('#expertcontent').hide();
	$('#'+id).toggle();		
}


//检索词解析
function send_url_message(url)
{
	var a=document.createElement("script");
	a.setAttribute("type","text/javascript");
	a.setAttribute("src",url);
	document.body.appendChild(a);
}

/***********载入中浮层*************/
$(document).ready(function(){
	var h = $(document).height();
	$(".overlay").css({"height": h });
});
function do_overlay(){
	$(".overlay").css({'display':'block','opacity':'0.8'});	
	$(".showbox").stop(true).animate({'margin-top':'300px','opacity':'1'},200);
}

/**************摘要****************/
function show_abs(dbid,an,show_key){
	var newan=an.replace(/\./g,"-"); 
	newan=newan.replace(/\+/g,"-");
	if($('#abs_on'+newan).attr('class')!=undefined){
		//$("#abs_"+newan).toggle();
	}else{
		$('#abs_on'+newan).addClass('abs_on');
		//$("#abs_"+newan).show();
		$.ajax({ 
			type:"get", //提交类型 
			url:"?h=show_abs&db="+dbid+'&an='+an+'&s_query='+show_key, //提交页面 
			success:function(msg) { 
				$('#abs_'+newan).html('<strong>Abstract: </strong>'+msg);
			}
		});
	}
}

function show_hide(num,url){
	if(url!=''){
		var google_url='http://demo.findplus.cn/QRcode.php?url='
		$(".img"+num).attr("src",google_url+url);
	}
	var temp= $('.code_'+num).is(":hidden");//是否隐藏
	if(temp==true){
		$('.hidcon').hide();
	}
	$('.code_'+num).toggle();
}

function show_txthtml(content,class_name,an,type){
	var txt_html='';
	if(content.length>1){
		var num=0;
		for(var i=0; i<content.length; i++ ){
			if(an!=content[i]['An'] && ((type=='Book' || type=='eBook') && (content[i]['PubType']=='Book' || content[i]['PubType']=='eBook') || (type=='Academic Journal' || type=='Periodical') && (content[i]['PubType']=='Academic Journal' || content[i]['PubType']=='Periodical'))){
			num=num+1;
			txt_html+='<table><tr>';
			txt_html+='		<td style="vertical-align:top;width:20px;">'+num+'.</td> ';
			txt_html+='		<td style="vertical-align:top;width:100px">';
			txt_html+='			<span title="'+content[i]['PubType']+'" class="pubtype-icon pt-'+content[i]['PubTypeId']+'" style="width:60px;"></span>';
			txt_html+='		</td>';
			txt_html+='		<td style="vertical-align:top">';
			txt_html+='			<div class="titleM">';
			txt_html+='				<a href="?h=articles&db='+content[i]['DbId']+'&an='+content[i]['An']+'" target="_blank">'+content[i]['Title']+'</a>';
			txt_html+='			</div>';
			if(content[i]['author']!=undefined){
				txt_html+='<div><strong>By</strong> : '+content[i]['author']+'</div>';
			}
			if(content[i]['source']!=undefined){
				txt_html+='<div><strong>Source</strong> : '+content[i]['source']+'</div>';
			}
			if(content[i]['subject']!=undefined){
				txt_html+='<div><strong>Subjects</strong> : '+content[i]['subject']+'</div>';
			}
			if(content[i]['pdf_link'] == '1'){
				txt_html+='<div><span><a class="icon link_c" href="?h=down_pdf&an='+content[i]['An']+'&db='+content[i]['DbId']+'" target="_blank"> 阅读全文</a></span></div>';
			}
			if(content[i]['pdf_link'] == '11'){
				txt_html+='<div><span><a class="icon linkdoi" href="?h=down_pdf&an='+content[i]['An']+'&db='+content[i]['DbId']+'" target="_blank"> 链接全文</a></span></div>';
			}
			
			txt_html+='		</td>';
			txt_html+='</tr></table>';
			}
		}
		if(txt_html==''){
			$('.'+class_name).html('系统未收录该文献的其它版本');
		}else{
			$('.'+class_name).html(txt_html);
		}
	}else{
		$('.'+class_name).html('系统未收录该文献的其它版本');
	}
}

//checkbox 全选
$(function(){
	$("#checkall,#checkal2").click(function() {
		$('input[name="selTag[]"]').attr("checked",this.checked);
	});
	var $subBox = $("input[name='selTag[]']");
	$subBox.click(function(){
		$("#checkAll").attr("checked",$subBox.length == $("input[name='selTag[]']:checked").length ? true : false);
	});
});


//获取checkbox值
function getSelect(names)
{
    var vals = "";
    var eles = document.getElementsByName(names);
    for(var i=0;i<eles.length;i++)
    {
		if(eles[i].checked){vals+=eles[i].value+",";}
    }
    vals = vals.length>0?(vals.substring(0,vals.length-1)):("");
	return vals;
}

//导出
function file_export(type,tag){
	if(tag== undefined || tag== 'undefined'){tag='en';}
	var ids=getSelect('selTag[]');
	if(ids==''){
		alert('请选择需要导出的文章');
	}else{
		var url='control.html?control_type=export_list&types='+type+'&pt_tag='+tag+'&selTag='+ids
		window.open(url,'_self');
	}
}
//导出下拉
$(".export_xl .option").mouseover(function() {
	$(this).parent().find("ul.subnav").slideDown(0).show(); 
	$(this).parent().hover(function() {
	}, function(){	
		$(this).parent().find("ul.subnav").slideUp(0); 
	});
});

/******导出功能******/
function show_local_export(cache_aid,tag){
	$('#permalinkDialog').hide();
	if($('#expert_cont').text()==''){
		$('#expertcontent').show();
		
		exp_html+='<div class="panel-footer">';
		exp_html+='<input type="submit" onclick="do_local_export(\''+cache_aid+'\',\''+tag+'\')" class="export_button export-panel-action" value="Save">';
		exp_html+='<input type="submit" onclick="$(\'#expertcontent\').hide()" class="export_button export-panel_cancel" value="Cancel">';
		exp_html+='</div>';
		exp_html+='</ul>';
		$('#expert_cont').html(exp_html);
	}else{
		$('#expertcontent').toggle();
	}
}


function do_local_export(cache_aid,tag){
	if(tag== undefined || tag== 'undefined'){tag='en';}
	var selval=getSex('exportSaveFormat');
	var urls='control.html?control_type=export_list&types='+selval+'&pt_tag='+tag+'&selTag='+cache_aid;
	window.open(urls,'_self')
}
/******导出功能******/	var articles_sc_titles		= 'urseew';
var articles_remove_titles	= 'urseew bpchange';
/**
* 用户收藏文件的总入口
*/
function user_articles_control(sid,tag)
{
	if($('#user_ou_a').css('display') != 'none')
	{
		var newan=sid;
		var tmp_v = $('#folder_'+newan).attr('class');
		if(tmp_v == articles_remove_titles)
		{
			del_user_articles(sid);
		}
		else if(tmp_v == articles_sc_titles)
		{
			dosave(sid,tag);
		}
	}
	else
	{
		$('#myModal_login').modal('show')
	}
}
//////////////////////////////////////////////////////////
/**
* 发送删除文献请求
*/
function del_user_articles(sid)
{
	$.ajax({
	  type: 'GET',
	  url: 'control.html?control_type=do_folder',
	  data: {'sid':sid,'type':3},
	  cache:false, 
	  timeout:4000,
	  success: del_user_articles_back,
	  dataType: 'json',
	  error:del_user_articles_error
	});
}
/**
* 删除成功返回
*/
function del_user_articles_back(data,textStatus)
{
	var newan=data.an_value;
	if(data.status == 'ok')
	{
		//$('#folder_'+newan).attr({"class":articles_sc_titles});
		attr_class(newan,articles_sc_titles)
	}
	else
	{
		//$('#folder_'+newan).attr({"class":articles_remove_titles});
		attr_class(newan,articles_remove_titles)
	}
	$('#user_sc_articles').html(data.save_articles_nums);
}

/**
* 删除错误返回
*/
function del_user_articles_error(XMLHttpRequest, textStatus, errorThrown)
{

}
/////////////////////////////////////////////////////////////////
/**
* 发送获取用户收藏文章列表请求
*/
function get_user_sc_articles_lists()
{
	$.ajax({
	  type: 'GET',
	  url: 'control.html?control_type=do_folder',
	  data: {'type':2},
	  cache:false, 
	  timeout:4000,
	  success: user_sc_articles_lists_return_back,
	  dataType: 'json',
	  error:save_articles_error
	});
	
}
/**
* 处理用户的收藏列表
*/
function user_sc_articles_lists_return_back(data,textStatus)
{
	if(data.save_articles_lists.length>0)
	{
		var tmp_v = data.save_articles_lists.split(',.');
		for(i=0;i<tmp_v.length;i++)
		{
			var newan=tmp_v[i];
			//$('#folder_'+newan).attr({"class":articles_remove_titles});
			attr_class(newan,articles_remove_titles)
		}
		$('#user_sc_articles').html(tmp_v.length);
	}
}
/////////////////////////////////////////////////////////////////
/**
* 发送文献收藏请求
*/
function dosave(sid,tag)
{
	var newan=sid;
	$.ajax({
	  type: 'GET',
	  url: 'control.html?control_type=do_folder',
	  data: {'sid':sid,'tag':tag,'type':1},
	  cache:false, 
	  timeout:4000,
	  success: save_articles_back,
	  dataType: 'json',
	  error:save_articles_error,
	  beforeSend:function(){},
	  complete:function(){}
	});
}

/**
* 保存文献成功返回
*/
function save_articles_back(data,textStatus)
{
	var newan=data.an_value;
	if(data.status == 'ok')
	{
		//$('#folder_'+newan).attr({"class":articles_remove_titles});
		attr_class(newan,articles_remove_titles)
	}
	else
	{
		attr_class(newan,articles_sc_titles)
		//$('#folder_'+newan).attr({"class":articles_sc_titles});
	}
	$('#user_sc_articles').html(data.save_articles_nums);
}

/**
* 保存文献系统出错
*/
function save_articles_error(XMLHttpRequest, textStatus, errorThrown)
{

}

//显示收藏夹内容
function attr_class(newan,class_name){
	$('#folder_'+newan).attr({"class":class_name});
	if(foldertag=='search_list'){
		//列表页显示样式
		if(class_name == 'urseew'){
			$('#folder_'+newan).attr({"title":'放入收藏夹'});
		}else{
			$('#folder_'+newan).attr({"title":'取消收藏'});
		}
	}else if(foldertag=='articles'){
		//详细页显示样式
		if(class_name == 'urseew'){
			$('#folder_'+newan).html('<img src="public/img/f_a.jpg">放入收藏夹');
		}else{
			$('#folder_'+newan).html('<img src="public/img/f_o.jpg">取消收藏');
		}
	}
}	
//获取馆藏信息的处理
function get_opac_holdings_information(marc_no,zb)
{
	if($('#gc_on'+zb).attr('class')!=undefined){
		$("#gc_"+zb).toggle();
	}else{
		$('#gc_on'+zb).addClass('gc_on');
		$('#gc_'+zb).html('馆藏信息载入中，请稍后');
		$.ajax({ 
			type:"get",
			cache:false,     
			dataType:'json',
			url:"widgets/findplus_opac/book_message.html?control=search_opac_detail&action=user_check&type=holdings&marc_no="+encodeURIComponent(marc_no)+"&zb="+zb, 
			success:get_opac_holdings_information_success,
			error:get_opac_holdings_information_error
		}); 
	}
}

/**
* 成功获取馆藏信息
*/
function get_opac_holdings_information_success(data)
{
	var disList = data.disList;//头部
	var res		= data.res;//信息
	var cell	= data.cell;//宽度
	var txtHtml = '';

	if(res == null)
	{
		txtHtml +='无法获取馆藏信息';
	}
	else
	{
		txtHtml +='<table width="100%" cellspacing="1" cellpadding="2" border="0" bgcolor="#d2d2d2">';
		//头部
		txtHtml +='<tr align="center">';
		for(var i=0;i<disList.length;i++)
		{
			txtHtml +='	<td bgcolor="#eeeeee"  class="greytext1">'+disList[i]+'</td>';
		}
		txtHtml +='</tr>';
		//头部
		
		
		for(var i=0;i<res.length;i++)
		{
			txtHtml +='<tr align="center">'; 
			for(var j=0;j<res[i].length;j++)
			{
				txtHtml +='<td width="'+cell[j]+'%" bgcolor="#FFFFFF"  class="whitetext">'+res[i][j]+' </td>';
			}
			txtHtml +='</tr>';
		}
		txtHtml+='</table>';
	}
	$('#gc_'+data.zb).html(txtHtml);		
}


/**
* 失败获取馆藏信息
*/
function get_opac_holdings_information_error()
{

}

//获取馆藏信息的处理



/***被引次数**/
function get_atr_cated(){
	for(var key in doi_arr){
		$.ajax({
			type	:	'get',
			url	:	'control.cx',
			data	:	{'control_type':'cated_wos_catch',doi:doi_arr[key],resultid:key,rand:Math.random()},
			success: function(msg){
				var data=eval('('+msg+')');
				if(data.wos_citedcount==0){
					//var innerhtml= 'WoS Cited: '+data.wos_citedcount+' &nbsp;&nbsp; Scopus Cited: '+data.citedcount
					var innerhtml= 'WoS Cited: '+data.wos_citedcount;
				}else{
					//var innerhtml= '<a href="'+data.wos_url+'" target="_blank">WoS Cited: '+data.wos_citedcount+'</a> &nbsp;&nbsp; Scopus Cited: '+data.citedcount
					var innerhtml= '<a href="'+data.wos_url+'" target="_blank">WoS Cited: '+data.wos_citedcount;
				}
				$('#cated_'+data.resultid).html(innerhtml)
				$('#cated_'+data.resultid).addClass('custom-link')
			}
		});
	}
}


