      q('nav_hp').className = 'hover';
    } else if (pathname.indexOf('/sj/zt/') >= 0) {
      q('nav_zt').className = 'hover';
    } else if (pathname.indexOf('/h5/') >= 0) {
      q('nav_h5').className = 'hover';
    } else if (pathname.indexOf('/azg/xin/') >= 0) {
      q('nav_azg_xin').className = 'hover';
    } else if (pathname.indexOf('/azg/ph/') >= 0) {
      q('nav_azg_ph').className = 'hover end';
    } else if (pathname.indexOf('/azg/') >= 0) {
      q('nav_azg').className = 'hover';
    } else if (pathname.indexOf('/azs/') >= 0) {
      q('nav_azs').className = 'hover';
    }
  })();</script>
