            wf.related.init();
            var changeText = function () {
                this.text(this.text() === '展开' ? '收起' : '展开');
            }
            $('.abstract .expend').click(function () {
                $(this).toggleClass('expended').next('.text').slideToggle(300);
            });
            $('.abstract-short .expend-short').click(function () {
                var $this = $(this),
                    $text = $this.find('span');
                changeText.call($text);
                $this.parent().prev('.row').toggle();
                $this.toggleClass('expended').prev('.text').toggle();
            });
            $('.related .expend').click(function () {
                var $this = $(this),
                    $text = $this.find('.text'),
                    $hd = $this.toggleClass('expended').parent().parent();
                if (!$hd.hasClass('wrap-expend')) { $hd.addClass('wrap-expend'); }
                $hd.toggleClass('wrap-expended').next('.bd').slideToggle(300);
                changeText.call($text);
            });
        })();
