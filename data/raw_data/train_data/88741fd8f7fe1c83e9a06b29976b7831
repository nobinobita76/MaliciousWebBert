<script type="text/javascript">
         var h_random = Math.random();
            //链接加随机数
            $("#aCollect").click(function () {
                clickact("wap_head_usercenter_attention");
                window.location.href = "http://m.taoche.com/buycar/attention.aspx?r=" + h_random;
            })
