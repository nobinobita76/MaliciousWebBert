            })

            //format
      String.prototype.format = function (obj) {
            var that = this;
            if (typeof obj == "object") {
                for (var attr in obj) {
                    var regAttr = eval("/\\{" + attr + "\\}/ig");
                    that = that.replace(regAttr, obj[attr]);
                }
            } else {
                //把参数放到数组里面 slice() 方法可从已有的数组中返回选定的元素。
                var arr = [].slice.call(arguments, 1);
                //调用自己
                return format(that, arr);
            }
            return that;
        }
            $(function () {
                  var moveEnd = function () {//禁止toucheMove滚动
                    return !1;
                  };
                 $("#menuid").click(function () {
                        var menu_layerId = $("#menu_layerId");
                        if (menu_layerId.hasClass("menu-transition")) {
                            menu_layerId.removeClass('menu-transition');
                            $("#banner_mask").hide();
                            $("body").unbind("touchmove",moveEnd());//接触禁用滚动限制
                        } else {
                            menu_layerId.show().addClass('menu-transition');
                            $("body").on("touchmove",moveEnd());//禁止滚动
                            $("#banner_mask").show();
                        }
                    });

                    $(".header-mask").click(function () {
                                 $("#menu_layerId").removeClass('menu-transition'); $("#banner_mask").hide();
                                 $("body").unbind("touchmove",moveEnd());//接触禁用滚动限制
                 })
       //非帮忙城市我要贷款链接
                if (!(bangmaiCity.indexOf("," + HeaderFunction.currentAreaId + ",") > -1)) {
                    $("#liDaikuan").attr("href", "http://cooper.m.taoche.com/mtaoche/chedai/?frompage=1");
                }
      //APP下载
    $("#divNavAppDownload").click(function(){
        DownloadApp.appDownload("headermask");
    })
            });

