  <script type="text/javascript">jQuery(document).ready(function () {
    $('#page').click(function(){
        var inputVal = $("input[id='pageNum']").val();
        inputVal = inputVal.replace(/(^\s*)|(\s*$)/g, "");
        if(!inputVal){
            return false;
        }
        var re = /^[1-9]+[0-9]*]*$/;
        if (!re.test(inputVal))  
        {  
            alert("请输入正整数");  
            return false;  
        } else {
            if ('article' == 'article' && 'search' == 'search') {
                location.href = '/search.html?key=路由器&page='+inputVal;
            } else {
                location.href = '/article/page/'+inputVal+'.html';
            }
        }  
    });

    $('#page').mouseover(function(){
        $(this).css('cursor','pointer');
        $(this).css('background','green');
    });

    $('#page').mouseout(function(){
        $(this).css('background','#b4b4b4');
    });
   $(".list-searchbox").delegate("#search-text","keyup",function(){
        var value = $(this).val();
        $.get("/expanded.html?key="+value,function(result){
            if(result.status == 1)
            {
                var html = '';
                for (var key in result.data) {
                    html += '<li><a href="/search.html?key='+ result.data[key] +'">'+ result.data[key] + '</a></li>';
                }
                $(".history-searchbox").show().find("ul").html(html);
            }
        },'json');
   });

   $("body").has("#search-text").parents("*").click(function(){
      $(".history-searchbox").hide();
   });


    $(".navhd li").hover(function(){
        var index = $(this).index();
        $(".navhd li").removeClass("on");
        $(this).addClass("on");
        $(".content").hide();
        $(".content"+ (index+1)).show();
    });

    $("body").on("click",".dk-select ul li",function() {
        xz = $(this).html();
        $(this).parent().parent().prev().val(xz);
        $(this).parent().parent().hide();
        $(this).parent().parent().prev().addClass("xunzhong");

    })

    $(".xbie").click(function() {
        $(this).parent().find(".sldown").toggleClass("slup");
        $(this).parent().find(".dk-select").stop(true, true).slideToggle();
        $(this).trigger("blur");
    });

    $("#d1").mouseleave(function() {
        $(this).find(".dk-select").stop(true, true).slideUp();
        $(this).find(".sldown").removeClass("slup");
        if (!$(this).find("input.xbie").hasClass("xunzhong")) {
            $(this).find(".xbie").val("请选择电脑类型");
        }
    })

    $("#d2").mouseleave(function() {
        $(this).find(".dk-select").stop(true, true).slideUp();
        $(this).find(".sldown").removeClass("slup");
    })

    window.onload = function (){
        if($("#banner").length > 0){
            $(".lgre-header").css({"background":"none"});
        }
    }
    tab(".ranklistbox .title",".ranklist-details");
    function tab(contex,contex1){
        $(contex + " ul li").click(function(){
            $(this).addClass("on").siblings().removeClass("on");
            var index = $(this).index();
            $(contex1 + " div.box").eq(index).show().animate({"opacity":1},500).siblings().animate({"opacity":0},500).hide();
        });
    }
//cookie判断banner图存在一天
/* cookie扩展 */
MyCookie = {
    /**
     * 设置cookie
     * @param name 名称
     * @param value 值
     * @param expires 有效时间（单位：小时）（可选）
     */
    set: function(name, value, expires) {
        var expTimes = expires * 1000; // 毫秒
        var expDate = new Date();
        expDate.setTime(expDate.getTime() + expTimes);
        var expString = expires ? '; expires=' + expDate.toUTCString() : '';
        var pathString = '; path=/';
        document.cookie = name + '=' + encodeURI(value) + expString + pathString;
    },
    /**
     * 读cookie
     * @param name 名称
     */
    get: function(name) {
        var cookieStr = '; ' + document.cookie + '; ';
        var index = cookieStr.indexOf('; ' + name + '=');
        if (index != -1) {
            var s = cookieStr.substring(index + name.length + 3, cookieStr.length);
            return decodeURI(s.substring(0, s.indexOf('; ')));
        } else {
            return null;
        }
    },
    /**
     * 删除cookie
     * @param name 名称
     */
    del: function(name) {
        var exp = new Date(new Date().getTime() - 1);
        var s = this.get(name);
        if (s != null) {
            document.cookie = name + '=' + s + '; expires=' + exp.toUTCString() + '; path=/';
        }
    }
};

var sd,a=0;
if($(".message a").attr('class') == 'newmsg' || $(".message a").attr('class') == 'newmsg c-red')
{
    sd = setInterval(function() {
        if ($(".newmsg").attr('class') == 'newmsg c-red') {
            $(".newmsg").removeClass("c-red");
        } else {
            $(".newmsg").addClass("c-red");
        }
        a++;
    }, '500');

}
else
{
    clearInterval(sd);
}


 //动态添加页面顶部banner
(function() {
    /*var close = $('<div class="close png-fixIe6"></div>');
    var headerBanner = $('<div class="header-banner"><a></a></div>');
    headerBanner.append(close).insertAfter($('.headerTop-wrap'));
    headerBanner.css({
        height: 0
    });*/
    setTimeout(function() {
        $(".header-banner").animate({
            height: '110px'
        }, 'slow');
    }, 300);
    $(".close").click(function(e) {
        e.preventDefault ? e.preventDefault() : e.cancelBubble = false;
        $(this).parent().hide();
        MyCookie.set('banner', 123, 24 * 60 * 60);
    })
})();


//cookie判断
if (MyCookie.get('banner') == 123) {
    $(".header-banner").css({ display: 'none' });
} else {
    $(".header-banner").css({ display: 'block' });
}

