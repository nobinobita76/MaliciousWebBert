		var tabIndex = 0;
		var hasViewGuide = 0;
		$(".code").bind("focus",function(){
        var val=$.trim($(this).val());
        if(val===$(this).attr("tip")){
            $(this).val("");
            $(this).css("color","#333");
        }
	    }).bind("blur",function(){
	        var val= $.trim($(this).val());
	        val=(val===$(this).attr("tip"))?$(this).val(""):val;
	        if(val===""){
	            $(this).val($(this).attr("tip"));
	            $(this).css("color","#999");
	        }else{
	            $(this).val(val);
	            $(this).css("color","#333");
	        }
	    })
		/*登录中英文面板切换*/
		$(".loginbox > .tabPanel > .tab").each(function(index, element) {
		    $(this).click(function()
			{
				$(this).parent().children().removeClass("focus");
				$(this).addClass("focus");
				$(".loginbox > .contPanel").children().hide();
				$(".loginbox > .contPanel").children().eq(index).show();
			})
		});
		
		/*新闻列表切换面板*/
		$(".newsPanel > .tabArea >.item").each(function(i, element) {
		    $(this).hover(function(){
				var index=$(this).index();
				$(this).parent().children().removeClass("focus");
				$(this).addClass("focus");
				$(".newsPanel > .contArea").children().hide();
				$(".newsPanel > .contArea").children().eq(index).show();
				
				tabIndex = i;
			});
		});
	
		//中部图片切换效果
		var autoPlaySwitchPanelFlag=true;
		$(".switchPanel").find(".ctrlpanel").children().each(function(index, element) {
		    $(this).click(function()
			{
				autoPlaySwitchPanelFlag=false;
				var owidth=$(".switchPanel").width();
				 var picdom=$(this).parent().parent().children().eq(0).children().eq(0); 
				 picdom.stop(true,false).animate({left:-index*owidth},500,function()
				 {
					 autoPlaySwitchPanelFlag=true;
			     });
				 $(this).parent().children().removeClass("focus");
				 $(this).addClass("focus");
			});
		});
		//中部图片自动播放
		setInterval(function()
		{
			if(autoPlaySwitchPanelFlag!=true){
				return;
			}
			var itemwidth=$(".switchPanel").width();
			var maxLeft=itemwidth*parseInt($(".switchPanel").find(".innerBox").children().length-1);
			var currentLeft=$(".switchPanel").find(".innerBox")[0].offsetLeft;
			if(-(currentLeft-itemwidth)>maxLeft)
			{
				 $(".switchPanel").find(".innerBox").stop(true,false).animate({left:0},500);
			}
			else
			{
				 $(".switchPanel").find(".innerBox").stop(true,false).animate({left:currentLeft-itemwidth},500);
			}
			var index=(-(currentLeft/itemwidth)+1)%3;
			$(".switchPanel").find(".ctrlpanel").children().removeClass("focus");
			$(".switchPanel").find(".ctrlpanel").children().eq(index).addClass("focus");
		},5000)
	
		
		
		
		//供应商产品图片自动播放
		function gysProducts()
		{
			var imgCount=$(".gysproducts .gysimgPanel").children().length;
			var imgWidth=$(".gysproducts .gysimgPanel").parent().width();
			$(".gysimgPanel").append($(".gysimgPanel").html());
			if(imgCount<=1){return;}

			var currentIndex=0;
			var AutoPlay=function()
			{
				currentIndex++;
				$(".gysimgPanel").animate({left:-imgWidth*currentIndex},500,function()
				{
					if(currentIndex>=imgCount)
					{
						currentIndex=0;
						$(".gysimgPanel")[0].style.left="0px";
					}
				})
			}
			setInterval(function(){
				AutoPlay();
			},4700)
		}
		
		function viewGuide(id){
			$("#attachs").attr("src", "queryAttachs.do?type=0&id=" + id);
			if(0 == hasViewGuide){
				var left = (document.body.offsetWidth - 500)/2;
				var top = (document.body.offsetHeight - 200)/2 + document.body.scrollTop;
				$(".dialog").css({"cursor":"move", "position":"fixed", "left": left}).show();
				hasViewGuide = 1;
			} else {
				$(".dialog").show();
			}
			
			return false;
		}
		$(".dialog .header").mousedown(function(event)
			{
				var startX=mouseX(event);
				var startY=mouseY(event);
				var msgOffset=$(".dialog").offset();
				
				
				$(document).mousemove(function(event)
				{
					var endX=mouseX(event);
					var endY=mouseY(event);
					var endLeft= msgOffset.left+(endX-startX);
					var endTop= msgOffset.top+(endY-startY);
					$(".dialog").css({left:endLeft,top:endTop});
				});
				
				
				$(document).mouseup(function(event)
				{
					var endX=mouseX(event);
					var endY=mouseY(event);
					var endLeft= msgOffset.left+(endX-startX);
					var endTop= msgOffset.top+(endY-startY);
					$(".dialog").css({left:endLeft,top:endTop});
					$(document).off("mouseup");
					$(document).off("mousemove");
				});
	
			
				//$(this).closest(".msgOuterBox").;
			});
		
		//登录的“回车”提交
		function onKeyUp(e, flag) {
			if (!e) var e = window.event;
	       	if (e.keyCode == 13) {
	           	e.returnValue=false;
	           	e.cancel = true;
	           	if(flag == 1) {
	              	login1();
			   	} else {
	              	login2();
			   	}
	       	}
		}
		
		function login1(){
			document.cookie = "local=zh_CN";
			var username = $.trim($("#username1").val());
			if("" == username){
				$("#loginTip1").html("账号不能为空");
				return;
			}
			var password = $("#password1").val();
			var passwordJ = null;
			if(password==""){
				$("#password1").focus();
				return false;
			}
			var verification = $("#verification").val();
			if(verification=="" || verification=="请输入验证码"){
				$("#verification").focus();
				return false;
			}
			passwordJ = password;
			password = CryptoJS.MD5(password).toString();
			doLogin(username, password,passwordJ,verification,"loginTip1");
		}	
		
		function login2(){
			var username = $.trim($("#username2").val());
			if("" == username){
				$("#loginTip2").html("Nub can not be empty");
				return;
			}
			var password = $("#password2").val();
			var passwordJ = null;
			if(password==""){
				$("#password2").focus();
				return false;
			}
			var verification = $("#verification2").val();
			// 2016/6/22 苗庆伟：英文界面输入密码时，赋值错误,导致取不到密码
			passwordJ = password;
			password = CryptoJS.MD5(password).toString();
			//英文界面登录使用国际化  modify by 李中伟 20160623
			document.cookie = "local=en_US";
			doLogin(username, password,passwordJ,verification,"loginTip2");
		}	
		
		function doLogin(username, password,passwordJ,verification,tipEid){
			var sid = getCookie("sid");
			if("请输入验证码" == verification){
				verification="";
			}
			$.ajax({
				url:"../f/CommonAction!encrypt.do",
				type: "post",
				async: false,
				data: {"str":passwordJ},
				success: function(res){
					passwordJ=res;
				},
				error: function(XMLHttpRequest, textStatus, errorThrown){
					mini.alert("系统繁忙，请检查网络情况并重试，status=" + textStatus);
					return;
				}
			});
			
			//alert(document.cookie);
			$.ajax({
				url:"login.do",
				type: "post",
				async: false,
				data: {"username":username, "password":password,"check":passwordJ,"verification":verification,"sid":sid},
				dataType: "json",
				success: function(res){
					if(res.status == 200){
						if (res.result == 0){
							window.location.href = "../desktop/";
						} else if(res.result == 100){
							mini.open({
							url:"modifyPassWord.jsp",
							title: "修改密码",
		       	            width: 740,
		                    height: 420,
		                    allowResize: true,       
		                    allowDrag: true,         
		                    showCloseButton: true,   
		                    showMaxButton: false,     
		                    showModal: true
							});
						}else if (res.result == 1||res.result == 201) {
                            //$("#" + tipEid).html("<font color=\"red\">" + res.message + "</font>");
						    mini.alert(res.message, "系统提示",function(){
						    window.location.href = "editArchives.do";
						});
						} else if (res.result == 2) {
                            /*供应商未完成注册跳转易派客*/
                            $("#authId").val(username);
                            $("#passwordA").val($("#password1").val());
                            $("#callBack").val("");
                            $("#loginurl").val("");
                            $("#form3").submit();
						}
					} 
					else {
						getVerfCode();
						mini.alert(res.message, "系统提示");
					}
				},
				error: function(XMLHttpRequest, textStatus, errorThrown){
					mini.alert("系统繁忙，请检查网络情况并重试，status=" + textStatus);
				}
			});
		}
		
		function cancel1(){
			$("#username1").val("");
			$("#password1").val("");
		}
		
		function cancel2(){
			$("#username2").val("");
			$("#password2").val("");
		}
	
		/**
		 * 根据Cookie名称获取值，如果不存在则返回null
		 * @param name cookie名称
		 */
		function getCookie(name){  
		    var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)")); 
			return (arr != null) ? arr[2]:null;
		}   
	
		$(function(){
			//处理友情链接选择事件
			$("#links").change(function(){
				var url = $("#links option:selected").val();
				window.open(url);
			});
			
			//处理注册指南附件窗口关闭事件
			$(".close").click(function(){
			    $(".dialog").hide();
			});
			
			//处理Tab页的更多链接事件
			$("#tabMore").click(function(){
				switch(tabIndex){
					/*case 2: 
						window.open("../f/supp/bid/initQuerySbmtBidConsigns.do");
						break; */
					case 0: //招标公告 xyw
						window.open("../f/supp/bid/bidNoticeList.do?type=10");
						break;
					case 1: //资格预审公告
						window.open("../f/supp/bid/bidNoticeList.do?type=20");
						break;
					case 2://独家采购公示 xyw
						window.open("../f/supp/bid/onlypublicitybillList.do");
						break;
					case 3: 
						window.open("../product/queryProdCtgrs.do");
						break; 
					case 4: 
						window.open("http://wz.sinopecnews.com.cn/");
						break; 
					default: break; 
				}
			});
			
			setCookie("sid", Math.uuid(), null, null, "/", null);
			gysProducts();
		});
		
		
		
