<script type="text/javascript">
setTimeout('setsdcjs()',0);
function setsdcjs(){
    var _wtt = _wtt || [];
    (function() {
		var wt = document.createElement("script");
		wt.src = "//www.csair.com/headfoot/js/sdc_gd.js";
		var s = document.getElementsByTagName("script")[0]; 
		s.parentNode.insertBefore(wt, s);
    })();
}

$(document).ready(function () {
	if(!document.cookie == ''){ 
		var arrCookie = document.cookie.split('; '); 
		//用spilt('; ')切割所有cookie保存在数组arrCookie中 
		var arrLength = arrCookie.length; 
		for(var i=0; i<arrLength; i++) { 
						
			if(arrCookie[i].split('=')[0]=='t' && arrCookie[i].split('=')[1]=='fromClient'){
							//alert(22);
				$(".cz-title").eq(0).addClass("am-hide"); // 追加样式
				$(".cz-title-div:eq(0)").addClass("am-hide"); // 追加样式
				//$(".cz-title-div").addClass("am-hide")
				$(".cz-nul:eq(0)").addClass("am-hide"); // 追加样式
				$(".fromClient_mar").css("margin-top","0");
				$(".fromClient_hei").css("height","0");
				break;
			}
		}
	} 
	
});
</script>
