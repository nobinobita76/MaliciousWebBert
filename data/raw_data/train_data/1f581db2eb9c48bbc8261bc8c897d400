   <script>
			    $(document).ready(function(){
			        $.get("http://www.yibu777.com/member/cart/getnumber/"+ new Date().getTime(),function(data){
			            $("#sCartTotal").text(data);
			        });
			        slide();
					// 活动倒计时（样式在js内）
					// newTime ();
			    });
			$(function(){
				$("#sCart").hover(
					function(){
						$("#sCartlist,#sCartLoading").show();
						$("#sCartlist p,#sCartlist h3").hide();
						$("#sCart .mycartcur").remove();
						$("#sCartTotal2").html("");
						$("#sCartTotalM").html("");
						$.getJSON("http://www.yibu777.com/member/cart/cartheader/="+ new Date().getTime(),function(data){
							$("#sCart .mycartcur").remove();
							$("#sCartLoading").before(data.li);
							$("#sCartTotal2").html(data.num);
							$("#sCartTotalM").html(data.sum);
							$("#sCartLoading").hide();
							$("#sCartlist p,#sCartlist h3").show();
						});
					},
					function(){
						$("#sCartlist").hide();
					}
				);
				$("#sGotoCart").click(function(){
					window.location.href="http://www.yibu777.com/member/cart/cartlist";
				});
			})
			function delheader(id){
				var Cartlist = $.cookie('Cartlist');
				var info = $.evalJSON(Cartlist);
				var num=$("#sCartTotal2").html()-1;
				var sum=$("#sCartTotalM").html();
				info['MoenyCount'] = sum-info[id]['money']*info[id]['num'];
				delete info[id];
				//$.cookie('Cartlist','',{path:'/'});
				$.cookie('Cartlist',$.toJSON(info),{expires:30,path:'/'});
				$("#sCartTotalM").html(info['MoenyCount']);
				$('#sCartTotal2').html(num);
				$('#sCartTotal').text(num);
				$('#btnMyCart em').text(num);
				$("#mycartcur"+id).remove();
			}
			$(function(){
				$("#txtSearch").focus(function(){
					$("#txtSearch").css({background:"#FFFFCC"});
					var va1=$("#txtSearch").val();
					if(va1=='输入"手机"试一试'){
						$("#txtSearch").val("");
					}
				});
				$("#txtSearch").blur(function(){
					$("#txtSearch").css({background:"#FFF"});
					var va2=$("#txtSearch").val();
					if(va2==""){
						$("#txtSearch").val('输入"手机"试一试');
					}
				});
				$("#butSearch").click(function(){
					window.location.href="http://www.yibu777.com/s_tag/"+$("#txtSearch").val();
				});
			});
			// 顶部导航条更多
			function slide(){
				$(".mainright").mouseover(function(){
					$(".mainright .box").stop(true,true).slideDown(500);
				})
				$(".mainright").mouseleave(function(){
					$(".mainright .box").stop(true,true).slideUp(500)
				});
				var m = $(".bannerLine .list").length;
				var sort = $(".box .list");
				 $(".bannerLine> div:nth-child(7)").nextAll().insertBefore(sort.eq(0));
				 if(m<=7){
				 	$(".mainright").hide();
				 }
			}
		</script> 
