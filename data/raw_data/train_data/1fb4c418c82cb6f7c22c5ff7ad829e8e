     <script type="text/javascript">
//相关推荐详情浮层
$(function(){
    $("#entries_right_div li[data-id]").bind({
        mouseenter: function(){
            detail.show(this, $(this).attr("data-id"));
            return false;
        },
        mouseleave: function(){
            detail.hide(this);
            return false;
        }
    })
});
var detail = {};
var d_cache = {};
detail.show = function (obj, id) {
    var p_li = $(obj);
    p_li.attr('tips_status', 'show');
    p_li.css({ position: "relative", 'z-index': 9 });
    p_li.children("div[name='detail_cover']:first").css("z-index", 6);
    if (d_cache[id] && d_cache[id].contents) {
        if (p_li.attr('tips_status') == 'show') {
            p_li.append(d_cache[id].contents);
        }
    } else if (d_cache[id] === undefined) {
        d_cache[id] = {contents: null};
        $.ajax({
          url: '/base/ajax/entry/relation_detail',
          type: 'POST',
          dataType: 'json',
          data: {id: id},
          success: function(data, textStatus, xhr) {
            if (data.code == 0 && data.msg) {
                d_cache[id].contents = data.msg;
                if (p_li.attr('tips_status') == 'show') {
                    p_li.append(d_cache[id].contents);
                }
            }
          },
          error: function(xhr, textStatus, errorThrown) {
            //called when there is an error
          }
        });
    }
}
detail.hide = function(obj) {
    var tips = $(obj).children(".tips_content_wrap");
    if (tips.length > 0) {
       tips.remove();
    }
    var p_li = $(obj);
    p_li.attr('tips_status', 'hide');
    p_li.css({ position: "", 'z-index': '' });
    $(obj).children("div[name='detail_cover']:first").css("z-index", '');
}
</script>
