function setyear(year){
	var year;
	if(year=="") return;
	$.ajax({
	    type:"GET",
	    url:"/index.php?c=historywea&a=ajaxmonth&y="+year+"&cityid=101300109",
	    dataType:"json",
	    success:function(rd){
		$("#month").empty();
		var options="";
		$.each(rd,function(i,n){
			options+='<a href="/binyang/'+i+'.html">'+n+'</a>';
		});
		$("#month").html(options);
	    },
	    error: function (XMLHttpRequest, textStatus, errorThrown) {
                alert(errorThrown); 
            } 
	});
}
</script>


