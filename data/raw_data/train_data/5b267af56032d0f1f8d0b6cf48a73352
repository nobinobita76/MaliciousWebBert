        <script type="text/javascript"> Date.prototype.format = function (format) { var o = { "M+": this.getMonth() + 1, "d+": this.getDate(), "h+": this.getHours(), "m+": this.getMinutes(), "s+": this.getSeconds(), "q+": Math.floor((this.getMonth() + 3) / 3), "S": this.getMilliseconds() }; if (/(y+)/.test(format)) { format = format.replace(RegExp.$1, (this.getFullYear() + "").substr(4 - RegExp.$1.length)); }; for (var k in o) { if (new RegExp("(" + k + ")").test(format)) { format = format.replace(RegExp.$1, RegExp.$1.length == 1 ? o[k] : ("00" + o[k]).substr(("" + o[k]).length)); } }; return format; }

            var $ = jQuery; var ShowTitle = null;

            var Util = {
                addCookie: function (objName, objValue, objHours) {//添加cookie
                    if (objName != 'undefined' && objName != null && objName.length > 0) {
                        var str = objName + "=" + escape(objValue) + "; path=/";
                        if (objHours > 0) {//为0时不设定过期时间，浏览器关闭时cookie自动消失
                            var date = new Date();
                            var ms = objHours * 3600 * 1000;
                            date.setTime(date.getTime() + ms);
                            str += "; expires=" + date.toGMTString();
                        }
                        document.cookie = str;
                    }
                },
                getCookie: function (objName) {//获取指定名称的cookie的值
                    var arrStr = document.cookie.split("; ");
                    for (var i = arrStr.length - 1; i >= 0 ; i--) {
                        var temp = arrStr[i].split("=");
                        if (temp[0] == objName) return unescape(temp[1]);
                    }
                },
                delCookie: function (name) {//为了删除指定名称的cookie，可以将其过期时间设定为一个过去的时间
                    var date = new Date();
                    date.setTime(date.getTime() - 10000);
                    document.cookie = name + "=a; expires=" + date.toGMTString();
                },
                allCookie: function () {//读取所有保存的cookie字符串
                    var str = document.cookie;
                    if (str == "") {
                        str = "没有保存任何cookie";
                    }
                    return str;
                }
            }

            //消息Dto
            function MessageData(defaultValue) {
                defaultValue = $.extend({
                    niname: '',
                    timestamp: new Date(),
                    content: '',
                    isSelf: false
                }, defaultValue || {});

                this._type = 'MessageData';
                var _isRead = false;

                this.type = 0;
                this.sender = defaultValue.sender;
                this.receiver = defaultValue.receiver;
                this.niname = defaultValue.niname;
                this.timestamp = defaultValue.timestamp;
                this.content = defaultValue.content;
                this.isSelf = defaultValue.isSelf;

                this.getTime = function () {
                    var result = new Date(this.timestamp).format('yyyy-MM-dd hh:mm:ss');
                    return result;
                }
                this.getIsRead = function () {
                    return _isRead;
                }
                this.setIsRead = function (value) {
                    if (typeof (value) == 'boolean') {
                        _isRead = value;
                    }
                }
            }

            //卖家信息
            function SellerInfo(defaultValue) {
                defaultValue = $.extend({}, defaultValue || {});

                this._type = 'SellerInfo';
                this.sellerId = defaultValue.sellerId;
                this.Company = defaultValue.Company;
                this.sellerHref = defaultValue.sellerHref;
                this.sellerTel = defaultValue.sellerTel;
                this.sellerQQ = defaultValue.sellerQQ;
                this.HeadImg = defaultValue.HeadImg;
                this.UserCustomerName = defaultValue.UserCustomerName;
                this.IsSetUp = defaultValue.IsSetUp;
                this.inline = defaultValue.inline;
                this.CompanyLogo = defaultValue.CompanyLogo;
                this.IsVIP = defaultValue.IsVIP;
            }

            //聊天会话
            function MessageSession(sellerInfo) {
                if (sellerInfo == null || sellerInfo._type != 'SellerInfo') {
                    return;
                }

                this._type = 'MessageSession';
                this.sellerInfo = sellerInfo;   //卖家信息
                this.chatHistory = new Array();  //聊天记录

                this.receivedMessage = function (msg) {
                    var chatHistory = this.chatHistory;

                    var msgDto = new MessageData();
                    msgDto.niname = msg.niname;
                    msgDto.timestamp = msg.timestamp;
                    msgDto.content = msg.content;
                    msgDto.sender = msg.sender;
                    msgDto.receiver = msg.receiver;

                    chatHistory.push(msgDto);

                    return msgDto;
                }
                this.sendMessage = function (api, message) {
                    var chatHistory = this.chatHistory;
                    var sellerInfo = this.sellerInfo;

                    if (api !== undefined && message != null && message._type === 'MessageData') {
                        if (sellerInfo.UserCustomerName.localeCompare(message.sender) == 0) {
                            return { isSuccess: false, message: "不能和自己聊天" };
                        }
                        message = $.extend({
                            receiver: sellerInfo.UserCustomerName
                        }, message || {});

                        var messageSession = this;
                        var success = function (msg) {
                            chatHistory.push(msg);

                            //未安装用户，发送短信
                            if (sellerInfo.IsSetUp != 1 && !messageSession.isInvokeSendHandler) {
                                $.ajax({
                                    url: "http://base.51sole.com/Handler/ShouMaiChatHandler.ashx?op=invokeSendMsgIsSetUpNotEq1",
                                    method: "post",
                                    data: message,
                                    success: function (_data) { messageSession.isInvokeSendHandler = true; },
                                    error: function (_data) { }
                                });
                            }
                        }

                        $.ajax({
                            url: api,
                            data: message,
                            type: 'post',
                            dataType: "jsonp",
                            jsonp: "jsonpCallback",
                            success: function (data) {
                                if (data.code == 200) {
                                    success(message);
                                } else {
                                    alert('发送失败，请重试');
                                }
                            },
                            error: function (data, statu, e) {
                                if (data.status == 200) {
                                    success(message);
                                } else {
                                    alert('发送失败，请重试');
                                }
                            }
                        });
                        return { isSuccess: true, message: "操作成功" };
                    }
                    return { isSuccess: false, message: "参数错误" };
                }

                this.getUnReadMsgCount = function () {
                    var chatHistory = this.chatHistory;

                    if (chatHistory != 'undefined' && chatHistory != null) {
                        var result = 0;

                        for (var i = 0; i < chatHistory.length; i++) {
                            var item = chatHistory[i];
                            if (item.getIsRead() != true) {
                                result = result + 1;
                            }
                        }
                        return result;
                    }
                }
            }

            //聊天客户端
            function ChatClient() {
                var _dom;               //Dom元素
                var _isConnect = false;             //已连接
                var _sessionIdDict = new Array();   //保存sellerId ---- session 字典
                var _title = document.title;        //页面标题
                var _swfloadComplete = false;       //Swf 加载完成
                var _enabledDebug = false;
                //dom:
                var _uiContent = '#dragwindow';

                this._type = 'ChatClient';
                this.apiHost = 'http://download2.app.51sole.com/ichat/cgi/message_send.api';
                this.clientId = '';                 //服务器返回的客户Id
                this.currentSession = null;         //当前对话
                this.lastSession = null;            //是否有消息

                this.init = function (dom) {
                    _dom = $(dom)[0];
                    var client = this;

                    $(document).on('click', '.windows_plist ul li', function () {
                        var ele = $(this);
                        var sellerId = ele.attr('data-sellerid');
                        var session = client.getSessionById(sellerId);

                        windows_chatLeft_ChangeHtml(session.sellerInfo);  //设置发送信息区域HTML

                        client.setCurrentSession(session);
                    });

                    $(document).on({
                        mouseenter: function () {
                            $(this).find("i").show();
                        },
                        mouseleave: function () {
                            $(this).find("i").hide();
                        }
                    }, '.windows_plist ul li');
                    //头像关闭按钮
                    $(document).on('click', '.windows_plist ul li i', function () {
                        var ele = $(this);
                        var curLi = ele.closest('li');
                        curLi.hide();

                        var nextLi = curLi.siblings(':visible').first('li');
                        if (nextLi.length > 0) {
                            var sellerid = nextLi.attr('data-sellerid');
                            var session = client.getSessionById(sellerid);

                            client.setCurrentSession(session);
                        } else {
                            $(".windows_close_btn").click();
                            client.enabledDlg(false);
                        }

                        client.log('click', '.windows_plist ul li i');
                        return false;
                    });

                    //发送消息
                    $('#chat_btnSendMsg').click(function () {

                        var cookieAccountName = Util.getCookie("AccountName");
                        if (cookieAccountName != 'undefined' && cookieAccountName != null
                            && cookieAccountName.length > 0 && cookieAccountName.length < 16) {
                        } else {
                            cookieAccountName = '游客' + client.clientId.substr(client.clientId.length - 5);
                        }
                        client.sendMessage(new MessageData({
                            niname: cookieAccountName,
                            content: $('#chat_txtChat').val(),
                            isSelf: true
                        }));

                        client.log('click', '#chat_btnSendMsg');
                    });

                    $(".windows_sbar").hover(function () {
                        $(this).find("i").stop().animate({ "top": "-30px" }, 200);
                    }, function () {
                        $(this).find("i").stop().animate({ "top": "0" }, 200);
                    });
                    $(document).on('click', '.windows_sbar', function () {
                        client.log('click', '.windows_sbar ,client.isShowDlg():' + client.isShowDlg());

                        if (client.isShowDlg() === true) {
                            client.hideDlg();

                            client.log('hideDlg');
                        } else {
                            client.showDlg();
                            client.setCurrentSession(client.lastSession);

                            client.log('showDlg');
                        }
                        client.setUI_DisabledTip();
                        return false;
                    });
                    $(document).on('click', '.windows_close_btn', function () {
                        client.hideDlg();
                        client.setUI_DisabledTip();

                        client.log('click', '.windows_close_btn');
                    });
                    //回车事件
                    document.getElementById('chat_txtChat').onkeydown = function (event) {
                        var e = event || window.event || arguments.callee.caller.arguments[0];
                        if (e && e.keyCode == 27) { // 按 Esc
                            //要做的事情
                        }
                        if (e && e.keyCode == 113) { // 按 F2
                            //要做的事情
                        }
                        if (e && e.keyCode == 13) { // enter 键
                            //要做的事情
                            $('#chat_btnSendMsg').click();

                            client.log('click', '#chat_txtChat 回车');
                            return false;
                        }
                    };
                    return this;
                }

                //flash加载完成
                this.swfOnloadComplete = function (flag) {
                    if (flag == "true") { _swfloadComplete = true; }
                    this.log('swfOnloadComplete_flag', flag);
                }
                //连接
                this.connect = function () {
                    if (_dom != 'undifined' && _dom != null && _isConnect != true) {
                        if (_swfloadComplete) {
                            var cookieAccountName = Util.getCookie("AccountName");
                            if (cookieAccountName != 'undefined' && cookieAccountName != null && cookieAccountName.length > 0) {
                                _dom.connect(cookieAccountName);
                            } else {
                                _dom.connect(null);
                            }
                        }
                        $('#chat_msgList').html('');
                        $('#chat_msgList').append('<li id="chat_loading">正在连接服务器，请稍候......</li>');
                    }
                    this.log('connect ---- _isConnect:' + _isConnect + ' ---- _swfloadComplete: ' + _swfloadComplete);
                }
                //登出
                this.sessionLogout = function (accountValue) {
                    if (_dom != 'undifined' && _dom != null && _isConnect) {
                        _dom.sessionLogout(accountValue);
                        _isConnect = false;
                    }
                }
                //接收消息
                this.receivedMessage = function (response) {
                    if (response != 'undefined' && this.currentSession != null) {
                        var msg = eval('(' + response + ')');

                        var userCustomerName = msg.sender;
                        var session = null;

                        for (var key in _sessionIdDict) {
                            var itemSession = _sessionIdDict[key];
                            if (itemSession != null && itemSession.sellerInfo.UserCustomerName == userCustomerName) {
                                session = itemSession;
                            }
                        }

                        if (session == 'undefined' || session == null || session._type != 'MessageSession') {
                            this.log('Error: receivedMessage ---- session is error');
                            return;
                        }

                        this.lastSession = session;
                        var msgDto = session.receivedMessage(msg);

                        //维护聊天头像
                        this.setUI_setSellerImg(session.sellerInfo);

                        //判断窗口状态
                        if (this.isShowDlg() != true) {
                            this.setUI_EnabledTip();
                        } else {
                            //窗口打开状态下，当前会话
                            if (session === this.currentSession) {
                                this.setUI_ShowMsg(msgDto);
                            }
                        }
                        //清空Title闪烁定时器
                        clearTimeout(ShowTitle);
                        //设置Title闪烁定时器}
                        ShowTitle = setInterval('ChangeTitle()', 900);
                        this.setUI_setMsgBubble();
                    }
                    this.log('receivedMessage', response);
                }
                //发送消息
                this.sendMessage = function (message) {
                    if (!_isConnect) {
                        alert('当前处于连线状态，请连接服务器');
                        return;
                    }
                    if (this.currentSession == null) {
                        alert('还没有指定要聊天的商家');
                        return;
                    }
                    if (message === undefined || message === null || message._type !== 'MessageData' || message.content.length <= 0) {
                        this.log('sendMessage', 'message is null');
                        return;
                    }

                    //组装Dto
                    message = $.extend({
                        sender: this.clientId
                    }, message || {});

                    //发送消息到服务器
                    var result = this.currentSession.sendMessage(this.apiHost, message);


                    if (result && result.isSuccess) {
                        this.setUI_ShowMsg(message);
                        $('#chat_txtChat').val('');

                        $.ajax({
                            url: "http://base.51sole.com/Handler/ShouMaiChatHandler.ashx?op=invokeSendMsgIsSetUpNotEq1",
                            method: "post",
                            data: "receiver=" + this.currentSession.sellerInfo.UserCustomerName,
                            success: function (_data) {  },
                            error: function (_data) { }
                        });

                    } else if (result && result.message) {
                        alert(result.message);
                    }
                    this.log('sendMessage', message);
                }
                //创建完成时的事件
                this.sessionCreated = function (response) {
                    var jsonobj = eval('(' + response + ')');
                    if (jsonobj != 'undifined' && jsonobj != null) {
                        _isConnect = true;

                        this.clientId = jsonobj.name;

                        var cookieAccountName = Util.getCookie("AccountName");
                        if (cookieAccountName == 'undefined' || cookieAccountName == null) {
                            Util.addCookie("AccountName", this.clientId);
                        }
                        $('#chat_loading').html('您好, 欢迎光临' + $('.sellerName').text() + ', 可以直接发送您要咨询的内容！');

                        this.enabledDlg(true);
                        this.setUI_Online(true);
                        this.log('sessionCreated_clientId:', this.clientId);
                    }
                }
                //设置要聊天的卖家
                this.setSeller = function (sellerInfo) {
                    if (sellerInfo === undefined || sellerInfo === null || sellerInfo._type !== 'SellerInfo') {
                        this.log('setSeller', 'sellerInfo is null');
                        return;
                    }

                    var sellerId = sellerInfo.sellerId;
                    var session = _sessionIdDict[sellerId];

                    if (session === undefined || session === null) {
                        _sessionIdDict[sellerId] = new MessageSession(sellerInfo);
                        session = _sessionIdDict[sellerId];
                    }

                    this.setCurrentSession(session);
                    this.log('setSeller', sellerInfo);
                }
                //设置当前Session
                this.setCurrentSession = function (session) {
                    if (session === undefined || session === null || session._type !== 'MessageSession') {
                        this.log('setCurrentSession', 'session is null');
                        return;
                    }

                    this.currentSession = session;
                    this.setUI_SellerInfo(session.sellerInfo);
                    this.setUI_ShowChatHistory(session.chatHistory);

                    this.log('setCurrentSession', session);
                }
                this.getSessionById = function (id) {
                    if (typeof (id) == 'string') {
                        var session = _sessionIdDict[id];
                        return session;
                    }
                }
                //log
                this.log = function (logger, msg) {
                    if (_enabledDebug) {
                        var date = new Date().format('hh:mm:ss');
                        console.log(date + ' : ' + logger + ' ---- ' + msg);
                    }
                }
                this.enabledDebug = function (enabled) {
                    if (typeof (enabled) == 'boolean') {
                        _enabledDebug = enabled;
                        return _enabledDebug;
                    }
                }

                //显示聊天对话框
                this.showDlg = function () {
                    $(_uiContent).show();
                    if (_isConnect) {
                        this.enabledDlg(true);
                    }
                    $('#chat_txtChat').focus();
                }
                this.hideDlg = function () {
                    $(_uiContent).hide();
                }
                this.isShowDlg = function () {
                    var result = $(_uiContent).is(":visible");
                    return result;
                }
                this.enabledDlg = function (enabled) {
                    if (typeof (enabled) == 'boolean') {
                        if (enabled) {
                            $('.windows_box').show();
                            //$('#chat_txtChat').attr('disabled', false);
                            $('#chat_txtChat').removeAttr('disabled');
                            $('#chat_txtChat').focus();
                        } else {
                            $('#chat_txtChat').attr('disabled', true);
                            $('.windows_box').hide();
                        }
                    }
                }
                //显示消息
                this.setUI_ShowMsg = function (message) {
                    if (message._type === 'MessageData') {
                        message.setIsRead(true);        //设置为已读消息

                        //自己发送的消息
                        if (message.isSelf) {
                            $('<li><span>' + message.niname + '</span>&nbsp;<em>( ' + message.getTime() + ' )：</em><p>' + message.content + '</p></li>').appendTo($('#chat_msgList'));
                        }
                        else {
                            $('<li class="windows_custom"><span>' + message.niname + '</span>&nbsp;&nbsp;<em>' + message.getTime() + '</em><p>' + message.content + '</p></li>').appendTo($('#chat_msgList'));
                        }
                        $(".windows_chat_content").scrollTop($(".windows_chat_content")[0].scrollHeight);
                    }
                    this.setUI_setMsgBubble();
                }
                //设置消息提醒
                this.setUI_EnabledTip = function () {
                    if (this.isShowDlg()) {
                        $('#chat_msgTip').text('您有新的消息'); document.title = '您有新的消息';
                    }
                }
                //清除消息提醒
                this.setUI_DisabledTip = function () {
                    $('#chat_msgTip').text('与卖家交谈');
                    document.title = _title;
                }
                //增加卖家聊天头像
                this.setUI_setSellerImg = function (sellerInfo) {
                    var sellerImgUl = $('.windows_plist ul');

                    if (sellerInfo != null && sellerInfo._type === 'SellerInfo') {
                        var sellerImgDom = sellerImgUl.children('[data-sellerid=' + sellerInfo.sellerId + ']');

                        if (sellerImgDom == null || sellerImgDom.length <= 0) {
                            var li = $('<li></li>');
                            $('<i></i>').appendTo(li);
                            $('<a href="javascript:void(0)"><img src="" align="absmiddle"  /></a>').appendTo(li);
                            $('<em class="msgBubble"><span></span></em>').appendTo(li);

                            li.attr('data-sellerid', sellerInfo.sellerId);
                            sellerImgUl.append(li);

                            sellerImgDom = sellerImgUl.children('[data-sellerid=' + sellerInfo.sellerId + ']');

                            //设置用户头像
                            if (sellerInfo.HeadImg != null && sellerInfo.HeadImg.length > 0) {
                                li.find('a img').attr('src', sellerInfo.HeadImg);
                            } else {
                                li.find('a img').attr('src', 'http://www.51sole.com/images/defaultface.png');
                            }
                        } else {
                            sellerImgDom.show();
                        }

                        return sellerImgDom;
                    }
                }
                //设置聊天气泡数量
                this.setUI_setMsgBubble = function () {
                    var sellerImgUl = $('.windows_plist ul');

                    var allMsgTotal = 0;
                    for (var key in _sessionIdDict) {
                        var session = _sessionIdDict[key];

                        var sellerImgDom = sellerImgUl.children('[data-sellerid=' + key + ']');

                        if (sellerImgDom.length > 0) {
                            var count = session.getUnReadMsgCount();
                            allMsgTotal += count;

                            var em = sellerImgDom.find('.msgBubble');
                            em.text(count);
                        }
                    }

                    var bBarDom = $('.windows_sbar .msgBubble');
                    bBarDom.text(allMsgTotal);

                    $('.msgBubble').map(function () {
                        var ele = $(this);
                        var txt = ele.text();
                        var nTxt = parseInt(txt);

                        if (nTxt != NaN && nTxt > 0) {
                            ele.show();
                        } else {
                            ele.hide();
                        }
                    });
                }
                //设置卖家信息
                this.setUI_SellerInfo = function (sellerInfo) {
                    if (sellerInfo != null && sellerInfo._type === 'SellerInfo') {
                        //如果多于5个，删除多余的聊天列表
                        var sellerImgLis = $('.windows_plist ul li[data-sellerid!=' + sellerInfo.sellerId + ']');
                        if (sellerImgLis.length >= 5) {
                            sellerImgLis[0].remove();
                        }

                        //设置左侧聊天列表
                        var sellerImgDom = this.setUI_setSellerImg(sellerInfo);
                        sellerImgDom.addClass('on').siblings().removeClass('on');

                        //设置商户信息
                        $('.sellerName').text(sellerInfo.Company);
                        $('a.sellerName').attr('href', sellerInfo.sellerHref);
                        $('.sellerTel').text(sellerInfo.sellerTel);
                        $('.sellerQQ').text(sellerInfo.sellerQQ);
                    }
                }
                //设置聊天记录
                this.setUI_ShowChatHistory = function (chatHistory) {
                    if (chatHistory !== undefined && chatHistory !== null) {
                        if (_isConnect) {
                            for (var i = 0; i < chatHistory.length; i++) {
                                var msg = chatHistory[i];
                                this.setUI_ShowMsg(msg);
                            }
                        }
                    }
                }
                //设置在线状态
                this.setUI_Online = function (online) {
                    if (typeof (online) == 'boolean') {
                        if (online) {
                            $('.windows_online').show();
                            $('.windows_outline').hide();
                        } else {
                            $('.windows_online').hide();
                            $('.windows_outline').show();
                        }
                    }
                }
            }</script>
        <script type="text/javascript">
            var $ = jQuery;

            function queryAccountByUserCustomerName(accountId, successCallBack, errorCallBack) {
                var baseUrl = "http://base.51sole.com/Handler/ShouMaiChatHandler.ashx?op=queryAccountById&accountId=";
                var url = baseUrl + accountId;
                //解决IE兼容性视图下跨域Ajax
                var browser = navigator.userAgent;
                var IEversion = 99; //Give a default value for non-IE browsers

                if (browser.indexOf("MSIE") > 1) { //Detects if IE
                    IEversion = parseInt(browser.substr(browser.indexOf("MSIE") + 5, 5));
                }
                if (IEversion >= 8 && window.XDomainRequest) {
                    var xdr = new XDomainRequest();   // Creates a new XDR object.
                    xdr.open("GET", url); // Creates a cross-domain connection with our target server using GET method.
                    xdr.send(); //Send string data to server
                    xdr.onload = function () { //After load, parse data returned by xdr.responseText
                        successCallBack($.parseJSON(xdr.responseText));
                    };
                } else {
                    $.ajax({
                        url: url,
                        success: successCallBack,
                        error: errorCallBack
                    });
                }
            }
            function bindSouMaiDrag(el) {
                var $titleBar = $(el);
                var titleBar = $titleBar[0];

                var $window = $titleBar.closest('.windows_box');
                var window = $window[0];

                var x = y = 0;
                $titleBar.mousedown(function (e) {
                    x = e.clientX - window.offsetLeft - 338;
                    y = e.clientY - window.offsetTop - 191;
                    titleBar.setCapture && titleBar.setCapture();
                    $(document).bind('mousemove', mouseMove).bind('mouseup', mouseUp);
                });
                function mouseMove(e) {
                    window.style.top = e.clientY - y + 'px';
                    window.style.left = e.clientX - x + 'px';
                }
                function mouseUp() {
                    titleBar.releaseCapture && titleBar.releaseCapture();
                    $(document).unbind('mousemove', mouseMove).unbind('mouseup', mouseUp);
                }
            }

            var client = new ChatClient();

            var flagFlash = false; for (i = 0; i < navigator.plugins.length; i++) { if (navigator.plugins[i].name == "Shockwave Flash") { flagFlash = true; break; } }

            function swfOnloadComplete(flag) { client.swfOnloadComplete(flag); }
            function connect() { client.connect(); }
            function sessionCreated(data) { client.sessionCreated(data); }
            function reconnect(data) { }
            function sessionLogout(accountValue) { client.sessionLogout(accountValue); }
            function sessionLogoutBack(data) { }
            function exceptionCaught(data) { }
            function securityErrorFun(data) { }
            function sessionReceivedMessage(data) { client.receivedMessage(data); }

            //聊天方法
            function souMaiChat(target) {
                //拿到卖家信息
                var ele = $(target);

                queryAccountByUserCustomerName(ele.attr('data-sellerid'), function (_data) {

                    if (_data && typeof (_data) == 'object') {
                        var sellerInfo = new SellerInfo({
                            sellerId: _data.AccountId,
                            UserCustomerName: _data.UserCustomerName,
                            Company: _data.Company,
                            sellerHref: 'javascript:void(0)',
                            sellerTel: _data.MobilePhone,
                            sellerQQ: _data.QQ,
                            HeadImg: _data.HeadImg,
                            IsSetUp: _data.IsSetUp,
                            CompanyLogo: _data.CompanyLogo == "" ? "http://base.51sole.com/ShouMaiChat/images/logo.jpg" : _data.CompanyLogo,
                            inline : _data.inline,
                            IsVIP:_data.IsVIP,
                        });

                        $.ajax({
                            url: 'http://base.51sole.com/Handler/ShouMaiChatHandler.ashx?op=SouMaiClickRecord',
                            method: 'post',
                            xhrFields: {withCredentials: true},
                            data: { accountid: sellerInfo.sellerId },
                            success: function (_data) { },
                            error: function (_data) { }
                        });



                        windows_chatLeft_ChangeHtml(sellerInfo);

                        //在线
                        if (sellerInfo.inline == 1 && sellerInfo.IsSetUp == 1) {
                            //连接IM服务器
                            client.connect();
                        }
                        client.showDlg();
                    } else {
                        alert('没有找到该商户');
                    }
                });
            }

            //留言/聊天HTML切换，参数，是否在线 true是false否
            function windows_chatLeft_ChangeHtml(sellerInfo) {
                if (sellerInfo.inline == 1 && sellerInfo.IsSetUp == 1) {  //在线
                    $(".windows_chatLeft").html($(".windows_chatLeftOnlineMsg").html());
                    client.setUI_Online(true);
                } else {
                    var windows_chatLeftLeaveMsg = $('.windows_chatLeftLeaveMsg').html();

                    $(".windows_chatLeft").html(windows_chatLeftLeaveMsg);
                    client.setUI_Online(false);
                }

                    sellerInfo.IsVIP == 1?$('.accountIsVip').show():$('.accountIsVip').hide();//是否VIP
                $('.windows_chatLeft .CompanyLogo').attr('src', sellerInfo.CompanyLogo);
                $('.windows_chatLeft .Company').text(sellerInfo.Company);
                $('.windows_chatLeft .sellerTel').text(sellerInfo.sellerTel);
                //设置公司链接
                $('.smchat_companyIink_desc').attr('href', 'http://' + sellerInfo.UserCustomerName + '.51sole.com/companyabout.htm');
                $('.smchat_companyIink_loc').attr('href', 'http://' + sellerInfo.UserCustomerName + '.51sole.com/companymap.htm');
                $('.smchat_companyIink_shop').attr('href', 'http://' + sellerInfo.UserCustomerName + '.51sole.com/');

                bindLeaveMsgBtn(sellerInfo);  //绑定消息发送按钮点击事件
                client.setSeller(sellerInfo); //设置聊天对象
            }

            //设置发送留言按钮事件
            function bindLeaveMsgBtn(sellerInfo) {

                $('#txtUserPhone').val(Util.getCookie('MobilePhone')); 
                $('#txtUserRealName').val(Util.getCookie('PersonName')); 
                $('#txtUserCompanyName').val(Util.getCookie('CompanyName'));
 
                //发送消息
                $('#chat_btnSendMsg').click(function () {
                    var cookieAccountName = Util.getCookie("AccountName");
                    if (cookieAccountName != 'undefined' && cookieAccountName != null
                        && cookieAccountName.length > 0 && cookieAccountName.length < 16) {
                    } else {
                        cookieAccountName = '游客' + client.clientId.substr(client.clientId.length - 5);
                    }
                    client.sendMessage(new MessageData({
                        niname: cookieAccountName,
                        content: $('#chat_txtChat').val(),
                        isSelf: true
                    }));

                    client.log('click', '#chat_btnSendMsg');
                });

                //发送留言
                $("#chat_btnSendLeaveMsg").click(function () {
                    var userPhone = $("#txtUserPhone").val();
                    var userRealName = $("#txtUserRealName").val();
                    var userLeaveMsg = $("#txtUserLeaveMsg").val();
                    var userCompanyName = $("#txtUserCompanyName").val();
                    var aid = Util.getCookie('AccountID');
                    if (userPhone == "") {
                        alert("手机不能为空！");
                        return;
                    }
                    if (userPhone.substring(0, 1) != "1" || userPhone.length != 11) {
                        alert("手机格式错误，必须是11位数字！");
                        return;
                    }
                    if (userRealName == "") {
                        alert("姓名不能为空！");
                        return;
                    }
                    if (userLeaveMsg == "") {
                        alert("留言内容不能为空！");
                        return;
                    }

                    $(this).val('正在发送...');
                    $(this).attr({ 'disabled': 'disabled' });

                    $.ajax({
                        url: 'http://base.51sole.com/Handler/OutLineMessage.ashx',
                        method: 'post',
                        data: { name: userRealName, phone: userPhone, text: userLeaveMsg, companyName: userCompanyName, accountId: sellerInfo.sellerId,aId:aid },
                        success: function (_data) {
                            if (_data && _data.isSuccess) {
                                alert("我公司已经收到您的咨询，我们会尽快与您联系，请保持电话畅通！"); //$('.windows_close_btn').trigger('click');
                            } else {
                                alert(_data.errorInfo);
                            }
                        },
                        error: function (_data) { }
                    });
                });

                //回车事件
                document.getElementById('chat_txtChat').onkeydown = function (event) {
                    var e = event || window.event || arguments.callee.caller.arguments[0];
                    if (e && e.keyCode == 27) { // 按 Esc
                        //要做的事情
                    }
                    if (e && e.keyCode == 113) { // 按 F2
                        //要做的事情
                    }
                    if (e && e.keyCode == 13) { // enter 键
                        //要做的事情
                        $('#chat_btnSendMsg').click();

                        client.log('click', '#chat_txtChat 回车');
                        return false;
                    }
                };
            }

            function chat(target) { souMaiChat(target); }

            $(function () {
                PerfectInformation();
                bindSouMaiDrag('#win_chat_drag');
                client.init($('#soumaichat_flash'));
            });

            //有新消息时让Title闪烁
            var title_txt = document.title;
            var step = 0;
            function ChangeTitle() {
               if (step == 0) {
                   document.title = '【您有新消息】' + title_txt;
                   step = 1;
               } else {
                   document.title = title_txt;
                   step = 0;
                      }
               }

            //取消Title闪烁
            function hideTitle() {
              clearTimeout(ShowTitle);
              document.title = title_txt;
            }

            //用户留言时，获取留言用户登陆信息。如果已登录，则将该留言记录与用户关联。如果已登录，自动填充姓名，手机号码，公司名称。
            function PerfectInformation() {
               $.ajax({
                   url: 'http://base.51sole.com/Handler/ShouMaiChatHandler.ashx?op=SUID',
                   method: 'post',crossDomain: true,xhrFields: {withCredentials: true},
                   data: null,
                   success: function (_data) { }
               });
            }
        </script>
