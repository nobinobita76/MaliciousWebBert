        $(".yzmhq").click(function () {
            $.ajax({
                type: "post",
                dataType: "json",
                url: "/Vote.aspx?action=sendmessage",
                data: { "p": $("#p").val() },
                success: function (data) {
                    if (data.state == 0) {
                        alert(data.message);
                    } else {
                        alert("发送成功");
                        settime();
                    }
                },
                error: function (data) {
                    alert(data.responseText);
                }
            });
        })
        $("#sureVote").click(function () {
            var vote = getCookie("videovote");
            $.ajax({
                type: "post",
                dataType: "json",
                url: "/Vote.aspx",
                data: { "type": 6003, "proids": vote, "p": $("#p").val(), "cd": $("#cd").val() },
                success: function (data) {
                    if (data.state == 0) {
                        alert(data.message);
                    } else {
                        setCookie("phone",$("#p").val());
                        setCookie("cd", $("#cd").val());
                        delCookie("videovote");
                        alert("投票成功");
                        location.reload();
                    }
                },
                error: function (data) {
                    alert("投票失败");
                }
            });
        });
    }
    var iNow = 60;
    var timer = null;
    function settime() {
        clearInterval(timer);
        function clock() {
            $(".yzmhq").val(iNow + 's');
            $(".yzmhq").attr("disabled","disabled");
            iNow--;
            if (iNow == -1) {
                clearInterval(timer);
                $(".yzmhq").removeAttr("disabled");
                iNow = 60;
                $(".yzmhq").val('点击获取');
            }
        }
        clock();
        timer = setInterval(clock, 1000);
    }
    function delCookie(name) {
        var exp = new Date();
        exp.setTime(exp.getTime() - 1);
        var cval = getCookie(name);
        if (cval != null)
            document.cookie = name + "=" + cval + ";expires=" + exp.toGMTString();
    }
    function setCookie(name, value) {
        document.cookie = name + "=" + escape(value);
    }

    function getCookie(name) {
        var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
        if (arr = document.cookie.match(reg))
            return unescape(arr[2]);
        else
            return null;
    }
	
    var totalpage = 1;
    var currentpage = 1;
    var lastChange = false;
    var mySwiper = new Swiper('.swiper-container', {
        onTransitionStart: function (swiper) {
            var pn = 1;
            if (swiper.isBeginning && currentpage>1) {
                if(swiper.swipeDirection=="prev"){
                    if(!lastChange){
                        lastChange = true;
                    }else{
                        if(window.location.search.indexOf("pn=")>-1){
                            window.location.search = changeURLPar(window.location.search,"pn",(currentpage-1));
                        }else{
                            window.location.search = changeURLPar(window.location.search,"pn",1);
                        }
                    }
                }
                
            }
            if (swiper.isEnd && currentpage >=1) {
                if(swiper.swipeDirection=="next"){
                    if(!lastChange){
                        lastChange = true;
                    }else{
                        if(window.location.search.indexOf("pn=")>-1){
                            window.location.search = changeURLPar(window.location.search,"pn",(currentpage+1));
                        }else{
                            window.location.search = changeURLPar(window.location.search,"pn",2);
                        }
                    }
                }
            }
            $('.shandow').height($('.swiper-slide-active dt ').height()+50);
        },
        onSlideChangeEnd: function(swiper){
            $('.swiper-container').height($('.swiper-slide-active .padding').height());
        }
    });
    function changeURLPar(destiny, par, par_value) 
    { 
        var pattern = par+'=([^&]*)'; 
        var replaceText = par+'='+par_value; 
        if (destiny.match(pattern)) 
        { 
            var tmp = '/\\'+par+'=[^&]*/'; 
            tmp = destiny.replace(eval(tmp), replaceText); 
            return (tmp); 
        } 
        else 
        { 
            if (destiny.match('[\?]')) 
            { 
                return destiny+'&'+ replaceText; 
            } 
            else 
            { 
                return destiny+'?'+replaceText; 
            } 
        } 
        return destiny+'\n'+par+'\n'+par_value; 
    } 
    function detail(e,id) {
        $("#detailInfo").val($(e).attr("data"));
        $("#detailform").attr("action", "detail.aspx?id=" + id);
        $("#detailform").submit();
    }

    $(window).on("orientationchange resize load", function () {
        orientation();
    });
    function orientation() {
		$('.shandow3').height($('.swiper-slide-active dt').height()+50);
        $('.shandow1,.shandow2').width($('.shandow3').width()).height($('.shandow3').height())
    }
    $('.tishi').on('mousedown mouseup touchstart touchend touchmove', function () {
        $(this).remove();
    })
    if(getCookie("VideoPageVisit")==-1) {
        setCookie("VideoPageVisit", "true");
        $('.tishi').show();
    }
    $('.scbtn').click(function () {
        $(".sjyzdl").css('display','none');
    })
    $(':text').focus(function(){
		if(this.value == this.defaultValue) {
			this.value = '';
		$(this).css('color','#000');
		}			
	})
	$(':text').blur(function(){
		if(this.value == '') {
			this.value = this.defaultValue;
		$(this).css('color','#8183b4');
		}
	})
</script>
</body></html>