{
	box111.Close();
}
$(document).ready(function() {
	$('.img-swiper-slide-v').click(function(){
		//$(".swiper-wrapper-h").find("img")[0].src = this.src;
		box111.Show();
		$(".swiper-pagination-bullet").removeClass("swiper-pagination-bullet-active");
		var pagination_num=Math.abs(parseInt($(this).parents('.swiper-wrapper').css('transform').split(',')[4]*0.985/(document.body.clientWidth-5)));
		$(".swiper-pagination-bullets").children().eq(pagination_num).addClass("swiper-pagination-bullet-active");
		$('.swiper-wrapper-max img').css("width","100% !important");
		$('.swiper-wrapper-max').css('transform',"translate3d("+($(this).parents('.swiper-wrapper').css('transform').split(',')[4]*0.985)+"px, 0px, 0px)");
		//console.log(pagination_num);
		//console.log(document.body.clientWidth);
	});
},false);
