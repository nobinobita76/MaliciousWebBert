<script>
/*返回顶部*/
//var img_url = "http://pic1.997788.com/images/top.png";
$(function(){	
    scrolltotop.offset(10,50);
    scrolltotop.init(); 
});
$(function(){
	$("#header span.right").on("click",function(e){
        var val = $(this).attr('class');
        if(val.indexOf('on') == -1){
            $(this).addClass('on');
            $(".menu").show(0).stop().animate({
                height:'100%'
            },0);
			document.addEventListener('touchmove', touchMoveFunc, false);
			//document.addEventListener('touchmove', bodyScroll(e), false);
        }else{
            $(this).removeClass('on');
            $(".menu").stop().animate({
                height:'0'
            },0);
			document.removeEventListener('touchmove',touchMoveFunc, false);
        }
    })
	
	$(".menu a").on("click",function(){
        $(this).parents(".menu").hide();
        $("#header span.right").removeClass('on');
    })
	//$(".menu").css('font-size',6);
	if(window.screen.height<=480)
		$(".e_border").css('line-height',22+"px");
});
function bindEvent() {
	//document.addEventListener('touchstart', touchSatrtFunc, false);
	document.addEventListener('touchmove', touchMoveFunc, false);
	//document.addEventListener('touchend', touchEndFunc, false);
}
function touchMoveFunc(evt) {
	evt.preventDefault();
}
function bodyScroll(e){
e.preventDefault();
}
</script>

