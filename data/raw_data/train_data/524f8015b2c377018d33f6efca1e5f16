<script language="javascript">
    function GetObj(element) {
        return document.getElementById(element);
    }
    function search() {
        GetObj("q").name = "q";
        var s_keywords = document.myform.q.value;
        if ((s_keywords == "" || s_keywords == "请输入关键字")) {
            alert("请输入查询关键字!");
            GetObj("q").focus();
            return false;
        }
        switch (GetObj("s_class").value) {
            case "0"://百度
                GetObj("q").name = "word";
                GetObj("myform").action = "http://www.baidu.com/baidu";
                break;
            case "1"://驱动
                GetObj("myform").action = "http://so.mydrivers.com/drivers.aspx?q=" + escape(s_keywords);
                break;
            case "2"://新闻
                GetObj("myform").action = "http://so.mydrivers.com/news.aspx?q=" + escape(s_keywords);
                break;
            case "3"://软件
                GetObj("myform").action = "http://so.myfiles.com.cn/soft.aspx?q=" + escape(s_keywords);
                break;
            case "4"://APP
                GetObj("myform").action = "http://app.mydrivers.com/search/default.aspx?q=" + escape(s_keywords);
                break;
            case "5"://图片
                GetObj("q").name = "word";
                GetObj("myform").action = "http://image.baidu.com/i?word=" + escape(s_keywords);
                break;
            case "6"://视频
                GetObj("q").name = "word";
                GetObj("myform").action = "http://video.baidu.com/v?ch=14&ie=utf-8&word=" + escape(s_keywords);
                break;
            case "7"://工具
                GetObj("q").name = "kw";
                GetObj("myform").action = "http://tools.mydrivers.com/searchlist.aspx";
                break;
            default:
                GetObj("q").name = "wd";
                GetObj("myform").action = "http://www.baidu.com/baidu";
                break;
        }
        return true;
    }
</script>
