    (function(){
        $('.button01').click(function(){
            var key = jQuery.trim($('#key01').val()),
                    url = "//mall.jd.com/advance_search-" + 405372 + '-' + 1000002376 + '-' + 1000002376 + '-0-0-0-1-1-60.html';

            key = encodeURIComponent(encodeURIComponent(key));

            if(key!='') {
                url += '?keyword=' + key;
            }
            location.href = url;
        });
    })();
</script>
