function rollto(v){
this.roll=function(s){$('html').animate({scrollTop:$(".info_box").eq(s).offset().top},'fast');$('body').animate({scrollTop:$(".info_box").eq(s).offset().top},'fast');}
switch(v){
case "酒店介绍":this.roll(0);break;
case "酒店图片":this.roll(1);break;
case "客人点评":this.roll(2);break;
case "交通地图":this.roll(3);break;
case "酒店问答":this.roll(4);break;
case "周边酒店":this.roll(5);break;}
}

var maplet = null,mData=[[20073,'澳门维景酒店(原京澳酒店)...','http://p.zhuna.cn//hotel_images/20073/160x120_43301002_0_10_0_1.jpg','ISHDFSZTWBWJI','澳门新口岸北京街199号','889','9'],[20074,'澳门新丽华酒店','http://p.zhuna.cn//hotel_images/20074/160x120_43301003_0_5_0_1.jpg','ISHBBBZTWBTFR','澳门约翰四世大马路','747','9'],[20077,'澳门假日酒店','http://p.zhuna.cn//hotel_images/20077/160x120_43301029_0_5_0_1.jpg','ISHDDRZTWBVWH','澳门新口岸北京街82-86号','1886','9'],[20084,'澳门葡京酒店','http://p.zhuna.cn//hotel_images/20084/160x120_53301003_0_5_0_1.jpg','ISHCJJZTWBTVV','澳门葡京路2-4号','977','9'],[20086,'永利澳门酒店','http://p.zhuna.cn//hotel_images/20086/160x120_53301015_0_10_0_1.jpg','ISHDUCZTWBUCB','澳门外港填海区仙德丽街','2401','9'],[20090,'澳门新葡京酒店','http://p.zhuna.cn//hotel_images/20090/160x120_53301023_0_5_0_1.jpg','ISHCBEZTWBUBB','澳门葡京路','2300','9'],[27396,'澳门帝濠酒店','http://p.zhuna.cn//hotel_images/27396/160x120_33301023_0_5_0_1.jpg','ISHDJRZTWBWAU','澳门上海街51号','880','9']],mks=new Array(),polyline;
function initMap(){
	maplet = new Maplet("DP_Map_Show");
	maplet.centerAndZoom(new MPoint('北京市'),8);
	maplet.addControl(new MStandardControl());
	maplet.showOverview(false);	
	var mk=new MMarker(   
				new MPoint("ISHCSBZTWBVDC"),   
				new MIcon("images/0.gif",34,36), 
				null,
				new MLabel("澳门富豪酒店",10,-8,80)
			);
	maplet.addOverlay(mk);
	mks.push(mk)
	for(var i=0;i<mData.length;i++){
		var mk=new MMarker(   
				new MPoint(mData[i][3]),   
				new MIcon("images/No/"+(i+1)+".gif",17,22),   

				new MInfoWindow(mData[i][1],'<img src="'+mData[i][2]+'" width="86" height="68" align="left" style="margin:5px;">名称：'+mData[i][1].substring(0,12)+'<br>价格：最低'+mData[i][5]+'元起订<br>地址：'+mData[i][4].substring(0,25)+'<br><a href="hotelinfo.asp?id='+mData[i][0]+'&types=0" target="_blank">了解酒店详情>></a>'),
			
				null); 
		maplet.setIwStdSize(300,150); 
		maplet.addOverlay(mk);
		mks.push(mk);
	}
	maplet.setAutoZoom();
};
function showhotel(id,i){
	maplet.removeOverlay(polyline); 
	mks[i].openInfoWindow();
	for(var i=0;i<mData.length;i++){
		if(mData[i][0]==id){
			    var brush = new MBrush("blue");brush.stroke = 15;brush.overlap.enable = true;   
				polyline = new MPolyline([new MPoint("ISHCSBZTWBVDC"),new MPoint(mData[i][3])],brush);
				polyline.setEditable(true);
				maplet.addOverlay(polyline); 
				mks.push(polyline);
				maplet.setAutoZoom();
		}
	}
}
