<script type="text/javascript">

$("#usernametip").click(function(){$(this).html('');$("#username").focus();});
$("#username").blur(function(){if($(this).val()==''){$('#usernametip').html('用户名/Email');}});

$("#username").focus(function(){if($('#usernametip').html()!=''){$('#usernametip').html('');};});

$("#pwdtip").click(function(){$(this).html('');$("#password").focus();});
$("#password").blur(function(){if($(this).val()==''){$('#pwdtip').html('登录密码');}});

if($('#username').val()!=''){$('#usernametip').html('');}

$("#password").focus(function(){if($('#pwdtip').html()!=''){$('#pwdtip').html('');};});
function onSubmit() 
{
	if ($("#username").val() == '') {
		alert('请填写用户名！');
		$("#username").focus();
		return false;
	}
	if ($("#password").val() == '') {
		alert('请填写密码！');
		$("#password").focus();
		return false;
	}

	$("#login").submit();
}

$("#password").keydown(function (event) {
	// 回车提交
	if (event.keyCode == 13) {
		onSubmit();
	}
});
//-->
