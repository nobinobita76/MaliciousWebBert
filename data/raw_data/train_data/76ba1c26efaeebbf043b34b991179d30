    <script type="text/javascript">
        jQuery("#tabServer").slide({titCell:".tabServer a",mainCell:".tabServerLite ",titOnClassName:"now",effect:"fold"});
        jQuery("#carousel").slide({effect:"leftLoop",titCell:".picSlider-btns span",mainCell:".bd ul",titOnClassName:"current-btn",autoPlay:true});
        jQuery("#tabNews").slide({titCell:".tabNews a",mainCell:".tabNewsLite ",titOnClassName:"now",effect:"fold"});
        function loadFLV() {
            var _flv = 'http://image.tanwan.com/tqcf/tqcf2.flv',
                _width = '856',
                _height = '480',
                video_html = '<embed width="'+_width+'" height="'+_height+'" wmode="transparent" src="http://image.tanwan.com/flvplayer.swf" allowfullscreen="true" flashvars="vcastr_file='+_flv+'&IsAutoPlay=1" quality="high" pluginspage="http://www.macromedia.com/go/getflashplayer" type="application/x-shockwave-flash" />';
            $("#flv").html(video_html);
            showLayer('flvWrap');
        }
        function showLayer(obj) {
            $("#"+obj).fadeIn(500);
            $(".layer").show(0, function() {
                $(this).animate({
                    "opacity": 0.7
                }, 500);
            })
        }
        function closeFLV() {
            $("#flv").empty();
            $("#flvWrap").fadeOut(500);
            $(".layer").animate({
                "opacity": 0
            }, 500, function() {
                $(this).hide();
            });
        }
        
        function loadFlash(url,id,width,height){
            var mainSwf = url;
            var flashVars = {isIE:window.ActiveXObject };
            var params = { quality:"high", wmode:"transparent", allowScriptAccess:"always" };
            swfobject.embedSWF(mainSwf, id, width, height, "10.0.0.52", null, flashVars, params);
        }
        
                loadFlash("http://image.tanwan.com/tqcf/js/startOnPage.swf","startOnPage",242,157);
                
        if(document.getElementById('loginform0')!=null){
            chkAccStatus('loginform',2);
            //上次登录游戏信息
            $.getJSON('http://www.tanwan.com/api/login_log.php?game_id=84&num=3&callback=?',function(data){
                var login_game_info='',login_game_info2='';
                for(i in data){
                    login_game_info += '<li><a href="'+data[i]['href']+'" target="_blank">'+data[i]['title']+'  火爆开启</a></li>';
                    login_game_info2 += '<a href="'+data[i]['href']+'" target="_blank"><span style="width:104px; text-align: center;">'+data[i]['title']+'</span></a>';
                }
                if( !login_game_info2 ) login_game_info2 = '<a href="" target="_blank" title=""><span>您还没有玩过游戏！</span></a>';
                $("#lastServersID").prepend(login_game_info);
                $("#lastServersID2").html(login_game_info2);
            });
        }
        
        //获取开服列表
        var game_id=84;
        var game_url = 'http://www.tanwan.com/index.php?act=gamelogin&game_id='+game_id+'&server_id=';
        $.getJSON('http://www.tanwan.com/api/games.php?game_id='+game_id+'&callback=?',function(data){
            var leftServers=rank_servers='',j=0;
            $.each(data,function(key,value){
                //双线
                if((value['show']==1 || value['show']==3) && j<3){
                    if(value['svrStateID']<3){stateName='待开';}else{stateName='火爆开启';}
                    leftServers +='<a href="'+game_url+value['svrID']+'" target="_blank"><span>'+value['svrName']+'</span><b>'+stateName+'</b></a></li>';
                }
                
                //排行 新手卡
                if(value['svrStateID']>=3){
                    rank_servers += '<option value="'+value['svrID']+'">'+value['svrName']+'</option>';
                }
                j++;
            });
            $("#leftServersID").prepend(leftServers);
            $("#xsk_server").html(rank_servers);
            
            var rank_serversid = document.getElementById("serverItems");
            if(rank_serversid){
                $("#serverItems").html(rank_servers);
                $('#serverItems option:last').attr('selected','selected');
                setTimeout(function(){
                    get_rank_data();
                },1000);
                $("#serverItems,#country").change(function(){get_rank_data();})
            }
        });
        
        //排行接口
        function get_rank_data(){       
            var server_id = $("#serverItems").val();
            var type = $("#country").val();
            
            var rank_data='',bgcolor='',oddid='',jobname='';
            rank_data += '<li class="title"><span>排行</span><b>角色名</b><em>等级</em></li>';
            
            $.getJSON('http://tqcf.tanwan.com/rank.php?server_id='+server_id+'&type='+type+'&callback=?',function(data){
                $.each(data,function(key,value){
                    if(key<=10){
                        j = parseInt(key); 
                        if(j%2==0){oddid='add'}else{oddid='';}
                        rank_data += '<li class="'+oddid+'"><span>'+value['rank']+'</span><b>'+value['role_name']+'</b><em>'+value['level']+'</em></li>';
                    }
                });
                $("#top10").html(rank_data);
            });
        }
