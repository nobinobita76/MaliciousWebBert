<script type="text/javascript" language="javascript">
	function submitCheck(){
		var keywords = document.getElementById('keywords_ad');
		var s_time = document.getElementById('startTime').value;
		var e_time = document.getElementById('endTime').value;
		var myDate = new Date();
		var tishi = '<font color="red">时间跨度请设置在3个月以内</font>';
		var ye = myDate.getFullYear();
		var mo =myDate.getMonth();  
		var da =myDate.getDate();       
		if(s_time=="")
		{
			s_time=ye+"-"+mo+"-"+da;
		}
		if(e_time=="")
		{
			e_time=ye+"-"+mo+"-"+da;
		}
		
		var s_y=s_time.substr(0,4);
		var s_m=s_time.substr(5,2);
		var s_d=s_time.substr(8,2);

		var e_y=e_time.substr(0,4);
		var e_m=e_time.substr(5,2);
		var e_d=e_time.substr(8,2);

		if(keywords.value == ''){
			document.getElementById('keywords_mes').innerHTML = '<font color="red">关键字不能为空</font>';
			return false;
			
		}
		if(e_time<s_time){
			document.getElementById('keywords_mes').innerHTML = '';
			document.getElementById('time_mes').innerHTML = '<font color="red">结束时间不能小于开始时间</font>';
			return false;
		
		}
		document.getElementById('time_mes').innerHTML ='';
		document.getElementById('keywords_mes').innerHTML = '';
		return true;
	}
	function showdate()
	{
		var today, theday;
		today = new Date();
		var d=new Array("星期日","星期一","星期二","星期三","星期四","星期五","星期六");
		var today_year = (today.getYear() < 1900) ? (1900 + today.getYear()) : today.getYear(); 
		theday = today_year+"年" + [today.getMonth()+1]+"月" +today.getDate()+"日 "+ d[today.getDay()];
		document.write(theday+" ");
	}
