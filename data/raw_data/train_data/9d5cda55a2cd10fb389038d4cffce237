                          <li class="menu-item user-btn"><a href="javascript:void(0);" class="user-badge" data-toggle="modal" data-target="#login"><i class="fa fa-user"></i></a></li>
                            <li class="hidden-sm hidden-xs">
                              <a href="/ravelijn-bridge-by-ro-ad.htm?is_mobile=&amp;lang=en_US">
                  En                </a>
              </li>
          </ul>

        </div><!-- /.navbar-collapse -->
