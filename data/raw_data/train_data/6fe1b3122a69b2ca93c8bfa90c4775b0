	<script type="text/javascript">
		var Kx_tag = [1,"Ck_Kx_e0759d487af3cac4f541e88b98c02614"];
		var Ck_tag = document.cookie.match(new RegExp("(^| )" + Kx_tag[1] + "=([^;]*)(;|$)"));
		if(Kx_tag[0]&&Ck_tag==null){
			$("#KuaiXun").stop(true,false).animate({"bottom":0},500);
			$("#KuaiXun i,#KuaiXun p a").click(function(){
				$("#KuaiXun").stop(true,false).animate({"bottom":-50});
				setCookie(Kx_tag[1],1);
			});
			function setCookie(name, value, expires, path, domain) {
				if (typeof expires == "undefined") {
					expires = new Date(new Date().getTime() + 1000 * 3600 * 24);
				}
				document.cookie = name + "=" + escape(value) + ((expires) ? "; expires=" + expires.toGMTString() : "") + ((path) ? "; path=" + path: "; path=/") + ((domain) ? ";domain=" + domain: "");
			}
		}
		else{$("#KuaiXun").hide();}
	</script>
