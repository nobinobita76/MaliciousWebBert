<script type="text/javascript">
var de_len = $("img.delay").length;
for(i=0;i<de_len;i++){var href=$("img.delay").eq(i).attr("data-original");$("img.delay").eq(i).attr("src",href);if(i==de_len-1){$("img.delay").removeClass("delay");}}
$(".minG_det .fr .box").eq(1).nortab("h6.ti a","ul.ph_1");
$(".minG_det .hei_2").nortab("h6.ti a",".minG_li");
;(function(){
    var tab1 = 0;
    var tab2 = 0;
    var play = false;
    var c_obj = $(".minG_det .fortab").eq(0).find("ul.minG_li");
    var c_len = c_obj.length;
    $(".minG_det h6.ti a").click(function(){
        if(play) return;
        var _i = $(this).index();
        if(_i<2){
            if(tab1==_i) return;
            paly = true;
            $(this).addClass("active").siblings(".active").removeClass("active")
            var par = $(this).parents(".det_box");
            par.find(".fortab").eq(tab1).fadeOut(300,function(){
                par.find(".fortab").eq(_i).fadeIn(300,function(){
                    play = false;
                    tab1 = _i;
                });
            });
            if(_i==1){par.find("h6.ti a.change").fadeOut(300);}
            else{par.find("h6.ti a.change").fadeIn(300);}
        }
        else{
            play = true;
            c_obj.eq(tab2%c_len).fadeOut(300,function(){
                tab2++;
                c_obj.eq(tab2%c_len).fadeIn(300,function(){
                    play = false;
                });
            });
        }
    });
})();
$("a").click(function(){
	if($(this).hasClass("bg_href")){return;}
	if($(this).parents(".nogame,.adv,.advs,._til,.til,.nav,.nav2,.friendly,.footer,h6.ti,.head,.top,.det,#loginBox,#crumb").length>0){return;}
	var url = $(this).attr("href");
	if(!url.match("http")){url = $(".nav li.active a").attr("href")+url;}
	var til,imgurl="";
	if($(this).find("img").length>0){
		if($(this).find("h3").length>0){til = $(this).find("h3").html();}
		else{til = $(this).find("p").html();}
		imgurl = $(this).find("img").attr("src");
	}
	else{til = $(this).html();}
	var tl = $(".nav li.active a").html();
	if(login.Logined){Tools.Add(til,tl,url,imgurl);}
	else{Tools.C_add(til,tl,url);}
});
</script>
