<script>
	var showMsg = function (msg) {
		if (window.layer) {
			layer.open({
				content: msg,
				style: "background-color: rgba(0, 0, 0, 0.7); color:#fff; border:none;",
				time: 1
			});
		} else {
			alert(msg);
		}
	}
</script>

<script type="text/javascript">
	$(function () {
		$(".comments")
			.on("click",
				function () {
					window.location.href = "/Comment/" + window.InfoId +
						"/" +
						$(this).attr("idFlag") + "/1";
				});
	});
</script>

<script>
	$(function () {
		$(".like")
			.on("click",
				function () {
					var commentId = $(this).attr("idFlag");
					var temp = $(this).find(".num");
					var temp2 = $(this).children().first();
					temp2.addClass("active");
					if (window.localStorage.getItem(window.UserName + "_" + commentId) === "Praised") {
						showMsg("您已经赞过了~");
						return;
					}
					temp.text(parseInt(temp.text()) + 1);

					var praiseError = function () {
						temp.text(parseInt(temp.text()) - 1);
						temp2.removeClass("active");
					}

					$.ajax({
						type: "GET",
						url: "http://api.comment.zxxk.com/" + "Comment/CommentApi.asmx/CommentPraise?commentId=" + commentId,   // 请求地址
						dataType: "jsonp", // 返回数据类型 jsonp - 跨域
						success: function (response) { // 200 ok status === "success"
							if (response[0].statuscode === 200) {
								window.localStorage.setItem(window.UserName + "_" + commentId, "Praised");
								showMsg("点赞成功~");
								return;
							}
							praiseError();
							showMsg(response[0].message);
							return;
						},
						error: function () {
							praiseError();
							showMsg("服务器开小差了~");
						}
					});
				});
	});
</script>

<script>
	$(function() {
		if (window.IsLogin()) {
			if (!window.CanComment) {
				$("#commentContent").attr("placeholder", "下载以后才能评论哦~下载以后请刷新");
			} else {
				$("#commentContent").attr("placeholder", "说点什么...不要超过180个字~~~");
			}
		} else {
			$("#commentContent").on("focus", function() {
				window.location.href = window.GetLoginUrl();
			});
		}
	});
</script>
<script>
	$("#commentContent").on("focus", function() {
		$('.ui-footer').addClass('ui-footer-keybord');
	});
	$("#commentContent").on("blur", function() {
		$('.ui-footer').removeClass('ui-footer-keybord');
	});
</script>

<script>
	$(function() {
		$("#commentSubmit")
			.on("click",
				function() {
					if (!window.IsLogin()) {
						window.location.href = window.GetLoginUrl();
						return;
					}

					if (!window.CanComment) {
						showMsg("下载以后才能评论哦~下载以后请刷新");
						return;
					}

					var content = $("#commentContent").val().trim();
					content = content.replace(/(\u2006)/g, " ");	// 将IOS Low B 输入法的全角空格改成普通的, 省的出现????乱码
					if (content.length > 180) {
						showMsg("字数超限~");
						return;
					}
					if (content.length < 1) {
						showMsg("不能为空~");
						return;
					}

					var data = {
						InfoId: window.InfoId,
						contents: escape(content),
						Score: 5,
						CommentUserID: 0,
						CommentUserName: '',
						SourceID: 1,
						ProductID: 1
					};
					$.ajax({
						type: "GET",
						url: "http://api.comment.zxxk.com/" +
							"Comment/CommentApi.asmx/AddComment?jsonStr=" +
							JSON.stringify(data), // 请求地址
						dataType: "jsonp", // 返回数据类型 jsonp - 跨域
						success: function() { // 200 ok status === "success"
							$("#commentContent").val("");
							AddNewComment(content);
							RefreshReplyNumLimit();
						},
						error: function() {
							RefreshReplyNumLimit();
						}
					});
				});
	});

	function AddNewComment(contents) {
		// 对Date的扩展，将 Date 转化为指定格式的String
		// 月(M)、日(d)、小时(h)、分(m)、秒(s)、季度(q) 可以用 1-2 个占位符，
		// 年(y)可以用 1-4 个占位符，毫秒(S)只能用 1 个占位符(是 1-3 位的数字)
		// 例子：
		// (new Date()).Format("yyyy-MM-dd hh:mm:ss.S") ==> 2006-07-02 08:09:04.423
		// (new Date()).Format("yyyy-M-d h:m:s.S")      ==> 2006-7-2 8:9:4.18
		// ReSharper disable once NativeTypePrototypeExtending
		Date.prototype.Format = function(fmt) { //author: meizz
			var o = {
				"M+": this.getMonth() + 1, //月份
				"d+": this.getDate(), //日
				"h+": this.getHours(), //小时
				"m+": this.getMinutes(), //分
				"s+": this.getSeconds(), //秒
				"q+": Math.floor((this.getMonth() + 3) / 3), //季度
				"S": this.getMilliseconds() //毫秒
			};
			if (/(y+)/.test(fmt)) fmt = fmt.replace(RegExp.$1, (this.getFullYear() + "").substr(4 - RegExp.$1.length));
			for (var k in o)
				if (o.hasOwnProperty(k))
					if (new RegExp("(" + k + ")")
						.test(fmt))
						fmt = fmt.replace(RegExp.$1,
							(RegExp.$1.length === 1) ? (o[k]) : (("00" + o[k]).substr(("" + o[k]).length)));
			return fmt;
		}

		var newComment =
			'                                                                                               \
            <li class="ui-row-flex ui-border-b">                                                                         \
                <div class="ui-avatar-s">                                                                                \
                    <span style="background-image: url(http://user.zxxk.com/userface/00001.jpg"></span>     \
                </div>                                                                                                   \
                <div class="ui-col user-box">                                                                            \
                    <span class="nickname">$comment.CommentUserName</span>                                               \
                    <span class="time">$comment.CommentTime</span>                                                       \
                    <span class="text">                                                                                  \
                        $comment.Contents                                                                                \
                    </span>                                                                                              \
                </div>                                                                                                   \
            </li>';
		newComment = newComment.replace("$comment.CommentUserName", decodeURIComponent(''));	// 防止中文URL编码
		newComment = newComment.replace("$comment.CommentTime", (new Date()).Format("yyyy-MM-dd hh:mm:ss"));
		newComment = newComment.replace("$comment.Contents", contents);
		$("#commentBox").prepend(newComment);
	}
</script>

<script>
	$(function() {
		$("#commentContent")
			.on("input propertychange",
				function() {
					RefreshReplyNumLimit();
				});
	});

	function RefreshReplyNumLimit() {
		var textarea = $("#commentContent");
		var int = 180 - parseInt(textarea.val().length);
		$("#reply-num").text(int);
		if (int < 0) {
			//$("#commentContent").attr("style", "border-color: red;");
			//$("#reply-num").attr("style", "color: red;");
			textarea.val(textarea.val().substr(0, 180));
			int = 180 - parseInt(textarea.val().length);
			$("#reply-num").text(int);
		} else {
			//$("#commentContent").attr("style","border-color: #bebebe;");
			//$("#reply-num").attr("style","color: #bebebe;");
		}
	}
</script></span>
