		function RemoveCollectionBtnClick() {
			var removeBtn = $("#RemoveCollectionBtn");
			var addBtn = $("#AddCollectionBtn");
			var infoId = removeBtn.attr("idFlag");
			if (!infoId) {return;}
			$.ajax({
				type: "POST",
				url: "/User/DelFavourList",   // 请求地址
				data: "{ eDelFavourListParam : [" + infoId + "] }",
				contentType: "application/json; charset=utf-8", // 数据类型 如果传入Json数据那么只有写成json后台才能有自动绑定, application/x-www-form-urlencoded
				//dataType: "json", // 返回数据类型 jsonp - 跨域
				success: function (response, status) { // 200 ok status === "success"
					if (status === "success") {
						if (response === "True") {
							removeBtn.hide();
							addBtn.show();
							layer.open({
								content: "取消收藏成功",
								style: "background-color: rgba(0, 0, 0, 0.7); color:#fff; border:none;",
								time: 1
							});
						}
					}
				},
				error: function (xmlHttpRequest, textStatus, errorThrown) { // 500
				}
			});
		}
	</script>

	
	<script>
		$(function() {GetCollectionInfo();});

		function GetCollectionInfo() {
			$.ajax({
				type: "POST",
				url: "/GetFavourInfo",   // 请求地址
				data: JSON.stringify({userId: 0, url:"http://www.zxxk.com/soft/5374241.html"}),
				contentType: "application/json; charset=utf-8", // 数据类型 如果传入Json数据那么只有写成json后台才能有自动绑定, application/x-www-form-urlencoded
				//dataType: "json", // 返回数据类型 jsonp - 跨域
				success: function (response,textStatus) { // 200 ok status === "success"
					if (response) {
						var addBtn = $("#AddCollectionBtn");
						var removeBtn = $("#RemoveCollectionBtn");
						addBtn.hide();
						removeBtn.show();
						removeBtn.attr("idFlag", response);
					}
				},
				error: function (xmlHttpRequest, textStatus, errorThrown) { // 500
				}
			});
		}
	</script>
	
