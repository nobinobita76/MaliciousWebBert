	<script>
		$(function() {
			$.ajax({
				type: "GET",
				url: "/Comment/5374241/0/1", // 请求地址
				contentType: "application/json; charset=utf-8",
				// 数据类型 如果传入Json数据那么只有写成json后台才能有自动绑定, application/x-www-form-urlencoded
				dataType: "json", // 返回数据类型 jsonp - 跨域
				success: function(response) { // 200 ok status === "success"
					$("#PreComment").text(response);
				},
				error: function(xmlHttpRequest) {	// 这里才是添加评论HTML的地方
					$("#PreComment").html(xmlHttpRequest.responseText);
					window.IsCommentDownload = true;
				}
			});
		});
	</script>
	
	<script>
		function Unload(cmd) {
			window.location.href = "objc://" + cmd;
		}
	</script>
	
	<script>
		function CommentLocator() {
			if (window.CommentLocatorIntervalTimeOut++ > 30 || window.IsLocatorFinished) {
				clearInterval(window.CommentLocatorIntervalIndex);
			}

			// 判断预览文件是否都加载完了
			var imgs = document.getElementsByTagName("img");
			for (var i = 0; i < imgs.length; i++) {
				if (imgs[i].complete === false) {
					return;
				}
				if (i === imgs.length - 1) {
					window.IsPreviewDownload = true;
				}
			}

			// 如果预览文件和评论内容都下载完了, 进行定位
			if (window.IsCommentDownload && window.IsPreviewDownload && !window.IsLocatorFinished) {
				window.IsLocatorFinished = true;
				var topOffset = $(".comment-area").offset().top;
				$(document).scrollTop(topOffset -
						$(window).height() +
						$(".comment-area").outerHeight() +
						$("#AddCollectionBtn").outerHeight());
			}
		}
		$(function() {
			if (window.location.href.match(/commentLocator/i)) {
				window.CommentLocatorIntervalTimeOut = 0;
				window.CommentLocatorIntervalIndex = setInterval(CommentLocator, 300);
			}
		});
	</script>

</body></html>