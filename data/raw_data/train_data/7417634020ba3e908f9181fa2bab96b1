			function qryAppRelation(){
				$.ajax({
					url: 'http://nm12366.net:80/org/getAppRelationByType',
					data:{"org_type":"2","target_type":"1","target_id":"31500000000"},
					type:'POST',
					dataType :"xml",
					success:function(msg){
						var code = $(msg).find("code").text();
						var message = $(msg).find("message").text();
						var pageNo = $(msg).find("pageNo").text();
						var pageSize = $(msg).find("pageSize").text();
					    var totalPage = $(msg).find("totalPage").text();
						if("0000"==code){//接口调用成功
							lhdj_app.splice(0,lhdj_app.length);
							spba_app.splice(0,spba_app.length);
							ssfp_app.splice(0,ssfp_app.length);
							sbgx_app.splice(0,sbgx_app.length);
							bsq_app.splice(0,bsq_app.length);
							fwq_app.splice(0,fwq_app.length);
							cxq_app.splice(0,cxq_app.length);
							xxq_app.splice(0,xxq_app.length);
							$(msg).find("AppRelation").each(function(i){
								var id=$(this).find("id").text();
								var app_id=$(this).find("app_id").text();
								var name=$(this).find("name").text();
								var group_url=$(this).find("group_url").text();
								var personal_url=$(this).find("personal_url").text();
								var photo=$(this).find("photo").text();
								var forward_url=$(this).find("forward_url").text();
								var islogin=$(this).find("islogin").text();
								var type_id=$(this).find("type_id").text();
								var apply_url="";
								if("" != forward_url && null != forward_url){
									apply_url=forward_url;
								}else{
									if(null != personal_url && "" != personal_url){
										apply_url="http://nm12366.net:80/"+personal_url.split("/")[0]+"/index";
									}
								}
								var photo_url="";//图片地址
								if(null!=photo && ""!=photo){
									if(photo.indexOf("http") > -1){
										if(photo.indexOf("UploadModule")>-1){
											photo_url=photo.substr(photo.indexOf("UploadModule/uploads")+22,photo.length);
											photo_url ="http://www.nm12366.net/UploadModule/uploads/"+photo_url;
										}else{
											photo_url =photo;
										}
									}else{
										photo_url ="http://www.nm12366.net/UploadModule/uploads/"+photo;
									}
								}else{
									photo_url ="http://nm12366.net:80/images/app_icon.png";
								}
								var apps=new Apps(id,name,app_id,apply_url,islogin,photo_url,forward_url);
								if(type_id=='105000'){
									lhdj_app.push(apps);
								}else if(type_id=='106000'){
									sbgx_app.push(apps);
								}else if(type_id=='107000'){
									ssfp_app.push(apps);
								}else if(type_id=='108000'){
									spba_app.push(apps);
								}else if(type_id=='101000'){
									bsq_app.push(apps);
								}else if(type_id=='102000'){
									fwq_app.push(apps);
								}else if(type_id=='103000'){
									xxq_app.push(apps);
								}else if(type_id=='104000'){
									cxq_app.push(apps);
								}
							});
							qryAppRelationByType('lhdj','105000');
							qryAppRelationByType('sbgx','106000');
							qryAppRelationByType('ssfp','107000');
							qryAppRelationByType('spba','108000');
							qryNfAppByType('bsq_nf','101000');
							qryNfAppByType('fwq_nf','102000');
							qryNfAppByType('cxq_nf','104000');
							qryNfAppByType('xxq_nf','103000');
							
						}else{
							//alert("服务器繁忙，请稍候重试");
						}
					},
					error:function(){
						//alert("服务器繁忙，请稍候重试");
					}
				});
			}
			
			
			
			//IE 版本检测
			function ieTest() {
				if($("#iealert").length != 0){
					var isIE=!!window.ActiveXObject; 
					var isIE6=isIE&&!window.XMLHttpRequest; 
					var isIE8=isIE&&!!document.documentMode; 
					var isIE7=isIE&&!isIE6&&!isIE8; 
					if (isIE){ 
						var browser=navigator.appName 
	  					var b_version=navigator.appVersion 
	  					var version=b_version.split(";"); 
	  					var trim_Version=version[1].replace(/[ ]/g,""); 
	  					if(browser=="Microsoft Internet Explorer" && trim_Version=="MSIE6.0") 
	  					{ 
	  						showIET();
	  					} 
	  					else if(browser=="Microsoft Internet Explorer" && trim_Version=="MSIE7.0") 
	  					{ 
	  						showIET();
	  					} 
					} 
				}
			}


			function showIET() {
				if($("#iealert").length != 0){
					$("#iealert").show()
					setTimeout(function(){$("#iealert").hide()}, 5000)
				}
			}
			
			/**取当前围的所有服务链列表**/
			function showLinkLists(showDivId){
			 $.ajax({
					url: 'http://nm12366.net:80/org/sLink',
					data:"opt_flag=qry&if_flag=one&org_id=31500000000&type=1&scope=2&islink=1",
					type:'POST',
					dataType :"xml",
					success:function(msg){
						var code = $(msg).find("code").text();
						var message = $(msg).find("message").text();
						if("0000"==code){//接口调用成功
							var htmlStr = "";
							var htmlStr_left = "";
							var link_l=$(msg).find("Link").length;
							$(msg).find("Link").each(function(i){
								var id=$(this).find("id").text();
								var name=$(this).find("name").text();
								var photo=$(this).find("photo").text();
								var intro=$(this).find("intro").text();
								var linkns=name.split("/");
								if(linkns&&linkns.length==2){
									if(user_type=="2"){
										name=linkns[0];											
									}else{
										name=linkns[1];
									}
								}
								var photo_url="";
								if(null!=photo && ""!=photo){
									if(photo.indexOf("http") > -1){
										if(photo.indexOf("UploadModule")>-1){
											photo_url=photo.substr(photo.indexOf("UploadModule/uploads")+21,photo.length);
											photo_url ="http://www.nm12366.net/UploadModule/uploads/"+photo_url;
										}else{
											photo_url =photo;
										}
									}else{
										photo_url ="http://www.nm12366.net/UploadModule/uploads/"+photo;
									}
								}else{
										photo_url="http://nm12366.net:80/images/nmgs/icon_zx.png";
									}
								if(i==0){
									htmlStr+="<ul class=\"qie_roll qie_roll_rgzz\"><li>"+
									  "      <a href=\"javascript:void(0);\" onclick=\"startLink('"+id+"','"+name+"');\"> "+
			                             "   <img src=\""+photo_url+"\" />"+
			                            "    <b>"+name+"</b>"+
			                           " </a></li>";
								}else if(i%5==0){
									htmlStr+="</ul><ul class=\"qie_roll qie_roll_rgzz\"><li>"+
									 "      <a href=\"javascript:void(0);\" onclick=\"startLink('"+id+"','"+name+"');\"> "+
		                             "   <img src=\""+photo_url+"\" />"+
		                            "    <b>"+name+"</b>"+
		                           " </a></li>";
								}else{
									htmlStr+="<li><a href=\"javascript:void(0);\" onclick=\"startLink('"+id+"','"+name+"');\"> "+
		                             "   <img src=\""+photo_url+"\" />"+
			                            "    <b>"+name+"</b>"+
			                           " </a></li>";
								}

								if(((i+1)%10)==1){
									htmlStr_left+="<ul class=\"qie_con\"><li><a href=\"javascript:void(0)\" onclick=\"startLink('"+id+"','"+name+"');\">"+name+"</a></li>";
								}else if(((i+1)%10)==0){
									htmlStr_left+="<li><a href=\"javascript:void(0)\" onclick=\"startLink('"+id+"','"+name+"');\">"+name+"</a></li></ul>";
								}else{
									htmlStr_left+="<li><a href=\"javascript:void(0)\" onclick=\"startLink('"+id+"','"+name+"');\">"+name+"</a></li>";
								}
							});
							if(null!=htmlStr && ''!=htmlStr){
								$("#"+showDivId).html(htmlStr+"</ul>");
							}else{
								$("#"+showDivId).html(htmlStr);
							}
							dzswj_yy_new_link();
							if(null==htmlStr || ''==htmlStr){
								$("#links_left").remove();
							}else{
								if(link_l%10==0){
								}else{
									htmlStr+="</ul>";
								}
								$("#links_left").html(htmlStr_left);
								left_link_qiehuan();
							}
						}else{
							alert("服务链出现异常："+message);
						}
					}
				});
			}


			function startLink(id,name){
				if(null==cur_user_id || ''==cur_user_id){
					showLoginTips();
					 return;
				}else{
					var id=$('input[name="link_radio"]:checked').val();
					var name=$('input[name="link_radio"]:checked').text();
					if(null==id || ""==id){
						alert("请选择咨询服务链！");
						return;
					}else{
						parent.showDialogue('5',id,name,true);
					}
				}
			}

			function showLoginTips(){
				//scrollTo(0,0);
				$('.login-box-gs,.loginbg-gs').show();
			}


			function qryAppRelationByType(showDivId,type_id){
				var temp=new Array();
				if(type_id=='105000'){
					temp=lhdj_app;
				}else if(type_id=='106000'){
					temp=sbgx_app;
				}else if(type_id=='107000'){
					temp=ssfp_app;
				}else if(type_id=='108000'){
					temp=spba_app;
				}
				var show_app="";
				var htmlStr="";
				if(temp.length>0){
					for(var i = 0;i < temp.length; i++) {
						var id=temp[i].id;
						var name=temp[i].name;
						var app_id=temp[i].app_id;
						var apply_url=temp[i].apply_url;
						var islogin=temp[i].islogin;
						var photo_url=temp[i].photo_url;
						var forward_url=temp[i].forward_url;
						if(null!=forward_url && ''!=forward_url && islogin=='1'){
							show_app=" onclick=\"javascript:showApp('"+forward_url+"','"+app_id+"','"+islogin+"')\"";
							show_app+=" href=\"javascript:void(0);\" ";
						}else{
							if(null==apply_url || ""==apply_url){
								show_app="  target=\"_blank\" ";
							}else{
								show_app=" href=\""+apply_url+"\" target=\"_blank\" ";
							}
						}
						var span_styles="";
						if(name.length<6 || name.length==6){
							span_styles=" class=\"big_line\" ";
						}
						if(i==0){
							htmlStr+="<ul class=\"qie_roll\"><li>"+
							"<a "+show_app+">"+
							"<img src=\""+photo_url+"\" height=\"36\" width=\"36\"/><b "+span_styles+">"+name+"</b></a></li>";
						}else if(i%4==0){
							htmlStr+="</ul><ul class=\"qie_roll\"><li>"+
							"<a "+show_app+">"+
							"<img src=\""+photo_url+"\" height=\"36\" width=\"36\"/><b "+span_styles+" >"+name+"</b></a></li>";
						}else{
							htmlStr+="<li><a "+show_app+">"+
							"<img src=\""+photo_url+"\" height=\"36\" width=\"36\"/><b "+span_styles+">"+name+"</b></a></li>";
						}
					}
					if(null!=htmlStr && ''!=htmlStr){
						$("#"+showDivId).html(htmlStr+"</ul><div class=\"clearfix\"></div>");
					}else{
						$("#"+showDivId).html(htmlStr+"<div class=\"clearfix\"></div>");
					}
					dzswj_yy_new(showDivId);
				}
				
			}
			
			function qryNfAppByType(showDivId,type_id){
				var temp=new Array();
				if(type_id=='101000'){
					temp=bsq_app;
				}else if(type_id=='102000'){
					temp=fwq_app;
				}else if(type_id=='103000'){
					temp=xxq_app;
				}else if(type_id=='104000'){
					temp=cxq_app;
				}
				var show_app="";
				var htmlStr="";
				if(temp.length>0){
					for(var i = 0;i < temp.length; i++) {
						var id=temp[i].id;
						var name=temp[i].name;
						var app_id=temp[i].app_id;
						var apply_url=temp[i].apply_url;
						var islogin=temp[i].islogin;
						var photo_url=temp[i].photo_url;
						var forward_url=temp[i].forward_url;
						
						if(null!=forward_url && ''!=forward_url && islogin=='1'){
							show_app=" onclick=\"javascript:showApp('"+forward_url+"','"+app_id+"','1');\"";
							show_app+=" href=\"javascript:void(0);\" ";
						}else{
							if(null==apply_url || ""==apply_url){
								show_app="  target=\"_blank\" ";
							}else{
								show_app=" href=\""+apply_url+"\" target=\"_blank\" ";
							}
						}
						var span_styles="";
						if(name.length<6 || name.length==6){
							span_styles=" class=\"big_line\" ";
						}
						/**
						if(i==0){
							htmlStr+="<li> <a "+show_app+"><img src=\""+photo_url+"\" style=\"width:33px;height:33px;\"/><span "+span_styles+">"+name+"</span></a>";
						}else if((i+1)%2==0 && i==temp.length-1){
							htmlStr+="<a "+show_app+"><img src=\""+photo_url+"\" style=\"width:33px;height:33px;\"/><span "+span_styles+">"+name+"</span></a></li>";
						}else if((i+1)%2==0 && i!=temp.length-1){
							htmlStr+="<a "+show_app+"><img src=\""+photo_url+"\" style=\"width:33px;height:33px;\"/><span "+span_styles+">"+name+"</span></a></li><li>";
						}else{
							htmlStr+="<a "+show_app+"><img src=\""+photo_url+"\" style=\"width:33px;height:33px;\"/><span "+span_styles+">"+name+"</span></a>";
						}**/
						if(i==0){
							htmlStr+="<ul class=\"qie_roll\"><li>"+
							"<a "+show_app+">"+
							"<img src=\""+photo_url+"\" height=\"42\" width=\"40\"/><b "+span_styles+">"+name+"</b></a></li>";
						}else if(i%4==0){
							htmlStr+="</ul><ul class=\"qie_roll\"><li>"+
							"<a "+show_app+">"+
							"<img src=\""+photo_url+"\" height=\"42\" width=\"40\"/><b "+span_styles+">"+name+"</b></a></li>";
						}else{
							htmlStr+="<li><a "+show_app+">"+
							"<img src=\""+photo_url+"\" height=\"42\" width=\"40\"/><b "+span_styles+">"+name+"</b></a></li>";
						}
					}
				}
				if('cxq_nf'==showDivId){
					if(temp.length%4==0){
						$("#"+showDivId).html(htmlStr+"</ul><ul class=\"qie_roll\"><li><a  href=\"javascript:void(0);\" onclick=\"javascript:showApp('','','1');\"><img src=\"http://nm12366.net:80/images/li_three_02.png\" /><b>群组交流</b></a></li></ul><div class=\"clearfix\"></div>");
					}else{
						$("#"+showDivId).html(htmlStr+"<li><a  href=\"javascript:void(0);\" onclick=\"javascript:showApp('','','1');\"><img src=\"http://nm12366.net:80/images/li_three_02.png\" /><b>群组交流</b></a></li></ul><div class=\"clearfix\"></div>");
					}
				}else{
					$("#"+showDivId).html(htmlStr);
				}
				dzswj_yy_new(showDivId);
		}
	</script>
