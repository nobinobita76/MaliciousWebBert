	function chage(id){
		  try{
			   if(window.parent.op_src && typeof(window.parent.op_src) == "function"){
				   window.parent.location.href = 'http://nm12366.net:80/main/main?chageOrg='+id;
			   }else{
				   location.href =  'http://nm12366.net:80/org/index?org_id='+id;
			   }
			  }catch(e){
				  location.href =  'http://nm12366.net:80/org/index?org_id='+id;
			  }
	}
	var basePath='http://nm12366.net:80/';
	var cur_user_id="";
	var user_type="";
	var state='';

	


	/*
	 * 头部登录后
	 */
	function showTopUserInfo(data){
		var mrw = data.seesion_org_id;
		cur_user_id=data.user_id;
		var html = '<ul>';
		html+='<li class="grzx"><a href="'+basePath+'main/myIndex"> '+data.realname+' </a></li>';
		$.each(JSON.parse(data.items),function(idx,obj){
			if(mrw == obj.id){
				html+='<li class="w_topbar" onmouseover="displaySubMenu(this)" onmouseout="hideSubMenu(this)" >'+obj.jc;
			}
		});
		html+='<ul class="w_barlist">';
		$.each(JSON.parse(data.items),function(idx,obj){
				html+='<li><a onclick="chage(\''+obj.id+'\');" href="javascript:void(0);">'+obj.jc+'</a></li>';
		});
		html+='</ul></li>'+
		        '</li>'+
		 		'<li><a href="javascript:void(0);" onclick="javascript:window.top.location.href=\'http://nm12366.net:80/login/loginout?loginout=true\'">[退出]</a></li>'+
			     '<div class="clearfix"></div>'+
			 '</ul>';
			$(".user_msg").html(html);
	}





	/**
	 * 申请加入外围
	 */
	function joinOrg(){
		console.log("cur_user_id="+cur_user_id);
		if(""==cur_user_id){
			window.location.href="http://nm12366.net:80/org/index?org_id=31500000000";
		}else{
			if(state=='2'){//审核中
				alert("您已经申请加入此组织，申请正在审核中！");
				return;
			}else if(state=='3'){//拒绝
				alert("对不起，您已经被拒绝加入此组织！");
				return;
			}else if(state=='4'){//停用
				alert("对不起，您在此组织中已经被停用！");
				return;
			}
			$.ajax({
				url: 'http://nm12366.net:80/org/sUserOrgDepRole',
				data:{"opt_flag":"save","type":"2","org_id":"31500000000","user_id":cur_user_id,"role_id":"NORMAL"},
				async: false,
				cache: false, 
				type:'POST',
				dataType :"xml",
				success:function(msg){
					var code = $(msg).find("code").text();
					var message = $(msg).find("message").text();
					var htmlStr="";
					if("0000"==code){
						alert("申请加入组织成功！");
						window.location.href="http://nm12366.net:80/org/index?org_id=31500000000";
					}else{
						alert(message);
						return;
					}
				}
			});
		}
	}
	
	
	/**
	通知公告
	**/
	function qryNotice(){
	$.ajax({
		url: 'http://nm12366.net:80/cms/getCurOrgAndPriorOrgNotice',
		data:{"pageNo":"1","pageSize":"4","siteroot":"31500000000","sitetype":"WW"},
		type:'POST',
		dataType :"xml",
		success:function(msg){
			var code = $(msg).find("RETURN_CODE").text();
			var message = $(msg).find("RETURN_MSG").text();
			if("0000"==code){//接口调用成功
				var htmlStr_loop = "";
				var htmlStr_more = "";
				$(msg).find("Info").each(function(i){
					var id=$(this).find("id").text();
					var title=$(this).find("title").text();
					var fabutime=$(this).find("fabutime").text();
					var siteroot=$(this).find("siteroot").text();
					var sitetype=$(this).find("sitetype").text();
					var outurl=$(this).find("outurl").text();
					var title_str="";
					if(title.length>18){
						title_str=title.substring(0,18);
					}else{
						title_str=title;
					}
					if(title.length>26){
						title=title.substring(0,26)+"...";
					}
					if(fabutime.length>10){
						fabutime=fabutime.substring(0,10);
					}
					
					if(i==0){
						if(null!=outurl && ''!=outurl){
							htmlStr_loop+=" <li>"+
				                 "   <a href=\"javascript:void(0);\" onclick=\"window.location.href='"+outurl+"'\">"+
				                 fabutime+"  "+title_str+
				                 "	</a>"+
				                 " <a href=\"http://nm12366.net:80/cms/list?menuid=33150&siteroot=31500000000&sitetype=GW\" class=\"more\">更多&gt;</a> "+
				                 "</li>";
							htmlStr_more+=" <li>"+
			                 " <span>"+fabutime+"</span> "+
			                 "   <a href=\"javascript:void(0);\" onclick=\"window.location.href='"+outurl+"'\">"+
			                 title+
			                 "	</a>"+
			                 "</li>";
						}else{
							htmlStr_loop+=" <li>"+
			                 "   <a href=\"javascript:void(0);\" onclick=\"window.location.href='http://nm12366.net:80/cms/infoDetail?siteroot="+siteroot+"&sitetype="+sitetype+"&infoid="+id+"'\">"+
			                 fabutime+"  "+title_str+
			                 "	</a>"+
			                 " <a href=\"http://nm12366.net:80/cms/list?menuid=33150&siteroot=31500000000&sitetype=GW\" class=\"more\">更多&gt;</a> "+
			                 "</li>";
							htmlStr_more+=" <li>"+
			                 " <span>"+fabutime+"</span> "+
			                 "   <a href=\"javascript:void(0);\" onclick=\"window.location.href='http://nm12366.net:80/cms/infoDetail?siteroot="+siteroot+"&sitetype="+sitetype+"&infoid="+id+"'\">"+
			                 title+
			                 "	</a>"+
			                 "</li>";
						}
					}else{
						if(null!=outurl && ''!=outurl){
							htmlStr_more+=" <li>"+
			                 " <span>"+fabutime+"</span> "+
			                 "   <a href=\"javascript:void(0);\" onclick=\"window.location.href='"+outurl+"'\">"+
			                 title+
			                 "	</a>"+
			                 "</li>";	
						}else{
							htmlStr_more+=" <li>"+
			                 " <span>"+fabutime+"</span> "+
			                 "   <a href=\"javascript:void(0);\" onclick=\"window.location.href='http://nm12366.net:80/cms/infoDetail?siteroot="+siteroot+"&sitetype="+sitetype+"&infoid="+id+"'\">"+
			                 title+
			                 "	</a>"+
			                 "</li>";	
						}
					}
				});
				$("#modWqNotice-1").html(htmlStr_loop);
				$("#orgNotice").html(htmlStr_more);
			}else{
				//alert("服务器繁忙，请稍候重试");
			}
		},
		error:function(){
			//alert("服务器繁忙，请稍候重试");
		}
	});
	}
	  		

