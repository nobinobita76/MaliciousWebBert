	/*加载围群cms的静态页面*/
	function loadCmsHtml(){
		$.ajax({
			url:"http://nm12366.net:80/org/cmsTempleteUrl",
			type:'POST',
			data:{"siteroot":"31500000000","sitetype":"GW"},
			dataType: "xml",
			beforeSend:function(){
				layer.load(2);
			},
			success: function(data) {
				var code=$(data).find("RETURN_CODE").text();
				var msg=$(data).find("RETURN_MSG").text();
				var url=$(data).find("URL").text();
				if("0000"==code&&null!=url&&""!=url){
					$.ajax({
						url:"http://nm12366.net:80/"+url,
						dataType: "html",
						success:function(htm){
							$("#con_show").html(htm);
						}
					});
					layer.closeAll();
				}else{
					alert(msg);
					$("#con_show").html(msg);
					layer.closeAll();
				}
			},
			error:function(XMLHttpRequest, textStatus, errorThrown){
				//console.log(XMLHttpRequest.status);
				//console.log(XMLHttpRequest.readyState);
				//console.log(textStatus);
				//console.log(errorThrown);
				layer.closeAll(2);
			},
			complete:function(XMLHttpRequest, textStatus){
				
			}
		});
	}

	function showLogin(){
		$.ajax({
			url: "http://nm12366.net:80/login/sessionCheck",
			contentType: "application/x-www-form-urlencoded; charset=utf-8", 
			type:'POST',
			dataType: "JSON",
			timeout:1000,
			success: function(data) {
				if("401" == data.code){//显示登录窗口
					$("#user_login").css("display","block");
					$("#user_join").css("display","none");
					
				}else{//显示登录提示信息
					var user_state=data.user_org_state;
					if(null!=user_state && "1"==user_state){//用户已在围中，跳转到围门户。但是并不会走到这一步
						window.location.href="http://nm12366.net:80/org/index?org_id="+data.current_org_id; 
					}else{//显示用户加入围提示
						$("#user_login").css("display","none");
						$("#user_join").css("display","block");
					}
					showTopUserInfo(data);
				}
			},
			error:function(xml){
				//layer.msg("服务器繁忙，请稍候重试",2, {type:2});
			}
		});
	}
  		
