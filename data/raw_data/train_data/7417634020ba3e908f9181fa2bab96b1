                    <!--label><input type="checkbox">记住密码</label-->
                   
                    <a href="http://nm12366.net:80/login/findPassword">忘记密码？</a>
                </li>
        	</ul>
        </div>
        </form>
    </div>
</div>

<!--登陆弹窗end-->
<script type="text/javascript">
	function showApp(apply_url,apply_id,islogin){
		if(null!=islogin&&"1"==islogin){//需要登录
			$(".login-box-gs,.loginbg-gs").show();
			scrollTo(0,0);
		}else{//不需要登录
			window.open("http://nm12366.net:80/org/showApp?url="+apply_url+"&apply_id="+apply_id+"&apply_flag=1",'_blank','height=600,width=1000,top=50,left=10,toolbar=no,menubar=no,scrollbars=yes,resizable=yes,location=no,status=no');
		}
	}

	function downHelp(file_url){
		window.location.href="http://nm12366.net:80/upload/download?filepath="+encodeURI(file_url)+"&rm="+new Date().getTime();
	}
    //登陆弹窗
    $(function (){
        // 关闭
        $(".close-login").click(function () {
            $(".login-box-gs,.loginbg-gs").hide();
        })
        //显示
        $("#login_dl").click(function () {
            $(".login-box-gs,.loginbg-gs").show();
        })
    })
	function reloadYZM(obj){
		$(obj).attr("src",'http://nm12366.net:80/login/vcode?r='+Math.random());
	}

     /*修改input的type  IE8*/
     /*text是点击时触发此事件，password是离开是触发*/
	function updInput(obj,className){
		if(null!=obj && null!=className && ""!=className){
			var value=obj.value;
			var type=obj.type;
			if(type=="password"){
				if(value==''){
					$("input[name='"+className+"'][type='password']").css("display","none");
					$("input[name='"+className+"'][type='text']").css("display","block");
				}
			}
			if(type=="text"){
				$("input[name='"+className+"'][type='text']").css("display","none");
				$("input[name='"+className+"'][type='password']").css("display","block");
				$("input[name='"+className+"'][type='password']").focus();
			}

		}
	}
	
	function removeSendTimes(){
			var mobile=$("#mobile").val();
			$.ajax({ 
	    	url:'http://nm12366.net:80/login/removeSendCodeTimes',
	    	async: false,
		    cache: false, 
		    type:'POST',
		    data:{'mobile':mobile},
	    	dataType: "xml",
	    	success: function(xml) {
	    		var code=$(xml).find("code").text();
		       if("0000" == code){
		       		
		       }
	   	 	} 
	 	});	
	}
	
	/**
	 * 登录验证
	 * flag:3税务人员内网登录，2税务人员登录，1税务人员获取验证码，0纳税人登录
	 */
	var login_u="";
	var login_p="";
	function loginCheck(flag){
		var u_n;
		var u_p;
		if("3"==flag){
			u_n=$("#swrybm").val();
			u_p=$("input[name='swrybm_pwd'][type='password']").val();
			if("税务人员编码"==u_n){
				u_n="";
			}
			if(u_n&&cTrim(u_n,0)!=""){
			}else{
				layer.alert("请输入税务人员编码！",8);
				return false;
			}
			if(u_n.length!=11){
				layer.alert("请输入合法的税务人员编码！",8);
				return false;
			}
			if("密码"==u_p){
				u_p="";
			}
			if(u_p&&cTrim(u_p,0)!=""){
			}else{
				layer.alert("请输入密码！",8);
				return false;
			}
			$.ajax({
	            url:"http://nm12366.net:80/login/loginDx",
	        	async: false,
			    cache: false, 
			    type:'POST',
			    data:{"username":u_n,"password":u_p,"flag":"3"},
	        	dataType: "xml",
	        	beforeSend:function(){
					layer.load(10, 0); //如不想让加载自动关闭，第一个参数设置0即可，如：layer.load(0);
				},
	    		success: function(msg) {
					var code = $(msg).find("code").first().text();
					var message = $(msg).find("message").text();
					layer.closeAll();
	    			if("0000"==code){
	    				var l_form = $("#login_form");
						l_form[0].submit();
	    			}else{
	    				layer.alert(message,8);
	    			}
	   	 		} ,
	    		error:function(msg){
	   	 			layer.alert("服务器繁忙，请稍候重试!",8);
	    		}
			});
		}else if("2"==flag){
			u_n=$("#swrybm").val();
			u_p=$("input[name='swrybm_pwd'][type='password']").val();
			if("税务人员编码"==u_n){
				u_n="";
			}
			if(u_n&&cTrim(u_n,0)!=""){
			}else{
				layer.alert("请输入税务人员编码！",8);
				return false;
			}
			if(u_n.length!=11){
				layer.alert("请输入合法的税务人员编码！",8);
				return false;
			}
			if(login_u==""){
				layer.alert("请先获取短信验证码",8);
				return false;
			}
			if(login_u!=u_n){
				layer.alert("您输入的税务人员编码已更改，请重新获取短信验证码",8);
				return false;
			}
			if("密码"==u_p){
				u_p="";
			}
			if(u_p&&cTrim(u_p,0)!=""){
			}else{
				layer.alert("请输入密码！",8);
				return false;
			}
			if(u_p==""){
				layer.alert("请先获取短信验证码",8);
				return false;
			}
			if(login_p!=u_p){
				layer.alert("您输入的密码已更改，请重新获取短信验证码",8);
				return false;
			}
			var u_dx=$("#dx_yzm").val();
			if("短信验证码"==u_dx){
				u_dx="";
			}
			if(""==u_dx){
				layer.alert("请输入您收到的短信验证码",8);
				return false;
			}
			/*获取短信验证码*/
			$.ajax({
	            url:"http://nm12366.net:80/login/loginDx",
	        	async: false,
			    cache: false, 
			    type:'POST',
			    data:{"username":u_n,"password":u_p,"flag":"2","dx_yzm":u_dx},
	        	dataType: "xml",
	        	beforeSend:function(){
					layer.load(10, 0); //如不想让加载自动关闭，第一个参数设置0即可，如：layer.load(0);
				},
	    		success: function(msg) {
					var code = $(msg).find("code").first().text();
					var message = $(msg).find("message").text();
					layer.closeAll();
	    			if("0000"==code){
	    				var l_form = $("#login_form");
						l_form[0].submit();
	    			}else{
	    				layer.alert(message,8);
	    			}
	   	 		} ,
	    		error:function(msg){
	   	 			layer.alert("服务器繁忙，请稍候重试!",8);
	    		}
			});
		}else if("1"==flag){
			u_n=$("#swrybm").val();
			u_p=$("input[name='swrybm_pwd'][type='password']").val();
			if("税务人员编码"==u_n){
				u_n="";
			}
			if(u_n&&cTrim(u_n,0)!=""){
			}else{
				layer.alert("请输入税务人员编码！",8);
				return false;
			}
			if(u_n.length!=11){
				layer.alert("请输入合法的税务人员编码！",8);
				return false;
			}
			login_u=u_n;
			if("密码"==u_p){
				u_p="";
			}
			if(u_p&&cTrim(u_p,0)!=""){
			}else{
				layer.alert("请输入密码！",8);
				return false;
			}
			login_p=u_p;
			/*获取短信验证码*/
			$.ajax({
	            url:"http://nm12366.net:80/login/loginDx",
	        	async: false,
			    cache: false, 
			    type:'POST',
			    data:{"username":u_n,"password":u_p,"flag":"1"},
	        	dataType: "xml",
	        	beforeSend:function(){
					layer.load(10, 0); //如不想让加载自动关闭，第一个参数设置0即可，如：layer.load(0);
				},
	    		success: function(msg) {
					var code = $(msg).find("code").first().text();
					var message = $(msg).find("message").text();
					layer.closeAll();
	    			if("0000"==code){
	    				sendtime(100,"shouji");//倒计时
	    			}else{
	    				layer.alert(message,8);
	    			}
	   	 		} ,
	    		error:function(msg){
	   	 			layer.alert("服务器繁忙，请稍候重试!",8);
	    		}
			});
		}else if("0"==flag){
			u_n = $("#nsrsbh").val();
			if("手机号码" == u_n){
				u_n="";
			} 
			if(u_n&&cTrim(u_n,0)!=""){
			}else{
				layer.alert("请输入手机号码！",8);
				return false;
			}
			u_p = $("input[name='nsrsbh_pwd'][type='password']").val();
			if("密码"==u_p){
				u_p="";
			}
			if(u_p&&cTrim(u_p,0)!=""){
			}else{
				layer.alert("请输入密码！",8);
				return false;
			}
			var u_m = $("input[name=yzm]").val();
			if("验证码"==u_m){
				u_m="";
			}
			if(u_m&&cTrim(u_m,0)!=""){
				var y_l = len(u_m);
				if(y_l==4){
					$.post('http://nm12366.net:80/login/checkVcode',{yzm:u_m},function(data){
						var json =null;
						try{
							json = eval("("+data+")")
						}catch(e){
						}
						var state = json.state;
						if(0==state){
						
							//页面数据进行验证之后，判断登录
				  			var org_id="";
				  			var org_name="";
					 		$.ajax({
					            url:"http://nm12366.net:80/login/loginCheck",
							    cache: false, 
							    type:'POST',
							    data:{"username":u_n,"password":u_p,"flag":"0","r":Math.random()},
					        	dataType: "text",
					        	beforeSend:function(){
									layer.load(10,0); //如不想让加载自动关闭，第一个参数设置0即可，如：layer.load(0);
								},
				        		success: function(msg) {
									if(msg=='用户被冻结'){
				        				layer.alert('用户被冻结，请点击激活！',8,function(){
				        					window.location.href="http://nm12366.net:80/login/activateUser";
					        			});
				        			}else if(msg=='yes'){
				        				var l_form = $("#login_form");
										l_form[0].submit();
				        			}else{
				        				layer.closeAll();
				        				alert(msg);
				        				$("#nsrsbh_pwd").val("");
				        				$("#yzm").val("");
				        				reloadYZM($("#yzm_img")[0]);
				        			}
				       	 		} ,
				        		error:function(msg){
				       	 			layer.alert("服务器繁忙，请稍候重试!",8);
					    		}
							});
							
						}else{
							layer.alert(json.msg,8);
							reloadYZM($("#yzm_img")[0]);
						}
					},'text');
				}else{
					layer.alert("请输入正确的验证码！",8);
					return false;
				}
			}else{
				layer.alert("请输入验证码！",8);
				$("#yzm").focus();
				return false;
			}
		}else{
			layer.alert("非法登录",8);
		}
	}
	
	//点击按钮后倒计时
	var secs;
	var againSend; 
	function sendtime(obj,type){
		secs=obj;
		againSend = document.getElementById('dx_btn');
		againSend.disabled=true;
		for(i=1;i<=secs;i++) { 
			window.setTimeout("update(" + i + ")", i * 1000); 
		} 
	}
	function update(num) { 
		if(num == secs) { 
			againSend.value ="获取手机验证码"; 
			againSend.disabled=false; 
		}else{ 
			printnr = secs-num; 
			againSend.value = "("+printnr+")秒后重试"; 
		} 
	} 
	
	$(function(){
		// 绑定回车事件 
		$('#yzm').keypress(function(e){
			if(e.keyCode==13){
				$('#login_btn_nsr').click();
			}
		});
		$("#dx_btn").keypress(function(e){
			if(e.keyCode==13){
				$('#login_btn_swr').click();
			}
		});
		
		/**
			培训辅导下拉二级菜单显示
		**/
		 $(".pxfd").mousemove(function(event) {
		        /* Act on the event */
		        $(this).css('background','url(http://nm12366.net:80//images/dzswj/nav_a_hover.jpg) no-repeat');
		        $(this).children('.pxfd_bg').css('color','#005bcc');
		    });
		    $(".pxfd").mouseleave(function(event) {
		        /* Act on the event */
		        $(this).css('background','none');
		        $(this).children('.pxfd_bg').css('color','#fff');
		    });
		  /**
		  	菜单切换
		  **/
		 $(".nav_right a").click(function(event){
		        /* Act on the event */
		        $(this).parent().addClass('selected');
		        $(this).parent().siblings().removeClass('selected');
		    });
	});
</script>

	
