	     document.onreadystatechange = showDiv;//当页面加载状态改变的时候执行这个方法. 
			function showDiv(){ 
				if(document.readyState == "complete"){
					if(null=='null' || ''=='null' || 'null'=='null'){
						$.layer({
						    type: 2,
						    shade: [0],
						    fix: false,
						    title: '温馨提示',
						    maxmin: true,
						    iframe: {src : 'http://nm12366.net:80/jsp/showTips.jsp'},
						    area: ['850px', '550px'],
						});
					}
				}
			}
	     
	</script>
 
	
