	  		function op_app(apply_url,apply_id){
	  			window.open("http://nm12366.net:80/org/showApp?url="+apply_url+"&apply_id="+apply_id+"&apply_flag=1",'_blank','height=600,width=1000,top=50,left=10,toolbar=no,menubar=no,scrollbars=yes,resizable=yes,location=no,status=no');
