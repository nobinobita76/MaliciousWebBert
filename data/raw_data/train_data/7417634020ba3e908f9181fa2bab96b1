	<script>new srcMarquee("ScrollMe",0,1,418,75,3,2000,2000,75);
	
	function dzswj_yy_new(obj){
		 var len=$('.qie_roll').width();
		    var wall=0;
		    $('#'+obj).each(function(){
		        var b=$(this).find('.qie_roll').length;
		        wall=len*b; 
		        $(this).width(wall);
		    })
		    $('#'+obj).siblings('.qie_right').click(function(){
		            var sz=$(this).parents('.remind').find('.qie_roll').length;
		            var wh=len*(sz-2);
		            var wid=len*(sz-1);
		            $(this).parents('.remind').find('.qie_left').removeClass('qie_left1');
		            var l=$(this).parents('.remind').find('.qie_text').css('right');
		            if(Math.abs(parseInt(l))>=wh){
		                $(this).addClass('qie_right1');
		                $(this).parents('.remind').find('.qie_text').animate({'right':'+'+wid+'px'})
		            }else{
		                $(this).parents('.remind').find('.qie_text').animate({'right':'+='+len+'px'})
		            }
		        })
		     $('#'+obj).siblings('.qie_left').click(function(){
		            $(this).parents('.remind').find('.qie_right').removeClass('qie_right1');
		            var l=$(this).parents('.remind').find('.qie_text').css('right');
		            var wh=len*(-1);
		            if(l>=wh+"px"){
		                $(this).addClass('qie_left1');
		                $(this).parents('.remind').find('.qie_text').animate({'right':'0px'})
		            }else{
		                $(this).parents('.remind').find('.qie_text').animate({'right':'+='+len+'px'})
		            }
		        })
		}

	function dzswj_yy_new_link(){
		var len=$('.qie_roll_rgzz').width();
        var wall=0;
        $('.qie_text_zxlist').each(function(){
            var b=$(this).find('.qie_roll_rgzz').length;
            wall=len*b; 
            $(this).width(wall);
        })
        $('.qie_right_rgzx').click(function(){
                var sz=$(this).parents('.zxlist_remind').find('.qie_roll_rgzz').length;
                var wh=len*(sz-2);
                var wid=len*(sz-1);
                $(this).parents('.zxlist_remind').find('.qie_left_rgzx').removeClass('qie_left_rgzx1');
                var l=$(this).parents('.zxlist_remind').find('.qie_text_zxlist').css('right');
                if(Math.abs(parseInt(l))>=wh){
                    $(this).addClass('qie_right_rgzx1');
                    $(this).parents('.zxlist_remind').find('.qie_text_zxlist').animate({'right':'+'+wid+'px'})
                }else{
                    $(this).parents('.zxlist_remind').find('.qie_text_zxlist').animate({'right':'+='+len+'px'})
                }
            })
        $('.qie_left_rgzx').click(function(){
                $(this).parents('.zxlist_remind').find('.qie_right_rgzx').removeClass('qie_right_rgzx1');
                var l=$(this).parents('.zxlist_remind').find('.qie_text_zxlist').css('right').replace("px","");
                if(l>=len){
	                var wh=len*(-1);
                    $(this).addClass('qie_left_rgzx1');
                    $(this).parents('.zxlist_remind').find('.qie_text_zxlist').animate({'right':Math.abs(parseInt(l)+wh)+'px'})
                }else{
                    $(this).parents('.zxlist_remind').find('.qie_text_zxlist').animate({'right':'0px'})
                }
            })
		}

	if(null!=cur_user_id && ''!=cur_user_id && 'undefined'!=cur_user_id){
		$(".zxzs").remove();
		$("#bg_div").removeAttr("class")
	}else{
		$("#bg_div").attr("class","all_bg")
		}

	$(function(){
        /**
        	气泡提示
        **/
	    $(".zxzs_cf").hover(function(){
	        $(".zxzs_right",this).show();
	    });
	    $(".zxzs_cf").mouseleave(function(){
	        $(".zxzs_right",this).hide();
	    });
	});

	function left_link_qiehuan(){
		 var len=$('.qie_con').width();
	        var wall=0;
	        $('.zxzs_con').each(function(){
	            var b=$(this).find('.qie_con').length;
	            wall=len*b; 
	            $(this).width(wall);
	        })
	        $('.zxzs_qie_right').click(function(){
	                var sz=$(this).parents('.zxzs_text').find('.qie_con').length;
	                var wh=len*(sz-2);
	                var wid=len*(sz-1);
	                $(this).parents('.zxzs_text').find('.zxzs_qie_left').removeClass('zxzs_qie_left1');
	                var l=$(this).parents('.zxzs_text').find('.zxzs_con').css('left');
	                if(Math.abs(parseInt(l))>=wh){
	                    $(this).addClass('zxzs_qie_right1');
	                    $(this).parents('.zxzs_text').find('.zxzs_con').animate({'left':'-'+wid+'px'})
	                }else{
	                    $(this).parents('.zxzs_text').find('.zxzs_con').animate({'left':'-='+len+'px'})
	                }
	            })
	        $('.zxzs_qie_left').click(function(){
	                $(this).parents('.zxzs_text').find('.zxzs_qie_right').removeClass('zxzs_qie_right1');
	                var l=$(this).parents('.zxzs_text').find('.zxzs_con').css('left');
	                var wh=len*(-1);
	                if(l>=wh+"px"){
	                    $(this).addClass('zxzs_qie_left1');
	                    $(this).parents('.zxzs_text').find('.zxzs_con').animate({'left':'0px'})
	                }else{
	                    $(this).parents('.zxzs_text').find('.zxzs_con').animate({'left':'+='+len+'px'})
	                }
	            });
		}

	</script>
