<script type="text/javascript">    (function (a) { a('[data-role="carousel"]').each(function () { var l = a(this), C = "click", c = { autoPlay: true, speed: 300, timeOut: 3000 }, b = l.data("opt"); if (b != undefined) { var i = b.replace(/\'/g, '"'); try { b = JSON.parse(i) } catch (f) { b = null } } a.extend(c, b); var s = l.find(".slider > .slider-box"), v = {}, e = 0, r = 0, d = 0, q = 0, A = 0, u = 0, B = 0, p = a(window).width(), k = 0, j = 0, m, g = a(".slider-item", s); a(window).on("resize", E); function E() { var n = g.length, o = a(window).width(); s.css({ width: n * o + "px" }); g.css({ width: o }); if (B != 0) { B = -u * o; s.css({ "-webkit-transform": "translate3d(" + B + "px,0,0)" }) } } E(); s.on("touchstart", function () { clearInterval(m); var n = event.targetTouches[0]; e = n.pageX; d = n.pageY; v.S = false; v.T = false; s.css({ "-webkit-transition-duration": "0ms" }) }).on("touchmove", function (o) { var t = o.targetTouches[0]; r = t.pageX; q = t.pageY; A = r - e; k = Math.abs(A); j = Math.abs(q - d); if (!v.T) { var n = k < j; v.T = true; v.S = n } if (!v.S) { o.stopPropagation(); o.preventDefault(); s.css({ "-webkit-transform": "translate3d(" + (A - p * u) + "px,0,0)" }) } }).on("touchend", function (n) { if (A > 100) { u-- } else { if (A < -100) { u++ } } if (u <= 0) { u = 0 } else { if (u >= g.length - 1) { u = g.length - 1 } } if (c.autoPlay) { h() } if (!v.S && !v.T) { } else { if (!v.S) { z(); n.preventDefault() } } }); function z() { var x = l.find(".navi"), o = a(window).width(); if (x.length > 0) { a("i", x).eq(u).addClass("activate").siblings().removeClass("activate") } var t = l.find(".navi").find(".prev"), n = l.find(".navi").find(".next"); if (u == 0) { t.hide() } else { if (u == g.length - 1) { n.hide() } } if (u > 0) { t.show() } if (u < g.length - 1) { n.show() } B = -u * o; s.animate({ "translate3d": B + "px,0,0" }, c.speed) } if (c.autoPlay) { D() } function D() { clearInterval(m); m = setInterval(function () { u++; if (u < 0) {