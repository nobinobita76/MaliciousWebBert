<script type="text/javascript" src="http://act.yzz.cn/libs/js/jquery-1.7.2.min.js"></script>
<script type="text/javascript" src="http://www.yzz.cn/act/libs/lightbox-2.6/js/lightbox-2.6.min.js"></script>
<script type="text/javascript" src="http://www.yzz.cn/sy/area/sy.picSlide.js"></script>
<script type="text/javascript" src="http://www.yzz.cn/sy/area/hidejs.js"></script>
<script type="text/javascript">
    $(function(){

        /*
         *   截图切换
         * */
        var img_len=$(".img-list ul li a img").length;
        var img_url=$(".img-list ul li a img").eq(img_len-1).attr("src");
        var img = new Image();
        img.src = img_url;
        img.onload = function(){
            PicSlide.init($('.picSlide'));
        };

        $(".col1 .turn .progress ol").css({"width":"auto"});
        var progress_ol=$(".progress ol");
        progress_ol.html('');
        var img_len=$(".img li").length;
        for(var i=0;i<img_len;i++){
            progress_ol.append("<li></li>");
        };
        progress_ol.find("li").eq(0).addClass("hover");
        if(img_len>4){
            progress_ol.find("li").eq(4).addClass("nomar");
        };


        var $aside=$('#J-aside');
        var $hs=$aside.find('.head span');
        var $new=$aside.find('.new-list');
        var $li=$new.find('li');
        var $turn=$('.turn');
        var $tli=$turn.find('.img li');
        var $tpl=$turn.find('.progress li');
        var timer=null;
        var index=0;
        $li.on('mouseenter',function(){
            var index=$(this).index();
            $li.removeClass('hover').eq(index).addClass('hover')});
        tab(0);
        $hs.on('mouseenter',function(){
            var index=$(this).index();
            tab(index)
        });
        function tab(index){
            $li=$new.eq(index).find('li');
            $hs.removeClass('hover').eq(index).addClass('hover');
            $new.addClass('none').eq(index).removeClass('none')};
        timer=setInterval(function(){
            run()
        },3000);
        $tpl.on('click',function(){
            index=$(this).index();
            showImg(index);
        });
        $aside.mouseenter(function(){
            clearTimeout(timer);
        }).mouseleave(function(){
            timer=setInterval(function(){run()},3000);
        });

        function run(){
            index++;
            if(index>img_len-1){
                index=0;
            };
            showImg(index);
        }
        function showImg(index){
            $tli.animate({opacity:0},100);
            $tli.eq(index).animate({
                opacity:1
            }, 100,function(){
                $tli.removeClass('hover').eq(index).addClass('hover');
                $tpl.removeClass('hover').eq(index).addClass('hover')
            })
        };
        /*
         * aside字符截取
         * */
        var asideLi=$(".col2 .right-aside .new-list ul li");
        $.each(asideLi,function(index,value){
            var asideStr=$(this).find("span").text();
            var lastStr=strsplic(asideStr,18);
            $(this).find("span").text(lastStr);

        });

        //截取字符封装函数
        function strsplic(str,num){
            str=str.substr(0,num);
            return str;
        };

    });


    $(function(){var pathName=window.location.pathname,pnSplit=pathName.split('/'),$nav=$('.nav a'),navName=['news','guide'];if(pathName=='/sy/mlarun/'){$nav.removeClass('hover').first().addClass('hover')}else{for(var i=0;i<pnSplit.length;i++){if(pnSplit[i]==""||typeof(pnSplit[i])=="undefined"){pnSplit.splice(i,1);i=i-1}if(pnSplit[i]==navName[0]){$nav.removeClass('hover').eq(1).addClass('hover');break}else if(pnSplit[i]==navName[1]){$nav.removeClass('hover').eq(2).addClass('hover');break}}}});



</script>
