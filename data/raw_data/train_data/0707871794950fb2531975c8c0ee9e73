    <p><a href="/zt/info/1.html" target="_blank">娱乐专题</a></p>
    <p class="channel-back-top">返回顶部</p>
</div>
<script type="text/javascript" src="/zt/asset/js/Comment.js"></script>
<script type="text/javascript">
    $(function () {
        var len = $(".focus-box li").length;
        var index = 0;
        var timer = null;
        var offset = 4000;

        $(".topic-focus").hover(function () {
            $(".foucs-arrow").fadeIn()
        }, function () {
            $(".foucs-arrow").fadeOut()
        });
        $(".total-num").text($(".topic-focus li").length);
        $(".focus-box ul li").eq(0).css({ "z-index": 2, "opacity": 1 });

        if (!timer && $(".topic-focus").find("li").length > 1) {
            timer = window.setTimeout(autoShow, offset);
        }
        $(".prev-page").click(function () {
            index--;
            if (index < 0) {
                index = len - 1;
            }
            showPics(index);
        });
        $(".next-page").click(function () {
            index++;
            if (index > len - 1) {
                index = 0;
            }
            showPics(index);
        });
        function showPics(index) {
            $(".focus-box ul li").eq(index).siblings().animate({ opacity: 0 }, 200).css("zIndex", 1);
            $(".focus-box ul li").eq(index).animate({ opacity: 1 }, 200).css("zIndex", 2);
            $(".focus-icon a").stop(true, false).removeClass("on").eq(index).addClass("on");
            $(".current-num").text(index + 1);
        }
        function autoShow() {
            n = $(".topic-focus").find("li").length - 1;
            index++;
            if (index > n || index < 0) {
                index = 0;
            }
            showPics(index);
            clearTimeout(timer);
            timer = window.setTimeout(autoShow, offset);
        }

        $(".topic-focus").hover(function () {
            clearTimeout(timer);
        }, function () {
            timer = window.setTimeout(autoShow, offset);
        });
    });

    $(function () {
        $(".timeline-cnt").hover(function () {
            $(this).addClass("timeline-hover").append('<p class="gray-line"></p>');

        }, function () {
            $(this).removeClass("timeline-hover");
            $(".gray-line").remove();
        });

        $(".other-news li:odd").addClass("add-border");

        $(".pic-news-list").find("li:lt(15):gt(9)").addClass("add-spacing");
        var _getLiLength = $(".pic-news-list li").length;
        if (_getLiLength > 10) {
            $(".pic-news").append('<p class="dotted-line"></p>');
        }
        if (_getLiLength >= 20) {
            $(".more-news").show();
        } else {
            $(".more-news").hide();
        }


        var _windowHeight = $(window).height();
        var contenter = $(".float-slidebar");
        contenter.css({ "right": parseInt(($(window).width() - 1000) / 2 - contenter.outerWidth(true) - 10) });
        $(window).resize(function () {
            contenter.css({ "right": parseInt(($(window).width() - 1000) / 2 - contenter.outerWidth(true) - 10) });
        });
        $(window).scroll(function () {
            var _scrollTop = $(window).scrollTop();
            if (_scrollTop >= parseInt(_windowHeight / 2 - 200)) {
                contenter.fadeIn();
            } else {
                contenter.fadeOut();
            }
        });
        $(".channel-back-top").click(function () {
            $("body, html").animate({ scrollTop: 0 }, 500);
        });
    });
</script>
