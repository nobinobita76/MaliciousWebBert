$('.J_share_icon').on('click', 'label', function(){
	//alert(share_lay_index);
	//layer.close(share_lay_index);
	layer.closeAll();
	imgUrl = $(this).find('img').attr('src');
	var icon = $(this).data('icon');
	var param = {
		tid : tid,
		icon: icon
	};
    $.post(readToolUrl, param, function(data){
        if(data.status=='success'){
            layer.msg(data.msg, {icon: 1});
        }else if(data.status=='error'){
            layer.alert(data.msg, {icon: 0});
        }
