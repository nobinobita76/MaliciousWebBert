function generateQuanUrl(id){
	var data = {
		id:String(id),
		type:parseInt(2)
	};
	var params = {
		inapp_data : JSON.stringify(data),
		download_title : "昆山论坛",
		download_msg : "昆山论坛"
	};
	//response.html("Waiting for response");
	var paramsJson = JSON.stringify(params);
	//request.html("Deep Share link(" + paramsJson + ");");
	$.post('https://fds.so/v2/url/6d459a593c22ea63', paramsJson, function(result) {
		//response.html(JSON.stringify(result));
		deepshareQuanurl = result.url;
		//add by zj QQ:13040
		if(!deepshareQuanurl){
			//window.location = "kunshanbbs://side/?sid="+id;
			window.location = "http://a.app.qq.com/o/simple.jsp?pkgname=com.kshot";
		}else{
			window.location = deepshareQuanurl;
		}
	}).error(function() {
		//Do your own bussiness logic in case that our servive is down.
	});
}if(getcookie('read_redpacket') == ""){
    $("#J_redpacket").show();
    $("#J_redpacketClose").on("click", function () {
        $("#J_redpacket").hide();
        setcookie('read_redpacket', 1, 1000 * 600, '/');
        return false;
    });
}else{
    $("#J_redpacket").hide();
