function vote(typeid,counts) {
	$.getJSON('/api.php?op=jsdata&ac=vote&isvoted=0&id=1276111&typeid='+typeid+'&'+Math.random(), function(data){
		if(data.status==1)	{
			counts=counts+1;
			$('#vote'+typeid).html(counts+'人');
			alert('投票成功，感谢你的参与！');
		}else {
			alert(data.data);
		}
	})
}
