function checkForm_sou()
{
if(document.getElementById('q').value == '请输入关键词')
{ 
document.getElementById('q').value = '中国教育在线';
}
document.charset='utf-8';
document.forms["w"].action="http://sou.eol.cn/search.s?all="+encodeURI(document.getElementById("q").value)+"&type=facet&selectStyle=cat&facet.field=keywords";
document.forms["w"].submit();
}

function ches_sou(){
if(event.keyCode==13){
 document.charset='utf-8';
 document.forms["w"].action="http://sou.eol.cn/search.s?all="+encodeURI(document.getElementById("q").value)+"&type=facet&selectStyle=cat&facet.field=keywords";
return false;
}
}

document.domain = "eol.cn";
