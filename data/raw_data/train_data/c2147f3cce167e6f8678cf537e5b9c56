<script language="JavaScript" type="text/javascript">
var categoryid = 22;var infoid = 568826;

function scrollDoor(menus,divs,current,notCurrent,show,hide){
      for(var i = 0 ; i < menus.length ; i++)
      {
           document.getElementById(menus[i]).value = i;
           document.getElementById(menus[i]).href = "javascript:void(0)";
           document.getElementById(menus[i]).onmouseover = function(){
            for(var j = 0 ; j < menus.length ; j++)
            {
                 document.getElementById(menus[j]).className = notCurrent;
                 document.getElementById(divs[j]).className = hide;
            }
            document.getElementById(menus[this.value]).className = current;
            document.getElementById(divs[this.value]).className = show;
           }
      }
 }
window.onbeforeunload = function(){
    saveComment2Cookie();
}
function saveComment2Cookie(){
    var content = document.getElementById("artcomment").value;
    $.cookie('usercomment',content,{expires:1});
}
$(function(){
    usercomment = $.cookie('usercomment');
    if('' != usercomment && null!= usercomment && undefined != usercomment){
        $("#artcomment").val(usercomment);
        $.cookie('usercomment','');
    }
    //关键词点击搜索
    $(".bdsw").click(function(){
        var word = $(this).html();
        $("#bdword").val(word);
        $("#baidusearch").submit();
    });

})
function comments_submits(id){
    if($("#"+id+" textarea[name='content']").val()==''){
        alert('请添写留言内容');return false;
    }
    if($("#"+id+" input[name='username']").length){
        alert('请登录后评论！');
        $("#"+id+" input[name='username']").focus();
        return false;
    }
    if($("#"+id+" textarea[name='content']").val().length>200){
        alert('内容请少于200字。');return false;
    }
    $("#gotocomment").submit();
    $("#artcomment").val('');
}
function setFontSizes(sid,id,size){
    $("#"+sid).removeClass('zt2').addClass('zt1');
    if(sid == 'sf14'){
        $("#sf16,#sf18").removeClass('zt1').addClass('zt2');
    }else if(sid == 'sf16'){
		$("#sf14,#sf18").removeClass('zt1').addClass('zt2');
    }else{
		$("#sf14,#sf16").removeClass('zt1').addClass('zt2');
	}
    setFontSize(id,size);
}
var flag=1;
var times=3000;
var loops = 1;
var intervalId = null;
function dofun(){
	if(loops>4) return;
	if(flag==1){
		$("#d1").attr('class','mr_text');
		$("#d2").attr('class','hide');
		$("#d4").attr('class','hide');
		$(".topsBtnDiv li:eq(0) a").addClass("current2");
		$(".topsBtnDiv li:eq(1) a").removeClass("current2");
		$(".topsBtnDiv li:eq(2) a").removeClass("current2");
		flag=2;
		times=3000;
		clearInterval(intervalId);
		intervalId = setInterval("dofun()",times);

	}else if(flag==2){
		$("#d2").attr('class','mr_text');
		$("#d1").attr('class','hide');
		$("#d4").attr('class','hide');

		$(".topsBtnDiv li:eq(0) a").removeClass("current2");
		$(".topsBtnDiv li:eq(1) a").addClass("current2");
		$(".topsBtnDiv li:eq(2) a").removeClass("current2");
		flag=3;
		times=3000;
		clearInterval(intervalId);
		intervalId = setInterval("dofun()",times);
	}else if(flag==3){
		$("#d4").attr('class','mr_text');
		$("#d2").attr('class','hide');
		$("#d1").attr('class','hide');

		$(".topsBtnDiv li:eq(0) a").removeClass("current2");
		$(".topsBtnDiv li:eq(1) a").removeClass("current2");
		$(".topsBtnDiv li:eq(2) a").addClass("current2");
		flag=1;
		times=3000;
		clearInterval(intervalId);
		intervalId = setInterval("dofun()",times);
	}
	loops += 1;
};


//社会聚焦  改为调用京华接口
$(function (){
	//intervalId=setInterval("dofun()",3000);
	var parameter = categoryid+'_'+infoid;
	var url = siteurl+"/do.php?inajax=1&do=comment&ac=all_comments&parameter="+parameter+"&json=1&jsoncallback=?";
	$.getJSON(url,function(data){
		$("#comments_usercount").html(data.usercount);
		$("#comments_usercount2").html(data.usercount);
		$("#comments_count").html(data.count);
		$("#comments_count2").html(data.count);
	});
	
	
});

</script>
