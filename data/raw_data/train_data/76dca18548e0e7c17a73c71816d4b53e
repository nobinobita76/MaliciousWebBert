<script type="text/javascript" src="http://www.danqi.com/skin/2015/js/jquery-powerSwitch.min.js"></script>
<script type="text/javascript">
$(function(){
	//幻灯
	var htmlAdBtn = '';
	$('.slide .item').each(function(index, image) {
		var id = "adImage" + index;
		htmlAdBtn = htmlAdBtn + '<a href="javascript:" data-rel="'+ id +'">'+ (index + 1) +'</a>';
		image.id = id;
	});
	$('.slide-btns').html(htmlAdBtn).find('a').powerSwitch({
		eventType: "hover",
		classAdd: "hover",
		animation: "translate",
		container: $('.slide-operate'),
		autoTime: 5000,
		onSwitch: function(target) {
			var img = target.find("img").get(0);
			if (img && !img.src) {
				img.src = img.getAttribute("data-src");    
			}
		}	
	}).eq(0).trigger("mouseover");

	//选项卡
	$('.tab-wrap').each(function(index, el) {
		var $this = $(this),
			$menus = $this.find('.tab-menu'),
			$conts = $this.find('.tab-cont');

		$menus.on('mouseenter', function() {
			$(this).addClass('tab-menu-hover').siblings().removeClass('tab-menu-hover')
			$conts.hide().eq($(this).index()).show().find('img').each(function(index, el) {
				var $this = $(this);

				if(!$this.attr('src')){
					$this.attr('src', $this.attr('data-src'));
				}
			});
		});
	});
});
</script>


</body></html>