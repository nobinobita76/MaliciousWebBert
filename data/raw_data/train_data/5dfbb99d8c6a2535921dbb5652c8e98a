<script>
document.domain = "cheshi.com";

var sig = "29";
var bca = "218";
var box = new LightBox("collectBox");
var url = window.location.href;

var user = cheshi.utility.getCookie('cheshi_user_info');//cs_getCookie('cheshi_pro_city');
if(user != ""){
	user = cheshi.utility.base64Decode(user);//cs_base64_decode(loc);
	var arr = user.split('\t');
    var uid = arr[0];
  
}

cheshi.ready(function() {
                	window.setTimeout(function(){
                		cheshi.show(cheshi.$('collect'));
                		//获取关注信息 
						get_collect();
						//取消收藏
						cheshi.$('yes_collect').onclick = function(){
							var id = cheshi.$('favid_collect').value;
							if(id){
								cheshi.getJSON('http://my.cheshi.com/user/ajax/ajaxAttention.php?act=del&signid='+sig+'&bcatesubid='+bca+'&carid='+id,function(data){
									if(data.status == 1){
										cheshi.$('favid_collect').value = 0;
										cheshi.show(cheshi.$('bad_collect'));
			    						cheshi.show(cheshi.$('no_collect'));
										cheshi.hide(cheshi.$('yes_collect'));
										window.setTimeout(function(){
											cheshi.hide(cheshi.$('bad_collect'));
											},2000); 
			    					}
			    				}); 
								
							}
	        			}

	        			//添加关注
	        			cheshi.$('no_collect').onclick = function(){
	        				if(!uid){
	    						login_box();	
	    					}else{
	    						cheshi.getJSON('http://my.cheshi.com/user/ajax/ajaxAttention.php?act=add&signid='+sig+'&bcatesubid='+bca,function(data){
	    							if(data.status == -2){
	    								login_box();
	    							}else if(data.status > 0){
	    								cheshi.$('favid_collect').value = data.data.id;
	    								cheshi.show(cheshi.$('success_collect'));
	    	    						cheshi.hide(cheshi.$('no_collect'));
	    								cheshi.show(cheshi.$('yes_collect'));
	    								window.setTimeout(function(){
	    									cheshi.hide(cheshi.$('success_collect'));
	    								},2000); 	
	    	    					}
	    	    				}); 

	    					}
		        		}
		        		
                    },2000);	
                });

				//获取是否关注信息 
                function get_collect(){               
                	cheshi.getJSON('http://my.cheshi.com/user/ajax/ajaxAttention.php?act=select&signid='+sig+'&bcatesubid='+bca,function(data){
						if(data > 0){
							cheshi.$('favid_collect').value = data;
							cheshi.show(cheshi.$('yes_collect'));
							cheshi.hide(cheshi.$('no_collect'));
						}else{
    						cheshi.show(cheshi.$('no_collect'));
							cheshi.hide(cheshi.$('yes_collect'));	
    					}
    				}); 
    				
                	//cheshi.$('yes_collect').setAttribute("onclick","del_collect();");
                	//cheshi.$('no_collect').setAttribute("onclick","add_collect();");
                }

				
				//登陆框 
				function login_box(){
					  document.domain = "cheshi.com";			  
					  box.Fixed = true;
					  box.Center = true;
					  box.Show();
				}

				//登陆成功 		
				function loginOk(){
				    parent.box.Close();
					//parent.window.location.reload();
				    window.location.href = url;
				}
				    			
    
</script>
   
