<script>
	if (! (typeof(_contrast_setCookie) == "function")) {
    function _contrast_setCookie(a, b) {
        var c = new Date();
        c.setTime(c.getTime() + 30 * 24 * 60 * 60 * 1000);
        return document.cookie = a + "=" + escape(b) + ";expires=" + c.toGMTString() + ";path=/;domain=.cheshi.com"
    }
    function _contrast_getCookie(b) {
        var a,
        c = new RegExp("(^| )" + b + "=([^;]*)(;|$)");
        if (a = document.cookie.match(c)) {
            return unescape(a[2])
        } else {
            return null
        }
    }
}
Array.prototype.del=function(n) {　
　if(n<0)
　　return this;
　else
　　return this.slice(0,n).concat(this.slice(n+1,this.length));
}
if (ad_cid) {
    var user_see_bseries_cookie = _contrast_getCookie("bseries_name");
    if (user_see_bseries_cookie) {
        var cookie_arr = user_see_bseries_cookie.split(",");
        if (cookie_arr.indexOf(ad_cid) >= 0) {
            cookie_arr=cookie_arr.del(cookie_arr.indexOf(ad_cid));　
        }
		var count_arr = cookie_arr.length;
        if (count_arr >= 10) {
            cookie_arr.shift();
            count_arr = 9
        }
		if(count_arr > 0){
			var cookie_str_new = cookie_arr.join(",");
        	var cookie_str_six = cookie_str_new + "," + ad_cid;			
		}else{
			var cookie_str_six = ad_cid;
		}
        _contrast_setCookie("bseries_name", cookie_str_six);
    } else {
        _contrast_setCookie("bseries_name", ad_cid)
    }
}
function load_seen_bseries() {
    var a = _contrast_getCookie("bseries_name");
    if (a) {
        cheshi.ajax({
            url: "/ajax/get_guan_zhu_0201.php?id=" + a,
            method: "get",
            callback: {
                success: function(b) {
                    if (typeof(b) == "string") {
                        cheshi.$("bseries_browser_record").innerHTML = b;
                        return false
                    } else {
                        return false
                    }
                },
                failure: function(b) {
                    return false
                }
            }
        })
    } else {
        cheshi.$("bseries_browser_record").innerHTML = "<li>暂无已浏览的车系!</li>"
    }
};

