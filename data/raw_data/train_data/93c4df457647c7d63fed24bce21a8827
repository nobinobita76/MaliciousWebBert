<script type="text/javascript">
    var schoolcardurl="http://api.jzb.com/school/web/249947/";
    jQuery(function(){
        var length = jQuery(".card_school").length;
        jQuery(".card_school").each(function(index, el) {
            if(index == 0) {
                jQuery(el).css('paddingLeft', '0px');
            }
            if(index == length-1) {
                jQuery(el).css('border', '0px');
            }
        });

        jQuery(".card_box").click(function(event) {
            jQuery(".mask").show();
            var _theframe = document.getElementById("theframe");
            if(_theframe.src != schoolcardurl){
                _theframe.src = schoolcardurl;
            }

        });

        jQuery(".mask").click(function(event) {
            if(event.target.className == "mask") {
                jQuery(".mask").hide();
            }
        });

        jQuery(".del").click(function(event) {
            jQuery(".mask").hide();
        });

    })() 
</script>                
