function postreview(url,pid) {
showErrorDialog("您需要先登录才能继续本操作");
doane();
return false;
var numObj = $('postreviewnum_'+pid);
if(numObj) {postreviewnum = parseInt(numObj.innerHTML);}	
jQuery.get(url+"&rd="+Math.random(), function(s) {
if(s==1) {
if(postreviewnum<=0){
numObj.style.display='inline-block';
}
jQuery("#postreviewnum_"+pid).html(postreviewnum+1+"<span><nobr>+1</nobr></span><i></i>");
