var ED = ED || {};
ED.session = ED.session || {};
ED.getCookie = function(a) {
    var b = document.cookie;
    var c = new RegExp(a + '=([^;]+);?');
    var d = '';
    var e;
    if (e = c.exec(b)) {
        d = e[1]
    }
    return d
};
ED.parseSessionCookie = function() {
    var a = decodeURIComponent(ED.getCookie('edengcookie'));
    if ('' != a) {
        var b = a.split(';;;');
        for (var i = 0; i < b.length; i++) {
            var c = b[i].split(':=:');
            ED.session[c[0]] = c[1]
        }
    }
};
