        //平台、设备和操作系统
        function getQueryString(name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
            var r = window.location.search.substr(1).match(reg);
            if (r != null) return unescape(r[2]); return null;
        }
        var system ={
            win : false,
            mac : false,
            xll : false
        };
        var domain='http://m.xue.huatu.com';
        var pathname=window.location.pathname;
        var query=window.location.search.substring(1);//获取查询串
        var jumpUrl=((pathname)?pathname:'')+((query)?'?'+query:'');
        var chkReg='^((?!/course/live/(?!(paid))|/course/view/|/course/video/|/course/detail/|/classes/|/tiku/|/evaluation/|/site/login).)*$';
        //检测平台
        var p = navigator.platform;
        system.win = p.indexOf("Win") == 0;
        system.mac = p.indexOf("Mac") == 0;
        system.x11 = (p == "X11") || (p.indexOf("Linux") == 0);
        //跳转语句
        if(!(system.win||system.mac||system.xll)){
            if(getQueryString("pc")!='1'){
                if(pathname.match(chkReg)==null){
                    window.location.href=domain+jumpUrl;
                }else{
                    window.location.href=domain;
                }
            }
        }
