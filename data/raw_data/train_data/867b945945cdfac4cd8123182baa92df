function pastFormCheck(form) {
    var begin=form.begin_time.value;
    var end=form.end_time.value;
    if(new Date(begin).getTime()>new Date(end).getTime()){
        $(form).find("span").eq(0).html("开始时间不能大于结束时间");
        return false;
    }
    return true;
}


