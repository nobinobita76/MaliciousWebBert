    <script>
		var isShowMsg = true;
		var timeSt;
		$(function(){
			$('#findAutoNumber').click(function(){
				getsn();
				$('#textfield').css('color','#000');
			});

		});

		function getsn(){
			$.ajax({
				url:"http://127.0.0.1:52131/MachineInfo?callback=showNo",  //注意地址和端口号不要修改，callback后的调用函数名可修改
				type:'get',
				cache:false,
				dataType:'jsonp',
				jsonpCallback:"showNo",
				timeout: 2600, // 超时判断，如果超时认为插件未安装
				error:function(XMLHttpRequest, textStatus, errorThrown){
					//未安装插件，调用下载方法
					if(isShowMsg){
						//alert("请安装XXX，下载地址：XXX");
						downloadplugin();
						isShowMsg=false;
					}
				}
			});
		}

		var x=0;
		//回调函数
		function showNo(data){
			x++;
			// $('#textfield').val(x);
			if(data){
				if(data.ver != MachineInfoVersion){
					downloadplugin();
				}else{
					if(data.sn){
						$('#textfield').val(data.sn);
						clearTimeout(timeSt);
					}else{
						alert('获取主机编号异常');
					}
				}

			}
		}

		function downloadplugin(){
			//alert("请安装XXX，下载地址：XXX");
			if(x > 1) return;
			timeSt = setInterval(getsn, 1000);
			alert('使用此功能需要下载检测主机编号插件，下载完成后请点击运行,IE用户请在运行插件后刷新本页面');
			window.location.href="http://tools.lenovo.com.cn/source/hostnumberPlugin/LenovoServicePluginMain.exe";
		}
    </script>
