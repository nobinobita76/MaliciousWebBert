        function jsreplace(v) {
            if (v != null && v.length > 0) {
                v = v.replace(/script/g, '');
                v = v.replace(/ /g, '');
                v = v.replace(/</g, '');
                v = v.replace(/>/g, '');
                v = v.replace(/script/g, '');
                v = v.replace(/#/g, '');
                v = v.replace(/%/g, '');
                v = v.replace(/&/g, '');
                v = v.replace(/:/g, '');
                v = v.replace(/'/g, '');
                v = v.replace(/"/g, '');
            }
            return v;
        }
        function CheckProductSn(mNumber, defaultText) {
            var ret;
            ret = true;
            if (mNumber == defaultText || mNumber == "") {
                alert('请输入主机编号');
                ret = false;
            }
            else if (mNumber.length != 8 && mNumber.length != 4 && mNumber.length != 7 && mNumber.length != 10 && mNumber.length != 14 && mNumber.length != 15 && mNumber.length != 18) {
                alert('主机编号位数不对');
                ret = false;
            }
            else if (!/^[A-Za-z0-9]+$/.test(mNumber)) {
                alert('主机编号输入错误，请确保输入的编号由字母及数字组成');
                ret = false;
            }
            return ret;
        }
        function GoPage() {
            var sn = $("#textfield").val();
            var HomeURL = 'http://' + window.location.host;
            if (!CheckProductSn(sn, "示例:EB10963046")) {
                return;
            }
            $.ajax({
                type: "get",
                url: "manage/getminfobysn.ashx",
                data: { sn: sn },
                async: true,
                dataType: 'json',
                success: function (info) {
                    if (info.success) {
                        if (info.data.BrandTypeId == "120") {
                            //                            window.location.href="http://think.lenovo.com.cn/support/driver/newdriversdownlist.aspx?categoryid=" + info.data.WebTreeId + "&CODEName=" + sn + "&SearchNodeCC=" + sn + "&SearchType=1&wherePage=1";
                            window.location.href = "http://think.lenovo.com.cn/support/driver/newdriversdownlist.aspx?categoryid=" + info.data.WebTreeId + "&CODEName=" + info.data.MachineCode + "&SearchNodeCC=" + info.data.MachineCode + "&SearchType=1&wherePage=1";
                        }
                        else {
                            location.href = HomeURL + '/lenovo/wsi/usercenter/computersearch/machinesearch.aspx?intcmp=index&id=' + jQuery.trim(sn.toUpperCase()) + '&showradio=1&showdriver=no';
                        }
                    } else {
                        alert("我们无法验证您所输入的主机编号。请重新输入或查看下方的如何查找主机编号教程。");
                    }

                }
            });
        }
        function userBrowser() {
            var userAgent = navigator.userAgent; //取得浏览器的userAgent字符串
            if (userAgent.indexOf("compatible") > -1 && userAgent.indexOf("MSIE") > -1 && !isOpera) {
                return "IE";
            }
            //判断是否IE浏览器

            var isOpera = userAgent.indexOf("Opera") > -1;
            if (isOpera) {
                return "Opera"
            }
            //判断是否Opera浏览器
            if (userAgent.indexOf("Firefox") > -1) {
                return "Firefox";
            } //判断是否Firefox浏览器
            if (userAgent.indexOf("Chrome") > -1) {
                return "Chrome";
            }
            if (userAgent.indexOf("Safari") > -1) {
                return "Safari";
            } //判断是否Safari浏览器

            return "unKnow";
        }

        function submitDriversDownloadCount(DriverId, Title, fileurl, filecount, detailUrl) {

            if (fileurl != "" && filecount != "") {
                if (filecount == "Y") {
                    //补丁文件
                    if (confirm("您当前选择的驱动含有相关程序，建议先下载安装。\r\r点击“确定”进入详细页面下载相关程序,点击“取消”直接下载当前程序")) {
                        window.open(detailUrl);
                        return;
                    } else {
                        window.open(fileurl);
                    }
                }
            }

            var dataString = '';
            var CustomerIP = '175.6.10.26';
            var url = '';
            var locationUrl = window.location.href;
            var browser = userBrowser();
            dataString = 'DriverId=' + DriverId + '&Title=' + Title + '&IP=' + CustomerIP + '&Url=' + url + '&Browser=' + browser + '&locationUrl=' + locationUrl;
            $.ajax({
                type: "Post",
                contentType: "application/html",
                url: "AjaxDownloadSoftwareCount.aspx?" + dataString,
                data: "",
                dataType: 'json',
                success: function (result) {
                    var status = parseInt(result.status, 10),
                                message = result.msg;
                    if (status != -1) {//
                        //	alert(message);

                    } else {//错误
                        //	alert(message);
                    }
                },
                error: function (result, status) {

                    if (status == 'error') {
                        //alert("网络繁忙，提交信息错误。");
                    }
                }
            });

            // 百度统计“驱动安装工具下载”
            if (Title == 'DriverLenovo') {
                _hmt && _hmt.push(['_trackEvent', '按钮：驱动安装工具下载', '下载']);
            }

        }


