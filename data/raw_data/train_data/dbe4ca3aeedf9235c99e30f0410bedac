common.iframeAjax({
	domain:"i.yanxiu.com",
	url:'http://i.yanxiu.com/yc/cn.tc',
	data:{
		clubId:'36563',
		start:1,
		limit:6
	},
	success:function(datas){
		datas = common.parseJson(datas);
		if(datas.code != 0) return;
		var o = common.idN('notice_1');
		var m = '<li><em class="new">【{level}】</em><a href="http://q.yanxiu.com/club/train/content.tc?nid={url}&f={area}" target="_blank" title="{title1}">{title}</a><i>{time}</i></li>';
		var h = [];
		var r = datas.result;
		if (!r) return;

		for(var i = 0, len = r.length, temp = null; i < len; i++){
			temp = r[i];
			var level = "";
			var area ="area";

			if (temp.level == 1)
				level = "省级";
			else if (temp.level == 2)
				level = "地市级";
			else if (temp.level == 3)
				level = "区县级";
			else if (temp.level == 4){
				level = "校级";
				area="school";
				}
			else if (temp.level == 0)
			level = "项目";	

			h.push(m.replace(/\{url\}/,temp.nid).replace(/\{level\}/g,level).replace(/\{title1\}/g,temp.title).replace(/\{title\}/g,temp.title).replace(/\{time\}/,temp.publishtime).replace(/\{area\}/,area));
		}
		o.innerHTML = h.join('');
	},
	failure:function(){}
});
window.onload = function(){
		if (Passport.isLogin()) {
		common.getScript({
			url: 'http://i.yanxiu.com/yc/checkUser.tc',
			data: {
				passport: decodeURIComponent(Passport.getDisplayName())
			},
			success: function(datas) {
				var o = common.idN('passportBox');
				if (datas.s == 'true') {
					o.innerHTML = '<span class="login_after"><p class="login_fang_name">欢迎您！<em>' + Passport.getNickName() + '</em></p><p class="login_fang_mine"><a target="_blank" href="http://gl.yanxiu.com/public/login/success.html">进入管理中心</a></p><p class="login_fang_exit"><a href="javascript:Passport.doLogout();">退出</a></p></span>';
				} else if (datas.s == 'editor') {
					o.innerHTML = '<span class="login_after"><p class="login_fang_name">欢迎您！<em>' + Passport.getNickName() + '</em></p><p class="login_fang_mine"><a target="_blank" href="http://q.yanxiu.com/manage/xb/bjzy.tc">进入内容管理</a></p><p class="login_fang_mine"><a target="_blank" href="http://i.yanxiu.com/?j=true">进入个人工作室</a></p><p class="login_fang_exit"><a href="javascript:Passport.doLogout();">退出</a></p></span>';
				} else if (datas.s == 'supervisor') {
					o.innerHTML = '<span class="login_after"><p class="login_fang_name">欢迎您！<em>' + Passport.getNickName() + '</em></p><p class="login_fang_mine"><a target="_blank" href="http://q.yanxiu.com/manage/xb/zjzy.tc">进入内容管理</a></p><p class="login_fang_mine"><a target="_blank" href="http://i.yanxiu.com/?j=true">进入个人工作室</a></p><p class="login_fang_exit"><a href="javascript:Passport.doLogout();">退出</a></p></span>';
				} else if (datas.s == 'teamleader') {
					o.innerHTML = '<span class="login_after"><p class="login_fang_name">欢迎您！<em>' + Passport.getNickName() + '</em></p><p class="login_fang_mine"><a target="_blank" href="http://i.yanxiu.com/xb/fang.tc?projectid=' + datas.projectid + '">进入个人工作室</a></p><p class="login_fang_exit"><a href="javascript:Passport.doLogout();">退出</a></p></span>';
				} else if (datas.s == 'trainee') {
					o.innerHTML = '<span class="login_after"><p class="login_fang_name">欢迎您！<em>' + Passport.getNickName() + '</em></p><p class="login_fang_mine"><a target="_blank" href="http://i.yanxiu.com/xb/taskHall.tc?projectid=' + datas.projectid + '">进入个人工作室</a></p><p class="login_fang_exit"><a href="javascript:Passport.doLogout();">退出</a></p></span>';
				} else {
					o.innerHTML = '<span class="login_after"><p class="login_fang_name">欢迎您！<em>' + Passport.getNickName() + '</em></p><p class="login_fang_mine"><a target="_blank" href="http://i.yanxiu.com/?j=true">进入个人工作室</a></p><p class="login_fang_exit"><a href="javascript:Passport.doLogout();">退出</a></p></span>';
				}
				header.drawLogin();
			},
			failure: function() {}
		});
	}
}

