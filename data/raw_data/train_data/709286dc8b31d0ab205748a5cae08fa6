<script>
    function isFunction(it) {
        return Object.prototype.toString.call(it) === '[object Function]';
    }
    if (initDaPageId && isFunction(initDaPageId)) {
        var initDaPageId_old = initDaPageId;
        initDaPageId = function (pid, cid){
            window.PAGE_ID = pid;
            initDaPageId_old(pid, cid);
            dgload(pid,cid);
        }
    } else {
        var initDaPageId = function (pid, cid){
            window.PAGE_ID = pid;
            dgload(pid, cid);
        }
    }
    window.dgload = function(pageId, cookieId){
        jQuery(function($) {
            var dq = new dqSystem({
                oneLoad: false,
                pageId: pageId, // pageId
                cookieid: cookieId
            });
            dq.init();

            $("#ads-login").click(function () {
                ga('send', 'event', 'login', 'ad_click_new');
            });
        });
    }
</script>
