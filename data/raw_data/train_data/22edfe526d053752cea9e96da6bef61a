<script>
    // 阻止运营商注入恶意代码
    var _appendChild = document.body.appendChild;
    document.body.appendChild = function(el) {
        if (!el) {
            return;
        }
        if (el.tagName.toUpperCase() == 'IFRAME' && el.src.indexOf('qunar') == -1) {
            console.log('WTF! 拦截运营商注入！');
        } else {
            _appendChild.call(document.body, el);
        }
        return el;
    }
</script>


