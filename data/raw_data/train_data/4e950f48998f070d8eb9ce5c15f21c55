<script async="" src="//www.google-analytics.com/analytics.js"></script><script type="text/x-handlebars-template" id="tpl">
    {{ if(list.length>1){ }}
    <div class="slide-pagination top-hd" data-page="{{=list.length}}">
        <div class="hd">
            <a class="prev"><i class="vvicon">&#xe611;</i></a>
            <span class="pageState"><span>1</span>/{{=list.length}}</span>
            <a class="next"><i class="vvicon">&#xe60f;</i></a>
        </div>
    </div>
    {{ } }}
    <div class="bd">
        <ul>
        {{ _.each(list, function(item, index1){ }}
        <li>
        <dl class="side-list clearfix {{ if(index1 == 0){ }} showed {{ } }}">
            {{ _.each(item.items, function(el, index2){ }}
            <dd class="clearfix ishide" style="display:none;">
                <a href="{{=el.href_url}}" title="{{=el.title}}" target="_blank" class="j-vda ishide" vda="tp|ad_itemcard|{{=el.item_id}}|{{=el.shop_id}}|0|{{=el.shop_status}}">
                    <div class="pic">
                        <img src="{{=el.img_url}}|220w_330h" class="lazy" />
                    </div>
                </a>
                <div class="price">
                    {{ if(el.price_type == 1){ }}
                    <span class="yuan">¥</span>{{=el.discount_price}}
                    {{ } }}
                    {{if(el.price_type == 2){ }}
                    <span class="yuan">¥</span>{{=el.discount_price}}
                    {{ } }}
                    {{if(el.price_type != 1 && el.price_type != 2){ }}
                    <strong class="yuan" style="display: none;">¥</strong>
                    <strong class="sale"
                            style="color: #9c9c9c;">价格错误
                    </strong>
                    {{ } }}
                </div>
                <!--<i class="fl ico_item_active_{{=el.is_active}} ico_item_active"></i>-->
                <div class="pos">
                    <a target="_blank" href="/shop/{{=el.shop_id}}" title="{{=el.shop_name}}">
                        {{ if(el.is_gold_shop == 1){ }}
                        <img src="http://src.vvic.com/statics/v2/css/img/common/gold.png" aglin="absmiddle" width="19">
                        {{ } }}
                        {{=el.shop_name}}
                    </a>
                    <div class="more-info">
                        <div class="shop-pos fl">{{=el.sub_name}} {{=el.position}}</div>
                        <div class="shop-ww fr">
                            <a  class="ww-status ww-on j-ww" data-ww="{{=el.ww_nickname_code}}" href="http://www.taobao.com/webww/ww.php?ver=3&amp;siteid=cntaobao&amp;status=2&amp;charset=utf-8&amp;touid={{=el.ww_nickname_code}"
                               target="_blank" class="kf" vda="ww|ad_itemcard|{{=el.id}}|0|0|{{=index2}}"></a>
                        </div>
                        <div class="clear"></div>

                        {{ if(el.authenticated == 1 || el.cloud_update == 1 || el.years > 1 || el.top > 0) { }}
                        <div class="shop-icon fl clearfix">
                            {{ if(el.authenticated == '1'){ }}
                            <i class="cert j-v-tip" data-tip="cert"></i>
                            {{ } }}
                            {{ if(el.cloud_update == '1'){ }}
                            <i class="miao j-v-tip" data-tip="miao"></i>
                            {{ } }}
                            {{ if(el.top > 0){ }}
                            <i class="top{{=el.top}} j-v-tip" data-tip="top" data-top="{{=el.top}}"></i>
                            {{ } }}
                        </div>
                        {{ if(el.years > 1){ }}
                        <div class="shop-year-icon year{{=el.years}} fr j-v-tip" data-tip="year" data-date="{{=el.start_business_date}}"></div>
                        {{ } }}
                        <div class="clear"></div>
                        {{ } }}
                    </div>
                    <div class="triangle triangle-up1"></div>
                    <div class="triangle triangle-up2"></div>
                </div>
            </dd>
            {{ }) }}
        </dl>
        </li>
        {{ }) }}
        </ul>
    </div>
    {{ if(list.length>1){ }}
    <div class="slide-pagination bottom-hd" data-page="{{=list.length}}">
        <a class="prev"><i class="vvicon">&#xe611;</i></a>
        <span class="pageState"><span>1</span>/{{=list.length}}</span>
        <a class="next"><i class="vvicon">&#xe60f;</i></a>
    </div>
    {{ } }}
</script>






