  <script type="text/javascript">
	//自定义bds_config
	var shareTitle = $(".conText h1")[0].innerHTML;
	var shareDescription = $("meta[name='description']").attr("content");
	var shareImg = $("#text img:eq(0)").attr("src");
	if(shareDescription.length == 0)
		shareDescription = "";
	if(typeof(shareImg) == "undefined"){
		shareImg = "http://img.cache.news-com.cn/2012/logos/news_logo.png";
	}
	var shareContents = shareTitle + "――分享自新文网@新文网 ";
	var bds_config = {
		'bdText':shareContents,
		'bdPic':shareImg,
		'snsKey':{
