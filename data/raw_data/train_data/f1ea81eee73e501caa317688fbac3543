<script type="text/javascript">
function autosetheight(){
	var v_height = document.body.clientHeight;
	var h_height = document.getElementById("head").offsetHeight;
	var _mheight = document.getElementById("main").scrollHeight;
	var m_height = v_height - h_height - 90 - 2;
	if(m_height < 600){
		m_height = 600;
	}
	if(_mheight > m_height){
		m_height = _mheight;
	}
	document.getElementById("main").style.height = m_height + "px";
}
autosetheight();
</script>

