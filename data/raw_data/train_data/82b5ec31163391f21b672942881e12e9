    var query = GetQueryString("Key");

    //加载站点基本信息
    LoadWebInfo(query, function (Res) {
        if (Res && Res != "") {
            var PrvID = Res.PrvID;
            var CityID = Res.CityID;
            var CountyID = Res.CountyID;
            var EplatformID = Res.EplatformID;
