    <script type="text/javascript">
    var preOrderURL = "/hallChannel/";
    function renderCallPlans(pList, selectPlanVal){
    	var listHtml = '';
    	var i = 0;
    	$.each(pList, function(){
    		if(i < 3){
    			listHtml += '<li _val="' + this.tPlanId + '" _remark="' + this.remark + '" _monthFee="' + this.monthFee +'" _pTypeParam="' + this.pTypeParam +'" _packTypeExtParam="' + this.pTypeExtParam + '" _callTime="' + this.callTime + '" _netFlow="' + this.netFlow + '" _enc="' + this.enc + '" class="p-li ' + (this.monthFee > 99 ? '' : 'short') + '">' + this.monthFee + '</li>';
    		}else if(i == 3){
    			listHtml += '<li id="downListLi" class="p-li more"><span id="downListLiText">其它</span><i class="i-down"></i><ul class="more-product hide">';
    		}else{
    			listHtml += '<li _val="' + this.tPlanId + '" _remark="' + this.remark + '" _monthFee="' + this.monthFee +'" _pTypeParam="' + this.pTypeParam +'" _packTypeExtParam="' + this.pTypeExtParam + '" _callTime="' + this.callTime + '" _netFlow="' + this.netFlow + '" _enc="' + this.enc + '" class="mp-li">' + this.monthFee + '</li>';
    		}
    		if(this.isRecommend == 1){
    			recommendId = this.tPlanId;
    		}
    		i++;
    	});
    	listHtml += '</ul></li>';
    	$("#productList ul").html(listHtml);
    	if($("#productList").find("li[_val=" + selectPlanVal + "]").attr("_val")){
    		selectPlanId = selectPlanVal;
    	}else{
    		selectPlanId = recommendId;
    	}
    	$("#productList").find("li").removeClass("curr");
    	if($("#productList ul .p-li[_val=" + selectPlanId + "]").attr("_val")){
    		$("#productList ul .p-li[_val=" + selectPlanId + "]").addClass("curr");
    	}else{
    		$("#downListLi").addClass("curr");
    		$("#downListLiText").html($(".more-product li[_val=" + selectPlanId + "]").html());
    		$(".more-product li[_val=" + selectPlanId + "]").addClass("curr");
    	}
    	productListRegClick();
    	downListRegClick();
    	return selectPlanId;
    }
  //档位点击事件
    function productListRegClick(){
    	$("#productList ul").children("li").unbind("click").click(function(){
    		if($(this).attr("id") != "downListLi"){
    			$("#productList ul").children("li").removeClass("curr");
    			$("#downListLiText").html("其它");
    			$(this).addClass("curr");
    			selectPlanId = $(this).attr("_val");
    			renderProductInfo(selectPlanId);
    			if(validated){
    				validateBizAvailability();
    			}
    		}
    	});
    }
    function authCodeError(msg){
    	$("#authCodeInput").parent().find("s").attr("class", "icon-error").attr("style", "display:inline-block;");
    	$("#authErrorMsg").html(msg.replace("请", ""));
    }
    productListRegClick();
    downListRegClick();
    $("#phoneContainer input").blur();
    </script>
