				<script>

				(function(){
					var picLi = $(".pic-show li"),
						index = 0,
						btnPrev = $(".pic-show .radarpro-prev"),
						btnNext = $(".pic-show .radarpro-next"),
						timeId = null;

					btnPrev.click(function(){
						clearInterval(timeId);
						changeLi(--index % picLi.length);
						autoChange();
						return false;
					});
					btnNext.click(function(){
						clearInterval(timeId);
						changeLi(++index % picLi.length);
						autoChange();
						return false;
					});

					function autoChange() {
						timeId = setInterval(function(){
							changeLi(++index % picLi.length);
						}, 5000);
					}

					function changeLi (i) {
						picLi.hide().eq(i).show();
					}

					autoChange();
				})();
				</script>
