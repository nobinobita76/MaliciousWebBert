<script>
$('#com-num').click(function(){
	var comPos = $('#tucomment-container').offset();
	window.scrollTo(0,comPos.top-50);
})
function tukulist(list){
	var html = '';
	if($.isEmptyObject(list)) return;
	var vids = [];
	$.each(list.data, function(i,item){
		var videoTpl = '<li class="uiVideo__item">\
						<a class="uiVideo__ori" href="{link}" target="_blank">\
						<img src="{picsrc}" alt="" class="uiVideo__img">\
						<span class="uiVideo__time">{time}</span></a>\
						<a href="{link}" class="uiVideo--title" target="_blank">{title}</a>\
						</li>';
		if(i < 6){
			html += tpl_render(videoTpl,item);	
			vids.push(item.vid);
		}	
	});
	if(html.length > 0){
		$('#duowan-video-list').html(html);
		//upPlaynum(vids);
		$('#meinv-video').show();
	}
}
function tpl_render(tpl, data){
	return tpl.replace(/\{([\w\_]+)\}/g, function(){
		return (typeof data[arguments[1]]==='undefined')?'':data[arguments[1]];	
	});
}
function upPlaynum(vids){
	$.ajax({url:'http://playstats.v.duowan.com/index.php?r=api/get&vid='+vids,
		dataType:'jsonp',
		success:function(res){
			$.each(res, function(vid,num){
				var playObj = $('#play_total_'+vid);
				if(typeof playObj != 'undefined'){
					playObj.text(formatVideoPlayNum(vid,num,'kan'));
				}
			});
		}
	});
}
function numFormat(num){
    var rs;
    if(num > 999999){
        rs = Math.round( num / 10000 ) + "万";
        return rs;
    }else if(num > 9999){
        rs = Math.round(num / 10000 * 10) / 10 + "万";
        return rs;
    }else if( num > 999 ){
        return parseFloat(num).toLocaleString();
    }else{
        return num;
    }
}

function formatVideoPlayNum(vid, num, channel){
    var num = parseInt(num);
    var salt = parseInt(vid * 1.2) + '';
    salt = parseInt( salt.substr(salt.length - 3, 3) );
    salt = salt > 500 ? salt : (1000 - salt);
    
    if ( -1 == $.inArray(channel, ['lol','df']) || 0==num ) {
        num = num + salt;
    } 
    
    num =  numFormat( num );
    return num;            
}

