                    $(_self.picSelector).each(function () { $(this).hide(); });
                    $(_self.picSelector + ':eq(' + _self.index + ')').show();
                    $(_self.switchSelector).each(function () { $(this).removeClass("active"); });
                    $(_self.switchSelector + ':eq(' + _self.index + ')').addClass("active");

                    _self.index = (++_self.index) % _self.totalCount;
                    _self.timeoutId = setTimeout(_self.toggle, _self.timeOut);
                };

                this.flip = function () {
                    $(_self.picSelector).each(function () { $(this).hide(); });
                    _self.index = $(this).index();
                    $(_self.picSelector + ':eq(' + _self.index + ')').show();
                    $(_self.switchSelector).each(function () { $(this).removeClass("active"); });
                    $(_self.switchSelector + ':eq(' + _self.index + ')').addClass("active");

                    clearTimeout(_self.timeoutId);
                    _self.index = (++_self.index) % _self.totalCount;
                    _self.timeoutId = setTimeout(_self.toggle, _self.timeOut);

                };

                this.slideTo = function (idx) {
                    $(_self.picSelector).each(function () { $(this).hide(); });
                    _self.index = idx;
                    $(_self.picSelector + ':eq(' + _self.index + ')').show();
                    clearTimeout(_self.timeoutId);
                    _self.index = (++_self.index) % _self.totalCount;
                    _self.timeoutId = setTimeout(_self.toggle, _self.timeOut);
                };

                $(this.switchSelector).each(function () {
                    $(this).mouseover(_self.flip);
                });

                //                this.timeoutId = setTimeout(_self.toggle,_self.timeOut);
                _self.toggle();

                this.flipTo = function () {
                    $(_self.picSelector).each(function () { $(this).hide(); });
                    $(_self.picSelector + ':eq(' + _self.index + ')').show();
                    $(_self.switchSelector).each(function () { $(this).removeClass("active"); });
                    $(_self.switchSelector + ':eq(' + _self.index + ')').addClass("active");

                    _self.index = (++_self.index) % _self.totalCount;
                    clearTimeout(_self.timeoutId);
                    _self.timeoutId = setTimeout(_self.toggle, _self.timeOut);
                };

            };

            var picSelector = ".pic-scroll .pic-scroll-layout";
            var switchSelector = ".slideNav li";
            focusSlides = new SlideObject(picSelector, switchSelector, 6, 6000);
            if (window.location.hash && window.location.hash.indexOf("#top") == 0) {
                var picId = parseInt(window.location.hash.substring(4));
                if (!isNaN(picId) && picId < focusSlides.totalCount) {
                    focusSlides.slideTo(picId);
                }
            }

            $(".pic-scroll .btn-prev").click(function () {
                focusSlides.index = (focusSlides.index + focusSlides.totalCount - 2) % focusSlides.totalCount;
                focusSlides.flipTo();
            });

            $(".pic-scroll .btn-next").click(function () {
                focusSlides.flipTo();
            });

            picSelector = ".showbiz-text .column-pic";
            switchSelector = "#start_dynamic li";
            entSlides = new SlideObject(picSelector, switchSelector, 5, 6000);
        })();
        (function () {
            function reset() {
                if ($("#query").val() != '')
                    $(".qreset2").show();
                else
                    $(".qreset2").hide();
            };
            $("#query").bind("keyup", reset).bind("focus", reset).bind("input", reset).bind("change", reset);
            $(".qreset2").click(function () { $("#query").val(""); $(".qreset2").hide(); });
        })();
        $(function () {
            $(".img_stock img").each(function (index) {
                $(this).error(function () {
                    $(this).attr("src", "images/bg_stock.jpg");
                })
            })

        });
        (function (win) {
            var curElemId = "rollnews";
            var nextElemId = "rollnews1";
            var delta = 1;
            var $roll_layout = $(".roll-layout");
            var rollWidth = $roll_layout.width();
            var newsWidth = $("#" + curElemId).width();
            var curLeft = $("#" + curElemId).css("left");
            var nextLeft = $("#" + nextElemId).css("left");
            var switchStatus = false;
            curLeft = parseInt(curLeft);
            nextLeft = parseInt(nextLeft);

            var rollLeft = function () {
                if (curLeft > rollWidth - newsWidth) { //当前条正常显示
                    curLeft = curLeft - delta;
                } else if (curLeft > 0 - newsWidth) { //当前条右侧到达右边界
                    curLeft = curLeft - delta;
                    nextLeft = nextLeft - delta;
                } else {//当前条右侧到左边界
                    curLeft = rollWidth;
                    nextLeft = nextLeft - delta;
                    switchStatus = true;
                }

                $("#" + curElemId).css("left", curLeft + "px");
                $("#" + nextElemId).css("left", nextLeft + "px");
                if (switchStatus == true) {
                    $("#" + curElemId).attr("id", nextElemId + "tmp");
                    $("#" + nextElemId).attr("id", curElemId);
                    $("#" + nextElemId + "tmp").attr("id", nextElemId);
                    curLeft = $("#" + curElemId).css("left");
                    nextLeft = $("#" + nextElemId).css("left");
                    switchStatus = false;
                    curLeft = parseInt(curLeft);
                    nextLeft = parseInt(nextLeft)
                }
            };
            var intId = setInterval(rollLeft, 36);

            var pauseRoll = function () {
                clearInterval(intId);
            };
            var continueRoll = function () {
                intId = setInterval(rollLeft, 36);
            };
            $roll_layout.mouseover(pauseRoll);
            $roll_layout.mouseout(continueRoll);
        })(window);

    </script>
