        (function () {
            var SlideObject = function (picSelector, switchSelector, totalCount, timeOut) {
                this.picSelector = picSelector;
                this.switchSelector = switchSelector;
                this.totalCount = totalCount;
                this.timeOut = timeOut;
                this.index = 0;
                this.timeoutId = -1;

