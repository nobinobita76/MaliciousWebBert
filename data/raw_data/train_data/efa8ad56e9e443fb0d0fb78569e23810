    <script type="text/javascript">
/*子页面左侧菜单随滚动条固定定位*/

    $(window).scroll(function(){
        var scroll=$(window).scrollTop(),
        mainTop=$('#rightxh').height(),
        mainLeftH=855,
        footH=1,
        mainLeftB=$(document).height()-scroll-855;

        if(scroll>mainTop-115){
            if(mainLeftB>footH){
                $("#main-left").css({'position':'fixed','top':10,'bottom':'auto'});
            }else{
                $("#main-left").css({'position':'fixed','top':'auto','bottom':'328px'});
            }
        }else{
            $("#main-left").css({'position':'static'});
        }
    })

</script> 
