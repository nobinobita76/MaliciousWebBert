						oflag = 1;
						setTimeout(function(){
							ga('send', 'event', 'index', 'popup', 'show');
							$('.bg-Fixed-shadow').show();
							$('.ad-window-center').show();
						},1000);
					}
				}
			})	
		} else {

		}
		*/
	
	if(!$.cookie(cookie_name) && MODEL == 'index' && ACTION == 'index' && (DOMAIN == 'meguo' || DOMAIN == 'jiu')){
			setTimeout(function(){
				//ga('send', 'event', 'index', 'popup', 'show');
				$('.bg-Fixed-shadow').show();
				$('.ad-window-center').show();
			},3000);
			//$('.right-fix-packets').show();
			//$('.left_fixed').show();
	}else{
		//setTimeout(function(){$('.bg-Fixed-shadow').show();$('.ad-window-center').show();},3000);
	}
		var w=$(window).width();
		$('.jumpto').click(function(){
			$.cookie(cookie_name, "true", {path: "/",expires: 7,domain:cookie_domain});
			$('.ad-window-center').hide();
			$('.bg-Fixed-shadow').hide();
			return true;
		})
		$('.close-red-jump').click(function(){
			$.cookie(cookie_name, "true", {path: "/",expires: 7,domain:cookie_domain});
			$('.ad-window-center').hide();
			$('.bg-Fixed-shadow').hide();
		});
		$('.close-red-tips').click(function(){
			$.cookie(cookie_name, "true", {path: "/",expires: 7,domain:cookie_domain});
			$('.ad-window-center').hide();
			$('.bg-Fixed-shadow').hide();
			//$('.right-fix-packets').fadeIn();
			//$('.left_fixed').fadeIn();
		})
		$('.red_fix_close').click(function(){
			$.cookie(cookie_name, "true", {path: "/",expires: 7,domain:cookie_domain});
			$('.bg_fix').animate({left:"-"+w},500);
			$('.btm_fixed').animate({left:"-"+w},500,function(){
			$('.btm_fixed').hide();
			$('.bg_fix').hide();
			//$('.left_fixed').animate({left:"-1px"},500);
			//$('.left_fixed').show();
			});	
		})
		/*$('.red_fix_open').click(function(){
			$('.left_fixed').animate({left:"-160px"},500,function(){
				$('.left_fixed').hide();
				$('.btm_fixed').animate({left:"0"},500);
				$('.btm_fixed').show();
				$('.bg_fix').animate({left:"0"},500);
				$('.bg_fix').show();
			})
		})*/
		$('.left_fix_close,.red_fix_open').click(function(){
			$('.left_fixed').hide();
		})
		/*
		$(window).scroll(function(){
		    var top = $(window).scrollTop();
		    if(top >= 500){
		      $('.pop-up-bcj').show();
		    }else{
		      $('.pop-up-bcj').hide();
		    }
		})
*/
		$('.pop-up-bcj').hover(function(){
				$('.pop-s-img').show();
			},function(){
				$('.pop-s-img').hide();
		})
		
	})


