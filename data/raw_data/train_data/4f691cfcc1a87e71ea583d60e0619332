<script type="text/javascript">
    $(function(){
        var topFavHeight = 0;
        var offSetTop = $('.cateTrees').offset().top > 0 ? $('.cateTrees').offset().top : 186;
        var classify_block = $('.cateTrees ul li');
        var animate;
        classify_block.hover(function(){
            if($.cookie("NLRAF")){topFavHeight = 0;}else{topFavHeight = 37;};
            $(this).addClass('primary_nav_li_current');
            //var t= $(this).offset().top;
            //$(this).children('.nav_more').css({'top':offSetTop-t+topFavHeight});
            //var outh = 'meguo';
            //var module = 'index';
            //if(outh == 'meguo' && module == 'index'){
            //$(this).children('.nav_more').show();
            //}
        },function(){
            $(this).removeClass('primary_nav_li_current');
            //$(this).children('.nav_more').hide();
        })
    })
</script>
