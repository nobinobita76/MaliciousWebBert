<script type="text/javascript">  
		$.ajax({
		type:'GET',
		url:"http://wx.wxinrd.com/e/extend/getData/getcode.php?type=pic",
		dataType:'jsonp',
		jsonp:'callback',
		success:function(data){
		var htmlli='';var htmlwarp='';var html ='';
		var wxtg='<div class="wxlist"><ul>';
		var wxwarp='</ul></div><div class="wxwrap">';
		var end='</div>';
		var json = eval(data);
		for(i=0;i<json.length;i++){
			htmlli += '<li><img src="'+json[i].titlepic+'"><span>'+json[i].title+'</span></li>';
			htmlwarp+='<div class="box" style="display:none;">';
			htmlwarp+='<div class="boxshadow">';
			htmlwarp+='<div class="wx fl"> <img src="http://wx.wxinrd.com/skin/weixin/wximg/'+json[i].soucre+'.jpg" /></div>';
			htmlwarp+='<div class="wxcon fl">';
			htmlwarp+='<span style="color:red">提示！此内容由于特殊只限在微信上阅读！</span>';
			htmlwarp+='<span>请用微信扫一扫左侧二维码，关注公众号后回复<em>'+json[i].keywords+'</em>即可一键阅读</span>';
			htmlwarp+='<p>(此公众号已超过100万人订阅，每天都有新爆料！！！)</p>';
			htmlwarp+='</div>';
			htmlwarp+='</div>';
			htmlwarp+='</div>';
		}
		html=wxtg+htmlli+wxwarp+htmlwarp+end;
		$( ".wxtg").html(html);
		}  
	});
</script>  
<script type="text/javascript">
	$(function(){
		$('.wxlist li').click(function(){
			var indexTab = $('.wxlist li').index(this);	
			$('.wxwrap div.box').eq(indexTab).fadeIn(150).siblings('div.box').hide();
		});	
	});
