            $(function() {
                $("#changsize #big").click(function(e) {
                    $(".text_txt").css('font-size', '22px');
                    $(".text_txt").css('line-height', '36px');
                    $("#changsize a").removeClass('on');
                    $(this).addClass('on');
                });
                $("#changsize #mid").click(function(e) {
                    $(".text_txt").css('font-size', '14px');
                    $(".text_txt").css('line-height', '26px');
                    $("#changsize a").removeClass('on');
                    $(this).addClass('on');
                });
                $("#changsize #small").click(function(e) {
                    $(".text_txt").css('font-size', '12px');
                    $(".text_txt").css('line-height', '24px');
                    $("#changsize a").removeClass('on');
                    $(this).addClass('on');
