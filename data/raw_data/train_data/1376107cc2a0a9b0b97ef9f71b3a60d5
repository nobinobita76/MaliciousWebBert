<script class="eqx-script">

    var HOST = window.eqxHOST = {
        DOMAIN: 'eqxiu',
        FILE: "http://res1.eqh5.com/",
        // 文件资源
        BIND_USER_FILE: 'http://res.eqh5.com/',
        FREE_USER_FILE: 'http://res1.eqh5.com/',
        VIP_USER_FILE: 'http://res2.eqh5.com/',
    };

    window.eqxVersion = '391a7f8';
    // console.log('version:', window.eqxVersion);
    (function () {
        function isMobile() {
            return /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini|Mobi/i.test(navigator.userAgent) ||
                    window.innerWidth < 500;
        }

        function setScale() {
            if (window.top !== window) {
                return;
            }
            var pageScale = 1;

            var width = document.documentElement.clientWidth || 320;
            var height = document.documentElement.clientHeight || 486;

            if (width / height >= 320 / 486) {
                pageScale = height / 486;
            } else {
                pageScale = width / 320;
            }
//             console.log('screen.width', width, 'screen.height', height, 'setScale', pageScale);
            // meta
            var content = 'width=320, initial-scale=' + pageScale + ', maximum-scale=' + pageScale + ', user-scalable=no';
            document.getElementById('viewport').setAttribute('content', content);
        }

        // logo
        var logo = document.getElementById('loading');
        var scene = window.scene;
        if (scene) {
            var serverUrl = HOST.FILE;
            // 不包含 eqxiu 域名 且 不是 app 端
            if (location.href.indexOf(HOST.DOMAIN) === -1 && !window.viewData) {
                serverUrl = HOST.BIND_USER_FILE;
            }
            if (serverUrl.slice(-1) != '/') {
                serverUrl += '/';
            }
            if (scene.loadingLogo) {
                var url = serverUrl + scene.loadingLogo;
                logo.innerHTML = '<div class="loading-logo"><img src="' + url + '" class="logo-image"></div>';
            }
            if (scene.cover) {
                var shareImage = document.createElement('img');
                shareImage.id = 'shareImage';
                shareImage.style = "display:none";
                shareImage.src = serverUrl + scene.cover;
                document.body.appendChild(shareImage);
            }
        }

        if (isMobile()) {
            setScale();
            logo.style.display = 'block';
        }

        var tpls = document.getElementsByClassName('eqx-script');
        var len = tpls.length;
        /*for (var i = len - 1; i >= 0; i--) {
            tpls[i].parentNode.removeChild(tpls[i]);
        }*/

    })();

</script><img id="shareImage" src="http://res1.eqh5.com/o_1bbsjn6ll1q3h6e811aeqbo1isu9.jpg?imageMogr2/auto-orient/crop/!767x767a137a0">

