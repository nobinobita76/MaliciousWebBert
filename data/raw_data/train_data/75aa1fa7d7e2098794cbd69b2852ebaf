					$(function () {
					    initialSearchValue();
						$(document).bind('keydown', function (e) {
							var key = e.which;
							if (key == 13) {
								$('#searchbtn').focus();
								searchbykey()
							}
						});
					});
					 function initialSearchValue(){
					   
					    $('#searchform').find('input,select').each(function(){
					    var name=$(this).attr("name");
					    if (name!=undefined){
						   if (name=='queryparam'){
						    
						   }else{
						      v=getSearchValue(name);
						     if ($(this).attr("type")=="radio"){
							   if ($(this).val()==v){
							    $(this)[0].checked=true;
							   }
							 }else{
					           $(this).val(v);
							 }
						   }
						 }
					   });
					   
					    $('#searchform').find('select').each(function(){
					       var name=$(this).attr("name");
						    if (name!=undefined){
							  str+=","+name+"-"+$(this).val();
							}
						});
					 
					 
					 }
					 function getSearchValue(field){
					  var url=location.href.replace(/.html/g,'');
                      var parr=url.split('/');
					  var qparam=parr[parr.length-1];
					  qparam=qparam.substring(1).split(',');
					  for(var i=0;i<qparam.length;i++){
					     var fa=qparam[i].split('-');
						 if (fa[0]==field){
						  return fa[1];
						 }
					  }
                        return "";
					 }
					
					 function searchbykey(){
					   var str='';
					   var queryparam='';
					  
					   $('#searchform').find('input,select').each(function(){
					    var name=$(this).attr("name");
					    if (name!=undefined){
						   if (name=='queryparam'){
						    queryparam=$(this).val();
						   }else{
						     if ($(this).attr("type")=="radio"){
							   if ($(this)[0].checked==true){
							    str+=","+name+"-"+$(this).val();
							   }
							 }else if ($(this).val()!=''){
					           str+=","+name+"-"+$(this).val();
							 }
						   }
						 }
					   });
					 //   alert(queryparam+str);
						location.href=queryparam+str+".html";
					  
					 }
