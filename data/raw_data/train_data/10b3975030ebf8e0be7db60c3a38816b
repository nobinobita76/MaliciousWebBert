<script type="text/javascript">
    $(function(){
        $(".ItrailNow").on('click', function () {
            $.get("/shop/is_user_login.json", {random:Math.random()}, function (result) {
                result = eval("(" + result + ")");
                if (!result.isLogin) {
                    location.href = result.redirect + encodeURIComponent(location.href);
                } else {
                    $.get("/shop/is_trial_product.json", {random:Math.random(), productId:$("#productCode").val()}, function (result) {
                        result = eval("(" + result + ")");
                        if (result.status == 1) {
                            var tpl$ = $('[type="text/tpl_trial_0"]');
                            new Alipw.Msg.alert('提示', tpl$.filter('[data-name="already"]').html(), null, {
                                width:500,
                                OKText:'确认',
                                buttonAlign:'center'
                            });
                        } else if (result.status == 2) {
                            var tpl$ = $('[type="text/tpl_trial_1"]');
                            //对话框
                            new Alipw.Msg.alert('提示', tpl$.filter('[data-name="already"]').html(), null, {
                                width:500,
                                OKText:'确认',
                                buttonAlign:'center'
                            });
                        } else if (result.status == 3) {
                            var accountParam = "";
                            if (parseInt($("#productAccountSize").val()) > 0) {
                                accountParam = "&account_quanlity=" + parseInt($("#productAccountSize").val());
                            }

                            var skuCode = $('#format-right a.current').attr('title');
                            if (skuCode == '' || skuCode == undefined) {
                                var tpl$ = $('[type="text/tpl_trial_0"]');
                                new Alipw.Msg.alert('提示', tpl$.filter('[data-name="already"]').html(), null, {
                                    width:500,
                                    OKText:'确认',
                                    buttonAlign:'center'
                                });
                                return;
                            }

                            var url = "/shop/order_confirm.htm?skuCode=" + skuCode + "&productId=" + productId+"&isTrial=true" + accountParam;
                            location.href = url;
                        } else {
                            var tpl$ = $('[type="text/tpl_0"]');
                            //对话框
                            new Alipw.Msg.alert('提示', tpl$.filter('[data-name="already"]').html(), null, {
                                width:500,
                                OKText:'确认',
                                buttonAlign:'center'
                            });
                        }
                    });
                }
            });

        });
    });
</script>


