$("#reserveHouse").click(function(){
var userName = $("#username").val();
var mobile = $("#mobile").val();
var explain = $("#explain").val();
var saleId = $("#saleid").val();
if(saleId==""){
alert("无效房源参数!");
}else if(userName == ""||userName=="您的姓名"){
$('#username').focus();
alert("请输入您的姓名!");
}else if(mobile=="" || mobile=="手机号码" || !isMobil(mobile)){
$('#mobile').focus();
alert("手机号为空或无效!");
}else if(explain == ""||explain=="您对本次预约特别说明"){
$('#explain').focus();
alert("请简要描述!");
}else{
reserveHouse(userName,mobile,explain,saleId);
}
});
