for(var i in params){
if(params[i]){
if(i=="ad"){
$.ajax(
{type : "post",
url : "/sale/doURLEncode.do",
cache : false,
async : false,
data : {"address":params[i]},
success : function(data){
if(data!="")
arrayObj[0] = i+data;
},
dataType : "html"}
);
}else{
delete params[name];
}
}
}
var seoUrl="";
for(var i=0; arrayObj.length>i; i++){
if(arrayObj[i]!=""&&arrayObj[i]!=null){
if(seoUrl==""){
seoUrl="/sale/search/"+arrayObj[i];
}else{
seoUrl+="_"+arrayObj[i];
}
}
}
if(seoUrl!=""){
window.location.href = seoUrl+".html";
}
});
