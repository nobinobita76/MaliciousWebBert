<script type="text/javascript">
function focusAction(Id){
var value = $("#"+Id).val();
if(value=="您的姓名"||value=="手机号码"||value=="您对本次预约特别说明"||value=="输入小区名称、地址或标题"){
$("#"+Id).val("");
}
$("#"+Id).css("color","#000");
}
function blurAction(Id){
var value = $("#"+Id).val();
if(value==""){
if(Id=="username"){
$("#"+Id).val("您的姓名");
}else if(Id=="mobile"){
$("#"+Id).val("手机号码");
}else if(Id=="explain"){
$("#"+Id).val("您对本次预约特别说明");
}else if(Id=="address"){
$("#"+Id).val("输入小区名称、地址或标题");
}
$("#"+Id).css("color","#999");
}
}
//验证是座机或手机
function isMobil(s) {
//var patrn1= /^(1(([35][0-9])|(47)|[8][0126789]))\d{8}$/;
//var patrn2= /^0\d{2,3}(\-)?\d{7,8}$/;
//&& !patrn2.exec(s)
//if (!patrn1.exec(s) ) {
// return false;
//} else {
// return true;
//}
var flag = false;
var reNums=new Array(/^1[38]{1}\d{9}$/,/^15[^4]{1}\d{8}$/,/^17[0678]{1}\d{8}$/,/^14[57]{1}\d{8}$/);
for(var i=0;i<reNums.length;i++){
if(reNums[i].test(s)==true){
flag = true;
}
}
return flag;
}
function showDiv(){
var show = $('#tel_online').css('display');
if(show=="block"){
$('#tel_online').css('display','none')
}
else{
$('#tel_online').css('display','block')}
}
$(document).ready(function() {
$("#address").autocomplete("/sale/floorList.do",{
extraParams:{
pageno : "1",
floor:function(){return $("#address").val();}
},
scrollHeight:300,
parse:function(data){
var parsed = [];
var rows = data.items;
for (var i=0; i < rows.length; i++) {
var row = rows[i];
if (row) {
parsed[parsed.length] = {
value: row.floor,
data: row,
result: row.floor
};
}
}
return parsed;
},
width:258,
formatItem: function(row){
return "<table width=228><tr><td>"+row.floor+"</td><td width=100 align=right>(约<b>"+(row.saleCountHouse+row.saleCountVilla)+"</b>套)</td></tr></table>";
},
dataType : "json"
});
