$("#collect").click(function(){
var urladdress = window.location.href;
var r=Math.random();
$.ajax({
url : "/sale/collect.do?"+r,
data : {"saleId":28763238,"city":377,"cityTable":"C","urladdress":urladdress,"title":encodeURIComponent("实验学校***") },
success : function(data){
if(data.type==0){
window.location.href='http://member.fccs.com/login.jhtml?forwardUrl='+encodeURIComponent(window.location);
}
if(data.type==1){
$("#collect").attr("class","i6");
$("#collect").html("已收藏");
}
if(data.type==2){
$("#collect").attr("class","i7");
$("#collect").html("收藏房源");
}
},
error:function(data){
alert("内部错误！")
}
});
});
});
//街景地图JS_Start
function init(logtitude,latitude){
$('#container2').hide();
mapBounds(logtitude,latitude,'container','container2',function(hasView){
if(hasView==0){
hideView();
$("#street_li").hide();
}else{
showView();
}
});
}
function showView(){
$('#container2').show();
}
function hideView(){
$('#container2').hide();
}
function changeVerificationCode() {
var time = new Date().getTime();
$('#verifyImg').attr("src", "/tools/fccsVerify.jhtml?date=" + time );
$('#verifyImg2').attr("src", "/tools/fccsVerify.jhtml?date=" + time );
}
function changeNum(strId, strVal) {
document.getElementById(strId).value = strVal.replace(/\D/gi, "");
}
function IsNum(e) {
var k = window.event ? e.keyCode : e.which;
if (((k >= 48) && (k <= 57)) || k == 8 || k == 0) {
} else {
if (window.event) {
window.event.returnValue = false;
} else {
e.preventDefault(); //for firefox
}
}
}
function callPhone(phone, validate) {
$(".btn").css("background-color","#AAAAAA");
$(".btn").removeAttr("onClick");
var floor = $("#floor400").val();
var recordid = $("#recordid400").val();
var city = $("#city400").val();
var tablename = $("#tablename400").val();
var userid = $("#userid400").val();
var phone = document.getElementById(phone).value;
if((validate == "mobileValidate" && isMobil(phone)) || (validate == "telValidate" && isPhone(phone))) {
var validate = document.getElementById(validate).value;
$.post("/Agency/four/freecall.do",{floor:encodeURIComponent(floor),callPhone:phone,userid:userid,city:city,tablename:tablename,recordid:recordid,validate:validate},function(data) {
if(data == "success") {
alert("正在接通中,请耐心等待!");
$(".btn").css("background-color","#fb7000");
$(".btn").attr("onClick","callPhone('mobilePhone','mobileValidate')");
$(".house_tel_online").hide();
changeVerificationCode();
} else {
alert("验证码错误，请重新输入！");
$(".btn").css("background-color","#fb7000");
$(".btn").attr("onClick","callPhone('mobilePhone','mobileValidate')");
}
});
} else {
alert("请输入正确的手机号码！");
$(".btn").css("background-color","#fb7000");
$(".btn").attr("onClick","callPhone('mobilePhone','mobileValidate')");
}
}
function addFavorite(title,location) {
var ctrl = (navigator.userAgent.toLowerCase()).indexOf('mac') != -1 ? 'Command/Cmd' : 'CTRL';
if (document.all) {
try{
window.external.addFavorite(location, title);
} catch (e){
window.external.addToFavoritesBar(location, title);
}
} else if (window.sidebar) {
try{
window.sidebar.addPanel(title, location, "");
} catch (e){
alert('您可以尝试通过快捷键' + ctrl + ' + D 加入到收藏夹~');
}
} else {
alert('您可以尝试通过快捷键' + ctrl + ' + D 加入到收藏夹~');
}
}
function resize_images(className){
$("#"+className).each(function(){
var imgWidth = $(this).width();
var imgHeight = $(this).height();
//var bbs_width = 499;
//var bbs_height = 368;
var bbs_width = 650;
var bbs_height = 485;
if(imgWidth>imgHeight){
if(imgWidth >= bbs_width){
var imgHeight = (bbs_width*$(this).height())/imgWidth;
var margins = parseInt((bbs_height-imgHeight)/2);
if(margins>0){
//$(this).attr("style","padding-top:"+margins+"px;");
}
$(this).width(bbs_width);
$(this).height(imgHeight);
}
}else{
if(imgHeight >= bbs_height){
var imgWidth = (bbs_height*$(this).width())/imgHeight;
var margins = parseInt((bbs_width-imgWidth)/2);
if(margins>0){
//$(this).attr("style","padding-left:"+margins+"px;");
}
$(this).width(imgWidth);
$(this).height(bbs_height);
}
}
});
}
//街景地图JS_end
//ajax提交预约看房
function reserveHouse(userName,mobile,explain,saleId) {
$.ajax({
type : 'GET',
url : "/sale/addUsersReserveHouse.do",
cache : false,
data : {"username":userName,"mobile":mobile,"explain":explain,"saleid":saleId},
success : function(data){
if(data > 0){
$("#username").val("您的姓名");
$("#mobile").val("手机号码");
$("#explain").val("您对本次预约特别说明");
$("#username").css("color","#999");
$("#mobile").css("color","#999");
$("#explain").css("color","#999");
alert("预约成功！");
}else{
alert("预约失败！");
}
}
});
}
function open1(){
$.ajax({
url: "/sale/showReport.do",
data: {
"recordId": '28763238',
"tableNamePrefix": 'sale'
},
type: "POST",
success: function(data){
if(data!=""){
$(".mobile_detail").html(data);
$(".mobile_detail").show();
}else{
$(".mobile_detail").hide();
alert("房源信息不存在");
}
},
dataType: "html"
});
return false;
}
