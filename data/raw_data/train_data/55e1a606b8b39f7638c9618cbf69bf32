option.yAxis[0].max=data[4].max;
option.legend.data=data[1].legend;
option.xAxis[0].data = data[2].xAxis;
option.series = data[0].series;
myChart.setOption(option);
}
});
}
);
<!-- 房贷计算器环图 -->
function img () {
require(
[
'echarts',
'echarts/chart/pie'
],
function (ec) {
var myChart = ec.init(document.getElementById('pic'));
var interest = parseFloat($("#interest1").val());
var downPayment = parseFloat($("#downPayment1").val());
var amount = parseFloat($("#amount1").val());
var tep1=(amount/(interest+downPayment+amount))*100;
var tep2=(downPayment/(interest+downPayment+amount))*100;
var tep3=(interest/(interest+downPayment+amount))*100;
var option = {
animation : false,
title : {
x : 200,
y : 20
},
tooltip : {
trigger: 'item',
formatter: "{a} <br/>{b} : {d}%"
},
legend: {
orient : 'vertical',
x : 260,
y : 120,
itemGap:12,
data:[
{name:'首付金额'},
{name:'贷款金额'},
{name:'支付利息'}],
formatter: function (name) {
if (name == "首付金额") {
return '首付金额'+':'+Math.round(tep2*100)/100+'%';
}
else if (name == "贷款金额") {
return '贷款金额'+':'+Math.round(tep1*100)/100+'%';
}
else if (name == "支付利息") {
return '支付利息'+':'+Math.round(tep3*100)/100+'%';
}
}
},
color : ['#EB6100', '#90C320', '#00A0EA'],
series : [
{
name:'房贷详情',
type:'pie',
center : ['30%','50%'],
radius : ['60%', '90%'],
itemStyle : {
normal : {
label : {
show : false
},
labelLine : {
show : false
}
},
emphasis : {
label : {
show : true,
position : 'center',
textStyle : {
fontSize : '22',
fontWeight : 'bold'
}
}
}
},
data:[
{value:Math.round(tep2*100)/100, name:'首付金额'},
{value:Math.round(tep1*100)/100, name:'贷款金额'},
{value:Math.round(tep3*100)/100, name:'支付利息'}
]
}
]
};
myChart.setOption(option);
}
);
}
img();
</script>
