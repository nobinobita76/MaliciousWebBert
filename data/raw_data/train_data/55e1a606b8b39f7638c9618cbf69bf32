"tableNamePrefix": 'sale'
},
type: "POST",
success: function(data){
if(data!=""){
$(".mobile_detail").html(data);
$(".mobile_detail").show();
}else{
$(".mobile_detail").hide();
alert("房源信息不存在");
}
},
dataType: "html"
});
return false;
}
