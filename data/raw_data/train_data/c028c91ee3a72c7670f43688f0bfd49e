    function showGame2Nav(){var $=document.getElementById("wrap-b");if($.style.display=="none"){document.getElementById("bar").className="win_ScrollCss s2";setNav(0);if(document.getElementById("gshow"))document.getElementById("gshow").style.width="740px";document.getElementById("wrap-m").style.margin="0 0 0 180px";tool.setCookieByDays("hid_bar",0,0)}else{document.getElementById("bar").className="win_ScrollCss s1";setNav(1);if(document.getElementById("gshow"))document.getElementById("gshow").style.width="920px";document.getElementById("wrap-m").style.margin="0 0 0 1px";tool.setCookieByDays("hid_bar",1,0)}}var NAV_TID=null,NAV_DSH="1";function setNav(A){if(typeof A!="undefined")NAV_DSH=A;var $=tool.getPos(tool.$("wrap")),B=tool.$("bar"),_=tool.$("wrap-b");B.style.left=$[0]+(NAV_DSH?0:180)+"px";B.style.top=$[1]+120+"px";B.value=$[1]+120;B.style.display="";_.style.left=$[0]+"px";_.style.top=$[1]+"px";_.value=$[1];tool.setWidgetPositionByScroll();if(NAV_DSH)_.style.display="none";else _.style.display=""}tool.addEventHandler(window,"scroll",function(){if(NAV_TID)window.clearTimeout(NAV_TID);NAV_TID=window.setTimeout(tool.setWidgetPositionByScroll,10)});tool.addEventHandler(window,"resize",function(){setNav()})

    function game2Navigation(fid)
    {
        if(document.getElementById('nav_ul_'+fid).style.display != '')
        {
                        document.getElementById('nav_fup_78').className='';
            document.getElementById('nav_ul_78').style.display='none';
                        document.getElementById('nav_fup_85').className='';
            document.getElementById('nav_ul_85').style.display='none';
                        document.getElementById('nav_fup_385').className='';
            document.getElementById('nav_ul_385').style.display='none';
                        document.getElementById('nav_fup_1').className='';
            document.getElementById('nav_ul_1').style.display='none';
                        document.getElementById('nav_fup_915').className='';
            document.getElementById('nav_ul_915').style.display='none';
                        document.getElementById('nav_fup_' + fid).className='on';
            document.getElementById('nav_ul_'+fid).style.display='';
        }
        else
        {
                        document.getElementById('nav_fup_78').className='';
            document.getElementById('nav_ul_78').style.display='none';
                        document.getElementById('nav_fup_85').className='';
            document.getElementById('nav_ul_85').style.display='none';
                        document.getElementById('nav_fup_385').className='';
            document.getElementById('nav_ul_385').style.display='none';
                        document.getElementById('nav_fup_1').className='';
            document.getElementById('nav_ul_1').style.display='none';
                        document.getElementById('nav_fup_915').className='';
            document.getElementById('nav_ul_915').style.display='none';
                    }
    }

    window.onload = function() {
        NAV_DSH = parseInt(NAV_DSH, 10);
        setNav();
    }
</script>
