		function loadWebcall() {
			$("#webcall").css("right", 10);
			var opos = GetObjPos($("#webcall").get(0));
			if (!isIE6()) {
				$("#webcall").css('position', 'fixed')
			} else {
				$(window).scroll(function () {
					$("#webcall").css({top: document.documentElement.scrollTop + opos.y});
				});
			}

			function isShowDiv() {
				var date = new Date();
				if (date.getHours() >= 9 && date.getHours() < 18 && !isRestDate(date)) {
					return true;
				} else {
					return false;
				}
			}

			//是否周末
			function isRestDate(date) {
				return ((date.getDay() == 6) || (date.getDay() == 7));
			}

			if (isShowDiv()) {
				$("#onlineIcon").attr("src", staticDomain + "/upload/20121022/20121022165017489.gif").height(15).attr('alt', '有客服在线，欢迎提问！');
			} else {
				$("#onlineIcon").attr("src", staticDomain + "/upload/20121022/20121022165226851.gif").height(15).attr('alt', '没有客服在线，您可以在线留言！');
			}
		}

		$(document).ready(function () {
			loadWebcall();
			$("#webcall").show();
		});

		//获取元素的绝对位置
		function GetObjPos(ATarget) {
			var target = ATarget;
			var pos = new CPos(target.offsetLeft, target.offsetTop);
			var target = target.offsetParent;
			while (target) {
				pos.x += target.offsetLeft;
				pos.y += target.offsetTop;
				target = target.offsetParent
			}
			return pos;
		}

		function isIE6() {
			var ie = "";
			if (window.ActiveXObject) {
				var ua = navigator.userAgent.toLowerCase();
				ie = ua.match(/msie ([\d.]+)/)[1]
			}
			return ie == 6.0;
		}

		//CPos 类
		function CPos(x, y) {
			this.x = x;
			this.y = y;
		}
	</script>
	<script>
		function getHqList($tr) {
			if (!$tr.length) return;
			var id = $tr.attr('data-id');
			$.ajax({
				type: "get",
				cache: true,
				url: "http://hq.sinajs.cn/list=" + id,
				dataType: "script",
				success: function (data) {
					var a = eval("hq_str_" + id);
					if (a) {
						var arr = a.split(',');
						$tr.find('.title').html(arr[0]);
						var num0 = arr[1];
						var num1 = arr[3];
						var $num1 = $tr.find('.num1');
						if (num1 < num0) {
							$num1.addClass('color-green')
						}
						if (num1 > num0) {
							$num1.addClass('color-red')
						}
						$num1.html(num1);
						var num2 = arr[8] / Math.pow(10, 8);
						$tr.find('.num2').html(num2.toFixed(2));
					}
				}
			});
		}

		$(function () {
			$("#dp-tab>tbody>tr").each(function () {
				getHqList($(this));
			});
		});
	</script>
	<script type="text/javascript">
	//登录框切换
   	function qiehuan(id){		
   		if(id=='login2'){
   		document.getElementById("login1").style.display='none';
   		document.getElementById("login2").style.display='block';
   	}
   		else if(id='login1') {
   		document.getElementById("login2").style.display='none';
   		document.getElementById("login1").style.display='block';
   	}
   }
   </script>
