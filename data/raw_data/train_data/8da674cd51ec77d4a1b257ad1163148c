<script type="text/javascript">
    if (getCookie('adClose')) {
        $(".footFixed").hide();
    } else {
        $(".footFixedClose").on('click', function(){
            $(".footFixed").hide();
            setCookie('adClose', 'yes');
        })
    }
    function getCookie(name) {
      if (document.cookie.length > 0) {
        var cVal = document.cookie,
          cVal_start = cVal.indexOf(name + "=");
        if (cVal_start > -1) {
          cVal_start += name.length + 1;
          cVal_end = cVal.indexOf(';', cVal_start);
          cVal_end == -1 && (cVal_end = cVal.length);
          return decodeURIComponent(cVal.substring(cVal_start, cVal_end));
        }
      }
      return '';
    }
    function setCookie(name, value) {
      var exdate = setExdate()
      document.cookie = name + "=" + encodeURIComponent(value) + ";expires=" + exdate.toGMTString() + ';domain=' + location.hostname + ';path=/';
    }
    function setExdate() {
      var exdate = new Date()
      exdate.setHours(23);
      exdate.setMinutes(59);
      exdate.setSeconds(59);
      return exdate;
    }
</script>
<!-- 底部固定块 End -->
