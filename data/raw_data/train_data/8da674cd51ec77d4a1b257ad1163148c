    background: url(/app_images/stock/index/footFixed.png) no-repeat center bottom;
}
.footFixedClose {
    position: absolute;
    top: 80px;
    left: 50%;
    margin-left: 530px;
    display: block;
    width: 20px;
    height: 20px;
    text-indent: -999em;
