    <script type="text/javascript">
function cr180_touch_field(){
//171717171717λ1717
var Scroll = $(document).scrollTop();

//1717171717071717001217
var Height = $('#swap_content').outerHeight();

//17091717171717001217
var Height_on = $('body').outerHeight();

//171717051717061717Y17171717
var Endpost = $('.cr180_postbox').eq($('.cr180_postbox').length -1);
var EndTop = Endpost.outerHeight() + Endpost.offset().top;
return {
'Scroll' : Scroll,
'Height' : Height,
'Height_on' : Height_on,
'EndpostTop' : EndTop,
'StartTop' : $('.cr180_postbox').eq(0).offset().top
}
}

function cr180_viewthread_post(e){
var X = cr180_touch_field();
//171717171717С171117051717061717 17171717171717051717061717171717
if(X.Scroll < X.StartTop || X.Scroll > X.EndpostTop){
//171717С171117051717061717  0417171717171717171717
$('#cr180_subject_fixed').removeClass('scroll');
}else{
$('#cr180_subject_fixed').addClass('scroll');
}

}
$(document).ready(function(){
$(document).scroll(function(){
   cr180_viewthread_post();
});
});
var Au;
$('#thread_list .cr180_postbox').click(function(e){
var Sp = $('#thread_positionmenu');
if(Sp.hasClass('a')){
Sp.removeClass('a');
if(Au) clearTimeout(Au);
$('#v_num_fixed').removeClass('a');
}else{
var X = cr180_touch_field();

var Nums =$(this).find('.cr180_view_member .cr_number').html();
if(Nums){
$('#v_num_fixed').addClass('a').html(Nums).css('top',(e.pageY - X.Scroll)+'px');
}else{
$('#v_num_fixed').removeClass('a').attr('style',' ');
}
var nextpid = $(this).attr('nextpid');

if(Au) clearTimeout(Au);
Au = setTimeout(function(){
$('#v_num_fixed').removeClass('a').attr('style',' ');
},1000);

}
});
var lochash = window.location.hash;
if(lochash){
lochash = lochash.replace(/#pid/,'');
CR_scrollTop('#cr180_posttableid_'+lochash,40);
}

    </script> 
