    <script type="text/javascript" charset="gb2312">
var SiteID=59;
function Showkc(classid,orgid)
{
	if(classid=="" || orgid=="")
	{
	$("NewsID").disabled=true;
	$("NewsID").options[0]=new Option("请选择课程","")
	
	}
	else
	{

   $("NewsID").disabled=false;
	$("NewsID").options[0]=new Option("请选择课程","")
 // document.write("classid="+classid+"&orgid="+orgid)
	selectpush("NewsID","action=n&classid="+classid+"&orgid="+orgid)
	}
}

function selectpush(objid,url)
{


	try
		{
			//document.write("/ajax/kaibansou.asp?"+url);
    		http.open("/ajax/kaibansou.asp?"+url);
			http.send('');
		//	alert(http.response)
			tem=http.response.split("|");
			$(objid).options.length=1
			for(i=0;i<tem.length;i++)
			{
			tem2=tem[i].split(",")
		//	alert(tem2[0]+"|"+tem2[1])
			$(objid).options[i+1]=new Option(tem2[0],tem2[1]);
			}
		}
		catch (e)
		{
		   // alert(e.message);
		}

}
selectpush("classid","action=b&orgid=5fc2b85871e824c0")
</script> 
