        function addgetparam (argument) {
            baseurl = decodeURIComponent(document.URL)
            baseurl = baseurl.split('?');
            if(baseurl[1]){
                baseurl = baseurl[0]+'?'+argument+'&'+baseurl[1]
            }else{
                baseurl = baseurl[0]+'?'+argument;
            }
            return encodeURIComponent(baseurl);


        }
