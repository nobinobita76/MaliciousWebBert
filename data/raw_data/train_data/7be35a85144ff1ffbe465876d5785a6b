	var $GF = function () {
		var queue = [],
			push = queue.push;
		queue.push = function (fn) {
			if(queue.complete === true){
				setTimeout(fn, 0);
			}
			else{
				push.call(queue, fn);
			}
		};
		queue.complete = false;
		return queue;
	}();
