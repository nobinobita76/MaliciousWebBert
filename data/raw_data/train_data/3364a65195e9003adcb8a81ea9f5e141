<script>
var fixCheck = function(){
	var limit = $(window).scrollTop() + $(window).height(),	o = $("#index-fixed-r"),
	t1 = o.parent().offset().top + o.height(), t2 = o.parent().offset().top + o.parent().height()-0, t3 = (o.parent().width() / 2) - o.width();
	if($(window).width()>=784 && o.height()<o.parent().height() && limit >= t1){
		if(IE()<=6){
			var a = limit>=t2 ? t2+3 : limit, b = a - o.parent().offset().top - o.height()-3;
			o.css("margin-top", b+"px")
		}else if(limit >= t2){
			o.css({"position":"fixed", "left":"50%", "margin-left":t3+"px", "bottom":(limit-t2)+"px"})
		}else{
			o.css({"position":"fixed", "left":"50%", "margin-left":t3+"px", "bottom":"3px"})
		}
	}else{
		o.css({"position":"relative", "bottom":"auto", "left":"auto", "margin-left":"-"+o.width()+"px", "margin-top":"auto"})
	}
};
fixCheck();
window.onload = window.onresize = window.onscroll = fixCheck;
</script>


