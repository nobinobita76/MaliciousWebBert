$(function() {
    $(".topnav").hover(function() {
        $(this).addClass("curr");
        $(this).children("div").show();
    }, function() {
        $(this).removeClass("curr");
        $(this).children("div").hide();
    });
    $(".topnav li").hover(function() {
        $(this).addClass("tcurr");
    }, function() {
        $(this).removeClass("tcurr");
    });
})
</script> 
