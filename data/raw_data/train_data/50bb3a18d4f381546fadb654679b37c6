  <script type="text/javascript">
$('.sdet_tab .li').click(function(){
	$(this).addClass('active').siblings().removeClass('active');
	var l = $(this).position().left;
	var w = $(this).outerWidth()-1;
	$('.sdet_tab .cur').css({'width':w+'px','left':l+'px'});
	var index = $(this).index();
	$('.tab_wrap .tab_box').eq(index).show().siblings().hide();
})
$('.sdet_tab .li').eq(0).click()

$('.bctosj').hover(function(){
