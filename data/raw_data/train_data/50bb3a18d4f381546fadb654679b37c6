var Tel = '18277188137';
$(function() {
    $.getJSON('/post/ajax/tel_area.asp', {
        tel: Tel
    },
    function(data) {
        if (data.QueryResult != 0) {
            var Address = data.Address;
            $('#LoadTel').html('( 归属地：' + Address.replace('_', ' ') + ' )');
        }
    })
})
