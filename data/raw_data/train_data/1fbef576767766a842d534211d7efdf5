	function updateCache (elems) {
		var error_html = '更新失败.<a href="javascript:void(0);" class="update-cache"><b>重新更新</b></a>',
			elem = $('.whois-table table tbody'),
			obj = document.getElementById('site'),
			domain = url2domain(obj.value),
			_this = elems;
		_this.hide();

		$('.update-loading').show();
			$.ajax({
				url:'http://whois.aizhan.com/api/whois',
				data:{'domain':domain,'cc':'1dfcafdc1f7b48193e7dd413fb8a61ae','rn':1491037953,'update':true},
				type:"GET",
				dataType:"json",
				timeout:30000,
				cache:false,
				error:function(){
					_this.show();
					_this.parents('td').html(error_html);
					$('.update-cache').bind('click',function(){
						updateCache($('.update-cache'));
					});
				},
			success:function(json){
				if(Object.prototype.toString.call(json) !== '[object Object]' || json==""){
					_this.html(error_html);
					return;
				}
				if(!!json['error']){
					_this.html('<font color="red">'+json['error']['message']+'</font>'+error_html);
					return;
				}

				if(!!(json['errstr'])){
					_this.html('<font color="red">'+json['errstr'].join(" ")+'</font>'+error_html);
				}else if(json['regrinfo']['registered']=='no' || json['regrinfo']['registered'] == "unknown"){
					elem.html('<tr><td colspan="2" height="60" align="center"><span id="main_title">该域名未被注册。</span></td></tr>');
				}else{
					var html = '';
					if(elems.context.className == 'update-cache'){
						html += '<tr><td colspan="2" height="28" align="center"><span id="main_title"><span>已更新</span></span></td></tr>';
					}

					html += '<tr><td class="tablehead">域名：</td><td>'+domain+'</td></tr><tr>';
					html += '<tr><td class="tablehead">注册商：</td><td>'+(!!json['regyinfo']['registrar']?json['regyinfo']['registrar']:'-')+'</td></tr>';
					html += '<tr><td class="tablehead">参照页：</td><td>'+(!!json['regyinfo']['referrer']?json['regyinfo']['referrer']:'-')+'</td></tr>';
					html += '<tr><td class="tablehead">域名持有人/机构名称：</td><td>'+(!!json['regrinfo']['name']?'<a target="_blank" href="/reverse-whois?q='+json['regrinfo']['name']+'&t=registrant">'+json['regrinfo']['name']+'</a>':'-')+'</td></tr>';
					html += '<tr><td class="tablehead">域名持有人/机构邮箱：</td><td>'+(!!json['regrinfo']['emailCode']?'<a target="_blank" href="/reverse-whois?q='+json['regrinfo']['emailCode']+'&t=emailCode"><img src="http://whois.aizhan.com/emailimage?email='+json['regrinfo']['emailCode']+'" align="absmiddle" /></a>':'-')+'</td></tr>';
					html += '<tr><td class="tablehead">创建时间：</td><td>'+(!!json['regrinfo']['domain']['created']?json['regrinfo']['domain']['created']:'-')+'</td></tr>';
					html += '<tr><td class="tablehead">更新时间：</td><td>'+(!!json['regrinfo']['domain']['changed']?json['regrinfo']['domain']['changed']:'-')+'</td></tr>';
					html += '<tr><td class="tablehead">过期时间：</td><td>'+(!!json['regrinfo']['domain']['expires']?json['regrinfo']['domain']['expires']:'-')+'</td></tr>';

					if(!!json['regyinfo']['servers']){
						for(var n = 0; n < json['regyinfo']['servers'].length; n++){
							html += '<tr><td class="tablehead">域名服务器：</td><td>'+json['regyinfo']['servers'][n]['server']+'</td></tr>';
						}
					}
					if(!!json['regrinfo']['domain']['nserver']){
						for(key in json['regrinfo']['domain']['nserver']){
							html += '<tr><td class="tablehead">DNS服务器：</td><td>'+key+' - '+json['regrinfo']['domain']['nserver'][key]+'</td></tr>';
						}
					}
					if(!!json['regrinfo']['domain']['status']){
						for(var n = 0; n < json['regrinfo']['domain']['status'].length; n++){
							html += '<tr><td class="tablehead">域名状态：</td><td>'+json['regrinfo']['domain']['status'][n]+'</td></tr>';
						}
					}
					html += '<tr><td class="tablehead td-row" colspan="2">详细信息</td></tr><tr><td class="domain-owner" colspan="2">'+(!!json['rawdataHtml']?json['rawdataHtml']:'-')+'</td></tr>';
					elem.html(html);
				}
				_this.parents('td').parents('tr').hide();
			}
		});
	}
function check_form(){
	var obj = document.getElementById('site'),
	query_domain = url2domain(obj.value);
	if(query_domain){
		location.href = '/'+query_domain+'/';
	}else{
		alert('请输入要查询的域名！');
		obj.focus();
	}
	return false;
}

var obj = document.getElementById('site'),
	query_domain = url2domain(obj.value);

if(query_domain){
	if(isDomain(query_domain)){
		addAllSites(query_domain);
	}
}
//nick
$('#zoned_bot').html(con_bot_content);
</script>

</body></html>