    $(".list img").lazyload({threshold:200});

    // 有下角二维码
    function resizePopqrcode() {
        var $qr = $("#popqrcode");
        if ($(window).width() <= 1280) {//小屏幕
            $("#popqrcode").addClass("fn-hide").addClass("smallscreen").removeClass("middlescreen");
            $("#gotop").addClass("smallscreen");
        }
        else if ($(window).width() <= 1366) {//1366
            $("#gotop").addClass("smallscreen");
            $("#popqrcode").removeClass("fn-hide").addClass("smallscreen").removeClass("middlescreen");
        }
        else if ($(window).width() <= 1440) {//1440
            $("#gotop").addClass("smallscreen");
            $("#popqrcode").removeClass("fn-hide").removeClass("smallscreen").addClass("middlescreen");
        }
        else {
            $("#gotop").removeClass("smallscreen");
            $("#popqrcode").removeClass("fn-hide").removeClass("smallscreen").removeClass("middlescreen");
        }
    }
    $(window).on("resize", function () {
        resizePopqrcode();
    }).trigger("resize");

    $(".popqrcode-close .icon").click(function(){$("#popqrcode").hide();return false;});
</script>
