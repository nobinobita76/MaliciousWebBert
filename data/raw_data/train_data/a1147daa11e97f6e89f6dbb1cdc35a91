<!-- 搜索日志统计、搜索点击日志统计-->
<script>
    var _mall_ahas = _mall_ahas || [];  // 这一句是为了防止在js没加载成功而点击提前触发时产生js报错，全局声明一次即可
    function trackMallEvent(action, vars, exs){
        _mall_ahas.push(['_trackEvent', action, vars, exs]);
    }
    /**
     * 搜索操作结果日志，在列表页统计
     * */
    /* 组装搜索筛选日志json对象及默认值，传递给搜索统计js
       参数定义：
     */
    var _mall_searchLog = {
        count:0
        ,sort:"default"
        ,q:__KEYWORDS
        ,fbrand:__BRAND_ID
        ,fseries:__SERIES_ID
        ,fprice:__MIN_PRICE+'-'+__MAX_PRICE
        ,flevel:__LEVEL_ID
        ,checkboxfilter:""
        ,offset:0
        ,size:24
        ,ev:0
        ,docs:""
        ,cost:""
        ,info:""
    };

    var _mall_searchLogExs = {
        fspec:__SPEC_ID
    };
    //搜索接口相关信息
        _mall_searchLog.count=36;
    _mall_searchLog.offset=0*24;
    
        //var qtime ="0ms".replace('ms','');
        _mall_searchLog.cost="0ms".replace('ms','');
    
    //排序
    if(__SORT_TYPE_ID==1 && __SORT_DIRECTION_ID==0){
        _mall_searchLog.sort="soldQuantity_desc";
    }else if(__SORT_TYPE_ID==1 && __SORT_DIRECTION_ID==1){
        _mall_searchLog.sort="soldQuantity_asc";
    }else if(__SORT_TYPE_ID==2 && __SORT_DIRECTION_ID==0){
        _mall_searchLog.sort="price_desc";
    }else if(__SORT_TYPE_ID==2 && __SORT_DIRECTION_ID==1){
        _mall_searchLog.sort="price_asc";
    }else if(__SORT_TYPE_ID==3 && __SORT_DIRECTION_ID==0){
        _mall_searchLog.sort="uv_desc";
    }else if(__SORT_TYPE_ID==3 && __SORT_DIRECTION_ID==1){
        _mall_searchLog.sort="uv_asc";
    }
    // 只看有货、只看分期、只看平行进口、只看购车节
    if(__STOCKTAG_ID=="1"){
        _mall_searchLog.checkboxfilter+="StockTag|";
    }
    if(__STAGE_TAG=="1"){
        _mall_searchLog.checkboxfilter+="StageTag|";
    }
    if(__IMPORT_TAG=="1"){
        _mall_searchLog.checkboxfilter+="importTag|";
    }
    if(__DOUBLE11_TAG=="1"){
        _mall_searchLog.checkboxfilter+="double11Tag|";
    }
    if(_mall_searchLog.checkboxfilter!=""){
        _mall_searchLog.checkboxfilter=_mall_searchLog.checkboxfilter.substring(0,_mall_searchLog.checkboxfilter.length-1)
    }
    //搜索商品信息数组
    var docs="";
    $("[searchLog]").find("[name='searchLog']").each(function(){
        var itemId= $(this).attr("itemId");
        var tagIds= $(this).attr("tagIds");
        //docs.push({itemId:itemId,tagIds:tagIds});
        docs+='{"itemId":"'+itemId+'","tagIds":"'+tagIds+'"},';
    });
    if(docs!=""){
        docs=docs.substring(0,docs.length-1);
    }
    _mall_searchLog.docs='{['+docs+']}';
    trackMallEvent('mall_search_filter',_mall_searchLog, _mall_searchLogExs);

    /**6
     * 搜索结果商品豆腐块点击事件日志-在列表页统计
     */
    $("[searchLog]").find("a").click(function(){
        try{
            var vars={};
            var obj = $(this).find("input[name='searchLog']");
            vars.x =event.clientX;
            vars.y =event.clientY;
            vars.seq = parseInt(_mall_searchLog.offset)+parseInt(obj.attr("index"))+1;
            vars.q = __KEYWORDS;
            vars.url_id = obj.attr("itemId");;
            vars.url_type= obj.attr("itemType");
            vars.anchor = obj.attr("itemName");
            if($(this).attr("href").indexOf(__MALL_WEB_HOST)>0){
                vars.target_url =$(this).attr("href");
            }else{
                vars.target_url =__MALL_WEB_HOST+$(this).attr("href");
            }
            trackMallEvent('mall_search_click',vars, null);
        }catch(e){
            try{
                console.log(e);
            }catch(error){
            }
        }
    });

</script>
</body></html>