<script type="text/javascript">
    function hsbcoverview(x)
    {
       if (typeof(HSBC) !=  "undefined"){
           HSBC.LOG.dcsuri= "/hsbc/fundreport/" + x;
       }
        DCSext.dcsVersion = "7.5.1";
        DCSext.HSBCCookie = dcsGetHSBCCookie('HSBC_COOKIEMI');
        dcsVar();
        dcsMapHSBC();
        dcsMeta();
        dcsFunc("dcsAdv");
        dcsTag();
    }
</script>

